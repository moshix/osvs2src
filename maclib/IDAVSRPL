.*                                      CHK FOR NEW BIT SET             09100040
         IDAVSERR 5,BRACKET,&BRACKET(&I)                       @ZA34940 09150040
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 09200040
&A       SETA  &A+1                     BUMP ERROR COUNTER      X04SVHS 09250040
         AGO   .R000700                 CHK NEXT SUBPARM       X03004HS 09300040
.R000800 ANOP                                                  X03004HS 09350040
         AIF   (NOT(&BB AND &NBB)).R000900 CHK MUTUALLY        X03004HS 09400040
.*                                      EXCLUSIVE KEYWORDS     X03004HS 09450040
         IDAVSERR 8,BRACKET,BB,NBB                             @ZA34940 09500040
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 09550040
.R000900 ANOP                                                  X03004HS 09600040
         AIF   (NOT(&EB AND &NEB)).R001000 CHK MUTUALLY        X03004HS 09650040
.*                                      EXCLUSIVE KEYWORDS     X03004HS 09700040
         IDAVSERR 8,BRACKET,EB,NEB                             @ZA34940 09750040
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 09800040
.R001000 ANOP                                                  X03004HS 09850040
         AIF   ('&CALLER' EQ 'RPL').R001010                     X04SVHS 09900040
.*             IF CALLED BY RPL, VALUE SET IN RPLRH3 UNDER CODESEL CODE 09950040
