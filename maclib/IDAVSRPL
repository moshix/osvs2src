.********************************************************************** 06180040
.*                                      OPTCD                           06220040
         IDAVSOPT OPTCD=&OPTCD,AM=&AM,MF=&MF,CALLER=&CALLER    @ZA34940 06260040
.*             OPTCD IS PROCESSED BY THE INNER MACRO IDAVSOPT  @ZA34940 06300040
.********************************************************************** 06340040
.*                                      CHNGDIR                         06380040
.R000100 AIF   ('&CHNGDIR' EQ '').R000600 *** CHNGDIR ***       X3004BS 06420040
         IDAVSCB3 ARG=&CHNGDIR,KWTC=X'65',BIT=YES,CHKAM=VTAM,  @ZA34940*06460040
               AM=&AM                                          @ZA34940 06500040
.*                                                              X04SVHS 06900040
&I       SETA  0                        CLEAR INDEX VARIABLE    X3004BS 06950040
&A       SETA  0                        ZERO ERROR COUNTER      X04SVHS 07000040
.R000200 ANOP                                                   X3004BS 07050040
&I       SETA  &I+1                     BUMP INDEX INTO LIST    X04SVHS 07100040
         AIF   (&I GT N'&CHNGDIR).R000300 CHECK FOR END OF LIST X04SVHS 07150040
&NCMD    SETB  ('&CHNGDIR(&I)' EQ 'NCMD' OR &NCMD)              X3004BS 07200040
&CMD     SETB  ('&CHNGDIR(&I)' EQ 'CMD' OR &CMD)                X3004BS 07250040
&NREQ    SETB  ('&CHNGDIR(&I)' EQ 'NREQ' OR &NREQ)              X3004BS 07300040
&CREQ    SETB  ('&CHNGDIR(&I)' EQ 'REQ' OR &CREQ)               X3004BS 07350040
         AIF   (&NCMD+&CMD+&NREQ+&CREQ+&A EQ &I).R000200        X04SVHS 07400040
.*             CHK FOR NEW BIT SET                                      07450040
         IDAVSERR 5,CHNGDIR,&CHNGDIR(&I)                       @ZA34940 07500040
&IDACB01 SETB  1                        SET ERROR SWITCH        X3004BS 07550040
&A       SETA  &A+1                     BUMP ERROR COUNTER      X04SVHS 07600040
         AGO   .R000200                 CHECK NEXT OPTION       X04SVHS 07650040
.*             CHECK FOR MUTUALLY EXCLUSIVE OPTIONS                     07700040
.R000300 AIF   (NOT(&NCMD AND &CMD)).R000400                    X3004BS 07750040
         IDAVSERR 8,CHNGDIR,NCMD,CMD                           @ZA34940 07800040
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 07840040
.R000400 AIF   (NOT(&NREQ AND &CREQ)).R000500                   X3004BS 07880040
         IDAVSERR 8,CHNGDIR,NREQ,REQ                           @ZA34940 07920040
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 08000040
.R000500 ANOP                                                   X3004BS 08050040
         AIF   ('&CALLER' EQ 'RPL').R000600                     X04SVHS 08100040
.*             IF CALLED BY RPL, VALUE SET IN RPLRH3 UNDER CODESEL CODE 08150040
&IDAVALC(&IDAENO) SETC '&NCMD&CMD&NREQ&CREQ.000000000000&Z16'   X3004BS 08200040
.********************************************************************** 08250040
.R000600 ANOP                                                  X03004HS 08300040
.*                                      BRACKET                         08350040
         AIF   ('&BRACKET' EQ '').R001010                      X03004HS 08400040
