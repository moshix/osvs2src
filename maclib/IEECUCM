         MACRO                                                          00030051
         IEECUCM &D,&REPLY=1,&BUFFERS=1,&MCS=,&EXIT=,&MASTCON=,        +00040051
               &HARDCDE=,&OLDWCDE=,&HARDCPY=(SYSLOG,NOCMDS,NOHCPY),    +00050051
