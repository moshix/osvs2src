    DCL 1 SMF5JSAF BASED BDY(BYTE), /* ACCT SECTION                  */ 67849102
         3 SMF5ANC PTR(8),        /* # CHAR IN THIS ACCT FIELD       */ 67899102
         3 SMF5BCD CHAR(*);       /* ACCT FIELD CHARACTERS           */ 67949102
 %IFAL06: IF IFAR06='' %THEN %GOTO IFALR1;                              67999100
    DCL 1 SMFRCD06 BASED(SMF06PTR) BDY(WORD),                           68009102
 /* RECORD TYPE 6 - SYSOUT WRITER                            @ZA10800*/ 68019100
 /* IN JES2, WRITTEN FOR EACH JOB OUTPUT ELEMENT, WHICH      @YL02120   68029102
     REPRESENTS A GROUP OF DS DIFFERENTIATED BY PUNCH OR     @YL02120   68039102
