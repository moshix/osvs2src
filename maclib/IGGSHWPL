*      %THEN %SHWPL999 = ';';           /* THEN CLOSE DCL STMNT     */  00700000
*    %IF SHWPLLVL = ''                  /* IF BLOCK NOT CONCATENATED*/  00750000
*      %THEN %GOTO SHWPL001;            /* THEN GENERATE DCL        */  00800000
*    %SHWPLDUM = SHWPLLVL||' SHWPL';    /* SET MINOR LEVEL NUMBER   */  00850000
