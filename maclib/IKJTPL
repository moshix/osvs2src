* %         THEN                                                        04950020
* %           GOTO TPLBSL4;                                             05000020
* %         ELSE                                                        05050020
* %           GOTO TPLBSL5;                                             05100020
* %TPLBSL4: /* STRUCTURE IS TO BE CONTINUED - DO NOT CLOSE     */ ;     05150020
*       Y TPLRESV  PTR(31),          /* RESERVED                     */ 05200020
* %       GOTO TPLBSL6;                                                 05250020
* %TPLBSL5: /* STRUCTURE IS COMPLETED - USE SEMI-COLON         */ ;     05300020
