 %DSVATTNN='''1''B';             /* ATTENTION PENDING                */ 27800002
 %DSVATTNF='''0''B';             /* ATTENTION BIT OFF                */ 27900002
                                                                        28000002
          3 DSVREFS  BIT(1),     /* LANGUAGE REFRESHING BIT          */ 28100002
 %DSVREFSN='''1''B';             /* LANGUAGE REFRESHING BIT ON       */ 28200002
 %DSVREFSF='''0''B';             /*  LANGUAGE REFRESHING BIT OFF     */ 28300002
                                                                        28400002
