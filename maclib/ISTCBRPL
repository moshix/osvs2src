         ISTCB3 ARG=&CODESEL,KWTC=X'91',CHKAM=VTAM,AM=&AM,     @ZA34940*10350000
               BIT=YES                                         @ZA34940 10400000
&STDRD SETB ('&CODESEL' EQ 'STANDARD' OR '&CODESEL' EQ '(STANDARD)')    10450000
