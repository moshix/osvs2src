.*                                      CHK FOR NEW BIT SET             09200000
         ISTERMAC 5,BRACKET,&BRACKET(&I)                       @ZA34940 09250000
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 09300000
&A       SETA  &A+1                     BUMP ERROR COUNTER      X04SVHS 09350000
         AGO   .R000700                 CHK NEXT SUBPARM       X03004HS 09400000
.R000800 ANOP                                                  X03004HS 09450000
         AIF   (NOT(&BB AND &NBB)).R000900 CHK MUTUALLY        X03004HS 09500000
.*                                      EXCLUSIVE KEYWORDS     X03004HS 09550000
         ISTERMAC 8,BRACKET,BB,NBB                             @ZA34940 09600000
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 09650000
.R000900 ANOP                                                  X03004HS 09700000
         AIF   (NOT(&EB AND &NEB)).R001000 CHK MUTUALLY        X03004HS 09750000
.*                                      EXCLUSIVE KEYWORDS     X03004HS 09800000
         ISTERMAC 8,BRACKET,EB,NEB                             @ZA34940 09850000
&IDACB01 SETB  1                        SET ERROR SWITCH        X04SVHS 09900000
.R001000 ANOP                                                  X03004HS 09950000
         AIF   ('&CALLER' EQ 'RPL').R001010                     X04SVHS 10000000
.*             IF CALLED BY RPL, VALUE SET IN RPLRH3 UNDER CODESEL CODE 10050000
&IDAVALC(&IDAENO) SETC '&BB&NBB&EB&NEB.000000000000&Z16'       X03004HS 10100000
.*                                                              X04SVHS 10150000
.********************************************************************** 10200000
.*                                      CODESEL                @Z40BHUC 10250000
