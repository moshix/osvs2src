.*                                                               Y06327 14300005
&A       SETA  1                                                 Y06327 14400005
&B       SETA  K'&GROUP                                          Y06327 14500005
.OK      ANOP                                                    Y06327 14600005
         AIF   ('&GROUP'(&A,1) EQ '$' OR '&GROUP'(&A,1) EQ '#' OR      X14700005
               '&GROUP'(&A,1) EQ '@').OK1                        Y06327 14800005
         AIF   ('&GROUP'(&A,1) GE 'A' AND '&GROUP'(&A,1) LE 'Z').OK1    14900005
.*                                                               Y06327 15000005
         AIF   ('&GROUP'(&A,1) GE '0' AND '&GROUP'(&A,1) LE '9' AND &A X15100005
               NE 1).OK1                                         Y06327 15200005
.GRP6    ANOP                                                    Y06327 15300005
         MNOTE 16,'IKJ54104I &GROUP INVALID FOR GROUP OPERAND'   Y06327 15400005
