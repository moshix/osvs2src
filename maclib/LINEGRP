&SNA     SETB  1                                               @YM08015 16250000
         AGO   .TERMCK                                           Y06327 16300005
.GRP5    ANOP                                                    Y06327 16400005
&GRPNAME(&I) SETC ''               FOR NON-3705 SET '' IN ARRAY  Y06327 16500005
.*  ------------------------------------------------------------------- 16600005
.*  -----      TERM= KEYWORD ANALYSIS       --------------------------- 16700005
.*  ------------------------------------------------------------------- 16800005
.TERMCK  ANOP                                                    Y06327 16900005
         AIF   ('&GROUP' EQ '').TERM CONINUE IF NOT 3705         Y06327 17000005
         AIF   (('&TERM'(1,3) EQ '226') OR ('&TERM'(4,1) EQ 'L')).T4    17100005
.*                                 2260 OR LOCAL DEVICES NOT     Y06327 17200005
.*                                 SUPPORTED ON 3705             Y06327 17300005
.TERM    AIF   (K'&TERM GT 4 OR N'&TERM GT 1).T4    TERM=KEYWORD VALID? 17400005
         AIF   ('&TERM' EQ '1050').T1   BRANCH IF TERM=1050             17500005
         AIF   ('&TERM' EQ '2741').T1   BRANCH IF TERM=2741             17600005
         AIF   ('&TERM' EQ '5041').T1   BRANCH IF TERM=5041             17700005
         AIF   ('&TERM' EQ '3335').T1   BRANCH IF TERM=3335             17800005
         AIF   ('&TERM' EQ '226R').T1   BRANCH IF TERM=226R             17900005
         AIF   ('&TERM' EQ '226L').T1   BRANCH IF TERM=226L             18000005
         AIF   ('&TERM' EQ '2265').T1   BRANCH IF TERM=2265             18100005
         AIF   ('&TERM' EQ '327L').T2   BRANCH IF TERM=327L             18200005
         AIF   ('&TERM' EQ '327R').T2   BRANCH IF TERM=327R             18300005
         AIF   ('&TERM' EQ '327S').T2   BRANCH IF TERM=327S             18400005
         AIF   ('&GROUP' EQ '').T4   327C AND PUNT NCP ONLY    @YM07280 18410000
         AIF   ('&TERM' EQ '327C').T2   BRANCH IF TERM=327C    @Y17XAYP 18430000
         AIF   ('&TERM' EQ 'PUNT').T1   BRANCH IF TERM=PUNT    @Y17XAYP 18460000
.T4      MNOTE 16,'IKJ54104I &TERM INVALID FOR TERM OPERAND'            18500005
&BADTERM SETB  1                       INDICATE BAD TERM= KEYWORD.      18600005
&QUITSW  SETB  1                       SET QUIT SW.                     18700005
         AGO   .DDNME              GO CHECK DD NAME PARAMETER           18800005
.T2      ANOP                                                           18900005
&BISYNC  SETB  1                   BISYNCHRONOUS LINE                   19000005
.T1      ANOP                                                           19100005
&T(&I)   SETC  '&TERM'             PUT VALUE IN ARRAY                   19200005
