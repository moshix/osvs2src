.********************************************************************** 04000000
         LCLA  &A                                                       04050000
         LCLB  &B1,&B2,&B3,&B4,&B5,&B6,&B7,&B8,&B9                      04100000
         LCLB  &BS1,&BS2,&BS3,&BS4                                      04150000
         LCLC  &C                                                       04200000
.E1      AIF   (T'&MF EQ 'O').ERR1     NO MF PARAMETER                  04250000
