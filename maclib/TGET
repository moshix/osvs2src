.CHKFORM ANOP                                                  @G76XRYU 44106003
         AIF   ('&MF' EQ 'L').LFORM    ..LIST EXPANSION...     @G76XRYU 44112003
         AIF   ('&MF' EQ 'I' OR '&MF' EQ '').OLDFORM STANDARD      @01C 44118003
.******************                                            @G76XRYU 44124003
.*  EXECUTE FORM  *                                            @G76XRYU 44130003
.******************                                            @G76XRYU 44136003
&NAME    CNOP  0,4                     TGET EXECUTE FORM       @G76XRYU 44142003
         AIF   ('&MF(2)'(1,1) EQ '(').MFREG  ..IF PARM IS RX.. @G76XRYU 44148003
         LA    1,&MF(2)                R1=> EXECUTE PARM LIST  @G76XRYU 44154003
         AGO   .GOTPARM                                        @G76XRYU 44160003
.MFREG   ANOP                                ..ELSE, IN A REG. @G76XRYU 44166003
         AIF   ('&MF(2)' EQ '(1)').GOTPARM  ..IF NOT REG 1..   @G76XRYU 44172003
         LR    1,&MF(2)                POINT R1 AT PARM LIST   @G76XRYU 44178003
.GOTPARM ANOP                    ...CHECK FLAG BIT SETTINGS    @G76XRYU 44184003
         MVI   0(1),0                  INSURE HI BIT OF R0 OFF @G76XRYU 44190003
         AIF   (&FLAGOFF EQ 145).SKIPAND .ANY FLAGS TURNED OFF @G76XRYU 44196003
         NI    4(1),&FLAGOFF           RESET TGET FLAGS1       @G76XRYU 44202003
.SKIPAND ANOP                    ...CHECK FOR FLAGS TO TURN ON @G76XRYU 44208003
         AIF   (&FLAGON EQ 0).PARMCK ..SKIP OR IF NO RESETS    @G76XRYU 44214003
         OI    4(1),&FLAGON            RESET TGET FLAGS 1      @G76XRYU 44220003
.PARMCK  ANOP                                                      @01C 44226003
         AIF   ('&SIZE' EQ '').CKBUFF  ...IF BUFF SIZE GIVEN.. @G76XRYU 44352003
         AIF   ('&SIZE'(1,1) EQ '(').SIZEREG ..IF INTEGER FORM @G76XRYU 44358003
         AIF   (T'&SIZE NE 'N').AROUND         .VALID & ^ NEG. @OZ42972 44364003
         AIF   (&SIZE GT 32767).SIZERR          ..NOT > 32767. @G76XRYU 44370003
.AROUND  ANOP                                                      @01C 44376003
         B     *+6                      BR AROUND SIZE             @01C 44402003
         DC    AL2(&SIZE)               SIZE CAN EXCEED 4095       @01C 44428003
         LH    0,*-2                   GET THE SIZE VALUE      @G76XRYU 44454003
         STH   0,2(1)                  INTO THE PARM LIST      @G76XRYU 44460003
         AGO   .CKBUFF                                         @G76XRYU 44466003
.SIZEREG ANOP                                  ..ELSE A REG..  @G76XRYU 44472003
         STH   &SIZE(1),2(1)           PUT BUFFER SIZE IN PARM @G76XRYU 44478003
.CKBUFF  ANOP                                                  @G76XRYU 44484003
         AIF   ('&BFF' EQ '').LOAD      ..IF A BUFF ADDR GIVEN @G76XRYU 44490003
         AIF   ('&BFF'(1,1) EQ '(').BUFFREG  ..IF AN RX ADDR.. @G76XRYU 44517003
         LA    0,&BFF                  GET ADDR OF INPUT BUFF  @G76XRYU 44544003
         STCM  0,B'0111',5(1)          PUT IN THE PARM LIST    @G76XRYU 44550003
         AGO   .LOAD                                           @G76XRYU 44556003
.BUFFREG ANOP                                ..ELSE A REG..    @G76XRYU 44562003
         STCM  &BFF(1),B'0111',5(1)    STORE BUFF ADDR IN PARM @G76XRYU 44568003
.LOAD    ANOP                          ..LOAD 0 & 1 FOR SVC..  @G76XRYU 44574003
         LM    0,1,0(1)                REG 0: BUFFER SIZE      @G76XRYU 44580003
*                                      REG 1: FLAGS,BUFF ADDR  @G76XRYU 44586003
         AGO   .SVC                                                @01C 44592003
.******************                                            @G76XRYU 44610003
.*   LIST FORM    *                                            @G76XRYU 44616003
.******************                                            @G76XRYU 44622003
.LFORM   ANOP                                                  @G76XRYU 44628003
&NAME    DS    0F                      TGET LIST FORM          @G76XRYU 44634003
         DC    H'0'                    MUST BE 0 FOR TGET      @G76XRYU 44640003
         AIF   ('&SIZE' NE '').LSTSIZE  ..IF NO BUFFER SIZE..  @G76XRYU 44646003
         DC    H'0'                    BUFFER SIZE HALFWORD    @G76XRYU 44652003
         AGO   .LBFADDR                                        @G76XRYU 44658003
.LSTSIZE ANOP                           ..ELSE A BUFFER SIZE.. @G76XRYU 44664003
         AIF   ('&SIZE'(1,1) EQ '(').REGERR  ..IF NOT A REG..  @G76XRYU 44670003
         AIF   (T'&SIZE NE 'N').ARNSIZL IF AN INTEGER              @01C 44676003
         AIF   (&SIZE GT 32767).SIZERR        .AND NOT >32767. @G76XRYU 44682003
.ARNSIZL DC    AL2(&SIZE)              INPUT BUFFER SIZE           @01C 44688003
.LBFADDR ANOP                          ...CHECK FOR BUFF ADDR. @G76XRYU 44694003
         DC    AL1(&OPT)               TGET FLAGS              @G76XRYU 44700003
         AIF   ('&BFF' NE '').LBFFLAB  ..IF NO BUFFER ADDR..   @G76XRYU 44706003
