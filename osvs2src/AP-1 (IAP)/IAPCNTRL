         L     @09,TCBPTR(,@09)                                 01S0499 00808000
         ST    @09,AP1TCBAD(,AP1REG)                            01S0499 00809000
*           DO;                     /* ATTACH EP(IAPTIMER) DPMOD(-1)    00810000
*                                      PARAM(AP1BLOCK) TCB(AP1TIMEA) */ 00811000
*             I042=I042&&I042;      /* ZERO THE LIST                 */ 00812000
