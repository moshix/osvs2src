*   /*****************************************************************/ 04036000
*   /*                                                               */ 04037000
*   /* MOVES CCHH IN BYTES 5 & 6 OF SENSE DATA INTO CCHH IN AP1SEEK  */ 04038000
*   /*                                                               */ 04039000
*   /*****************************************************************/ 04040000
*                                                               01S1246 04041000
*   CCHH=0;                         /* CLEAR DESTINATION             */ 04042000
