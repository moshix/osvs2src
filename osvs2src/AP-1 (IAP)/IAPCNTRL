*   /*****************************************************************/ 04111000
*   /*                                                               */ 04112000
*   /* GET INTO DEB KEY                                              */ 04113000
*   /*                                                               */ 04114000
*   /*****************************************************************/ 04115000
*                                                               01S1264 04116000
*   DO;                             /* MODESET EXTKEY(DATAMGT)       */ 04117000
*     SPKA(80);                     /* SET PSW KEY                   */ 04118000
         LA    @09,80                                           01S1265 04119000
         SPKA  0(@09)                                           01S1265 04120000
*   END;                            /* MODESET EXTKEY(DATAMGT) DATA     04121000
