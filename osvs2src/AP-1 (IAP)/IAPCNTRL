*   RFY                                                         01S1268 04131000
*     GPR04F RSTD;                                              01S1268 04132000
*   DO;                             /* MODESET EXTKEY(TCB)      01S1269 04133000
*                                      WORKREG(GPR04F)               */ 04134000
*     GPR04F=TCBPKF;                /* GET KEY FROM TCB              */ 04135000
         L     @06,FLCCVT                                       01S1270 04136000
