         LA    @05,STRTCHNK(,@05)                                  0354 00463000
         ST    @05,CHUNKPTR                                        0354 00464000
*     /***************************************************************/ 00465000
*     /*                                                             */ 00466000
*     /* THIS LOOP SCANS THE CHUNKS IN A BLOCK. THERE ARE 8  @Z40LB38*/ 00467000
*     /* 32 BYTE CHUNKS PER BLOCK.                           @Z40LB38*/ 00468000
*     /*                                                             */ 00469000
*     /***************************************************************/ 00470000
*                                                                  0355 00471000
*     DO CHKINDEX=1 TO CHUNKNUM BY 1;/*                      @Z40LB38*/ 00472000
         LA    @05,1                                               0355 00473000
         ST    @05,CHKINDEX                                        0355 00474000
@DL00355 DS    0H                                                  0356 00475000
*       CUBEPTR=CHUNKPTR;           /*                       @Z40LB38*/ 00476000
*                                                                  0356 00477000
         L     CUBEPTR,CHUNKPTR                                    0356 00478000
*       /*************************************************************/ 00479000
*       /*                                                           */ 00480000
*       /* THIS LOOP SCANS THE CUBES PER CHUNK. THERE ARE 8  @Z40LB38*/ 00481000
*       /* 4 BYTE CUBES PER CHUNK. HOWEVER, THE LAST CUBE IN EACH    */ 00482000
*       /* CHUNK IS UNUSED AND IS SET TO ZEROES.             @Z40LB38*/ 00483000
*       /*                                                           */ 00484000
