*       /*************************************************************/ 02844000
*       /*                                                           */ 02845000
*       /*                                                   @Y30LB39*/ 02846000
*       /* BEGIN DO LOOP TO SEARCH FOR THE MESSAGE           @Y30LB39*/ 02847000
*       /* INDEX CORRESPONDING TO THE REASON CODE            @Y30LB39*/ 02848000
*       /* PASSED AS THE FIRST ARGUMENT.                     @Y30LB39*/ 02849000
*       /* THE DO LOOP BEGINS WITH THE SECOND ARRAY          @Y30LB39*/ 02850000
*       /* ENTRY SINCE THE FIRST IS REQUIRED BY THE          @Y30LB39*/ 02851000
*       /* SEARCH ARRAY ITSELF.                              @Y30LB39*/ 02852000
*       /* THE SEARCH CONTINUES UNTIL THE REASON CODE        @Y30LB39*/ 02853000
*       /* IS FOUND, AT WHICH TIME THE SEARCH INDEX          @Y30LB39*/ 02854000
*       /* IS PASSED TO THE PRINT MODULE TO PRINT            @Y30LB39*/ 02855000
*       /* THE MESSAGE ASSOCIATED WITH THAT PARTICULAR       @Y30LB39*/ 02856000
*       /* REASON CODE.                                      @Y30LB39*/ 02857000
*       /* IF THERE IS NO MATCH FOUND IN THE ARRAY,          @Y30LB39*/ 02858000
*       /* THEN THE INDEX WILL BE THE LAST POSITION IN       @Y30LB39*/ 02859000
*       /* ARRAY. THIS WILL BE THE INDICATION THAT           @Y30LB39*/ 02860000
*       /* THE REASON CODE ITSELF SHOULD BE PRINTED.         @Y30LB39*/ 02861000
*       /*                                                   @Y30LB39*/ 02862000
*       /*                                                           */ 02863000
*       /*************************************************************/ 02864000
*                                                                  0980 02865000
*                                   /*                       @Y30LB39*/ 02866000
*       ENTRYNUM=1;                 /*                       @Y30LB39*/ 02867000
         LA    ENTRYNUM,1                                          0980 02868000
*       DO UNTIL(SSRHALF=SSARRAY(ENTRYNUM)|SSARRAY(ENTRYNUM)=0);   0981 02869000
@DL00981 DS    0H                                                  0982 02870000
*         ENTRYNUM=ENTRYNUM+1;      /*                       @Y30LB39*/ 02871000
         AL    ENTRYNUM,@CF00218                                   0982 02872000
*       END;                        /*                       @Y30LB39   02873000
*                                                            @Y30LB39*/ 02874000
*                                                                  0983 02875000
@DE00981 L     @03,@PC00004                                        0983 02876000
