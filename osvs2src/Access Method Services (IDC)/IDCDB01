*                    /************************************************/ 00418000
*                    /*                                              */ 00419000
*                    /*  DETERMINE IF 'COUNT' DUMP LIMIT REACHED     */ 00420000
*                    /*                                              */ 00421000
*                    /************************************************/ 00422000
*                                                                  0216 00423000
*                    IF REALCNT(FULLSCAN) <= FDUMPCNT(FULLSCAN)    0216 00424000
*                      THEN                                        0216 00425000
         AL    @05,@CF00624                                        0216 00426000
         CH    @03,FDUMPCNT-10(@10,@05)                            0216 00427000
         BH    @RF00216                                            0216 00428000
*SNAPDUMP:               DO;                                       0217 00429000
SNAPDUMP DS    0H                                                  0218 00430000
*                          /******************************************/ 00431000
*                          /*                                        */ 00432000
*                          /*  'COUNT' DUMP LIMIT NOT REACHED -      */ 00433000
*                          /*  PRODUCE FULL REGION (SNAP) DUMP       */ 00434000
*                          /*                                        */ 00435000
*                          /******************************************/ 00436000
*                                                                  0218 00437000
*                          SNAPID = SNAPID + 1;                    0218 00438000
*                                                                  0218 00439000
         L     @10,@PC00001                                        0218 00440000
         L     @05,GDTDBH(,@10)                                    0218 00441000
         LA    @15,1                                               0218 00442000
         AH    @15,SNAPID(,@05)                                    0218 00443000
         STH   @15,SNAPID(,@05)                                    0218 00444000
