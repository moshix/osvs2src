         L     @06,CTGFVT(,@06)                                    0899 01601000
         L     @06,CTGFVDCH(,@06)                                  0899 01602000
         L     @06,CTGFVSPC(,@06)                                  0899 01603000
         LTR   @06,@06                                             0899 01604000
         BZ    @RF00899                                            0899 01605000
*                    INTGTOTL = INTGTOTL + 1;              /*@Z40WSJH*/ 01606000
*                                                                  0900 01607000
         AL    INTGTOTL,@CF00218                                   0900 01608000
*                /****************************************************/ 01609000
*                /*                                          @Z40WSJH*/ 01610000
*                /* VERIFY THAT THE INTGTOTL VALUE IS PERMISSABLE:   */ 01611000
*                /* THE ALLOWABLE LEVELS OF SPACE SPECIFICATIONS ARE */ 01612000
*                /*  V,C,D,I - THE C LEVEL IS REMOVED,INTGTOTL=10    */ 01613000
*                /*  V,C     - LEAVE AS IS           ,INTGTOTL=7     */ 01614000
*                /*  V,C,D   - THE C LEVEL IS REMOVED,INTGTOTL=8     */ 01615000
*                /*                                          @Z40WSJH*/ 01616000
*                /****************************************************/ 01617000
*                                                                  0901 01618000
