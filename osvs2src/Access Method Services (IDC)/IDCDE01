*                RETURN;                                           1074 02752000
*                                                                  1074 02753000
         B     @EL00003                                            1074 02754000
*                END;                                              1075 02755000
*                                                                  1075 02756000
*            /********************************************************/ 02757000
*            /*                                                      */ 02758000
*            /* LOOP THRU THE LIST OF VOLUMES REFERENCED BY DALVLPTR */ 02759000
*            /* MOVING EACH VOLSER INTO THE LIST PASSED VIA THE      */ 02760000
*            /* ALLAGL AND COMPUTE THE LENGTH OF THE NON-BLANK       */ 02761000
*            /* PORTION OF THE VOLSER, THIS LENGTH PRECEEDES EACH    */ 02762000
*            /* VOLSER IN THE ALLAGL VOLUME LIST                     */ 02763000
*            /*                                                      */ 02764000
*            /********************************************************/ 02765000
*                                                                  1076 02766000
*DALOOP1:    DO N = 1 TO ALC#ENT;                                  1076 02767000
