         LA    @01,@AL00001                                        0630 00256000
         BALR  @14,@15                                             0630 00257000
*         /*@Z40WSJH*/                                             0631 00258000
*                                                                  0631 00259000
*        /************************************************************/ 00260000
*        /*                                                          */ 00261000
*        /* PROCEDURE NAME - CTLGPROC (CATALOG PROCESSOR, LEVEL TWO) */ 00262000
*        /*                                                          */ 00263000
*        /* FUNCTION - OVERSEES THE CONSTRUCTION OF THE VSAM CPL-FVT-*/ 00264000
*        /*            FPL COMPLEX FOR DEFINING A VSAM MASTER OR USER*/ 00265000
*        /*            CATALOG.                                      */ 00266000
*        /*            THE CPL-FVT-FPL COMPLEX CONSTRUCTED CONSISTS  */ 00267000
*        /*            OF:                                           */ 00268000
*        /*              . CPL - WHICH BASICALLY INDICATES THE TYPE  */ 00269000
*        /*                OF CATALOG REQUEST AND REFERENCES THE     */ 00270000
*        /*                CLUSTER FVT.                              */ 00271000
*        /*              . FVT - FOUR FVTS ARE INCLUDED IN THE       */ 00272000
*        /*                COMPLEX. THE CLUSTER FVT(REFERENCED BY    */ 00273000
*        /*                THE CPL) REFERENCES THE - DATA,INDEX AND  */ 00274000
*        /*                SPACE FVTS. THE FVTS BASICALLY ORGANIZE   */ 00275000
*        /*                THE REFERENCE TO THE VARIOUS FPLS AND     */ 00276000
*        /*                'PSEUDO FIELDS' (CONSTRUCTED IN THE LEVEL */ 00277000
*        /*                THREE PARAMETER PROCESSORS).              */ 00278000
*        /*              . FVT/'PSEUDO FIELDS' - THESE STRUCTURES    */ 00279000
*        /*                (REFERENCED BY THE FVTS) SPECIFY  THE     */ 00280000
*        /*                ATTRIBUTES OF THE OBJECT BEING DEFINED    */ 00281000
*        /*                                                          */ 00282000
*        /*            EACH OBJECT (CLUSTER,DATA,INDEX AND SPACE)    */ 00283000
*        /*            DEFINITION IS INITIALIZED BY INVOCATION OF THE*/ 00284000
*        /*            LEVEL THREE PARAMETER PROCESSORS UNDER THE    */ 00285000
*        /*            CONTROL OF CTLGPROC.                          */ 00286000
*        /*            INDEX AND DATA OBJECTS REQUIRE TWO PASSES IN  */ 00287000
*        /*            THEIR CONSTRUCTION:                           */ 00288000
*        /*              . IMPLICIT PASS - ATTRIBUTES FROM CATALOG   */ 00289000
*        /*                PARAMETERLIST ARE INITILAIZED             */ 00290000
*        /*              . EXPLICIT PASS - ATTRIBUTES FROM THE APPLIC*/ 00291000
*        /*                ABLE INDEX OR DATA PARAMETER LIST ARE     */ 00292000
*        /*                INITIALIZED                               */ 00293000
*        /*                                                          */ 00294000
*        /*            CTLGPROC IS INVOKED BY AND RETURNS TO IDCDE01 */ 00295000
*        /*            DEFSTID IS USED TO COMMUNICATE THE PROCESSING */ 00296000
*        /*            STATUS.                                       */ 00297000
*        /*                                                          */ 00298000
*        /* INPUT -                                                  */ 00299000
*        /*            . DEFSTID - INTERNAL STATUS COMMUNICATOR      */ 00300000
*        /*            . DECPLFVT - AREA INTO WHICH THE CPL-FVT      */ 00301000
*        /*              PORTIONS OF THE COMPLEX ARE CONSTRUCTED     */ 00302000
*        /*              THIS AREA IS DECLARED IN THE MAIN PROCEDURE */ 00303000
*        /*              (IDCDE01) AND IS THUS REFERENCABLE BY ALL   */ 00304000
*        /*              INTERNAL PROCEDURES                         */ 00305000
*        /*            . FDTTBL - GLOBAL TABLE CONTAINING THE ENCODED*/ 00306000
*        /*              COMMAND DESCRIPTOR                          */ 00307000
*        /*                                                          */ 00308000
*        /* OUTPUT -                                                 */ 00309000
*        /*            . DEFSTID IS INITIALIZED TO THE PROCESSING    */ 00310000
*        /*              STATUS. ONLY A 'NORMAL' CONDITION WILL      */ 00311000
*        /*              PERMIT CATALOG INVOCATION                   */ 00312000
*        /*            . DECPLFVT - THE CPL-FVT-FPL COMPLEX IS       */ 00313000
*        /*              COMPLETELY INITIALIZED                      */ 00314000
*        /*                                                          */ 00315000
*        /************************************************************/ 00316000
*CTLGPROC:                                                         0631 00317000
*        PROCEDURE;                                                0631 00318000
@EL00001 L     @13,4(,@13)                                         0631 00319000
@EF00001 L     @00,@SIZDATD                                        0631 00320000
         LR    @01,@08                                             0631 00321000
         FREEMAIN R,LV=(0),A=(1)                                        00322000
@ER00001 LM    @14,@12,12(@13)                                     0631 00323000
         BR    @14                                                 0631 00324000
CTLGPROC STM   @14,@12,@SA00002                                    0631 00325000
*                                                                  0632 00326000
*        /************************************************************/ 00327000
*        /*                                                          */ 00328000
*        /* ENTRY POINT TO CTLGPROC - CATALOG PROCESSOR              */ 00329000
*        /*                                                          */ 00330000
*        /************************************************************/ 00331000
*                                                                  0632 00332000
*        RFY REG7   RESTRICTED;                            /*@Z40WSJH*/ 00333000
*        RFY FBASEREG RESTRICTED;                                  0633 00334000
*        OLDERID2 = NEWERID2;                                      0634 00335000
         L     @06,@PC00001                                        0634 00336000
         L     @06,GDTTR2(,@06)                                    0634 00337000
         MVC   @TS00001(95),NEWERID2(@06)                          0634 00338000
         MVC   OLDERID2(95,@06),@TS00001                           0634 00339000
*        NEWID2 = 'DE21';                                          0635 00340000
*                                                                  0635 00341000
         MVC   NEWID2(4,@06),@CC02337                              0635 00342000
*        /***********************************************************/  00343000
*        /*                                                          */ 00344000
*        /* BUILD FVT FOR CLUSTER OBJECT                             */ 00345000
*        /* THIS PROCESS INVOLVES INITIALIZATION OF THE CLUSTER FVT  */ 00346000
*        /* AND THE INVOCATION OF LEVEL THREE PROCESSORS TO BUILD    */ 00347000
*        /* THE FVT/'PSEUDO FIELDS'                                  */ 00348000
*        /*                                                          */ 00349000
*        /************************************************************/ 00350000
*                                                                  0636 00351000
*        CLSTRID = 'CLSTRFVT';           /* CLUSTER FVT DUMP ID      */ 00352000
         L     @06,@PC00001+8                                      0636 00353000
         L     @05,DEPLPTR(,@06)                                   0636 00354000
         MVC   CLSTRID(8,@05),@CC02338                             0636 00355000
*        CTGFVPTR = ADDR(CLSTRFVT);      /* SET FVT POINTER TO CLUSTR*/ 00356000
         LA    @15,CLSTRFVT(,@05)                                  0637 00357000
         ST    @15,CTGFVPTR(,@06)                                  0637 00358000
*        CTGFVT = CTGFVPTR;              /* CLUSTER FVT ADDR IN CPL  */ 00359000
         L     @14,CTGPLPTR(,@06)                                  0638 00360000
         ST    @15,CTGFVT(,@14)                                    0638 00361000
*        CTGFVTYP =CTGFVCL;              /* SET TYPE IN CLUSTER FVT  */ 00362000
         MVI   CTGFVTYP(@15),C'C'                                  0639 00363000
*        CTGFVDCH = ADDR(DATAFVT);       /* DATA FVT ADDR IN CL. FVT */ 00364000
         LA    @03,DATAFVT(,@05)                                   0640 00365000
         ST    @03,CTGFVDCH(,@15)                                  0640 00366000
*        CTGFVICH = ADDR(INDEXFVT);      /* INDEX FVT ADDR IN CL. FVT*/ 00367000
         LA    @03,INDEXFVT(,@05)                                  0641 00368000
         ST    @03,CTGFVICH(,@15)                                  0641 00369000
*        CTGFVVCH = ADDR(VOLFVT);        /* VOL FVT ADDR IN CL. FVT  */ 00370000
         LA    @05,VOLFVT(,@05)                                    0642 00371000
         ST    @05,CTGFVVCH(,@15)                                  0642 00372000
*        PROTFLAG = GO;                  /* SET PROTPROC 1ST TIME FLG*/ 00373000
         MVI   PROTFLAG(@06),X'00'                                 0643 00374000
*        NAMEFLG = GO;                   /* FIRST TIME NAMEPROC FLAG */ 00375000
         MVI   NAMEFLG(@06),X'00'                                  0644 00376000
*        ALLCFLAG = GO;                  /* FIRST TIME ALLCPROC FLAG */ 00377000
         MVI   ALLCFLAG(@06),X'00'                                 0645 00378000
*        CATINDEX = 2;                   /* INDEX TO ACB/IOCS PTRS   */ 00379000
*                                                                  0646 00380000
         MVC   CATINDEX(4,@06),@CF00154                            0646 00381000
*        /************************************************************/ 00382000
*        /*                                                          */ 00383000
*        /* INIT CATLG TYPE IN CPL AND CATLG NAME IN CLUSTER FVT     */ 00384000
*        /*                                                          */ 00385000
*        /************************************************************/ 00386000
*        IF (ADDR(MCAT)) ^= NULLPTR                                0647 00387000
*          THEN                                                    0647 00388000
         L     @06,FDTPTR+12(,FBASEREG)                            0647 00389000
         LTR   @06,@06                                             0647 00390000
         BZ    @RF00647                                            0647 00391000
*            DO;                                                   0648 00392000
*                                                                  0648 00393000
*            CTGTYPE = CTGTMCAT;                           /*@YL026UG*/ 00394000
         MVI   CTGTYPE(@14),C'M'                                   0649 00395000
*            CTGFVENT = ADDR(METRYVAL);                    /*@YL026UG*/ 00396000
*                                                                  0650 00397000
         L     @06,FDTPTR+60(,FBASEREG)                            0650 00398000
         LA    @06,METRYVAL(,@06)                                  0650 00399000
         ST    @06,CTGFVENT(,@15)                                  0650 00400000
*            END;                                                  0651 00401000
*                                                                  0651 00402000
*          ELSE                                                    0652 00403000
*            DO;                                                   0652 00404000
*                                                                  0652 00405000
         B     @RC00647                                            0652 00406000
@RF00647 DS    0H                                                  0653 00407000
*            CTGTYPE = CTGTUCAT;                           /*@YL026UG*/ 00408000
         L     @06,@PC00001+8                                      0653 00409000
         L     @05,CTGPLPTR(,@06)                                  0653 00410000
         MVI   CTGTYPE(@05),C'U'                                   0653 00411000
*            CTGFVENT = ADDR(UETRYVAL);                    /*@YL026UG*/ 00412000
*                                                                  0654 00413000
         L     @06,CTGFVPTR(,@06)                                  0654 00414000
         L     @05,FDTPTR+788(,FBASEREG)                           0654 00415000
         LA    @05,UETRYVAL(,@05)                                  0654 00416000
         ST    @05,CTGFVENT(,@06)                                  0654 00417000
*            END;                                                  0655 00418000
*        /************************************************************/ 00419000
*        /*                                                          */ 00420000
*        /* MODELING DOES NOT APPLY TO CLUSTER FVT OF A CATALOG FOR  */ 00421000
*        /* ALLOCATION ATTRIBUTES                                    */ 00422000
*        /*                                                          */ 00423000
*        /************************************************************/ 00424000
*                                                                  0656 00425000
*        MDLTABL = ''B;                                            0656 00426000
