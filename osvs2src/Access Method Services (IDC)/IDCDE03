         BR    @14                                                 0749 01865000
*END CTLGPROC;                                                     0750 01866000
         B     @EL00002                                            0750 01867000
*        /************************************************************/ 01868000
*        /*                                                          */ 01869000
*        /* PROCEDURE NAME - DSETPROC (DATA SET PROCESSOR, LEVEL TWO)*/ 01870000
*        /*                                                          */ 01871000
*        /* FUNCTION - OVERSEES THE CONSTRUCTION OF VSAM KEY SEQUENCE*/ 01872000
*        /*            (KSDS), EVENT SEQUENCE(ESDS) ,RELATIVE RECORD */ 01873000
*        /*            (RRDS) , AND PAGESPACE DATA SETS      @Y30SSJH*/ 01874000
*        /*                                                          */ 01875000
*        /*            THE CPL-FVT-FPL COMPLEX,WHICH DESCRIBES THE   */ 01876000
*        /*            DATA SET, CONSISTS OF:                        */ 01877000
*        /*                                                  @YL026UG*/ 01878000
*        /*              . CPL - BASICALLY INDICATES THE TYPE OF     */ 01879000
*        /*                CATALOG REQUEST AND REFERENCES THE CLUSTER*/ 01880000
*        /*                FVT.                                      */ 01881000
*        /*                                                  @YL026UG*/ 01882000
*        /*              . FVT - A MAXIMUM OF FOUR FVTS ARE INCLUDED */ 01883000
*        /*                IN THE COMPLEX. THE CLUSTER FVT(REFERENCED*/ 01884000
*        /*                BY THE CPL) REFERENCES THE DATA AND OPTION*/ 01885000
*        /*                ALLY THE INDEX AND SPACE FVTS.            */ 01886000
*        /*                THE INDEX FVT IS PRESENT ONLY IF THE DATA */ 01887000
*        /*                SET IS KSDS.                              */ 01888000
*        /*                THE SPACE FVT IS INCLUDED ONLY IF THE DATA*/ 01889000
*        /*                AND/OR INDEX COMPONENTS ARE 'UNIQUE'.     */ 01890000
*        /*                                                          */ 01891000
*        /*              . FPL/'PSEUDO FIELDS' - THESE STRUCTURES    */ 01892000
*        /*                (REFERENCED BY THE FVTS) SPECIFY ATRIBUTES*/ 01893000
*        /*                OF THE STRUCTURE BEING DEFINED.           */ 01894000
*        /*                                                          */ 01895000
*        /*            EACH COMPONENT(CLUSTER,DATA,INDEX) DEFINITION */ 01896000
*        /*            IS INITIALIZED BY INVOCATION OF THE LEVEL 3   */ 01897000
*        /*            PARAMETER PROCESSOR                           */ 01898000
*        /*                                                          */ 01899000
*        /*            INDEX AND DATA COMPONENTS REQUIRE TWO PASSES  */ 01900000
*        /*            IN THEIR CONSTRUCTION:                        */ 01901000
*        /*              . IMPLICIT PASS - ATTRIBUTES FROM THE       */ 01902000
*        /*                CLUSTER PARAMETER LIST ARE INITIALIZED    */ 01903000
*        /*              . EXPLICIT PASS - ATTRIBUTES FROM THE       */ 01904000
*        /*                DATA OR INDEX PARAMETER LIST ARE USED IN  */ 01905000
*        /*                INITIALIZATION                            */ 01906000
*        /*            THE DATA COMPONENT OF A PAGESPACE     @YL026UG*/ 01907000
*        /*            RECEIVES EXPLICIT ATTRIBUTES ONLY FROM        */ 01908000
*        /*            THE MODEL OF A DATA COMPONENT, SINCE  @YL026UG*/ 01909000
*        /*            THE DATA PARAMETERLIST IS NOT INCLUDED        */ 01910000
*        /*            AS PART OF THE DEFINE PAGESPACE COMMAND       */ 01911000
*        /*                                                          */ 01912000
*        /*            MODELING IS EFFECTED BY INVOKING MODELPRC,    */ 01913000
*        /*            WHICH LOCATES THE MODEL OBJECT. THE ATTRIBUTES*/ 01914000
*        /*            OF THE MODEL OBJECT ARE REFERENCED BY THE     */ 01915000
*        /*            MODEL TABLE(BUILT BY MODELPRC). THIS MODEL    */ 01916000
*        /*            TABLE IS PASSED AS AN ARGUMENT TO PARAMETER   */ 01917000
*        /*            PROCESSORS                                    */ 01918000
*        /*                                                          */ 01919000
*        /*            DSETPROC IS INVOKED BY AND RETURNS TO IDCDE01.*/ 01920000
*        /*                                                          */ 01921000
*        /* INPUT -                                                  */ 01922000
*        /*        . DEFSTID - GLOBAL INTERNAL STATUS COMMUNICATOR   */ 01923000
*        /*        . DECPLFVT - AREA INTO WHICH THE CPL-FVT PORTIONS */ 01924000
*        /*          OF THE COMPLEX ARE CONSTRUCTED                  */ 01925000
*        /*        . FDTTBL - DSETPROC REFERENCES THE FDTTBL         */ 01926000
*        /*                                                          */ 01927000
*        /* OUTPUT -                                                 */ 01928000
*        /*        . DEFSTID - INITIALIZED TO PROCESSING STATUS,ONLY */ 01929000
*        /*          A 'NORMAL' CONDITION ALLOWS CATALOG INVOCATION  */ 01930000
*        /*        . DECPLFVT - THE CPL-FVT-FPL COMPLEX IS COMPLETELY*/ 01931000
*        /*          INITIALIZED FOR THE VSAM DATA SET               */ 01932000
*        /*                                                          */ 01933000
*        /************************************************************/ 01934000
*                                                                  0751 01935000
*DSETPROC:                                                         0751 01936000
*PROCEDURE;                                                        0751 01937000
DSETPROC STM   @14,@12,@SA00003                                    0751 01938000
*                                                                  0752 01939000
*        /************************************************************/ 01940000
*        /*                                                          */ 01941000
*        /* ENTRY POINT TO DSETPROC - DATA SET PROCESSOR             */ 01942000
*        /*                                                          */ 01943000
*        /************************************************************/ 01944000
*                                                                  0752 01945000
*        RFY REG7   RESTRICTED;                            /*@Z40WSJH*/ 01946000
*        RFY FBASEREG RESTRICTED;                                  0753 01947000
*        OLDERID2 = NEWERID2;                                      0754 01948000
         L     @06,@PC00001                                        0754 01949000
         L     @06,GDTTR2(,@06)                                    0754 01950000
         MVC   @TS00001(95),NEWERID2(@06)                          0754 01951000
         MVC   OLDERID2(95,@06),@TS00001                           0754 01952000
*        NEWID2 = 'DE22';                                          0755 01953000
*                                                                  0755 01954000
         MVC   NEWID2(4,@06),@CC02346                              0755 01955000
*        /************************************************************/ 01956000
*        /*                                                          */ 01957000
*        /* CONSTRUCT FVT DESCRIBING CLUSTER OBJECT                  */ 01958000
*        /*                                                          */ 01959000
*        /************************************************************/ 01960000
*                                                                  0756 01961000
*        CLSTRID = 'CLSTRFVT';                                     0756 01962000
         L     @06,@PC00001+8                                      0756 01963000
         L     @05,DEPLPTR(,@06)                                   0756 01964000
         MVC   CLSTRID(8,@05),@CC02338                             0756 01965000
*        CTGFVPTR = ADDR(CLSTRFVT);                                0757 01966000
         LA    @05,CLSTRFVT(,@05)                                  0757 01967000
         ST    @05,CTGFVPTR(,@06)                                  0757 01968000
*        CTGFVT = CTGFVPTR;                                        0758 01969000
         L     @15,CTGPLPTR(,@06)                                  0758 01970000
         ST    @05,CTGFVT(,@15)                                    0758 01971000
*        CTGFVTYP = 'C';                                           0759 01972000
         MVI   CTGFVTYP(@05),C'C'                                  0759 01973000
*        PROTFLAG = GO;                                            0760 01974000
         MVI   PROTFLAG(@06),X'00'                                 0760 01975000
*        ALLCFLAG = GO;                                            0761 01976000
         MVI   ALLCFLAG(@06),X'00'                                 0761 01977000
*        NAMEFLG  = GO;                                            0762 01978000
         MVI   NAMEFLG(@06),X'00'                                  0762 01979000
*        CATINDEX = 2;                                             0763 01980000
*                                                                  0763 01981000
         MVC   CATINDEX(4,@06),@CF00154                            0763 01982000
*        /************************************************************/ 01983000
*        /*                                                  @YL026UG*/ 01984000
*        /* PAGESPACE DATA SETS REQUIRE THAT THE CPL FIELD   @Y30SSJH*/ 01985000
*        /* CTGTYPE BE INITIALIZED TO 'P'.                   @YL026UG*/ 01986000
*        /*                                                  @YL026UG*/ 01987000
*        /************************************************************/ 01988000
*        IF (ADDR(PGSP)) ^= NULLPTR                                0764 01989000
*          THEN                                                    0764 01990000
         L     @06,FDTPTR+24(,FBASEREG)                            0764 01991000
         LTR   @06,@06                                             0764 01992000
         BZ    @RF00764                                            0764 01993000
*            CTGTYPE = CTGTPGSP;                           /*@YL026UG*/ 01994000
*                                                                  0765 01995000
         MVI   CTGTYPE(@15),C'P'                                   0765 01996000
