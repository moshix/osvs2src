*                                                                  0485 01663000
*                                   /*                       @Y30LB39*/ 01664000
*   DECLARE                                                        0485 01665000
*     ENTRYNUM FIXED(8) UNSIGNED;   /*                       @Y30LB39   01666000
*                                                            @Y30LB39*/ 01667000
*                                                                  0485 01668000
*   /*****************************************************************/ 01669000
*   /*                                                               */ 01670000
*   /*                                                       @Y30LB39*/ 01671000
*   /* THE NEXT DECLARE IS FOR THE COMMON MESSAGE            @Y30LB39*/ 01672000
*   /* NUMBER TO IDENTIFY THE ECB POST CODE                  @Y30LB39*/ 01673000
*   /* MESSAGE CONTAINED IN STATIC TEXT MODULE               @Y30LB39*/ 01674000
*   /* IDCTSCM0.                                             @Y30LB39*/ 01675000
*   /*                                                       @Y30LB39*/ 01676000
*   /*                                                               */ 01677000
*   /*****************************************************************/ 01678000
*                                                                  0486 01679000
