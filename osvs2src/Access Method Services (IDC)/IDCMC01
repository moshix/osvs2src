*   /*****************************************************************/ 00755000
*   /*                                                               */ 00756000
*   /* CONVERT -                                             @G24LB22*/ 00757000
*   /* THIS ROUTINE IS CALLED BY EITHER 'SETUPXYZ' AND/OR 'SETUPCHG'.*/ 00758000
*   /* IT PURPOSE IS TO TRANSLATE A CELL LOCATION INTO A TWO BYTE    */ 00759000
*   /* FIELD WITH THE LZYX IN THE FORM OF 'LLZYYYYYXXXXXXXX. IF THE  */ 00760000
*   /* LOCATION IS THE EXIT STATION ADDRESS, THEN THE MSFID (LL) IS  */ 00761000
*   /* OMITTED.                                              @G24LB22*/ 00762000
*   /*                                                               */ 00763000
*   /*****************************************************************/ 00764000
*                                                                  0427 00765000
*CONVERT:                                                          0427 00766000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB22*/ 00767000
CONVERT  DS    0H                                                  0428 00768000
*   RFY                                                            0428 00769000
*     REG14 RSTD;                   /* RESTRICT R14          @G24LB22*/ 00770000
*   RETADDR2=REG14;                 /* SAVE RET. ADDR.       @G24LB22*/ 00771000
         LR    RETADDR2,REG14                                      0429 00772000
*   RFY                                                            0430 00773000
*     REG14 UNRSTD;                 /* FREE R14              @G24LB22*/ 00774000
*   OLDERID2=NEWERID2;                                             0431 00775000
         L     @10,@PC00001                                        0431 00776000
         L     @10,GDTTR2(,@10)                                    0431 00777000
         MVC   @TS00001(95),NEWERID2(@10)                          0431 00778000
         MVC   OLDERID2(95,@10),@TS00001                           0431 00779000
*   NEWID2='MCAF';                  /*                       @G24LB22*/ 00780000
         MVC   NEWID2(4,@10),@CC01298                              0432 00781000
*   CELNEWX=XVAL;                   /* SAVE THE X VALUE      @G24LB22*/ 00782000
         STC   XVAL,CELNEWX                                        0433 00783000
*   CELLZY=0;                       /* CLEAR THIS FIELD      @G24LB22*/ 00784000
         MVI   CELLZY,X'00'                                        0434 00785000
*   CELLZY=YVAL;                    /* SAVE THE Y VALUE      @G24LB22*/ 00786000
         STC   YVAL,CELLZY                                         0435 00787000
*   CELLZY=CELLZY|(ZVAL*32);        /* CALC AND SAVE Z       @G24LB22*/ 00788000
*                                                                  0436 00789000
         SLR   @10,@10                                             0436 00790000
         IC    @10,CELLZY                                          0436 00791000
         LR    @09,ZVAL                                            0436 00792000
         SLA   @09,5                                               0436 00793000
         OR    @10,@09                                             0436 00794000
         STC   @10,CELLZY                                          0436 00795000
*   /*****************************************************************/ 00796000
*   /*                                                               */ 00797000
*   /* TEST TO SEE IF THE LOCATION SPECIFIED IS THE          @G24LB22*/ 00798000
*   /* EXIT STATION ADDRESS, IF NOT, PACK THE L VALUE        @G24LB22*/ 00799000
*   /*                                                               */ 00800000
*   /*****************************************************************/ 00801000
*                                                                  0437 00802000
*   IF CELLOC^=EXITSTA THEN         /* EXIT STATION          @G24LB22*/ 00803000
         CLC   CELLOC(2),EXITSTA                                   0437 00804000
         BE    @RF00437                                            0437 00805000
*     CELLZY=CELLZY|(LVAL*64);      /* NOT EXIT STATION      @G24LB22*/ 00806000
         LR    @09,LVAL                                            0438 00807000
         SLA   @09,6                                               0438 00808000
         OR    @10,@09                                             0438 00809000
         STC   @10,CELLZY                                          0438 00810000
*   RFY                                                            0439 00811000
*     REG14 RSTD;                   /* RESTRICT R14          @G24LB22*/ 00812000
@RF00437 DS    0H                                                  0440 00813000
*   REG14=RETADDR2;                 /* SAVE RET. ADDR.       @G24LB22*/ 00814000
         LR    REG14,RETADDR2                                      0440 00815000
*   RETURN;                         /* RETURN TO CALLER      @G24LB22*/ 00816000
