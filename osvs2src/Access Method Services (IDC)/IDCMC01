         L     @15,GDTDBG(,@10)                                    0281 00060000
         LA    @01,@AL00001                                        0281 00061000
         BALR  @14,@15                                             0281 00062000
*   /*****************************************************************/ 00063000
*   /*                                                               */ 00064000
*   /* THIS IS THE MAIN PROCEDURE FOR MODIFYC FSR            @G24LB22*/ 00065000
*   /* IT CALLS FUNCTION PROCEDURES TO ASSIST IN BUILDING THE ICBMODC*/ 00066000
*   /* REQUEST BLOCK AND TO TERMINATE PROCESSING. THE PROCEDURES     */ 00067000
*   /* CALLED BY IDCMC01 ARE SETUPVOL, SETUPXYZ, SETUPDEJ, AND       */ 00068000
*   /* SETUPCHG.                                             @G24LB22*/ 00069000
*   /* INITIALIZE POINTERS AND SET THE PARAMETER LIST TO     @G24LB22*/ 00070000
*   /* ZERO. PRINTAB IS USED FOR THE PRINT PARM LIST AND AND PARMLIST*/ 00071000
*   /* IS THE PARAMETER LIST PASSED TO THE USSC MACRO TO DO THE      */ 00072000
*   /* MODIFYC FUNCTION.                                     @G24LB22*/ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*                                                                  0282 00076000
*   DDSTRU=ADDR(PRINTAB);           /*                       @G24LB22*/ 00077000
@RT00279 LA    @10,PRINTAB                                         0282 00078000
         ST    @10,DDSTRU                                          0282 00079000
*   MODCPTR=ADDR(PARMLIST);         /*                       @G24LB22*/ 00080000
         LA    @10,PARMLIST                                        0283 00081000
         ST    @10,MODCPTR                                         0283 00082000
*   PARMLIST=''B;                   /*                       @G24LB22*/ 00083000
         XC    PARMLIST(52),PARMLIST                               0284 00084000
*   PRINTAB=''B;                    /*                       @G24LB22*/ 00085000
