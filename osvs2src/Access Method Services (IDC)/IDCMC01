         L     @15,GDTTR2(,@10)                                    0277 00042000
         MVC   @TS00001(95),NEWERID2(@15)                          0277 00043000
         MVC   OLDERID2(95,@15),@TS00001                           0277 00044000
*   NEWID2='MCAA';                  /* ENTRY TO PROGRAM      @G24LB22*/ 00045000
         MVC   NEWID2(4,@15),@CC01269                              0278 00046000
*   IF GDTDBG=NULLPTR THEN                                         0279 00047000
         L     @10,GDTDBG(,@10)                                    0279 00048000
         LTR   @10,@10                                             0279 00049000
         BZ    @RT00279                                            0279 00050000
*     ;                                                            0280 00051000
*   ELSE                                                           0281 00052000
*     CALL IDCDB010(GDTTBL,'MCAA'); /* DUMP POINT            @G24LB22*/ 00053000
*                                                                  0281 00054000
