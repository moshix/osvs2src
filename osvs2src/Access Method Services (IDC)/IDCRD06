         LA    @01,@AL00001                                        0760 03976000
         BALR  @14,@15                                             0760 03977000
*   END IDCRDER;                                                   0761 03978000
@EL00003 DS    0H                                                  0761 03979000
@EF00003 DS    0H                                                  0761 03980000
@ER00003 LM    @14,@12,@SA00003                                    0761 03981000
         BR    @14                                                 0761 03982000
*IDCRDIO:                                                          0762 03983000
*   PROCEDURE;                                                     0762 03984000
*                                                                  0762 03985000
IDCRDIO  STM   @14,@03,@SA00004                                    0762 03986000
         STM   @05,@12,@SA00004+24                                 0762 03987000
*/********************************************************************/ 03988000
*/*                                                                  */ 03989000
*/* PROCEDURE NAME - IDCRDIO                                         */ 03990000
*/*                                                                  */ 03991000
*/* FUNCTION - THIS PROCEDURE PERFORMS ALL OF THE I/O TO THE         */ 03992000
*/*            REPAIRV WORKFILE                                      */ 03993000
*/*                                                                  */ 03994000
*/* ENTRY POINT - IDCRDIO (ONLY ENTRY POINT FOR THIS PROCEDURE)      */ 03995000
*/*                                                                  */ 03996000
*/*           LINKAGE - '?UPDRCD;'                                   */ 03997000
*/*                     '?POSCRD (DS (IN | OUT);'                    */ 03998000
*/*                     '?ADDRCD;'                                   */ 03999000
*/*                     '?LOADRCD;'                                  */ 04000000
*/*                     '?GETRCD DS (IN | OUT) FOR (NUP | UPD);'     */ 04001000
*/*                                                                  */ 04002000
*/*           INPUT -                                                */ 04003000
*/*                                                                  */ 04004000
*/*             WFIODAD - ADDRESS OF DATA BUFFER                     */ 04005000
*/*             WFIODLN - LENGTH OF DATA                             */ 04006000
*/*             WFIOKYA - ADDRESS OF KEY AREA                        */ 04007000
*/*                                                                  */ 04008000
*/*           OUTPUT -                                               */ 04009000
*/*                                                                  */ 04010000
*/*             IDCRDIO CALLS ONE OF THE FOLLOWING SUBROUTINES       */ 04011000
*/*             TO PERFORM I/O ON THE WORKFILE:                      */ 04012000
*/*                                                                  */ 04013000
*/*                IDCRDOA - TO ADD A RECORD                         */ 04014000
*/*                IDCRDOG - TO GET A RECORD                         */ 04015000
*/*                IDCRDOU - TO UPDATE A RECORD                      */ 04016000
*/*                IDCRDOP - TO POSITION TO A RECORD                 */ 04017000
*/*                                                                  */ 04018000
*/*           EXIT, NORMAL -                                         */ 04019000
*/*                                                                  */ 04020000
*/*             LASTCC = 00        FUNCTION COMPLETED NORMALLY       */ 04021000
*/*                                                                  */ 04022000
*/*           EXIT, ERROR -                                          */ 04023000
*/*                                                                  */ 04024000
*/*             LASTCC = 04        I/O ERROR                         */ 04025000
*/*                                                                  */ 04026000
*/*             LASTCC = 08        RECORD NOT FOUND                  */ 04027000
*/*                                                                  */ 04028000
*/*             LASTCC = 16        END OF FILE                       */ 04029000
*/*                                                                  */ 04030000
*/* INTERNAL INTERFACES -                                            */ 04031000
*/*                                                                  */ 04032000
*/*      MACROS/SVCS                                                 */ 04033000
*/*                                                                  */ 04034000
*/*           ?CALLCKER - REPAIRV MESSAGE WRITER ROUTINE             */ 04035000
*/*              WRITES ALL MESSAGES                                 */ 04036000
*/*              GENERATES A CALL TO PROCEDURE IDCRDER               */ 04037000
*/*                                                                  */ 04038000
*/*      PROCEDURES/SUBROUTINES CALLED                               */ 04039000
*/*                                                                  */ 04040000
*/*           IDCRDOU (?UPDRCD) - TO UPDATE A RECORD IN THE REPAIR   */ 04041000
*/*                   WORKFILE                                       */ 04042000
*/*                                                                  */ 04043000
*/*           IDCRDOP (?POSRCD) - TO POSITION TO A RECORD IN THE     */ 04044000
*/*                   REPAIR WORKFILE                                */ 04045000
*/*                                                                  */ 04046000
*/*           IDCRDOA (?ADDRCD | ?LOADRCD) - TO ADD OR WRITE A       */ 04047000
*/*                   RECORD TO THE REPAIR WORKFILE                  */ 04048000
*/*                                                                  */ 04049000
*/*           IDCRDOG (?GETRCD) - TO READ A RECORD FROM THE REPAIR   */ 04050000
*/*                   WORKFILE                                       */ 04051000
*/*                                                                  */ 04052000
*/*      DATA AREAS                                                  */ 04053000
*/*                                                                  */ 04054000
*/*           NONE                                                   */ 04055000
*/*                                                                  */ 04056000
*/* EXTERNAL INTERFACES -                                            */ 04057000
*/*                                                                  */ 04058000
*/*      MACROS/SVCS                                                 */ 04059000
*/*                                                                  */ 04060000
*/*           UDUMP - IDCAMS DUMP POINT ROUTINE                      */ 04061000
*/*              PRINTS A DUMP AT THE SPECIFIED DUMP POINT           */ 04062000
*/*              GENERATES A CALL TO MODULE IDCDB01                  */ 04063000
*/*                                                                  */ 04064000
*/*           UPOSIT - IDCAMS I/O ADAPTER POSITION RECORD ROUTINE    */ 04065000
*/*              POSITIONS TO THE REQUESTED RECORD IN REPAIR WORKFILE*/ 04066000
*/*              GENERATES A CALL TO MODULE IDCIOPO                  */ 04067000
*/*                                                                  */ 04068000
*/*           UGET - IDCAMS I/O ADAPTER GET ROUTINE                  */ 04069000
*/*              READS RECORDS FROM THE REPAIR WORKFILE              */ 04070000
*/*              GENERATES A CALL TO MODULE IDCIOGT                  */ 04071000
*/*                                                                  */ 04072000
*/*           UPUT - IDCAMS I/O ADAPTER PUT ROUTINE                  */ 04073000
*/*              WRITES UPDATE REQUESTS TO THE REPAIR WORKFILE       */ 04074000
*/*              GENERATES A CALL TO MODULE IDCIOPT                  */ 04075000
*/*                                                                  */ 04076000
*/*           UTRACE - IDCAMS TRACE POINT ROUTINE                    */ 04077000
*/*              SETS TRACE POINT                                    */ 04078000
*/*                                                                  */ 04079000
*/*      PROCEDURES/SUBROUTINES CALLED -                             */ 04080000
*/*                                                                  */ 04081000
*/*           NONE                                                   */ 04082000
*/*                                                                  */ 04083000
*/*      DATA AREAS -                                                */ 04084000
*/*                                                                  */ 04085000
*/*           REPAIR WORKFILE DATA BUFFER                            */ 04086000
*/*                                                                  */ 04087000
*/********************************************************************/ 04088000
*                                                                  0763 04089000
*                                                                  0763 04090000
*   /*****************************************************************/ 04091000
*   /*                                                               */ 04092000
