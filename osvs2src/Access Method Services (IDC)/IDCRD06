         MVC   OLDERID2(95,@03),@TS00001                           1597 08668000
*   NEWID2='RD62';                                                 1598 08669000
*                                                                  1598 08670000
         MVC   NEWID2(4,@03),@CC03460                              1598 08671000
*   /*****************************************************************/ 08672000
*   /*                                                               */ 08673000
*   /* ISSUE UEPIL TO FREE STORAGE AND RETURN TO IDCRD01 AFTER       */ 08674000
*   /* ESTABLISHING LINKAGE TO THE RD06 SAVE AREA.           @ZA27486*/ 08675000
*   /*                                                               */ 08676000
*   /*****************************************************************/ 08677000
*                                                                  1599 08678000
*   RESPECIFY                                                      1599 08679000
*    (REG8,                                                        1599 08680000
*     REG13) RESTRICTED;            /*                       @ZA27486*/ 08681000
*   FORCHAIN=REG8;                  /*                       @ZA27486*/ 08682000
         ST    REG8,FORCHAIN(,REG13)                               1600 08683000
*   SAVER13=REG13;                  /*                       @ZA27486*/ 08684000
         LR    SAVER13,REG13                                       1601 08685000
*   REG13=REG8;                     /*                       @ZA27486*/ 08686000
         LR    REG13,REG8                                          1602 08687000
*   BACKCHAN=SAVER13;               /*                       @ZA27486*/ 08688000
