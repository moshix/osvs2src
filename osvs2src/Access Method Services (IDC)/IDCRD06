@RT00443 L     @06,@PC00001                                        0446 01573000
         L     @06,GDTTR2(,@06)                                    0446 01574000
         MVC   @TS00001(95),NEWERID2(@06)                          0446 01575000
         MVC   OLDERID2(95,@06),@TS00001                           0446 01576000
*   NEWID2='RD64';                                                 0447 01577000
         MVC   NEWID2(4,@06),@CC02352                              0447 01578000
*   IF SRVRECL^=NULLPTR                                            0448 01579000
*                                                                  0448 01580000
*/********************************************************************/ 01581000
*/*                                                                  */ 01582000
*/* THE FOLLOWING MATRIX INDICATES THE FIELDS THAT MUST BE           */ 01583000
*/* FILLED IN IN THE PRINT ARGUMENT LIST SRPRTARG WHEN               */ 01584000
*/* ROUTINE IDCRDPT IS CALLED                                        */ 01585000
*/*                                                                  */ 01586000
*/* FIELD        FORMAT1 FORMAT2 FORMAT3 FORMAT4 FORMAT5 FORMAT6     */ 01587000
*/*                                                                  */ 01588000
*/* SRVRECL      YES     NO      NO      YES     NO      NO          */ 01589000
*/* SRVREPTR     YES     NO      NO      YES     NO      NO          */ 01590000
*/* SRDSNHDR     YES     NO      NO      YES     NO      NO          */ 01591000
*/* SRVOLPTR     YES     YES     YES     YES     YES     YES         */ 01592000
*/* SRRWFDSN     YES     NO      NO      YES     NO      NO          */ 01593000
*/* SRRWFKEY     YES     NO      NO      YES     NO      NO          */ 01594000
*/* SRERCODE     NO      YES     YES     YES     YES     YES         */ 01595000
*/* SRDSNPTR     NO      YES     NO      NO      YES     NO          */ 01596000
*/* SRSPRECL     NO      NO      NO      NO      NO      YES         */ 01597000
*/* SRRECPTR     NO      YES     NO      NO      YES     YES         */ 01598000
*/* SRCCHHRP     NO      NO      YES     NO      NO      YES         */ 01599000
*/*                                                                  */ 01600000
*/********       IDONLY  IDONLY  IDONLY  FULLREC FULLREC  FULLREC    */ 01601000
*/********       REPWRKF DATASET STGPACK REPWRKF DATASET  STGPACK    */ 01602000
*/********************************************************************/ 01603000
*                                                                  0448 01604000
*     THEN                                                         0448 01605000
         SLR   @06,@06                                             0448 01606000
         L     @15,@PC00001+8                                      0448 01607000
         L     @01,REPSPRIT(,@15)                                  0448 01608000
         C     @06,SRVRECL(,@01)                                   0448 01609000
         BE    @RF00448                                            0448 01610000
*     DO;                                                          0449 01611000
*       IF ADDR(IDONL)^=NULLPTR THEN                               0450 01612000
         L     @15,@PC00001+4                                      0450 01613000
         C     @06,FDTPTR+40(,@15)                                 0450 01614000
         BE    @RF00450                                            0450 01615000
*         SRFINDEX=SRFMT1;                                         0451 01616000
         LA    SRFINDEX,1                                          0451 01617000
*       ELSE                                                       0452 01618000
*         SRFINDEX=SRFMT4;                                         0452 01619000
         B     @RC00450                                            0452 01620000
@RF00450 LA    SRFINDEX,4                                          0452 01621000
*     END;                                                         0453 01622000
*   ELSE                                                           0454 01623000
*     DO;                                                          0454 01624000
         B     @RC00448                                            0454 01625000
@RF00448 DS    0H                                                  0455 01626000
*       IF SRDSNPTR^=NULLPTR THEN                                  0455 01627000
         SLR   @06,@06                                             0455 01628000
