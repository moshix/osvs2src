*   END IDCRDDC;                                                   1236 06955000
*                                                                  1237 06956000
*/********************************************************************/ 06957000
*/*                                                                  */ 06958000
*/*   PROCEDURE NAME - IDCRDRF                                       */ 06959000
*/*                                                                  */ 06960000
*/*   DESCRIPTION - THIS SERVICE ROUTINE IS USED TO OPEN THE         */ 06961000
*/*            REPAIR WORKFILE AND TO ENSURE ITS VALIDITY            */ 06962000
*/*            BY CHECKING THE HEADER RECORD                         */ 06963000
*/*                                                                  */ 06964000
*/*            IT GETS CORE FOR TWO NON HEADER RECORDS               */ 06965000
*/*            EACH ONE EQUAL TO THE MAXIMUM BLOCKSIZE OF            */ 06966000
*/*            THE REPAIR WORKFILE + OVERHEAD AREA                   */ 06967000
*/*                                                                  */ 06968000
*/*            IT OBTAINS TWO DATA BUFFER AREAS OF SIZE              */ 06969000
*/*            IOCTRN + 16                                           */ 06970000
*/*                                                                  */ 06971000
*/*            IT CHECKS THE VALIDITY OF THE CCHH(R) PARAMETERS      */ 06972000
*/*                                                                  */ 06973000
*/*   MACROS ISSUED - UOPEN - TO OPEN THE REPAIR WORKFILE            */ 06974000
*/*                   UGPOOL - TO GET BUFFER SPACE                   */ 06975000
*/*                   UCLOSE - TO CLOSE THE REPAIR WORKFILE          */ 06976000
*/*                   UIOINFO - TO GET THE DATA SET NAME             */ 06977000
*/*                   ?GETRCD - TO READ THE HEADER RECORD            */ 06978000
*/*                   ?CALLCKER - TO PRINT A MESSAGE                 */ 06979000
*/*                                                                  */ 06980000
*/*                                                                  */ 06981000
*/*   MESSAGES -                                                     */ 06982000
*/*       IDC31220I - INDICATING THE REPAIR WORKFILE HEADER          */ 06983000
*/*                   WAS NOT FOUND OR NOT VALID                     */ 06984000
*/*                                                                  */ 06985000
*/*       IDC31233I - INDICATING THE CCHH OR CCHHR WAS NOT VALID     */ 06986000
*/*                                                                  */ 06987000
*/*                                                                  */ 06988000
*/*                                                                  */ 06989000
*/*   INPUT - A BYTE INDICATING I TO OPEN FOR INPUT OR               */ 06990000
*/*               O TO OPEN FOR OUTPUT                               */ 06991000
*/*                                                                  */ 06992000
*/*   OUTPUT - REPBUFF0, REPBUFF1, REPR0LN, REPRILN                  */ 06993000
*/*            RWF HEADER IN REPRWHDI (FOR INPUT) OR                 */ 06994000
*/*                     REPRWHDO (FOR OUTPUT)                        */ 06995000
*/*                                                                  */ 06996000
*/********************************************************************/ 06997000
*                                                                  1237 06998000
*IDCRDRF:                                                          1237 06999000
*   PROC(RFIO);                                                    1237 07000000
*                                                                  1237 07001000
IDCRDRF  STM   @14,@12,@SA00010                                    1237 07002000
         MVC   @PC00010(4),0(@01)                                  1237 07003000
*   /*****************************************************************/ 07004000
*   /*                                                               */ 07005000
*   /* THE FOLLOWING FIELDS ARE USED BY IDCRDRF                      */ 07006000
*   /*                                                               */ 07007000
*   /*****************************************************************/ 07008000
*                                                                  1238 07009000
*   DCL                                                            1238 07010000
*     SAVCLOSE PTR(31);             /* COMMOM PTR FOR CLOSE          */ 07011000
*   DCL                                                            1239 07012000
*     RFIO CHAR(1);                 /* INPUT/OUTPUT INDICATOR        */ 07013000
*   DCL                                                            1240 07014000
*     RFDD CHAR(8);                 /* DDNAME AREA                   */ 07015000
*   DCL                                                            1241 07016000
*     RFDSN CHAR(44);               /* DATA SET NAME AREA            */ 07017000
*   DCL                                                            1242 07018000
*     RFPASS CHAR(8);               /* PASS WORD AREA                */ 07019000
*   DCL                                                            1243 07020000
*     MAXSIZ FIXED(31);             /* MAXIMUM SIZE OF BUFFERS       */ 07021000
*   DCL                                                            1244 07022000
*     REPKEY0 PTR(31);              /* BUFFER AREA PTR               */ 07023000
*   DCL                                                            1245 07024000
*     RWFNAM CHAR(44);              /* AREA FOR UINFO DATA SET NAME  */ 07025000
*   DCL                                                            1246 07026000
*     MES33PTR PTR(31);             /* COMMON PRT FOR MESSAGE 33     */ 07027000
*   DCL                                                            1247 07028000
*     MES33LEN FIXED(8);            /* COMMON LENGTH FOR MESSAGE 33  */ 07029000
*   DCL                                                            1248 07030000
*     RWFPTR1 PTR(31);              /* TO GET DANAME                 */ 07031000
*                                                                  1248 07032000
*   /*****************************************************************/ 07033000
*   /*                                                               */ 07034000
*   /* THE FOLLOWING STRUCTURE IS USED BY IDCRDRF TO DEFINE THE      */ 07035000
*   /* HEADER RECORD THAT IS READ IN BY GETRCD                       */ 07036000
*   /*                                                               */ 07037000
*   /*****************************************************************/ 07038000
*                                                                  1249 07039000
*   DCL                                                            1249 07040000
*     1 RCDHEAD BASED(WFIODAD),                                    1249 07041000
*      2 RCDKEY CHAR(11),           /* REPAIR WF KEY AREA            */ 07042000
*      2 RCKRWNM CHAR(15),          /* REPAIR WF DS NAME             */ 07043000
*      2 * CHAR(114);               /* REST OF HEADER RECORD         */ 07044000
*                                                                  1249 07045000
*   /*****************************************************************/ 07046000
*   /*                                                               */ 07047000
*   /* THE FOLLOWING AREAS ARE BASED                                 */ 07048000
*   /*                                                               */ 07049000
*   /*****************************************************************/ 07050000
*                                                                  1250 07051000
*   DCL                                                            1250 07052000
*     RCDAREA CHAR(140) BASED;      /* AREA FOR HEADER RECORD        */ 07053000
*   DCL                                                            1251 07054000
*     RWFDANA CHAR(44) BASED(RWFPTR1);/* AREA FOR DATA SET NAME      */ 07055000
*                                                                  1251 07056000
*   /*****************************************************************/ 07057000
*   /*                                                               */ 07058000
*   /* THE FOLLOWING ARE LITERALS AND CONSTANTS USED BY IDCRDRF      */ 07059000
*   /*                                                               */ 07060000
*   /*****************************************************************/ 07061000
*                                                                  1252 07062000
*   DCL                                                            1252 07063000
*     HEADRNM CHAR(15) INIT('REPAIRVWORKFILE');/* REPAIRWORKFILE   1252 07064000
*                                      LITERAL                       */ 07065000
*   DCL                                                            1253 07066000
*     RD01 CHAR(8) INIT('RD01');    /* SUBPOOL ID                    */ 07067000
*   DCL                                                            1254 07068000
*     BLANK8 CHAR(8) INIT('        ');/* FIELD WITH 8 BLANKS         */ 07069000
*   DCL                                                            1255 07070000
*     INPUTIND CHAR(1) INIT('I');   /* TO INDICATE INPUT             */ 07071000
*   DCL                                                            1256 07072000
*     MSG20 FIXED(8) CONSTANT(20);  /* CONSTANT FOR MSG IDC31220I    */ 07073000
*   DCL                                                            1257 07074000
*     MSG33 FIXED(8) CONSTANT(33);  /* CONSTANT FOR MSG IDC31233I    */ 07075000
*   DCL                                                            1258 07076000
*     REPRNG CHAR(17) INIT('REPAIRRANGE');/* LITERAL FOR MSG       1258 07077000
*                                      IDC31233I                     */ 07078000
*   DCL                                                            1259 07079000
*     RANG CHAR(17) INIT('RANGE');  /* LITERAL FOR MSG IDC31233I     */ 07080000
*   DCL                                                            1260 07081000
*     OUTREPK CHAR(17) INIT('OUTREPAIRKEY');/* LITERAL FOR MSG     1260 07082000
*                                      IDC31233I                     */ 07083000
*   DCL                                                            1261 07084000
*     INREPK CHAR(17) INIT('INREPAIRKEY');/* LITERAL FOR MSG       1261 07085000
*                                      IDC31233I                     */ 07086000
*   DCL                                                            1262 07087000
*     INREPRNG CHAR(17) INIT('INREPAIRRANGE');/* LITERAL FOR MSG   1262 07088000
*                                      IDC31233I                     */ 07089000
*   DCL                                                            1263 07090000
*     KEYLEN FIXED(8) CONSTANT(16); /* RWF KEYLENGTH                 */ 07091000
*                                                                  1263 07092000
*   /*****************************************************************/ 07093000
*   /*                                                               */ 07094000
*   /* THESE FIELDS ARE FOR THE UIOINFO MACRO                        */ 07095000
*   /*                                                               */ 07096000
*   /*****************************************************************/ 07097000
*                                                                  1264 07098000
*   DCL                                                            1264 07099000
*     INFOBYT CHAR(1);              /* FOR UIOINFO                   */ 07100000
*   DCL                                                            1265 07101000
*     INFOPTR PTR(31);              /* FOR UIOINFO                   */ 07102000
*   DCL                                                            1266 07103000
*     INFODD CHAR(8);               /* DDNAME FOR UIOINFO            */ 07104000
*   DCL                                                            1267 07105000
*     INFDSNPT PTR(31);             /* DSN PTR FOR UIOINFO           */ 07106000
*   DCL                                                            1268 07107000
*     DSNIDENT FIXED(16) CONSTANT(1);/* DSN IDENTF FOR UIOINFO       */ 07108000
*                                                                  1268 07109000
*   /*****************************************************************/ 07110000
*   /*                                                               */ 07111000
*   /* THIS STRUCTURE DEFINES A FIELD OF 44 BLANKS                   */ 07112000
*   /*                                                               */ 07113000
*   /*****************************************************************/ 07114000
*                                                                  1269 07115000
*   DCL                                                            1269 07116000
*     1 BLANKS STATIC LOCAL,                                       1269 07117000
*      2 *(44) CHAR(1) INIT((44)'40'X);                            1269 07118000
*                                                                  1269 07119000
*   /*****************************************************************/ 07120000
*   /*                                                               */ 07121000
*   /* THIS STRUCTURE USED TO DEFINE THE REPAIR WORKFILE KEY         */ 07122000
*   /*                                                               */ 07123000
