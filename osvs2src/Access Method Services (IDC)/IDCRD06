@ER00008 LM    @14,@03,@SA00008                                    0902 04874000
         LM    @05,@12,@SA00008+24                                 0902 04875000
         BR    @14                                                 0902 04876000
*   END IDCRDIO;                                                   0903 04877000
*IDCRDDC:                                                          0904 04878000
*   PROCEDURE;                                                     0904 04879000
*                                                                  0904 04880000
IDCRDDC  STM   @14,@12,@SA00009                                    0904 04881000
*/********************************************************************/ 04882000
*/*                                                                  */ 04883000
*/*  PROCEDURE NAME - IDCRDDC                                        */ 04884000
*/*                                                                  */ 04885000
*/*  FUNCTION - THIS PROCEDURE IS CALLED BY MODULE IDCRD02 TO READ   */ 04886000
*/*             RECORDS FROM THE FIRST TRACK OF A VTOC (VTOCHEADER), */ 04887000
*/*             OR VTOC, OR DATA SET, OR STAGING PACK OFFLINE TO THE */ 04888000
*/*             MSS AS SPECIFIED BY THE EXTENT TABLE ENTRY, AND TO   */ 04889000
*/*             DIRECT THEM TO EITHER SYSPRINT OR AN ALTERNATE       */ 04890000
*/*             REPORT FILE.                                         */ 04891000
*/*             THIS PROCEDURE IS CALLED BY MODULE IDCRD03 TO READ   */ 04892000
*/*             RECORDS FROM THE FIRST TRACK OF A VTOC (VTOCHEADER), */ 04893000
*/*             OR VTOC, OR DATA SET, OR STAGING PACK OFFLINE TO THE */ 04894000
*/*             MSS AS SPECIFIED BY THE EXTENT TABLE ENTRY, AND TO   */ 04895000
*/*             DIRECT THEM TO A REPAIR WORKFILE.                    */ 04896000
*/*                                                                  */ 04897000
*/*  ENTRY POINT - IDCRDDC (ONLY ENTRY POINT FOR THIS PROCEDURE)     */ 04898000
*/*                                                                  */ 04899000
*/*             LINKAGE - 'CALL IDCRDDC'                             */ 04900000
*/*                                                                  */ 04901000
*/*             INPUT-                                               */ 04902000
*/*                                                                  */ 04903000
*/*               VTOC, OR                                           */ 04904000
*/*               VTOCHEADER, OR                                     */ 04905000
*/*               DATA SET, OR                                       */ 04906000
*/*               STAGING PACK OFFLINE TO THE MSS                    */ 04907000
*/*                                                                  */ 04908000
*/*             OUTPUT -                                             */ 04909000
*/*                                                                  */ 04910000
*/*               ERROR MESSAGE IDC01215I                            */ 04911000
*/*                                                                  */ 04912000
*/*             EXIT NORMAL -                                        */ 04913000
*/*                                                                  */ 04914000
*/*               LASTCC = 00      FUNCTION COMPLETED NORMALLY       */ 04915000
*/*                                                                  */ 04916000
*/*             EXIT ERROR -                                         */ 04917000
*/*                                                                  */ 04918000
*/*               LASTCC = 12      FUNCTION DID NOT COMPLETE         */ 04919000
*/*                                                                  */ 04920000
*/*  INTERNAL INTERFACES -                                           */ 04921000
*/*                                                                  */ 04922000
*/*       MACROS/SVCS                                                */ 04923000
*/*                                                                  */ 04924000
*/*            ?CALLCKER - REPAIRV MESSAGE WRITER ROUTINE            */ 04925000
*/*              WRITES ALL MESSAGES                                 */ 04926000
*/*              GENERATES A CALL TO MODULE IDCRDER                  */ 04927000
*/*                                                                  */ 04928000
*/*            ?LOADRCD - REPAIRV REPAIR WORKFILE WRITE ROUTINE      */ 04929000
*/*              WRITES INPUT RECORDS TO THE REPAIR WORKFILE         */ 04930000
*/*              GENERATES A CALL TO MODULE IDCRDIO                  */ 04931000
*/*                                                                  */ 04932000
*/*       PROCEDURES/SUBROUTINES CALLED                              */ 04933000
*/*                                                                  */ 04934000
*/*            IDCRDPT - TO WRITE THE INPUT RECORDS TO EITHER        */ 04935000
*/*              SYSPRINT OR AN ALTERNATE REPORT FILE USING THE      */ 04936000
*/*              UPRINT MACRO INTERFACE                              */ 04937000
*/*                                                                  */ 04938000
*/*       DATA AREAS                                                 */ 04939000
*/*                                                                  */ 04940000
*/*            NONE                                                  */ 04941000
*/*                                                                  */ 04942000
*/*  EXTERNAL INTERFACES -                                           */ 04943000
*/*                                                                  */ 04944000
*/*       MACROS/SVCS-                                               */ 04945000
*/*                                                                  */ 04946000
*/*            UDUMP - IDCAMS DUMP POINT ROUTINE                     */ 04947000
*/*              PRINTS A DUMP AT THE SPECIFIED DUMP POINT           */ 04948000
*/*              GENERATES A CALL TO MODULE IDCDB01                  */ 04949000
*/*                                                                  */ 04950000
*/*            UTRACE - IDCAMS TRACE POINT ROUTINE                   */ 04951000
*/*              SETS TRACE POINT                                    */ 04952000
*/*                                                                  */ 04953000
*/*       PROCEDURES/SUBROUTINES CALLED                              */ 04954000
*/*                                                                  */ 04955000
*/*            IDCRDRD - TO READ INPUT RECORDS USING THE UEXCP       */ 04956000
*/*              MACRO INTERFACE                                     */ 04957000
*/*                                                                  */ 04958000
*/*       DATA AREAS-                                                */ 04959000
*/*                                                                  */ 04960000
*/*            RDARG - IDCAMS I/O ADAPTER READ ARGUMENT LIST         */ 04961000
*/*              COMMUNICATES THE PARAMETERS FOR READING THE         */ 04962000
*/*              INPUT RECORD                                        */ 04963000
*/*                                                                  */ 04964000
*/*            DEVAREA - DEVTYPE SVC RETURN AREA                     */ 04965000
*/*              COMMUNICATES THE CHARACTERISTICS OF THE INPUT       */ 04966000
*/*              DEVICE                                              */ 04967000
*/*                                                                  */ 04968000
*/*            RBUF - IDCAMS I/O ADAPTER INPUT BUFFER                */ 04969000
*/*              CONTAINS THE REQUESTED INPUT RECORD                 */ 04970000
*/*                                                                  */ 04971000
*/*            XTNTTBL - REPAIRV EXTENT TABLE                        */ 04972000
*/*              CONTAINS USER SUPPLIED INFORMATION VIA SYNTAX IN    */ 04973000
*/*              THE HEADER. CONTAINS EXTENT INFORMATION FOR EACH    */ 04974000
*/*              EXTENT WITHIN THE USER SUPPLIES RANGE.              */ 04975000
*/*                                                                  */ 04976000
*/********************************************************************/ 04977000
