*   /*                                                               */ 05181000
*   /*****************************************************************/ 05182000
*                                                                  0924 05183000
*   IF GDTDBG=NULLPTR THEN                                         0924 05184000
         L     @06,@PC00001                                        0924 05185000
         L     @06,GDTDBG(,@06)                                    0924 05186000
