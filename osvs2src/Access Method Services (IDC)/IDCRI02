         ST    @15,@AL00001+4                                      0207 00179000
         LA    @15,WORKPTR(,@10)                                   0207 00180000
         ST    @15,@AL00001+8                                      0207 00181000
         LA    @10,POOLID(,@10)                                    0207 00182000
         ST    @10,@AL00001+12                                     0207 00183000
         LA    @10,SETZERO                                         0207 00184000
         ST    @10,@AL00001+16                                     0207 00185000
         MVI   @AL00001+16,X'80'                                   0207 00186000
         L     @15,GDTGPL(,@05)                                    0207 00187000
         LA    @01,@AL00001                                        0207 00188000
         BALR  @14,@15                                             0207 00189000
*                  RESPECIFY RTNREG RSTD;                          0208 00190000
*                  TESTRC = RTNREG;                                0209 00191000
         LR    TESTRC,RTNREG                                       0209 00192000
*                  RESPECIFY RTNREG UNRSTD;                        0210 00193000
*                                                                  0210 00194000
*                  END;                                            0211 00195000
