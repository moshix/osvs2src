*                  TESTRC=0;                                       0212 00197000
         B     @RC00204                                            0212 00198000
@RF00204 SLR   TESTRC,TESTRC                                       0212 00199000
*              IF TESTRC^=0                                        0213 00200000
*                THEN                                              0213 00201000
@RC00204 LTR   TESTRC,TESTRC                                       0213 00202000
         BZ    @RF00213                                            0213 00203000
*                  RISTATUS=NOSPACE;                               0214 00204000
         L     @10,@PC00001+4                                      0214 00205000
