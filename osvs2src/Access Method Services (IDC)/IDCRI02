*                    NEWID2='RI17';                                0217 00217000
*                                                                  0217 00218000
         MVC   NEWID2(4,@10),@CC00750                              0217 00219000
*                    /************************************************/ 00220000
*                    /*  SET UP THE PARMINFO TABLE THAT IS   @Y30SSFR*/ 00221000
*                    /*  USED BY RI01 TO PARSE THE COMMAND.  @Y30SSFR*/ 00222000
*                    /*  ADJUST SETUPPTR TO POINT TO THE     @Y30SSFR*/ 00223000
*                    /*  PARAMETER DATA AREA OF THE COMMAND  @Y30SSFR*/ 00224000
*                    /*  DESCRIPTOR.  NOTE THAT THE READER   @Y30SSFR*/ 00225000
*                    /*  INTERPRETER USES THE CONVENTION     @Y30SSFR*/ 00226000
*                    /*  THAT LENGTHS ARE STORED IN NUMBERS  @Y30SSFR*/ 00227000
*                    /*  OF HALF WORDS.  HENCE, THE LENGTH   @Y30SSFR*/ 00228000
*                    /*  OF THE VERB DATA AREA MUST BE       @Y30SSFR*/ 00229000
*                    /*  DOUBLED PRIOR TO ADDING IT TO       @Y30SSFR*/ 00230000
*                    /*  DESCPTR.                            @Y30SSFR*/ 00231000
*                    /*                                      @Y30SSFR*/ 00232000
*                    /************************************************/ 00233000
*                                                                  0218 00234000
*                    SETUPPTR=DESCPTR+2*VDATALEN;                  0218 00235000
         L     @10,@PC00001+4                                      0218 00236000
         L     @10,DESCPTR(,@10)                                   0218 00237000
         LH    SETUPPTR,VDATALEN(,@10)                             0218 00238000
         ALR   SETUPPTR,SETUPPTR                                   0218 00239000
         ALR   @10,SETUPPTR                                        0218 00240000
         LR    SETUPPTR,@10                                        0218 00241000
*                    RESPECIFY(PARMDATA) BASED(SETUPPTR);          0219 00242000
*                                                                  0219 00243000
*                    /************************************************/ 00244000
*                    /*                                      @Y30SSFR*/ 00245000
*                    /*  LOOP THROUGH THE PARAMETERS FOR     @Y30SSFR*/ 00246000
*                    /*  THE COMMAND, SETTING UP POINTERS    @Y30SSFR*/ 00247000
*                    /*  IN THE PARMINFO TABLE.              @Y30SSFR*/ 00248000
*                    /*                                      @Y30SSFR*/ 00249000
*                    /************************************************/ 00250000
*                                                                  0220 00251000
*                    DO INDEX=1 TO PARMCNT;                        0220 00252000
         LA    INDEX,1                                             0220 00253000
         B     @DE00220                                            0220 00254000
@DL00220 DS    0H                                                  0221 00255000
*                      TEMPPTR=SETUPPTR+2*IDDSPL;                  0221 00256000
         SLR   TEMPPTR,TEMPPTR                                     0221 00257000
         IC    TEMPPTR,IDDSPL(,SETUPPTR)                           0221 00258000
         ALR   TEMPPTR,TEMPPTR                                     0221 00259000
         ALR   TEMPPTR,SETUPPTR                                    0221 00260000
*                      DO INDEX2=1 TO TEMPPTR->IDCOUNT;            0222 00261000
         LA    INDEX2,1                                            0222 00262000
         B     @DE00222                                            0222 00263000
@DL00222 DS    0H                                                  0223 00264000
*                        PARMPTR(TEMPPTR->IDNUM(INDEX2))=SETUPPTR; 0223 00265000
         LR    @10,INDEX2                                          0223 00266000
         SLA   @10,2                                               0223 00267000
         BCTR  @10,0                                               0223 00268000
         BCTR  @10,0                                               0223 00269000
         LH    @10,IDNUM-2(@10,TEMPPTR)                            0223 00270000
         MH    @10,@CH00565                                        0223 00271000
         L     @15,@PC00001+4                                      0223 00272000
         L     @01,WORKPTR(,@15)                                   0223 00273000
         AL    @01,@CF00766                                        0223 00274000
         ST    SETUPPTR,PARMPTR(@10,@01)                           0223 00275000
*                      END;                                        0224 00276000
         AL    INDEX2,@CF00214                                     0224 00277000
@DE00222 CH    INDEX2,IDCOUNT(,TEMPPTR)                            0224 00278000
         BNH   @DL00222                                            0224 00279000
*                      SETUPPTR=SETUPPTR+2*PDEFLEN;                0225 00280000
         SLR   @10,@10                                             0225 00281000
         IC    @10,PDEFLEN(,SETUPPTR)                              0225 00282000
         ALR   @10,@10                                             0225 00283000
         ALR   SETUPPTR,@10                                        0225 00284000
*                    END;                                          0226 00285000
         AL    INDEX,@CF00214                                      0226 00286000
@DE00220 L     @10,@PC00001+4                                      0226 00287000
         L     @10,DESCPTR(,@10)                                   0226 00288000
         CH    INDEX,PARMCNT(,@10)                                 0226 00289000
         BNH   @DL00220                                            0226 00290000
*                    RESPECIFY(PARMDATA) BASED(PARMPTR(PARMID));   0227 00291000
*                                                                  0227 00292000
*                    /************************************************/ 00293000
*                    /*                                              */ 00294000
*                    /* DETERMINE THE SUPERPARAMETER ID# OF EACH     */ 00295000
*                    /* PARAMETER AND PLACE IT IN THE TABLE.         */ 00296000
*                    /*                                              */ 00297000
*                    /************************************************/ 00298000
*                    DO PARMID=1 TO MAXID;                         0228 00299000
         LA    PARMID,1                                            0228 00300000
         B     @DE00228                                            0228 00301000
@DL00228 DS    0H                                                  0229 00302000
*                      IF PARMPTR(PARMID) ^= NULLPTR               0229 00303000
*                        THEN                                      0229 00304000
         LR    @10,PARMID                                          0229 00305000
