*                  DO;                                             0205 00169000
*                  SPACEAMT=MAXID*(LENGTH(PARMINFO));      /*@YL026UL*/ 00170000
*                                                                  0206 00171000
         MH    @15,@CH00565                                        0206 00172000
         ST    @15,SPACEAMT                                        0206 00173000
*                  CALL IDCSAGP0(GDTTBL,SPACEAMT,WORKPTR,POOLID,SETZERO 00174000
*;                                                                 0207 00175000
