         AL    @02,@CF00766                                        0229 00309000
         L     @10,PARMPTR(@10,@02)                                0229 00310000
         LTR   @10,@10                                             0229 00311000
         BZ    @RF00229                                            0229 00312000
*                          IF SUBLIST = YES                /*@Y30LSBE*/ 00313000
*                            THEN                                  0230 00314000
         TM    SUBLIST(@10),B'00000010'                            0230 00315000
         BNO   @RF00230                                            0230 00316000
*                              DO INDEX=1 TO SUBCOUNT;             0231 00317000
         LA    INDEX,1                                             0231 00318000
         B     @DE00231                                            0231 00319000
@DL00231 DS    0H                                                  0232 00320000
*                                SUPRPARM(SUBID(INDEX))=PARMID;    0232 00321000
