@RF00515 DS    0H                                                  0537 01757000
*       SSDRGLT=''B;                /*                       @Y30LB39*/ 01758000
         XC    SSDRGLT(20),SSDRGLT                                 0537 01759000
*       SSDLPTR->DARGSMOD=SSMODCM0; /*                       @Y30LB39*/ 01760000
         L     @10,SSDLPTR                                         0538 01761000
         MVC   DARGSMOD(3,@10),SSMODCM0                            0538 01762000
*       SSDLPTR->DARGSENT=SSMSGNUM; /*                       @Y30LB39*/ 01763000
