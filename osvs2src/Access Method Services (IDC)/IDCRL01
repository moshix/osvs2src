         LA    @03,@CF00112                                        0522 01654000
         ST    @03,@AL00001+4                                      0522 01655000
         LA    @03,SSDLPTR                                         0522 01656000
         ST    @03,@AL00001+8                                      0522 01657000
         MVI   @AL00001+8,X'80'                                    0522 01658000
         L     @15,GDTPRT(,@10)                                    0522 01659000
         LA    @01,@AL00001                                        0522 01660000
         BALR  @14,@15                                             0522 01661000
*       /*************************************************************/ 01662000
*       /*                                                           */ 01663000
*       /*                                                   @Y30LB39*/ 01664000
*       /* BEGIN DO LOOP TO SEARCH FOR THE MESSAGE           @Y30LB39*/ 01665000
*       /* INDEX CORRESPONDING TO THE REASON CODE            @Y30LB39*/ 01666000
*       /* PASSED AS THE FIRST ARGUMENT.                     @Y30LB39*/ 01667000
*       /* THE DO LOOP BEGINS WITH THE SECOND ARRAY          @Y30LB39*/ 01668000
*       /* ENTRY SINCE THE FIRST IS REQUIRED BY THE          @Y30LB39*/ 01669000
*       /* SEARCH ARRAY ITSELF.                              @Y30LB39*/ 01670000
*       /* THE SEARCH CONTINUES UNTIL THE REASON CODE        @Y30LB39*/ 01671000
*       /* IS FOUND, AT WHICH TIME THE SEARCH INDEX          @Y30LB39*/ 01672000
*       /* IS PASSED TO THE PRINT MODULE TO PRINT            @Y30LB39*/ 01673000
*       /* THE MESSAGE ASSOCIATED WITH THAT PARTICULAR       @Y30LB39*/ 01674000
*       /* REASON CODE.                                      @Y30LB39*/ 01675000
*       /* IF THERE IS NO MATCH FOUND IN THE ARRAY,          @Y30LB39*/ 01676000
*       /* THEN THE INDEX WILL BE THE LAST POSITION IN       @Y30LB39*/ 01677000
*       /* ARRAY. THIS WILL BE THE INDICATION THAT           @Y30LB39*/ 01678000
*       /* THE REASON CODE ITSELF SHOULD BE PRINTED.         @Y30LB39*/ 01679000
*       /*                                                   @Y30LB39*/ 01680000
*       /*                                                           */ 01681000
*       /*************************************************************/ 01682000
*                                                                  0523 01683000
*                                   /*                       @Y30LB39*/ 01684000
*       ENTRYNUM=1;                 /*                       @Y30LB39*/ 01685000
         LA    ENTRYNUM,1                                          0523 01686000
*       DO UNTIL(SSRHALF=SSARRAY(ENTRYNUM)|SSARRAY(ENTRYNUM)=0);   0524 01687000
@DL00524 DS    0H                                                  0525 01688000
*         ENTRYNUM=ENTRYNUM+1;      /*                       @Y30LB39*/ 01689000
         AL    ENTRYNUM,@CF00216                                   0525 01690000
*       END;                        /*                       @Y30LB39   01691000
*                                                            @Y30LB39*/ 01692000
*                                                                  0526 01693000
@DE00524 L     @10,@PC00004                                        0526 01694000
         LR    @03,ENTRYNUM                                        0526 01695000
         ALR   @03,@03                                             0526 01696000
         LH    @03,SSARRAY-2(@03)                                  0526 01697000
         CH    @03,SSRHALF(,@10)                                   0526 01698000
         BE    @DC00524                                            0526 01699000
         SLR   @15,@15                                             0526 01700000
         CR    @03,@15                                             0526 01701000
         BNE   @DL00524                                            0526 01702000
@DC00524 DS    0H                                                  0527 01703000
*       /*************************************************************/ 01704000
*       /*                                                           */ 01705000
*       /*                                                   @Y30LB39*/ 01706000
*       /* THE CORRECT INDEX HAS BEEN FOUND                  @Y30LB39*/ 01707000
*       /*                                                   @Y30LB39*/ 01708000
*       /* CLEAR THE DYNAMIC ARGUMENT LIST AND FILL          @Y30LB39*/ 01709000
*       /* OUT THE PRINT ARGUMENT FIELDS WITH EITHER:        @Y30LB39*/ 01710000
*       /* 1) THE INDEX CORRESPONDING TO THE REASON          @Y30LB39*/ 01711000
*       /* CODE OR                                           @Y30LB39*/ 01712000
*       /* 2) THE LAST INDEX AND THE REASON CODE             @Y30LB39*/ 01713000
*       /* ITSELF AS A STATIC TEXT INSERT.                   @Y30LB39*/ 01714000
*       /*                                                   @Y30LB39*/ 01715000
*       /*                                                           */ 01716000
*       /*************************************************************/ 01717000
*                                                                  0527 01718000
*                                   /*                       @Y30LB39*/ 01719000
*       SSDRGLT=''B;                /*                       @Y30LB39*/ 01720000
         XC    SSDRGLT(20),SSDRGLT                                 0527 01721000
*       SSDLPTR->DARGSMOD=SSMODSS0; /*                       @Y30LB39*/ 01722000
         L     @14,SSDLPTR                                         0528 01723000
         MVC   DARGSMOD(3,@14),SSMODSS0                            0528 01724000
*       SSDLPTR->DARGSENT=ENTRYNUM; /*                       @Y30LB39*/ 01725000
         STC   ENTRYNUM,DARGSENT(,@14)                             0529 01726000
*       IF SSARRAY(ENTRYNUM)=0 THEN /*                       @Y30LB39*/ 01727000
         CR    @03,@15                                             0530 01728000
         BNE   @RF00530                                            0530 01729000
*         DO;                       /*                       @Y30LB39*/ 01730000
*           SSDLPTR->DARGDBP=ADDR(SSRCODE);/*                @Y30LB39*/ 01731000
         ST    @10,DARGDBP(,@14)                                   0532 01732000
*           SSDLPTR->DARGILP=LENGTH(SSRCODE);/*              @Y30LB39*/ 01733000
         MVC   DARGILP(2,@14),@CH00039                             0533 01734000
*         END;                      /*                       @Y30LB39*/ 01735000
*     END;                          /*                       @Y30LB39*/ 01736000
*                                   /*                       @Y30LB39*/ 01737000
*                                                                  0536 01738000
*   /*****************************************************************/ 01739000
*   /*                                                               */ 01740000
*   /*                                                       @Y30LB39*/ 01741000
*   /* THE FIRST ARGUMENT WAS ZERO, INDICATING THAT          @Y30LB39*/ 01742000
*   /* THIS ROUTINE IS TO PRINT OUT THE ECB POST             @Y30LB39*/ 01743000
*   /* CODE. THEREFORE, CLEAR THE DYNAMIC ARGUMENT           @Y30LB39*/ 01744000
*   /* LIST AND FILL OUT THE PRINT ARGUMENT FIELDS           @Y30LB39*/ 01745000
*   /* SO THE STATIC TEXT MODULE, IDCTSCM0, WILL             @Y30LB39*/ 01746000
*   /* PRINT OUT THE MESSAGE ASSOCIATED WITH THE             @Y30LB39*/ 01747000
*   /* POST CODE.                                            @Y30LB39*/ 01748000
*   /*                                                       @Y30LB39*/ 01749000
*   /*                                                               */ 01750000
*   /*****************************************************************/ 01751000
*                                                                  0536 01752000
*                                   /*                       @Y30LB39*/ 01753000
*   ELSE                            /*                       @Y30LB39*/ 01754000
*     DO;                           /*                       @Y30LB39*/ 01755000
