*        RSCKTERM = 'TERM';                                        0988 03189000
*                                                                  0988 03190000
         MVC   RSCKTERM(4,REG6),@CC03625                           0988 03191000
*      /* CALL TO MODULE IDCRS05 FOR PROCEDURE CKERR                 */ 03192000
*      CALL CKERR;                                                 0989 03193000
         L     @15,@CV03449                                        0989 03194000
         BALR  @14,@15                                             0989 03195000
*      END;                                                        0990 03196000
