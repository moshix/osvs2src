*    RSFODATA = ACTLHDR;               /*SAVE OLD DATA ADDR          */ 03147000
*                                                                  0978 03148000
         ST    @14,RSFODATA(,@07)                                  0978 03149000
*    /****************************************************************/ 03150000
*    /*                                                              */ 03151000
*    /*     GET NEW STORAGE AREA                                     */ 03152000
*    /*                                                              */ 03153000
*    /****************************************************************/ 03154000
*                                                                  0979 03155000
*    RSFNLEN = CTLLEN + CTLDLN + LENGTH(CTLHDR); /*NEW LENGTH        */ 03156000
