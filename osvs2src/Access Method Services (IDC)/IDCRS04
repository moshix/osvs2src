*RFY    VOLSERTB  BASED(AVOLSRE);      /* VOLUME SERIAL TABLE        */ 00251000
*                                                                  0325 00252000
*/********************************************************************/ 00253000
*/* MODULE NAME - IDCRS04  - DELGO                                   */ 00254000
*/*                                                                  */ 00255000
*/* DECRIPTIVE NAME - DELETE A GROUP OCCURRENCE                      */ 00256000
*/*                                                                  */ 00257000
*/* FUNCTION -                                                       */ 00258000
*/*   DELETE A GROUP OCCURRENCE RETURNED BY FIND.                    */ 00259000
*/*                                                                  */ 00260000
*/*      INPUT -                                                     */ 00261000
*/*         RSWASNVT CONTAINS THE ADDRESS OF A SEQUENCE NUMBER       */ 00262000
*/*             VECTOR TABLE (SNVT) WHICH WAS RETURNED FROM FIND.    */ 00263000
*/*         RSFAENT CONTAINS THE ADDRESS OF AN ENTRY IN A SEQUENCE   */ 00264000
*/*             NUMBER TABLE (SNTAB) WHICH IDENTIFIES WHICH GROUP    */ 00265000
*/*             OCCURRENCE (GO) TO DELETE.                           */ 00266000
*/*                                                                  */ 00267000
*/*      OUTPUT -                                                    */ 00268000
*/*         ONE OR MORE WORKFILE RECORDS IS/ARE CHANGED.             */ 00269000
*/*                                                                  */ 00270000
*/********************************************************************/ 00271000
*                                                                  0326 00272000
*      GENERATE;                                                   0326 00273000
         DS    0F                                                       00274000
NAME0002 DC    CL8'DELGO'                                               00275000
*DELGO: PROCEDURE                                                  0327 00276000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0327 00277000
*                                                                  0327 00278000
         B     @PB00002                                            0327 00279000
DELGO    DS    0H                                                  0328 00280000
*      GENERATE REFS(RS04RG08, REG6);                              0328 00281000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          00282000
         LR    @15,@13  OLD SAVE AREA                                   00283000
         LA    @13,72(@13)  NEW SAVE AREA                               00284000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     00285000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        00286000
         ENTRY DELGO  ENTRY POINT                                       00287000
         LM    @08,@12,RS04RG08(REG6)  PRIME BASE AND DATA REGS         00288000
         MVC   0(4,@15),NAME0002  PROCNAME TO REGSAVE                   00289000
*    OLDERID2 = NEWERID2;                                          0329 00290000
         L     @07,GDTTR2(,REG5)                                   0329 00291000
         MVC   @TS00001(95),NEWERID2(@07)                          0329 00292000
         MVC   OLDERID2(95,@07),@TS00001                           0329 00293000
*    NEWID2='RSDE';                                                0330 00294000
         MVC   NEWID2(4,@07),@CC03622                              0330 00295000
*    /****************************************************************/ 00296000
*    /*                                                              */ 00297000
*    /*      DELGO - DELETE A GROUP OCCURRENCE INITIALIZATION        */ 00298000
*    /*                                                              */ 00299000
*    /*                                                              */ 00300000
*    /****************************************************************/ 00301000
*                                                                  0331 00302000
*    FFLAGS=''B;                       /* CLEAR FLAGS                */ 00303000
         L     @07,RSWAFINF(,REG6)                                 0331 00304000
         XC    FFLAGS(4,@07),FFLAGS(@07)                           0331 00305000
*    UPDSW=ON;                         /* SET TO UPDATE WORKFILE     */ 00306000
         OI    UPDSW(@07),B'10000000'                              0332 00307000
*    CINSAC=0;                         /*RESET ALTERNATE             */ 00308000
         SLR   @04,@04                                             0333 00309000
         STCM  @04,7,CINSAC(@07)                                   0333 00310000
*    CIN1=SNCI;                        /* GET CI NUMBER              */ 00311000
         L     @04,RSWASNVT(,REG6)                                 0334 00312000
         L     @04,RSFAENT(,@04)                                   0334 00313000
         MVC   CIN1(3,@07),SNCI(@04)                               0334 00314000
*    IF SNFUSE=OFF THEN                /*NO GO FOUND                 */ 00315000
         TM    SNFUSE(@04),B'10000000'                             0335 00316000
         BNZ   @RF00335                                            0335 00317000
*      DO;                                                         0336 00318000
*        CIN1=SNACI;                   /* DELETE GOP ONLY            */ 00319000
         MVC   CIN1(3,@07),SNACI(@04)                              0337 00320000
*        ALTCINSW=ON;                  /* IND DELETE OF GOP          */ 00321000
         OI    ALTCINSW(@07),B'00010000'                           0338 00322000
*      END;                                                        0339 00323000
*                                                                  0339 00324000
*    /****************************************************************/ 00325000
*    /*                                                              */ 00326000
*    /*   PROCESS UNTIL ALL NECESSARY RECORDS ARE CHANGED            */ 00327000
*    /*                                                              */ 00328000
*    /****************************************************************/ 00329000
*                                                                  0340 00330000
*    DO WHILE ENDSW=OFF;                                           0340 00331000
*                                                                  0340 00332000
@RF00335 B     @DE00340                                            0340 00333000
@DL00340 DS    0H                                                  0341 00334000
*      /**************************************************************/ 00335000
*      /*                                                            */ 00336000
*      /* READ A RECORD AND INITIALIZE POINTERS                      */ 00337000
*      /*                                                            */ 00338000
*      /**************************************************************/ 00339000
*                                                                  0341 00340000
*      CALL READREC;               /* READ A RECORD              */     00341000
         BAL   @14,READREC                                         0341 00342000
*      IF RSFNRF = ON THEN             /*NO RECORD FOUND             */ 00343000
*                                                                  0342 00344000
         L     @07,RSWASNVT(,REG6)                                 0342 00345000
         TM    RSFNRF(@07),B'10000000'                             0342 00346000
         BNO   @RF00342                                            0342 00347000
*        DO;                                                       0343 00348000
*        /*CALL CKERR*/                                            0344 00349000
*          RSCKERR = RSCKERR && RSCKERR;                           0344 00350000
         XC    RSCKERR(48,REG6),RSCKERR(REG6)                      0344 00351000
*          RSCKERN = RSE100;                                       0345 00352000
         MVC   RSCKERN(2,REG6),@CH00143                            0345 00353000
*          RSCKTERM = 'TERM';                                      0346 00354000
*                                                                  0346 00355000
         MVC   RSCKTERM(4,REG6),@CC03625                           0346 00356000
*        /* CALL TO MODULE IDCRS05 FOR PROCEDURE CKERR               */ 00357000
*        CALL CKERR;                                               0347 00358000
         L     @15,@CV03449                                        0347 00359000
         BALR  @14,@15                                             0347 00360000
*        END;     /*SHOULD FIND RCD IN DELGO    */                 0348 00361000
*      RSFCPE5=RSFCPE1+SNGOP;          /* POINT TO GOP               */ 00362000
*                                                                  0349 00363000
@RF00342 L     @07,RSFCPE1(,REG6)                                  0349 00364000
         L     @04,RSWASNVT(,REG6)                                 0349 00365000
         L     @04,RSFAENT(,@04)                                   0349 00366000
         LH    @15,SNGOP(,@04)                                     0349 00367000
         ALR   @15,@07                                             0349 00368000
         ST    @15,RSFCPE5(,REG6)                                  0349 00369000
*      /**************************************************************/ 00370000
*      /*                                                            */ 00371000
*      /*            DELETE GROUP OCCURRENCE POINTER                 */ 00372000
*      /*                                                            */ 00373000
*      /*************************************************************** 00374000
*                                                                  0350 00375000
*      IF ALTCINSW=ON THEN                                         0350 00376000
         L     @15,RSWAFINF(,REG6)                                 0350 00377000
         TM    ALTCINSW(@15),B'00010000'                           0350 00378000
         BNO   @RF00350                                            0350 00379000
*        DO;                                                       0351 00380000
*          RSFCPE5=RSFCPE1+SNAGOP;     /* POINT TO GOP               */ 00381000
         ICM   @14,12,SNAGOP(@04)                                  0352 00382000
         SRA   @14,16                                              0352 00383000
         ALR   @07,@14                                             0352 00384000
         ST    @07,RSFCPE5(,REG6)                                  0352 00385000
*          CATGODEL=ON;                /* DELETE GOP                 */ 00386000
         OI    CATGODEL(@07),B'01000000'                           0353 00387000
*          CATGOCI = CATGOCI && CATGOCI; /*CLEAR CIN                 */ 00388000
         XC    CATGOCI(3,@07),CATGOCI(@07)                         0354 00389000
*          FNDSW=ON;                   /* INDICATE FOUND             */ 00390000
*                                                                  0355 00391000
         OI    FNDSW(@15),B'00000100'                              0355 00392000
*          /**********************************************************/ 00393000
*          /*                                                        */ 00394000
*          /*     CLEAR ASPT (AVAILABLE SPACE POINTER)               */ 00395000
*          /*     FOR DELETED VERTICAL EXTENSIONS.                   */ 00396000
*          /*                                                        */ 00397000
*          /**********************************************************/ 00398000
*                                                                  0356 00399000
