*      CALL READREC;                                               0469 00943000
         BAL   @14,READREC                                         0469 00944000
*      IF RSFNRF = ON THEN             /*NO RECORD FOUND             */ 00945000
         L     @07,RSWASNVT(,REG6)                                 0470 00946000
         TM    RSFNRF(@07),B'10000000'                             0470 00947000
