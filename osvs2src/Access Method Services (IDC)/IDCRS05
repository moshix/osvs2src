         L     @07,GRABCURR(,@07)                                  0964 04695000
         CLC   RSVIADSC(4,REG6),RCDDICTS+8(@07)                    0964 04696000
         BNE   @RF00964                                            0964 04697000
*                      RSVIAFND=ON;    /*THIS IS IT                  */ 04698000
         OI    RSVIAFND(REG6),B'00100000'                          0965 04699000
*            END;                                                  0966 04700000
*                                                                  0966 04701000
@RF00964 DS    0H                                                  0966 04702000
@RF00963 DS    0H                                                  0966 04703000
@RC00961 DS    0H                                                  0966 04704000
@RF00960 DS    0H                                                  0966 04705000
@RC00958 DS    0H                                                  0967 04706000
*        /************************************************************/ 04707000
*        /*                                                          */ 04708000
*        /* IF THE RECORD DOES NOT ALREADY EXIST IN THE BUFFER       */ 04709000
*        /* THEN PREPARE TO GET IT.                                  */ 04710000
*        /* INITIALIZE TO ALLOW A NO RECORD FOUND CONDITION.         */ 04711000
*        /*                                                          */ 04712000
*        /************************************************************/ 04713000
*                                                                  0967 04714000
*        IF RSVIAFND=OFF THEN                                      0967 04715000
@RF00956 DS    0H                                                  0967 04716000
@RC00931 TM    RSVIAFND(REG6),B'00100000'                          0967 04717000
         BNZ   @RF00967                                            0967 04718000
*          DO;                                                     0968 04719000
*            RSVIAGRB = GRABFLGS;      /*SAVE CALLER'S FILE OPTIONS  */ 04720000
         L     @07,RSGRABCU(,REG6)                                 0969 04721000
         MVC   RSVIAGRB(3,REG6),GRABFLGS(@07)                      0969 04722000
*            GRABAFL1 = ''B;           /*CLEAR FILE OPTIONS          */ 04723000
         MVI   GRABAFL1(@07),X'00'                                 0970 04724000
*            GRABAFL2 = ''B;                                       0971 04725000
         MVI   GRABAFL2(@07),X'00'                                 0971 04726000
*            GRABSFXT = ON;            /*ALLOW XLATE ERROR ON CIN    */ 04727000
*            GRABSNRN = ON;            /*TREAT XLATE ERR AS NO RCDFND*/ 04728000
         OI    GRABSFXT(@07),B'10000010'                           0973 04729000
*            RSVIADUP = OFF;           /*CLEAR DUPE CIN INDR         */ 04730000
*            RSVIAUPD = OFF;           /*CLEAR GET UPDATE INDR       */ 04731000
*            RSVIARRN = OFF;           /*CLEAR ARG IS RRN INDR       */ 04732000
         NI    RSVIADUP(REG6),B'10110011'                          0976 04733000
*            RABNRF = OFF;             /*CLEAR NO RECORD FOUND INDR  */ 04734000
         L     @07,GRABCURR(,@07)                                  0977 04735000
         NI    RABNRF(@07),B'11111101'                             0977 04736000
*            IF RSRUPD = ON THEN                                   0978 04737000
         TM    RSRUPD(REG6),B'00001000'                            0978 04738000
         BNO   @RF00978                                            0978 04739000
*              RSVIAUPD = ON;          /*SAVE CALLER'S UPDATE IND    */ 04740000
*                                                                  0979 04741000
         OI    RSVIAUPD(REG6),B'00001000'                          0979 04742000
*        /************************************************************/ 04743000
*        /*                                                          */ 04744000
*        /* GET THE RECORD UNTIL THE RECORD IS FOUND OR              */ 04745000
*        /* THERE ARE NO MORE DUPLICATE CINS.                        */ 04746000
*        /*                                                          */ 04747000
*        /* USE THE CALLER'S OPTIONS EXCEPT UPDATE. SINCE DUPE       */ 04748000
*        /* PROCESSING IS INVOLVED, A DUPE RECORD MAY ALREADY EXIST  */ 04749000
*        /* IN ANOTHER RAB FOR UPDATE. IT MAY POINT TO THE RECORD WE */ 04750000
*        /* WANT. IF WE ATTEMPTED A GET UPDATE ON THE TRANSITION     */ 04751000
*        /* RECORD, WE WOULD GET A VSAM EXCLUSIVE CONTROL            */ 04752000
*        /* ERROR. WE WILL DO A GET FOR NO UPDATE IF THE RECORD IS   */ 04753000
*        /* HELD FOR UPDATE IN ANOTHER BUFFER. IF IT IS TRUELY THE   */ 04754000
*        /* ONE THAT WE WANT, THEN THIS IS A PROGRAMMING ERROR.      */ 04755000
*        /*                                                          */ 04756000
*        /************************************************************/ 04757000
*                                                                  0980 04758000
*  FINDRCD:                                                        0980 04759000
*            DO UNTIL (RABNRF=ON | RSVIAFND=ON);                   0980 04760000
@RF00978 DS    0H                                                  0980 04761000
FINDRCD  DS    0H                                                  0980 04762000
@DL00980 DS    0H                                                  0981 04763000
*              IF RABINUSE=ON & RABUPD=ON THEN                     0981 04764000
*                                                                  0981 04765000
         L     @07,RSGRABCU(,REG6)                                 0981 04766000
         L     @07,GRABCURR(,@07)                                  0981 04767000
         TM    RABINUSE(@07),B'11000000'                           0981 04768000
         BNO   @RF00981                                            0981 04769000
*                DO;                                               0982 04770000
*                  RSRTYPE = ''B;                                  0983 04771000
         MVI   RSRTYPE(REG6),X'00'                                 0983 04772000
*                  RSRFREE = ON;     /* FREERCD  */                0984 04773000
         OI    RSRFREE(REG6),B'00000001'                           0984 04774000
*                  RSRUPD = ON;      /* UPDATE  */                 0985 04775000
         OI    RSRUPD(REG6),B'00001000'                            0985 04776000
*                  RSRDS = ''B;                                    0986 04777000
         NI    RSRDS(REG6),B'00011111'                             0986 04778000
*                  RSRWF = ON;       /* WORKFILE  */               0987 04779000
*                  RSRDIR = ON;      /* DIRECT  */                 0988 04780000
*                  RSRKEY = ON;      /* KEY  */                    0989 04781000
*                                                                  0989 04782000
         OI    RSRWF(REG6),B'10010100'                             0989 04783000
*                /* CALL TO MODULE IDCRS06 FOR PROCEDURE RECMGMT     */ 04784000
*                CALL RECMGMT;                                     0990 04785000
         L     @15,@CV03446                                        0990 04786000
         BALR  @14,@15                                             0990 04787000
*                  RFY RTNREG RSTD;                                0991 04788000
*                  TESTRC = RTNREG;                                0992 04789000
         STH   RTNREG,TESTRC(,REG6)                                0992 04790000
*                  RFY RTNREG UNRSTD;                              0993 04791000
