         TITLE '************** IDCRS05: IDCAMS RESETCAT ROUTINE ********00001000
               *********'                                               00002000
IDCRS05  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IDCRS05   78.038'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  LA    @11,4095(,@12)                                      0001 00012000
         LA    @10,4095(,@11)                                      0001 00013000
         LA    @09,4095(,@10)                                      0001 00014000
         USING @PSTART,@12                                         0001 00015000
         USING @PSTART+4095,@11                                    0001 00016000
         USING @PSTART+8190,@10                                    0001 00017000
         USING @PSTART+12285,@09                                   0001 00018000
         USING @DATD,@08  DATA BASE REGISTER                            00019000
         L     @15,@SIZDATD  SIZE OF AUTOMATIC DATA                     00020000
         LA    @15,0(0,@15)  CLEAR SUBPOOL                              00021000
         STM   @09,@12,RS05RG09(REG6)  SAVE BASE REGISTERS              00022000
*        RETURN CODE(REG15);  /*REG 15 HAS LENGTH OF AUTO DATA*/   0288 00023000
         L     @14,12(,@13)                                        0288 00024000
         LM    @00,@12,20(@13)                                     0288 00025000
         BR    @14                                                 0288 00026000
*/********************************************************************/ 00027000
*/*                                                                  */ 00028000
*/*            RESETCAT LOCAL DECLARATIONS                           */ 00029000
*/*                                                                  */ 00030000
*/********************************************************************/ 00031000
*                                                                  0289 00032000
*DCL MODID CHAR(4) INIT('RS05');       /*MODID FOR UEPIL             */ 00033000
*                                                                  0289 00034000
*DCL                                                               0290 00035000
*   1 PATCH    CHAR(800)               /*PATCH AREA                  */ 00036000
*              LOCAL STATIC BDY(WORD), /* 5 PERCENT                  */ 00037000
*     2 *      CHAR(32)                /*PATCHAREA                   */ 00038000
*              INIT('PATCH 800 BYTES LAST USED 000000'),           0290 00039000
*                                      /*IDENTIFIER                  */ 00040000
*     2 *(768) CHAR(1)                 /*FORMAT UNUSED AREA          */ 00041000
*              INIT((768)'00'X);       /*WITH ZEROS                  */ 00042000
*/********************************************************************/ 00043000
*/*                                                                  */ 00044000
*/* PROCEDURE NAME - ADDUPCR                                         */ 00045000
*/*                                                                  */ 00046000
*/* FUNCTION - PREPARE FOR UPDATE CRA PROCESSING:                    */ 00047000
*/*             SET UPDATE CRA PROCESSING FLAG IN WORKFILE RECORD    */ 00048000
*/*             ENSURE THAT CRA VOLUME IS ALLOCATED                  */ 00049000
*/*                                                                  */ 00050000
*/* INPUT -    WORKFILE BUFFER AND PROCESSING AREA WILL BE           */ 00051000
*/*            OBTAINED VIA THE CURRENT WORKFILE RAB                 */ 00052000
*/*                                                                  */ 00053000
*/* OUTPUT -   UPDATE CRA FLAG IS SET IN WORKFILE RECORD             */ 00054000
*/*            AN ENTRY MAY BE ADDED TO RESVOL                       */ 00055000
*/*                                                                  */ 00056000
*/* RETURN                                                           */ 00057000
*/*  CODES -   0 = SUCCESSFUL                                        */ 00058000
*/*            4 = CRA VOLUME COULD NOT BE ALLOCATED                 */ 00059000
*/*                                                                  */ 00060000
*/* EXIT, NORMAL - RETURN TO CALLER                                  */ 00061000
*/*                                                                  */ 00062000
*/********************************************************************/ 00063000
*      GENERATE;                                                   0291 00064000
         DS    0F                                                       00065000
NAME0002 DC    CL8'ADDUPCR'                                             00066000
*ADDUPCR: PROCEDURE                                                0292 00067000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0292 00068000
*                                                                  0292 00069000
         B     @PB00002                                            0292 00070000
ADDUPCR  DS    0H                                                  0293 00071000
*      GENERATE REFS(RS05RG08, REG6);                              0293 00072000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          00073000
         LR    @15,@13  OLD SAVE AREA                                   00074000
         LA    @13,72(@13)  NEW SAVE AREA                               00075000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     00076000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        00077000
         ENTRY ADDUPCR  ENTRY POINT                                     00078000
         LM    @08,@12,RS05RG08(REG6)  PRIME BASE AND DATA REGS         00079000
         MVC   0(4,@15),NAME0002  PROCNAME TO REGSAVE                   00080000
*         RFY RESVOL BASED(RSADENP);                               0294 00081000
*         RFY GRAB   BASED(RSADGRBP);                              0295 00082000
*                                                                  0295 00083000
*         OLDERID2 = NEWERID2;                                     0296 00084000
         L     @07,GDTTR2(,REG5)                                   0296 00085000
         MVC   @TS00001(95),NEWERID2(@07)                          0296 00086000
         MVC   OLDERID2(95,@07),@TS00001                           0296 00087000
*         NEWID2 = 'RSAD';                                         0297 00088000
*                                                                  0297 00089000
         MVC   NEWID2(4,@07),@CC03474                              0297 00090000
*         /***********************************************************/ 00091000
*         /*                                                         */ 00092000
*         /*  SET WORKFILE GRAB CONTROL BLOCK ADDRESS                */ 00093000
*         /*                                                         */ 00094000
*         /***********************************************************/ 00095000
*                                                                  0298 00096000
*         RSADGRBP = ADDR(GRABSWF);                                0298 00097000
*                                                                  0298 00098000
         L     @07,RSIOTBL(,REG6)                                  0298 00099000
         ST    @07,RSADGRBP(,REG6)                                 0298 00100000
*         /***********************************************************/ 00101000
*         /*                                                         */ 00102000
*         /*  SET UPDATE CRA FLAG ON IN WORKFILE PROCESSING AREA     */ 00103000
*         /*                                                         */ 00104000
*         /***********************************************************/ 00105000
*                                                                  0299 00106000
*         RSWUPCRA = ON;                                           0299 00107000
*                                                                  0299 00108000
         L     @07,GRABCURR(,@07)                                  0299 00109000
         OI    RSWUPCRA(@07),B'00010000'                           0299 00110000
*         /***********************************************************/ 00111000
*         /*                                                         */ 00112000
*         /*  CALL SCNRLST TO LOCATE CRA VOLSER ENTRY IN RESVOL.     */ 00113000
*         /*                                                         */ 00114000
*         /***********************************************************/ 00115000
*                                                                  0300 00116000
*         RSADVSRP = ADDR(RCDCRAVL);                               0300 00117000
*                                                                  0300 00118000
         LA    @07,RCDCRAVL+8(,@07)                                0300 00119000
         ST    @07,RSADVSRP(,REG6)                                 0300 00120000
*         CALL SCNRLST('V','B',RSADENP,RSADVSRP);                  0301 00121000
*                                                                  0301 00122000
*                                                                  0301 00123000
         LA    @07,@CC00828                                        0301 00124000
         ST    @07,@AL00001                                        0301 00125000
         LA    @07,@CC00810                                        0301 00126000
         ST    @07,@AL00001+4                                      0301 00127000
         LA    @07,RSADENP(,REG6)                                  0301 00128000
         ST    @07,@AL00001+8                                      0301 00129000
         LA    @07,RSADVSRP(,REG6)                                 0301 00130000
         ST    @07,@AL00001+12                                     0301 00131000
         LA    @01,@AL00001                                        0301 00132000
         BAL   @14,SCNRLST                                         0301 00133000
*         /***********************************************************/ 00134000
*         /*                                                         */ 00135000
*         /*  ENTRY NOT FOUND IN RESVOL.                             */ 00136000
*         /*                                                         */ 00137000
*         /***********************************************************/ 00138000
*                                                                  0302 00139000
*         IF RSADENP = 0                                           0302 00140000
*           THEN                                                   0302 00141000
         L     @07,RSADENP(,REG6)                                  0302 00142000
         LTR   @07,@07                                             0302 00143000
         BNZ   @RF00302                                            0302 00144000
*ADDUBLD:     DO;                                                  0303 00145000
*                                                                  0303 00146000
ADDUBLD  DS    0H                                                  0304 00147000
*         /***********************************************************/ 00148000
*         /*                                                         */ 00149000
*         /*    BUILD AN RESVOL ENTRY FOR THE CRA.                   */ 00150000
*         /*                                                         */ 00151000
*         /***********************************************************/ 00152000
*                                                                  0304 00153000
*               RSADENP = ADDR(RSVOLENT);                          0304 00154000
         L     @07,RSIOTBL(,REG6)                                  0304 00155000
         LA    @07,RSVOLENT(,@07)                                  0304 00156000
         ST    @07,RSADENP(,REG6)                                  0304 00157000
*               RESVOL = ''B;                                      0305 00158000
         XC    RESVOL(92,@07),RESVOL(@07)                          0305 00159000
*               RESVLSER = RCDCRAVL;                               0306 00160000
*                                                                  0306 00161000
         L     @04,RSADGRBP(,REG6)                                 0306 00162000
         LA    @15,8                                               0306 00163000
         AL    @15,GRABCURR(,@04)                                  0306 00164000
         MVC   RESVLSER(6,@07),RCDCRAVL(@15)                       0306 00165000
*         /***********************************************************/ 00166000
*         /*                                                         */ 00167000
*         /*    ALLOCATE THE CRA VOLUME.                             */ 00168000
*         /*                                                         */ 00169000
*         /***********************************************************/ 00170000
*                                                                  0307 00171000
*                                                                  0307 00172000
*         /***********************************************************/ 00173000
*         /*                                                         */ 00174000
*         /*    SET UP VOLUME LIST FOR CALL TO CRAMNT.               */ 00175000
*         /*    HAVE TO COMPUTE THE LENGTH OF THE NON-BLANK          */ 00176000
*         /*    PART OF THE CRA VOLSER.                              */ 00177000
*         /*                                                         */ 00178000
*         /***********************************************************/ 00179000
*                                                                  0307 00180000
*               RSADSTP = OFF;                                     0307 00181000
         NI    RSADSTP(REG6),B'01111111'                           0307 00182000
*               RSADVLSR = RCDCRAVL;                               0308 00183000
*                                                                  0308 00184000
         MVC   RSADVLSR(6,REG6),RCDCRAVL(@15)                      0308 00185000
*ADDULP1:       DO RSIDX3 = 1 TO LENGTH(RSADVLSR)                  0309 00186000
*                   WHILE (RSADSTP = OFF);                         0309 00187000
*                                                                  0309 00188000
ADDULP1  LA    @07,1                                               0309 00189000
         STH   @07,RSIDX3(,REG6)                                   0309 00190000
@DL00309 TM    RSADSTP(REG6),B'10000000'                           0309 00191000
         BNZ   @DC00309                                            0309 00192000
*                 IF RSADVLSR(RSIDX3:RSIDX3) = ' '                 0310 00193000
*                   THEN                                           0310 00194000
         LH    @07,RSIDX3(,REG6)                                   0310 00195000
         ALR   @07,REG6                                            0310 00196000
         CLI   RSADVLSR-1(@07),C' '                                0310 00197000
         BNE   @RF00310                                            0310 00198000
*                     RSADSTP = ON;                                0311 00199000
         OI    RSADSTP(REG6),B'10000000'                           0311 00200000
*                   ELSE                                           0312 00201000
*                     RSADVLC = RSIDX3;                            0312 00202000
*                                                                  0312 00203000
         B     @RC00310                                            0312 00204000
@RF00310 LH    @07,RSIDX3(,REG6)                                   0312 00205000
         STC   @07,RSADVLC(,REG6)                                  0312 00206000
*               END ADDULP1;                                       0313 00207000
*                                                                  0313 00208000
*                                                                  0313 00209000
@RC00310 LA    @07,1                                               0313 00210000
         AH    @07,RSIDX3(,REG6)                                   0313 00211000
         STH   @07,RSIDX3(,REG6)                                   0313 00212000
         C     @07,@CF00152                                        0313 00213000
         BNH   @DL00309                                            0313 00214000
@DC00309 DS    0H                                                  0314 00215000
*         /***********************************************************/ 00216000
*         /*                                                         */ 00217000
*         /*    SET UP UNIT NAME LIST FOR CALL TO CRAMNT.            */ 00218000
*         /*                                                         */ 00219000
*         /***********************************************************/ 00220000
*                                                                  0314 00221000
*               RSADULC = LENGTH(SYSALLDA);                        0314 00222000
         MVI   RSADULC(REG6),X'08'                                 0314 00223000
*               RSADULNM = SYSALLDA;                               0315 00224000
*                                                                  0315 00225000
*                                                                  0315 00226000
         MVC   RSADULNM(8,REG6),@CC00774                           0315 00227000
*         /***********************************************************/ 00228000
*         /*                                                         */ 00229000
*         /*    SET UP PARAMETERS FOR CALL TO CRAMNT.                */ 00230000
*         /*                                                         */ 00231000
*         /***********************************************************/ 00232000
*                                                                  0316 00233000
*               RSADDDNP = ADDR(RSADDDN);                          0316 00234000
         LA    @07,RSADDDN(,REG6)                                  0316 00235000
         ST    @07,RSADDDNP(,REG6)                                 0316 00236000
*               RSADVLP = ADDR(RSADVL);                            0317 00237000
         LA    @07,RSADVL(,REG6)                                   0317 00238000
         ST    @07,RSADVLP(,REG6)                                  0317 00239000
*               RSADULP = ADDR(RSADUL);                            0318 00240000
*                                                                  0318 00241000
         LA    @07,RSADUL(,REG6)                                   0318 00242000
         ST    @07,RSADULP(,REG6)                                  0318 00243000
*         /***********************************************************/ 00244000
*         /*                                                         */ 00245000
*         /*    CALL CRAMNT TO ALLOCATE THE CRA VOLUME.              */ 00246000
*         /*                                                         */ 00247000
*         /***********************************************************/ 00248000
*                                                                  0319 00249000
*               /* CALL TO MODULE IDCRS06 FOR PROCEDURE CRAMNT       */ 00250000
*               CALL IDCRS6CM(RSADVLP,RSADULP,RSADDDNP);           0319 00251000
*                                                                  0319 00252000
         LA    @07,RSADVLP(,REG6)                                  0319 00253000
         ST    @07,@AL00001                                        0319 00254000
         LA    @07,RSADULP(,REG6)                                  0319 00255000
         ST    @07,@AL00001+4                                      0319 00256000
         LA    @07,RSADDDNP(,REG6)                                 0319 00257000
         ST    @07,@AL00001+8                                      0319 00258000
         L     @15,@CV03447                                        0319 00259000
         LA    @01,@AL00001                                        0319 00260000
         BALR  @14,@15                                             0319 00261000
*               RESPECIFY RTNREG RSTD;                             0320 00262000
*               TESTRC = RTNREG;                                   0321 00263000
         STH   RTNREG,TESTRC(,REG6)                                0321 00264000
*               RESPECIFY RTNREG UNRSTD;                           0322 00265000
*                                                                  0322 00266000
*                                                                  0322 00267000
*         /***********************************************************/ 00268000
*         /*                                                         */ 00269000
*         /*    IF ALLOCATION FAILED, SET UPDATE CRA UNAVAILABLE FLAG*/ 00270000
*         /*                                                         */ 00271000
*         /***********************************************************/ 00272000
*                                                                  0323 00273000
*               IF TESTRC ^= 0                                     0323 00274000
*                 THEN                                             0323 00275000
         LH    @07,TESTRC(,REG6)                                   0323 00276000
         LTR   @07,@07                                             0323 00277000
         BZ    @RF00323                                            0323 00278000
*                   RSCRAUA = ON;                                  0324 00279000
*                                                                  0324 00280000
         OI    RSCRAUA(REG6),B'00010000'                           0324 00281000
*         /***********************************************************/ 00282000
*         /*                                                         */ 00283000
*         /*    IF ALLOCATION SUCCESSFUL, PLACE DDNAME IN RESVOL     */ 00284000
*         /*    ENTRY AND SET CRA ALLOCATED FLAG.                    */ 00285000
*         /*                                                         */ 00286000
*         /***********************************************************/ 00287000
*                                                                  0325 00288000
*                 ELSE                                             0325 00289000
*                   DO;                                            0325 00290000
         B     @RC00323                                            0325 00291000
@RF00323 DS    0H                                                  0326 00292000
*                     RESVLDDN = RSADDDN;                          0326 00293000
         L     @07,RSADENP(,REG6)                                  0326 00294000
         MVC   RESVLDDN(8,@07),RSADDDN(REG6)                       0326 00295000
*                     RESVLMNT = ON;                               0327 00296000
         OI    RESVLMNT(@07),B'10000000'                           0327 00297000
*                   END;                                           0328 00298000
*                                                                  0328 00299000
*         /***********************************************************/ 00300000
*         /*                                                         */ 00301000
*         /*    CALL BLDRLST TO ADD THE ENTRY TO RESVOL.             */ 00302000
*         /*                                                         */ 00303000
*         /***********************************************************/ 00304000
*                                                                  0329 00305000
*               CALL BLDRLST(RSADENP);                             0329 00306000
*                                                                  0329 00307000
@RC00323 LA    @07,RSADENP(,REG6)                                  0329 00308000
         ST    @07,@AL00001                                        0329 00309000
         LA    @01,@AL00001                                        0329 00310000
         BAL   @14,BLDRLST                                         0329 00311000
*             END ADDUBLD;                                         0330 00312000
*                                                                  0330 00313000
*         /***********************************************************/ 00314000
*         /*                                                         */ 00315000
*         /*    AT THIS POINT THE ENTRY HAS BEEN FOUND OR ADDED,     */ 00316000
*         /*    AND PROCESSING IS COMPLETE. RETURN.                  */ 00317000
*         /*                                                         */ 00318000
*         /***********************************************************/ 00319000
*                                                                  0331 00320000
*         IF GDTDBG = NULLPTR                                      0331 00321000
*           THEN;                                                  0331 00322000
@RF00302 L     @07,GDTDBG(,REG5)                                   0331 00323000
         LTR   @07,@07                                             0331 00324000
         BZ    @RT00331                                            0331 00325000
*           ELSE                                                   0333 00326000
*             CALL IDCDB010 (GDTTBL,'RSA1');                       0333 00327000
*                                                                  0333 00328000
         ST    REG5,@AL00001                                       0333 00329000
         LA    @07,@CC03483                                        0333 00330000
         ST    @07,@AL00001+4                                      0333 00331000
         MVI   @AL00001+4,X'80'                                    0333 00332000
         L     @15,GDTDBG(,REG5)                                   0333 00333000
         LA    @01,@AL00001                                        0333 00334000
         BALR  @14,@15                                             0333 00335000
*         RFY GRAB BASED(RSGRABCU);                                0334 00336000
*                                                                  0334 00337000
@RT00331 DS    0H                                                  0335 00338000
*  CALL EXIT;  /*TERMINATE*/                                       0335 00339000
         BAL   @14,EXIT                                            0335 00340000
*  END ADDUPCR; /*END OF PROCEDURE*/                               0336 00341000
@EL00002 DS    0H                                                  0336 00342000
@EF00002 DS    0H                                                  0336 00343000
@ER00002 BR    @14                                                 0336 00344000
@PB00002 DS    0H                                                  0337 00345000
*/********************************************************************/ 00346000
*/*                                                                  */ 00347000
*/* PROCEDURE NAME - BLDRLST                                         */ 00348000
*/*                                                                  */ 00349000
*/* FUNCTION - ADD AN ENTRY TO RESVOL                                */ 00350000
*/*                                                                  */ 00351000
*/* INPUT -    ADDRESS OF ENTRY TO BE ADDED                          */ 00352000
*/*                                                                  */ 00353000
*/* OUTPUT -   ENTRY IS ADDED TO RESVOL                              */ 00354000
*/*            STORAGE IS OBTAINED AS NEEDED                         */ 00355000
*/*                                                                  */ 00356000
*/* EXIT, NORMAL - RETURN TO CALLER                                  */ 00357000
*/* EXIT, ERROR - TO CKERR                                           */ 00358000
*/*                                                                  */ 00359000
*/********************************************************************/ 00360000
*      GENERATE;                                                   0337 00361000
         DS    0F                                                       00362000
NAME05BR DC    CL8'IDCRS5BR'                                            00363000
*IDCRS5BR: PROCEDURE                                               0338 00364000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0338 00365000
*                                                                  0338 00366000
         B     @PB00003                                            0338 00367000
IDCRS5BR DS    0H                                                  0339 00368000
*      /*SAVE AND PRIME REGISTERS*/                                0339 00369000
*      GENERATE REFS(RS05RG08, REG6);                              0339 00370000
*                                                                  0339 00371000
         ENTRY IDCRS5BR  EXTERNAL ENTRY POINT                           00372000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          00373000
         LR    @15,@13  OLD SAVE AREA                                   00374000
         LA    @13,72(@13)  NEW SAVE AREA                               00375000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     00376000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        00377000
         LM    @08,@12,RS05RG08(REG6)  PRIME BASE AND DATA REGS         00378000
         MVC   0(4,@15),NAME05BR+4 PROCNAME TO REGSAVE                  00379000
*        RFY REG1 RESTRICTED;                                      0340 00380000
*                                                                  0340 00381000
*        CALL BLDRLST; /*REG1 HAS PARAM LIST ADDR*/                0341 00382000
*                                                                  0341 00383000
         BAL   @14,BLDRLST                                         0341 00384000
*        RFY REG1;                                                 0342 00385000
*                                                                  0342 00386000
*           /*RESTORE CALLER'S REGISTERS AND RETURN*/              0343 00387000
*           DO;                                                    0343 00388000
*             GENERATE;                                            0344 00389000
         L     @13,4(,@13)  RESTORE CALLER'S REG SAVE                   00390000
         L     @14,12(,@13)  RESTORE RETURN ADDRESS                     00391000
         LM    @00,@12,20(@13)  RESTORE REGS                            00392000
         MVI   12(@13),255  MARK END OF SAVE LIST                       00393000
*             RETURN CODE(REG15); /*RETURN*/                       0345 00394000
@EL00003 DS    0H                                                  0345 00395000
@EF00003 DS    0H                                                  0345 00396000
@ER00003 BR    @14                                                 0345 00397000
*           END;                                                   0346 00398000
*                                                                  0346 00399000
*           CALL EXIT;  /*TERMINATE*/                              0347 00400000
*           END IDCRS5BR; /*END OF PROCEDURE*/                     0348 00401000
@PB00003 DS    0H                                                  0349 00402000
*                                                                  0349 00403000
*      GENERATE;                                                   0349 00404000
         DS    0F                                                       00405000
NAME0007 DC    CL8'BLDRLST'                                             00406000
*BLDRLST: PROCEDURE (BLDRENP)                                      0350 00407000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0350 00408000
*                                                                  0350 00409000
         B     @PB00004                                            0350 00410000
BLDRLST  MVC   @PC00004(4),0(@01)                                  0350 00411000
*      GENERATE REFS(RS05RG08, REG6);                              0351 00412000
*                                                                  0351 00413000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          00414000
         LR    @15,@13  OLD SAVE AREA                                   00415000
         LA    @13,72(@13)  NEW SAVE AREA                               00416000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     00417000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        00418000
         MVC   0(4,@15),NAME0007  PROCNAME TO REGSAVE                   00419000
*         /***********************************************************/ 00420000
*         /*                                                         */ 00421000
*         /*  ARGUMENT OF INVOCATION                                 */ 00422000
*         /*                                                         */ 00423000
*         /***********************************************************/ 00424000
*                                                                  0352 00425000
*         DCL BLDRENP PTR(31);         /*ADDRESS OF ENTRY TO BE ADDED*/ 00426000
*                                                                  0352 00427000
*         RFY RESVOL   BASED(RSSUBENP);                            0353 00428000
*                                                                  0353 00429000
*         OLDERID2 = NEWERID2;                                     0354 00430000
         L     @07,GDTTR2(,REG5)                                   0354 00431000
         MVC   @TS00001(95),NEWERID2(@07)                          0354 00432000
         MVC   OLDERID2(95,@07),@TS00001                           0354 00433000
*         NEWID2 = 'RSBR';                                         0355 00434000
*                                                                  0355 00435000
         MVC   NEWID2(4,@07),@CC03496                              0355 00436000
*         RSHAVECR = OFF;                                          0356 00437000
*                                                                  0356 00438000
         NI    RSHAVECR(REG6),B'11101111'                          0356 00439000
*         /***********************************************************/ 00440000
*         /*                                                         */ 00441000
*         /* IF RESVOL   TABLE EXISTS, THEN CONTINUE BUILDING IT     */ 00442000
*         /*                                                         */ 00443000
*         /***********************************************************/ 00444000
*                                                                  0357 00445000
*         IF RSVOLALL ^= 0                                         0357 00446000
*           THEN                                                   0357 00447000
         SLR   @07,@07                                             0357 00448000
         C     @07,RSVOLALL(,REG6)                                 0357 00449000
         BE    @RF00357                                            0357 00450000
