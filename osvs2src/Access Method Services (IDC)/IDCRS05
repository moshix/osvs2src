*        /************************************************************/ 04378000
*        CALL IDCSATI0 (GDTTBL,RSGENTME,RSGENDTE,CLOCK);           0901 04379000
         ST    REG5,@AL00001                                       0901 04380000
         LA    @07,RSGENTME(,REG6)                                 0901 04381000
         ST    @07,@AL00001+4                                      0901 04382000
         LA    @07,RSGENDTE(,REG6)                                 0901 04383000
         ST    @07,@AL00001+8                                      0901 04384000
         LA    @07,CLOCK                                           0901 04385000
         ST    @07,@AL00001+12                                     0901 04386000
         MVI   @AL00001+12,X'80'                                   0901 04387000
         L     @15,GDTTIM(,REG5)                                   0901 04388000
         LA    @01,@AL00001                                        0901 04389000
         BALR  @14,@15                                             0901 04390000
*        RESPECIFY RTNREG RSTD;                                    0902 04391000
*        TESTRC = RTNREG;                                          0903 04392000
         STH   RTNREG,TESTRC(,REG6)                                0903 04393000
*        RESPECIFY RTNREG UNRSTD;                                  0904 04394000
*        IF TESTRC ^= 0                                            0905 04395000
*          THEN                                                    0905 04396000
*                                                                  0905 04397000
         LH    @07,TESTRC(,REG6)                                   0905 04398000
         LTR   @07,@07                                             0905 04399000
         BZ    @RF00905                                            0905 04400000
*            DO;                                                   0906 04401000
*            /*CALL CKERR*/                                        0907 04402000
*              RSCKERR = RSCKERR && RSCKERR;                       0907 04403000
         XC    RSCKERR(48,REG6),RSCKERR(REG6)                      0907 04404000
*              RSCKERN = RSE061;                                   0908 04405000
         MVC   RSCKERN(2,REG6),@CH01006                            0908 04406000
*              RSCKTERM = 'TERM';                                  0909 04407000
*                                                                  0909 04408000
         MVC   RSCKTERM(4,REG6),@CC03499                           0909 04409000
*            CALL CKERR;                                           0910 04410000
         BAL   @14,CKERR                                           0910 04411000
*            END;                                                  0911 04412000
*        /************************************************************/ 04413000
*        /*                                                          */ 04414000
*        /*  POSITION DATE, FIRST SEVEN DIGITS AND SECOND SEVEN      */ 04415000
*        /*    DIGITS OF TIMESTAMP- THEN CONVERT TO ALPHAS           */ 04416000
*        /*                                                          */ 04417000
*        /************************************************************/ 04418000
*        RSGENTPT = ADDR(TRTBLE) - 240;                            0912 04419000
@RF00905 LA    @07,TRTBLE                                          0912 04420000
         SL    @07,@CF04939                                        0912 04421000
         ST    @07,RSGENTPT(,REG6)                                 0912 04422000
*        UNPK (RSGDATE,RSGENDT);                                   0913 04423000
         UNPK  RSGDATE(5,REG6),RSGENDT(3,REG6)                     0913 04424000
*        UNPK (RSGTIME(3:11),RSGENTME(1:5));                       0914 04425000
         UNPK  RSGTIME+2(9,REG6),RSGENTME(5,REG6)                  0914 04426000
*        UNPK (RSGUNPK,RSGENTME(4:8));                             0915 04427000
         UNPK  RSGUNPK(9,REG6),RSGENTME+3(5,REG6)                  0915 04428000
*        RSGTMEB = RSGUNPK(2:8);                                   0916 04429000
         MVC   RSGTMEB(7,REG6),RSGUNPK+1(REG6)                     0916 04430000
*        TR (RSGTIME(1:18),TRTAB);                                 0917 04431000
         TR    RSGTIME(18,REG6),TRTAB(@07)                         0917 04432000
*        /************************************************************/ 04433000
*        /*                                                          */ 04434000
*        /*  COMPLETE REMAINDER OF ENTRYNAME                         */ 04435000
*        /*                                                          */ 04436000
*        /************************************************************/ 04437000
*        RSGDELM = ENTNMDEL;                                       0918 04438000
         MVI   RSGDELM(REG6),C'T'                                  0918 04439000
*        RSGTMEA = RSGTMEB;                                        0919 04440000
         MVC   RSGTMEA(7,REG6),RSGTMEB(REG6)                       0919 04441000
*        RSGFIXID = ENTNMFIX;                                      0920 04442000
         MVC   RSGFIXID(13,REG6),@CC00843                          0920 04443000
*        RSGDELM1 = ENTNMDLM;                                      0921 04444000
         MVC   RSGDELM1(2,REG6),@CC00841                           0921 04445000
*        RSGDELM2 = ENTNMDLM;                                      0922 04446000
         MVC   RSGDELM2(2,REG6),@CC00841                           0922 04447000
*  CALL EXIT;  /*TERMINATE*/                                       0923 04448000
         BAL   @14,EXIT                                            0923 04449000
*  END GENNAME; /*END OF PROCEDURE*/                               0924 04450000
@EL00017 DS    0H                                                  0924 04451000
@EF00017 DS    0H                                                  0924 04452000
@ER00017 BR    @14                                                 0924 04453000
@PB00017 DS    0H                                                  0925 04454000
*/********************************************************************/ 04455000
*/*                                                                  */ 04456000
*/* PROCEDURE NAME - GETVIA                                          */ 04457000
*/*                                                                  */ 04458000
*/* FUNCTION -                                                       */ 04459000
*/*    GET VIA WILL ATTEMPT TO LOCATE A RECORD IN THE WORKFILE       */ 04460000
*/*    GIVEN ITS CIN AND EITHER THE CRAVOL WHICH IT IS ON OR THE     */ 04461000
*/*    DATA SET CREATION TIME STAMP. THIS ROUTINE IS TAILORED TO     */ 04462000
*/*    THE NEEDS FOR GETTING RECORDS IN ASSOC AND ITS SUBPROCEDURES. */ 04463000
*/*                                                                  */ 04464000
*/* INPUT -                                                          */ 04465000
*/*    GETVIA IS ENTERED BY ISSUING GETRCD WITH THE VIA OPTION. THE  */ 04466000
*/*    FORMAT IS VIA(CRA|DSCRETS,ADDR OF CRAVOL OR DSCRETS), WHERE   */ 04467000
*/*    CRAVOL IS THE 6 CHARACTER VOLUME SERIAL OF THE CRA VOLUME AND */ 04468000
*/*    DSCRETS IS THE 4 CHARACTER DATA SET CREATION TIME STAMP.      */ 04469000
*/*                                                                  */ 04470000
*/*    RSGETVIA STRUCTURE IN RSWORK CONTAINS THE INPUT               */ 04471000
*/*                                                                  */ 04472000
*/* OUTPUT -                                                         */ 04473000
*/*    RABNRF - IF ON THEN THE RECORD COULD NOT BE LOCATED.          */ 04474000
*/*             IF OFF THE RECORD IS IN THE RAB SPECIFIED.           */ 04475000
*/********************************************************************/ 04476000
*      GENERATE;                                                   0925 04477000
         DS    0F                                                       04478000
NAME0055 DC    CL8'GETVIA'                                              04479000
*GETVIA: PROCEDURE                                                 0926 04480000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0926 04481000
*                                                                  0926 04482000
         B     @PB00018                                            0926 04483000
GETVIA   DS    0H                                                  0927 04484000
*      GENERATE REFS(RS05RG08, REG6);                              0927 04485000
*                                                                  0927 04486000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          04487000
         LR    @15,@13  OLD SAVE AREA                                   04488000
         LA    @13,72(@13)  NEW SAVE AREA                               04489000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     04490000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        04491000
         ENTRY GETVIA  ENTRY POINT                                      04492000
         LM    @08,@12,RS05RG08(REG6)  PRIME BASE AND DATA REGS         04493000
         MVC   0(4,@15),NAME0055  PROCNAME TO REGSAVE                   04494000
*        OLDERID2 = NEWERID2;                                      0928 04495000
         L     @07,GDTTR2(,REG5)                                   0928 04496000
         MVC   @TS00001(95),NEWERID2(@07)                          0928 04497000
         MVC   OLDERID2(95,@07),@TS00001                           0928 04498000
*        NEWID2 = 'RSGV';                                          0929 04499000
*                                                                  0929 04500000
         MVC   NEWID2(4,@07),@CC04946                              0929 04501000
*        RSVIAFND = OFF;               /*CLEAR RECORD FOUND INDR     */ 04502000
*                                                                  0930 04503000
         NI    RSVIAFND(REG6),B'11011111'                          0930 04504000
*        /************************************************************/ 04505000
*        /*                                                          */ 04506000
*        /*  HANDLE GET FROM THE CATALOG OR CRA                      */ 04507000
*        /*                                                          */ 04508000
*        /************************************************************/ 04509000
*                                                                  0931 04510000
*        IF (RSGRABCU = ADDR(GRABSCA)) | (RSGRABCU = ADDR(GRABSCR))     04511000
*         THEN                                                     0931 04512000
         L     @07,RSGRABCU(,REG6)                                 0931 04513000
         L     @04,RSIOTBL(,REG6)                                  0931 04514000
         LA    @15,GRABSCA(,@04)                                   0931 04515000
         CR    @07,@15                                             0931 04516000
         BE    @RT00931                                            0931 04517000
         LA    @04,GRABSCR(,@04)                                   0931 04518000
         CR    @07,@04                                             0931 04519000
         BNE   @RF00931                                            0931 04520000
@RT00931 DS    0H                                                  0932 04521000
*          DO;                                                     0932 04522000
*                                                                  0932 04523000
*        /************************************************************/ 04524000
*        /*                                                          */ 04525000
*        /*     RECORD ALREADY EXISTS IN THE BUFFER                  */ 04526000
*        /*                                                          */ 04527000
*        /************************************************************/ 04528000
*                                                                  0933 04529000
*            IF RABINUSE=ON & RCDCINO=RSVIACIN THEN;               0933 04530000
*                                                                  0933 04531000
         L     @07,RSGRABCU(,REG6)                                 0933 04532000
         L     @07,GRABCURR(,@07)                                  0933 04533000
         TM    RABINUSE(@07),B'10000000'                           0933 04534000
         BNO   @RF00933                                            0933 04535000
         CLC   RCDCINO+8(3,@07),RSVIACIN(REG6)                     0933 04536000
         BE    @RT00933                                            0933 04537000
*        /************************************************************/ 04538000
*        /*                                                          */ 04539000
*        /*     SET GET OPTIONS                                      */ 04540000
*        /*                                                          */ 04541000
*        /************************************************************/ 04542000
*                                                                  0935 04543000
*            ELSE                                                  0935 04544000
*              DO;                                                 0935 04545000
@RF00933 DS    0H                                                  0936 04546000
*                RSVIAGRB = GRABFLGS;  /*SAVE CURRENT OPTIONS        */ 04547000
         L     @07,RSGRABCU(,REG6)                                 0936 04548000
         MVC   RSVIAGRB(3,REG6),GRABFLGS(@07)                      0936 04549000
*                GRABAFL1 = ''B;                                   0937 04550000
         MVI   GRABAFL1(@07),X'00'                                 0937 04551000
*                GRABAFL2 = ''B;       /*CLEAR OPTIONS               */ 04552000
         MVI   GRABAFL2(@07),X'00'                                 0938 04553000
*                GRABSIOI = ON;        /*ALLOW I/O ERROR             */ 04554000
*                GRABSNRN = ON;        /*ALLOW NO RECORD FOUND       */ 04555000
*                                                                  0940 04556000
         OI    GRABSIOI(@07),B'00010010'                           0940 04557000
*        /************************************************************/ 04558000
*        /*                                                          */ 04559000
*        /*     GET THE RECORD                                       */ 04560000
*        /*                                                          */ 04561000
*        /************************************************************/ 04562000
*                                                                  0941 04563000
*                RABRCDA = RSVIACIN;   /*CIN OF THE DESIRED RECORD   */ 04564000
*                                                                  0941 04565000
         L     @07,GRABCURR(,@07)                                  0941 04566000
         L     @04,RSVIACIN-1(,REG6)                               0941 04567000
         LA    @04,0(,@04)                                         0941 04568000
         ST    @04,RABRCDA(,@07)                                   0941 04569000
*                DO;                                               0942 04570000
*                  RSRTYPE = ''B;                                  0943 04571000
         MVI   RSRTYPE(REG6),X'00'                                 0943 04572000
*                  RSRGET = ON;      /* GETRCD  */                 0944 04573000
*                                                                  0944 04574000
         OI    RSRGET(REG6),B'10000000'                            0944 04575000
*                /* CALL TO MODULE IDCRS06 FOR PROCEDURE RECMGMT     */ 04576000
*                CALL RECMGMT;                                     0945 04577000
         L     @15,@CV03446                                        0945 04578000
         BALR  @14,@15                                             0945 04579000
*                  RFY RTNREG RSTD;                                0946 04580000
*                  TESTRC = RTNREG;                                0947 04581000
         STH   RTNREG,TESTRC(,REG6)                                0947 04582000
*                  RFY RTNREG UNRSTD;                              0948 04583000
*                END;                                              0949 04584000
*                                                                  0949 04585000
*        /************************************************************/ 04586000
*        /*                                                          */ 04587000
*        /*     TREAT AN I/O ERROR AS NO RECORD FOUND                */ 04588000
*        /*                                                          */ 04589000
*        /************************************************************/ 04590000
*                                                                  0950 04591000
*                IF RABIOIGN = ON THEN                             0950 04592000
         L     @07,RSGRABCU(,REG6)                                 0950 04593000
         L     @07,GRABCURR(,@07)                                  0950 04594000
         TM    RABIOIGN(@07),B'00000100'                           0950 04595000
         BNO   @RF00950                                            0950 04596000
*                  RABNRF = ON;                                    0951 04597000
*                                                                  0951 04598000
         OI    RABNRF(@07),B'00000010'                             0951 04599000
*        /************************************************************/ 04600000
*        /*                                                          */ 04601000
*        /*     RESTORE THE CALLER'S FILE OPTIONS                    */ 04602000
*        /*                                                          */ 04603000
*        /************************************************************/ 04604000
*                                                                  0952 04605000
*                GRABFLGS = RSVIAGRB;                              0952 04606000
@RF00950 L     @07,RSGRABCU(,REG6)                                 0952 04607000
         MVC   GRABFLGS(3,@07),RSVIAGRB(REG6)                      0952 04608000
*              END;                                                0953 04609000
*            RSVIAFND = ON;            /*END OF GETVIA PROC          */ 04610000
@RT00933 OI    RSVIAFND(REG6),B'00100000'                          0954 04611000
*          END;                                                    0955 04612000
*                                                                  0955 04613000
*        /************************************************************/ 04614000
*        /*                                                          */ 04615000
*        /* HANDLE WORKFILE REQUESTS -                               */ 04616000
*        /* CHECK TO SEE IF THE RECORD ALREADY EXISTS IN THE         */ 04617000
*        /* CURRENT RAB IN THE CORRECT MODE. IF SO THEN JUST         */ 04618000
*        /* RETURN - NO I/O WILL BE REQUIRED.                        */ 04619000
*        /*                                                          */ 04620000
*        /************************************************************/ 04621000
*                                                                  0956 04622000
*        ELSE                                                      0956 04623000
*  RCDEXIST:                                                       0956 04624000
*          IF RABINUSE=ON & RCDCINO=RSVIACIN &                     0956 04625000
*           (RSWDLCAT=OFF) THEN                                    0956 04626000
         B     @RC00931                                            0956 04627000
@RF00931 DS    0H                                                  0956 04628000
RCDEXIST L     @07,RSGRABCU(,REG6)                                 0956 04629000
         L     @07,GRABCURR(,@07)                                  0956 04630000
         TM    RABINUSE(@07),B'10000000'                           0956 04631000
         BNO   @RF00956                                            0956 04632000
         CLC   RCDCINO+8(3,@07),RSVIACIN(REG6)                     0956 04633000
         BNE   @RF00956                                            0956 04634000
         TM    RSWDLCAT(@07),B'01000000'                           0956 04635000
         BNZ   @RF00956                                            0956 04636000
*                                      /*CHECK FOR POSSIBLE RIGHT RCD*/ 04637000
*            DO;                                                   0957 04638000
*                                                                  0957 04639000
*        /************************************************************/ 04640000
*        /*                                                          */ 04641000
*        /*   HANDLE SELF-DEFINING RECORDS SINCE CRA DOES NOT APPLY  */ 04642000
*        /*                                                          */ 04643000
*        /************************************************************/ 04644000
*                                                                  0958 04645000
*              IF (RSVIACIN < VOLCI) THEN                          0958 04646000
         CLC   RSVIACIN(3,REG6),@CF00035+1                         0958 04647000
         BNL   @RF00958                                            0958 04648000
*                RSVIAFND = ON;      /*RECORD FOUND                */   04649000
*                                                                  0959 04650000
         OI    RSVIAFND(REG6),B'00100000'                          0959 04651000
*        /************************************************************/ 04652000
*        /*                                                          */ 04653000
*        /*   IF THIS IS A SEARCH FOR A CIN VIA A SPECIFIC           */ 04654000
*        /*   CRA THEN CHECK IF THE CRA OF THIS RECORD IS THE        */ 04655000
*        /*   ONE THAT WE WANT.                                      */ 04656000
*        /*                                                          */ 04657000
*        /************************************************************/ 04658000
*                                                                  0960 04659000
*              ELSE                                                0960 04660000
*                IF RSVIAFCR=ON THEN                               0960 04661000
         B     @RC00958                                            0960 04662000
@RF00958 TM    RSVIAFCR(REG6),B'10000000'                          0960 04663000
         BNO   @RF00960                                            0960 04664000
*                  IF RSVIACRA=RCDCRAVL THEN                       0961 04665000
         L     @07,RSGRABCU(,REG6)                                 0961 04666000
         L     @07,GRABCURR(,@07)                                  0961 04667000
         CLC   RSVIACRA(6,REG6),RCDCRAVL+8(@07)                    0961 04668000
         BNE   @RF00961                                            0961 04669000
*                    RSVIAFND=ON;      /*THIS IS IT                  */ 04670000
*                                                                  0962 04671000
         OI    RSVIAFND(REG6),B'00100000'                          0962 04672000
*        /************************************************************/ 04673000
*        /*                                                          */ 04674000
*        /*   IF THIS IS A SEARCH VIA DATA SET CREATION              */ 04675000
*        /*   TIME STAMP THEN CHECK IF THIS IS A DATA OR INDEX       */ 04676000
*        /*   AND, IF SO, IF IT BEARS THE SAME CREATION TIME         */ 04677000
*        /*   STAMP.                                                 */ 04678000
*        /*                                                          */ 04679000
*        /************************************************************/ 04680000
*                                                                  0963 04681000
*                ELSE                                              0963 04682000
*                  IF RCDID=DATAID | RCDID=INDEXID THEN            0963 04683000
         B     @RC00961                                            0963 04684000
@RF00961 L     @07,RSGRABCU(,REG6)                                 0963 04685000
         LA    @04,8                                               0963 04686000
         AL    @04,GRABCURR(,@07)                                  0963 04687000
         CLI   RCDID(@04),C'D'                                     0963 04688000
         BE    @RT00963                                            0963 04689000
         CLI   RCDID(@04),C'I'                                     0963 04690000
         BNE   @RF00963                                            0963 04691000
@RT00963 DS    0H                                                  0964 04692000
*                    IF RSVIADSC=RCDDICTS THEN                     0964 04693000
