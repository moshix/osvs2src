         L     @07,GRABCURR(,@07)                                  0998 04811000
         NI    RABRRN(@07),B'11101111'                             0998 04812000
*        /************************************************************/ 04813000
*        /*                                                          */ 04814000
*        /*     CHECK TO SEE IF THIS RECORD EXISTS IN ANOTHER BUFFER */ 04815000
*        /*     FOR UPDATE.                                          */ 04816000
*        /*                                                          */ 04817000
*        /************************************************************/ 04818000
*                                                                  0999 04819000
*              RSVIAIS = OFF;          /*INDICATE RCD NOT IN BUFFER  */ 04820000
@RC00996 NI    RSVIAIS(REG6),B'11101111'                           0999 04821000
*              IF RSVIAUPD = ON & RABRRN = OFF THEN                1000 04822000
