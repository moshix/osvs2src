         STH   @07,RSRRCPY(,REG6)                                  0725 02065000
*            END;                                                  0726 02066000
*                                                                  0726 02067000
*        /************************************************************/ 02068000
*        /*                                                          */ 02069000
*        /*   FOR CATALOG, THERE ARE TWO RMBS - ONE FOR THE          */ 02070000
*        /*   LOW KEY RANGE AND ONE FOR THE HIGH KEY RANGE           */ 02071000
*        /*   SET RSRMBP TO THE LOW KEY RANGE RMB                    */ 02072000
*        /*                                                          */ 02073000
*        /************************************************************/ 02074000
*                                                                  0727 02075000
*        IF RSRCAT = ON                                            0727 02076000
*          THEN                                                    0727 02077000
@RF00720 TM    RSRCAT(REG6),B'01000000'                            0727 02078000
         BNO   @RF00727                                            0727 02079000
*            DO;                                                   0728 02080000
*              RSRMBP = ADDR(CATLRMB);                             0729 02081000
         L     @07,RSIOTBL(,REG6)                                  0729 02082000
         LA    @07,CATLRMB(,@07)                                   0729 02083000
         ST    @07,RSRMBP(,REG6)                                   0729 02084000
*              IF RSRINIT = ON                                     0730 02085000
*                THEN                                              0730 02086000
