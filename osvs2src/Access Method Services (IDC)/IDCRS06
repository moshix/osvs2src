*              RECPTRCD(257:CELEN) = RECRCD(257:CELEN);            1055 03883000
*                                                                  1055 03884000
         MVC   RECPTRCD+256(249,@07),RECRCD+256(@04)               1055 03885000
*        /************************************************************/ 03886000
*        /*                                                          */ 03887000
*        /*   FOR WORKFILE, SAVE THE USERS FLAG FIELD                */ 03888000
*        /*                                                          */ 03889000
*        /************************************************************/ 03890000
*                                                                  1056 03891000
*                                                                  1056 03892000
*              IF RSRWF = ON                                       1056 03893000
*                THEN                                              1056 03894000
         TM    RSRWF(REG6),B'10000000'                             1056 03895000
         BNO   @RF01056                                            1056 03896000
*                  RSRSVFLG = RECFFLG;                             1057 03897000
*                                                                  1057 03898000
         MVC   RSRSVFLG(10,REG6),RECFFLG(@04)                      1057 03899000
*        /************************************************************/ 03900000
*        /*                                                          */ 03901000
*        /*   ISSUE GET UPD REQUEST FOR THE RECORD WITH              */ 03902000
*        /*   THE SPECIFIED ARGUMENT.                                */ 03903000
*        /*   IF THE REQUEST IS FOR CNV, THE RDF,CIDF WILL BE        */ 03904000
*        /*   RETRIEVED.                                             */ 03905000
*        /*                                                          */ 03906000
*        /************************************************************/ 03907000
*                                                                  1058 03908000
*              RSRGET = ON;                                        1058 03909000
*                                                                  1058 03910000
