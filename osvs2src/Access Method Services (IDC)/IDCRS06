*                                                                  1044 03821000
         L     @07,IOCSPTR(,REG6)                                  1044 03822000
         OI    IOCMACEN(@07),B'00001000'                           1044 03823000
*                    CALL IDCIOPT0 (GDTTBL,IOCSPTR);               1045 03824000
*                                                                  1045 03825000
         ST    REG5,@AL00001                                       1045 03826000
         LA    @07,IOCSPTR(,REG6)                                  1045 03827000
         ST    @07,@AL00001+4                                      1045 03828000
         MVI   @AL00001+4,X'80'                                    1045 03829000
         L     @15,GDTPUT(,REG5)                                   1045 03830000
         LA    @01,@AL00001                                        1045 03831000
         BALR  @14,@15                                             1045 03832000
*                  END;                                            1046 03833000
*              RSRRC = 0;                                          1047 03834000
@RF01042 SLR   @07,@07                                             1047 03835000
         ST    @07,RSRRC(,REG6)                                    1047 03836000
*              RSRSKPSW = ON;                                      1048 03837000
         OI    RSRSKPSW(REG6),B'00010000'                          1048 03838000
*            END;                                                  1049 03839000
*                                                                  1049 03840000
*        /************************************************************/ 03841000
*        /*                                                          */ 03842000
*        /*   IF TRANSLATE FAILED, SET SKIP PROCESSING FLAG TO       */ 03843000
*        /*   CAUSE ALL REMAINING CODE IN THIS PROC TO BE BYPASSED   */ 03844000
*        /*                                                          */ 03845000
*        /************************************************************/ 03846000
*                                                                  1050 03847000
*        IF TESTRC = RCXLERR                                       1050 03848000
*          THEN                                                    1050 03849000
@RF01040 CLC   TESTRC(2,REG6),@CH00175                             1050 03850000
         BNE   @RF01050                                            1050 03851000
*            RSRSKPSW = ON;                                        1051 03852000
*                                                                  1051 03853000
         OI    RSRSKPSW(REG6),B'00010000'                          1051 03854000
*        /************************************************************/ 03855000
*        /*                                                          */ 03856000
*        /*   IF THE ARGUMENT WAS NOT FOUND IN ANY OF THE RMBS,      */ 03857000
*        /*   THEN A GET UPDATE MUST FIRST BE DONE FOR THE RECORD    */ 03858000
*        /*                                                          */ 03859000
*        /************************************************************/ 03860000
