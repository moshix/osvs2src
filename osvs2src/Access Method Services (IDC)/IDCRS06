*      GENERATE;                                                   0296 00059000
         DS    0F                                                       00060000
NAME06CD DC    CL8'IDCRS6CD'                                            00061000
*IDCRS6CD: PROCEDURE                                               0297 00062000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0297 00063000
*                                                                  0297 00064000
         B     @PB00002                                            0297 00065000
IDCRS6CD DS    0H                                                  0298 00066000
*      /*SAVE AND PRIME REGISTERS*/                                0298 00067000
*      GENERATE REFS(RS06RG08, REG6);                              0298 00068000
*                                                                  0298 00069000
         ENTRY IDCRS6CD  EXTERNAL ENTRY POINT                           00070000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          00071000
         LR    @15,@13  OLD SAVE AREA                                   00072000
         LA    @13,72(@13)  NEW SAVE AREA                               00073000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     00074000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        00075000
         LM    @08,@12,RS06RG08(REG6)  PRIME BASE AND DATA REGS         00076000
         MVC   0(4,@15),NAME06CD+4 PROCNAME TO REGSAVE                  00077000
*        RFY REG1 RESTRICTED;                                      0299 00078000
*                                                                  0299 00079000
*        CALL CRADMNT; /*REG1 HAS PARAM LIST ADDR*/                0300 00080000
*                                                                  0300 00081000
         BAL   @14,CRADMNT                                         0300 00082000
*        RFY REG1;                                                 0301 00083000
*                                                                  0301 00084000
*           /*RESTORE CALLER'S REGISTERS AND RETURN*/              0302 00085000
*           DO;                                                    0302 00086000
*             GENERATE;                                            0303 00087000
         L     @13,4(,@13)  RESTORE CALLER'S REG SAVE                   00088000
         L     @14,12(,@13)  RESTORE RETURN ADDRESS                     00089000
         LM    @00,@12,20(@13)  RESTORE REGS                            00090000
         MVI   12(@13),255  MARK END OF SAVE LIST                       00091000
*             RETURN CODE(REG15); /*RETURN*/                       0304 00092000
@EL00002 DS    0H                                                  0304 00093000
@EF00002 DS    0H                                                  0304 00094000
@ER00002 BR    @14                                                 0304 00095000
*           END;                                                   0305 00096000
*                                                                  0305 00097000
*           CALL EXIT;  /*TERMINATE*/                              0306 00098000
*           END IDCRS6CD; /*END OF PROCEDURE*/                     0307 00099000
@PB00002 DS    0H                                                  0308 00100000
*                                                                  0308 00101000
*      GENERATE;                                                   0308 00102000
         DS    0F                                                       00103000
NAME0002 DC    CL8'CRADMNT'                                             00104000
*CRADMNT: PROCEDURE (CRADDDP)                                      0309 00105000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0309 00106000
*                                                                  0309 00107000
         B     @PB00003                                            0309 00108000
CRADMNT  MVC   @PC00003(4),0(@01)                                  0309 00109000
*      GENERATE REFS(RS06RG08, REG6);                              0310 00110000
*                                                                  0310 00111000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          00112000
         LR    @15,@13  OLD SAVE AREA                                   00113000
         LA    @13,72(@13)  NEW SAVE AREA                               00114000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     00115000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        00116000
         MVC   0(4,@15),NAME0002  PROCNAME TO REGSAVE                   00117000
*         /***********************************************************/ 00118000
*         /*                                                         */ 00119000
*         /*  ARGUMENT OF INVOCATION AND DDNAME                      */ 00120000
*         /*                                                         */ 00121000
*         /***********************************************************/ 00122000
*                                                                  0311 00123000
*         DCL CRADDDP PTR(31);         /*ADDRESS OF DDNAME           */ 00124000
*                                                                  0311 00125000
*         DCL CRADUDN CHAR(8) BASED(CRADDDP); /*DDNAME               */ 00126000
*                                                                  0312 00127000
*         RFY ALLAGL BASED(RSALLAGP);                              0313 00128000
*                                                                  0313 00129000
*         OLDERID2 = NEWERID2;                                     0314 00130000
         L     @07,GDTTR2(,REG5)                                   0314 00131000
         MVC   @TS00001(95),NEWERID2(@07)                          0314 00132000
         MVC   OLDERID2(95,@07),@TS00001                           0314 00133000
*         NEWID2 = 'RSCD';                                         0315 00134000
*                                                                  0315 00135000
         MVC   NEWID2(4,@07),@CC03488                              0315 00136000
*         /***********************************************************/ 00137000
*         /*                                                         */ 00138000
*         /*  BUILD THE ALLAGL FOR THE UDEALLOC REQUEST              */ 00139000
*         /*                                                         */ 00140000
*         /***********************************************************/ 00141000
*                                                                  0316 00142000
*         RSALLAGP = ADDR(RSALLAGL);                               0316 00143000
         L     @07,RSIOTBL(,REG6)                                  0316 00144000
         LA    @07,RSALLAGL(,@07)                                  0316 00145000
         ST    @07,RSALLAGP(,REG6)                                 0316 00146000
*         ALLAGL = ''B;                                            0317 00147000
         XC    ALLAGL(31,@07),ALLAGL(@07)                          0317 00148000
*         ALLDDN = CRADUDN;                                        0318 00149000
*                                                                  0318 00150000
         L     @04,@PC00003                                        0318 00151000
         L     @04,CRADDDP(,@04)                                   0318 00152000
         MVC   ALLDDN(8,@07),CRADUDN(@04)                          0318 00153000
*         /***********************************************************/ 00154000
*         /*                                                         */ 00155000
*         /*  ISSUE UDEALLOC REQUEST                                 */ 00156000
*         /*                                                         */ 00157000
*         /***********************************************************/ 00158000
*                                                                  0319 00159000
*         CALL IDCSADL0 (GDTTBL,ALLAGL);                           0319 00160000
*                                                                  0319 00161000
         ST    REG5,@AL00001                                       0319 00162000
         ST    @07,@AL00001+4                                      0319 00163000
         MVI   @AL00001+4,X'80'                                    0319 00164000
         L     @15,GDTDLC(,REG5)                                   0319 00165000
         LA    @01,@AL00001                                        0319 00166000
         BALR  @14,@15                                             0319 00167000
*         RESPECIFY RTNREG RSTD;                                   0320 00168000
*         TESTRC = RTNREG;                                         0321 00169000
         STH   RTNREG,TESTRC(,REG6)                                0321 00170000
*         RESPECIFY RTNREG UNRSTD;                                 0322 00171000
*                                                                  0322 00172000
*         /***********************************************************/ 00173000
*         /*                                                         */ 00174000
*         /*    IF UDEALLOC FAILED, SET RETURN CODE OF 4, OTHERWISE  */ 00175000
*         /*    SET RETURN CODE OF 0.                                */ 00176000
*         /*                                                         */ 00177000
*         /***********************************************************/ 00178000
*                                                                  0323 00179000
*         IF TESTRC ^= 0                                           0323 00180000
*           THEN                                                   0323 00181000
         LH    @07,TESTRC(,REG6)                                   0323 00182000
         LTR   @07,@07                                             0323 00183000
         BZ    @RF00323                                            0323 00184000
*             RSSUBRC = RCUERR1;                                   0324 00185000
         MVC   RSSUBRC(4,REG6),@CF00038                            0324 00186000
*           ELSE                                                   0325 00187000
*             RSSUBRC = 0;                                         0325 00188000
*                                                                  0325 00189000
         B     @RC00323                                            0325 00190000
@RF00323 SLR   @07,@07                                             0325 00191000
         ST    @07,RSSUBRC(,REG6)                                  0325 00192000
*         /***********************************************************/ 00193000
*         /*                                                         */ 00194000
*         /*  PROCESSING COMPLETED. RETURN.                          */ 00195000
*         /*                                                         */ 00196000
*         /***********************************************************/ 00197000
*                                                                  0326 00198000
*         IF GDTDBG = NULLPTR                                      0326 00199000
*           THEN;                                                  0326 00200000
@RC00323 L     @07,GDTDBG(,REG5)                                   0326 00201000
         LTR   @07,@07                                             0326 00202000
         BZ    @RT00326                                            0326 00203000
*           ELSE                                                   0328 00204000
*             CALL IDCDB010 (GDTTBL,'RSC6');                       0328 00205000
*                                                                  0328 00206000
         ST    REG5,@AL00001                                       0328 00207000
         LA    @07,@CC03490                                        0328 00208000
         ST    @07,@AL00001+4                                      0328 00209000
         MVI   @AL00001+4,X'80'                                    0328 00210000
         L     @15,GDTDBG(,REG5)                                   0328 00211000
         LA    @01,@AL00001                                        0328 00212000
         BALR  @14,@15                                             0328 00213000
*         /*RESTORE CALLER'S REGISTERS AND RETURN*/                0329 00214000
*         DO;                                                      0329 00215000
@RT00326 DS    0H                                                  0330 00216000
*           GENERATE;                                              0330 00217000
         L     @13,4(,@13)  RESTORE CALLER'S REG SAVE                   00218000
         L     @14,12(,@13)  RESTORE RETURN ADDRESS                     00219000
         LM    @00,@12,20(@13)  RESTORE REGS                            00220000
         MVI   12(@13),255  MARK END OF SAVE LIST                       00221000
*           RETURN CODE(RSSUBRC); /*RETURN*/                       0331 00222000
         L     @15,RSSUBRC(,REG6)                                  0331 00223000
@EL00003 DS    0H                                                  0331 00224000
@EF00003 DS    0H                                                  0331 00225000
@ER00003 BR    @14                                                 0331 00226000
*         END;                                                     0332 00227000
*                                                                  0332 00228000
* CALL EXIT;  /*TERMINATE*/                                        0333 00229000
* END CRADMNT; /*END OF PROCEDURE*/                                0334 00230000
@PB00003 DS    0H                                                  0335 00231000
*/********************************************************************/ 00232000
*/*                                                                  */ 00233000
