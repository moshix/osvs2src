*        /*                                                          */ 02488000
*        /************************************************************/ 02489000
*                                                                  0805 02490000
*            IF RSRWF = ON                                         0805 02491000
*              THEN                                                0805 02492000
         TM    RSRWF(REG6),B'10000000'                             0805 02493000
         BNO   @RF00805                                            0805 02494000
*                DO;                                               0806 02495000
*                                                                  0806 02496000
*        /************************************************************/ 02497000
*        /*                                                          */ 02498000
*        /*   ARG IS AN RRN, NO TRANSLATION NECESSARY                */ 02499000
*        /*                                                          */ 02500000
*        /************************************************************/ 02501000
*                                                                  0807 02502000
*                  IF RABRRN = ON                                  0807 02503000
*                    THEN                                          0807 02504000
         L     @07,RSRGRBPT(,REG6)                                 0807 02505000
         L     @07,GRABCURR(,@07)                                  0807 02506000
         TM    RABRRN(@07),B'00010000'                             0807 02507000
         BNO   @RF00807                                            0807 02508000
*                      DO;                                         0808 02509000
*                        IOCREL = RABRCDA;                         0809 02510000
*                                                                  0809 02511000
         L     @04,IOCSPTR(,REG6)                                  0809 02512000
         L     @07,RABRCDA(,@07)                                   0809 02513000
         ST    @07,IOCREL(,@04)                                    0809 02514000
*        /************************************************************/ 02515000
*        /*                                                          */ 02516000
*        /*   IF REQUEST IS FOR GET UPDATE, SAVE THE ARGUMENT IN     */ 02517000
*        /*   THE RMB.                                               */ 02518000
*        /*                                                          */ 02519000
*        /************************************************************/ 02520000
*                                                                  0810 02521000
*                        IF RMBGETU = ON                           0810 02522000
*                          THEN                                    0810 02523000
         L     @04,RSRMBP(,REG6)                                   0810 02524000
         TM    RMBGETU(@04),B'00000100'                            0810 02525000
         BNO   @RF00810                                            0810 02526000
*                            RMBARG = IOCREL;                      0811 02527000
*                                                                  0811 02528000
         ST    @07,RMBARG(,@04)                                    0811 02529000
*                      END;                                        0812 02530000
*                                                                  0812 02531000
*        /************************************************************/ 02532000
*        /*                                                          */ 02533000
*        /*   ARG IS A CATCI, TRANSLATION IS NECESSARY               */ 02534000
*        /*                                                          */ 02535000
*        /************************************************************/ 02536000
*                                                                  0813 02537000
*                    ELSE                                          0813 02538000
*                      DO;                                         0813 02539000
