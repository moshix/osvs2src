*DCL MODID CHAR(4) INIT('RS06');       /*MODID FOR UEPIL             */ 00033000
*                                                                  0294 00034000
*DCL                                                               0295 00035000
*   1 PATCH    CHAR(1024)              /*PATCH AREA                  */ 00036000
*              LOCAL STATIC BDY(WORD), /* 5 PERCENT                  */ 00037000
*     2 *      CHAR(12)                /*PATCHAREA                   */ 00038000
*              INIT('PATCHAREA 1K'),   /*IDENTIFIER                  */ 00039000
*     2 *      CHAR(1012)              /*FORMAT UNUSED AREA          */ 00040000
*              INIT('0');              /*WITH ZEROS                  */ 00041000
*/********************************************************************/ 00042000
*/*                                                                  */ 00043000
*/* PROCEDURE NAME - CRADMNT                                         */ 00044000
*/*                                                                  */ 00045000
*/* FUNCTION - REQUEST CRA VOLUME DEALLOCATION                       */ 00046000
*/*                                                                  */ 00047000
