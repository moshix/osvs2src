@RF00740 MVC   RSRWFRN(2,REG6),@CH00212                            0742 02135000
*                  END;                                            0743 02136000
@RC00740 DS    0H                                                  0744 02137000
*              RSRRCPY = RSRWFRN;                                  0744 02138000
@RF00737 LH    @07,RSRWFRN(,REG6)                                  0744 02139000
         STH   @07,RSRRCPY(,REG6)                                  0744 02140000
*            END;                                                  0745 02141000
*                                                                  0745 02142000
*        /************************************************************/ 02143000
*        /*                                                          */ 02144000
*        /* INITRCD REQUEST, SET LOGICAL ERROR MESSAGE               */ 02145000
*        /* SUPPRESSION FLAG                                         */ 02146000
*        /*                                                          */ 02147000
*        /************************************************************/ 02148000
*                                                                  0746 02149000
*        IF RSRINIT = ON                                           0746 02150000
*          THEN                                                    0746 02151000
