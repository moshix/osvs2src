*          END IDCRS6CM; /*END OF PROCEDURE*/                      0346 00291000
@PB00004 DS    0H                                                  0347 00292000
*                                                                  0347 00293000
*      GENERATE;                                                   0347 00294000
         DS    0F                                                       00295000
NAME0007 DC    CL8'CRAMNT'                                              00296000
*CRAMNT: PROCEDURE (CRAMVLP,CRAMULP,CRAMDDP)                       0348 00297000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0348 00298000
*                                                                  0348 00299000
         B     @PB00005                                            0348 00300000
CRAMNT   MVC   @PC00005(12),0(@01)                                 0348 00301000
*      GENERATE REFS(RS06RG08, REG6);                              0349 00302000
*                                                                  0349 00303000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          00304000
         LR    @15,@13  OLD SAVE AREA                                   00305000
         LA    @13,72(@13)  NEW SAVE AREA                               00306000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     00307000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        00308000
         MVC   0(4,@15),NAME0007  PROCNAME TO REGSAVE                   00309000
*         /***********************************************************/ 00310000
*         /*                                                         */ 00311000
*         /*  ARGUMENTS OF INVOCATION AND DDNAME AREA                */ 00312000
*         /*                                                         */ 00313000
*         /***********************************************************/ 00314000
*                                                                  0350 00315000
*         DCL CRAMVLP PTR(31);         /*ADDRESS OF VOLUME LIST      */ 00316000
*         DCL CRAMULP PTR(31);         /*ADDRESS OF UNIT NAME LIST   */ 00317000
*         DCL CRAMDDP PTR(31);         /*ADDRESS OF DDNAME AREA      */ 00318000
*                                                                  0352 00319000
*         DCL CRAMUDN CHAR(8) BASED(CRAMDDP); /*DDNAME AREA          */ 00320000
*                                                                  0353 00321000
*         /***********************************************************/ 00322000
*         /*                                                         */ 00323000
*         /* VOLUME LIST AND UNIT LIST                               */ 00324000
*         /*                                                         */ 00325000
*         /***********************************************************/ 00326000
*                                                                  0354 00327000
*         DCL 1 CRAMVL BASED(CRAMVLP), /*VOLUME LIST                 */ 00328000
*               2 CRAMVLLN FIXED(8),   /* LENGTH OF VOLSER           */ 00329000
*               2 CRAMVLSR CHAR(6);    /* VOLSER                     */ 00330000
*         DCL 1 CRAMUL BASED(CRAMULP), /*UNIT NAME LIST              */ 00331000
*               2 CRAMULLN FIXED(8),   /* LENGTH OF UNIT NAME        */ 00332000
*               2 CRAMULNM CHAR(8);    /* UNIT NAME                  */ 00333000
*                                                                  0355 00334000
*         RFY ALLAGL BASED(RSALLAGP);                              0356 00335000
*                                                                  0356 00336000
*         OLDERID2 = NEWERID2;                                     0357 00337000
         L     @07,GDTTR2(,REG5)                                   0357 00338000
         MVC   @TS00001(95),NEWERID2(@07)                          0357 00339000
         MVC   OLDERID2(95,@07),@TS00001                           0357 00340000
*         NEWID2 = 'RSCM';                                         0358 00341000
*                                                                  0358 00342000
         MVC   NEWID2(4,@07),@CC03513                              0358 00343000
*         /***********************************************************/ 00344000
*         /*                                                         */ 00345000
*         /*  BUILD THE ALLAGL FOR THE UALLOC REQUEST                */ 00346000
*         /*                                                         */ 00347000
*         /***********************************************************/ 00348000
*                                                                  0359 00349000
*         RSALLAGP = ADDR(RSALLAGL);                               0359 00350000
         L     @07,RSIOTBL(,REG6)                                  0359 00351000
         LA    @04,RSALLAGL(,@07)                                  0359 00352000
         ST    @04,RSALLAGP(,REG6)                                 0359 00353000
*         ALLAGL = ''B;                                            0360 00354000
*                                                                  0360 00355000
         XC    ALLAGL(31,@04),ALLAGL(@04)                          0360 00356000
*         /***********************************************************/ 00357000
*         /*                                                         */ 00358000
*         /*    INDICATE VOLUME MOUNT REQUEST.                       */ 00359000
*         /*                                                         */ 00360000
*         /***********************************************************/ 00361000
*                                                                  0361 00362000
*         ALLOPTVL = ON;                                           0361 00363000
*                                                                  0361 00364000
         OI    ALLOPTVL(@04),B'10000000'                           0361 00365000
*         /***********************************************************/ 00366000
*         /*                                                         */ 00367000
*         /*    CONSTRUCT VOLUME LIST FOR UALLOC                     */ 00368000
*         /*                                                         */ 00369000
*         /***********************************************************/ 00370000
*                                                                  0362 00371000
*         ALLVLP = ADDR(ALLVL);                                    0362 00372000
         LA    @15,ALLVL(,@07)                                     0362 00373000
         ST    @15,ALLVLP(,@04)                                    0362 00374000
*         ALLVLC = 1;                                              0363 00375000
         LA    @15,1                                               0363 00376000
         STH   @15,ALLVLC(,@07)                                    0363 00377000
*         ALLVLLN = CRAMVLLN;                                      0364 00378000
         L     @14,@PC00005                                        0364 00379000
         L     @14,CRAMVLP(,@14)                                   0364 00380000
         SLR   @03,@03                                             0364 00381000
         IC    @03,CRAMVLLN(,@14)                                  0364 00382000
         STH   @03,ALLVLLN(,@07)                                   0364 00383000
*         ALLVLSR = CRAMVLSR(1:CRAMVLLN);                          0365 00384000
*                                                                  0365 00385000
         MVI   ALLVLSR+1(@07),C' '                                 0365 00386000
         MVC   ALLVLSR+2(4,@07),ALLVLSR+1(@07)                     0365 00387000
         BCTR  @03,0                                               0365 00388000
         EX    @03,@SM03971                                        0365 00389000
*         /***********************************************************/ 00390000
*         /*                                                         */ 00391000
*         /*    CONSTRUCT UNIT NAME LIST FOR UALLOC                  */ 00392000
*         /*                                                         */ 00393000
*         /***********************************************************/ 00394000
*                                                                  0366 00395000
*         ALLULP = ADDR(ALLUL);                                    0366 00396000
         LA    @14,ALLUL(,@07)                                     0366 00397000
         ST    @14,ALLULP(,@04)                                    0366 00398000
*         ALLULC = 1;                                              0367 00399000
*                                                                  0367 00400000
         STH   @15,ALLULC(,@07)                                    0367 00401000
*         /***********************************************************/ 00402000
*         /*                                                         */ 00403000
*         /*    IF NO UNIT NAME SUPPLIED, USE SYSALLDA               */ 00404000
*         /*                                                         */ 00405000
*         /***********************************************************/ 00406000
*                                                                  0368 00407000
*         IF CRAMULLN = 0                                          0368 00408000
*           THEN                                                   0368 00409000
         L     @04,@PC00005+4                                      0368 00410000
         L     @04,CRAMULP(,@04)                                   0368 00411000
         CLI   CRAMULLN(@04),0                                     0368 00412000
         BNE   @RF00368                                            0368 00413000
*             DO;                                                  0369 00414000
*               ALLULLN = LENGTH(SYSALLDA);                        0370 00415000
         MVC   ALLULLN(2,@07),@CH00175                             0370 00416000
*               ALLULNM = SYSALLDA;                                0371 00417000
         MVC   ALLULNM(8,@07),@CC00774                             0371 00418000
*             END;                                                 0372 00419000
*           ELSE                                                   0373 00420000
*             DO;                                                  0373 00421000
         B     @RC00368                                            0373 00422000
@RF00368 DS    0H                                                  0374 00423000
*               ALLULLN = CRAMULLN;                                0374 00424000
         L     @07,RSIOTBL(,REG6)                                  0374 00425000
         L     @04,@PC00005+4                                      0374 00426000
         L     @04,CRAMULP(,@04)                                   0374 00427000
         SLR   @15,@15                                             0374 00428000
         IC    @15,CRAMULLN(,@04)                                  0374 00429000
         STH   @15,ALLULLN(,@07)                                   0374 00430000
*               ALLULNM = CRAMULNM;                                0375 00431000
         MVC   ALLULNM(8,@07),CRAMULNM(@04)                        0375 00432000
*             END;                                                 0376 00433000
*                                                                  0376 00434000
*         /***********************************************************/ 00435000
*         /*                                                         */ 00436000
*         /* ISSUE UALLOC REQUEST                                    */ 00437000
*         /*                                                         */ 00438000
*         /***********************************************************/ 00439000
*                                                                  0377 00440000
*         CALL IDCSAAL0 (GDTTBL,ALLAGL);                           0377 00441000
