         STH   @04,SETRC(,REG6)                                    0731 02318000
*        IF RPLRTNCD ^= '00'X THEN     /* IF ERROR                   */ 02319000
         CLI   RPLRTNCD(@07),0                                     0732 02320000
         BE    @RF00732                                            0732 02321000
*          DO;                                                     0733 02322000
*            IF RPLRTNCD ^= '08'X THEN    /*IF PHYSICAL ERROR        */ 02323000
         CLI   RPLRTNCD(@07),8                                     0734 02324000
