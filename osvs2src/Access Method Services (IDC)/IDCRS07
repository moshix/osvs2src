         MVC   @TS00001(95),NEWERID2(@07)                          0555 01466000
         MVC   OLDERID2(95,@07),@TS00001                           0555 01467000
*        NEWID2 = 'RSCC';                                          0556 01468000
*                                                                  0556 01469000
         MVC   NEWID2(4,@07),@CC03971                              0556 01470000
*        RSNN = 0;                     /*CLEAR THE NUMBER OF TRACKS  */ 01471000
         SLR   @07,@07                                             0557 01472000
         STH   @07,RSNN(,REG6)                                     0557 01473000
*        RSTT = (RSCNCC1*VOLSRNTK) + RSCNHH1;                      0558 01474000
         L     @07,RSCCHHP(,REG6)                                  0558 01475000
         L     @04,RSVTBENT(,REG6)                                 0558 01476000
