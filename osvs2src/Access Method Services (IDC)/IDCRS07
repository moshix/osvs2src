*          DO;                                                     0602 01712000
*                                                                  0602 01713000
*        /************************************************************/ 01714000
*        /*                                                          */ 01715000
*        /* UPDATE THE CCR HIGH RBAS FROM THE RECORD                 */ 01716000
*        /* MANAGEMENT CONTROL BLOCKS (ARDB)                         */ 01717000
*        /*                                                          */ 01718000
*        /************************************************************/ 01719000
*                                                                  0603 01720000
*            RFY (REG0,REG1) RESTRICTED;                           0603 01721000
*             CALL EOVPRBAP;       /*PT TO RM CB HI RBA FIELDS   */     01722000
         BAL   @14,EOVPRBAP                                        0604 01723000
*            DO REG0=1 TO NOCCRRBA;    /*PROCESS THE HI RBAS         */ 01724000
         LA    REG0,1                                              0605 01725000
@DL00605 DS    0H                                                  0606 01726000
*              REG1=RBAPTRA(REG0);     /*PT TO NXT HI RBA FIELD      */ 01727000
         LR    @07,REG0                                            0606 01728000
         SLA   @07,2                                               0606 01729000
         LA    @04,8                                               0606 01730000
         AL    @04,RSEOVRAB(,REG6)                                 0606 01731000
         LA    @15,0(@07,@04)                                      0606 01732000
         AL    @15,@CF04173                                        0606 01733000
         L     REG1,RBAPTRA(,@15)                                  0606 01734000
*              CCRRBAS(REG0) = RBASLOT;/*MOVE HI RBA TO CCR          */ 01735000
         L     @15,RBASLOT(,REG1)                                  0607 01736000
         ALR   @04,@07                                             0607 01737000
         STCM  @15,15,CCRRBAS-4(@04)                               0607 01738000
*            END;                                                  0608 01739000
         AL    REG0,@CF00212                                       0608 01740000
         C     REG0,@CF00032                                       0608 01741000
         BNH   @DL00605                                            0608 01742000
*            RFY (REG0,REG1);          /*REVERT                      */ 01743000
*                                                                  0609 01744000
*            ARAKEY=ARAKEY&&ARAKEY;   /*RESTORE CCR KEY,ID           */ 01745000
         LA    @07,8                                               0610 01746000
         AL    @07,RSEOVRAB(,REG6)                                 0610 01747000
         XC    ARAKEY(44,@07),ARAKEY(@07)                          0610 01748000
*            ARACI=CATCRID;                                        0611 01749000
         MVC   ARACI(3,@07),@CF00158+1                             0611 01750000
*            ARAID=CCRTYPE;                                        0612 01751000
*                                                                  0612 01752000
         MVI   ARAID(@07),C'L'                                     0612 01753000
*        /************************************************************/ 01754000
*        /*                                                          */ 01755000
*        /* UPDATE THE FREE CI INFORMATION IN THE CCR                */ 01756000
*        /*                                                          */ 01757000
*        /************************************************************/ 01758000
*                                                                  0613 01759000
*            ARACCRD=CAXCCR;          /*MOVE CTL FLDS TO CCR      */    01760000
*                                                                  0613 01761000
         L     @04,CAXWAPTR(,REG6)                                 0613 01762000
         MVC   ARACCRD(12,@07),CAXCCR(@04)                         0613 01763000
*        /************************************************************/ 01764000
*        /*                                                          */ 01765000
*        /* UPDATE THE CCR                                           */ 01766000
*        /*                                                          */ 01767000
*        /************************************************************/ 01768000
*                                                                  0614 01769000
*                                                                  0614 01770000
*             XIOOPT = 0 ;              /* PUT OPTIONS              */  01771000
         MVI   XIOOPT(REG6),X'00'                                  0614 01772000
*Z034XIO:     CALL EOVPXIO;                                        0615 01773000
