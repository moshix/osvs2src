*/*    IN THE ARDBS.                                                 */ 01856000
*/*    REG2 = ADDRESS OF FIRST DATA ARDB                             */ 01857000
*/*    REG3 = ADDRESS OF FIRST INDEX ARDB                            */ 01858000
*/*                                                                  */ 01859000
*/********************************************************************/ 01860000
*      GENERATE;                                                   0631 01861000
         DS    0F                                                       01862000
NAME0038 DC    CL8'EOVPRBAP'                                            01863000
*EOVPRBAP: PROCEDURE                                               0632 01864000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0632 01865000
         B     @PB00008                                            0632 01866000
EOVPRBAP DS    0H                                                  0633 01867000
*        RFY REG14 RESTRICTED;                                     0633 01868000
*                                      /*BLD TBL OF HI RBA FLD PTRS  */ 01869000
*        REG1=AMBLDTA->AMBDSB->AMDPARDB;/*PT TO 1ST DATA ARDB        */ 01870000
         L     @15,RSCATACB(,REG6)                                 0634 01871000
         L     @15,ACBAMBL(,@15)                                   0634 01872000
         L     @15,AMBLDTA(,@15)                                   0634 01873000
         L     @15,AMBDSB(,@15)                                    0634 01874000
         L     REG1,AMDPARDB(,@15)                                 0634 01875000
*        RFY (REG0,REG1,REG2,REG3) RSTD;                           0635 01876000
*        REG2=REG1;                    /*SAVE 1ST DATA ARDB PT       */ 01877000
*                                                                  0636 01878000
         LR    REG2,REG1                                           0636 01879000
*        /************************************************************/ 01880000
*        /*                                                          */ 01881000
*        /* GET THE TWELVE HIGH RBAS FOR THE CATALOG - SEE THE CCR   */ 01882000
*        /* DESCRIPTION IN THE CATALOG PLM                           */ 01883000
*        /*                                                          */ 01884000
*        /************************************************************/ 01885000
*                                                                  0637 01886000
*        DO REG0=1 BY 2 TO NOCCRRBA;   /* PROCESS 12 HI RBAS         */ 01887000
         LA    REG0,1                                              0637 01888000
@DL00637 DS    0H                                                  0638 01889000
*          IF REG0 < INDXARDB THEN     /*IF PROCESSING DATA ARDBS    */ 01890000
         C     REG0,@CF00152                                       0638 01891000
         BNL   @RF00638                                            0638 01892000
*            DO;                                                   0639 01893000
*              RBAPTRA(REG0)  =  ADDR(ARDHKRBA);/*ST HI KEY RBA PTR  */ 01894000
         LR    @15,REG0                                            0640 01895000
         SLA   @15,2                                               0640 01896000
         L     @07,RSEOVRAB(,REG6)                                 0640 01897000
         LA    @04,ARDHKRBA(,REG1)                                 0640 01898000
         ST    @04,RBAPTRA+4(@15,@07)                              0640 01899000
*              REG0=REG0+1;            /*ADJUST ARRAY INDEX          */ 01900000
         AL    REG0,@CF00212                                       0641 01901000
*            END;                                                  0642 01902000
*          RBAPTRA(REG0)  =  ADDR(ARDHRBA); /*ST HI USED RBA PT      */ 01903000
@RF00638 LR    @15,REG0                                            0643 01904000
         SLA   @15,2                                               0643 01905000
         LA    @07,8                                               0643 01906000
         AL    @07,RSEOVRAB(,REG6)                                 0643 01907000
         LA    @04,ARDHRBA(,REG1)                                  0643 01908000
         ST    @15,@TF00001                                        0643 01909000
         LA    @15,0(@15,@07)                                      0643 01910000
         AL    @15,@CF04173                                        0643 01911000
         ST    @04,RBAPTRA(,@15)                                   0643 01912000
*          RBAPTRA(REG0+1)  =  ADDR(ARDERBA); /*ST HI AL RBA PTR     */ 01913000
         LA    @15,ARDERBA(,REG1)                                  0644 01914000
         L     @04,@TF00001                                        0644 01915000
         ST    @15,RBAPTRA(@04,@07)                                0644 01916000
*          IF ARDNPTR=0 THEN           /*IF END OF ARDB CH           */ 01917000
         L     @15,ARDNPTR(,REG1)                                  0645 01918000
         LTR   @15,@15                                             0645 01919000
         BNZ   @RF00645                                            0645 01920000
*            DO;                                                   0646 01921000
*              REG1=AMBLIX->AMBDSB->AMDPARDB; /*PT TO 1ST INDEX ARDB */ 01922000
         L     @15,RSCATACB(,REG6)                                 0647 01923000
         L     @15,ACBAMBL(,@15)                                   0647 01924000
         L     @15,AMBLIX(,@15)                                    0647 01925000
         L     @15,AMBDSB(,@15)                                    0647 01926000
         L     REG1,AMDPARDB(,@15)                                 0647 01927000
*              REG3=REG1;              /*SAVE 1ST INDEX ARDB PT      */ 01928000
         LR    REG3,REG1                                           0648 01929000
*            END;                                                  0649 01930000
*          ELSE                                                    0650 01931000
*            REG1=ARDNPTR;             /*PT TO NXT DATA/INDEX ARDB   */ 01932000
         B     @RC00645                                            0650 01933000
@RF00645 L     REG1,ARDNPTR(,REG1)                                 0650 01934000
*        END;                                                      0651 01935000
@RC00645 AL    REG0,@CF00149                                       0651 01936000
         C     REG0,@CF00032                                       0651 01937000
         BNH   @DL00637                                            0651 01938000
*        RFY (REG0,REG1,REG2,REG3);    /*REVERT                      */ 01939000
*  RETURN;                                                         0653 01940000
@EL00008 DS    0H                                                  0653 01941000
@EF00008 DS    0H                                                  0653 01942000
@ER00008 BR    @14                                                 0653 01943000
*  RFY REG14; /*REVERT*/                                           0654 01944000
*  END EOVPRBAP; /*END OF PROCEDURE*/                              0655 01945000
@PB00008 DS    0H                                                  0656 01946000
*/********************************************************************/ 01947000
*/*                                                                  */ 01948000
*/* PROCEDURE - EOVPRCCR                                             */ 01949000
*/*                                                                  */ 01950000
*/* FUNCTION -                                                       */ 01951000
*/*    READ THE CCR AND UPDATE THE HIGH ALLOCATED AND USED           */ 01952000
*/*    CINS IN THE RECORD MANAGEMENT CONTROL BLOCKS.                 */ 01953000
*/*                                                                  */ 01954000
*/*    NOTE *** THIS ROUTINE RUNS IN SUPERVISOR STATE, KEY 0.        */ 01955000
*/*                                                                  */ 01956000
*/*    NOTE *** THIS ROUTINE IS MODELLED AFTER IGGPRCCR IN IGG0CLAG  */ 01957000
*/*                                                                  */ 01958000
*/* INPUT -                                                          */ 01959000
*/*    PRINCIPAL INPUT CONSISTS OF THE CATALOG ACB AND AN RPL        */ 01960000
*/*                                                                  */ 01961000
*/* OUTPUT -                                                         */ 01962000
*/*    UPDATED RECORD MANAGEMENT CONTROL BLOCKS.                     */ 01963000
*/*                                                                  */ 01964000
*/********************************************************************/ 01965000
*      GENERATE;                                                   0656 01966000
         DS    0F                                                       01967000
NAME0040 DC    CL8'EOVPRCCR'                                            01968000
*EOVPRCCR: PROCEDURE                                               0657 01969000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0657 01970000
*                                                                  0657 01971000
         B     @PB00009                                            0657 01972000
EOVPRCCR DS    0H                                                  0658 01973000
*      GENERATE REFS(RS07RG08, REG6);                              0658 01974000
*                                                                  0658 01975000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          01976000
         LR    @15,@13  OLD SAVE AREA                                   01977000
         LA    @13,72(@13)  NEW SAVE AREA                               01978000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     01979000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        01980000
         MVC   0(4,@15),NAME0040  PROCNAME TO REGSAVE                   01981000
*        /************************************************************/ 01982000
*        /*                                                          */ 01983000
*        /* GET THE CCR                                              */ 01984000
*        /*                                                          */ 01985000
*        /************************************************************/ 01986000
*                                                                  0659 01987000
*        RABRCDA = CATCRID * CILEN ;   /*RBA OF CCR                  */ 01988000
*                                                                  0659 01989000
         L     @07,RSEOVRAB(,REG6)                                 0659 01990000
         MVC   RABRCDA(4,@07),@CF04147                             0659 01991000
*         XIOOPT = 136 ;                /* GET OPTIONS              */  01992000
         MVI   XIOOPT(REG6),X'88'                                  0660 01993000
*Z041XIO: CALL EOVPXIO;                /*GET THE CATLG CTL RCD       */ 01994000
Z041XIO  BAL   @14,EOVPXIO                                         0661 01995000
*                                      /*OP=ADR-UPD-CIE----          */ 01996000
*                                                                  0662 01997000
*        /************************************************************/ 01998000
*        /*                                                          */ 01999000
*        /* UPDATE THE RECORD MANAGEMENT CONTROL BLOCKS              */ 02000000
*        /*                                                          */ 02001000
*        /************************************************************/ 02002000
*                                                                  0662 02003000
*        IF SETRC = 0 THEN             /* IF NO ERROR SO FAR         */ 02004000
         LH    @07,SETRC(,REG6)                                    0662 02005000
         LTR   @07,@07                                             0662 02006000
         BNZ   @RF00662                                            0662 02007000
*          DO;                                                     0663 02008000
*            CAXCCR = ARACCRD ;        /*MOVE CCR CTL FLDS TO CAXWA  */ 02009000
         L     @07,CAXWAPTR(,REG6)                                 0664 02010000
         L     @04,RSEOVRAB(,REG6)                                 0664 02011000
         MVC   CAXCCR(12,@07),ARACCRD+8(@04)                       0664 02012000
*            RFY (REG0,REG1,REG2,REG3) RSTD;                       0665 02013000
*             CALL EOVPRBAP;       /*PT TO RM CB HI RBA FIELDS   */     02014000
*                                                                  0666 02015000
         BAL   @14,EOVPRBAP                                        0666 02016000
*        /************************************************************/ 02017000
*        /*                                                          */ 02018000
*        /*     UPDATE THE HIGH RBAS IN THE ARDBS. SINCE             */ 02019000
*        /*     CATALOG MAY HAVE BEEN EXTENDED BY ANOTHER CPU, THE   */ 02020000
*        /*     GEOMETRY MAY BE DIFFERENT THAN THE ONE WE HAVE.      */ 02021000
*        /*                                                          */ 02022000
*        /************************************************************/ 02023000
*                                                                  0667 02024000
*            DO REG0=1 TO NOCCRRBA;    /*PROCESS THE HI RBAS         */ 02025000
         LA    REG0,1                                              0667 02026000
@DL00667 DS    0H                                                  0668 02027000
*              REG1=RBAPTRA(REG0);     /*RETRIEVE PTR TO NXT CB FLD  */ 02028000
         LR    @07,REG0                                            0668 02029000
         SLA   @07,2                                               0668 02030000
         LA    @04,8                                               0668 02031000
         AL    @04,RSEOVRAB(,REG6)                                 0668 02032000
         LA    @15,0(@07,@04)                                      0668 02033000
         AL    @15,@CF04173                                        0668 02034000
         L     REG1,RBAPTRA(,@15)                                  0668 02035000
*              RBASLOT=MAX(RBASLOT,CCRRBAS(REG0));                 0669 02036000
         L     @15,RBASLOT(,REG1)                                  0669 02037000
         ALR   @04,@07                                             0669 02038000
         ICM   @07,15,CCRRBAS-4(@04)                               0669 02039000
         CLR   @15,@07                                             0669 02040000
         BNL   *+6                                                      02041000
         LR    @15,@07                                             0669 02042000
         ST    @15,RBASLOT(,REG1)                                  0669 02043000
*                                      /*MOVE HI RBA FROM CCR TO CB  */ 02044000
*            END;                                                  0670 02045000
*                                                                  0670 02046000
         AL    REG0,@CF00212                                       0670 02047000
         C     REG0,@CF00032                                       0670 02048000
         BNH   @DL00667                                            0670 02049000
*            REG1=REG2;                /* 1ST DATA ARDB PTR          */ 02050000
*                                                                  0671 02051000
         LR    REG1,REG2                                           0671 02052000
*        /************************************************************/ 02053000
*        /*                                                          */ 02054000
*        /*     SET NEW END OF DATA FOR HIGH RBA INDEX AND           */ 02055000
*        /*     HIGH RBA DATA - IN THE ARDBS                         */ 02056000
*        /*                                                          */ 02057000
*        /************************************************************/ 02058000
*                                                                  0672 02059000
*            DO REG0=1 TO NOARDBS;     /* FIND, MARK EOD ARDB        */ 02060000
         LA    REG0,1                                              0672 02061000
@DL00672 DS    0H                                                  0673 02062000
*              REG2->ARDEOD=OFF;       /*CLEAR OLD EOD MARK          */ 02063000
         NI    ARDEOD(REG2),B'11110111'                            0673 02064000
*              IF REG1->ARDERBA<REG2->ARDERBA THEN /*COMP HI AL RBA  */ 02065000
         CLC   ARDERBA(4,REG1),ARDERBA(REG2)                       0674 02066000
         BNL   @RF00674                                            0674 02067000
*                REG1=REG2;            /*REG1 PT TO HI RBA ARD       */ 02068000
         LR    REG1,REG2                                           0675 02069000
*              IF REG2->ARDNPTR=0 THEN /*END OF ARDB CHN             */ 02070000
@RF00674 L     @07,ARDNPTR(,REG2)                                  0676 02071000
         LTR   @07,@07                                             0676 02072000
         BNZ   @RF00676                                            0676 02073000
*                DO;                   /* YES                        */ 02074000
*                  REG1->ARDEOD=ON;    /*MARK NEW EOD ARDB           */ 02075000
         OI    ARDEOD(REG1),B'00001000'                            0678 02076000
*                  REG2=REG3;          /*PT TO 1ST IX ARDB           */ 02077000
         LR    REG2,REG3                                           0679 02078000
*                  REG1=REG2;          /*PT TO 1ST IX ARDB           */ 02079000
         LR    REG1,REG2                                           0680 02080000
*                END;                                              0681 02081000
*              ELSE                    /*NOT LAST ARDB               */ 02082000
*                REG2=REG2->ARDNPTR;   /*PT TO NEXT ARDB             */ 02083000
         B     @RC00676                                            0682 02084000
@RF00676 L     REG2,ARDNPTR(,REG2)                                 0682 02085000
*            END;                                                  0683 02086000
@RC00676 AL    REG0,@CF00212                                       0683 02087000
         C     REG0,@CF00280                                       0683 02088000
         BNH   @DL00672                                            0683 02089000
*            RFY (REG0,REG1,REG2,REG3); /*REVERT                     */ 02090000
*          END;                                                    0685 02091000
*  CALL EXIT;  /*TERMINATE*/                                       0686 02092000
@RF00662 BAL   @14,EXIT                                            0686 02093000
*  END EOVPRCCR; /*END OF PROCEDURE*/                              0687 02094000
@EL00009 DS    0H                                                  0687 02095000
@EF00009 DS    0H                                                  0687 02096000
@ER00009 BR    @14                                                 0687 02097000
@PB00009 DS    0H                                                  0688 02098000
*/********************************************************************/ 02099000
*/*                                                                  */ 02100000
*/* PROCEDURE - EOVPWFLR                                             */ 02101000
*/*                                                                  */ 02102000
*/* FUNCTION -                                                       */ 02103000
*/*    WRITE A DELETED FREE RECORD TO THE CATALOG.                   */ 02104000
*/*                                                                  */ 02105000
*/*    NOTE *** THIS ROUTINE RUNS IN SUPERVISOR STATE, KEY 0.        */ 02106000
*/*                                                                  */ 02107000
*/*    NOTE *** THIS ROUTINE IS MODELLED AFTER IGGPWFLR IN IGG0CLAG  */ 02108000
*/*                                                                  */ 02109000
*/* INPUT -                                                          */ 02110000
*/*    PRINCIPAL INPUT CONSISTS OF THE CAXWA FIELDS:                 */ 02111000
*/*      CAXNFCI - FIRST UNFORMATTED FREE CI                         */ 02112000
*/*      CAXCDCI - NUMBER OF DELETED FREE CIS                        */ 02113000
*/*      CAXFDCI - FIRST DELETED FREE CI                             */ 02114000
*/*                                                                  */ 02115000
*/* OUTPUT -                                                         */ 02116000
*/*    A DELETED FREE RECORD IS WRITTEN.                             */ 02117000
*/*                                                                  */ 02118000
*/********************************************************************/ 02119000
*      GENERATE;                                                   0688 02120000
         DS    0F                                                       02121000
NAME0044 DC    CL8'EOVPWFLR'                                            02122000
*EOVPWFLR: PROCEDURE                                               0689 02123000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0689 02124000
*                                                                  0689 02125000
         B     @PB00010                                            0689 02126000
EOVPWFLR DS    0H                                                  0690 02127000
*      GENERATE REFS(RS07RG08, REG6);                              0690 02128000
*                                                                  0690 02129000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          02130000
         LR    @15,@13  OLD SAVE AREA                                   02131000
         LA    @13,72(@13)  NEW SAVE AREA                               02132000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     02133000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        02134000
         MVC   0(4,@15),NAME0044  PROCNAME TO REGSAVE                   02135000
*        /************************************************************/ 02136000
*        /*                                                          */ 02137000
*        /* CHAIN THE FREE RECORD TO THE LAST DELETED FREE           */ 02138000
*        /*                                                          */ 02139000
*        /************************************************************/ 02140000
*                                                                  0691 02141000
*        ARACI = CAXNFCI ;             /*SET CI #                    */ 02142000
         L     @07,RSEOVRAB(,REG6)                                 0691 02143000
         LA    @04,8                                               0691 02144000
         ALR   @04,@07                                             0691 02145000
         L     @15,CAXWAPTR(,REG6)                                 0691 02146000
         MVC   ARACI(3,@04),CAXNFCI(@15)                           0691 02147000
*        RABRCDA = ADDR(ARAKEY);       /*SET UP KEY                  */ 02148000
         ST    @04,RABRCDA(,@07)                                   0692 02149000
*        ARAFCHN = CAXFDCI ;           /*INSERT CI INTO DEL CHN      */ 02150000
*                                                                  0693 02151000
         MVC   ARAFCHN(3,@04),CAXFDCI(@15)                         0693 02152000
*        /************************************************************/ 02153000
*        /*                                                          */ 02154000
*        /* ADD THE DELETED FREE RECORD                              */ 02155000
*        /*                                                          */ 02156000
*        /************************************************************/ 02157000
*                                                                  0694 02158000
*                                                                  0694 02159000
*          XIOOPT = 28 ;                /* PUT OPTIONS              */  02160000
         MVI   XIOOPT(REG6),X'1C'                                  0694 02161000
*Z045XIO:  CALL EOVPXIO;               /*PUT PREFORMAT CI            */ 02162000
Z045XIO  BAL   @14,EOVPXIO                                         0695 02163000
*                                      /*OP=KEY-NUP-LET-CIE-KEQ      */ 02164000
*                                                                  0696 02165000
*        /************************************************************/ 02166000
*        /*                                                          */ 02167000
*        /* UPDATE THE CAXWA CCR INFORMATION                         */ 02168000
*        /*                                                          */ 02169000
*        /************************************************************/ 02170000
*                                                                  0696 02171000
*        IF SETRC = 0 THEN             /*IF NO ERROR ON PUT          */ 02172000
         LH    @07,SETRC(,REG6)                                    0696 02173000
         LTR   @07,@07                                             0696 02174000
         BNZ   @RF00696                                            0696 02175000
*          DO;                                                     0697 02176000
*            CAXFDCI = ARACI ;         /*SET NEW DEL CI CHN PTR      */ 02177000
         L     @07,CAXWAPTR(,REG6)                                 0698 02178000
         L     @04,RSEOVRAB(,REG6)                                 0698 02179000
         MVC   CAXFDCI(3,@07),ARACI+8(@04)                         0698 02180000
*            CAXCDCI = CAXCDCI +1 ;    /*INCR DEL CI COUNT           */ 02181000
         LA    @04,1                                               0699 02182000
         SLR   @15,@15                                             0699 02183000
         ICM   @15,7,CAXCDCI(@07)                                  0699 02184000
         ALR   @15,@04                                             0699 02185000
         STCM  @15,7,CAXCDCI(@07)                                  0699 02186000
*            CAXNFCI = CAXNFCI +1 ;    /*INCR NXT ASSIGNED CI #      */ 02187000
         SLR   @15,@15                                             0700 02188000
         ICM   @15,7,CAXNFCI(@07)                                  0700 02189000
         ALR   @04,@15                                             0700 02190000
         STCM  @04,7,CAXNFCI(@07)                                  0700 02191000
*          END;                                                    0701 02192000
*  CALL EXIT;  /*TERMINATE*/                                       0702 02193000
@RF00696 BAL   @14,EXIT                                            0702 02194000
*  END EOVPWFLR; /*END OF PROCEDURE*/                              0703 02195000
@EL00010 DS    0H                                                  0703 02196000
@EF00010 DS    0H                                                  0703 02197000
@ER00010 BR    @14                                                 0703 02198000
@PB00010 DS    0H                                                  0704 02199000
*/********************************************************************/ 02200000
*/*                                                                  */ 02201000
*/* PROCEDURE -  EOVPXIO                                             */ 02202000
*/*                                                                  */ 02203000
*/* FUNCTION -                                                       */ 02204000
*/*    INTERFACE TO VSAM GET AND PUT FUNCTIONS                       */ 02205000
*/*                                                                  */ 02206000
*/*    NOTE *** THIS ROUTINE RUNS IN SUPERVISOR STATE KEY 0          */ 02207000
*/*                                                                  */ 02208000
*/*    NOTE *** THIS ROUTINE IS MODELLED AFTER IGGPXIO IN IGG0CLAG   */ 02209000
*/*                                                                  */ 02210000
*/* INVOCATION -                                                     */ 02211000
*/*    ISSUE ?EXIO MACRO INSTRUCTION WHICH EXPANDS TO:               */ 02212000
*/*            XIOOPT = #;       SET OPTIONS                         */ 02213000
*/*            CALL EOVPXIO;                                         */ 02214000
*/*                                                                  */ 02215000
*/*      # - AN 8 BIT CODE WHICH DESCRIBES THE TYPE OF FUNCTION TO   */ 02216000
*/*          BE PERFORMED.                                           */ 02217000
*/*                                                                  */ 02218000
*/*      BIT#    BINARY VALUE    BIT=0        BIT=1                  */ 02219000
*/*                                                                  */ 02220000
*/*       0         128          PUT          GET                    */ 02221000
*/*       1          64          PUT                                 */ 02222000
*/*       2          32          (RESERVED)                          */ 02223000
*/*       3          16          ADDRESSED    KEYED                  */ 02224000
*/*       4           8          UPDATE       NON UPDATE             */ 02225000
*/*       5           4          LET          NO LET                 */ 02226000
*/*       6           2          C.I. RECORD  TRUE-NAME RECORD       */ 02227000
*/*       7           1             -         KEY GREATER/EQUAL      */ 02228000
*/*                                                                  */ 02229000
*/********************************************************************/ 02230000
*      GENERATE;                                                   0704 02231000
         DS    0F                                                       02232000
NAME0047 DC    CL8'EOVPXIO'                                             02233000
*EOVPXIO: PROCEDURE                                                0705 02234000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0705 02235000
*                                                                  0705 02236000
         B     @PB00011                                            0705 02237000
EOVPXIO  DS    0H                                                  0706 02238000
*      GENERATE REFS(RS07RG08, REG6);                              0706 02239000
*                                                                  0706 02240000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          02241000
         LR    @15,@13  OLD SAVE AREA                                   02242000
         LA    @13,72(@13)  NEW SAVE AREA                               02243000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     02244000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        02245000
         MVC   0(4,@15),NAME0047  PROCNAME TO REGSAVE                   02246000
*        RPLRLEN = CELEN;              /* SET NORMAL ENT LENGTH      */ 02247000
         L     @07,RSCATRPL(,REG6)                                 0707 02248000
         MVC   RPLRLEN(4,@07),@CF00738                             0707 02249000
*        RPLAREA = ADDR(RABBUF);       /* SET PTR TO BUFFER          */ 02250000
         L     @04,RSEOVRAB(,REG6)                                 0708 02251000
         LA    @15,RABBUF(,@04)                                    0708 02252000
         ST    @15,RPLAREA(,@07)                                   0708 02253000
*        RPLARG = ADDR(RABRCDA);       /*SET UP SEARCH ARG PTR       */ 02254000
*                                                                  0709 02255000
         LA    @04,RABRCDA(,@04)                                   0709 02256000
         ST    @04,RPLARG(,@07)                                    0709 02257000
*        RPLDIR=ON;                    /* SET UP TO DO               */ 02258000
*        RPLSEQ=OFF;                   /*       DIRECT OPERATION     */ 02259000
*        RPLKGE = OFF;                 /* CLEAR PERTINENT OPTIONS    */ 02260000
         OI    RPLDIR(@07),B'01000000'                             0712 02261000
         NI    RPLSEQ(@07),B'11011011'                             0712 02262000
*        RPLKEY = OFF;                                             0713 02263000
*        RPLADR = OFF;                                             0714 02264000
*        RPLCNV = OFF;                 /* CLEAR CNV OPTION           */ 02265000
*        RPLUPD = OFF;                                             0716 02266000
*                                                                  0716 02267000
         NI    RPLKEY(@07),B'00011101'                             0716 02268000
*        IF XIOKEY = ON THEN           /*IF KEYED REQ.               */ 02269000
         TM    XIOKEY(REG6),B'00010000'                            0717 02270000
         BNO   @RF00717                                            0717 02271000
*          RPLKEY = ON ;               /* SET KEYED REQ. BIT         */ 02272000
         OI    RPLKEY(@07),B'10000000'                             0718 02273000
*        ELSE                          /*OTHERWISE                   */ 02274000
*          DO;                         /* SETUP CI PROCESS           */ 02275000
         B     @RC00717                                            0719 02276000
@RF00717 DS    0H                                                  0720 02277000
*            RPLRLEN = CILEN;          /* SET CI LENGTH              */ 02278000
