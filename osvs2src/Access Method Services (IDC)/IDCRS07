*              2 ARDERBA      FIXED(32),/* HIGH ALLOCATED RBA        */ 00673000
*              2 ARDVOLSR     CHAR(6),  /* SER OF HIGH RBA VOLUME    */ 00674000
*              2 ARDRELNO     FIXED(15),/* I/O SUPPORT-CATALOG INFO  */ 00675000
*              2 ARDPRF BIT(8),         /* PREFORMAT BYTE            */ 00676000
*                                       /* CORRESPONDS TO FIELD      */ 00677000
*                                       /*  ITYPEXT OF THE VSAM      */ 00678000
*                                       /*  CATALOG                  */ 00679000
*                3 ARDPRSS BIT(1),      /* SEQ SET WITH DATA         */ 00680000
*                3 ARDPRFMT BIT(1),     /* EXTENTS NOT PREFORMATTED  */ 00681000
*                3 * BIT(6),            /* RESERVED AND MUST AGREE   */ 00682000
*                                       /*  WITH ITYPEXT             */ 00683000
*              2 ARDKEYS      CHAR(*);  /* LOW AND HIGH KEY VALUES   */ 00684000
*                                       /* FOR KEY RANGE ENTRY,      */ 00685000
*                                       /* SIZE = 2*AMDKEYLN         */ 00686000
*        RFY IFGRPL BASED(RSCATRPL);   /*BASE ON RPL PTR FOR CATEOV  */ 00687000
*        RFY RSRAB BASED(RSEOVRAB);    /*CATALOG RAB FOR EOV         */ 00688000
*        RFY IFGACB BASED(RSCATACB);   /* CATALOG ACB                */ 00689000
*        RFY IDAAMBL BASED(ACBAMBL);                               0376 00690000
*        RFY IDAAMB  BASED(AMBLDTA);                               0377 00691000
*        RFY IDAAMDSB BASED(AMBDSB);                               0378 00692000
*        RFY IDAARDB BASED(REG1);                                  0379 00693000
*        /* CATALOG RECORD STRUCTURE - IN CATALOG RECORD AREA        */ 00694000
*          DCL 1 ARA BASED(ADDR(RABBUF)), /* RECORD AREA             */ 00695000
*                2 ARASGA CHAR(255)  , /* 1ST HALF                   */ 00696000
*                  3 ARAKEY CHAR(44) , /* KEY OF RECORD              */ 00697000
*                    4 * CHAR(1)     , /* FILLER                     */ 00698000
*                    4 ARACI PTR(24) BDY(BYTE), /* CI # OF RECORD    */ 00699000
*                    4 * CHAR(40)    , /* FILLER                     */ 00700000
*                  3 ARATNCI PTR(24) BDY(BYTE), /* TRUE NAME ENT CI# */ 00701000
*                  3 * CHAR(208)     , /* FILLER                     */ 00702000
*                2 ARASGB CHAR(250)  , /* 2HD HALF                   */ 00703000
*                2 ARARDF CHAR(7)    ; /* RDF FIELD IN RCD           */ 00704000
*        /* CATALOG LOW KEY RANGE RECORD                             */ 00705000
*          DCL 1 ARA2 BASED(ADDR(RABBUF)), /* FREE RECORD            */ 00706000
*                2 * CHAR(44)        , /* FILLER                     */ 00707000
*                2 ARAID CHAR(1)     , /* RECORD ID                  */ 00708000
*                2 ARACCRD CHAR(12)  , /* CCR DATA                   */ 00709000
*                  3 ARAFCHN CHAR(3) , /* FREE CHAIN PTR (TYPE F)    */ 00710000
*                  3 * CHAR(9)       ; /* FILLER                     */ 00711000
*        /* CATALOG RBA ARRAY FOR SAVING THE CATALOG RBAS            */ 00712000
*          DCL RBAPTRA (12) PTR(31) BASED(ADDR(RABBUF));           0382 00713000
*                                      /* HI RBA FLD PTR ARRAY       */ 00714000
*        /* RBA WORK VARIABLE FOR RETRIEVING ALL RBA DATA OF CAT     */ 00715000
*          DCL RBASLOT FIXED(32) BASED(REG1);  /*CURRENT HI RBA      */ 00716000
*        /* STRUCTURE FOR THE I/O OPTIONS PASSED TO EOVPXIO          */ 00717000
*          DCL 1 XIOOPT PTR(8) DEF(RSEOVXIO), /*I/O OPTIONS          */ 00718000
*                2 XIOGET BIT(1)           , /* ON=GET OFF=PUT      */  00719000
*                2 XIOERS BIT(1)           , /* ON=ERASE            */  00720000
*                2 *      BIT(1)           , /* RESERVED             */ 00721000
*                2 XIOKEY BIT(1)           , /* ON=KEY OFF=ADR      */  00722000
*                2 XIONUP BIT(1)           , /* ON=NVP OFF=UPD      */  00723000
*                2 XIOLET BIT(1)           , /* ON=LET OFF=NLET     */  00724000
*                2 XIOTNE BIT(1)           , /* ON=TNE OFF=NORM ENT */  00725000
*                2 XIOKGE BIT(1)           , /* ON=GET NEXT (GET)   */  00726000
*                  3 XIOSEQ BIT(1)         ; /* ON=PUT SEQ  (PUT)   */  00727000
*        /* EXTENDED RPL STRUCTURE                                   */ 00728000
*          DCL 1 XTNDRPL BASED(RSCATRPL), /* EXTENDED RPL            */ 00729000
*                2 * CHAR(LENGTH(IFGRPL))  , /* SPACE FOR RPL        */ 00730000
*                2 RXTN CHAR(4)            , /* RPL EXTENSION        */ 00731000
*                  3 RXTNAV CHAR(1)        , /* AVAIL. LOCK BYTE     */ 00732000
*                  3 *    CHAR(3)          ; /* RESERVED             */ 00733000
*        /* RPL POOL STRUCTURE FOR THE CATALOG                       */ 00734000
*          DCL RPLPOOL (*) CHAR(LENGTH(XTNDRPL))                   0386 00735000
*                          BASED(CAXRPL) ;   /* CATALOG RPL POOL     */ 00736000
*        /* VARIOUS CONSTANT AND STRUCTURE DECLARATIONS              */ 00737000
*  DECLARE                                                         0387 00738000
*      CATCLID  PTR(24) CONSTANT(2),   /* CATLG CLUSTER CI #         */ 00739000
*      CIRESCNT PTR(24) CONSTANT(1),   /* NUMBER OF CIS TO RESERVE   */ 00740000
*      MRECIS   PTR(15) CONSTANT(2),   /* MAX CIS REQUIRED FOR EOV   */ 00741000
*      CATCRID  PTR(24) CONSTANT(3),   /* CCR CI #                   */ 00742000
*      CCRTYPE  CHAR(1) CONSTANT('L'), /* CCR TYPE CODE              */ 00743000
*      NOARDBS  FIXED(8) CONSTANT(5),  /* NO. OF CAT ARDBS           */ 00744000
*      NOCCRRBA FIXED(8) CONSTANT(12), /* NO. OF CCR RBAS            */ 00745000
*      INDXARDB FIXED(8) CONSTANT(6);  /* NO. ARDBS PLUS ONE         */ 00746000
*        /* MODESET MACRO DECLARATIONS                               */ 00747000
*  DECLARE MODELSTS GENERATED LABEL;   /*MODESET SUPR - KEY 0        */ 00748000
*  DECLARE MODELSTP GENERATED LABEL;   /*MODESET PROB STATE - USERKEY*/ 00749000
*  DECLARE MODES    CHAR(4) DEFINED(MODELSTS); /*PARAM LIST SUPR     */ 00750000
*  DECLARE MODEP    CHAR(4) DEFINED(MODELSTP); /*PARAM LIST PROB     */ 00751000
*  DECLARE MLST     CHAR(4) BDY(WORD); /*MODESET PARAM LIST         */  00752000
*  DCL 1 CLVLST BASED,                  /*CAMLST VOLUME LIST         */ 00753000
*        2 CLVLCT   FIXED(15),          /*COUNT OF ENTRIES           */ 00754000
*        2 CLVLENT(*) CHAR(12),         /*VOLUME ENTRIES             */ 00755000
*          3 CLVLDVT  CHAR(4),          /*VOLUME DEVICE TYPE         */ 00756000
*          3 CLVLSR   CHAR(6),          /*VOLSER                     */ 00757000
*          3 *        CHAR(1),          /*NOT USED                   */ 00758000
*          3 CLVLSTAT CHAR(1);          /*SVC STATUS                 */ 00759000
*       /*  RENAME SVC CAMLST DECLARES                       @ZA18274*/ 00760000
*  DCL CLISTR GENERATED LABEL;          /*CAMLST MACRO LIST  @ZA18274*/ 00761000
*                                       /* LABEL             @ZA18274*/ 00762000
*  DCL CAMLSTR CHAR(16) DEF(CLISTR);                       /*@ZA18274*/ 00763000
*                                       /*GEND CAMLST MACRO  @ZA18274*/ 00764000
*/********************************************************************/ 00765000
*/*                                                                  */ 00766000
*/* PROCEDURE - CATEOV                                               */ 00767000
*/*                                                                  */ 00768000
*/* FUNCTION -                                                       */ 00769000
*/*    CATEOV CONTROLS THE FORCE EXTEND OF THE LKR OF THE            */ 00770000
*/*    CATALOG.                                                      */ 00771000
*/*                                                                  */ 00772000
*/*    NOTE *** THIS ROUTINE RUNS IN SUPERVISOR STATE, KEY 0.        */ 00773000
*/*                                                                  */ 00774000
*/* INPUT -                                                          */ 00775000
*/*    PRINCIPAL INPUT CONSISTS OF:                                  */ 00776000
*/*      RSCAHACI - HIGH ALLOCATED CI IN LKR                         */ 00777000
*/*      RSREASSN - NUMBER OF CINS TO REASSIGN                       */ 00778000
*/*      RSAVAILN - NUMBER OF CINS AVAILABLE FOR REASSIGN            */ 00779000
*/*      RSCATACB - ADDRESS OF THE JOB/STEPCAT ACB                   */ 00780000
*/*      RSCAFUCN - NUMBER OF UNFORMATTED FREES LEFT INVALID         */ 00781000
*/*                                                                  */ 00782000
*/* OUTPUT -                                                         */ 00783000
*/*    REG15 = 0 IF OK                                               */ 00784000
*/*    REG15 ^= 0 IF ERROR                                           */ 00785000
*/*                                                                  */ 00786000
*/********************************************************************/ 00787000
*      GENERATE;                                                   0396 00788000
         DS    0F                                                       00789000
NAME0002 DC    CL8'CATEOV'                                              00790000
*CATEOV: PROCEDURE                                                 0397 00791000
*        OPTIONS(NOSAVE,NOSAVEAREA);                               0397 00792000
*                                                                  0397 00793000
         B     @PB00002                                            0397 00794000
CATEOV   DS    0H                                                  0398 00795000
*      GENERATE REFS(RS07RG08, REG6);                              0398 00796000
*                                                                  0398 00797000
         STM   @14,@12,12(@13)  SAVE REGISTERS                          00798000
         LR    @15,@13  OLD SAVE AREA                                   00799000
         LA    @13,72(@13)  NEW SAVE AREA                               00800000
         ST    @13,8(@15)  FORWARD CHAIN OLD TO NEW                     00801000
         ST    @15,4(@13)  BACK CHAIN NEW TO OLD                        00802000
         ENTRY CATEOV  ENTRY POINT                                      00803000
         LM    @08,@12,RS07RG08(REG6)  PRIME BASE AND DATA REGS         00804000
         MVC   0(4,@15),NAME0002  PROCNAME TO REGSAVE                   00805000
*        RSEOVLOG = OFF;               /*RESET LOGICAL ERROR         */ 00806000
*        RSEOVPHY = OFF;               /*RESET PHYSICAL ERROR        */ 00807000
*                                                                  0400 00808000
         NI    RSEOVLOG(REG6),B'11001111'                          0400 00809000
*        /************************************************************/ 00810000
*        /*                                                          */ 00811000
*        /* COMPUTE THE TARGET CIN FOR EXTENSION:                    */ 00812000
*        /*     TARGET CIN =                                         */ 00813000
*        /*       THE CURRENTLY HIGH ALLOCATED DATA CIN        +     */ 00814000
*        /*       THE ADDITIONAL CINS REQUIRED FOR                   */ 00815000
*        /*         REASSIGNMENT (# TO BE REASSIGNED -               */ 00816000
*        /*         # AVAILABLE FOR REASSIGNMENT)              +     */ 00817000
*        /*       3 FREE CINS TO BE LEFT UNFORMATTED. THIS           */ 00818000
*        /*         IS TO ENSURE ENOUGH FREE CINS TO HOLD THE        */ 00819000
*        /*         ADDITIONAL RECORDS REQUIRED TO DESCRIBE          */ 00820000
*        /*         A NEW EXTENT OF THE CATALOG (3 -                 */ 00821000
*        /*         THE NUMBER ALREADY ACCOUNTED FOR)                */ 00822000
*        /*                                                          */ 00823000
*        /************************************************************/ 00824000
*                                                                  0401 00825000
*        RSCATARG = RSCAHACI + (RSREASSN - RSAVAILN) +             0401 00826000
*          (CIRESCNT + MRECIS - RSCAFUCN);                         0401 00827000
*                                                                  0401 00828000
         L     @07,RSREASSN(,REG6)                                 0401 00829000
         SL    @07,RSAVAILN(,REG6)                                 0401 00830000
         AL    @07,RSCAHACI(,REG6)                                 0401 00831000
         LA    @04,3                                               0401 00832000
         SH    @04,RSCAFUCN(,REG6)                                 0401 00833000
         ALR   @07,@04                                             0401 00834000
         STCM  @07,7,RSCATARG(REG6)                                0401 00835000
*        /************************************************************/ 00836000
*        /*                                                          */ 00837000
*        /*  ENTER SUPERVISOR STATE, KEY ZERO TO WORK ON              * 00838000
*        /*  CATALOG CONTROL BLOCKS                                  */ 00839000
*        /*                                                          */ 00840000
*        /************************************************************/ 00841000
*                                                                  0402 00842000
*        MLST = MODES;                 /*MODESET PARAM LIST          */ 00843000
         MVC   MLST(4),MODES                                       0402 00844000
*        RFY REG1 RESTRICTED;                                      0403 00845000
*        REG1 = ADDR(MLST);                                        0404 00846000
*                                                                  0404 00847000
         LA    REG1,MLST                                           0404 00848000
* CATSUPR:                                                         0405 00849000
*        GEN(MODESET RELATED=CATPROB,MF=(E,(1)));                  0405 00850000
*                                                                  0405 00851000
CATSUPR  MODESET RELATED=CATPROB,MF=(E,(1))                             00852000
*        RFY REG1;                     /*REVERT                      */ 00853000
*                                                                  0406 00854000
*        /************************************************************/ 00855000
*        /*                                                          */ 00856000
*        /*  POINT TO RAB (BUFFER) FOR USE IN EXTENDING THE CATALOG  */ 00857000
*        /*                                                          */ 00858000
*        /************************************************************/ 00859000
*                                                                  0407 00860000
*        RSEOVRAB = ADDR(GRABSCA)->GRABRAB1;                       0407 00861000
*                                                                  0407 00862000
         L     @07,RSIOTBL(,REG6)                                  0407 00863000
         L     @07,GRABRAB1+40(,@07)                               0407 00864000
         ST    @07,RSEOVRAB(,REG6)                                 0407 00865000
*        /************************************************************/ 00866000
*        /*                                                          */ 00867000
*        /*  GET AN RPL FROM THE CATALOG'S RPL POOL                  */ 00868000
*        /*                                                          */ 00869000
*        /*  NOTE***THIS IS MODELLED AFTER IGG0CLA3 (IGGPRPLM)       */ 00870000
*        /*                                                          */ 00871000
*        /************************************************************/ 00872000
*                                                                  0408 00873000
*        CAXWAPTR = ACBUAPTR;          /*CAXWA ADDRESS              */  00874000
         L     @07,RSCATACB(,REG6)                                 0408 00875000
         L     @07,ACBUAPTR(,@07)                                  0408 00876000
         ST    @07,CAXWAPTR(,REG6)                                 0408 00877000
*        RSEOVFND = OFF;               /*IND NO RPL FOUND            */ 00878000
         NI    RSEOVFND(REG6),B'01111111'                          0409 00879000
*        RFY REG1 RESTRICTED;                                      0410 00880000
*                                                                  0410 00881000
* CATRPLG:                                                         0411 00882000
*        DO RSIDX = 1 TO CAXRPLCT WHILE (RSEOVFND = OFF);          0411 00883000
CATRPLG  LA    @07,1                                               0411 00884000
         B     @DE00411                                            0411 00885000
@DL00411 TM    RSEOVFND(REG6),B'10000000'                          0411 00886000
         BNZ   @DC00411                                            0411 00887000
*          RSCATRPL = ADDR(RPLPOOL(RSIDX)); /*POINT TO AN RPL        */ 00888000
         LH    @07,RSIDX(,REG6)                                    0412 00889000
         MH    @07,@CH01043                                        0412 00890000
         L     @04,CAXWAPTR(,REG6)                                 0412 00891000
         L     @04,CAXRPL(,@04)                                    0412 00892000
         AL    @04,@CF04164                                        0412 00893000
         LA    @07,RPLPOOL(@07,@04)                                0412 00894000
         ST    @07,RSCATRPL(,REG6)                                 0412 00895000
*          REG1 = ADDR(RXTNAV);        /*POINT RPL LOCK BYTE         */ 00896000
         LA    REG1,RXTNAV(,@07)                                   0413 00897000
*          GEN(TS    0(REG1));         /*CHK IF RPL FREE             */ 00898000
         TS    0(REG1)                                                  00899000
*          GEN(BNZ CATRPLNA);          /*BR IF NOT FREE              */ 00900000
         BNZ CATRPLNA                                                   00901000
*          GEN(XR    REG1,REG1);       /*INDICATE RPL AVAILABLE      */ 00902000
         XR    REG1,REG1                                                00903000
* CATRPLNA:                                                        0417 00904000
*        IF (REG1 = 0 & RPLACTIV = 0) THEN /*CHK IF RPL FOUND        */ 00905000
CATRPLNA LTR   REG1,REG1                                           0417 00906000
         BNZ   @RF00417                                            0417 00907000
         L     @07,RSCATRPL(,REG6)                                 0417 00908000
         CLI   RPLACTIV(@07),0                                     0417 00909000
         BNE   @RF00417                                            0417 00910000
*          RSEOVFND = ON;                                          0418 00911000
         OI    RSEOVFND(REG6),B'10000000'                          0418 00912000
*        END;                                                      0419 00913000
@RF00417 LA    @07,1                                               0419 00914000
         AH    @07,RSIDX(,REG6)                                    0419 00915000
@DE00411 STH   @07,RSIDX(,REG6)                                    0419 00916000
         L     @04,CAXWAPTR(,REG6)                                 0419 00917000
         CH    @07,CAXRPLCT(,@04)                                  0419 00918000
         BNH   @DL00411                                            0419 00919000
@DC00411 DS    0H                                                  0420 00920000
*        RFY REG1;                     /*REVERT                      */ 00921000
*        /************************************************************/ 00922000
*        /*                                                          */ 00923000
*        /* FORCE END OF VOLUME UNTIL ENOUGH CINS ARE                */ 00924000
*        /* MADE AVAILABLE.                                          */ 00925000
*        /*                                                          */ 00926000
*        /************************************************************/ 00927000
*                                                                  0421 00928000
*        SETRC = 0;                    /*CLEAR ERROR                 */ 00929000
         SLR   @07,@07                                             0421 00930000
         STH   @07,SETRC(,REG6)                                    0421 00931000
*        RSEOVEND = OFF;               /*INDICATE EOV REQ'D          */ 00932000
*                                                                  0422 00933000
         NI    RSEOVEND(REG6),B'10111111'                          0422 00934000
* CATEXTND:                                                        0423 00935000
*        DO UNTIL (RSEOVEND = ON | SETRC ^= 0);                    0423 00936000
*                                                                  0423 00937000
CATEXTND DS    0H                                                  0423 00938000
@DL00423 DS    0H                                                  0424 00939000
*        /************************************************************/ 00940000
*        /*                                                          */ 00941000
*        /*     READ THE CCR AND UPDATE RECORD MANAGEMENT            */ 00942000
*        /*     CONTROL BLOCKS.                                      */ 00943000
*        /*                                                          */ 00944000
*        /************************************************************/ 00945000
*                                                                  0424 00946000
*            CALL EOVPRCCR;                                        0424 00947000
         BAL   @14,EOVPRCCR                                        0424 00948000
*          IF SETRC = 0 THEN                                       0425 00949000
         LH    @07,SETRC(,REG6)                                    0425 00950000
         LTR   @07,@07                                             0425 00951000
         BNZ   @RF00425                                            0425 00952000
*            DO;                                                   0426 00953000
*                                                                  0426 00954000
*        /************************************************************/ 00955000
*        /*                                                          */ 00956000
*        /*     FORMAT A CATALOG FREE RECORD                         */ 00957000
*        /*                                                          */ 00958000
*        /************************************************************/ 00959000
*                                                                  0427 00960000
*              ARASGA = ARASGA && ARASGA; /*CLEAR BUFFER             */ 00961000
         LA    @07,8                                               0427 00962000
         AL    @07,RSEOVRAB(,REG6)                                 0427 00963000
         XC    ARASGA(255,@07),ARASGA(@07)                         0427 00964000
*              ARASGB = ARASGB && ARASGB; /*CLEAR BUFFER             */ 00965000
         XC    ARASGB(250,@07),ARASGB(@07)                         0428 00966000
*              ARAID = 'F';            /*SET FREE RECORD ID          */ 00967000
*                                                                  0429 00968000
         MVI   ARAID(@07),C'F'                                     0429 00969000
*        /************************************************************/ 00970000
*        /*                                                          */ 00971000
*        /*     FORCE END OF VOLUME                                  */ 00972000
*        /*                                                          */ 00973000
*        /************************************************************/ 00974000
*                                                                  0430 00975000
*                CALL EOVPANCI;                                    0430 00976000
         BAL   @14,EOVPANCI                                        0430 00977000
*              IF SETRC = 0 THEN    /*IF NO ERROR CONTINUE           */ 00978000
         LH    @07,SETRC(,REG6)                                    0431 00979000
         LTR   @07,@07                                             0431 00980000
         BNZ   @RF00431                                            0431 00981000
*                DO;                                               0432 00982000
*                                                                  0432 00983000
*        /************************************************************/ 00984000
*        /*                                                          */ 00985000
*        /*     CHECKPOINT THE CCR (REFLECTS FREE CINS)              */ 00986000
*        /*                                                          */ 00987000
*        /************************************************************/ 00988000
*                                                                  0433 00989000
*                    CALL EOVPCCCR;                                0433 00990000
         BAL   @14,EOVPCCCR                                        0433 00991000
*                  IF SETRC = 0 THEN    /*IF NO ERROR CONTIN         */ 00992000
         LH    @07,SETRC(,REG6)                                    0434 00993000
         LTR   @07,@07                                             0434 00994000
         BNZ   @RF00434                                            0434 00995000
*                    DO;                                           0435 00996000
*                                                                  0435 00997000
*        /************************************************************/ 00998000
*        /*                                                          */ 00999000
*        /*     CHECK IF ENOUGH CINS ARE AVAILABLE - I.E.,           */ 01000000
*        /*     WHETHER THE TARGET CIN WAS REACHED; IF NOT THEN      */ 01001000
*        /*     ADD THE MINIMUM NUMBER OF CINS FOR EXTENSION         */ 01002000
*        /*     TO THE TARGET AND EXTEND AGAIN.                      */ 01003000
*        /*                                                          */ 01004000
*        /************************************************************/ 01005000
*                                                                  0436 01006000
*                      IF RSCATARG =< CAXHACI THEN                 0436 01007000
         L     @07,CAXWAPTR(,REG6)                                 0436 01008000
         CLC   RSCATARG(3,REG6),CAXHACI(@07)                       0436 01009000
         BH    @RF00436                                            0436 01010000
*                        RSEOVEND = ON;                            0437 01011000
         OI    RSEOVEND(REG6),B'01000000'                          0437 01012000
*                      ELSE                                        0438 01013000
*                        RSCATARG = RSCATARG +                     0438 01014000
*                          (CIRESCNT + MRECIS);                    0438 01015000
*                                                                  0438 01016000
         B     @RC00436                                            0438 01017000
@RF00436 LA    @07,3                                               0438 01018000
         L     @04,RSCATARG-1(,REG6)                               0438 01019000
         LA    @04,0(,@04)                                         0438 01020000
         ALR   @07,@04                                             0438 01021000
         STCM  @07,7,RSCATARG(REG6)                                0438 01022000
*                    END;                                          0439 01023000
@RC00436 DS    0H                                                  0440 01024000
*                END;                                              0440 01025000
@RF00434 DS    0H                                                  0441 01026000
*            END;                                                  0441 01027000
@RF00431 DS    0H                                                  0442 01028000
*        END CATEXTND;                                             0442 01029000
*                                                                  0442 01030000
@RF00425 DS    0H                                                  0442 01031000
@DE00423 TM    RSEOVEND(REG6),B'01000000'                          0442 01032000
         BO    @DC00423                                            0442 01033000
         LH    @07,SETRC(,REG6)                                    0442 01034000
         LTR   @07,@07                                             0442 01035000
         BZ    @DL00423                                            0442 01036000
@DC00423 DS    0H                                                  0443 01037000
*        /************************************************************/ 01038000
*        /*                                                          */ 01039000
*        /* FREE THE RPL                                             */ 01040000
*        /*                                                          */ 01041000
*        /************************************************************/ 01042000
*                                                                  0443 01043000
*        RXTNAV = 0;                                               0443 01044000
*                                                                  0443 01045000
         L     @07,RSCATRPL(,REG6)                                 0443 01046000
         MVI   RXTNAV(@07),X'00'                                   0443 01047000
*        /************************************************************/ 01048000
*        /*                                                          */ 01049000
*        /* RETURN TO PROBLEM STATE AND KEY                          */ 01050000
*        /*                                                          */ 01051000
*        /************************************************************/ 01052000
*                                                                  0444 01053000
*        MLST = MODEP;                 /*MODESET PARAM LIST          */ 01054000
         MVC   MLST(4),MODEP                                       0444 01055000
*        RFY REG1 RESTRICTED;                                      0445 01056000
*        REG1 = ADDR(MLST);                                        0446 01057000
*                                                                  0446 01058000
         LA    REG1,MLST                                           0446 01059000
* CATPROB:                                                         0447 01060000
*        GEN(MODESET RELATED=CATSUPR,MF=(E,(1)));                  0447 01061000
*                                                                  0447 01062000
CATPROB  MODESET RELATED=CATSUPR,MF=(E,(1))                             01063000
*        RFY REG1;                     /*REVERT                      */ 01064000
*                                                                  0448 01065000
*        /************************************************************/ 01066000
*        /*                                                          */ 01067000
*        /* PRINT ERROR MESSAGE IF I/O ERROR ENCOUNTERED             */ 01068000
*        /*                                                          */ 01069000
*        /************************************************************/ 01070000
*                                                                  0449 01071000
*        IF RSEOVPHY = ON THEN                                     0449 01072000
*                                                                  0449 01073000
