*                                      /*WRITE UPDATED CCR           */ 01775000
*                                      /*OP=ADR-UPD-NLET-CIE----     */ 01776000
*          END;                                                    0616 01777000
*  CALL EXIT;  /*TERMINATE*/                                       0617 01778000
@RF00601 BAL   @14,EXIT                                            0617 01779000
*  END EOVPCCCR; /*END OF PROCEDURE*/                              0618 01780000
@EL00006 DS    0H                                                  0618 01781000
@EF00006 DS    0H                                                  0618 01782000
@ER00006 BR    @14                                                 0618 01783000
@PB00006 DS    0H                                                  0619 01784000
