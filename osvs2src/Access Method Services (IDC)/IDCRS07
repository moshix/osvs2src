*                DO;                                               0740 02343000
*                  RSEOVLOG = ON;      /*LOGICAL ERROR               */ 02344000
         OI    RSEOVLOG(REG6),B'00100000'                          0741 02345000
*                  RSEOVERR = RPLERRCD; /*SAVE ERROR CODE            */ 02346000
         L     @07,RSCATRPL(,REG6)                                 0742 02347000
         IC    @04,RPLERRCD(,@07)                                  0742 02348000
         STC   @04,RSEOVERR(,REG6)                                 0742 02349000
*                END;                                              0743 02350000
*              ELSE                                                0744 02351000
*                DO;                                               0744 02352000
         B     @RC00739                                            0744 02353000
@RF00739 DS    0H                                                  0745 02354000
*                  SETRC = 0;          /*RESET ERROR IND             */ 02355000
*                                                                  0745 02356000
         SLR   @07,@07                                             0745 02357000
         STH   @07,SETRC(,REG6)                                    0745 02358000
*                                      /* SAVE RECORD IN BUFFER 2    */ 02359000
*                  ADDR(GRABSCA)->GRABRAB2->ARASGA = ARASGA;       0746 02360000
         L     @07,RSIOTBL(,REG6)                                  0746 02361000
         L     @07,GRABRAB2+40(,@07)                               0746 02362000
         LA    @04,8                                               0746 02363000
         AL    @04,RSEOVRAB(,REG6)                                 0746 02364000
         MVC   ARASGA(255,@07),ARASGA(@04)                         0746 02365000
*                  ADDR(GRABSCA)->GRABRAB2->ARASGB = ARASGB;       0747 02366000
*                                                                  0747 02367000
*                                                                  0747 02368000
         MVC   ARASGB(250,@07),ARASGB(@04)                         0747 02369000
*                    XIOOPT = 128 ;     /* GET OPTIONS              */  02370000
         MVI   XIOOPT(REG6),X'80'                                  0748 02371000
*Z048XIO:            CALL EOVPXIO;                                 0749 02372000
Z048XIO  BAL   @14,EOVPXIO                                         0749 02373000
*                                      /*GET RECORD FOR UPDATE       */ 02374000
*                                                                  0750 02375000
*                  IF SETRC = 0 THEN    /* IF NO ERROR               */ 02376000
         LH    @07,SETRC(,REG6)                                    0750 02377000
         LTR   @07,@07                                             0750 02378000
         BNZ   @RF00750                                            0750 02379000
