*              DO;                                                 0735 02326000
*                RSEOVPHY = ON;        /*PHYSICAL ERROR              */ 02327000
         OI    RSEOVPHY(REG6),B'00010000'                          0736 02328000
*                RSEOVERR = RPLERRCD;  /*SAVE ERROR CODE             */ 02329000
         IC    @04,RPLERRCD(,@07)                                  0737 02330000
         STC   @04,RSEOVERR(,REG6)                                 0737 02331000
*              END;                                                0738 02332000
*            ELSE                      /* LOGICAL ERROR              */ 02333000
*              IF XIOLET = OFF | RPLERRCD ^= RPLDUP THEN           0739 02334000
         B     @RC00734                                            0739 02335000
@RF00734 TM    XIOLET(REG6),B'00000100'                            0739 02336000
         BZ    @RT00739                                            0739 02337000
         L     @07,RSCATRPL(,REG6)                                 0739 02338000
