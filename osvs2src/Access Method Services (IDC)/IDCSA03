         L     REG13,TEMPPTR                                       0246 00448000
*        GEN(L     REG13,8(REG13));                                0247 00449000
         L     REG13,8(REG13)                                           00450000
*        RESPECIFY (REG13) UNRSTD;                                 0248 00451000
*                                                                  0248 00452000
*        /************************************************************/ 00453000
*        /*                                                          */ 00454000
*        /* CALCULATE ADDRESS OF AREA TO BE FREED--USER'S AUTOMATIC  */ 00455000
*        /* STORAGE AREA POINTER MINUS 8 (CONTROL INFORMATION)       */ 00456000
*        /*                                                          */ 00457000
*        /************************************************************/ 00458000
*                                                                  0249 00459000
*        TEMPPTR = TEMPPTR - 8;                                    0249 00460000
*                                                                  0249 00461000
         L     @08,TEMPPTR                                         0249 00462000
         SL    @08,@CF00173                                        0249 00463000
         ST    @08,TEMPPTR                                         0249 00464000
*        /************************************************************/ 00465000
*        /*                                                          */ 00466000
*        /* CHECK IF USER SPECIFIED A RETURN CODE                    */ 00467000
*        /*                                                          */ 00468000
*        /************************************************************/ 00469000
*                                                                  0250 00470000
*        IF LISTLN = 3                                             0250 00471000
*          THEN                                                    0250 00472000
         C     LISTLN,@CF00156                                     0250 00473000
         BNE   @RF00250                                            0250 00474000
