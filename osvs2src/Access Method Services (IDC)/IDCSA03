         LA    @01,@AL00001                                        0196 00159000
         BAL   @14,GETCORE                                         0196 00160000
*              /******************************************************/ 00161000
*              /*                                                    */ 00162000
*              /* STORE ADDRESS OF ACQUIRED CORE INTO REGISTER 5 AND */ 00163000
*              /* INSERT CONTROL INFORMATION IN FRONT OF AREA        */ 00164000
*              /*                                                    */ 00165000
