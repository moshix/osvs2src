*        TEMPID = MODID;                                           0187 00086000
*                                                                  0187 00087000
         MVC   TEMPID(4),MODID(MODIDPR)                            0187 00088000
*        /************************************************************/ 00089000
*        /*                                                          */ 00090000
*        /* DETERMINE IF MODULE ID IS CONTAINED IN           @YL026UK*/ 00091000
*        /* STORAGE TABLE                                            */ 00092000
*        /*                                                          */ 00093000
*        /************************************************************/ 00094000
*                                                                  0188 00095000
*        DOCNTRL = ON;                                             0188 00096000
         OI    DOCNTRL,B'10000000'                                 0188 00097000
*PRDO:   DO I=1 TO DIM(AUTOARAY) WHILE(DOCNTRL = ON);      /*@Y30LSMK*/ 00098000
*                                                                  0189 00099000
PRDO     LA    I,1                                                 0189 00100000
@DL00189 TM    DOCNTRL,B'10000000'                                 0189 00101000
         BNO   @DC00189                                            0189 00102000
*          /**********************************************************/ 00103000
*          /*                                                        */ 00104000
*          /* DETERMINE IF MODULE ID MATCHES ID IN           @YL026UK*/ 00105000
*          /* STORAGE TABLE                                          */ 00106000
*          /*                                                        */ 00107000
*          /**********************************************************/ 00108000
*                                                                  0190 00109000
*          IF AREAID(I) = TEMPID                                   0190 00110000
*            THEN                                                  0190 00111000
         LR    @08,I                                               0190 00112000
         MH    @08,@CH00032                                        0190 00113000
