*                    AUTOID = IDENT;                               0210 00263000
         MVC   AUTOID(4,@08),IDENT                                 0210 00264000
*                    AUTOSIZE = TEMPSIZE;                          0211 00265000
         MVC   AUTOSIZE(4,@08),TEMPSIZE                            0211 00266000
*                  END;                                            0212 00267000
*                                                                  0212 00268000
*              /******************************************************/ 00269000
*              /*                                                    */ 00270000
*              /* INCREMENT STATUS BY ONE                            */ 00271000
*              /*                                                    */ 00272000
