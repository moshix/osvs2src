         SRA   @08,16                                              0204 00212000
         LTR   @08,@08                                             0204 00213000
         BNZ   @RF00204                                            0204 00214000
*                  /**************************************************/ 00215000
*                  /*                                                */ 00216000
*                  /* INSERT ADDRESS OF PREALLOCATED FROZEN          */ 00217000
*                  /* AUTOMATIC STORAGE INTO REGISTER 5              */ 00218000
*                  /*                                                */ 00219000
*                  /**************************************************/ 00220000
*                                                                  0205 00221000
*                  REG5 = PTR1(I);                                 0205 00222000
