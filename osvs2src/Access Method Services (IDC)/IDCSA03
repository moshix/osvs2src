         LR    REG1,REG5                                           0224 00342000
*        RESPECIFY(REG5) UNRSTD;                                   0225 00343000
*        GEN(LM    REG2,REG12,PROLSAV);                            0226 00344000
         LM    REG2,REG12,PROLSAV                                       00345000
*        GEN(BR    REG14);                                         0227 00346000
         BR    REG14                                                    00347000
*        RESPECIFY (REG1,REG14) UNRSTD;                            0228 00348000
*        /************************************************************/ 00349000
*        /*                                                          */ 00350000
*        /* UEPIL ROUTINE ENTRY POINT                                */ 00351000
*        /*                                                          */ 00352000
*        /************************************************************/ 00353000
*                                                                  0229 00354000
*IDCSAEP:  ENTRY(GDTTBLX,MODIDX,RCODEX);                           0229 00355000
*                                                                  0229 00356000
@EC00229 DS    0H                                                  0230 00357000
*AMSSAEP1:                                                         0230 00358000
*                                                                  0230 00359000
*        /************************************************************/ 00360000
*        /*                                                          */ 00361000
*        /* PROLOG CODE FOR THE UEPIL ROUTINE                        */ 00362000
*        /*                                                          */ 00363000
