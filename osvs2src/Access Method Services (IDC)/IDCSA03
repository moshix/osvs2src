*        GEN(L     RCODEPR,8(REG1));                               0234 00390000
*                                                                  0234 00391000
*                                                                  0234 00392000
         L     RCODEPR,8(REG1)                                          00393000
*                                                                  0235 00394000
*        /************************************************************/ 00395000
*        /*                                                          */ 00396000
*        /* ULISTLN MACRO INCLUDED CODE                              */ 00397000
*        /*                                                          */ 00398000
*        /*     LISTLN - TO CONTAIN THE NUMBER OF ARGUMENTS PASSED   */ 00399000
*        /*              TO THIS ROUTINE                             */ 00400000
*        /*                                                          */ 00401000
*        /*     LISTPTR - TO CONTAIN THE ADDRESS OF THE CORRESPONDING*/ 00402000
*        /*               ADDRESS LIST                               */ 00403000
*        /*                                                          */ 00404000
*        /************************************************************/ 00405000
*                                                                  0235 00406000
*        GEN;                                                      0235 00407000
        ST    1,LISTPTR                                                 00408000
        ST    1,LISTSCAN                                                00409000
*        DO LISTLN = 1 TO 255 WHILE(LISTTEST = '0'B);              0236 00410000
         LA    LISTLN,1                                            0236 00411000
@DL00236 L     @10,LISTSCAN                                        0236 00412000
