*        /************************************************************/ 00305000
*        /*                                                          */ 00306000
*        /* RESTORE THE RETURN ADDRESS INTO REGISTER 14              */ 00307000
*        /*                                                          */ 00308000
*        /************************************************************/ 00309000
*                                                                  0217 00310000
*        RESPECIFY (REG14) RSTD;                                   0217 00311000
*        REG14 = RTNADDR;                                          0218 00312000
*                                                                  0218 00313000
         LR    REG14,RTNADDR                                       0218 00314000
*        /************************************************************/ 00315000
*        /*                                                          */ 00316000
*        /* RESTORE SAVEAREA POINTER AND STORE USER'S MODULE ID INTO */ 00317000
*        /* THE FIRST 4 BYTES OF THE SAVEAREA THAT HE SAVED IN       */ 00318000
*        /*                                                          */ 00319000
*        /************************************************************/ 00320000
*                                                                  0219 00321000
*        RESPECIFY (REG13) RSTD;                                   0219 00322000
*        GEN(L     REG13,4(REG13));                                0220 00323000
