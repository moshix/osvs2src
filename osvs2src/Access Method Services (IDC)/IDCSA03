*        GEN(MVC   0(4,REG13),TEMPID);                             0221 00325000
         MVC   0(4,REG13),TEMPID                                        00326000
*        RESPECIFY (REG13) UNRSTD;                                 0222 00327000
*                                                                  0222 00328000
*        /************************************************************/ 00329000
*        /*                                                          */ 00330000
*        /* THE PROL ROUTINE AUTOMATIC STORAGE AREA DOESN'T NEED     */ 00331000
*        /* TO BE RELEASED AS IT IS USED AGAIN--NOW LOAD THE ADDRESS */ 00332000
*        /* OF THE GOTTEN CORE INTO REGISTER 1, RESTORE REGISTERS    */ 00333000
*        /* 2 THROUGH 12 FROM THE SAVEAREA SAVED IN BY THIS ROUTINE  */ 00334000
*        /* (PROLSAVE) AND BRANCH ON REGISTER 14 (RETURN TO THE      */ 00335000
*        /* CALLER)                                                  */ 00336000
*        /*                                                          */ 00337000
*        /************************************************************/ 00338000
*                                                                  0223 00339000
*        RESPECIFY (REG1) RSTD;                                    0223 00340000
