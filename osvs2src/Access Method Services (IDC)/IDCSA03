*            END;                                                  0214 00290000
*                                                                  0214 00291000
*        /************************************************************/ 00292000
*        /*                                                          */ 00293000
*        /* UPDATE INTER MODULE TRACE TABLE WITH USER'S MODULE ID    */ 00294000
*        /*                                                          */ 00295000
*        /************************************************************/ 00296000
*                                                                  0215 00297000
*        OLDERID1 = NEWERID1;                                      0215 00298000
