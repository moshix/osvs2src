         L     @08,TEMPPTR                                         0198 00170000
         LA    REG5,8                                              0198 00171000
         ALR   REG5,@08                                            0198 00172000
*              AUTOID = TEMPID;                                    0199 00173000
         MVC   AUTOID(4,@08),TEMPID                                0199 00174000
*              AUTOSIZE = TEMPSIZE;                                0200 00175000
         MVC   AUTOSIZE(4,@08),TEMPSIZE                            0200 00176000
*            END;                                                  0201 00177000
*          ELSE                                                    0202 00178000
*                                                                  0202 00179000
*            /********************************************************/ 00180000
*            /*                                                      */ 00181000
*            /* MODULE ID IS CONTAINED IN STORAGE TABLE SO   @YL026UK*/ 00182000
*            /* USE THIS TABLE FOR AUTOMATIC STORAGE REQUEST         */ 00183000
*            /* ALLOCATION                                           */ 00184000
*            /*                                                      */ 00185000
*            /********************************************************/ 00186000
*                                                                  0202 00187000
*            DO;                                                   0202 00188000
