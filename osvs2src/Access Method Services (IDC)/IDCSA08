*                  ACHKPRF = ON;                           /*@G24LSBS*/ 06684000
         OI    ACHKPRF(ACHKPTR),B'00100000'                        0726 06685000
*                END;                                      /*@G24LSBS*/ 06686000
*                                                                  0727 06687000
*              ELSE                                        /*@G24LSBS*/ 06688000
*                /*******************************************@G24LSBS*/ 06689000
*                /* OTHERWISE, IF THE FLAG INDICATES AN EN-  @G24LSBS*/ 06690000
*                /* TITY AND THE ENTITY POINTER IS NOT ZERO, @G24LSBS*/ 06691000
*                /*******************************************@G24LSBS*/ 06692000
*                                                                  0728 06693000
*                IF RACFPRF = OFF &                        /*@G24LSBS*/ 06694000
