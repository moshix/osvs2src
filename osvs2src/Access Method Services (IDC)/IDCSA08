*                                ACHKTRD = ON;             /*@G24LSBS*/ 06575000
         OI    ACHKTRD(ACHKPTR),B'00000010'                        0714 06576000
*                              ELSE                        /*@G24LSBS*/ 06577000
*                                                                  0715 06578000
*                                /***************************@G24LSBS*/ 06579000
*                                /* NONE OF THE ABOVE AT-    @G24LSBS*/ 06580000
*                                /* TRIBUTES WERE SPECIFIED, @G24LSBS*/ 06581000
*                                /* SET THE INVALID URACHECK @G24LSBS*/ 06582000
*                                /* PARAMETER LIST RET CODE. @G24LSBS*/ 06583000
*                                /***************************@G24LSBS*/ 06584000
*                                URACFRC = RACFRC12;       /*@G24LSBS*/ 06585000
         B     @RC00713                                            0715 06586000
@RF00713 LA    URACFRC,12                                          0715 06587000
*              END;                                        /*@G24LSBS*/ 06588000
*            ELSE                                          /*@G24LSBS*/ 06589000
*                                                                  0717 06590000
*              /*********************************************@G24LSBS*/ 06591000
*              /* THE CLASS NAME OR LENGTH IS INVALID, SET   @G24LSBS*/ 06592000
*              /* THE INVALID URACHECK PARAMETER LIST RET CD.@G24LSBS*/ 06593000
*              /*********************************************@G24LSBS*/ 06594000
*              URACFRC = RACFRC12;                         /*@G24LSBS*/ 06595000
         B     @RC00704                                            0717 06596000
@RF00704 LA    URACFRC,12                                          0717 06597000
*        /***************************************************@G24LSBS*/ 06598000
*        /*                                                  @G24LSBS*/ 06599000
*        /* RACHECK REQUIRES A VALID VOLUME SERIAL POINTER,  @G24LSBS*/ 06600000
*        /* EXCEPT WHEN A PROFILE HAS BEEN SPECIFIED.        @G24LSBS*/ 06601000
*        /* URACHECK HAS TO ASSUME THAT IF THE POINTER TO    @G24LSBS*/ 06602000
*        /* THE VOLUME SERIAL IS NOT ZERO IT MUST BE VALID.  @G24LSBS*/ 06603000
*        /* MAKE THE TEST ONLY IF THE RET CODE IS STILL ZERO.@G24LSBS*/ 06604000
*        /*                                                  @G24LSBS*/ 06605000
*        /***************************************************@G24LSBS*/ 06606000
*                                                                  0718 06607000
*        IF RACFVOLS ^= NULLPTR & URACFRC = RACFRC0        /*@G24LSBS*/ 06608000
*          THEN                                            /*@G24LSBS*/ 06609000
*                                                                  0718 06610000
@RC00704 SLR   @09,@09                                             0718 06611000
         L     @06,RKPLIST(,PARMPTR2)                              0718 06612000
         L     @06,RACFVOLS(,@06)                                  0718 06613000
