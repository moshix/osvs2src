*        /* BLOCK                                                    */ 02862000
*        /*                                                          */ 02863000
*        /************************************************************/ 02864000
*                                                                  0204 02865000
* /*******************************************************************/ 02866000
* /*                                                                 */ 02867000
* /*                       IHADSAB                                   */ 02868000
* /*                                                                 */ 02869000
* /*               DATA SET ASSOCIATION BLOCK                        */ 02870000
* /*                                                                 */ 02871000
* /* MACMEAN DSAB MEANS DATA SET ASSOCIATION BLOCK                   */ 02872000
* /* MACCOMP Y-2 SC1B4/PZD82                                         */ 02873000
* /* MACSTAT Y-2 73082/016236                                        */ 02874000
* /*                                                                 */ 02875000
* /* THE DATA SET ASSOCIATION BLOCK, DSAB, AND ITS CORRESPONDING     */ 02876000
* /* TIOT DD ENTRY CONTAIN INFORMATION WHICH SERVE AS AN INTERFACE   */ 02877000
* /* BETWEEN ALLOCATION (BOTH STEP AND DYNAMIC) AND OTHER SYSTEM     */ 02878000
* /* COMPONENTS. DSAB'S ARE ELEMENTS OF A NON-CONTIGUOUS, DOUBLE     */ 02879000
* /* THREADED CHAIN. THIS CHAIN IS HEADED BY A QUEUE DESCRIPTOR      */ 02880000
* /* BLOCK WHICH IS ADDRESSED BY A FIELD IN THE JSCB, JSCBDSAB.      */ 02881000
* /* THE TIOT ENTRY IS ADDRESSED FROM THE DSAB FIELD, DSABTIOT.      */ 02882000
* /*                                                                 */ 02883000
* /*******************************************************************/ 02884000
*                                                                  0204 02885000
* DCL                                                              0204 02886000
*   1 DSAB BASED(DSABPTR) BDY(WORD),                               0204 02887000
*     2 DSABID CHAR(4),                /*IN-CORE ID, CHARACTERS DSAB */ 02888000
*     2 DSABFCHN PTR(31),              /*NEXT DSAB PTR,0 IF LAST     */ 02889000
*     2 DSABBCHN PTR(31),              /*PREVIOUS DSAB PTR,0 IF FIRST*/ 02890000
*     2 DSABLNTH FIXED(15),            /*LENGTH OF DSAB              */ 02891000
*     2 DSABOPCT FIXED(15),            /*OPEN DCB COUNT FOR DD ENTRY */ 02892000
*     2 DSABTIOT PTR(31),              /*TIOT DD ENTRY PTR           */ 02893000
*     2 DSABRS01 CHAR(1),              /*RESERVED                    */ 02894000
*     2 DSABSSVA PTR(24),              /*SWA VIRTUAL ADDRESS OF SIOT */ 02895000
*     2 DSABGIDP PTR(31),              /*DEVICE GROUP-ID LIST PTR    */ 02896000
*     2 DSABANMP PTR(31),              /*&NAME OR GDG-ALL DSNAME PTR,   02897000
*                                        0 IF NONE                   */ 02898000
*     2 DSABORG CHAR(2) BDY(HWORD),    /*DATA SET ORGANIZATION       */ 02899000
*       3 DSABORG1 CHAR(1),            /*FIRST DSORG BYTE            */ 02900000
*         4 DSABIS BIT(1),             /*INDEXED SEQUENTIAL ORGANIZ. */ 02901000
*         4 DSABPS BIT(1),             /*PHYSICAL SEQUENTIAL ORGANIZ.*/ 02902000
*         4 DSABDA BIT(1),             /*DIRECT ACCESS ORGANIZATION  */ 02903000
*         4 DSABCX BIT(1),             /*COMMUNICATIONS LINE GROUP   */ 02904000
*         4 DSABCQ BIT(1),             /*DIRECT ACCESS MESSAGE QUEUE */ 02905000
*         4 DSABMQ BIT(1),             /*TELECOMMUNIC. MSG. QUEUE    */ 02906000
*         4 DSABPO BIT(1),             /*PARTITIONED ORGANIZATION    */ 02907000
*         4 DSABU BIT(1),              /*UNMOVEABLE                  */ 02908000
*       3 DSABORG2 CHAR(1),            /*SECOND DSORG BYTE           */ 02909000
*         4 DSABGS BIT(1),             /*GRAPHICS ORGANIZATION       */ 02910000
*         4 DSABTX BIT(1),             /*TCAM LINE GROUP             */ 02911000
*         4 DSABTQ BIT(1),             /*TCAM MESSAGE QUEUE          */ 02912000
*         4 * BIT(1),                  /*RESERVED                    */ 02913000
*         4 DSABAM BIT(1),             /*VSAM                        */ 02914000
*         4 DSABTR BIT(1),             /*TCAM 3705                   */ 02915000
*         4 * BIT(2),                  /*RESERVED                    */ 02916000
*     2 DSABFLG1 CHAR(1),              /*FLAGS-RESTORED BY RESTART   */ 02917000
*       3 DSABDALC BIT(1),             /*DYNAMICALLY ALLOCATED       */ 02918000
*       3 DSABPALC BIT(1),             /*PERMANENTLY ALLOCATED     0204 02919000
*                                        ATTRIBUTE                   */ 02920000
*       3 DSABDCNV BIT(1),             /*DYNAMICALLY CONVERTED       */ 02921000
*       3 DSABCONV BIT(1),             /*CONVERTIBLE ATTRIBUTE       */ 02922000
*       3 DSABDCAT BIT(1),             /*DYNAMICALLY CONCATENATED    */ 02923000
*       3 DSABPCAT BIT(1),             /*PERMANENTLY CONCATENATED  0204 02924000
*                                        ATTRIBUTE                   */ 02925000
*       3 DSABCATM BIT(1),             /*CONCATENATED GROUP MEMBER   */ 02926000
*       3 DSABNUSE BIT(1),             /*IN-USE ATTRIBUTE            */ 02927000
*     2 DSABFLG2 CHAR(1),              /*FLAGS-RESTORED BY RESTART   */ 02928000
*       3 DSABOPEN BIT(1),             /*DATA SET HAS BEEN OPENED    */ 02929000
*       3 DSABIRM BIT(1),              /*D.S. REVERSE MERGE FOR INPUT*/ 02930000
*       3 DSABUNAL BIT(1),             /*UNALLOCATED WHEN CLOSED     */ 02931000
*       3 * BIT(5),                                                0204 02932000
*     2 DSABFLG3 CHAR(1),              /*FLAG-NOT RESTORED BY RESTART*/ 02933000
*       3 DSABDEFR BIT(1),             /*DEFERRED MOUNTING           */ 02934000
*       3 DSABPASS BIT(1),             /*PASSED DATA SET             */ 02935000
*       3 DSABVAM  BIT(1),             /*VAM DATA SET                */ 02936000
*       3 DSABVMSC BIT(1),             /*VAM PAGING SPACE RELEASED   */ 02937000
*       3 DSABCATL BIT(1),             /*DATA SET IS A CATALOG       */ 02938000
*       3 DSABJSCT BIT(1),             /*JOBCAT OR STEPCAT DATA SET  */ 02939000
*       3 * BIT(2),                    /*RESERVED                    */ 02940000
*     2 DSABFLG4 CHAR(1),              /*FLAG-NOT RESTORED BY RESTART*/ 02941000
*       3 DSABCKDS BIT(1),             /*THIS IS A CHECKPT DATA SET  */ 02942000
*       3 DSABCKVL BIT(1),             /*VOLUME CONTAINING CHECKPT 0204 02943000
*                                        DATA SET IS SECURE          */ 02944000
*       3 DSABCKSI BIT(1),             /*SECURITY INTERFACE EXISTS 0204 02945000
*                                        FOR THE CHECKPT DATA SET    */ 02946000
*       3 *  BIT(5),                   /*RESERVED                    */ 02947000
*     2 DSABRS02 CHAR(2),              /*RESERVED                    */ 02948000
*     2 DSABTCBP PTR(31),              /*TCB UNDER WHICH SET IN-USE  */ 02949000
*                                                                  0204 02950000
*     2 DSABPTTR FIXED(31),            /*RELATIVE TTR OF DATA SET  0204 02951000
*                                        PASSWORD                    */ 02952000
*     2 DSABSSNM CHAR(4),              /*SUB-SYSTEM NAME             */ 02953000
*     2 DSABSSCM PTR(31),              /*SUB-SYSTEM COMMUNICATION  0204 02954000
*                                        AREA POINTER                */ 02955000
*     2 DSABDCBM CHAR(6),              /*BIT MAP OF DCB FIELDS       */ 02956000
*     2 DSABRS03 CHAR(2),              /*RESERVED                    */ 02957000
*     2 DSABSIOT PTR(31),              /*IN-CORE ADDRESS OF SIOT     */ 02958000
*     2 DSABRS04 FIXED(31);            /*RESERVED                    */ 02959000
* DCL                                                              0205 02960000
*   1 DSABANMI BASED(DSABANMP),        /*ALTERNATE DSNAME INFORMATION*/ 02961000
*     2 DSABANML PTR(8),               /*LENGTH OF ALTERNATE DSNAME  */ 02962000
*     2 DSABANAM CHAR(*);              /*ALTERNATE DSNAME            */ 02963000
* DCL                                                              0206 02964000
*   1 DSABGIDL BASED(DSABGIDP),        /*DEVICE GROUP-ID LIST        */ 02965000
*     2 DSABLLEN FIXED(31),            /*DEVICE GROUP-ID LIST LENGTH */ 02966000
*     2 DSABGRID (*) CHAR(4);          /*DEVICE GROUP-ID             */ 02967000
