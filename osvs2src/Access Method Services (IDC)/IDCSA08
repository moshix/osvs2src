         L     REG14,GDTABT(,GDTPT)                                0694 06472000
*              REG13 = GDTABH;                                     0695 06473000
         L     REG13,GDTABH(,GDTPT)                                0695 06474000
*              GEN(BR    REG14);                                   0696 06475000
         BR    REG14                                                    06476000
*              RESPECIFY(REG13,REG14,REG15)UNRSTD;                 0697 06477000
*            END;                                                  0698 06478000
*        /***************************************************@G24LSBS*/ 06479000
*        /*                                                  @G24LSBS*/ 06480000
*        /* BASE THE RACHECK DSECT ON THE NEW CORE, ZERO IT  @G24LSBS*/ 06481000
*        /* OUT, SET THE PARAMETER LENGTH, AND ZERO THE      @G24LSBS*/ 06482000
*        /* RETURN CODE.                                     @G24LSBS*/ 06483000
*        /*                                                  @G24LSBS*/ 06484000
*        /***************************************************@G24LSBS*/ 06485000
*                                                                  0699 06486000
*        ACHKPTR = ADDR(ACHKCORE);                         /*@G24LSBS*/ 06487000
@RF00690 LA    ACHKPTR,ACHKCORE                                    0699 06488000
*        ACHKLIST = ''B;                                   /*@G24LSBS*/ 06489000
         XC    ACHKLIST(16,ACHKPTR),ACHKLIST(ACHKPTR)              0700 06490000
*        ACHKLENG = LENGTH(ACHKLIST);                      /*@G24LSBS*/ 06491000
*                                                                  0701 06492000
         MVI   ACHKLENG(ACHKPTR),X'10'                             0701 06493000
*        URACFRC = RACFRC0;                                /*@G24LSBS*/ 06494000
*                                                                  0702 06495000
*                                                                  0702 06496000
*                                                                  0702 06497000
*                                                                  0702 06498000
*                                                                  0702 06499000
         SLR   URACFRC,URACFRC                                     0702 06500000
*        /***************************************************@G24LSBS*/ 06501000
*        /***************************************************@G24LSBS*/ 06502000
*        /*                                                  @G24LSBS*/ 06503000
*        /* BUILD THE RACHECK PARAMETER LIST BY MOVING THE   @G24LSBS*/ 06504000
*        /* NECESSARY INFORMATION FROM THE INCOMING          @G24LSBS*/ 06505000
*        /* ARGUMENT TO THE RACHECK PARAMETER LIST.          @G24LSBS*/ 06506000
*        /*                                                  @G24LSBS*/ 06507000
*        /***************************************************@G24LSBS*/ 06508000
*        /***************************************************@G24LSBS*/ 06509000
*                                                                  0703 06510000
*        RESPECIFY RACFAGL BASED(RKPLIST);                 /*@G24LSBS*/ 06511000
*        /***************************************************@G24LSBS*/ 06512000
*        /* IF THE CLASSNAME POINTER IS NOT ZERO AND THE     @G24LSBS*/ 06513000
*        /* CLASSNAME LENGTH IS '7' AND THE CLASS NAME IS    @G24LSBS*/ 06514000
*        /* 'DATASET',                                       @G24LSBS*/ 06515000
*        /***************************************************@G24LSBS*/ 06516000
*          IF RACFCLN ^= NULLPTR &                         /*@G24LSBS*/ 06517000
*             RACFXL = RACFDSL7 &                          /*@G24LSBS*/ 06518000
*             RACFXC = RACFDSNM                            /*@G24LSBS*/ 06519000
*            THEN                                          /*@G24LSBS*/ 06520000
*                                                                  0704 06521000
         L     @09,RKPLIST(,PARMPTR2)                              0704 06522000
         L     @06,RACFCLN(,@09)                                   0704 06523000
         LTR   @06,@06                                             0704 06524000
         BZ    @RF00704                                            0704 06525000
         CLI   RACFXL(@06),7                                       0704 06526000
         BNE   @RF00704                                            0704 06527000
         CLC   RACFXC(7,@06),RACFDSNM                              0704 06528000
         BNE   @RF00704                                            0704 06529000
*              DO;                                         /*@G24LSBS*/ 06530000
*                /*******************************************@G24LSBS*/ 06531000
*                /* SET THE CLASS POINTER,                   @G24LSBS*/ 06532000
*                /*******************************************@G24LSBS*/ 06533000
*                ACHKCLNW = RACFCLN;                       /*@G24LSBS*/ 06534000
*                                                                  0706 06535000
         ST    @06,ACHKCLNW(,ACHKPTR)                              0706 06536000
*                /*******************************************@G24LSBS*/ 06537000
*                /* AND SET THE APPROPRIATE ATTRIBUTE BIT:   @G24LSBS*/ 06538000
*                /* RACFTALT INDICATES ALTER   ATTRIBUTE     @G24LSBS*/ 06539000
*                /* RACFTCTL INDICATES CONTROL ATTRIBUTE     @G24LSBS*/ 06540000
*                /* RACFTUPD INDICATES UPDATE  ATTRIBUTE     @G24LSBS*/ 06541000
*                /* RACFTRD  INDICATES READ    ATTRIBUTE     @G24LSBS*/ 06542000
*                /*******************************************@G24LSBS*/ 06543000
*                IF RACFTALT = ON                          /*@G24LSBS*/ 06544000
*                  THEN                                    /*@G24LSBS*/ 06545000
         TM    RACFTALT(@09),B'10000000'                           0707 06546000
         BNO   @RF00707                                            0707 06547000
*                    ACHKTALT = ON;                        /*@G24LSBS*/ 06548000
         OI    ACHKTALT(ACHKPTR),B'10000000'                       0708 06549000
*                  ELSE                                    /*@G24LSBS*/ 06550000
*                    IF RACFTCTL = ON                      /*@G24LSBS*/ 06551000
*                      THEN                                /*@G24LSBS*/ 06552000
         B     @RC00707                                            0709 06553000
@RF00707 L     @09,RKPLIST(,PARMPTR2)                              0709 06554000
         TM    RACFTCTL(@09),B'00001000'                           0709 06555000
         BNO   @RF00709                                            0709 06556000
*                        ACHKTCTL = ON;                    /*@G24LSBS*/ 06557000
         OI    ACHKTCTL(ACHKPTR),B'00001000'                       0710 06558000
*                      ELSE                                /*@G24LSBS*/ 06559000
*                        IF RACFTUPD = ON                  /*@G24LSBS*/ 06560000
*                          THEN                            /*@G24LSBS*/ 06561000
         B     @RC00709                                            0711 06562000
@RF00709 L     @09,RKPLIST(,PARMPTR2)                              0711 06563000
         TM    RACFTUPD(@09),B'00000100'                           0711 06564000
         BNO   @RF00711                                            0711 06565000
*                            ACHKTUPD = ON;                /*@G24LSBS*/ 06566000
         OI    ACHKTUPD(ACHKPTR),B'00000100'                       0712 06567000
*                          ELSE                            /*@G24LSBS*/ 06568000
*                            IF RACFTRD = ON               /*@G24LSBS*/ 06569000
*                              THEN                        /*@G24LSBS*/ 06570000
         B     @RC00711                                            0713 06571000
@RF00711 L     @09,RKPLIST(,PARMPTR2)                              0713 06572000
