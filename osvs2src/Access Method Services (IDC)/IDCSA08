*        RESPECIFY RTNREG UNRSTD;                                  0751 06857000
*                                                                  0751 06858000
*        /***************************************************@G24LSBS*/ 06859000
*        /* IF THE RACFCSA FLAG IS ON INDICATING A PROFILE   @G24LSBS*/ 06860000
*        /* HAS BEEN REQUESTED FROM THE CALLER OF URACHECK,  @G24LSBS*/ 06861000
*        /***************************************************@G24LSBS*/ 06862000
*        IF RACFCSA = ON                                   /*@G24LSBS*/ 06863000
*          THEN                                            /*@G24LSBS*/ 06864000
*                                                                  0752 06865000
         L     @09,RKPLIST(,PARMPTR2)                              0752 06866000
         TM    RACFCSA(@09),B'00000001'                            0752 06867000
         BNO   @RF00752                                            0752 06868000
*            /***********************************************@G24LSBS*/ 06869000
*            /* SET THE RACF PROFILE POINTER WITH THE        @G24LSBS*/ 06870000
*            /* POINTER RETURNED IN REGISTER ONE(1).         @G24LSBS*/ 06871000
*            /***********************************************@G24LSBS*/ 06872000
*            RACPFPTR = REG1;                              /*@G24LSBS*/ 06873000
*                                                                  0753 06874000
         ST    REG1,RACPFPTR                                       0753 06875000
*        RESPECIFY (REG1) UNRSTD;                          /*@G24LSBS*/ 06876000
@RF00752 DS    0H                                                  0755 06877000
         EJECT                                                          06878000
         SPACE 9                                                        06879000
*        /***************************************************@G24LSBS*/ 06880000
*        /* THIS IS A DUMP POINT FOR URACHECK MACRO.         @G24LSBS*/ 06881000
*        /***************************************************@G24LSBS*/ 06882000
*        IF GDTDBG = NULLPTR                                       0755 06883000
*          THEN;                                                   0755 06884000
         L     @09,GDTDBG(,GDTPT)                                  0755 06885000
         LTR   @09,@09                                             0755 06886000
         BZ    @RT00755                                            0755 06887000
*          ELSE                                                    0757 06888000
*            CALL IDCDB010(GDTTBL,'SAR2');                         0757 06889000
         ST    GDTPT,@AL00001                                      0757 06890000
         LA    @09,@CC02730                                        0757 06891000
         ST    @09,@AL00001+4                                      0757 06892000
         MVI   @AL00001+4,X'80'                                    0757 06893000
         L     @15,GDTDBG(,GDTPT)                                  0757 06894000
         LA    @01,@AL00001                                        0757 06895000
         BALR  @14,@15                                             0757 06896000
*                                                          /*@G24LSBS*/ 06897000
         EJECT                                                          06898000
*        /***************************************************@G24LSBS*/ 06899000
*        /* IF THE RETURN CODE DOES NOT INDICATE 'NO RACF    @G24LSBS*/ 06900000
*        /* PROFILE ON 'ENTITY''(RACF RET CODE 4),           @G24LSBS*/ 06901000
*        /* DO THE FOLLOWING:                                @G24LSBS*/ 06902000
*        /***************************************************@G24LSBS*/ 06903000
*                                                                  0758 06904000
*        IF TESTRC ^= RACFRC4                              /*@G24LSBS*/ 06905000
*          THEN                                            /*@G24LSBS*/ 06906000
*                                                                  0758 06907000
@RT00755 C     TESTRC,@CF00041                                     0758 06908000
         BE    @RF00758                                            0758 06909000
*RACKN4:     DO;                                           /*@G24LSBS*/ 06910000
*                                                                  0759 06911000
RACKN4   DS    0H                                                  0760 06912000
*              /*********************************************@G24LSBS*/ 06913000
*              /* IF THE USER IS AUTHORIZED, SET THE CALLERS @G24LSBS*/ 06914000
*              /* RETURN CODE TO ZERO.                       @G24LSBS*/ 06915000
*              /*********************************************@G24LSBS*/ 06916000
*                                                                  0760 06917000
*              IF TESTRC = RACFRC0                         /*@G24LSBS*/ 06918000
*                THEN                                      /*@G24LSBS*/ 06919000
         SLR   @09,@09                                             0760 06920000
         CR    TESTRC,@09                                          0760 06921000
         BNE   @RF00760                                            0760 06922000
*                  SETRC = RACFRC0;                        /*@G24LSBS*/ 06923000
*                                                                  0761 06924000
         STH   @09,SETRC                                           0761 06925000
*                ELSE                                      /*@G24LSBS*/ 06926000
*                                                                  0762 06927000
*                /*******************************************@G24LSBS*/ 06928000
*                /* OTHERWISE, THE USER MUST NOT BE          @G24LSBS*/ 06929000
*                /* AUTHORIZED.  THEREFORE...                @G24LSBS*/ 06930000
*                /*******************************************@G24LSBS*/ 06931000
*                                                                  0762 06932000
*                  DO;                                     /*@G24LSBS*/ 06933000
         B     @RC00760                                            0762 06934000
@RF00760 DS    0H                                                  0763 06935000
*                    /***************************************@G24LSBS*/ 06936000
*                    /* SETUP THE MESSAGE:                   @G24LSBS*/ 06937000
*                    /* 'RACHECK FAILED FOR -'ENTITY'- '.    @G24LSBS*/ 06938000
*                    /***************************************@G24LSBS*/ 06939000
*                                                                  0763 06940000
*                    DDSTRU = ADDR(MSGAREA);               /*@G24LSBS*/ 06941000
         LA    @09,MSGAREA                                         0763 06942000
         ST    @09,DDSTRU                                          0763 06943000
*                    DARGLIST = ''B;                       /*@G24LSBS*/ 06944000
         XC    DARGLIST(20,@09),DARGLIST(@09)                      0764 06945000
*                    DARGSMOD = SA0ID;                     /*@G24LSBS*/ 06946000
         MVC   DARGSMOD(3,@09),@CC00638                            0765 06947000
*                    DARGSENT = SAMSG10;                   /*@G24LSBS*/ 06948000
         MVI   DARGSENT(@09),X'0A'                                 0766 06949000
*                    DARGILP  = RACFENTL;                  /*@G24LSBS*/ 06950000
*                                                                  0767 06951000
         MVC   DARGILP(2,@09),@CH02457                             0767 06952000
*                    /***************************************@G24LSBS*/ 06953000
*                    /* IF RACF PROFILE WAS NOT REQUESTED,   @G24LSBS*/ 06954000
*                    /***************************************@G24LSBS*/ 06955000
*                                                                  0768 06956000
*                    IF RACFPRF ^= ON                      /*@G24LSBS*/ 06957000
*                      THEN                                /*@G24LSBS*/ 06958000
*                                                                  0768 06959000
         L     @06,RKPLIST(,PARMPTR2)                              0768 06960000
         TM    RACFPRF(@06),B'00100000'                            0768 06961000
         BO    @RF00768                                            0768 06962000
*                        /***********************************@G24LSBS*/ 06963000
*                        /* GET DSN FROM ORIGINAL PARMLIST.  @G24LSBS*/ 06964000
*                        /***********************************@G24LSBS*/ 06965000
*                                                                  0769 06966000
*                        DARGDBP = RACFENT;                /*@G24LSBS*/ 06967000
*                                                                  0769 06968000
         L     @06,RACFENT(,@06)                                   0769 06969000
         ST    @06,DARGDBP(,@09)                                   0769 06970000
*                      ELSE                                /*@G24LSBS*/ 06971000
*                                                                  0770 06972000
*                        /***********************************@G24LSBS*/ 06973000
*                        /* OTHERWISE, GET IT FROM PROFILE.  @G24LSBS*/ 06974000
*                        /***********************************@G24LSBS*/ 06975000
*                                                                  0770 06976000
*                        DARGDBP = ADDR(RACPDSN);          /*@G24LSBS*/ 06977000
*                                                                  0770 06978000
         B     @RC00768                                            0770 06979000
@RF00768 L     @09,RACPFPTR                                        0770 06980000
         LA    @09,RACPDSN(,@09)                                   0770 06981000
         L     @06,DDSTRU                                          0770 06982000
         ST    @09,DARGDBP(,@06)                                   0770 06983000
*                    /***************************************@G24LSBS*/ 06984000
*                    /* PRINT THE MESSAGE                    @G24LSBS*/ 06985000
*                    /***************************************@G24LSBS*/ 06986000
*                                                                  0771 06987000
*                    CALL AMSTP011(GDTTBL,PRTFILE,DDSTRU); /*@G24LSBS*/ 06988000
*                                                                  0771 06989000
@RC00768 ST    GDTPT,@AL00001                                      0771 06990000
         LA    @09,@CF00115                                        0771 06991000
         ST    @09,@AL00001+4                                      0771 06992000
         LA    @09,DDSTRU                                          0771 06993000
         ST    @09,@AL00001+8                                      0771 06994000
         MVI   @AL00001+8,X'80'                                    0771 06995000
         L     @15,GDTPRT(,GDTPT)                                  0771 06996000
         LA    @01,@AL00001                                        0771 06997000
         BALR  @14,@15                                             0771 06998000
*                    /***************************************@G24LSBS*/ 06999000
*                    /* SET THE CALLERS RETURN CODE 8 FOR    @G24LSBS*/ 07000000
*                    /* INVALID RACF AUTHORIZATION.          @G24LSBS*/ 07001000
*                    /***************************************@G24LSBS*/ 07002000
*                                                                  0772 07003000
*                    SETRC = RACFRC8;                      /*@G24LSBS*/ 07004000
*                                                                  0772 07005000
         MVC   SETRC(2),@CH00179                                   0772 07006000
*                  END;                                    /*@G24LSBS*/ 07007000
*        /***************************************************@G24LSBS*/ 07008000
*        /***************************************************@G24LSBS*/ 07009000
*        /*                                                  @G24LSBS*/ 07010000
*        /* IF THE CALLER REQUESTED A PROFILE FROM RACF,     @G24LSBS*/ 07011000
*        /*                                                  @G24LSBS*/ 07012000
*        /***************************************************@G24LSBS*/ 07013000
*        /***************************************************@G24LSBS*/ 07014000
*                                                                  0774 07015000
*        IF RACFCSA = ON                                   /*@G24LSBS*/ 07016000
*          THEN                                            /*@G24LSBS*/ 07017000
*                                                                  0774 07018000
@RC00760 L     @09,RKPLIST(,PARMPTR2)                              0774 07019000
         TM    RACFCSA(@09),B'00000001'                            0774 07020000
         BNO   @RF00774                                            0774 07021000
*RACKPF:     DO;                                           /*@G24LSBS*/ 07022000
*                                                                  0775 07023000
RACKPF   DS    0H                                                  0776 07024000
*              /*********************************************@G24LSBS*/ 07025000
*              /* MOVE THE PROFILE LENGTH TO A FULL WORD.    @G24LSBS*/ 07026000
*              /*********************************************@G24LSBS*/ 07027000
*              RACPLNFW = RACPLN;                          /*@G24LSBS*/ 07028000
*                                                                  0776 07029000
         L     @09,RACPFPTR                                        0776 07030000
         L     @09,RACPLN-1(,@09)                                  0776 07031000
         LA    @09,0(,@09)                                         0776 07032000
         ST    @09,RACPLNFW                                        0776 07033000
*              /*********************************************@G24LSBS*/ 07034000
*              /* GET CORE FOR THE PROFILE                   @G24LSBS*/ 07035000
*              /*********************************************@G24LSBS*/ 07036000
*                                                                  0777 07037000
*              CALL IDCSAGS0 (GDTTBL,                              0777 07038000
*                       RACPLNFW,                                  0777 07039000
*                       RACFPROF,                                  0777 07040000
*                       SETZERO);                                  0777 07041000
*                                                                  0777 07042000
         ST    GDTPT,@AL00001                                      0777 07043000
         LA    @09,RACPLNFW                                        0777 07044000
         ST    @09,@AL00001+4                                      0777 07045000
         L     @09,RKPLIST(,PARMPTR2)                              0777 07046000
         LA    @09,RACFPROF(,@09)                                  0777 07047000
         ST    @09,@AL00001+8                                      0777 07048000
         LA    @09,SETZERO                                         0777 07049000
         ST    @09,@AL00001+12                                     0777 07050000
         MVI   @AL00001+12,X'80'                                   0777 07051000
         L     @15,GDTGSP(,GDTPT)                                  0777 07052000
         LA    @01,@AL00001                                        0777 07053000
         BALR  @14,@15                                             0777 07054000
*              /*********************************************@G24LSBS*/ 07055000
*              /* SAVE THE RETURN CODE                       @G24LSBS*/ 07056000
*              /*********************************************@G24LSBS*/ 07057000
*                                                                  0778 07058000
*              RESPECIFY RTNREG RSTD;                              0778 07059000
*              TESTRC = RTNREG;                                    0779 07060000
         LR    TESTRC,RTNREG                                       0779 07061000
*              RESPECIFY RTNREG UNRSTD;                            0780 07062000
*                                                                  0780 07063000
*              IF TESTRC ^= 0                              /*@G24LSBS*/ 07064000
*                THEN                                              0781 07065000
*                                                                  0781 07066000
         LTR   TESTRC,TESTRC                                       0781 07067000
         BZ    @RF00781                                            0781 07068000
*                  /*****************************************@G24LSBS*/ 07069000
*                  /* THE REQUEST FOR MAIN STORAGE FAILED.   @G24LSBS*/ 07070000
*                  /* OUTPUT THE APPROPRIATE ERROR MESSAGE   @G24LSBS*/ 07071000
*                  /* AND SET THE RETURN CODE TO 8.          @G24LSBS*/ 07072000
*                  /*****************************************@G24LSBS*/ 07073000
*                                                                  0782 07074000
*                  DO;                                             0782 07075000
*                    DDSTRU = ADDR(MSGAREA);                       0783 07076000
         LA    @09,MSGAREA                                         0783 07077000
         ST    @09,DDSTRU                                          0783 07078000
*                    DARGLIST = ''B;                               0784 07079000
         XC    DARGLIST(20,@09),DARGLIST(@09)                      0784 07080000
*                    DARGSMOD = UV0ID;                             0785 07081000
         MVC   DARGSMOD(3,@09),@CC00634                            0785 07082000
*                    DARGSENT = UVMSG12;                           0786 07083000
*                                                                  0786 07084000
         MVI   DARGSENT(@09),X'0C'                                 0786 07085000
*                    CALL AMSTP011 (GDTTBL,PRTFILE,DDSTRU);        0787 07086000
*                                                                  0787 07087000
         ST    GDTPT,@AL00001                                      0787 07088000
         LA    @09,@CF00115                                        0787 07089000
         ST    @09,@AL00001+4                                      0787 07090000
         LA    @09,DDSTRU                                          0787 07091000
         ST    @09,@AL00001+8                                      0787 07092000
         MVI   @AL00001+8,X'80'                                    0787 07093000
         L     @15,GDTPRT(,GDTPT)                                  0787 07094000
         LA    @01,@AL00001                                        0787 07095000
         BALR  @14,@15                                             0787 07096000
*                    SETRC = 8;                                    0788 07097000
*                                                                  0788 07098000
         MVC   SETRC(2),@CH00179                                   0788 07099000
*                  END;                                            0789 07100000
*                                                                  0790 07101000
*          ELSE                                            /*@G24LSBS*/ 07102000
*            DO;                                           /*@G24LSBS*/ 07103000
         B     @RC00781                                            0790 07104000
@RF00781 DS    0H                                                  0791 07105000
*              /*********************************************@G24LSBS*/ 07106000
*              /* OTHERWISE, THE REQUEST FOR SPACE WAS OK.   @G24LSBS*/ 07107000
*              /* PROCEED TO SETUP THE SIZE AND THE POINTERS @G24LSBS*/ 07108000
*              /* OF THE RACF PROFILE AREA TO BE MOVED.      @G24LSBS*/ 07109000
*              /* THEN SET THE DO LOOP CONTROL FLAG.         @G24LSBS*/ 07110000
*              /*********************************************@G24LSBS*/ 07111000
*              MSIZE = RACPLN;                             /*@G24LSBS*/ 07112000
         L     @09,RACPFPTR                                        0791 07113000
         L     MSIZE,RACPLN-1(,@09)                                0791 07114000
         LA    MSIZE,0(,MSIZE)                                     0791 07115000
*              MTPTR = RACFPROF;                           /*@G24LSBS*/ 07116000
         L     @06,RKPLIST(,PARMPTR2)                              0792 07117000
         L     MTPTR,RACFPROF(,@06)                                0792 07118000
*              MFPTR = RACPFPTR;                           /*@G24LSBS*/ 07119000
         LR    MFPTR,@09                                           0793 07120000
*              DOCNTRL = ON;                               /*@G24LSBS*/ 07121000
*                                                                  0794 07122000
         OI    DOCNTRL,B'10000000'                                 0794 07123000
*              /*********************************************@G24LSBS*/ 07124000
*              /* PROCEED THROUGH THE LOOP WHILE THERE IS    @G24LSBS*/ 07125000
*              /* PROFILE AREA TO BE MOVED TO URACHECK       @G24LSBS*/ 07126000
*              /* STORAGE.                                   @G24LSBS*/ 07127000
*              /*********************************************@G24LSBS*/ 07128000
*              DO WHILE (DOCNTRL = ON);                    /*@G24LSBS*/ 07129000
*                                                                  0795 07130000
         B     @DE00795                                            0795 07131000
@DL00795 DS    0H                                                  0796 07132000
*                /*******************************************@G24LSBS*/ 07133000
*                /* IF THERE IS MORE THAN 256 BYTES TO MOVE, @G24LSBS*/ 07134000
*                /*******************************************@G24LSBS*/ 07135000
*                IF MSIZE > 256                            /*@G24LSBS*/ 07136000
*                  THEN                                    /*@G24LSBS*/ 07137000
*                                                                  0796 07138000
         LA    @09,256                                             0796 07139000
         CR    MSIZE,@09                                           0796 07140000
         BNH   @RF00796                                            0796 07141000
*                    DO;                                   /*@G24LSBS*/ 07142000
*                      /*************************************@G24LSBS*/ 07143000
*                      /* MOVE THE 256 BYTES AND UPDATE THE  @G24LSBS*/ 07144000
*                      /* SIZE AND POINTERS.                 @G24LSBS*/ 07145000
*                      /*************************************@G24LSBS*/ 07146000
*                      TOAREA(1:256)=FROMAREA(1:256);      /*@G24LSBS*/ 07147000
         MVC   TOAREA(256,MTPTR),FROMAREA(MFPTR)                   0798 07148000
*                      MSIZE = MSIZE - 256;                /*@G24LSBS*/ 07149000
         SLR   MSIZE,@09                                           0799 07150000
*                      MTPTR = MTPTR + 256;                /*@G24LSBS*/ 07151000
         ALR   MTPTR,@09                                           0800 07152000
*                      MFPTR = MFPTR + 256;                /*@G24LSBS*/ 07153000
         ALR   MFPTR,@09                                           0801 07154000
*                    END;                                  /*@G24LSBS*/ 07155000
*                                                                  0802 07156000
*                  ELSE                                    /*@G24LSBS*/ 07157000
*                                                                  0803 07158000
*                    DO;                                   /*@G24LSBS*/ 07159000
         B     @RC00796                                            0803 07160000
@RF00796 DS    0H                                                  0804 07161000
*                      /*************************************@G24LSBS*/ 07162000
*                      /* OTHERWISE, THERE IS LESS THAN 256  @G24LSBS*/ 07163000
*                      /* BYTES TO MOVE.  MOVE THE RIGHT SIZE@G24LSBS*/ 07164000
*                      /* AND RESET THE DO LOOP CONTROL FLAG.@G24LSBS*/ 07165000
*                      /*************************************@G24LSBS*/ 07166000
*                      TOAREA(1:MSIZE)=FROMAREA(1:MSIZE);  /*@G24LSBS*/ 07167000
         LR    @09,MSIZE                                           0804 07168000
         BCTR  @09,0                                               0804 07169000
         EX    @09,@SM02752                                        0804 07170000
*                      DOCNTRL = OFF;                      /*@G24LSBS*/ 07171000
         NI    DOCNTRL,B'01111111'                                 0805 07172000
*                    END;                                  /*@G24LSBS*/ 07173000
*              END;                                        /*@G24LSBS*/ 07174000
@RC00796 DS    0H                                                  0807 07175000
@DE00795 TM    DOCNTRL,B'10000000'                                 0807 07176000
         BO    @DL00795                                            0807 07177000
*            END;                                          /*@G24LSBS*/ 07178000
*                                                                  0808 07179000
*              /*********************************************@G24LSBS*/ 07180000
*              /* CONTINUE NOW TO FREE THE PROFILE STORAGE   @G24LSBS*/ 07181000
*              /* OBTAINED BY RACHECK.                       @G24LSBS*/ 07182000
*              /*********************************************@G24LSBS*/ 07183000
*                                                                  0809 07184000
*              RESPECIFY (REG0,REG1) RSTD;                 /*@G24LSBS*/ 07185000
@RC00781 DS    0H                                                  0810 07186000
*              REG0 = RACPFST;                             /*@G24LSBS*/ 07187000
         L     @09,RACPFPTR                                        0810 07188000
         L     REG0,RACPFST(,@09)                                  0810 07189000
*              REG1 = RACPFPTR;                            /*@G24LSBS*/ 07190000
*                                                                  0811 07191000
         LR    REG1,@09                                            0811 07192000
*LFREM1:       GEN(FREEMAIN R,LV=(REG0),A=(REG1));         /*@G24LSBS*/ 07193000
*                                                                  0812 07194000
LFREM1   FREEMAIN R,LV=(REG0),A=(REG1)                                  07195000
*              RESPECIFY (REG0,REG1) UNRSTD;               /*@G24LSBS*/ 07196000
*                                                                  0813 07197000
*            END RACKPF;                                   /*@G24LSBS*/ 07198000
*                                                                  0814 07199000
*          END RACKN4;                                     /*@G24LSBS*/ 07200000
*                                                                  0815 07201000
*        ELSE                                              /*@G24LSBS*/ 07202000
*                                                                  0816 07203000
*              DO;                                         /*@G24LSBS*/ 07204000
         B     @RC00758                                            0816 07205000
@RF00758 DS    0H                                                  0817 07206000
*                /*******************************************@G24LSBS*/ 07207000
*                /* RETURN CODE INDICATES EITHER THE RACF    @G24LSBS*/ 07208000
*                /* PROFILE IS NOT DEFINED OR IS UNPROTECTED.@G24LSBS*/ 07209000
*                /* SETUP A MESSAGE:                         @G24LSBS*/ 07210000
*                /* 'NO RACF PROFILE ON -'ENTITY'-'          @G24LSBS*/ 07211000
*                /*******************************************@G24LSBS*/ 07212000
*                DDSTRU = ADDR(MSGAREA);                   /*@G24LSBS*/ 07213000
         LA    @09,MSGAREA                                         0817 07214000
         ST    @09,DDSTRU                                          0817 07215000
*                DARGLIST = ''B;                           /*@G24LSBS*/ 07216000
         XC    DARGLIST(20,@09),DARGLIST(@09)                      0818 07217000
*                DARGSMOD = SA0ID;                         /*@G24LSBS*/ 07218000
         MVC   DARGSMOD(3,@09),@CC00638                            0819 07219000
*                DARGSENT = SAMSG09;                       /*@G24LSBS*/ 07220000
         MVI   DARGSENT(@09),X'09'                                 0820 07221000
*                DARGILP  = RACFENTL;                      /*@G24LSBS*/ 07222000
*                                                                  0821 07223000
         MVC   DARGILP(2,@09),@CH02457                             0821 07224000
*                /*******************************************@G24LSBS*/ 07225000
*                /* IF RACF PROFILE WAS NOT REQUESTED,       @G24LSBS*/ 07226000
*                /*******************************************@G24LSBS*/ 07227000
*                IF RACFPRF ^= ON                          /*@G24LSBS*/ 07228000
*                  THEN                                    /*@G24LSBS*/ 07229000
*                                                                  0822 07230000
         L     @06,RKPLIST(,PARMPTR2)                              0822 07231000
         TM    RACFPRF(@06),B'00100000'                            0822 07232000
         BO    @RF00822                                            0822 07233000
*                    /***************************************@G24LSBS*/ 07234000
*                    /* GET DSN FROM ORIGINAL PARMLIST.      @G24LSBS*/ 07235000
*                    /***************************************@G24LSBS*/ 07236000
*                    DARGDBP = RACFENT;                    /*@G24LSBS*/ 07237000
*                                                                  0823 07238000
         L     @06,RACFENT(,@06)                                   0823 07239000
         ST    @06,DARGDBP(,@09)                                   0823 07240000
*                  ELSE                                    /*@G24LSBS*/ 07241000
*                    /***************************************@G24LSBS*/ 07242000
*                    /* OTHERWISE, GET IT FROM PROFILE.      @G24LSBS*/ 07243000
*                    /***************************************@G24LSBS*/ 07244000
*                    DARGDBP = ADDR(RACPDSN);              /*@G24LSBS*/ 07245000
*                                                                  0824 07246000
         B     @RC00822                                            0824 07247000
@RF00822 L     @09,RACPFPTR                                        0824 07248000
         LA    @09,RACPDSN(,@09)                                   0824 07249000
         L     @01,DDSTRU                                          0824 07250000
         ST    @09,DARGDBP(,@01)                                   0824 07251000
*                /*******************************************@G24LSBS*/ 07252000
*                /* PRINT THE MESSAGE.                       @G24LSBS*/ 07253000
*                /*******************************************@G24LSBS*/ 07254000
*                CALL AMSTP011(GDTTBL,PRTFILE,DDSTRU);     /*@G24LSBS*/ 07255000
*                                                                  0825 07256000
@RC00822 ST    GDTPT,@AL00001                                      0825 07257000
         LA    @09,@CF00115                                        0825 07258000
         ST    @09,@AL00001+4                                      0825 07259000
         LA    @09,DDSTRU                                          0825 07260000
         ST    @09,@AL00001+8                                      0825 07261000
         MVI   @AL00001+8,X'80'                                    0825 07262000
         L     @15,GDTPRT(,GDTPT)                                  0825 07263000
         LA    @01,@AL00001                                        0825 07264000
         BALR  @14,@15                                             0825 07265000
