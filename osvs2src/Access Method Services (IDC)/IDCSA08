         BNO   @RF00723                                            0723 06672000
         L     @06,RACFPROF(,@06)                                  0723 06673000
         CR    @06,@09                                             0723 06674000
         BE    @RF00723                                            0723 06675000
*                /*******************************************@G24LSBS*/ 06676000
*                /* SET THE PROFILE POINTER AND THE PROFILE  @G24LSBS*/ 06677000
*                /* FLAG IN THE RACHECK PARAMETER LIST.      @G24LSBS*/ 06678000
*                /*******************************************@G24LSBS*/ 06679000
*                                                                  0724 06680000
*                DO;                                       /*@G24LSBS*/ 06681000
*                  ACHKENTW = RACFPROF;                    /*@G24LSBS*/ 06682000
