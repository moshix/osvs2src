         BAL   @14,SSCMSG                                          0215 00198000
*     END;                          /*                       @Y30LB36*/ 00199000
*   DARGSMOD=UNIVMSG;               /*                       @Y30LB36*/ 00200000
@RC00207 L     @10,DDSTRU                                          0217 00201000
         MVC   DARGSMOD(3,@10),@CC00840                            0217 00202000
*   DARGDBP=ADDR(LASTCOND);         /*                       @Y30LB36*/ 00203000
         L     @15,@PC00001+8                                      0218 00204000
         ST    @15,DARGDBP(,@10)                                   0218 00205000
*   DARGILP=LENGTH(LASTCOND);       /*                       @Y30LB36*/ 00206000
