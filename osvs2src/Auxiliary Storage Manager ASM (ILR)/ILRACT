*                 AFSA=AFSONE;      /* RESET ASST SUBSCRIPT          */ 01404000
         LA    @05,1                                               0354 01405000
         LR    AFSA,@05                                            0354 01406000
*               END;                                               0355 01407000
*             ELSE                  /* YES, REACHED END OF ARRAY     */ 01408000
*               AFSEXPTR=AFSZERO;   /* FORCE LOOP EXIT               */ 01409000
         B     @RC00351                                            0356 01410000
@RF00351 SLR   AFSEXPTR,AFSEXPTR                                   0356 01411000
*           END;                    /* THIS THE LOGICAL END OF THE DO   01412000
*                                      WHILE LOOP FOR ASST         0357 01413000
*                                      EXTENSIONS. PHYSICAL END IS 0357 01414000
*                                      AFTER THE ERROR CODE PROCESS     01415000
*                                      ING FOR A NON-ZERO RETURN FROM   01416000
*                                      AFSFREE                       */ 01417000
         EJECT                                                          01418000
*                                                                  0358 01419000
*/********************************************************************/ 01420000
*/* THIS SECTION WILL PROCESS A NON-ZERO RETURN CODE FROM AFSFREE    */ 01421000
*/* FOR ASST EXTENSIONS. PROCESSING CONSISTS OF GETTING THE POINTER  */ 01422000
*/* TO THE NEXT ASST EXTENSION, AND VERIFYING THAT IT IS AN ASST     */ 01423000
*/* EXTENSION. IF IT IS WE WILL CONTINUE FREEING ASST EXTENSIONS,    */ 01424000
