*     ASMVT BASED(ASMVTWPT);                                       0072 00195000
         EJECT                                                          00196000
*                                                                  0073 00197000
*/********************************************************************/ 00198000
*/*  RECORD POINTERS, BUILD PARAMETER LIST FOR ILRVSAMI, AND         */ 00199000
*/*  RETRIEVE THE ASPCT FROM SYS1.STGINDEX.                          */ 00200000
*/********************************************************************/ 00201000
*                                                                  0073 00202000
*   LGEPTR=ACELGE;                  /* GET POINTER TO LGE            */ 00203000
         L     @12,ACELGE(,ACEPTR)                                 0073 00204000
         ST    @12,LGEPTR                                          0073 00205000
*   SSYMPTR=ADDR(ACESYM);           /* STORE PTR TO 'S' SYMBOL IN  0074 00206000
*                                      PARM LIST FOR VSAMI           */ 00207000
         LA    @12,ACESYM(,ACEPTR)                                 0074 00208000
         ST    @12,SSYMPTR                                         0074 00209000
*   OPCODE=GETREQ;                  /* SET REQUEST OPCODE FOR GET    */ 00210000
         MVI   OPCODE,X'01'                                        0075 00211000
*   RFY                                                            0076 00212000
*     R1 RSTD;                                                     0076 00213000
*   R1=ADDR(PARMLIST);              /* GET ADDRESS OF PARM LIST      */ 00214000
         LA    R1,PARMLIST                                         0077 00215000
*   CALL ILRVSAMI;                  /* RETRIEVE THE ASPCT FROM     0078 00216000
*                                      SYS1.STGINDEX                 */ 00217000
         L     @15,@CV00112                                        0078 00218000
         BALR  @14,@15                                             0078 00219000
*   RFY                                                            0079 00220000
*     R15 RSTD;                                                    0079 00221000
*   IF R15^=ZERO                    /* RETRIEVAL SUCCESSFUL?         */ 00222000
*     THEN                          /* NO, SKIP TO EXIT PROCESSING   */ 00223000
         LTR   R15,R15                                             0080 00224000
         BZ    @RF00080                                            0080 00225000
*     RETCODE=R15;                  /* SAVE THE RETURN CODE          */ 00226000
         ST    R15,RETCODE                                         0081 00227000
*   ELSE                                                           0082 00228000
*     DO;                           /* YES                           */ 00229000
         B     @RC00080                                            0082 00230000
@RF00080 DS    0H                                                  0083 00231000
*       ASPCTPTR=R1;                /* SAVE PTR TO RETRIEVED ASPCT   */ 00232000
         ST    R1,ASPCTPTR                                         0083 00233000
*       RFY                                                        0084 00234000
*        (R1,                                                      0084 00235000
*         R15) UNRSTD;                                             0084 00236000
         EJECT                                                          00237000
*                                                                  0085 00238000
*/********************************************************************/ 00239000
*/*  REBUILD THE ASPCT IN USER'S MEMORY AND ANCHOR IT IN LGE         */ 00240000
*/********************************************************************/ 00241000
*                                                                  0085 00242000
*       ASPLGID=LGELGID;            /* COPY NEW LGID TO ASPCT      0085 00243000
*                                      RECALCULATE THE NUMBER OF   0085 00244000
*                                      SLOTS REQUIRED TO BACK THIS 0085 00245000
*                                      VIO DATASET                   */ 00246000
