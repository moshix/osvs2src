         CLC   ASPEIDNT(4,AFSWORK),@CC01424                        0338 01354000
         BE    @RF00338                                            0338 01355000
*                     AFSWORK=AFSZERO;/* FORCE LOOP EXIT             */ 01356000
         SLR   AFSWORK,AFSWORK                                     0339 01357000
*                 END;                                             0340 01358000
*               ELSE                /* YES, REACHED END OF ARRAY     */ 01359000
*                 AFSWORK=AFSZERO;  /* FORCE LOOP EXIT               */ 01360000
         B     @RC00335                                            0341 01361000
@RF00335 SLR   AFSWORK,AFSWORK                                     0341 01362000
*             END;                                                 0342 01363000
@RC00335 DS    0H                                                  0343 01364000
*         END;                      /* THIS IS THE PHYSICAL END OF 0343 01365000
*                                      THE DO WHILE LOOP FOR LPME  0343 01366000
