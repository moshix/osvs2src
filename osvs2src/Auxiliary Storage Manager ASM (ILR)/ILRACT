         ST    EPAPTR,EPATHPTR                                     0067 00182000
*   EPAACT=ON;                      /* RECORD ENTRY                  */ 00183000
         OI    EPAACT(EPAPTR),B'00010000'                          0068 00184000
*   EPAOWKA=R9;                     /* RECORD WORKAREA ADDRESS       */ 00185000
