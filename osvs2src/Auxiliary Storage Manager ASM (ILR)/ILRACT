         L     @11,ASMSLOTV(,@11)                                  0086 00253000
         L     @11,ILRSLOTV(,@11)                                  0086 00254000
         LTR   @11,@11                                             0086 00255000
         BNZ   @RF00086                                            0086 00256000
*         ASPBKSLT=ASPMAXPN;                                       0087 00257000
         L     @11,ASPMAXPN(,@12)                                  0087 00258000
         ST    @11,ASPBKSLT(,@12)                                  0087 00259000
*       ELSE                                                       0088 00260000
*         ASPBKSLT=ASPMAXPN/ILRSLOTV;                              0088 00261000
         B     @RC00086                                            0088 00262000
@RF00086 L     @12,ASPCTPTR                                        0088 00263000
         L     @11,ASMVTWPT                                        0088 00264000
         L     @11,ASMSLOTV(,@11)                                  0088 00265000
         L     @01,ASPMAXPN(,@12)                                  0088 00266000
         SLR   @00,@00                                             0088 00267000
         D     @00,ILRSLOTV(,@11)                                  0088 00268000
         ST    @01,ASPBKSLT(,@12)                                  0088 00269000
*       ASPASCB=PSAAOLD;            /* SET NEW ASCB POINTER          */ 00270000
@RC00086 L     @12,ASPCTPTR                                        0089 00271000
         L     @11,PSAAOLD                                         0089 00272000
         ST    @11,ASPASCB(,@12)                                   0089 00273000
*       ASPLGE=LGEPTR;              /* COPY NEW LGE ADDR TO ASPCT    */ 00274000
         L     @11,LGEPTR                                          0090 00275000
         ST    @11,ASPLGE(,@12)                                    0090 00276000
*       CALL REBUILD;               /* REBUILD THE ASPCT IN USER LSQA   00277000
*                                      STORAGE                       */ 00278000
         BAL   @14,REBUILD                                         0091 00279000
*       RFY                                                        0092 00280000
*         R15 RSTD;                                                0092 00281000
*       IF R15^=ZERO                /* SUCCESSFULLY REBUILD ASPCT?   */ 00282000
*         THEN                      /* NO,COULD NOT OBTAIN    STORAGE*/ 00283000
         LTR   R15,R15                                             0093 00284000
         BZ    @RF00093                                            0093 00285000
*         RETCODE=R15;              /* SAVE THE RETURN CODE          */ 00286000
         ST    R15,RETCODE                                         0094 00287000
*       ELSE                                                       0095 00288000
*         DO;                       /* YES, COMPLETE PROCESSING      */ 00289000
         B     @RC00093                                            0095 00290000
@RF00093 DS    0H                                                  0096 00291000
*           LGEASPCT=REBASPCT;      /* ANCHOR THE REBUILT ASPCT IN 0096 00292000
*                                      THE LGE                       */ 00293000
         L     @14,LGEPTR                                          0096 00294000
         L     @12,REBASPCT                                        0096 00295000
         ST    @12,LGEASPCT(,@14)                                  0096 00296000
*           LGESLTCT=ASPSAVCT;      /* COPY THE SAVED SLOT COUNT TO     00297000
*                                      THE LGE FOR ILRGOS TO UPDATE     00298000
*                                      THE ASCB VIO SLOT COUNT       */ 00299000
         L     @12,ASPCTPTR                                        0097 00300000
         L     @12,ASPSAVCT(,@12)                                  0097 00301000
         STH   @12,LGESLTCT(,@14)                                  0097 00302000
*           EPARBASP=ZERO;          /* ZERO RECOVERYS POINTER TO     */ 00303000
         SLR   @14,@14                                             0098 00304000
         ST    @14,EPARBASP(,EPAPTR)                               0098 00305000
*           EPARSIZE=ZERO;          /* THE LSQA STORAGE              */ 00306000
         STH   @14,EPARSIZE(,EPAPTR)                               0099 00307000
*         END;                                                     0100 00308000
*     END;                                                         0101 00309000
@RC00093 DS    0H                                                  0102 00310000
         EJECT                                                          00311000
*                                                                  0102 00312000
*/********************************************************************/ 00313000
*/*  EXIT PROCCESING - CLEAN-UP AND RETURN TO ILRGOS                 */ 00314000
*/********************************************************************/ 00315000
*                                                                  0102 00316000
*   GENERATE REFS(RETCODE) SETS(R13);                              0102 00317000
@RC00080 DS    0H                                                  0102 00318000
        L     R13,4(R13)                 GET PTR TO CALLERS SAVEAREA    00319000
        MVC   16(4,R13),RETCODE          STORE RETURN CODE IN CALLERS   00320000
*                                        REGISTER 15                    00321000
*   RFY                                                            0103 00322000
*    (R0,                                                          0103 00323000
*     R4,                                                          0103 00324000
*     R7,                                                          0103 00325000
*     R8) UNRSTD;                                                  0103 00326000
*   EPAOWKA=ZERO;                   /* ZERO RECOVERY'S WKA PTR       */ 00327000
         SLR   @14,@14                                             0104 00328000
         ST    @14,EPAOWKA(,EPAPTR)                                0104 00329000
*   EPARTYRG=ZERO;                  /* ZERO REG SAVAREA PTR          */ 00330000
         ST    @14,EPARTYRG(,EPAPTR)                               0105 00331000
*   EPAACT=OFF;                     /* TURN OFF ENTRY FLAG           */ 00332000
         NI    EPAACT(EPAPTR),B'11101111'                          0106 00333000
*   RFY                                                            0107 00334000
*     R1 RSTD;                                                     0107 00335000
*   R1=R9;                          /* GET PTR TO WORKAREA           */ 00336000
*                                                                  0108 00337000
         LR    R1,R9                                               0108 00338000
*   /*****************************************************************/ 00339000
*   /*                                                               */ 00340000
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */ 00341000
*   /*                                                               */ 00342000
*   /*****************************************************************/ 00343000
*                                                                  0109 00344000
*   DO;                             /* ILRGMA FREE                 0109 00345000
*                                      CELL(BWK)WORKREGS(R11,R12)    */ 00346000
*     RESPECIFY                                                    0110 00347000
*      (R0,                                                        0110 00348000
*       R1,                                                        0110 00349000
*       R11,                                                       0110 00350000
*       R12,                                                       0110 00351000
*       R14,                                                       0110 00352000
*       R15) RESTRICTED;                                           0110 00353000
*     RESPECIFY                                                    0111 00354000
*       ASMPOOLS BASED(R11);                                       0111 00355000
*     RESPECIFY                                                    0112 00356000
*       GMACELL BASED(R1);                                         0112 00357000
*     R11=ADDR(ASMBWKPC);                                          0113 00358000
         L     R11,ASMVTWPT                                        0113 00359000
         LA    R11,ASMBWKPC(,R11)                                  0113 00360000
*     R14=ASMCPAVL;                                                0114 00361000
         L     R14,ASMCPAVL(,R11)                                  0114 00362000
*GMA00022:                                                         0115 00363000
*     GMANEXT=R14;                  /* PUT ADDR CURRENT FIRST IN   0115 00364000
*                                      INPUT CELL                    */ 00365000
GMA00022 ST    R14,GMANEXT(,R1)                                    0115 00366000
*     CS(R14,R1,ASMCPAVL);          /* ADD INPUT CELL TO POOL        */ 00367000
         CS    R14,@01,ASMCPAVL(R11)                               0116 00368000
*     BC(7,GMA00022);                                              0117 00369000
         BC    7,GMA00022                                          0117 00370000
*     RESPECIFY                                                    0118 00371000
*      (R0,                                                        0118 00372000
