         ST    R1,AFSASPCT                                         0306 01224000
*   AFSLNGTH=R0;                    /* SAVE INPUT LENGTH             */ 01225000
         ST    R0,AFSLNGTH                                         0307 01226000
*   AFSREG3=R3;                     /* SAVE CALLER'S REG 3           */ 01227000
         ST    R3,AFSREG3                                          0308 01228000
*   AFSREG4=R4;                     /* SAVE CALLER'S REG 4           */ 01229000
         ST    R4,AFSREG4                                          0309 01230000
*   AFSREG7=R7;                     /* SAVE CALLER'S REG 7           */ 01231000
         ST    R7,AFSREG7                                          0310 01232000
*   RFY                                                            0311 01233000
*    (R0,                                                          0311 01234000
*     R1,                                                          0311 01235000
*     R3,                                                          0311 01236000
*     R4,                                                          0311 01237000
*     R7,                                                          0311 01238000
*     R14) UNRSTD;                                                 0311 01239000
*   IF AFSLNGTH=LENGTH(ASPCT)       /* CHECK WHICH SUBPOOL TO USE    */ 01240000
*     THEN                          /* ACTIVE ASPCT                  */ 01241000
         L     @12,AFSLNGTH                                        0312 01242000
         C     @12,@CF01429                                        0312 01243000
         BNE   @RF00312                                            0312 01244000
*     AFSSPID=AFSSP255;             /* SET SUBPOOL FOR LSQA          */ 01245000
         MVC   AFSSPID(4),@CF00105                                 0313 01246000
*   ELSE                            /* I/O BUFFERS                   */ 01247000
*     AFSSPID=AFSSP230;             /* SET SUBPOOL FOR USER KEY      */ 01248000
         B     @RC00312                                            0314 01249000
@RF00312 MVC   AFSSPID(4),@CF01412                                 0314 01250000
         EJECT                                                          01251000
*                                                                  0315 01252000
*/********************************************************************/ 01253000
*/*                                                                  */ 01254000
*/*  A TEST IS NOW MADE FOR EXTENSIONS. IF THERE ARE EXTENSIONS,     */ 01255000
*/*  THEY ARE FREED FIRST. FIRST ALL LPME EXTENSIONS FOR A PARTICULAR*/ 01256000
*/*  ASST EXTENSION ARE FREED, THEN THAT ASST EXTENSION IS FREED.    */ 01257000
*/*  THIS PROCESS IS REPEATED UNTIL ALL EXTENSIONS ARE FREED.        */ 01258000
*/*                                                                  */ 01259000
*/********************************************************************/ 01260000
*                                                                  0315 01261000
*   IF AFSASPCT->ASPAEXCT^=AFSZERO  /* ARE THERE ANY EXTENSIONS?     */ 01262000
*     THEN                                                         0315 01263000
@RC00312 L     @12,AFSASPCT                                        0315 01264000
         LH    @11,ASPAEXCT(,@12)                                  0315 01265000
         LTR   @11,@11                                             0315 01266000
         BZ    @RF00315                                            0315 01267000
*     DO;                           /* YES                           */ 01268000
*       AFSB=AFSONE;                /* SET BASE SUBSCRIPT FOR FIRST     01269000
*                                      ASST EXTENSION                */ 01270000
         LA    AFSB,1                                              0317 01271000
*       AFSA=AFSTWO;                /* SET ASST SUBSCRIPT FOR FIRST     01272000
*                                      LPME EXTENSION OFF FIRST ASST    01273000
*                                      EXTENSION. GET PTR TO FIRST 0318 01274000
*                                      ASST EXT                      */ 01275000
         LA    AFSA,2                                              0318 01276000
*       AFSEXPTR=AFSASPCT->ASPASSTP(AFSB);                         0319 01277000
         L     @05,AFSASPCT                                        0319 01278000
         LR    @15,AFSB                                            0319 01279000
         SLA   @15,2                                               0319 01280000
         L     AFSEXPTR,ASPASSTP-4(@15,@05)                        0319 01281000
*       DO WHILE AFSEXPTR^=AFSZERO; /* LOOP UNTIL WE HAVE PROCESSED     01282000
*                                      ALL ASST EXTENSIONS GET PTR TO   01283000
*                                      LPME EXT                      */ 01284000
         B     @DE00320                                            0320 01285000
@DL00320 DS    0H                                                  0321 01286000
*         AFSWORK=AFSEXPTR->ASPASSTE(AFSA);                        0321 01287000
         LR    @05,AFSA                                            0321 01288000
         SLA   @05,2                                               0321 01289000
         L     AFSWORK,ASPASSTE-4(@05,AFSEXPTR)                    0321 01290000
*         DO WHILE AFSWORK^=AFSZERO;/* LOOP UNTIL WE HAVE PROCESSED     01291000
*                                      ALL LPME EXTENSIONS FOR A   0322 01292000
*                                      PARTICULAR ASST EXTENSION     */ 01293000
         B     @DE00322                                            0322 01294000
@DL00322 DS    0H                                                  0323 01295000
*           CALL AFSFREE;           /* FREE AN LPME EXTENSION        */ 01296000
         BAL   @14,AFSFREE                                         0323 01297000
*           RFY                                                    0324 01298000
*             R15 RSTD;                                            0324 01299000
*           IF R15=AFSZERO          /* SUCCESSFUL FREEMAIN?          */ 01300000
*             THEN                                                 0325 01301000
         LTR   R15,R15                                             0325 01302000
         BNZ   @RF00325                                            0325 01303000
*             DO;                   /* YES                           */ 01304000
*               RFY                                                0327 01305000
*                 R15 UNRSTD;                                      0327 01306000
*               AFSA=AFSA+AFSONE;   /* INCREMENT ASST SUBSCRIPT      */ 01307000
         AL    AFSA,@CF00080                                       0328 01308000
*               IF AFSA<AFS257      /* REACHED END OF ARRAY          */ 01309000
*                 THEN              /* NO, GET PTR TO NEXT LPME EX   */ 01310000
         C     AFSA,@CF01415                                       0329 01311000
         BNL   @RF00329                                            0329 01312000
*                 AFSWORK=AFSEXPTR->ASPASSTE(AFSA);                0330 01313000
         LR    @05,AFSA                                            0330 01314000
         SLA   @05,2                                               0330 01315000
         L     AFSWORK,ASPASSTE-4(@05,AFSEXPTR)                    0330 01316000
*               ELSE                /* YES, FORCE EXIT FROM LOOP     */ 01317000
*                 AFSWORK=AFSZERO;                                 0331 01318000
         B     @RC00329                                            0331 01319000
@RF00329 SLR   AFSWORK,AFSWORK                                     0331 01320000
*             END;                  /* THIS IS THE LOGICAL END OF THE   01321000
*                                      DO WHILE LOOP FOR LPME      0332 01322000
*                                      EXTENSIONS. PHYSICAL END IS 0332 01323000
*                                      AFTER THE ERROR CODE PROCESS     01324000
*                                      ING FOR A NON-ZERO RETURN FROM   01325000
*                                      AFSFREE.                      */ 01326000
         EJECT                                                          01327000
*                                                                  0333 01328000
*/********************************************************************/ 01329000
*/* THIS SECTION WILL PROCESS A NON-ZERO RETURN CODE FROM AFSFREE    */ 01330000
*/* FOR LPME EXTENSIONS. PROCESSING CONSISTS OF GETTING THE POINTER  */ 01331000
*/* TO THE NEXT LPME EXTENSION, AND VERIFYING THAT IT IS AN LPME     */ 01332000
*/* EXTENSION. IF IT IS WE WILL CONTINUE FREEING LPME EXTENSIONS,    */ 01333000
*/* IF NOT, WE WILL FORCE AN EXIT FROM THE LOOP.                     */ 01334000
*/********************************************************************/ 01335000
*                                                                  0333 01336000
*           ELSE                                                   0333 01337000
*             DO;                   /* NON-ZERO RETURN FROM AFSFREE  */ 01338000
         B     @RC00325                                            0333 01339000
@RF00325 DS    0H                                                  0334 01340000
*               AFSA=AFSA+AFSONE;   /* INCREMENT ASST SUBSCRIPT      */ 01341000
         AL    AFSA,@CF00080                                       0334 01342000
*               IF AFSA<AFS257      /* REACHED END OF ASST ARRAY?    */ 01343000
*                 THEN                                             0335 01344000
