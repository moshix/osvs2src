*               END;                                               0366 01457000
*             ELSE                  /* YES, REACHED END OF ARRAY     */ 01458000
*               AFSEXPTR=AFSZERO;   /* FORCE LOOP EXIT               */ 01459000
         B     @RC00360                                            0367 01460000
@RF00360 SLR   AFSEXPTR,AFSEXPTR                                   0367 01461000
*           END;                                                   0368 01462000
