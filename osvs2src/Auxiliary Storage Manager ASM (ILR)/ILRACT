         TITLE 'ILRACT, ACTIVATE A LOGICAL GROUP                       *00001000
                        '                                               00002000
ILRACT   CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRACT    76.087'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@10                                         0001 00013000
*/*                    END OF CVT MACRO                              */ 00014000
*                                                                  0037 00015000
         EJECT                                                          00016000
*                                                                  0037 00017000
*/*ILRACT: CHART (AREF)                                              */ 00018000
*/*HEADER                                                          0037 00019000
*/*            ILRACT - ACTIVATE GROUP OPERATOR                      */ 00020000
*/*ILRACT: E ENTRY                                                   */ 00021000
*/*         P  SAVE CALLER'S REGISTERS                               */ 00022000
*/*         S  ILRGMA: GET A WORKAREA FROM POOL                      */ 00023000
*/*         D  (YES,%ACT01,NO,) ZERO RETURN CODE?                    */ 00024000
*/*         P  SET RETURN CODE TO 28                                 */ 00025000
*/*         R  RETURN TO CALLER                                      */ 00026000
*/*%ACT01:  P  SAVE POINTER TO WORKAREA IN REGISTER 9                */ 00027000
*/*         P  SETUP ENTRY REGISTERS                                 */ 00028000
*/*         P  GET PTR TO EPATH FROM INPUT REG 4                     */ 00029000
*/*         P  RECORD ENTRY INFORMATION IN EPATH                     */ 00030000
*/*         P  RECORD PTR TO WORKAREA IN EPATH                       */ 00031000
*/*         P  GET PTR TO ACE FROM INPUT REG 1                       */ 00032000
*/*         P  GET PTR TO ASMVT FROM INPUT REG 3                     */ 00033000
*/*         P  GET PTR TO LGE FROM ACE                               */ 00034000
*/*         P  STORE PTR TO EPATH IN VSAMI PARM LIST                 */ 00035000
*/*         P  STORE PTR TO 'S' SYMBOL IN VSAMI PARM LIST            */ 00036000
*/*         P  SET REQUEST OP-CODE TO 01                             */ 00037000
*/*         S  ILRVSAMI: GET                                         */ 00038000
*/*         D  (YES,%ACT04,NO,) ZERO RETURN CODE?                    */ 00039000
*/*         P  (,%ACT08) SAVE THE RETURN CODE                        */ 00040000
*/*%ACT04:  P  SAVE THE POINTER TO RETRIEVED ASPCT                   */ 00041000
*/*         P  COPY THE LGN TO RETRIEVED ASPCT                       */ 00042000
*/*         P  RECALCULATE THE NUMBER OF BACKUP SLOTS REQUIRED       */ 00043000
*/*         P  INITIALIZE ASPCT'S PTR TO ASCB                        */ 00044000
*/*         P  INITIALIZE ASPCT'S PTR TO LGE                         */ 00045000
*/*         S  REBUILD: REBUILD THE ASPCT IN LSQA STORAGE            */ 00046000
*/*         D  (YES,%ACT08,NO,) RETURN CODE 28?                      */ 00047000
*/*%ACT06:  P  ANCHOR REBUILT ASPCT IN LGE                           */ 00048000
*/*         P  ZERO PTR TO LSQA STORAGE & LENGTH IN EPATH            */ 00049000
*/*         P  COPY THE SAVED COUNT TO THE LGE                       */ 00050000
*/*%ACT08:  P  GET CALLER'S REG 13                                   */ 00051000
*/*         P  COPY INTERNAL RETURN CODE TO CALLER'S REG 15          */ 00052000
*/*         S  ILRGMA: FREE THE WORKAREA                             */ 00053000
*/*         P  RESTORE REGISTERS                                     */ 00054000
*/*         R  RETURN TO CALLER                                      */ 00055000
*/*ILRACT: END                                                       */ 00056000
*                                                                  0037 00057000
         EJECT                                                          00058000
*                                                                  0037 00059000
*/********************************************************************/ 00060000
*/*  INITIALIZATION: GET A WORKAREA & ESTABLISH ADDRESSIBILITY       */ 00061000
*/********************************************************************/ 00062000
*                                                                  0037 00063000
*   /*****************************************************************/ 00064000
*   /*                                                               */ 00065000
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */ 00066000
*   /*                                                               */ 00067000
*   /*****************************************************************/ 00068000
*                                                                  0037 00069000
*   DO;                             /* ILRGMA GET                  0037 00070000
*                                      CELL(BWK)WORKREGS(R11,R12)    */ 00071000
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */ 00072000
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0038 00073000
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */ 00074000
*       1 GMACELL BASED BDY(WORD),                                 0039 00075000
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */ 00076000
*     RESPECIFY                                                    0040 00077000
*      (R0,                                                        0040 00078000
*       R1,                                                        0040 00079000
*       R11,                                                       0040 00080000
*       R12,                                                       0040 00081000
*       R14,                                                       0040 00082000
*       R15) RESTRICTED;                                           0040 00083000
*     RESPECIFY                                                    0041 00084000
*       ASMPOOLS BASED(R11);                                       0041 00085000
*     RESPECIFY                                                    0042 00086000
*       GMACELL BASED(R14);                                        0042 00087000
*     R11=ADDR(ASMBWKPC);                                          0043 00088000
         LA    R11,ASMBWKPC(,ASMVTPTR)                             0043 00089000
*     LM(R14,R15,ASMCPAVQ);         /* GET ADDR OF 1ST CELL          */ 00090000
         LM    R14,R15,ASMCPAVQ(R11)                               0044 00091000
*GMA00010:                                                         0045 00092000
*     IF R14^=0 THEN                /* TEST CELL POOL                */ 00093000
GMA00010 LTR   R14,R14                                             0045 00094000
         BZ    @RF00045                                            0045 00095000
*       DO;                         /* CELL AVAILABLE, REMOVE CELL   */ 00096000
*         R0=GMANEXT;                                              0047 00097000
         L     R0,GMANEXT(,R14)                                    0047 00098000
*         R1=R15-1;                                                0048 00099000
         LR    R1,R15                                              0048 00100000
         BCTR  R1,0                                                0048 00101000
*         CDS(R14,R0,ASMCPAVQ);                                    0049 00102000
         CDS   R14,R0,ASMCPAVQ(R11)                                0049 00103000
*         BC(7,GMA00010);                                          0050 00104000
         BC    7,GMA00010                                          0050 00105000
*         R1=R14;                                                  0051 00106000
         LR    R1,R14                                              0051 00107000
*       END;                                                       0052 00108000
*     ELSE                                                         0053 00109000
*       DO;                         /* CELL POOL EMPTY               */ 00110000
         B     @RC00045                                            0053 00111000
@RF00045 DS    0H                                                  0054 00112000
*         R1=R11;                   /* PREPARE TO CALL PEX           */ 00113000
         LR    R1,R11                                              0054 00114000
*         CALL ILRPEX;              /* EXTEND THE POOL               */ 00115000
         L     @15,ASMPEX(,ASMVTPTR)                               0055 00116000
         BALR  @14,@15                                             0055 00117000
*       END;                                                       0056 00118000
*     RESPECIFY                                                    0057 00119000
*      (R0,                                                        0057 00120000
*       R1,                                                        0057 00121000
*       R11,                                                       0057 00122000
*       R12,                                                       0057 00123000
*       R14,                                                       0057 00124000
*       R15) UNRESTRICTED;                                         0057 00125000
@RC00045 DS    0H                                                  0058 00126000
*   END;                            /* ILRGMA GET                  0058 00127000
*                                      CELL(BWK)WORKREGS(R11,R12)    */ 00128000
*                                                                  0058 00129000
*   /*****************************************************************/ 00130000
*   /*                                                               */ 00131000
*   /* END ILRGMA MACRO EXPANSION                                    */ 00132000
*   /*                                                               */ 00133000
*   /*****************************************************************/ 00134000
*                                                                  0059 00135000
*   RFY                                                            0059 00136000
*    (R1,                                                          0059 00137000
*     R3) RSTD;                                                    0059 00138000
         EJECT                                                          00139000
*                                                                  0060 00140000
*/********************************************************************/ 00141000
*/* IF A WORKAREA WAS NOT OBTAINED, RETURN TO ILRGOS WITH A RETURN   */ 00142000
*/* CODE OF 28 TO INDICATE WE COULD NOT OBTAIN NECESSARY STORAGE.    */ 00143000
*/* IF A WORKAREA WAS OBTAINED, ESTABLISH ADDRESSIBILITY TO IT,      */ 00144000
*/* SETUP A SAVEAREA AND RELOAD INPUT REGISTERS 1 & 3.               */ 00145000
*/********************************************************************/ 00146000
*                                                                  0060 00147000
*   IF R1=ZERO                      /* DID WE GET A WORKAREA?        */ 00148000
*     THEN                          /* NO, RETURN TO ILRGOS          */ 00149000
         LTR   R1,R1                                               0060 00150000
         BNZ   @RF00060                                            0060 00151000
*     RETURN CODE(STORAGE);         /* RETURN INDICATING WE COULD NOT   00152000
*                                      OBTAIN STORAGE                */ 00153000
         LA    @15,28                                              0061 00154000
         L     @14,12(,@13)                                        0061 00155000
         LM    @00,@12,20(@13)                                     0061 00156000
         BR    @14                                                 0061 00157000
*   ELSE                                                           0062 00158000
*     DO;                           /* YES, ESTAB. ADDRESSIBILITY    */ 00159000
@RF00060 DS    0H                                                  0063 00160000
*       GENERATE REFS(R1,R13) SETS(R1,R3,R8,R9,R13);               0063 00161000
        LR    R9,R1                      GET ADDRESS OF WORKAREA        00162000
        USING @DATD,R9                                                  00163000
        LA    R8,SAVEREGA                GET ADDRESS OF SAVEAREA        00164000
        ST    R13,4(R8)                  SET BACKWARD POINTER           00165000
        ST    R8,8(R13)                  SET FORWARD POINTER            00166000
        L     R1,24(R13)                 RESTORE INPUT REG 1            00167000
        LR    R13,R8                     ESTABLISH SAVEAREA POINTER     00168000
*     END;                                                         0064 00169000
         EJECT                                                          00170000
*                                                                  0065 00171000
*/********************************************************************/ 00172000
*/*  INITIALIZE INPUT POINTER                                        */ 00173000
*/********************************************************************/ 00174000
*                                                                  0065 00175000
*   ACEPTR=R1;                      /* SAVE PTR TO INPUT ACE         */ 00176000
         LR    ACEPTR,R1                                           0065 00177000
*   ASMVTWPT=R3;                    /* SAVE INPUT PTR TO ASMVT       */ 00178000
         ST    R3,ASMVTWPT                                         0066 00179000
*   EPATHPTR=EPAPTR;                /* SAVE INPUT PTR TO EPATH IN  0067 00180000
