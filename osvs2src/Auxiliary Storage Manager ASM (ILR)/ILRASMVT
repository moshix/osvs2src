*%ASMVTPLS:;                                                            03324040
*DECLARE                                                                03332040
*  1 ASMVT BASED(ASMVTPTR) BDY(DWORD), /* ASM VECTOR TABLE           */ 03350040
*    3 ASMFLAG1 BIT(8),          /* ASM GLOBAL FLAG FIELD 1          */ 03358040
*      5 ASMDUPLX BIT(1),        /* DUPLEXING OPTION FLAG,              03366040
*                                   1 = DUPLEXING OF COMMON REQUESTED   03374040
