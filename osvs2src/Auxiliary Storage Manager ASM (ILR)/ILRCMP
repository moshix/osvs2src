*                                      CT                            */ 01025000
         L     @12,PAREPTR                                         0385 01026000
         L     R0,PARERRCT(,@12)                                   0385 01027000
*RETRYER1:                                                         0386 01028000
*           R1=R0+ONE;              /* ADD THIS ERROR                */ 01029000
RETRYER1 LA    R1,1                                                0386 01030000
         ALR   R1,R0                                               0386 01031000
*           CS(R0,R1,PARERRCT);     /* UPDATE PARTE/SARTE ERROR CT   */ 01032000
         L     @12,PAREPTR                                         0387 01033000
         CS    R0,@01,PARERRCT(@12)                                0387 01034000
*           BC(NE,RETRYER1);                                       0388 01035000
         BC    7,RETRYER1                                          0388 01036000
*           RFY                                                    0389 01037000
*            (R0,                                                  0389 01038000
*             R1) UNRSTD;                                          0389 01039000
*           CALL RECERR;            /* RECORD THIS BAD SLOT ID       */ 01040000
         BAL   @14,RECERR                                          0390 01041000
*         END;                      /* END SLOT ERROR                */ 01042000
*                                                                  0392 01043000
*       /*************************************************************/ 01044000
*       /*                                                           */ 01045000
*       /* MARK BAD SLOT AIA(S) (MAYBE MORE THAN ONE IF PACK ERROR)  */ 01046000
*       /* AS PRI OR SEC ERROR + PLACE ON INTERNAL POST QUEUE. PLACE */ 01047000
*       /* P/SCCW ON INTERNAL FREE QUEUE.                            */ 01048000
*       /*                                                           */ 01049000
*       /*************************************************************/ 01050000
*                                                                  0392 01051000
