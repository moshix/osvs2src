*         ATACOMPQ=PCCWAIA;         /* PUT THIS AIA ON TOP OF      0549 01866000
*                                      INTERNAL POST QUEUE   @ZA16409*/ 01867000
         ST    @15,ATACOMPQ(,ATAPTR)                               0549 01868000
*         AIAPRIER=ON;              /* SET PRIMARY ERROR BIT @ZA16409*/ 01869000
