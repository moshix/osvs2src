*   /*****************************************************************/ 01194000
*   /*                                                               */ 01195000
*   /* INITIALIZE REQUIRED VARIABLES                                 */ 01196000
*   /*                                                               */ 01197000
*   /*****************************************************************/ 01198000
*                                                                  0423 01199000
*   ASMVTPTR=CVTASMVT;              /* BASE FOR ASMVT                */ 01200000
         L     @15,CVTPTR                                          0423 01201000
         L     ASMVTPTR,CVTASMVT(,@15)                             0423 01202000
*   IOSBPTR=R9;                     /* SET IOSB BASE                 */ 01203000
         ST    R9,IOSBPTR                                          0424 01204000
*   IORBPTR=IOSUSE;                 /* SET IORB BASE                 */ 01205000
         L     @15,IOSUSE(,R9)                                     0425 01206000
         ST    @15,IORBPTR                                         0425 01207000
*   RFY                                                            0426 01208000
