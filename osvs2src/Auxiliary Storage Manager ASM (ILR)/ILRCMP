         L     @12,PAREPTR                                         0374 00994000
         TM    PARELOCL(@12),B'00010000'                           0374 00995000
         BNO   @RF00374                                            0374 00996000
*             DO;                   /* YES- UPDATE BAD LOCAL PAGE  0375 00997000
*                                      SLOTS CT- ASMERRS             */ 00998000
*               RFY                                                0376 00999000
*                (R0,                                              0376 01000000
*                 R1) RSTD;                                        0376 01001000
*               R0=ASMERRS;         /* GET CURRENT CT                */ 01002000
         L     R0,ASMERRS(,ASMVTPTR)                               0377 01003000
