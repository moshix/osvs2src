         LA    RECPTR,SDWAVRA(,SDWAPTR)                            0149 00151000
         ALR   RECPTR,@08                                          0149 00152000
*           RECCOD=IOSCOD;          /* RECORD IOS COMPLETION CODE    */ 00153000
         MVC   RECCOD(1,RECPTR),IOSCOD(IOSBPTR)                    0150 00154000
*           RECUCB=RUCBCSW;         /* RECORD UCB ADDRESS AND CSW    */ 00155000
         MVC   RECUCB(19,RECPTR),RUCBCSW(IOSBPTR)                  0151 00156000
*           RECSEEK=IOSEEK;         /* RECORD FULL SEEK ADDRESS      */ 00157000
         MVC   RECSEEK(8,RECPTR),IOSEEK(IOSBPTR)                   0152 00158000
*           RECVST=IOSVST;          /* RECORD STARTING ADDRESS OF  0153 00159000
*                                      CHANNEL PROGRAM               */ 00160000
         MVC   RECVST(4,RECPTR),IOSVST(IOSBPTR)                    0153 00161000
*           RFY                                                    0154 00162000
*             RECPTR UNRSTD;        /* RELEASE REGISTER              */ 00163000
*           URAL=URAL+LENGTH(RECAREA);/* UPDATE LENGTH OF USER     0155 00164000
*                                      SUPPLIED INFORMATION          */ 00165000
         STC   @15,URAL(,SDWAPTR)                                  0155 00166000
*         END;                      /* END OF RECORDING              */ 00167000
*       ELSE                                                       0157 00168000
*         ;                         /* NO RECORDING IF NOT ENOUGH  0157 00169000
*                                      ROOM                          */ 00170000
@RF00146 DS    0H                                                  0158 00171000
*     END;                          /* END FOR RECORD ONLY ABEND     */ 00172000
*   ELSE                                                           0159 00173000
*     ;                             /* FALL INTO COMMON PROCESSING   */ 00174000
@RF00144 DS    0H                                                  0160 00175000
*                                                                  0160 00176000
*   /*****************************************************************/ 00177000
*   /*                                                               */ 00178000
*   /* VERIFY CHECKPOINTED QUEUES BEFORE STARTING RECOVERY PROCESSING*/ 00179000
*   /*                                                               */ 00180000
*   /*****************************************************************/ 00181000
*                                                                  0160 00182000
*   RFY                                                            0160 00183000
*     ASMVTPTR RSTD;                /* RESTRICT REG                  */ 00184000
*   ASMVTPTR=CVTASMVT;              /* SET UP ASMVT ADDRESS          */ 00185000
