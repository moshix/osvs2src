*                                      ONE TO INDICATE IT IS       0319 00814000
*                                      SCHEDULED                     */ 00815000
         LA    REPLV,1                                             0319 00816000
*           CS(COMPV,REPLV,ASMSRBCT);/* TRY TO SET SRB SCHEDULED   0320 00817000
*                                      COUNT                         */ 00818000
         CS    COMPV,@05,ASMSRBCT(ASMVTPTR)                        0320 00819000
*           BC(NE,NOTSCHED);        /* DO NOT SCHEDULE IF ALREADY  0321 00820000
*                                      SCHEDULED                     */ 00821000
         BC    7,NOTSCHED                                          0321 00822000
*           RFY                                                    0322 00823000
*            (PARMPTR,                                             0322 00824000
*             R14,                                                 0322 00825000
*             R15) RSTD;            /* RESTRICT INTERFACE REGS       */ 00826000
*           PARMPTR=ASMPSRB;        /* GET ADDRESS OF PART MONITOR 0323 00827000
*                                      SRB                           */ 00828000
         L     PARMPTR,ASMPSRB(,ASMVTPTR)                          0323 00829000
*           GEN(SCHEDULE SRB=(1),SCOPE=GLOBAL)                     0324 00830000
