         TITLE 'ILRCMP01, I/O COMPLETION RECOVERY                      *00001000
                        '                                               00002000
ILRCMP01 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRCMP01  76.091'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @10,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@10                                         0001 00012000
*                                      WORKAREA & RETURN ADDRESS     */ 00013000
*   DATARG=R0;                      /* SET UP DATA REGISTER          */ 00014000
         LR    DATARG,R0                                           0110 00015000
*   RFY                                                            0111 00016000
*     R0 UNRSTD;                    /* RELEASE REG 0                 */ 00017000
*   GEN( USING @DATD,DATARG);       /* SET UP ADDRESSABILITY TO DATA    00018000
*                                      AREA                          */ 00019000
          USING @DATD,DATARG                                            00020000
*   ADATA=''B;                      /* ZERO DATA AREA                */ 00021000
         XC    ADATA(200,DATARG),ADATA(DATARG)                     0113 00022000
*   SAVERTN=R14;                    /* SAVE RETURN ADDRESS           */ 00023000
         ST    R14,SAVERTN                                         0114 00024000
*   RFY                                                            0115 00025000
*     R14 UNRSTD;                   /* RELEASE REG14                 */ 00026000
*   RFY                                                            0116 00027000
*     ATAPTR RSTD;                  /* RESTRICT ATA POINTER          */ 00028000
*   ATAPTR=SDWAPARM;                /* SET UP ADDRESS OF ATA         */ 00029000
         L     ATAPTR,SDWAPARM(,SDWAPTR)                           0117 00030000
*   SDWAVRA(1:LENGTH(ATA))=ATA;     /* MOVE ATA TO SDWA              */ 00031000
         MVC   SDWAVRA(24,SDWAPTR),ATA(ATAPTR)                     0118 00032000
*   URAL=LENGTH(ATA);               /* SET LENGTH OF AREA USED       */ 00033000
         MVI   URAL(SDWAPTR),X'18'                                 0119 00034000
*   DO;                             /* SETRP RC(0)RECPARM(MODID)RECOR   00035000
*                                      D(YES)FRELOCK(SALLOC)         */ 00036000
*     RESPECIFY                                                    0121 00037000
*      (GPR00P,                                                    0121 00038000
*       GPR01P,                                                    0121 00039000
*       GPR14P,                                                    0121 00040000
*       GPR15P) RSTD;                                              0121 00041000
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */ 00042000
         MVI   SDWARCDE(GPR01P),X'00'                              0122 00043000
*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */ 00044000
         OI    SDWARCRD(GPR01P),B'10000000'                        0123 00045000
*     GPR15P=ADDR(MODID);           /* ACCESS RECORD PARAMETER LIST  */ 00046000
         LA    GPR15P,MODID                                        0124 00047000
*     GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD PARAMETERS */ 00048000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   0125 00049000
*     GPR01P->SDWAACF3=GPR01P->SDWAACF3|'00000100'B;/* INITIALIZE  0126 00050000
*                                      LOCK FLAGS                    */ 00051000
         OI    SDWAACF3(GPR01P),B'00000100'                        0126 00052000
*     RESPECIFY                                                    0127 00053000
*      (GPR00P,                                                    0127 00054000
*       GPR01P,                                                    0127 00055000
*       GPR14P,                                                    0127 00056000
*       GPR15P) UNRSTD;                                            0127 00057000
*   END;                            /* INDICATE CONTINUE           0128 00058000
*                                      WITHTTERMINATION, RECORD    0128 00059000
*                                      MODULE NAMES, INDICATE RECORD    00060000
*                                      ERROR, AND FREE SALLOC LOCK,     00061000
*                                      IF HELD                       */ 00062000
*                                                                  0129 00063000
*   /*****************************************************************/ 00064000
*   /*                                                               */ 00065000
*   /* VALIDITY CHECK IOSB                                           */ 00066000
*   /*                                                               */ 00067000
*   /*****************************************************************/ 00068000
*                                                                  0129 00069000
*   IF ATAIOSB=ZERO                 /* IOSB CHECKPOINTED?            */ 00070000
