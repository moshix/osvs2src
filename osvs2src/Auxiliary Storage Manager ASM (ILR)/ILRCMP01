*     THEN                                                         0173 00222000
         L     @05,IORBPTR(,IOSBPTR)                               0173 00223000
         TM    IORSWAP(@05),B'00001000'                            0173 00224000
         BNO   @RF00173                                            0173 00225000
*     DO;                           /* SWAP DATA SET                 */ 00226000
*       QVERRTNP=ADDR(ILRVSCWQ);    /* SET UP VERIFICATION RTN     0175 00227000
