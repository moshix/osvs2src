*             R15) UNRSTD;          /* RELEASE REGS                  */ 00837000
*         END;                      /* END FOR SCHEDULING PART     0326 00838000
*                                      MONITOR                       */ 00839000
*NOTSCHED:                                                         0327 00840000
*       ;                           /* LABEL FOR UNSUCCESSFUL ATTEMPT   00841000
*                                      TO SCHEDULE PART MONITOR      */ 00842000
@RF00316 DS    0H                                                  0327 00843000
NOTSCHED DS    0H                                                  0328 00844000
*       RFY                                                        0328 00845000
*        (COMPV,                                                   0328 00846000
*         REPLV) UNRSTD;            /* RELEASE REGS                  */ 00847000
*     END;                          /* END FOR PART MONITOR TO BE  0329 00848000
*                                      SCHEDULED                     */ 00849000
*   RFY                                                            0330 00850000
*     ASMVTPTR UNRSTD;              /* RELEASE REG                   */ 00851000
@RF00313 DS    0H                                                  0331 00852000
*   RFY                                                            0331 00853000
*     R14 RSTD;                                                    0331 00854000
*   R14=SAVERTN;                    /* RESTORE RETURN ADDRESS        */ 00855000
