*     R3,                                                          0135 00095000
*     R4,                                                          0135 00096000
*     R5,                                                          0135 00097000
*     R6,                                                          0135 00098000
*     R7) UNRSTD;                   /* RELEASE REGS                  */ 00099000
*   IF RETCODE^=ZERO                /* VALIDITY CHECK SUCCESSFULLY?  */ 00100000
*     THEN                                                         0136 00101000
         SLR   @12,@12                                             0136 00102000
         CR    RETCODE,@12                                         0136 00103000
         BE    @RF00136                                            0136 00104000
*     DO;                           /* NO, RETURN TO RTM             */ 00105000
*       ATAIOSB=ZERO;               /* SET UP SO PART MONITOR AND/OR    00106000
*                                      SWAP DRIVER WILL BE SCHEDULED */ 00107000
         ST    @12,ATAIOSB(,ATAPTR)                                0138 00108000
*       GOTO EXIT;                  /* GO TO EXIT LOGIC              */ 00109000
         B     EXIT                                                0139 00110000
*     END;                          /* END OF INVALID IOSB           */ 00111000
*   RFY                                                            0141 00112000
*    (R14,                                                         0141 00113000
*     RETCODE) UNRSTD;              /* RELEASE REGS                  */ 00114000
@RF00136 DS    0H                                                  0142 00115000
*                                                                  0142 00116000
*   /*****************************************************************/ 00117000
*   /*                                                               */ 00118000
*   /* IORB-IOSB-SRB VALID - CHECK FOR RECORD ONLY ABEND             */ 00119000
*   /*                                                               */ 00120000
