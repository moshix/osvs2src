         TITLE 'ILRFMT00   ASM CONTROL-BLOCK FORMATTING MODULE         *00001000
                        '                                               00002000
ILRFMT00 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRFMT00  78.136'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @09,0                                               0001 00011000
@PSTART  LA    @10,4095(,@09)                                      0001 00012000
         LA    @12,4095(,@10)                                      0001 00013000
         USING @PSTART,@09                                         0001 00014000
         USING @PSTART+4095,@10                                    0001 00015000
         USING @PSTART+8190,@12                                    0001 00016000
         ST    @13,@SA00001+4                                      0001 00017000
         LA    @14,@SA00001                                        0001 00018000
         ST    @14,8(,@13)                                         0001 00019000
         LR    @13,@14                                             0001 00020000
*   ABDPLPTR=R1;                    /* GET ADDR OF PARM LIST         */ 00021000
         ST    R1,ABDPLPTR                                         0083 00022000
*   IF(ADPLSYTM^=OFF                /* MUST BE VS/2 SYSTEM           */ 00023000
*       |ADPLCVT=NULLPTR) THEN      /* CHECK FOR CVT PTR             */ 00024000
         L     @11,ABDPLPTR                                        0084 00025000
         TM    ADPLSYTM(@11),B'01000000'                           0084 00026000
         BNZ   @RT00084                                            0084 00027000
         ICM   @08,15,ADPLCVT(@11)                                 0084 00028000
         BNZ   @RF00084                                            0084 00029000
@RT00084 DS    0H                                                  0085 00030000
*     DO;                                                          0085 00031000
*       R1=ENVMSG;                  /* GET ADDRESS OF ERROR MSG      */ 00032000
         L     R1,ENVMSG                                           0086 00033000
*       CALL ERRMSG;                /* GO TO MESSAGE SEGMENT         */ 00034000
         BAL   @14,ERRMSG                                          0087 00035000
*       RETURN CODE(0);             /* RETURN TO CALLER              */ 00036000
         SLR   @15,@15                                             0088 00037000
         L     @13,4(,@13)                                         0088 00038000
         L     @14,12(,@13)                                        0088 00039000
         LM    @00,@12,20(@13)                                     0088 00040000
         BR    @14                                                 0088 00041000
*     END;                                                         0089 00042000
*                                                                  0089 00043000
*/********************************************************************/ 00044000
*/*****    PRINT TITLE TO HEAD ALL PRINTED INFORMATION           *****/ 00045000
*/********************************************************************/ 00046000
*                                                                  0090 00047000
*   BUFFER(40:40+LENGTH(ASMTITLE))=ASMTITLE;/* SET UP BUFFER         */ 00048000
@RF00084 L     @11,ABDPLPTR                                        0090 00049000
         L     @08,ADPLBUF(,@11)                                   0090 00050000
         MVI   BUFFER+69(@08),C' '                                 0090 00051000
         MVC   BUFFER+39(30,@08),ASMTITLE                          0090 00052000
*   R1=ABDPLPTR;                    /* PARM LIST PTR FOR PRINT       */ 00053000
         LR    R1,@11                                              0091 00054000
*   CALL PRINT;                     /* PRINT OUT HEADER              */ 00055000
         L     @15,ADPLPRNT(,@11)                                  0092 00056000
         BALR  @14,@15                                             0092 00057000
*   CALL PRINT;                     /* PRINT BLANK LINE              */ 00058000
         L     @11,ABDPLPTR                                        0093 00059000
         L     @15,ADPLPRNT(,@11)                                  0093 00060000
         BALR  @14,@15                                             0093 00061000
*   CALL PRINT;                     /* PRINT BLANK LINE              */ 00062000
         L     @11,ABDPLPTR                                        0094 00063000
         L     @15,ADPLPRNT(,@11)                                  0094 00064000
         BALR  @14,@15                                             0094 00065000
*   RFY                                                            0095 00066000
*     R1 UNRSTD;                                                   0095 00067000
*   CVTPTR=ADPLCVT;                 /* GET CVT PTR FROM PARMLIST     */ 00068000
         L     @11,ABDPLPTR                                        0096 00069000
         L     @08,ADPLCVT(,@11)                                   0096 00070000
         ST    @08,CVTPTR                                          0096 00071000
*   DO;                             /* ILRMACCS (ASMVT) VIA(CVTASMVT)*/ 00072000
*     RFY                                                          0098 00073000
*      (R0,                                                        0098 00074000
*       R1,                                                        0098 00075000
*       R14,                                                       0098 00076000
*       R15) RSTD;                                                 0098 00077000
*     R1=ABDPLPTR;                                                 0099 00078000
         LR    R1,@11                                              0099 00079000
*     R0=ADDR(CVTASMVT);            /* ADDR OF CELL WITH PTR         */ 00080000
         LA    R0,CVTASMVT(,@08)                                   0100 00081000
*     CALL ACCESS;                  /* ACCESS PTR TO ASMVT           */ 00082000
         L     @15,ADPLMEMA(,@11)                                  0101 00083000
         BALR  @14,@15                                             0101 00084000
*     IF R15=GOOD THEN                                             0102 00085000
         LTR   R15,R15                                             0102 00086000
         BNZ   @RF00102                                            0102 00087000
*       DO;                         /* FOUND PTR - NOW FIND BLOCK    */ 00088000
*         R14=R0;                   /* CANNOT USE R0 AS BASE REG     */ 00089000
         LR    R14,R0                                              0104 00090000
*         R0=CURRENT;               /* POINTER ITSELF                */ 00091000
         L     R0,CURRENT(,R14)                                    0105 00092000
*         ASMVTPTR=R0;              /* SAVE POINTER                  */ 00093000
         ST    R0,ASMVTPTR                                         0106 00094000
*         CALL ACCESS;              /* ACCESS ASMVT ITSELF           */ 00095000
         L     @11,ABDPLPTR                                        0107 00096000
         L     @15,ADPLMEMA(,@11)                                  0107 00097000
         BALR  @14,@15                                             0107 00098000
*         IF R15^=GOOD THEN                                        0108 00099000
         SLR   @11,@11                                             0108 00100000
         CR    R15,@11                                             0108 00101000
         BE    @RF00108                                            0108 00102000
*           ASMVTPTR=NULLPTR;                                      0109 00103000
         ST    @11,ASMVTPTR                                        0109 00104000
*       END;                                                       0110 00105000
*     ELSE                                                         0111 00106000
*       ASMVTPTR=NULLPTR;                                          0111 00107000
         B     @RC00102                                            0111 00108000
@RF00102 SLR   @11,@11                                             0111 00109000
         ST    @11,ASMVTPTR                                        0111 00110000
*     RFY                                                          0112 00111000
*      (R0,                                                        0112 00112000
*       R1,                                                        0112 00113000
*       R14,                                                       0112 00114000
*       R15) UNRSTD;                                               0112 00115000
@RC00102 DS    0H                                                  0113 00116000
*   END;                            /* GET ADDR OF ASMVT             */ 00117000
*   IF ASMVTPTR=NULLPTR THEN        /* IF ASMVT INACCESSIBLE         */ 00118000
         ICM   @11,15,ASMVTPTR                                     0114 00119000
         BNZ   @RF00114                                            0114 00120000
*     DO;                                                          0115 00121000
*       RFY                                                        0116 00122000
*         R1 RSTD;                                                 0116 00123000
*       R1=CVTMSG;                  /* GET ADDRESS OF ERROR MSG      */ 00124000
         L     R1,CVTMSG                                           0117 00125000
*       CALL ERRMSG;                /* GO TO MESSAGE SEGMENT         */ 00126000
         BAL   @14,ERRMSG                                          0118 00127000
*       RETURN CODE(0);             /* RETURN TO CALLER              */ 00128000
         SLR   @15,@15                                             0119 00129000
         L     @13,4(,@13)                                         0119 00130000
         L     @14,12(,@13)                                        0119 00131000
         LM    @00,@12,20(@13)                                     0119 00132000
         BR    @14                                                 0119 00133000
*       RFY                                                        0120 00134000
*         R1 UNRSTD;                                               0120 00135000
*     END;                                                         0121 00136000
*   ELSE                                                           0122 00137000
*     ;                             /* ACCESS OKAY FOR ASMVT         */ 00138000
@RF00114 DS    0H                                                  0123 00139000
*                                                                  0123 00140000
*/********************************************************************/ 00141000
*/*****    DETERMINE IF THIS DUMP IS THE CORRECT RELEASE         *****/ 00142000
*/********************************************************************/ 00143000
*                                                                  0123 00144000
*   RFY                                                            0123 00145000
*    (R0,                                                          0123 00146000
*     R1,                                                          0123 00147000
*     R15) RSTD;                                                   0123 00148000
*   R0=ASMVTPTR-24;                 /* POINT TO ASMVT MODULE ID      */ 00149000
         L     R0,ASMVTPTR                                         0124 00150000
         SH    R0,@CH00198                                         0124 00151000
*   R1=ABDPLPTR;                    /* POINT TO PARM LIST            */ 00152000
         L     @14,ABDPLPTR                                        0125 00153000
         LR    R1,@14                                              0125 00154000
*   CALL ACCESS;                    /* ACCESS MOD ID                 */ 00155000
         L     @15,ADPLMEMA(,@14)                                  0126 00156000
         BALR  @14,@15                                             0126 00157000
*   IF R15=GOOD THEN                /* ACCESS WAS GOOD               */ 00158000
         LTR   R15,R15                                             0127 00159000
         BNZ   @RF00127                                            0127 00160000
*     DO;                                                          0128 00161000
*       ASMIDPTR=R0;                /* SAVE ACCESSED ADDRESS         */ 00162000
         LR    ASMIDPTR,R0                                         0129 00163000
*       IF ASMIDPTR->IDASM^=MODID THEN/* MODULE ID NOT PRESENT       */ 00164000
         CLC   IDASM(8,ASMIDPTR),MODID                             0130 00165000
         BE    @RF00130                                            0130 00166000
*         DO;                                                      0131 00167000
*           R1=RELMSG;              /* GET CODE OF MESSAGE           */ 00168000
         L     R1,RELMSG                                           0132 00169000
*           CALL ERRMSG;            /* GO TO MESSAGE SEGMENT         */ 00170000
         BAL   @14,ERRMSG                                          0133 00171000
*           RETURN CODE(0);         /* RETURN TO CALLER              */ 00172000
         SLR   @15,@15                                             0134 00173000
         L     @13,4(,@13)                                         0134 00174000
         L     @14,12(,@13)                                        0134 00175000
         LM    @00,@12,20(@13)                                     0134 00176000
         BR    @14                                                 0134 00177000
*         END;                                                     0135 00178000
*       ELSE                                                       0136 00179000
*         ;                         /* MODID OKAY - CONTINUE         */ 00180000
@RF00130 DS    0H                                                  0137 00181000
*     END;                                                         0137 00182000
*   ELSE                            /* ACCESS FAILED FOR MODID       */ 00183000
*     DO;                                                          0138 00184000
         B     @RC00127                                            0138 00185000
@RF00127 DS    0H                                                  0139 00186000
*       R1=CVTMSG;                  /* GET MESSAGE CODE              */ 00187000
         L     R1,CVTMSG                                           0139 00188000
*       CALL ERRMSG;                /* GO TO MESSAGE SEGMENT         */ 00189000
         BAL   @14,ERRMSG                                          0140 00190000
*       RETURN CODE(0);             /* RETURN TO CALLER              */ 00191000
         SLR   @15,@15                                             0141 00192000
         L     @13,4(,@13)                                         0141 00193000
         L     @14,12(,@13)                                        0141 00194000
         LM    @00,@12,20(@13)                                     0141 00195000
         BR    @14                                                 0141 00196000
*     END;                                                         0142 00197000
*   RFY                                                            0143 00198000
*    (R0,                                                          0143 00199000
*     R1,                                                          0143 00200000
*     R15) UNRSTD;                                                 0143 00201000
@RC00127 DS    0H                                                  0144 00202000
*                                                                  0144 00203000
*/********************************************************************/ 00204000
*/*  CALL ROUTINES TO FORMAT CONTROL BLOCKS                          */ 00205000
*/********************************************************************/ 00206000
*                                                                  0144 00207000
*   CALL FMTASMVT;                  /* CALL SEGMENT TO FMT ASMVT,  0144 00208000
*                                      ACES, AIAS, ERROR RECORD AND     00209000
*                                      MESSAGE BUFFER                */ 00210000
         BAL   @14,FMTASMVT                                        0144 00211000
*   RFY                                                            0145 00212000
*    (R1,                                                          0145 00213000
*     R3,                                                          0145 00214000
*     R15) RSTD;                                                   0145 00215000
*   IF R15^=GOOD THEN               /* RET. CODE NOT GOOD    @ZM43074*/ 00216000
         LTR   R15,R15                                             0146 00217000
         BZ    @RF00146                                            0146 00218000
*     DO;                                                          0147 00219000
*       R1=ASMVMSG;                 /* GET ADDRESS OF ERROR MSG      */ 00220000
         L     R1,ASMVMSG                                          0148 00221000
*       CALL ERRMSG;                /* GO TO MESSAGE SEGMENT         */ 00222000
         BAL   @14,ERRMSG                                          0149 00223000
*     END;                                                         0150 00224000
*   ELSE                                                           0151 00225000
*     ;                             /* RET CODE GOOD FROM FMTASMVT   */ 00226000
@RF00146 DS    0H                                                  0152 00227000
*   R1=ABDPLPTR;                    /* SET UP PARAMETERS FOR CALL    */ 00228000
         L     R1,ABDPLPTR                                         0152 00229000
*   R3=ASMVTPTR;                                                   0153 00230000
         L     R3,ASMVTPTR                                         0153 00231000
*   CALL ILRFMTPG;                  /* CALL SEGMENT TO FORMAT PAGING    00232000
*                                      RELATED CONTROL BLOCKS        */ 00233000
         L     @15,@CV01270                                        0154 00234000
         BALR  @14,@15                                             0154 00235000
*   IF R15^=GOOD THEN               /* RETURN CODE NOT ZERO          */ 00236000
         LTR   R15,R15                                             0155 00237000
         BZ    @RF00155                                            0155 00238000
*     DO;                                                          0156 00239000
*       IF R15=ERROR THEN           /* RETURN CODE IS FOUR           */ 00240000
         CH    R15,@CH00168                                        0157 00241000
         BNE   @RF00157                                            0157 00242000
*         DO;                                                      0158 00243000
*           R1=PAGEMSG;             /* GET MESSAGE CODE              */ 00244000
         L     R1,PAGEMSG                                          0159 00245000
*           CALL ERRMSG;            /* GO TO MESSAGE SEGMENT         */ 00246000
         BAL   @14,ERRMSG                                          0160 00247000
*         END;                                                     0161 00248000
*       ELSE                        /* RETURN CODE IS EIGHT          */ 00249000
*         DO;                                                      0162 00250000
         B     @RC00157                                            0162 00251000
@RF00157 DS    0H                                                  0163 00252000
*           R1=PARTMSG;             /* GET MESSAGE CODE              */ 00253000
         L     R1,PARTMSG                                          0163 00254000
*           CALL ERRMSG;            /* GO TO MESSAGE SEGMENT         */ 00255000
         BAL   @14,ERRMSG                                          0164 00256000
*         END;                                                     0165 00257000
*     END;                                                         0166 00258000
*   ELSE                                                           0167 00259000
*     ;                             /* RET CODE WAS GOOD-CONTINUE    */ 00260000
@RF00155 DS    0H                                                  0168 00261000
*   R1=ABDPLPTR;                    /* SET UP PARAMETERS FOR CALL    */ 00262000
@RC00155 L     R1,ABDPLPTR                                         0168 00263000
*   R3=ASMVTPTR;                                                   0169 00264000
         L     R3,ASMVTPTR                                         0169 00265000
*   CALL ILRFMTSW;                  /* CALL SEGMENT TO FORMAT      0170 00266000
*                                      SWAPPING RELATED BLOCKS       */ 00267000
         L     @15,@CV01271                                        0170 00268000
         BALR  @14,@15                                             0170 00269000
*   IF R15^=GOOD THEN               /* RETURN CODE NOT ZERO          */ 00270000
         LTR   R15,R15                                             0171 00271000
         BZ    @RF00171                                            0171 00272000
*     DO;                                                          0172 00273000
*       IF R15=ERROR THEN           /* RETURN CODE IS FOUR           */ 00274000
         CH    R15,@CH00168                                        0173 00275000
         BNE   @RF00173                                            0173 00276000
*         DO;                                                      0174 00277000
*           R1=SWAPMSG;             /* GET MESSAGE CODE              */ 00278000
         L     R1,SWAPMSG                                          0175 00279000
*           CALL ERRMSG;            /* GO TO MESSAGE SEGMENT         */ 00280000
         BAL   @14,ERRMSG                                          0176 00281000
*         END;                                                     0177 00282000
*       ELSE                        /* RETURN CODE WAS EIGHT         */ 00283000
*         DO;                                                      0178 00284000
         B     @RC00173                                            0178 00285000
@RF00173 DS    0H                                                  0179 00286000
*           R1=SARTMSG;             /* GET MESSAGE CODE              */ 00287000
         L     R1,SARTMSG                                          0179 00288000
*           CALL ERRMSG;            /* GO TO MESSAGE SEGMENT         */ 00289000
         BAL   @14,ERRMSG                                          0180 00290000
*         END;                                                     0181 00291000
*     END;                                                         0182 00292000
*   ELSE                                                           0183 00293000
*     ;                             /* RETURN CODE GOOD FROM SWAP    */ 00294000
@RF00171 DS    0H                                                  0184 00295000
*   R1=ABDPLPTR;                    /* SET UP PARAMETERS FOR CALL    */ 00296000
@RC00171 L     R1,ABDPLPTR                                         0184 00297000
*   CALL ILRFMTC;                   /* CALL SEGMENT TO FORMAT COMMON    00298000
*                                      SERVICE AREA PGT/XPT'S        */ 00299000
         L     @15,@CV01272                                        0185 00300000
         BALR  @14,@15                                             0185 00301000
*   IF R15^=GOOD THEN               /* RETURN CODE NOT ZERO          */ 00302000
         LTR   R15,R15                                             0186 00303000
         BZ    @RF00186                                            0186 00304000
*     DO;                                                          0187 00305000
*       R1=COMMSG;                  /* GET ADDRESS OF ERROR MSG      */ 00306000
         L     R1,COMMSG                                           0188 00307000
*       CALL ERRMSG;                /* GO TO MESSAGE SEGMENT         */ 00308000
         BAL   @14,ERRMSG                                          0189 00309000
*     END;                                                         0190 00310000
*   ELSE                                                           0191 00311000
*     ;                             /* RETURN CODE GOOD FROM COMMON  */ 00312000
@RF00186 DS    0H                                                  0192 00313000
*   RFY                                                            0192 00314000
*    (R1,                                                          0192 00315000
*     R3,                                                          0192 00316000
*     R15) UNRSTD;                                                 0192 00317000
*                                                                  0193 00318000
*/********************************************************************/ 00319000
*/*                                                                  */ 00320000
*/* THIS SECTION CAUSES FORMATTING OF THE HEADERS AND OTHER BLOCKS   */ 00321000
*/*  FOR EACH ADDRESS SPACE                                          */ 00322000
*/*  1. GET THE MAXIMUM NUMBER OF ADDRESS SPACES FROM THE ASVT       */ 00323000
*/*  2. THEN FOR EACH ENTRY, DO -                                    */ 00324000
*/*     A. ACCESS ENTRY. IF ACCESS FAILS, PUT OUT MESSAGE AND LEAVE  */ 00325000
*/*         LOOP                                                     */ 00326000
*/*     B. CHECK VALID FLAG. IF ENTRY NOT VALID, GO ON TO NEXT ENTRY */ 00327000
*/*     C. GET ASCB ADDRESS                                          */ 00328000
*/*     D. PUT ASID INTO ABDUMP PARAMETER LIST                       */ 00329000
*/*     E. GET RSMHD ADDRESS. IF UNAVAILABLE, GO ON TO NEXT ENTRY    */ 00330000
*/*     F. CALL ILRFMTH TO FORMAT RSMHD, SPCT, ASMHD, AIA'S AND      */ 00331000
*/*         PRIVATE AREA PGT/XPT'S.                                  */ 00332000
*/*                                                                  */ 00333000
*/********************************************************************/ 00334000
*                                                                  0193 00335000
*   SAVEASID=ADPLASID;              /* SAVE ENTRY ASID       @ZM43066*/ 00336000
         L     @11,ABDPLPTR                                        0193 00337000
         MVC   SAVEASID(2),ADPLASID(@11)                           0193 00338000
*   DO;                             /* ILRMACCS (ASVTPTR)          0194 00339000
*                                      FROM(CVTASVT)                 */ 00340000
*     RFY                                                          0195 00341000
*      (R0,                                                        0195 00342000
*       R1,                                                        0195 00343000
*       R14,                                                       0195 00344000
*       R15) RSTD;                                                 0195 00345000
*     R0=ADDR(CVTASVT);                                            0196 00346000
         L     @08,CVTPTR                                          0196 00347000
         LA    R0,CVTASVT(,@08)                                    0196 00348000
*     R1=ABDPLPTR;                  /* POINT TO PARM LIST            */ 00349000
         LR    R1,@11                                              0197 00350000
*     CALL ACCESS;                  /* ACCESS DATA                   */ 00351000
         L     @15,ADPLMEMA(,@11)                                  0198 00352000
         BALR  @14,@15                                             0198 00353000
*     R14=R0;                       /* CANNOT USE R0 AS BASE         */ 00354000
         LR    R14,R0                                              0199 00355000
*     IF R15=GOOD THEN              /* CHECK ACCESS                  */ 00356000
         LTR   R15,R15                                             0200 00357000
         BNZ   @RF00200                                            0200 00358000
*       ASVTPTR=CURRENT;            /* GET REQUESTED FIELD           */ 00359000
         MVC   ASVTPTR(4),CURRENT(R14)                             0201 00360000
*     ELSE                          /* ACCESS NO GOOD                */ 00361000
*       ASVTPTR=ZERO;               /* ZERO FIELD                    */ 00362000
         B     @RC00200                                            0202 00363000
@RF00200 SLR   @11,@11                                             0202 00364000
         ST    @11,ASVTPTR                                         0202 00365000
*     RFY                                                          0203 00366000
*      (R0,                                                        0203 00367000
*       R1,                                                        0203 00368000
*       R14,                                                       0203 00369000
*       R15) UNRSTD;                                               0203 00370000
@RC00200 DS    0H                                                  0204 00371000
*   END;                            /* GET ASVT ADDRESS              */ 00372000
*   IF ASVTPTR=NULLPTR THEN         /* ACCESS FAILED                 */ 00373000
         ICM   @11,15,ASVTPTR                                      0205 00374000
         BNZ   @RF00205                                            0205 00375000
*     DO;                                                          0206 00376000
*       RFY                                                        0207 00377000
*         R1 RSTD;                                                 0207 00378000
*       R1=ASVTMSG;                 /* GET MESSAGE CODE              */ 00379000
         L     R1,ASVTMSG                                          0208 00380000
*       CALL ERRMSG;                /* GO TO MESSAGE SEGMENT         */ 00381000
         BAL   @14,ERRMSG                                          0209 00382000
*       GOTO FMTVIO;                /* BRANCH OUT OF LOOP            */ 00383000
         B     FMTVIO                                              0210 00384000
*       RFY                                                        0211 00385000
*         R1 UNRSTD;                                               0211 00386000
*     END;                                                         0212 00387000
*   ELSE                            /* GOT ASVT ADDRESS              */ 00388000
*     DO;                                                          0213 00389000
@RF00205 DS    0H                                                  0214 00390000
*       DO;                         /* ILRMACCS (MAXUSER)          0214 00391000
*                                      FROM(ASVTMAXU)                */ 00392000
*         RFY                                                      0215 00393000
*          (R0,                                                    0215 00394000
*           R1,                                                    0215 00395000
*           R14,                                                   0215 00396000
*           R15) RSTD;                                             0215 00397000
*         R0=ADDR(ASVTMAXU);                                       0216 00398000
         L     @11,ASVTPTR                                         0216 00399000
         LA    R0,ASVTMAXU(,@11)                                   0216 00400000
*         R1=ABDPLPTR;              /* POINT TO PARM LIST            */ 00401000
         L     @11,ABDPLPTR                                        0217 00402000
         LR    R1,@11                                              0217 00403000
*         CALL ACCESS;              /* ACCESS DATA                   */ 00404000
         L     @15,ADPLMEMA(,@11)                                  0218 00405000
         BALR  @14,@15                                             0218 00406000
*         R14=R0;                   /* CANNOT USE R0 AS BASE         */ 00407000
         LR    R14,R0                                              0219 00408000
*         IF R15=GOOD THEN          /* CHECK ACCESS                  */ 00409000
         LTR   R15,R15                                             0220 00410000
         BNZ   @RF00220                                            0220 00411000
*           MAXUSER=CURRENT;        /* GET REQUESTED FIELD           */ 00412000
         L     MAXUSER,CURRENT(,R14)                               0221 00413000
*         ELSE                      /* ACCESS NO GOOD                */ 00414000
*           MAXUSER=ZERO;           /* ZERO FIELD                    */ 00415000
         B     @RC00220                                            0222 00416000
@RF00220 SLR   MAXUSER,MAXUSER                                     0222 00417000
*         RFY                                                      0223 00418000
*          (R0,                                                    0223 00419000
*           R1,                                                    0223 00420000
*           R14,                                                   0223 00421000
*           R15) UNRSTD;                                           0223 00422000
@RC00220 DS    0H                                                  0224 00423000
*       END;                        /* GET MAX NUMBER OF MEMORIES    */ 00424000
*       IF MAXUSER=NULLPTR THEN     /* ACCESS FAILED FOR MAXUSER     */ 00425000
         LTR   MAXUSER,MAXUSER                                     0225 00426000
         BNZ   @RF00225                                            0225 00427000
*         DO;                                                      0226 00428000
*           RFY                                                    0227 00429000
*             R1 RSTD;                                             0227 00430000
*           R1=ASVTMSG;             /* GET MESSAGE CODE              */ 00431000
         L     R1,ASVTMSG                                          0228 00432000
*           CALL ERRMSG;            /* GO TO MESSAGE SEGMENT         */ 00433000
         BAL   @14,ERRMSG                                          0229 00434000
*           GOTO FMTVIO;            /* BRANCH OUT OF LOOP            */ 00435000
         B     FMTVIO                                              0230 00436000
*           RFY                                                    0231 00437000
*             R1 UNRSTD;                                           0231 00438000
*         END;                                                     0232 00439000
*       ELSE                        /* GOT NUMBER MEMORIES           */ 00440000
*         DO I=1 TO MAXUSER;        /* UNTIL ALL ADDR SPCS PROCESSD  */ 00441000
@RF00225 LA    I,1                                                 0233 00442000
         B     @DE00233                                            0233 00443000
@DL00233 DS    0H                                                  0234 00444000
*           RFY                                                    0234 00445000
*            (R0,                                                  0234 00446000
*             R1,                                                  0234 00447000
*             R15) RSTD;                                           0234 00448000
*           ENTRYPTR=ADDR(ASVTENTY(I));/* ADDRESS OF ENTRY           */ 00449000
         LR    @14,I                                               0235 00450000
         SLA   @14,2                                               0235 00451000
         L     ENTRYPTR,ASVTPTR                                    0235 00452000
         LA    ENTRYPTR,ASVTENTY-4(@14,ENTRYPTR)                   0235 00453000
*           R0=ENTRYPTR;            /* SET UP PARMS FOR ACCESS       */ 00454000
