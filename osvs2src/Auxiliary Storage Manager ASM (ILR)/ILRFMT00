*     R1=ABDPLPTR;                  /* POINT TO PARM LIST            */ 03584000
         L     @11,ABDPLPTR                                        0556 03585000
         LR    R1,@11                                              0556 03586000
*     CALL ACCESS;                  /* ACCESS DATA                   */ 03587000
         L     @15,ADPLMEMA(,@11)                                  0557 03588000
         BALR  @14,@15                                             0557 03589000
*     R14=R0;                       /* CANNOT USE R0 AS BASE         */ 03590000
         LR    R14,R0                                              0558 03591000
