         CLC   OPBYTE(1,OPPTR),ACEOP(@14)                          1020 05728000
         BNE   @RF01020                                            1020 05729000
*             DO;                   /* ILRMFRMT (ACE)              1021 05730000
*                                      LINES(ACE1,ACE2,ACE3)SPACE    */ 05731000
*               FORMRC=0;           /* ZERO THE RETURN CODE          */ 05732000
         SLR   FORMRC,FORMRC                                       1022 05733000
*               ACE1DT1=ADDR(ACEFQPA);                             1023 05734000
