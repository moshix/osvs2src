         L     @14,ABDPLPTR                                        0567 03616000
         L     @14,ADPLBUF(,@14)                                   0567 03617000
         MVC   ANCHNAME(8,@14),ANCHACE2                            0567 03618000
*       ANCHTXT=ANCH;                                              0568 03619000
         MVC   ANCHTXT(11,@14),ANCH                                0568 03620000
*       SINGLE=OFF;                 /* NOT A SINGLE BLK REQUEST      */ 03621000
         NI    SINGLE,B'01111111'                                  0569 03622000
*       CALL FMTACE;                /* CALL SEGMENT THAT FMTS ACES   */ 03623000
