*   R0=ADDR(ERE2PTS);               /* ADDR OF FORMAT PATTERNS       */ 03905000
         LA    R0,ERE2PTS                                          0618 03906000
*   R1=ABDPLPTR;                    /* ADDR OF PARM LIST             */ 03907000
