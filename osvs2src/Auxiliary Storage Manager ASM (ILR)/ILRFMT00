*           R1=ABDPLPTR;                                           0237 00456000
         L     @14,ABDPLPTR                                        0237 00457000
         LR    R1,@14                                              0237 00458000
*           CALL ACCESS;            /* GET THE ENTRY                 */ 00459000
         L     @15,ADPLMEMA(,@14)                                  0238 00460000
         BALR  @14,@15                                             0238 00461000
*           IF R15=GOOD THEN        /* ACCESS WAS OKAY               */ 00462000
         LTR   R15,R15                                             0239 00463000
         BNZ   @RF00239                                            0239 00464000
*             DO;                                                  0240 00465000
*               ENTRYPTR=R0;        /* SAVE ACCESSED ADDRESS         */ 00466000
         LR    ENTRYPTR,R0                                         0241 00467000
*               IF VALIDFLG=OFF THEN/* ADDRESS SPACE IS IN USE       */ 00468000
         TM    VALIDFLG(ENTRYPTR),B'10000000'                      0242 00469000
         BNZ   @RF00242                                            0242 00470000
*                 DO;                                              0243 00471000
*                   ADPLASID=I;     /* PUT ASID INTO PARM LIST       */ 00472000
