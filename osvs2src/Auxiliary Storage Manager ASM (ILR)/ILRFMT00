*           R1=LGVTMSG;             /* GET MESSAGE CODE              */ 00621000
         L     R1,LGVTMSG                                          0302 00622000
*           CALL ERRMSG;            /* GO TO MESSAGE SEGMENT         */ 00623000
         BAL   @14,ERRMSG                                          0303 00624000
*         END;                                                     0304 00625000
*     END;                                                         0305 00626000
*   ELSE                                                           0306 00627000
*     ;                             /* RETURN CODE GOOD FROM VIO     */ 00628000
@RF00294 DS    0H                                                  0307 00629000
*   RETURN CODE(0);                 /* RETURN TO CALLER              */ 00630000
@RC00294 SLR   @15,@15                                             0307 00631000
         L     @13,4(,@13)                                         0307 00632000
         L     @14,12(,@13)                                        0307 00633000
