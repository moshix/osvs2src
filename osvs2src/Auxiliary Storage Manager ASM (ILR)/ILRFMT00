*                   ACEPTR=CURRENT; /* GET REQUESTED FIELD           */ 05898000
         MVC   ACEPTR(4),CURRENT(R14)                              1082 05899000
*                 ELSE              /* ACCESS NO GOOD                */ 05900000
*                   ACEPTR=ZERO;    /* ZERO FIELD                    */ 05901000
         B     @RC01081                                            1083 05902000
