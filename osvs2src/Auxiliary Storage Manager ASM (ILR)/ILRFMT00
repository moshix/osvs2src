*   DO WHILE ACEPTR^=NULLPTR&       /* UNTIL ACCESS FAILS -OR- UNTIL    05680000
*                                      NEXT ACE = 0 -OR-             */ 05681000
*         MAXCNT<MAXACES;           /* UNTIL MAXIMUM EXCEEDED        */ 05682000
