*       R1) UNRSTD;                                                1009 05698000
*     CALL PRTHED;                  /* PRINT ACE HEADER              */ 05699000
         BAL   @14,PRTHED                                          1010 05700000
*     RFY                                                          1011 05701000
*      (R0,                                                        1011 05702000
*       R1,                                                        1011 05703000
*       R15) RSTD;                                                 1011 05704000
*     R0=ADDR(ACEOP);               /* ADDR OF FIELD TO ACCESS       */ 05705000
         L     @14,ACEPTR                                          1012 05706000
         LA    R0,ACEOP(,@14)                                      1012 05707000
*     R1=ABDPLPTR;                  /* PARM LIST PTR                 */ 05708000
         L     @14,ABDPLPTR                                        1013 05709000
         LR    R1,@14                                              1013 05710000
*     CALL ACCESS;                  /* ACCESS DATA                   */ 05711000
         L     @15,ADPLMEMA(,@14)                                  1014 05712000
         BALR  @14,@15                                             1014 05713000
*     OPPTR=R0;                     /* PUT DATA INTO ANOTHER AREA    */ 05714000
         LR    OPPTR,R0                                            1015 05715000
*     RFY                                                          1016 05716000
*      (R0,                                                        1016 05717000
*       R1) UNRSTD;                                                1016 05718000
*     IF R15^=ERROR THEN            /* ACCESS WAS GOOD               */ 05719000
         CH    R15,@CH00168                                        1017 05720000
