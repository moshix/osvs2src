         ST    @14,RCOD3                                           0680 04265000
*   RFY                                                            0681 04266000
*     R15 UNRSTD;                                                  0681 04267000
*                                                                  0682 04268000
*/********************************************************************/ 04269000
*/*  FORMAT ALL OF THE REGULAR MESSAGE BUFFERS, PLUS THE TERMINATION */ 04270000
*/*   BUFFER.                                                        */ 04271000
*/********************************************************************/ 04272000
*                                                                  0682 04273000
*   RFY                                                            0682 04274000
*    (R0,                                                          0682 04275000
*     R1,                                                          0682 04276000
*     R15) RSTD;                                                   0682 04277000
*   MSGBFPTR=ADDR(MSGBUFFR);        /* PTR TO ACTUAL BUFFER          */ 04278000
         L     @14,MSGBFPTR                                        0683 04279000
         LA    @14,MSGBUFFR(,@14)                                  0683 04280000
         ST    @14,MSGBFPTR                                        0683 04281000
*   DO I=1 TO NUMBUFS;              /* PRINT ALL BUFFERS             */ 04282000
         LA    I,1                                                 0684 04283000
         B     @DE00684                                            0684 04284000
@DL00684 DS    0H                                                  0685 04285000
*     MSG2DT1=ADDR(MSGBFPTR);       /* SET DATA PTRS                 */ 04286000
         LA    @14,MSGBFPTR                                        0685 04287000
         ST    @14,MSG2DT1                                         0685 04288000
*     MSG2DT2=MSGBFPTR;                                            0686 04289000
         MVC   MSG2DT2(4),MSGBFPTR                                 0686 04290000
*     R0=ADDR(MSG2PTS);             /* FORMAT PATTERN ADDRESS        */ 04291000
         LA    R0,MSG2PTS                                          0687 04292000
*     R1=ABDPLPTR;                  /* PARM LIST PTR                 */ 04293000
         L     @14,ABDPLPTR                                        0688 04294000
         LR    R1,@14                                              0688 04295000
*     CALL FORMAT;                  /* FORMAT LINE                   */ 04296000
         L     @15,ADPLFRMT(,@14)                                  0689 04297000
         BALR  @14,@15                                             0689 04298000
*     RCOD3=RCOD3|R15;              /* OR IN RETURN CODE             */ 04299000
         L     @14,RCOD3                                           0690 04300000
         OR    @14,R15                                             0690 04301000
         ST    @14,RCOD3                                           0690 04302000
*     CALL PRINT;                   /* PRINT LINE                    */ 04303000
         L     @11,ABDPLPTR                                        0691 04304000
         L     @15,ADPLPRNT(,@11)                                  0691 04305000
         BALR  @14,@15                                             0691 04306000
*     R0=ADDR(MSG3PTS);             /* FORMAT PATTERN ADDRESS        */ 04307000
         LA    R0,MSG3PTS                                          0692 04308000
*     CALL FORMAT;                  /* FORMAT LINE                   */ 04309000
         L     @11,ABDPLPTR                                        0693 04310000
         L     @15,ADPLFRMT(,@11)                                  0693 04311000
         BALR  @14,@15                                             0693 04312000
*     RCOD3=RCOD3|R15;              /* OR IN RETURN CODE             */ 04313000
         L     @14,RCOD3                                           0694 04314000
         OR    @14,R15                                             0694 04315000
         ST    @14,RCOD3                                           0694 04316000
*     CALL PRINT;                   /* PRINT LINE                    */ 04317000
         L     @11,ABDPLPTR                                        0695 04318000
         L     @15,ADPLPRNT(,@11)                                  0695 04319000
         BALR  @14,@15                                             0695 04320000
*     MSGBFPTR=MSGBFPTR+LENGTH(MSGBUFFR)/NUMBUFS;/* POINT TO NEXT  0696 04321000
*                                      BUFFER                        */ 04322000
         LA    @05,240                                             0696 04323000
         SLR   @04,@04                                             0696 04324000
         D     @04,NUMBUFS                                         0696 04325000
         AL    @05,MSGBFPTR                                        0696 04326000
         ST    @05,MSGBFPTR                                        0696 04327000
*   END;                                                           0697 04328000
         AH    I,@CH00042                                          0697 04329000
@DE00684 C     I,NUMBUFS                                           0697 04330000
         BNH   @DL00684                                            0697 04331000
*   BUFFER(1:1+LENGTH(TERMBUF))=TERMBUF;/* SET UP SUB-HEADER         */ 04332000
         L     @14,ABDPLPTR                                        0698 04333000
         L     @11,ADPLBUF(,@14)                                   0698 04334000
         MVI   BUFFER+18(@11),C' '                                 0698 04335000
         MVC   BUFFER(18,@11),TERMBUF                              0698 04336000
*   CALL PRINT;                     /* PRINT SUB-HEADER              */ 04337000
         L     @15,ADPLPRNT(,@14)                                  0699 04338000
         BALR  @14,@15                                             0699 04339000
*   MSG2DT1=ADDR(MSGBFPTR);         /* SET DATA PTRS                 */ 04340000
         LA    @14,MSGBFPTR                                        0700 04341000
         ST    @14,MSG2DT1                                         0700 04342000
*   MSG2DT2=MSGBFPTR;                                              0701 04343000
         MVC   MSG2DT2(4),MSGBFPTR                                 0701 04344000
*   R0=ADDR(MSG2PTS);               /* FORMAT PATTERN ADDRESS        */ 04345000
         LA    R0,MSG2PTS                                          0702 04346000
*   R1=ABDPLPTR;                    /* PARM LIST PTR                 */ 04347000
         L     @14,ABDPLPTR                                        0703 04348000
         LR    R1,@14                                              0703 04349000
*   CALL FORMAT;                    /* FORMAT ONE LINE               */ 04350000
         L     @15,ADPLFRMT(,@14)                                  0704 04351000
         BALR  @14,@15                                             0704 04352000
*   RCOD3=RCOD3|R15;                /* OR IN RETURN CODE             */ 04353000
         L     @14,RCOD3                                           0705 04354000
         OR    @14,R15                                             0705 04355000
         ST    @14,RCOD3                                           0705 04356000
*   CALL PRINT;                     /* PRINT ONE LINE                */ 04357000
         L     @11,ABDPLPTR                                        0706 04358000
         L     @15,ADPLPRNT(,@11)                                  0706 04359000
         BALR  @14,@15                                             0706 04360000
*   R0=ADDR(MSG3PTS);               /* NEXT FMT PATTERN ADDR         */ 04361000
         LA    R0,MSG3PTS                                          0707 04362000
*   CALL FORMAT;                    /* FORMAT ONE LINE               */ 04363000
         L     @11,ABDPLPTR                                        0708 04364000
         L     @15,ADPLFRMT(,@11)                                  0708 04365000
         BALR  @14,@15                                             0708 04366000
*   RCOD3=RCOD3|R15;                /* OR IN RETURN CODE             */ 04367000
         L     @14,RCOD3                                           0709 04368000
         OR    @14,R15                                             0709 04369000
         ST    @14,RCOD3                                           0709 04370000
*   CALL PRINT;                     /* PRINT ONE LINE                */ 04371000
         L     @11,ABDPLPTR                                        0710 04372000
         L     @15,ADPLPRNT(,@11)                                  0710 04373000
         BALR  @14,@15                                             0710 04374000
*   R0=ADDR(MSG3PTS);               /* FORMAT PATTERN ADDRESS        */ 04375000
         LA    R0,MSG3PTS                                          0711 04376000
*   CALL FORMAT;                    /* FORMAT LAST LINE              */ 04377000
         L     @11,ABDPLPTR                                        0712 04378000
         L     @15,ADPLFRMT(,@11)                                  0712 04379000
         BALR  @14,@15                                             0712 04380000
*   RCOD3=RCOD3|R15;                /* OR IN RETURN CODE             */ 04381000
         L     @14,RCOD3                                           0713 04382000
         OR    @14,R15                                             0713 04383000
         ST    @14,RCOD3                                           0713 04384000
*   CALL PRINT;                     /* PRINT LAST LINE               */ 04385000
         L     @11,ABDPLPTR                                        0714 04386000
         L     @15,ADPLPRNT(,@11)                                  0714 04387000
         BALR  @14,@15                                             0714 04388000
*   CALL PRINT;                     /* SPACE ONE LINE                */ 04389000
         L     @11,ABDPLPTR                                        0715 04390000
         L     @15,ADPLPRNT(,@11)                                  0715 04391000
         BALR  @14,@15                                             0715 04392000
*   R15=RCOD3;                      /* SET RETURN CODE FOR CALLER    */ 04393000
         L     R15,RCOD3                                           0716 04394000
*   RFY                                                            0717 04395000
*    (R0,                                                          0717 04396000
*     R1,                                                          0717 04397000
*     R15) UNRSTD;                                                 0717 04398000
*   RFY                                                            0718 04399000
*     R14 RSTD;                                                    0718 04400000
*   R14=RTNADDR3;                   /* RESTORE RETURN ADDRESS        */ 04401000
         LR    R14,RTNADDR3                                        0719 04402000
*   RFY                                                            0720 04403000
*    (R14,                                                         0720 04404000
*     R15) UNRSTD;                                                 0720 04405000
*   END FMTMSGBF;                   /* RETURN TO CALLER              */ 04406000
@EL00006 DS    0H                                                  0721 04407000
@EF00006 DS    0H                                                  0721 04408000
@ER00006 BR    @14                                                 0721 04409000
*                                                                  0722 04410000
*/********************************************************************/ 04411000
*/*                                                                  */ 04412000
*/* FMTAIA:   SEGMENT TO FORMAT A QUEUE OF AIA'S OR AIA/ACE'S        */ 04413000
*/*                                                                  */ 04414000
*/* INPUT:    VIRTUAL ADDRESS OF AIA OR ACE IN AIAPTR                */ 04415000
*/*                                                                  */ 04416000
*/* PROCESS:  IF AIAPTR POINTS TO AN ACE, CALL FMTACE.  IF AIAPTR    */ 04417000
*/*            POINTS TO AIA, FORMAT PCB AND AIA AND CONTINUE        */ 04418000
*/*            ALONG THE CHAIN UNTIL NEXT AIA IS 0 OR MAX IS EXCEEDED*/ 04419000
*/*                                                                  */ 04420000
*/* OUTPUT:   RETURN CODE = 0 IF ACCESSES AND FORMATS WERE           */ 04421000
*/*            SUCCESSFUL AND RETURN CODE FROM FMTACE WAS 0.         */ 04422000
