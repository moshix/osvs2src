*   RCOD5=GOOD;                     /* INITIALIZE VARIABLES          */ 05675000
         SLR   @14,@14                                             1002 05676000
         ST    @14,RCOD5                                           1002 05677000
*   MAXCNT=ZERO;                                                   1003 05678000
