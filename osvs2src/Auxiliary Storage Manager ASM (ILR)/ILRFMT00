         L     @15,ADPLPRNT(,@11)                                  0452 03257000
         BALR  @14,@15                                             0452 03258000
*   CALL PRINT;                                                    0453 03259000
         L     @11,ABDPLPTR                                        0453 03260000
         L     @15,ADPLPRNT(,@11)                                  0453 03261000
         BALR  @14,@15                                             0453 03262000
*   RFY                                                            0454 03263000
*    (R0,                                                          0454 03264000
*     R1,                                                          0454 03265000
*     R15) UNRSTD;                                                 0454 03266000
*                                                                  0455 03267000
*/********************************************************************/ 03268000
*/* GET THE ADDRESS OF THE BAD SLOT ERROR RECORD.  IF THE            */ 03269000
*/*  ACCESS IS GOOD AND THE POINTER NOT EQUAL ZERO, THEN CALL        */ 03270000
*/*  SEGMENT TO FORMAT THE ERROR RECORD, OTHERWISE, SKIP ALL         */ 03271000
*/*  EREC PROCESSING.                                                */ 03272000
*/********************************************************************/ 03273000
*                                                                  0455 03274000
*   DO;                             /* ILRMACCS (ERECPTR)          0455 03275000
*                                      FROM(ASMEREC)                 */ 03276000
*     RFY                                                          0456 03277000
*      (R0,                                                        0456 03278000
*       R1,                                                        0456 03279000
*       R14,                                                       0456 03280000
*       R15) RSTD;                                                 0456 03281000
*     R0=ADDR(ASMEREC);                                            0457 03282000
         L     @11,ASMVTPTR                                        0457 03283000
         LA    R0,ASMEREC(,@11)                                    0457 03284000
*     R1=ABDPLPTR;                  /* POINT TO PARM LIST            */ 03285000
         L     @11,ABDPLPTR                                        0458 03286000
         LR    R1,@11                                              0458 03287000
*     CALL ACCESS;                  /* ACCESS DATA                   */ 03288000
         L     @15,ADPLMEMA(,@11)                                  0459 03289000
         BALR  @14,@15                                             0459 03290000
*     R14=R0;                       /* CANNOT USE R0 AS BASE         */ 03291000
         LR    R14,R0                                              0460 03292000
*     IF R15=GOOD THEN              /* CHECK ACCESS                  */ 03293000
         LTR   R15,R15                                             0461 03294000
         BNZ   @RF00461                                            0461 03295000
*       ERECPTR=CURRENT;            /* GET REQUESTED FIELD           */ 03296000
         MVC   ERECPTR(4),CURRENT(R14)                             0462 03297000
*     ELSE                          /* ACCESS NO GOOD                */ 03298000
*       ERECPTR=ZERO;               /* ZERO FIELD                    */ 03299000
         B     @RC00461                                            0463 03300000
@RF00461 SLR   @11,@11                                             0463 03301000
         ST    @11,ERECPTR                                         0463 03302000
*     RFY                                                          0464 03303000
*      (R0,                                                        0464 03304000
*       R1,                                                        0464 03305000
*       R14,                                                       0464 03306000
*       R15) UNRSTD;                                               0464 03307000
@RC00461 DS    0H                                                  0465 03308000
*   END;                            /* ADDRESS OF BAD SLOT ERROR   0465 03309000
*                                      RECORD                        */ 03310000
*   IF ERECPTR^=NULLPTR THEN        /* THERE ARE ENTRIES IN THE ERROR   03311000
*                                      RECORD                        */ 03312000
         L     @11,ERECPTR                                         0466 03313000
         LTR   @11,@11                                             0466 03314000
         BZ    @RF00466                                            0466 03315000
*     DO;                                                          0467 03316000
*       RFY                                                        0468 03317000
*        (R0,                                                      0468 03318000
*         R1,                                                      0468 03319000
*         R15) RSTD;                /*                       @ZA11398*/ 03320000
*       R0=ERECPTR;                 /* INPUT TO ACCESS       @ZA11398*/ 03321000
         LR    R0,@11                                              0469 03322000
*       R1=ABDPLPTR;                /* INPUT TO ACCESS       @ZA11398*/ 03323000
         L     @14,ABDPLPTR                                        0470 03324000
         LR    R1,@14                                              0470 03325000
*       CALL ACCESS;                /* RECORD ACCESSIBLE?    @ZA11398*/ 03326000
         L     @15,ADPLMEMA(,@14)                                  0471 03327000
         BALR  @14,@15                                             0471 03328000
*       IF R15=GOOD THEN                                           0472 03329000
         LTR   R15,R15                                             0472 03330000
         BNZ   @RF00472                                            0472 03331000
*         CALL FMTEREC;             /* CALL SEGMENT TO FORMAT THE  0473 03332000
*                                      ERROR RECORD          @ZA11398*/ 03333000
         BAL   @14,FMTEREC                                         0473 03334000
*       ELSE                                                       0474 03335000
*         ;                         /*                       @ZA11398*/ 03336000
@RF00472 DS    0H                                                  0475 03337000
*       RFY                                                        0475 03338000
*        (R0,                                                      0475 03339000
*         R1) UNRSTD;               /*                       @ZA11398*/ 03340000
*     END;                                                         0476 03341000
*   ELSE                                                           0477 03342000
*     ;                             /* PTR WAS BAD                   */ 03343000
@RF00466 DS    0H                                                  0478 03344000
*   RCOD1=RCOD1|R15;                /* OR IN RETURN CODE             */ 03345000
         L     @14,RCOD1                                           0478 03346000
         OR    @14,R15                                             0478 03347000
         ST    @14,RCOD1                                           0478 03348000
*   RFY                                                            0479 03349000
*     R15 UNRSTD;                                                  0479 03350000
*                                                                  0480 03351000
*/********************************************************************/ 03352000
*/* GET THE ADDRESS OF THE MESSAGE BUFFER.  IF THE ACCESS            */ 03353000
*/*  IS GOOD, CALL SEGMENT TO FORMAT THE MESSAGE BUFFER,             */ 03354000
*/*  OTHERWISE, SKIP MSGBF PROCESSING.                               */ 03355000
*/********************************************************************/ 03356000
*                                                                  0480 03357000
*   DO;                             /* ILRMACCS (MSGBF) VIA(ASMMSGBF)*/ 03358000
*     RFY                                                          0481 03359000
*      (R0,                                                        0481 03360000
*       R1,                                                        0481 03361000
*       R14,                                                       0481 03362000
*       R15) RSTD;                                                 0481 03363000
*     R1=ABDPLPTR;                                                 0482 03364000
