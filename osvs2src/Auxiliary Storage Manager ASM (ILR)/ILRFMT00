*     CALL ACCESS;                  /* ACCESS PTR TO MSGBF           */ 03370000
         L     @15,ADPLMEMA(,@11)                                  0484 03371000
         BALR  @14,@15                                             0484 03372000
*     IF R15=GOOD THEN                                             0485 03373000
         LTR   R15,R15                                             0485 03374000
         BNZ   @RF00485                                            0485 03375000
*       DO;                         /* FOUND PTR - NOW FIND BLOCK    */ 03376000
*         R14=R0;                   /* CANNOT USE R0 AS BASE REG     */ 03377000
         LR    R14,R0                                              0487 03378000
*         R0=CURRENT;               /* POINTER ITSELF                */ 03379000
