*   DO;                             /* ILRMFRMT (EREC) LINES(ERE1)   */ 03841000
*     FORMRC=0;                     /* ZERO THE RETURN CODE          */ 03842000
         SLR   FORMRC,FORMRC                                       0597 03843000
*     ERE1DT1=ADDR(ERECURNT);                                      0598 03844000
