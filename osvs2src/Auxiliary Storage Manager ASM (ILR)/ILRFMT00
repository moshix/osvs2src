*                   ASCBPTR=PTRASCB;/* GET ASCB ADDRESS FROM ENTRY   */ 00475000
         SLR   ASCBPTR,ASCBPTR                                     0245 00476000
         ICM   ASCBPTR,7,PTRASCB(ENTRYPTR)                         0245 00477000
*                   DO;             /* ILRMACCS (RSMHD) VIA(ASCBRSM) */ 00478000
*                     RFY                                          0247 00479000
*                      (R0,                                        0247 00480000
*                       R1,                                        0247 00481000
