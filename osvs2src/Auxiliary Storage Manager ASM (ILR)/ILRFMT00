*                 RFY                                              1084 05905000
*                  (R0,                                            1084 05906000
*                   R1,                                            1084 05907000
*                   R14,                                           1084 05908000
*                   R15) UNRSTD;                                   1084 05909000
@RC01081 DS    0H                                                  1085 05910000
*               END;                /* GET NEXT PTR                  */ 05911000
*               RFY                                                1086 05912000
*                 R15 RSTD;                                        1086 05913000
*               RCOD5=RCOD5|R15;    /* OR IN RETURN CODE             */ 05914000
         L     @14,RCOD5                                           1087 05915000
         OR    @14,R15                                             1087 05916000
         ST    @14,RCOD5                                           1087 05917000
*             END;                                                 1088 05918000
*           ELSE                    /* SINGLE BLOCK REQUEST          */ 05919000
*             DO;                                                  1089 05920000
         B     @RC01073                                            1089 05921000
@RF01073 DS    0H                                                  1090 05922000
*               SINGLE=OFF;         /* RESET SWITCH                  */ 05923000
         NI    SINGLE,B'01111111'                                  1090 05924000
*               ACEPTR=NULLPTR;     /* RESET PTR TO LEAVE LOOP FOR 1091 05925000
*                                      SINGLE BLOCK REQUEST          */ 05926000
         SLR   @14,@14                                             1091 05927000
         ST    @14,ACEPTR                                          1091 05928000
*             END;                                                 1092 05929000
*         END;                      /* END OF ACE Q PROCESSING       */ 05930000
@RC01073 DS    0H                                                  1094 05931000
*       END;                                                       1094 05932000
*     ELSE                                                         1095 05933000
*       RCOD5=ERROR;                /* ACCESS FAILED                 */ 05934000
         B     @RC01017                                            1095 05935000
@RF01017 MVC   RCOD5(4),@CF00168                                   1095 05936000
*     RFY                                                          1096 05937000
*       R15 UNRSTD;                                                1096 05938000
@RC01017 DS    0H                                                  1097 05939000
*   END;                            /* END ALL PROCESSING            */ 05940000
@DE01004 ICM   @14,15,ACEPTR                                       1097 05941000
         BZ    @DC01004                                            1097 05942000
         L     @14,MAXCNT                                          1097 05943000
         C     @14,MAXACES                                         1097 05944000
         BL    @DL01004                                            1097 05945000
