*                 CALL ACCESS;      /* ACCESS DATA                   */ 05890000
         L     @15,ADPLMEMA(,@08)                                  1079 05891000
         BALR  @14,@15                                             1079 05892000
*                 R14=R0;           /* CANNOT USE R0 AS BASE         */ 05893000
         LR    R14,R0                                              1080 05894000
*                 IF R15=GOOD THEN  /* CHECK ACCESS                  */ 05895000
         LTR   R15,R15                                             1081 05896000
