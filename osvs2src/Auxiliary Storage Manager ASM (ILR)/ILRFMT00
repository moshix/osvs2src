*     RFY                                                          0540 03541000
*      (R0,                                                        0540 03542000
*       R1,                                                        0540 03543000
*       R14,                                                       0540 03544000
*       R15) UNRSTD;                                               0540 03545000
@RC00537 DS    0H                                                  0541 03546000
*   END;                            /* GET ACE QUEUE                 */ 03547000
*   RFY                                                            0542 03548000
*     R15 RSTD;                                                    0542 03549000
*   IF ACEPTR^=NULLPTR THEN         /* THERE ARE ACES ON THIS Q      */ 03550000
         ICM   @14,15,ACEPTR                                       0543 03551000
         BZ    @RF00543                                            0543 03552000
*     DO;                                                          0544 03553000
*       ANCHNAME=ANCHACE1;          /* SET UP CORRECT ANCHOR         */ 03554000
         L     @14,ABDPLPTR                                        0545 03555000
         L     @14,ADPLBUF(,@14)                                   0545 03556000
         MVC   ANCHNAME(8,@14),ANCHACE1                            0545 03557000
*       ANCHTXT=ANCH;                                              0546 03558000
         MVC   ANCHTXT(11,@14),ANCH                                0546 03559000
*       SINGLE=OFF;                 /* NOT A SINGLE BLK REQUEST      */ 03560000
         NI    SINGLE,B'01111111'                                  0547 03561000
*       CALL FMTACE;                /* CALL SEGMENT TO FMT ACES      */ 03562000
         BAL   @14,FMTACE                                          0548 03563000
*     END;                          /* END PROCESS IF ACCESS GOOD    */ 03564000
*   ELSE                                                           0550 03565000
*     ;                             /* PTR WAS NOT GOOD              */ 03566000
@RF00543 DS    0H                                                  0551 03567000
*   RCOD1=RCOD1|R15;                /* OR IN RETURN CODE             */ 03568000
         L     @14,RCOD1                                           0551 03569000
         OR    @14,R15                                             0551 03570000
         ST    @14,RCOD1                                           0551 03571000
*   RFY                                                            0552 03572000
*     R15 UNRSTD;                                                  0552 03573000
*   DO;                             /* ILRMACCS (ACEPTR)           0553 03574000
*                                      FROM(ASMRLGWQ)                */ 03575000
*     RFY                                                          0554 03576000
*      (R0,                                                        0554 03577000
*       R1,                                                        0554 03578000
*       R14,                                                       0554 03579000
*       R15) RSTD;                                                 0554 03580000
*     R0=ADDR(ASMRLGWQ);                                           0555 03581000
         L     @11,ASMVTPTR                                        0555 03582000
