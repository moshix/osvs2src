*/*  SAVE RETURN ADDRESS, SET UP ANCHOR, CALL ILRFPRTH AND FORMAT    */ 04187000
*/*   HEADER SECTION OF THE MESSAGE BUFFER.                          */ 04188000
*/********************************************************************/ 04189000
*                                                                  0651 04190000
*   RFY                                                            0651 04191000
*     R14 RSTD;                                                    0651 04192000
*   RTNADDR3=R14;                   /* SAVE RETURN ADDRESS           */ 04193000
         LR    RTNADDR3,R14                                        0652 04194000
*   RFY                                                            0653 04195000
*     R14 UNRSTD;                                                  0653 04196000
*   ANCHNAME=ANCHMSG;               /* SET UP HEADER                 */ 04197000
         L     @11,ABDPLPTR                                        0654 04198000
         L     @11,ADPLBUF(,@11)                                   0654 04199000
         MVC   ANCHNAME(8,@11),ANCHMSG                             0654 04200000
*   ANCHTXT=ANCH;                                                  0655 04201000
         MVC   ANCHTXT(11,@11),ANCH                                0655 04202000
*   RFY                                                            0656 04203000
*    (R0,                                                          0656 04204000
*     R1,                                                          0656 04205000
*     R15) RSTD;                                                   0656 04206000
*   R0=ADDR(MSGBFPTR);              /* DATA FOR HEADER               */ 04207000
         LA    R0,MSGBFPTR                                         0657 04208000
*   R1=ADDR(MSGNAM);                /* LABEL FOR HEADER              */ 04209000
         LA    R1,MSGNAM                                           0658 04210000
*   CALL PRTHED;                    /* PRINT HEADER                  */ 04211000
         BAL   @14,PRTHED                                          0659 04212000
*   DO;                             /* ILRMFRMT (MSGBF) LINES(MSG1)  */ 04213000
*     FORMRC=0;                     /* ZERO THE RETURN CODE          */ 04214000
         SLR   FORMRC,FORMRC                                       0661 04215000
*     MSG1DT1=ADDR(MSGCURNT);                                      0662 04216000
         L     @14,MSGBFPTR                                        0662 04217000
         ST    @14,MSG1DT1                                         0662 04218000
*     MSG1DT2=ADDR(MSGFIRST);                                      0663 04219000
         LA    @11,MSGFIRST(,@14)                                  0663 04220000
         ST    @11,MSG1DT2                                         0663 04221000
*     MSG1DT3=ADDR(MSGLAST);                                       0664 04222000
         LA    @11,MSGLAST(,@14)                                   0664 04223000
         ST    @11,MSG1DT3                                         0664 04224000
*     MSG1DT4=ADDR(MSGTERM);                                       0665 04225000
         LA    @14,MSGTERM(,@14)                                   0665 04226000
         ST    @14,MSG1DT4                                         0665 04227000
*     RFY                                                          0666 04228000
*      (R0,                                                        0666 04229000
*       R1) RSTD;                                                  0666 04230000
*     R0=ADDR(MSG1PTS);                                            0667 04231000
         LA    R0,MSG1PTS                                          0667 04232000
*     R1=ABDPLPTR;                  /* GET PARM ADDR                 */ 04233000
         L     @14,ABDPLPTR                                        0668 04234000
         LR    R1,@14                                              0668 04235000
*     CALL FORMAT;                  /* FORMAT THE LINE               */ 04236000
         L     @15,ADPLFRMT(,@14)                                  0669 04237000
         BALR  @14,@15                                             0669 04238000
*     RFY                                                          0670 04239000
*       R15 RSTD;                                                  0670 04240000
*     FORMRC=FORMRC|R15;            /* SAVE RETURN CODE              */ 04241000
         OR    FORMRC,R15                                          0671 04242000
*     RFY                                                          0672 04243000
*       R15 UNRSTD;                                                0672 04244000
*     CALL PRINT;                   /* CALL PRINT ROUTINE            */ 04245000
         L     @11,ABDPLPTR                                        0673 04246000
         L     @15,ADPLPRNT(,@11)                                  0673 04247000
         BALR  @14,@15                                             0673 04248000
*     RFY                                                          0674 04249000
*      (R0,                                                        0674 04250000
*       R1) UNRSTD;                                                0674 04251000
*     RFY                                                          0675 04252000
*       R15 RSTD;                                                  0675 04253000
*     R15=FORMRC;                   /* SAVE RETURN CODE FROM FORMAT  */ 04254000
         LR    R15,FORMRC                                          0676 04255000
*     RFY                                                          0677 04256000
*       R15 UNRSTD;                                                0677 04257000
*   END;                            /* ILRMFRMT (MSGBF) LINES(MSG1)     04258000
*                                      FORMAT HEADER SECTION         */ 04259000
*   RFY                                                            0679 04260000
*     R15 RSTD;                                                    0679 04261000
*   RCOD3=RCOD3|R15;                /* OR IN RETURN CODE             */ 04262000
         L     @14,RCOD3                                           0680 04263000
