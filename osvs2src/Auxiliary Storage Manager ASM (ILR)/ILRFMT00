@ER00003 BR    @14                                                 0337 00744000
*                                                                  0338 00745000
*/********************************************************************/ 00746000
*/*                                                                  */ 00747000
*/* FMTASMVT: SEGMENT TO FORMAT THE ASMVT AND OTHER BLOCKS           */ 00748000
*/*                                                                  */ 00749000
*/* INPUT:    VIRTUAL ADDRESS OF ASMVT IN ASMVTPTR                   */ 00750000
*/*                                                                  */ 00751000
*/* PROCESS:  USE FORMAT TO PRINT THE ASMVT WITH THE POOL CONTROLLERS*/ 00752000
*/*           AND WORK SAVE AREAS.  ALSO, CALL SEGMENTS TO PRINT THE */ 00753000
*/*           BAD SLOT ERROR RECORD, MESSAGE BUFFER, AIA'S ON THE    */ 00754000
*/*           I/O STAGING QUEUE, AND TWO  QUEUES OF ACES.            */ 00755000
*/*                                                                  */ 00756000
*/* OUTPUT:   RETURN CODE = 0 IF ALL FORMATS AND ACCESSES WERE       */ 00757000
*/*           SUCCESSFUL, ELSE RETURN CODE NOT ZERO.                 */ 00758000
*/*                                                                  */ 00759000
*/********************************************************************/ 00760000
*                                                                  0338 00761000
*FMTASMVT:                                                         0338 00762000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0338 00763000
FMTASMVT DS    0H                                                  0339 00764000
*                                                                  0339 00765000
*   /*****************************************************************/ 00766000
*   /*                                                               */ 00767000
*   /* ASM1:((FLAG1,,BIT),FLAG1,,BIT),DSSFS,SART,PART,GOS)   @ZA14260*/ 00768000
*   /* LABEL VARIABLES REFERENCED IN FMT PTTRNS                      */ 00769000
*   /*                                                               */ 00770000
*   /*****************************************************************/ 00771000
*                                                                  0339 00772000
*   DCL                                                            0339 00773000
*     ASM1LB1 CHAR(LENGTH('+  0')) INIT('+  0'),                   0339 00774000
*     ASM1LB2 CHAR(LENGTH('FLAG1')) INIT('FLAG1'),                 0339 00775000
*     * CHAR(LENGTH('FLAG2')) INIT('FLAG2'),                       0339 00776000
*     * CHAR(LENGTH('DSSFS')) INIT('DSSFS'),/*               @ZA14260*/ 00777000
*     * CHAR(LENGTH('SART')) INIT('SART'),                         0339 00778000
*     * CHAR(LENGTH('PART')) INIT('PART'),                         0339 00779000
*     * CHAR(LENGTH('GOS')) INIT('GOS');                           0339 00780000
*                                                                  0339 00781000
*   /*****************************************************************/ 00782000
*   /*                                                               */ 00783000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT (HAND CODED)           */ 00784000
*   /*                                                               */ 00785000
*   /*****************************************************************/ 00786000
*                                                                  0340 00787000
*   DCL                                                            0340 00788000
*     1 ASM1PTS BDY(WORD),          /* FORMAT PATTERNS - SEE PTTRNS  */ 00789000
*      2 * CHAR(8) BDY(WORD),                                      0340 00790000
*       3 * BIT(8) BDY(BYTE) INIT('00000011'B),                    0340 00791000
*       3 * FIXED(8) INIT(16*LENGTH('+  0')-16),                   0340 00792000
*       3 * FIXED(8) INIT(0),                                      0340 00793000
*       3 * FIXED(8) INIT(0),                                      0340 00794000
*       3 * PTR(31) INIT(ADDR(ASM1LB1)),                           0340 00795000
*      2 * CHAR(12) BDY(WORD),                                     0340 00796000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),                    0340 00797000
*       3 * FIXED(8) INIT(LENGTH(ASMFLAG1)/8-1+16*LENGTH('FLAG1')-16),  00798000
*       3 * FIXED(8) INIT(5),                                      0340 00799000
*       3 * FIXED(8) INIT(11),                                     0340 00800000
*       3 * PTR(31) INIT(ADDR(ASM1LB2)),                           0340 00801000
*       3 ASM1DT2 PTR(31) INIT(ADDR(ASMFLAG1)),                    0340 00802000
*      2 * CHAR(4) BDY(WORD),                                      0340 00803000
*       3 * BIT(8) BDY(BYTE) INIT('00000101'B),                    0340 00804000
*       3 * FIXED(8) INIT(LENGTH(ASMFLAG2)/8-1+16*LENGTH('FLAG2')-16),  00805000
*       3 * FIXED(8) INIT(23),                                     0340 00806000
*       3 * FIXED(8) INIT(29),                                     0340 00807000
*      2 * CHAR(4) BDY(WORD),                                      0340 00808000
*       3 * BIT(8) BDY(BYTE) INIT('00000101'B),                    0340 00809000
*       3 * FIXED(8) INIT(LENGTH(ASMDSSFS)-1+16*LENGTH('DSSFS')-16),    00810000
*                                   /*                       @ZA14260*/ 00811000
*       3 * FIXED(8) INIT(41),                                     0340 00812000
*       3 * FIXED(8) INIT(47),                                     0340 00813000
*      2 * CHAR(4) BDY(WORD),                                      0340 00814000
*       3 * BIT(8) BDY(BYTE) INIT('00000101'B),                    0340 00815000
*       3 * FIXED(8) INIT(LENGTH(ASMSART)-1+16*LENGTH('SART')-16), 0340 00816000
*       3 * FIXED(8) INIT(59),                                     0340 00817000
*       3 * FIXED(8) INIT(65),                                     0340 00818000
*      2 * CHAR(4) BDY(WORD),                                      0340 00819000
*       3 * BIT(8) BDY(BYTE) INIT('00000101'B),                    0340 00820000
*       3 * FIXED(8) INIT(LENGTH(ASMPART)-1+16*LENGTH('PART')-16), 0340 00821000
*       3 * FIXED(8) INIT(77),                                     0340 00822000
*       3 * FIXED(8) INIT(83),                                     0340 00823000
*      2 * CHAR(4) BDY(WORD),                                      0340 00824000
*       3 * BIT(8) BDY(BYTE) INIT('00000101'B),                    0340 00825000
*       3 * FIXED(8) INIT(LENGTH(ASMGOS)-1+16*LENGTH('GOS')-16),   0340 00826000
*       3 * FIXED(8) INIT(95),                                     0340 00827000
*       3 * FIXED(8) INIT(101),                                    0340 00828000
*      2 * FIXED(31) INIT(0);                                      0340 00829000
*                                                                  0341 00830000
*   /*****************************************************************/ 00831000
*   /*                                                               */ 00832000
*   /* ASM2: (TRPAG,EREC,MSGBF,RSV5,STAGF,STAGL) LABEL VARIABLES     */ 00833000
*   /* REFERENCED IN FORMAT PTTRNS                                   */ 00834000
*   /*                                                               */ 00835000
*   /*****************************************************************/ 00836000
*                                                                  0341 00837000
*   DCL                                                            0341 00838000
*     ASM2LB1 CHAR(LENGTH('+ 10')) INIT('+ 10'),                   0341 00839000
*     ASM2LB2 CHAR(LENGTH('TRPAG')) INIT('TRPAG'),                 0341 00840000
*     * CHAR(LENGTH('EREC')) INIT('EREC'),                         0341 00841000
*     * CHAR(LENGTH('MSGBF')) INIT('MSGBF'),                       0341 00842000
