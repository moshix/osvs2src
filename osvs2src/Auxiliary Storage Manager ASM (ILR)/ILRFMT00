*                 END;                                             0809 04795000
*                 RFY                                              0810 04796000
*                   R15 RSTD;                                      0810 04797000
*                 RCOD4=RCOD4|R15;  /* OR IN RETURN CODE             */ 04798000
         L     @14,RCOD4                                           0811 04799000
         OR    @14,R15                                             0811 04800000
         ST    @14,RCOD4                                           0811 04801000
*                 RFY                                              0812 04802000
*                   R15 UNRSTD;                                    0812 04803000
*               END;                                               0813 04804000
*                                                                  0814 04805000
*             /*******************************************************/ 04806000
*             /*                                                     */ 04807000
*             /* IF THIS QUEUE IS CHAINED BY AIANXAIA AND AIAFQPA,   */ 04808000
*             /* CONTINUE PROCESSING ALONG THE FQPA CHAIN UNTIL      */ 04809000
*             /* AIAFQPA = ZERO.                                     */ 04810000
*             /*                                                     */ 04811000
*             /*******************************************************/ 04812000
*                                                                  0814 04813000
*             ELSE                                                 0814 04814000
*               DO;                                                0814 04815000
         B     @RC00797                                            0814 04816000
@RF00797 DS    0H                                                  0815 04817000
*                 IF CHAIN=DOUBLE THEN/* CHAINED VIA NXAIA & FQPA    */ 04818000
         CLC   CHAIN(4),@CF00107                                   0815 04819000
         BNE   @RF00815                                            0815 04820000
*                   DO;                                            0816 04821000
*                     DO;           /* ILRMACCS (AIAPTR)           0817 04822000
*                                      FROM(AIAFQPA)                 */ 04823000
*                       RFY                                        0818 04824000
*                        (R0,                                      0818 04825000
*                         R1,                                      0818 04826000
*                         R14,                                     0818 04827000
*                         R15) RSTD;                               0818 04828000
*                       R0=ADDR(AIAFQPA);                          0819 04829000
         LR    R0,AIAPTR                                           0819 04830000
*                       R1=ABDPLPTR;/* POINT TO PARM LIST            */ 04831000
         L     @11,ABDPLPTR                                        0820 04832000
         LR    R1,@11                                              0820 04833000
*                       CALL ACCESS;/* ACCESS DATA                   */ 04834000
         L     @15,ADPLMEMA(,@11)                                  0821 04835000
         BALR  @14,@15                                             0821 04836000
*                       R14=R0;     /* CANNOT USE R0 AS BASE         */ 04837000
         LR    R14,R0                                              0822 04838000
*                       IF R15=GOOD THEN/* CHECK ACCESS              */ 04839000
         LTR   R15,R15                                             0823 04840000
         BNZ   @RF00823                                            0823 04841000
*                         AIAPTR=CURRENT;/* GET REQUESTED FIELD      */ 04842000
         L     AIAPTR,CURRENT(,R14)                                0824 04843000
*                       ELSE        /* ACCESS NO GOOD                */ 04844000
*                         AIAPTR=ZERO;/* ZERO FIELD                  */ 04845000
         B     @RC00823                                            0825 04846000
@RF00823 SLR   AIAPTR,AIAPTR                                       0825 04847000
*                       RFY                                        0826 04848000
*                        (R0,                                      0826 04849000
*                         R1,                                      0826 04850000
*                         R14,                                     0826 04851000
*                         R15) UNRSTD;                             0826 04852000
@RC00823 DS    0H                                                  0827 04853000
*                     END;                                         0827 04854000
*                     RFY                                          0828 04855000
*                       R15 RSTD;                                  0828 04856000
*                     IF R15=ERROR THEN/* ACCESS FAILED              */ 04857000
         LA    @14,4                                               0829 04858000
         CR    R15,@14                                             0829 04859000
         BNE   @RF00829                                            0829 04860000
*                       RCOD4=ERROR;/* SET ERROR INDICATION          */ 04861000
         ST    @14,RCOD4                                           0830 04862000
*                     ELSE                                         0831 04863000
*                       DO;                                        0831 04864000
         B     @RC00829                                            0831 04865000
@RF00829 DS    0H                                                  0832 04866000
*                         IF AIAPTR=NULLPTR THEN/* FQPA IS NOW ZERO  */ 04867000
         SLR   @14,@14                                             0832 04868000
         CR    AIAPTR,@14                                          0832 04869000
         BNE   @RF00832                                            0832 04870000
*                           DO;                                    0833 04871000
*                             IF SAVEAIA^=NULLPTR THEN/* NEXTPTR NOT    04872000
*                                      ZERO                          */ 04873000
         L     @08,SAVEAIA                                         0834 04874000
         CR    @08,@14                                             0834 04875000
         BE    @RF00834                                            0834 04876000
*                               DO;                                0835 04877000
*                                 AIAPTR=SAVEAIA;/* FIRST BLOCK ON 0836 04878000
*                                      FQPA CHAIN                    */ 04879000
         LR    AIAPTR,@08                                          0836 04880000
*                                 DO;/* ILRMACCS (AIAPTR)          0837 04881000
*                                      FROM(AIANXAIA)                */ 04882000
*                                   RFY                            0838 04883000
*                                    (R0,                          0838 04884000
*                                     R1,                          0838 04885000
*                                     R14,                         0838 04886000
*                                     R15) RSTD;                   0838 04887000
*                                   R0=ADDR(AIANXAIA);             0839 04888000
         LA    R0,AIANXAIA(,AIAPTR)                                0839 04889000
*                                   R1=ABDPLPTR;/* POINT TO PARM LIST*/ 04890000
         L     @11,ABDPLPTR                                        0840 04891000
         LR    R1,@11                                              0840 04892000
*                                   CALL ACCESS;/* ACCESS DATA       */ 04893000
         L     @15,ADPLMEMA(,@11)                                  0841 04894000
         BALR  @14,@15                                             0841 04895000
*                                   R14=R0;/* CANNOT USE R0 AS BASE  */ 04896000
         LR    R14,R0                                              0842 04897000
*                                   IF R15=GOOD THEN/* CHECK ACCESS  */ 04898000
         LTR   R15,R15                                             0843 04899000
         BNZ   @RF00843                                            0843 04900000
*                                    AIAPTR=CURRENT;/* GET REQUESTED    04901000
*                                      FIELD                         */ 04902000
         L     AIAPTR,CURRENT(,R14)                                0844 04903000
*                                   ELSE/* ACCESS NO GOOD            */ 04904000
*                                    AIAPTR=ZERO;/* ZERO FIELD       */ 04905000
         B     @RC00843                                            0845 04906000
@RF00843 SLR   AIAPTR,AIAPTR                                       0845 04907000
*                                   RFY                            0846 04908000
*                                    (R0,                          0846 04909000
*                                     R1,                          0846 04910000
*                                     R14,                         0846 04911000
*                                     R15) UNRSTD;                 0846 04912000
@RC00843 DS    0H                                                  0847 04913000
*                                 END;/* NEXT BLOCK ON NXAIA CHAIN   */ 04914000
*                                 SAVEAIA=AIAPTR;/* SAVE AS 1ST ON 0848 04915000
*                                      FQPA CHAIN                    */ 04916000
         ST    AIAPTR,SAVEAIA                                      0848 04917000
*                                 RFY                              0849 04918000
*                                   R15 RSTD;                      0849 04919000
*                                 RCOD4=RCOD4|R15;/* OR IN RETURN  0850 04920000
*                                      CODE                          */ 04921000
         L     @14,RCOD4                                           0850 04922000
         OR    @14,R15                                             0850 04923000
         ST    @14,RCOD4                                           0850 04924000
*                               END;                               0851 04925000
*                             ELSE                                 0852 04926000
*                               ;   /* SAVEAIA = ZERO                */ 04927000
@RF00834 DS    0H                                                  0853 04928000
*                           END;                                   0853 04929000
*                         ELSE                                     0854 04930000
*                           ;       /* AIAPTR = ZERO                 */ 04931000
@RF00832 DS    0H                                                  0855 04932000
*                       END;                                       0855 04933000
*                     RFY                                          0856 04934000
*                       R15 UNRSTD;                                0856 04935000
@RC00829 DS    0H                                                  0857 04936000
*                   END;                                           0857 04937000
*                                                                  0858 04938000
*                 /***************************************************/ 04939000
*                 /*                                                 */ 04940000
*                 /* IF CHAINED BY AIAFQPA ONLY, GO TO SECTION OF    */ 04941000
*                 /* CODE THAT ACCESSES THE NEXT BLOCK FROM AIAFQPA. */ 04942000
*                 /*                                                 */ 04943000
*                 /***************************************************/ 04944000
*                                                                  0858 04945000
*                 ELSE                                             0858 04946000
*                   DO;             /* CHAINED VIA FQPA ONLY         */ 04947000
         B     @RC00815                                            0858 04948000
@RF00815 DS    0H                                                  0859 04949000
*                     IF CHAIN=FQPA THEN                           0859 04950000
         CLC   CHAIN(4),@CF00156                                   0859 04951000
         BE    @RT00859                                            0859 04952000
*                       GOTO ACCFQPA;                              0860 04953000
*                                                                  0860 04954000
*                     /***********************************************/ 04955000
*                     /*                                             */ 04956000
*                     /* IF CHAINED BY NOTHING (ONLY ONE AIA)        */ 04957000
*                     /*                                             */ 04958000
*                     /***********************************************/ 04959000
*                                                                  0861 04960000
*                     ELSE                                         0861 04961000
*                       AIAPTR=NULLPTR;/* NO MORE AIAS               */ 04962000
         SLR   AIAPTR,AIAPTR                                       0861 04963000
*                   END;                                           0862 04964000
*               END;                                               0863 04965000
*           END;                                                   0864 04966000
*                                                                  0865 04967000
*/********************************************************************/ 04968000
*/* WHEN THE BLOCK IS AN ACE,                                        */ 04969000
*/*   1. SET THE SWITCH TO INDICATE A SINGLE BLOCK REQUEST           */ 04970000
*/*   2. SET UP CORRECT HEADER ANCHOR                                */ 04971000
*/*   3. PUT BLOCK ADDRESS IN ACEPTR                                 */ 04972000
*/*   4. CALL SEGMENT TO FORMAT ACE.   OR IN RETURN CODE             */ 04973000
*/*   5. GET THE BLOCK POINTED TO BY ACEFQPA                         */ 04974000
*/*   6. INCREMENT MAXCTR                                            */ 04975000
*/*   7. RETURN TO BEGINNING OF PROCESSING                           */ 04976000
*/********************************************************************/ 04977000
*                                                                  0865 04978000
*         ELSE                      /* THIS IS AN ACE                */ 04979000
*           DO;                                                    0865 04980000
         B     @RC00751                                            0865 04981000
@RF00751 DS    0H                                                  0866 04982000
*             SINGLE=ON;            /* SET SWITCH FOR SINGLE BLK     */ 04983000
         OI    SINGLE,B'10000000'                                  0866 04984000
*             ANCHNAME=ANCHAIA8;    /* SET UP HEADER                 */ 04985000
         L     @08,ABDPLPTR                                        0867 04986000
         L     @08,ADPLBUF(,@08)                                   0867 04987000
         MVC   ANCHNAME(8,@08),ANCHAIA8                            0867 04988000
*             ANCHTXT=ANCH;                                        0868 04989000
         MVC   ANCHTXT(11,@08),ANCH                                0868 04990000
*             ACEPTR=AIAPTR;        /* PUT ADDR OF BLK INTO ACEPTR   */ 04991000
         ST    AIAPTR,ACEPTR                                       0869 04992000
*             CALL FMTACE;          /* CALL SEGMENT TO FMT ACE       */ 04993000
         BAL   @14,FMTACE                                          0870 04994000
*             RFY                                                  0871 04995000
*               R15 RSTD;                                          0871 04996000
*             RCOD4=RCOD4|R15;      /* OR IN RETURN CODE             */ 04997000
         L     @14,RCOD4                                           0872 04998000
         OR    @14,R15                                             0872 04999000
         ST    @14,RCOD4                                           0872 05000000
*             RFY                                                  0873 05001000
*               R15 UNRSTD;                                        0873 05002000
*ACCFQPA:                                                          0874 05003000
*             DO;                   /* ILRMACCS (AIAPTR)           0874 05004000
*                                      FROM(AIAFQPA)                 */ 05005000
ACCFQPA  DS    0H                                                  0875 05006000
*               RFY                                                0875 05007000
*                (R0,                                              0875 05008000
*                 R1,                                              0875 05009000
*                 R14,                                             0875 05010000
*                 R15) RSTD;                                       0875 05011000
*               R0=ADDR(AIAFQPA);                                  0876 05012000
         LR    R0,AIAPTR                                           0876 05013000
*               R1=ABDPLPTR;        /* POINT TO PARM LIST            */ 05014000
         L     @11,ABDPLPTR                                        0877 05015000
         LR    R1,@11                                              0877 05016000
*               CALL ACCESS;        /* ACCESS DATA                   */ 05017000
         L     @15,ADPLMEMA(,@11)                                  0878 05018000
         BALR  @14,@15                                             0878 05019000
*               R14=R0;             /* CANNOT USE R0 AS BASE         */ 05020000
         LR    R14,R0                                              0879 05021000
*               IF R15=GOOD THEN    /* CHECK ACCESS                  */ 05022000
         LTR   R15,R15                                             0880 05023000
         BNZ   @RF00880                                            0880 05024000
*                 AIAPTR=CURRENT;   /* GET REQUESTED FIELD           */ 05025000
         L     AIAPTR,CURRENT(,R14)                                0881 05026000
*               ELSE                /* ACCESS NO GOOD                */ 05027000
*                 AIAPTR=ZERO;      /* ZERO FIELD                    */ 05028000
         B     @RC00880                                            0882 05029000
@RF00880 SLR   AIAPTR,AIAPTR                                       0882 05030000
*               RFY                                                0883 05031000
*                (R0,                                              0883 05032000
*                 R1,                                              0883 05033000
*                 R14,                                             0883 05034000
*                 R15) UNRSTD;                                     0883 05035000
@RC00880 DS    0H                                                  0884 05036000
*             END;                  /* GET PTR TO NEXT BLK           */ 05037000
*             RFY                                                  0885 05038000
*               R15 RSTD;                                          0885 05039000
*             RCOD4=RCOD4|R15;      /* OR IN RETURN CODE             */ 05040000
         L     @14,RCOD4                                           0886 05041000
         OR    @14,R15                                             0886 05042000
         ST    @14,RCOD4                                           0886 05043000
*             RFY                                                  0887 05044000
*               R15 UNRSTD;                                        0887 05045000
*           END;                                                   0888 05046000
*       END;                        /* END WHEN ACC GOOD FOR AIAOP   */ 05047000
*     ELSE                          /* ACCESS FAILED FOR AIAOP       */ 05048000
*       DO;                                                        0890 05049000
         B     @RC00749                                            0890 05050000
@RF00749 DS    0H                                                  0891 05051000
*         RCOD4=ERROR;              /* SET ERROR CONDITION           */ 05052000
         MVC   RCOD4(4),@CF00168                                   0891 05053000
*         AIAPTR=NULLPTR;           /* RESET PTR TO LEAVE LOOP       */ 05054000
         SLR   AIAPTR,AIAPTR                                       0892 05055000
*       END;                                                       0893 05056000
*   END;                            /* END ALL PROCESSING            */ 05057000
@RC00749 DS    0H                                                  0894 05058000
@DE00742 L     @08,MAXCTR                                          0894 05059000
         C     @08,MAXAIAS                                         0894 05060000
         BNL   @DC00742                                            0894 05061000
         LTR   AIAPTR,AIAPTR                                       0894 05062000
         BNZ   @DL00742                                            0894 05063000
@DC00742 DS    0H                                                  0895 05064000
*                                                                  0895 05065000
*/********************************************************************/ 05066000
*/*  THIS SECTION DETERMINES IF WE FELL OUT OF THE PROCESS LOOP      */ 05067000
*/*   BECAUSE END OF QUEUE WAS REACHED, OR THE MAXIMUM NUMBER OF     */ 05068000
*/*   BLOCKS PRINTED WAS REACHED.  IF THE LATTER CASE, ILRFMSG IS    */ 05069000
*/*   CALLED TO PRINT 'POSSIBLE LOOP IN CHAIN'.                      */ 05070000
*/********************************************************************/ 05071000
*                                                                  0895 05072000
*   IF MAXCTR>=MAXAIAS THEN         /* MAX WAS EXCEEDED              */ 05073000
         L     @11,MAXCTR                                          0895 05074000
         C     @11,MAXAIAS                                         0895 05075000
         BL    @RF00895                                            0895 05076000
*     DO;                                                          0896 05077000
*       RFY                                                        0897 05078000
*         R1 RSTD;                                                 0897 05079000
*       R1=LOOPMSG;                 /* ADDRESS OF MESSAGE            */ 05080000
         L     R1,LOOPMSG                                          0898 05081000
*       CALL ERRMSG;                /* GO TO MESSAGE SEGMENT         */ 05082000
         BAL   @14,ERRMSG                                          0899 05083000
*       RCOD4=ERROR;                /* SET RETURN CODE TO FOUR       */ 05084000
         MVC   RCOD4(4),@CF00168                                   0900 05085000
*     END;                                                         0901 05086000
*   ELSE                                                           0902 05087000
*     ;                             /* ALL'S OKAY                    */ 05088000
@RF00895 DS    0H                                                  0903 05089000
*   RFY                                                            0903 05090000
*    (R14,                                                         0903 05091000
*     R15) RSTD;                                                   0903 05092000
*   R15=RCOD4;                      /* SET RETURN CODE FOR CALL      */ 05093000
         L     R15,RCOD4                                           0904 05094000
*   R14=RTNADDR7;                   /* RESTORE RETURN ADDRESS        */ 05095000
         L     R14,RTNADDR7                                        0905 05096000
*   RFY                                                            0906 05097000
*    (R14,                                                         0906 05098000
*     R15) UNRSTD;                                                 0906 05099000
*FMTPCB:                                                           0907 05100000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0907 05101000
         B     @PB00008                                            0907 05102000
FMTPCB   DS    0H                                                  0908 05103000
*                                                                  0908 05104000
*   /*****************************************************************/ 05105000
*   /*                                                               */ 05106000
*   /* PCB1:ILRMLINE (CQN,FQPA,BQPA,(FL1,,BIT),RTPA) OF(PCB) LABEL   */ 05107000
*   /* VARIABLES REFERENCED IN FORMAT PATTERNS                       */ 05108000
*   /*                                                               */ 05109000
*   /*****************************************************************/ 05110000
*                                                                  0908 05111000
*   DCL                                                            0908 05112000
*     PCB1LB1 CHAR(LENGTH('CQN')) INIT('CQN');                     0908 05113000
*   DCL                                                            0909 05114000
*     PCB1LB2 CHAR(LENGTH('FQPA')) INIT('FQPA');                   0909 05115000
*   DCL                                                            0910 05116000
*     PCB1LB3 CHAR(LENGTH('BQPA')) INIT('BQPA');                   0910 05117000
*   DCL                                                            0911 05118000
*     PCB1LB4 CHAR(LENGTH('FL1')) INIT('FL1');                     0911 05119000
*   DCL                                                            0912 05120000
*     PCB1LB5 CHAR(LENGTH('RTPA')) INIT('RTPA');                   0912 05121000
*                                                                  0912 05122000
*   /*****************************************************************/ 05123000
*   /*                                                               */ 05124000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 05125000
*   /*                                                               */ 05126000
*   /*****************************************************************/ 05127000
*                                                                  0913 05128000
*   DCL                                                            0913 05129000
*     1 PCB1PTS BDY(WORD),                                         0913 05130000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 05131000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 05132000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PCBCQN)-1+16*LENGTH('CQN')-  05133000
*             16),                  /* SEE PTTLBLGH AND PTTDTLGH     */ 05134000
*       3 * FIXED(8) INIT(5),                                      0913 05135000
*       3 * FIXED(8) INIT(11),                                     0913 05136000
*       3 * PTR(31) INIT(ADDR(PCB1LB1)),/* SEE PTTLBPTR              */ 05137000
*       3 PCB1DT1 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 05138000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 05139000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 05140000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PCBFQPA)-1+16*LENGTH('FQPA') 05141000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 05142000
*       3 * FIXED(8) INIT(15+8),                                   0913 05143000
*       3 * FIXED(8) INIT(21+8),                                   0913 05144000
*       3 * PTR(31) INIT(ADDR(PCB1LB2)),/* SEE PTTLBPTR              */ 05145000
*       3 PCB1DT2 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 05146000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 05147000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 05148000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PCBBQPA)-1+16*LENGTH('BQPA') 05149000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 05150000
*       3 * FIXED(8) INIT(25+8+8),                                 0913 05151000
*       3 * FIXED(8) INIT(31+8+8),                                 0913 05152000
*       3 * PTR(31) INIT(ADDR(PCB1LB3)),/* SEE PTTLBPTR              */ 05153000
*       3 PCB1DT3 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 05154000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 05155000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 05156000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PCBFL1)/8-1+16*LENGTH('FL1') 05157000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 05158000
*       3 * FIXED(8) INIT(35+8+8+8),                               0913 05159000
*       3 * FIXED(8) INIT(41+8+8+8),                               0913 05160000
*       3 * PTR(31) INIT(ADDR(PCB1LB4)),/* SEE PTTLBPTR              */ 05161000
*       3 PCB1DT4 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 05162000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 05163000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 05164000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PCBRTPA)-1+16*LENGTH('RTPA') 05165000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 05166000
*       3 * FIXED(8) INIT(45+8+8+8+8),                             0913 05167000
*       3 * FIXED(8) INIT(51+8+8+8+8),                             0913 05168000
*       3 * PTR(31) INIT(ADDR(PCB1LB5)),/* SEE PTTLBPTR              */ 05169000
*       3 PCB1DT5 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 05170000
*      2 * FIXED(31) INIT(0);       /* END OF FORMAT PATTERNS        */ 05171000
*                                                                  0914 05172000
*   /*****************************************************************/ 05173000
*   /*                                                               */ 05174000
*   /* PCB2:ILRMLINE (FXC,RLPA,(FL2,,BIT),XPTA,(FL3,,BIT),PGTA) LABEL*/ 05175000
*   /* VARIABLES REFERENCED IN FORMAT PATTERNS                       */ 05176000
*   /*                                                               */ 05177000
*   /*****************************************************************/ 05178000
*                                                                  0914 05179000
*   DCL                                                            0914 05180000
*     PCB2LB1 CHAR(LENGTH('FXC')) INIT('FXC');                     0914 05181000
*   DCL                                                            0915 05182000
*     PCB2LB2 CHAR(LENGTH('RLPA')) INIT('RLPA');                   0915 05183000
*   DCL                                                            0916 05184000
*     PCB2LB3 CHAR(LENGTH('FL2')) INIT('FL2');                     0916 05185000
*   DCL                                                            0917 05186000
*     PCB2LB4 CHAR(LENGTH('XPTA')) INIT('XPTA');                   0917 05187000
*   DCL                                                            0918 05188000
*     PCB2LB5 CHAR(LENGTH('FL3')) INIT('FL3');                     0918 05189000
*   DCL                                                            0919 05190000
*     PCB2LB6 CHAR(LENGTH('PGTA')) INIT('PGTA');                   0919 05191000
*                                                                  0919 05192000
*   /*****************************************************************/ 05193000
*   /*                                                               */ 05194000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 05195000
*   /*                                                               */ 05196000
*   /*****************************************************************/ 05197000
*                                                                  0920 05198000
*   DCL                                                            0920 05199000
*     1 PCB2PTS BDY(WORD),                                         0920 05200000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 05201000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 05202000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PCBFXC)-1+16*LENGTH('FXC')-  05203000
*             16),                  /* SEE PTTLBLGH AND PTTDTLGH     */ 05204000
*       3 * FIXED(8) INIT(5),                                      0920 05205000
*       3 * FIXED(8) INIT(11),                                     0920 05206000
