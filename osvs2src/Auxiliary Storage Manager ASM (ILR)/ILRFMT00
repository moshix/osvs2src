*   ELSE                                                           0572 03626000
*     ;                             /* THE PTR WAS ZERO              */ 03627000
@RF00565 DS    0H                                                  0573 03628000
*   RCOD1=RCOD1|R15;                /* OR IN RETURN CODE             */ 03629000
         L     @14,RCOD1                                           0573 03630000
         OR    @14,R15                                             0573 03631000
         ST    @14,RCOD1                                           0573 03632000
*   R15=RCOD1;                      /* SET RETURN CODE FOR CALLER    */ 03633000
         LR    R15,@14                                             0574 03634000
*   RFY                                                            0575 03635000
*     R14 RSTD;                                                    0575 03636000
*   R14=RTNADDR1;                   /* RESTORE RETURN ADDRESS        */ 03637000
         L     R14,RTNADDR1                                        0576 03638000
*   RFY                                                            0577 03639000
*    (R14,                                                         0577 03640000
*     R15) UNRSTD;                                                 0577 03641000
*   END FMTASMVT;                   /* RETURN TO CALLER              */ 03642000
@EL00004 DS    0H                                                  0578 03643000
@EF00004 DS    0H                                                  0578 03644000
@ER00004 BR    @14                                                 0578 03645000
*                                                                  0579 03646000
*/********************************************************************/ 03647000
*/*                                                                  */ 03648000
*/* FMTEREC:  SEGMENT THAT FORMAT THE BAD SLOT ERROR RECORD          */ 03649000
*/*                                                                  */ 03650000
*/* INPUT:    VIRTUAL ADDRESS OF EREC IN ERECPTR                     */ 03651000
*/*                                                                  */ 03652000
*/* PROCESS:  FORMAT THE HEADER SECTION, THEN THE RECORD ITSELF      */ 03653000
*/*            IS DUMPED  EIGHT ENTRIES TO A LINE PRECEDED BY        */ 03654000
*/*            ADDRESSES                                             */ 03655000
*/*                                                                  */ 03656000
*/* OUTPUT:   RETURN CODE = 0 IF ACCESSES AND FORMAT WERE            */ 03657000
*/*            SUCCESSFUL.  ELSE, RETURN GREATER THAN ZERO           */ 03658000
*/*                                                                  */ 03659000
*/********************************************************************/ 03660000
*                                                                  0579 03661000
*FMTEREC:                                                          0579 03662000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0579 03663000
FMTEREC  DS    0H                                                  0580 03664000
*                                                                  0580 03665000
*   /*****************************************************************/ 03666000
*   /*                                                               */ 03667000
*   /* ERE1:ILRMLINE (CURNT,CFRST,CLAST) OF(EREC) LABEL VARIABLES    */ 03668000
*   /* REFERENCED IN FORMAT PATTERNS                                 */ 03669000
*   /*                                                               */ 03670000
*   /*****************************************************************/ 03671000
*                                                                  0580 03672000
*   DCL                                                            0580 03673000
*     ERE1LB1 CHAR(LENGTH('CURNT')) INIT('CURNT');                 0580 03674000
*   DCL                                                            0581 03675000
