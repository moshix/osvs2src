         ST    @11,SEGSPTR                                         0719 02772000
*                 PPTRNPTR=PPTRNPTR+LENGTH(PTTRNS);/* OBTAIN ADDR  0720 02773000
*                                      NEXT PATTERN IN SPCT5         */ 02774000
         AH    @10,@CH00080                                        0720 02775000
         ST    @10,PPTRNPTR                                        0720 02776000
*                 REM=REM-ONE;      /* DECR NUMBER SEGES LEFT TO PUT    02777000
*                                      IN PARTIAL LINE               */ 02778000
         L     @11,REM                                             0721 02779000
         BCTR  @11,0                                               0721 02780000
         ST    @11,REM                                             0721 02781000
*               END;                /* END BUILD PARTIAL PATTERNS    */ 02782000
@DE00717 SLR   @11,@11                                             0722 02783000
         C     @11,REM                                             0722 02784000
         BL    @DL00717                                            0722 02785000
*               PTTRNSAV=PTTSTOP;   /* SAVE PATTERN FORMAT TEMP      */ 02786000
         L     @10,PPTRNPTR                                        0723 02787000
         MVC   PTTRNSAV(4),PTTSTOP(@10)                            0723 02788000
*               PTTSTOP=ZERO;       /* ZERO ENDS FORMAT PATTERNS     */ 02789000
         ST    @11,PTTSTOP(,@10)                                   0724 02790000
*               R0=ADDR(SPCT5PTS);  /* PATTERNS FOR PARTIAL LINE OF     02791000
*                                      SEGES                         */ 02792000
         LA    R0,SPCT5PTS                                         0725 02793000
*               R1=ABDPLPTR;        /* ADDR INPUT PARM LIST          */ 02794000
