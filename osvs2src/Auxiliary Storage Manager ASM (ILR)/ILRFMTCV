*               PTTSTOP=PTTRNSAV;   /* RESTORE PATTERN FORMAT        */ 02806000
         L     @11,PPTRNPTR                                        0730 02807000
         MVC   PTTSTOP(4,@11),PTTRNSAV                             0730 02808000
*             END;                  /* END PARTIAL LINE              */ 02809000
*         END;                      /* ENTRYCTR NOT ZERO             */ 02810000
*     END;                          /* ACCESS GOOD FOR NUMBER OF   0733 02811000
*                                      ENTRIES                       */ 02812000
*   ELSE                                                           0734 02813000
*     CUMRC=CUMRC|R15;              /* ACCESS FAILED FOR SSEG        */ 02814000
