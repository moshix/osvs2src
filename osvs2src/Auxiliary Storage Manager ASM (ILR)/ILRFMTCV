*               DO WHILE REM>ZERO;  /* UNTIL ALL REMAINING ENTRIES 0717 02760000
*                                      PUT IN LINE                   */ 02761000
         B     @DE00717                                            0717 02762000
@DL00717 DS    0H                                                  0718 02763000
*                 PTTDTPTR=SEGSPTR; /* STORE SEGE ADDR IN PATTERN IN    02764000
*                                      SPCT5                         */ 02765000
         L     @11,SEGSPTR                                         0718 02766000
         L     @10,PPTRNPTR                                        0718 02767000
         ST    @11,PTTDTPTR(,@10)                                  0718 02768000
*                 SEGSPTR=SEGSPTR+LENGTH(SPCTSEGE);/* ADDRESS NEXT 0719 02769000
