         BZ    @RF00384                                            0384 01239000
*         DO;                       /* ACCESS WAS GOOD               */ 01240000
*           R0=ADDR(SPCTSSEG);                                     0386 01241000
         LA    R0,SPCTSSEG(,@11)                                   0386 01242000
*           R1=ABDPLPTR;            /* POINT TO PARM LIST            */ 01243000
         LR    R1,ABDPLPTR                                         0387 01244000
*           CALL ACCESS;            /* ACCESS DATA                   */ 01245000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0388 01246000
         BALR  @14,@15                                             0388 01247000
*           R14=R0;                 /* CANNOT USE R0 AS BASE         */ 01248000
         LR    R14,R0                                              0389 01249000
*           IF R15=ZERO THEN                                       0390 01250000
         SLR   @11,@11                                             0390 01251000
         CR    R15,@11                                             0390 01252000
         BNE   @RF00390                                            0390 01253000
*             DO;                   /* ACCESS WAS GOOD               */ 01254000
*               ENTRYCTR=CURRBYTE;  /* GET REQUESTED FIELD BYTE      */ 01255000
         SLR   @10,@10                                             0392 01256000
         IC    @10,CURRBYTE(,R14)                                  0392 01257000
         ST    @10,ENTRYCTR                                        0392 01258000
*               IF ENTRYCTR^=ZERO THEN                             0393 01259000
         CR    @10,@11                                             0393 01260000
         BE    @RF00393                                            0393 01261000
*                 DO;               /* THERE ARE ENTRIES             */ 01262000
*                   SEGSPTR=ADDR(SPCTSEGS);/* CALC ADDR OF FIRST   0395 01263000
*                                      ENTRY                         */ 01264000
         L     @11,SPCTPTR                                         0395 01265000
         LA    @11,SPCTSEGS(,@11)                                  0395 01266000
         ST    @11,SEGSPTR                                         0395 01267000
*                   DO I=1 TO ENTRYCTR;/* UNTIL ALL SEGMENTS ENTRIES    01268000
*                                      ARE DONE                      */ 01269000
         LA    @11,1                                               0396 01270000
         B     @DE00396                                            0396 01271000
@DL00396 DS    0H                                                  0397 01272000
*                     DO;           /* ILRMACCS (SENTRY)           0397 01273000
*                                      FROM(SPCTSEGE)                */ 01274000
*                       RFY                                        0398 01275000
*                        (R0,                                      0398 01276000
*                         R1,                                      0398 01277000
*                         R14,                                     0398 01278000
*                         R15) RSTD;                               0398 01279000
*                       R0=ADDR(SPCTSEGE);                         0399 01280000
         L     R0,SEGSPTR                                          0399 01281000
*                       R1=ABDPLPTR;/* POINT TO PARM LIST            */ 01282000
         LR    R1,ABDPLPTR                                         0400 01283000
*                       CALL ACCESS;/* ACCESS DATA                   */ 01284000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0401 01285000
         BALR  @14,@15                                             0401 01286000
*                       R14=R0;     /* CANNOT USE R0 AS BASE         */ 01287000
         LR    R14,R0                                              0402 01288000
*                       IF R15=GOOD THEN/* CHECK ACCESS              */ 01289000
         LTR   R15,R15                                             0403 01290000
         BNZ   @RF00403                                            0403 01291000
*                         SENTRY=CURRENT;/* GET REQUESTED FIELD      */ 01292000
         MVC   SENTRY(4),CURRENT(R14)                              0404 01293000
*                       ELSE        /* ACCESS NO GOOD                */ 01294000
*                         SENTRY=ZERO;/* ZERO FIELD                  */ 01295000
         B     @RC00403                                            0405 01296000
@RF00403 SLR   @11,@11                                             0405 01297000
         ST    @11,SENTRY                                          0405 01298000
*                       RFY                                        0406 01299000
*                        (R0,                                      0406 01300000
*                         R1,                                      0406 01301000
*                         R14,                                     0406 01302000
*                         R15) UNRSTD;                             0406 01303000
@RC00403 DS    0H                                                  0407 01304000
*                     END;                                         0407 01305000
*                     IF SENTRY^=ZERO THEN                         0408 01306000
         ICM   @11,15,SENTRY                                       0408 01307000
         BZ    @RF00408                                            0408 01308000
*                       DO;         /* ACCESS WAS GOOD               */ 01309000
*                         RFY                                      0410 01310000
*                           SPCTSEGE BASED(ADDR(SENTRY));          0410 01311000
*                         CTR=SPCTSEGX;/* FOR VSA TO BE PRINTED      */ 01312000
         LA    @11,SENTRY                                          0411 01313000
         SLR   @10,@10                                             0411 01314000
         IC    @10,SPCTSEGX(,@11)                                  0411 01315000
         ST    @10,CTR                                             0411 01316000
*                         PGTPTR=SPCTPGT;/* PAGE TABLE VIRTUAL ADDR  */ 01317000
         SLR   @10,@10                                             0412 01318000
         ICM   @10,7,SPCTPGT(@11)                                  0412 01319000
         ST    @10,PGTPTR                                          0412 01320000
*                         RFY                                      0413 01321000
*                           SPCTSEGE BASED(SEGSPTR);               0413 01322000
*                         CALL FMTPGXPT;/* CALL ROUTINE TO FORMAT  0414 01323000
*                                      PAGE TABLES                   */ 01324000
         BAL   @14,FMTPGXPT                                        0414 01325000
*                       END;        /* ACCESS GOOD FOR SEGMENT ENTRY */ 01326000
*                     ELSE                                         0416 01327000
*                       DO;         /* ACCESS FAILED FOR SEGMENT   0416 01328000
*                                      ENTRY                         */ 01329000
         B     @RC00408                                            0416 01330000
@RF00408 DS    0H                                                  0417 01331000
*                         CUMRC=CUMRC|ERROR;/* OR IN ERROR           */ 01332000
         L     @11,CUMRC                                           0417 01333000
         O     @11,@CF00064                                        0417 01334000
         ST    @11,CUMRC                                           0417 01335000
*                         I=ENTRYCTR;/* FORCE END OF LOOP            */ 01336000
         MVC   I(4),ENTRYCTR                                       0418 01337000
*                       END;                                       0419 01338000
*                     SEGSPTR=SEGSPTR+LENGTH(SPCTSEGE);/* CALC ADDR     01339000
*                                      NEXT SEGE                     */ 01340000
@RC00408 LA    @11,6                                               0420 01341000
         AL    @11,SEGSPTR                                         0420 01342000
         ST    @11,SEGSPTR                                         0420 01343000
*                   END;            /* END DO FOR ENTRIES            */ 01344000
         LA    @11,1                                               0421 01345000
         AL    @11,I                                               0421 01346000
@DE00396 ST    @11,I                                               0421 01347000
         C     @11,ENTRYCTR                                        0421 01348000
         BNH   @DL00396                                            0421 01349000
*                 END;              /* THERE ARE ENTRIES             */ 01350000
*             END;                  /* ACCESS GOOD FOR NUMBER OF   0423 01351000
*                                      ENTRIES                       */ 01352000
*           ELSE                                                   0424 01353000
*             CUMRC=CUMRC|R15;      /* ACC FAILED FOR SSEG OR ZERO   */ 01354000
         B     @RC00390                                            0424 01355000
@RF00390 L     @12,CUMRC                                           0424 01356000
         OR    @12,R15                                             0424 01357000
         ST    @12,CUMRC                                           0424 01358000
*         END;                      /* END ACCESS GOOD FOR SPCT      */ 01359000
*       ELSE                                                       0426 01360000
*         CUMRC=CUMRC|R15;          /* ACCESS FAILED FOR SPCT OR ZERO*/ 01361000
