         L     @15,ADPLMEMA(,ABDPLPTR)                             1109 04351000
         BALR  @14,@15                                             1109 04352000
*     R14=R0;                       /* CANNOT USE R0 AS BASE         */ 04353000
         LR    R14,R0                                              1110 04354000
*     IF R15=GOOD THEN              /* CHECK ACCESS                  */ 04355000
         LTR   R15,R15                                             1111 04356000
         BNZ   @RF01111                                            1111 04357000
*       ASPCTPTR=CURRENT;           /* GET REQUESTED FIELD           */ 04358000
         MVC   ASPCTPTR(4),CURRENT(R14)                            1112 04359000
*     ELSE                          /* ACCESS NO GOOD                */ 04360000
*       ASPCTPTR=ZERO;              /* ZERO FIELD                    */ 04361000
         B     @RC01111                                            1113 04362000
@RF01111 SLR   @11,@11                                             1113 04363000
