         L     @11,FORMRC                                          0700 02708000
         OR    @11,R15                                             0700 02709000
         ST    @11,FORMRC                                          0700 02710000
*                   RFY                                            0701 02711000
*                     R15 UNRSTD;                                  0701 02712000
*                   CALL PRINT;     /* CALL PRINT ROUTINE            */ 02713000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0702 02714000
         BALR  @14,@15                                             0702 02715000
*                   RFY                                            0703 02716000
*                    (R0,                                          0703 02717000
*                     R1) UNRSTD;                                  0703 02718000
*                   RFY                                            0704 02719000
*                     R15 RSTD;                                    0704 02720000
*                   R15=FORMRC;     /* SAVE RETURN CODE FROM FORMAT  */ 02721000
         L     R15,FORMRC                                          0705 02722000
*                   RFY                                            0706 02723000
*                     R15 UNRSTD;                                  0706 02724000
*                 END;              /* ILRMFRMT ARRAY(SPCT5)         */ 02725000
*                 CUMRC=CUMRC|R15;  /* OR IN RETURN CODE             */ 02726000
         L     @11,CUMRC                                           0708 02727000
         OR    @11,R15                                             0708 02728000
         ST    @11,CUMRC                                           0708 02729000
*                 IF R15^=ZERO THEN                                0709 02730000
         LTR   R15,R15                                             0709 02731000
         BZ    @RF00709                                            0709 02732000
*                   I=CTR;          /* FORCE END OF LOOP IF ERROR IN    02733000
*                                      FORMAT                        */ 02734000
         MVC   I(4),CTR                                            0710 02735000
*                 SEGSPTR=SEGSPTR+SEGELINE;/* NUMBER OF BYTES PER  0711 02736000
*                                      LINE FOR SEGES                */ 02737000
@RF00709 LA    @11,36                                              0711 02738000
         AL    @11,SEGSPTR                                         0711 02739000
         ST    @11,SEGSPTR                                         0711 02740000
*               END;                /* ALL FULL-LINE ENTRIES         */ 02741000
         LA    @11,1                                               0712 02742000
         AL    @11,I                                               0712 02743000
@DE00686 ST    @11,I                                               0712 02744000
         C     @11,CTR                                             0712 02745000
         BNH   @DL00686                                            0712 02746000
*             END;                  /* END ENTRYCTR >= 6             */ 02747000
*           IF REM^=ZERO&R15=ZERO THEN                             0714 02748000
