         BE    @RF00862                                            0862 03358000
*           LGVTPTR=NULLPTR;                                       0863 03359000
         ST    @11,LGVTPTR                                         0863 03360000
*       END;                                                       0864 03361000
*     ELSE                                                         0865 03362000
*       LGVTPTR=NULLPTR;                                           0865 03363000
         B     @RC00856                                            0865 03364000
@RF00856 SLR   @11,@11                                             0865 03365000
         ST    @11,LGVTPTR                                         0865 03366000
*     RFY                                                          0866 03367000
*      (R0,                                                        0866 03368000
*       R1,                                                        0866 03369000
*       R14,                                                       0866 03370000
*       R15) UNRSTD;                                               0866 03371000
@RC00856 DS    0H                                                  0867 03372000
*   END;                            /*                       @ZA11398*/ 03373000
*   IF LGVTPTR^=ZERO THEN           /* ACCESS WAS SUCCESSFUL         */ 03374000
         ICM   @11,15,LGVTPTR                                      0868 03375000
         BZ    @RF00868                                            0868 03376000
*     CALL FMTLGVT;                 /* CALL SEGMENT TO FMT LGVT AND     03377000
*                                      RELATED BLOCKS                */ 03378000
         BAL   @14,FMTLGVT                                         0869 03379000
*   ELSE                                                           0870 03380000
*     R15=LGVTERR;                  /* ACCESS FAILED, FMT00 PRINTS 0870 03381000
*                                      LGVT INACCESSIBLE             */ 03382000
         B     @RC00868                                            0870 03383000
@RF00868 LA    R15,8                                               0870 03384000
*FMTVEND:                                                          0871 03385000
*   RETURN CODE(R15);               /* RETURNS TO FMT00, ENDS VIO    */ 03386000
@RC00868 DS    0H                                                  0871 03387000
FMTVEND  L     @13,4(,@13)                                         0871 03388000
         L     @14,12(,@13)                                        0871 03389000
         LM    @00,@12,20(@13)                                     0871 03390000
         BR    @14                                                 0871 03391000
*                                                                  0872 03392000
*/********************************************************************/ 03393000
*/*FMTLGVT:   CHART (SEQ)                                            */ 03394000
*/*FMTLGVT:   E FMTLGVT                                              */ 03395000
*/*           P SAVE RETURN ADDRESS                                  */ 03396000
*/*           L LINE - FORMAT PATTERNS FOR LGVT AND LGVTE            */ 03397000
*/*           P CLEAR CUMULATIVE RETURN CODE                         */ 03398000
*/*           P SET UP HEADER                                        */ 03399000
*/*           P PRINT HEADER                                         */ 03400000
*/*           L FORMAT - FORMAT AND PRINT LGVT BASE                  */ 03401000
*/*           P OR IN RETURN CODE                                    */ 03402000
*/*           L ACCESS:      LGVMAXLG, MAXIMUM LGID                  */ 03403000
*/*           D (YES,,NO,%ERRET) RETURN CODE GOOD ?                  */ 03404000
*/*%AA:       P INITIALIZE LGN = 0                                   */ 03405000
*/*           P POINT TO FIRST LGVTE                                 */ 03406000
*/*%MORE:     D (NO,,YES,%RETURN) ALL ENTRIES DONE ? (LGN > MAXLG)   */ 03407000
*/*           L ACCESS: GET LGVTLGID                                 */ 03408000
*/*           D (YES,,NO,%ERROR) RETURN CODE GOOD ?                  */ 03409000
*/*           D (YES,,NO,%INVALID) LGVTE ASSIGNED ? (LGN ^= LGID)    */ 03410000
*/*           L FORMAT - FORMAT AND PRINT ASSIGNED LGVTE             */ 03411000
*/*           P OR IN RETURN CODE                                    */ 03412000
*/*%BB:       L ACCESS:      LGVELGEP                                */ 03413000
*/*           D (YES,,NO,%ERROR) POINTER GOOD ?                      */ 03414000
*/*           S FMTLGE: CALL ROUTINE TO FORMAT LGE AND RELATED BLKS  */ 03415000
*/*%ERROR:    P OR IN RETURN CODE                                    */ 03416000
*/*%NEXT:     P POINT TO NEXT ENTRY                                  */ 03417000
*/*           P (,%MORE) INCREMENT LGN                               */ 03418000
*/*%RETURN:   P RESTORE RETURN ADDRESS                               */ 03419000
*/*           R RETURN                                               */ 03420000
*/*%INVALID:  L (,%ERROR) FORMAT: FORMAT AND PRINT UNASSIGNED LGVTE  */ 03421000
*/*%ERRET:    P (,%RETURN)  OR IN RETURN CODE                        */ 03422000
*/*FMTLGVT:   END                                                    */ 03423000
*/********************************************************************/ 03424000
*                                                                  0872 03425000
*                                                                  0872 03426000
*/********************************************************************/ 03427000
*/*                                                                  */ 03428000
*/* FMTLGVT: SEGMENT TO FORMAT LGVT AND ITS ENTRIES                  */ 03429000
*/*                                                                  */ 03430000
*/* INPUT:   VIRTUAL ADDRESS IN LGVTPTR                              */ 03431000
*/*                                                                  */ 03432000
*/* PROCESS: FORMAT THE LGVT.  FORMAT EACH ENTRY. IF THE ENTRY       */ 03433000
*/*           IS ASSIGNED, CALL SEGMENT TO FORMAT LGE AND ITS        */ 03434000
*/*           RELATED BLOCKS.                                        */ 03435000
*/*                                                                  */ 03436000
*/* OUTPUT:  THE CUMULATIVE RETURN CODE FROM ACCESSES, FORMATS       */ 03437000
*/*           AND SEGMENTS CALLED                                    */ 03438000
*/*                                                                  */ 03439000
*/********************************************************************/ 03440000
*                                                                  0872 03441000
*FMTLGVT:                                                          0872 03442000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0872 03443000
FMTLGVT  DS    0H                                                  0873 03444000
*                                                                  0873 03445000
*   /*****************************************************************/ 03446000
*   /*                                                               */ 03447000
*   /* LGV1:ILRMLINE ((IDENT,ASIS),LGVEP,MAXLG,SIZE) OF(LGVT) LABEL  */ 03448000
*   /* VARIABLES REFERENCED IN FORMAT PATTERNS                       */ 03449000
*   /*                                                               */ 03450000
*   /*****************************************************************/ 03451000
*                                                                  0873 03452000
*   DCL                                                            0873 03453000
*     LGV1LB1 CHAR(LENGTH('IDENT')) INIT('IDENT');                 0873 03454000
*   DCL                                                            0874 03455000
*     LGV1LB2 CHAR(LENGTH('LGVEP')) INIT('LGVEP');                 0874 03456000
*   DCL                                                            0875 03457000
*     LGV1LB3 CHAR(LENGTH('MAXLG')) INIT('MAXLG');                 0875 03458000
*   DCL                                                            0876 03459000
*     LGV1LB4 CHAR(LENGTH('SIZE')) INIT('SIZE');                   0876 03460000
*                                                                  0876 03461000
*   /*****************************************************************/ 03462000
*   /*                                                               */ 03463000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 03464000
*   /*                                                               */ 03465000
*   /*****************************************************************/ 03466000
*                                                                  0877 03467000
*   DCL                                                            0877 03468000
*     1 LGV1PTS BDY(WORD),                                         0877 03469000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 03470000
*       3 * BIT(8) BDY(BYTE) INIT('00101111'B),/* SEE PTTFLGS        */ 03471000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(LGVIDENT)-1+16*LENGTH('IDENT 03472000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 03473000
*       3 * FIXED(8) INIT(5),                                      0877 03474000
*       3 * FIXED(8) INIT(11),                                     0877 03475000
*       3 * PTR(31) INIT(ADDR(LGV1LB1)),/* SEE PTTLBPTR              */ 03476000
