*/********************************************************************/ 03287000
*/*ILRFMTV:   CHART (SEQ)                                            */ 03288000
*/*ILRFMTV:   E VIO SEGMENT                                          */ 03289000
*/*           P SAVE CALLERS REGS, ESTABLISH ADDRESSIBILITY          */ 03290000
*/*           P OBTAIN ABDUMP PARM LIST PTR (R1)                     */ 03291000
*/*           P OBTAIN ASMVT PTR (R3)                                */ 03292000
*/*           L ACCESS:      ASMLGVT                                 */ 03293000
*/*           D (YES,,NO,%ERRET2)  POINTER GOOD ?                    */ 03294000
*/*           S FMTLGVT: CALL RTN TO FORMAT LGVT AND RELATED BLOCKS  */ 03295000
*/*           P OR IN RETURN CODE                                    */ 03296000
*/*%RETURN:   P RESTORE CALLERS REGS                                 */ 03297000
*/*           R RETURN                                               */ 03298000
*/*%ERRET2:   P (,%RETURN) RC = 8, FMT00 PRINTS LGVT BAD             */ 03299000
*/*ILRFMTV:   END                                                    */ 03300000
*/********************************************************************/ 03301000
*                                                                  0847 03302000
*                                                                  0847 03303000
*/********************************************************************/ 03304000
*/*                                                                  */ 03305000
*/* VIO:      SEGMENT TO CAUSE THE FORMATTING OF VIO RELATED         */ 03306000
*/*            CONTROL BLOCKS                                        */ 03307000
*/*                                                                  */ 03308000
*/* INPUT:    R1=ABDPLPTR (ABDUMP PARM LIST)                         */ 03309000
*/*            VIRTUAL ADDRESS OF THE ASMVT IN R3                    */ 03310000
*/*                                                                  */ 03311000
*/* PROCESS:  GETS LGVT ADDRESS AND CALLS SEGMENT TO FORMAT          */ 03312000
*/*            LGVT AND RELATED BLOCKS.                              */ 03313000
*/*                                                                  */ 03314000
*/* OUTPUT:   RETURN CODE IN R15 (0, 4, OR 8)                        */ 03315000
*/*                                                                  */ 03316000
*/********************************************************************/ 03317000
*                                                                  0847 03318000
*ILRFMTV:                                                          0847 03319000
*   ENTRY;                          /* FORMAT VIO ENTRY              */ 03320000
@EP00847 DS    0H                                                  0848 03321000
*   ABDPLPTR=R1;                    /* GET ADDR OF PARMLIST          */ 03322000
         LR    ABDPLPTR,R1                                         0848 03323000
*   CVTPTR=ADPLCVT;                 /* GET CVT PTR FROM PARM LIST    */ 03324000
         MVC   CVTPTR(4),ADPLCVT(ABDPLPTR)                         0849 03325000
*   ASMVTPTR=R3;                    /* GET ASMVT PTR                 */ 03326000
         ST    R3,ASMVTPTR                                         0850 03327000
*   DO;                             /* ILRMACCS (LGVT) VIA(ASMLGVT)  */ 03328000
*     RFY                                                          0852 03329000
*      (R0,                                                        0852 03330000
*       R1,                                                        0852 03331000
*       R14,                                                       0852 03332000
*       R15) RSTD;                                                 0852 03333000
*     R1=ABDPLPTR;                                                 0853 03334000
         LR    R1,ABDPLPTR                                         0853 03335000
*     R0=ADDR(ASMLGVT);             /* ADDR OF CELL WITH PTR         */ 03336000
         L     @11,ASMVTPTR                                        0854 03337000
         LA    R0,ASMLGVT(,@11)                                    0854 03338000
*     CALL ACCESS;                  /* ACCESS PTR TO LGVT            */ 03339000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0855 03340000
         BALR  @14,@15                                             0855 03341000
*     IF R15=GOOD THEN                                             0856 03342000
         LTR   R15,R15                                             0856 03343000
         BNZ   @RF00856                                            0856 03344000
*       DO;                         /* FOUND PTR - NOW FIND BLOCK    */ 03345000
*         R14=R0;                   /* CANNOT USE R0 AS BASE REG     */ 03346000
         LR    R14,R0                                              0858 03347000
*         R0=CURRENT;               /* POINTER ITSELF                */ 03348000
         L     R0,CURRENT(,R14)                                    0859 03349000
*         LGVTPTR=R0;               /* SAVE POINTER                  */ 03350000
         ST    R0,LGVTPTR                                          0860 03351000
*         CALL ACCESS;              /* ACCESS LGVT ITSELF            */ 03352000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0861 03353000
         BALR  @14,@15                                             0861 03354000
*         IF R15^=GOOD THEN                                        0862 03355000
         SLR   @11,@11                                             0862 03356000
