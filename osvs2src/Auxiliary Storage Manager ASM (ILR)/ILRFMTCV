*   CALL FMTASMHD;                  /* CALL SEGMENT TO FMT ASMHD     */ 00964000
*                                                                  0291 00965000
         BAL   @14,FMTASMHD                                        0291 00966000
*   /*****************************************************************/ 00967000
*   /*                                                               */ 00968000
*   /* FORMAT PGT'S AMD XPT'S FOR MASTER'S MEMORY. FIRST TIME ILRFMTH*/ 00969000
*   /* CALLED FROM ILRFMT00 ONLY.                                    */ 00970000
*   /*                                                               */ 00971000
*   /*****************************************************************/ 00972000
*                                                                  0292 00973000
*   IF ADPLASID=ONE THEN                                           0292 00974000
