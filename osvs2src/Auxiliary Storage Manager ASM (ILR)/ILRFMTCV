         TITLE 'ILRFMTCV    COMMON, HEADER, AND VIO CONTROL-BLOCK FORMA*00001000
               ATTING   '                                               00002000
ILRFMTCV CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ILRFMTCV  76.169'                                 0001 00009000
ILRFMTC  DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00059-ILRFMTC)                                    00013000
         ENTRY ILRFMTC                                                  00014000
ILRFMTH  DS    0H                                                  0001 00015000
         USING *,@15                                               0001 00016000
         B     @PROLOG                                             0001 00017000
         DC    AL2(@EP00278-ILRFMTH)                                    00018000
         ENTRY ILRFMTH                                                  00019000
ILRFMTV  DS    0H                                                  0001 00020000
         USING *,@15                                               0001 00021000
         B     @PROLOG                                             0001 00022000
         DC    AL2(@EP00847-ILRFMTV)                                    00023000
         ENTRY ILRFMTV                                                  00024000
         DROP  @15                                                      00025000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00026000
         BALR  @05,0                                               0001 00027000
@PSTART  LA    @06,4095(,@05)                                      0001 00028000
         LA    @07,4095(,@06)                                      0001 00029000
         USING @PSTART,@05                                         0001 00030000
         USING @PSTART+4095,@06                                    0001 00031000
         USING @PSTART+8190,@07                                    0001 00032000
         ST    @13,@SA00001+4                                      0001 00033000
         LA    @12,@SA00001                                        0001 00034000
         ST    @12,8(,@13)                                         0001 00035000
         LR    @13,@12                                             0001 00036000
         AH    @15,4(,@15)                                         0001 00037000
         BR    @15                                                 0001 00038000
@EP00001 DS    0H                                                  0002 00039000
*                                                                  0059 00040000
*/********************************************************************/ 00041000
*/*ILRFMTC:  CHART (SEQ)                                             */ 00042000
*/*ILRFMTC:  E COMMON SEGMENT                                        */ 00043000
*/*          P SAVE CALLERS REGS, ESTABLISH ADDRESSIBILITY           */ 00044000
*/*          P OBTAIN ABDUMP PARM LIST PTR (R1)                      */ 00045000
*/*          P OBTAIN CVT PTR FORM PARM LIST                         */ 00046000
*/*          L ACCESS: PSASTOR MASTER SGT                            */ 00047000
*/*          D (YES,,NO,%ERROR1) POINTER GOOD ?                      */ 00048000
*/*          L ACCESS:      CVTPVTP OBTAIN PVT PTR                   */ 00049000
*/*          D (YES,,NO,%ERROR2) POINTER GOOD ?                      */ 00050000
*/*          L ACCESS:      PVTLCSA VBN OF FIRST COMMON PAGE         */ 00051000
*/*          D (YES,,NO,%ERROR2) RETURN CODE GOOD ?                  */ 00052000
*/*          P STARTVBN = PVTLCSA                                    */ 00053000
*/*          L ACCESS:      PVTLQSA VBN FIRST QUICK START            */ 00054000
*/*          D (YES,,NO,%ERROR2) RETURN CODE GOOD ?                  */ 00055000
*/*          P CALCULATE NUMBER SEGMENTS, (PVTLQSA - PVTLCSA) / 64   */ 00056000
*/*          S SEGFIND: CALL RTN TO FIND SGTES                       */ 00057000
*/*          P MAINTAIN RETURN CODE TO PASS TO CALLER                */ 00058000
*/*%RETURN:  P RESTORE CALLERS REGS                                  */ 00059000
*/*          R RETURN                                                */ 00060000
*/*%ERROR1:  S (,%RETURN) ERRMSG: R1=10 MASTER SGT INACCESSIBLE      */ 00061000
*/*%ERROR2:  S (,%RETURN) ERRMSG: R1=6 PRINTS PVT INACCESSIBLE       */ 00062000
*/*ILRFMTC:  END                                                     */ 00063000
*/********************************************************************/ 00064000
*                                                                  0059 00065000
*                                                                  0059 00066000
*/********************************************************************/ 00067000
*/*                                                                  */ 00068000
*/* COMMON:  SEGMENT TO CAUSE FORMATTING OF PAGE AND EXTERNAL        */ 00069000
*/*           PAGE TABLES FOR THE COMMON SERVICE AREA                */ 00070000
*/*                                                                  */ 00071000
*/* INPUT:   R1=ABDPLPTR (ABDUMP PARM LIST)                          */ 00072000
*/*                                                                  */ 00073000
*/* PROCESS: (1) GET ADDRESS OF MASTER SEGMENT TABLE FROM PSA        */ 00074000
*/*          (2) GET VBN OF FIRST CSA SEGMENT FROM PVT               */ 00075000
*/*          (3) CALCULATE NUMBER OF CSA SEGMENTS                    */ 00076000
*/*          (4) PASS THIS INFORMATION TO SEGFIND SEGMENT            */ 00077000
*/*                                                                  */ 00078000
*/* OUTPUT:  THE RETURN CODE IN R15 (0 OR 4)                         */ 00079000
*/*                                                                  */ 00080000
*/********************************************************************/ 00081000
*                                                                  0059 00082000
*ILRFMTC:                                                          0059 00083000
*   ENTRY;                          /* FORMAT COMMMON ENTRY          */ 00084000
@EP00059 DS    0H                                                  0060 00085000
*   ABDPLPTR=R1;                    /* GET ADDR OF PARMLIST          */ 00086000
         LR    ABDPLPTR,R1                                         0060 00087000
*   CVTPTR=ADPLCVT;                 /* GET CVT PTR FROM PARM LIST    */ 00088000
         MVC   CVTPTR(4),ADPLCVT(ABDPLPTR)                         0061 00089000
*   DO;                             /* ILRMACCS (SEGPTR)           0062 00090000
*                                      FROM(PSASTOR)                 */ 00091000
*     RFY                                                          0063 00092000
*      (R0,                                                        0063 00093000
*       R1,                                                        0063 00094000
*       R14,                                                       0063 00095000
*       R15) RSTD;                                                 0063 00096000
*     R0=ADDR(PSASTOR);                                            0064 00097000
         LA    R0,PSASTOR                                          0064 00098000
*     R1=ABDPLPTR;                  /* POINT TO PARM LIST            */ 00099000
         LR    R1,ABDPLPTR                                         0065 00100000
*     CALL ACCESS;                  /* ACCESS DATA                   */ 00101000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0066 00102000
         BALR  @14,@15                                             0066 00103000
*     R14=R0;                       /* CANNOT USE R0 AS BASE         */ 00104000
         LR    R14,R0                                              0067 00105000
*     IF R15=GOOD THEN              /* CHECK ACCESS                  */ 00106000
         LTR   R15,R15                                             0068 00107000
         BNZ   @RF00068                                            0068 00108000
*       SEGPTR=CURRENT;             /* GET REQUESTED FIELD           */ 00109000
         MVC   SEGPTR(4),CURRENT(R14)                              0069 00110000
*     ELSE                          /* ACCESS NO GOOD                */ 00111000
*       SEGPTR=ZERO;                /* ZERO FIELD                    */ 00112000
         B     @RC00068                                            0070 00113000
@RF00068 SLR   @11,@11                                             0070 00114000
         ST    @11,SEGPTR                                          0070 00115000
*     RFY                                                          0071 00116000
*      (R0,                                                        0071 00117000
*       R1,                                                        0071 00118000
*       R14,                                                       0071 00119000
*       R15) UNRSTD;                                               0071 00120000
@RC00068 DS    0H                                                  0072 00121000
*   END;                                                           0072 00122000
*   IF SEGPTR^=ZERO THEN                                           0073 00123000
         ICM   @11,15,SEGPTR                                       0073 00124000
         BZ    @RF00073                                            0073 00125000
*     DO;                           /* ACCESS WAS GOOD               */ 00126000
*       DO;                         /* ILRMACCS (PVTPTR)           0075 00127000
*                                      FROM(CVTPVTP)                 */ 00128000
*         RFY                                                      0076 00129000
*          (R0,                                                    0076 00130000
*           R1,                                                    0076 00131000
*           R14,                                                   0076 00132000
*           R15) RSTD;                                             0076 00133000
*         R0=ADDR(CVTPVTP);                                        0077 00134000
         L     @11,CVTPTR                                          0077 00135000
         LA    R0,CVTPVTP(,@11)                                    0077 00136000
*         R1=ABDPLPTR;              /* POINT TO PARM LIST            */ 00137000
         LR    R1,ABDPLPTR                                         0078 00138000
*         CALL ACCESS;              /* ACCESS DATA                   */ 00139000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0079 00140000
         BALR  @14,@15                                             0079 00141000
*         R14=R0;                   /* CANNOT USE R0 AS BASE         */ 00142000
         LR    R14,R0                                              0080 00143000
*         IF R15=GOOD THEN          /* CHECK ACCESS                  */ 00144000
         LTR   R15,R15                                             0081 00145000
         BNZ   @RF00081                                            0081 00146000
*           PVTPTR=CURRENT;         /* GET REQUESTED FIELD           */ 00147000
         MVC   PVTPTR(4),CURRENT(R14)                              0082 00148000
*         ELSE                      /* ACCESS NO GOOD                */ 00149000
*           PVTPTR=ZERO;            /* ZERO FIELD                    */ 00150000
         B     @RC00081                                            0083 00151000
@RF00081 SLR   @11,@11                                             0083 00152000
         ST    @11,PVTPTR                                          0083 00153000
*         RFY                                                      0084 00154000
*          (R0,                                                    0084 00155000
*           R1,                                                    0084 00156000
*           R14,                                                   0084 00157000
*           R15) UNRSTD;                                           0084 00158000
@RC00081 DS    0H                                                  0085 00159000
*       END;                                                       0085 00160000
*       IF PVTPTR^=ZERO THEN                                       0086 00161000
         L     @11,PVTPTR                                          0086 00162000
         LTR   @11,@11                                             0086 00163000
         BZ    @RF00086                                            0086 00164000
*         DO;                       /* ACCESS WAS GOOD               */ 00165000
*           R0=ADDR(PVTLCSA);                                      0088 00166000
         LA    R0,PVTLCSA(,@11)                                    0088 00167000
*           R1=ABDPLPTR;            /* POINT TO PARM LIST            */ 00168000
         LR    R1,ABDPLPTR                                         0089 00169000
*           CALL ACCESS;            /* ACCESS DATA                   */ 00170000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0090 00171000
         BALR  @14,@15                                             0090 00172000
*           R14=R0;                 /* CANNOT USE R0 AS BASE         */ 00173000
         LR    R14,R0                                              0091 00174000
*           IF R15=ZERO THEN                                       0092 00175000
         LTR   R15,R15                                             0092 00176000
         BNZ   @RF00092                                            0092 00177000
*             DO;                   /* ACCESS WAS GOOD               */ 00178000
*               STARTVBN=CURRHALF;  /* GET REQUESTED FIELD HALFWORD  */ 00179000
         SLR   @11,@11                                             0094 00180000
         ICM   @11,3,CURRHALF(R14)                                 0094 00181000
         ST    @11,STARTVBN                                        0094 00182000
*               R0=ADDR(PVTLQSA);                                  0095 00183000
         L     @11,PVTPTR                                          0095 00184000
         LA    R0,PVTLQSA(,@11)                                    0095 00185000
*               R1=ABDPLPTR;        /* POINT TO PARM LIST            */ 00186000
         LR    R1,ABDPLPTR                                         0096 00187000
*               CALL ACCESS;        /* ACCESS DATA                   */ 00188000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0097 00189000
         BALR  @14,@15                                             0097 00190000
*               R14=R0;             /* CANNOT USE R0 AS BASE         */ 00191000
         LR    R14,R0                                              0098 00192000
*               IF R15=ZERO THEN                                   0099 00193000
         LTR   R15,R15                                             0099 00194000
         BNZ   @RF00099                                            0099 00195000
*                 DO;               /* ACCESS WAS GOOD               */ 00196000
*                   HIVBN=CURRHALF; /* GET REQUESTED FIELD HALFWORD  */ 00197000
         SLR   @11,@11                                             0101 00198000
         ICM   @11,3,CURRHALF(R14)                                 0101 00199000
         ST    @11,HIVBN                                           0101 00200000
*                   NUMSEGS=(HIVBN-STARTVBN)/JUSTIFY;/* CLACULATE NO.   00201000
*                                      OF SEGMENTS                   */ 00202000
         LR    @08,@11                                             0102 00203000
         SL    @08,STARTVBN                                        0102 00204000
         SRDA  @08,32                                              0102 00205000
         D     @08,@CF00389                                        0102 00206000
         ST    @09,NUMSEGS                                         0102 00207000
*                   R2=SEGPTR;      /* REAL ADDRESS FOR TRANSLATION  */ 00208000
         L     R2,SEGPTR                                           0103 00209000
*                   CALL TRANSLAT;  /* TRANSLATE REAL TO VIRTUAL     */ 00210000
         BAL   @14,TRANSLAT                                        0104 00211000
*                   IF R15=ZERO&R3^=ZERO THEN                      0105 00212000
         SLR   @11,@11                                             0105 00213000
         CR    R15,@11                                             0105 00214000
         BNE   @RF00105                                            0105 00215000
         CR    R3,@11                                              0105 00216000
         BE    @RF00105                                            0105 00217000
*                     DO;           /* TRANSLATION SUCCESSFUL        */ 00218000
*                       SEGPTR=R3;  /* VIRTUAL SEGMENT TABLE ADDRESS */ 00219000
         ST    R3,SEGPTR                                           0107 00220000
*                       CUMRC=ZERO; /* CLEAR CUMULATIVE RETURN CODE  */ 00221000
         ST    @11,CUMRC                                           0108 00222000
*                       BUFFER(1:49)=                              0109 00223000
*'SYSTEM COMMON AREA PAGE AND EXTERNAL PAGE TABLES';               0109 00224000
         L     @11,ADPLBUF(,ABDPLPTR)                              0109 00225000
         MVI   BUFFER+48(@11),C' '                                 0109 00226000
         MVC   BUFFER(48,@11),@CC01927                             0109 00227000
*                       R1=ABDPLPTR;/* PTR TO INPUT PARM LIST        */ 00228000
         LR    R1,ABDPLPTR                                         0110 00229000
*                       CALL PRINT; /* PRINT ONE LINE                */ 00230000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0111 00231000
         BALR  @14,@15                                             0111 00232000
*                       CALL PRINT; /* SPACE ONE LINE                */ 00233000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0112 00234000
         BALR  @14,@15                                             0112 00235000
*                       CALL SEGFIND;/* OBTAINS ADDRESS EACH SEGMENT    00236000
*                                      ENTRY                         */ 00237000
         BAL   @14,SEGFIND                                         0113 00238000
*                       R15=CUMRC;  /* GET RETURN CODE               */ 00239000
         L     R15,CUMRC                                           0114 00240000
*                     END;          /* TRANSLATION SUCCESSFUL        */ 00241000
*                   ELSE                                           0116 00242000
*                     DO;           /* TRANSLATION FAILED            */ 00243000
         B     @RC00105                                            0116 00244000
@RF00105 DS    0H                                                  0117 00245000
*                       R1=SGTERR;  /* MASTER SGT ERROR MESSAGE      */ 00246000
         LA    R1,10                                               0117 00247000
*                       CALL ILRFMSG;/* PRINT MESSAGE                */ 00248000
         L     @15,@CV01769                                        0118 00249000
         BALR  @14,@15                                             0118 00250000
*                       R15=ERROR;  /* SET ERROR RETURN CODE         */ 00251000
         LA    R15,4                                               0119 00252000
*                     END;                                         0120 00253000
*                 END;                                             0121 00254000
*               ELSE                                               0122 00255000
*                 DO;               /* ACCESS FAILED                 */ 00256000
         B     @RC00099                                            0122 00257000
@RF00099 DS    0H                                                  0123 00258000
*                   R1=PVTERR;      /* PVT ERROR MESSAGE             */ 00259000
         LA    R1,6                                                0123 00260000
*                   CALL ILRFMSG;   /* PRINT MESSAGE                 */ 00261000
         L     @15,@CV01769                                        0124 00262000
         BALR  @14,@15                                             0124 00263000
*                   R15=ERROR;      /* SET ERROR RETURN CODE         */ 00264000
         LA    R15,4                                               0125 00265000
*                 END;                                             0126 00266000
*             END;                                                 0127 00267000
*           ELSE                                                   0128 00268000
*             DO;                   /* ACCESS FAILED                 */ 00269000
         B     @RC00092                                            0128 00270000
@RF00092 DS    0H                                                  0129 00271000
*               R1=PVTERR;          /* PVT ERROR MESSAGE             */ 00272000
         LA    R1,6                                                0129 00273000
*               CALL ILRFMSG;       /* PRINT MESSAGE                 */ 00274000
         L     @15,@CV01769                                        0130 00275000
         BALR  @14,@15                                             0130 00276000
*               R15=ERROR;          /* SET ERROR RETURN CODE         */ 00277000
         LA    R15,4                                               0131 00278000
*             END;                                                 0132 00279000
*         END;                                                     0133 00280000
*       ELSE                                                       0134 00281000
*         DO;                       /* ACCESS FAILED                 */ 00282000
         B     @RC00086                                            0134 00283000
@RF00086 DS    0H                                                  0135 00284000
*           R1=PVTERR;              /* PVT ERROR MESSAGE             */ 00285000
         LA    R1,6                                                0135 00286000
*           CALL ILRFMSG;           /* PRINT MESSAGE                 */ 00287000
         L     @15,@CV01769                                        0136 00288000
         BALR  @14,@15                                             0136 00289000
*           R15=ERROR;              /* SET ERROR RETURN CODE         */ 00290000
         LA    R15,4                                               0137 00291000
*         END;                                                     0138 00292000
*     END;                                                         0139 00293000
*   ELSE                                                           0140 00294000
*     DO;                           /* ACCESS FAILED                 */ 00295000
         B     @RC00073                                            0140 00296000
@RF00073 DS    0H                                                  0141 00297000
*       R1=SGTERR;                  /* MASTER SGT ERROR MESSAGE      */ 00298000
         LA    R1,10                                               0141 00299000
*       CALL ILRFMSG;               /* PRINT MESSAGE                 */ 00300000
         L     @15,@CV01769                                        0142 00301000
         BALR  @14,@15                                             0142 00302000
*       R15=ERROR;                  /* SET ERROR RETURN CODE         */ 00303000
         LA    R15,4                                               0143 00304000
*     END;                                                         0144 00305000
*FMTCEND:                                                          0145 00306000
*   RETURN CODE(R15);               /* RETURNS TO FMT00, ENDS COMMON */ 00307000
@RC00073 DS    0H                                                  0145 00308000
FMTCEND  L     @13,4(,@13)                                         0145 00309000
         L     @14,12(,@13)                                        0145 00310000
         LM    @00,@12,20(@13)                                     0145 00311000
         BR    @14                                                 0145 00312000
*                                                                  0146 00313000
*/********************************************************************/ 00314000
*/*SEGFIND:  CHART (SEQ)                                             */ 00315000
*/*          E SEGFIND                                               */ 00316000
*/*          P SAVE RETURN ADDRESS                                   */ 00317000
*/*          P CLEAR CUMULATIVE RETURN CODE                          */ 00318000
*/*          P SET COUNTER = STARTVBN                                */ 00319000
*/*%SEGS:    D (NO,,YES,%RETURN) ALL SGTES DONE? (I = NUMSEGS)       */ 00320000
*/*          P INCREMENT COUNTER                                     */ 00321000
*/*          L ACCESS: GET SEGMENT ENTRY VIA COUNTER AND PSASTOR     */ 00322000
*/*          D (YES,,NO,%ERRET) ACCESS GOOD ?                        */ 00323000
*/*          D (YES,,NO,%INCR) SEGMENT VALID ? (SGTPAM)              */ 00324000
*/*          P R2 = SGTPTO, REAL ADDR PAGE TABLE ORIGIN              */ 00325000
*/*          S TRANSLAT: CALL SEGMENT TO TRANSLATE REAL TO VIRTUAL   */ 00326000
*/*          D (YES,,NO,%ERRET) RETURN CODE GOOD ?                   */ 00327000
*/*          P PGTPTR = R3 VIRTUAL ADDRESS OF PGT                    */ 00328000
*/*          S FMTPGXPT: CALL RTN TO FORMAT PGT/XPT'S                */ 00329000
*/*%ERRET:   P OR IN RETURN CODE                                     */ 00330000
*/*%INCR:    P (,%SEGS) INCR I                                       */ 00331000
*/*%RETURN:  P RESTORE RETURN ADDRESS                                */ 00332000
*/*          R RETURN                                                */ 00333000
*/*SEGFIND:  END                                                     */ 00334000
*/********************************************************************/ 00335000
*                                                                  0146 00336000
*                                                                  0146 00337000
*/********************************************************************/ 00338000
*/*                                                                  */ 00339000
*/* SEGFIND: LOCATES SEGMENTS                                        */ 00340000
*/*                                                                  */ 00341000
*/* INPUT:   STARTVBN = VIRTUAL BLOCK NUMBER OF FIRST SEGMENT        */ 00342000
*/*                      TO BE LOCATED                               */ 00343000
*/*          NUMSEGS  = THE NUMBER OF SEGMENTS TO BE LOCATED         */ 00344000
*/*          SEGPTR   = ADDRESS OF THE SEGMENT TABLE                 */ 00345000
*/*                                                                  */ 00346000
*/* PROCESS: GET SEGMENT, CHECK ITS VALIDITY, IF VALID, CALL         */ 00347000
*/*           SUBROUTINE TO FORMAT THE PAGE TABLES FOR THAT SEGMENT  */ 00348000
*/*           FOLLOW THIS PROCEDURE UNTIL ALL SEGS REQUESTED HAVE    */ 00349000
*/*           PROCESSED.                                             */ 00350000
*/*                                                                  */ 00351000
*/* OUTPUT:  THE CUMULATIVE RETURN CODE                              */ 00352000
*/*                                                                  */ 00353000
*/********************************************************************/ 00354000
*                                                                  0146 00355000
*SEGFIND:                                                          0146 00356000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0146 00357000
SEGFIND  DS    0H                                                  0147 00358000
*   RTNADDR1=R14;                   /* SAVE RETURN ADDRESS           */ 00359000
         ST    R14,RTNADDR1                                        0147 00360000
*   CTR=STARTVBN/JUSTIFY;           /* SET CTR TO USE AS INDEX       */ 00361000
         L     @08,STARTVBN                                        0148 00362000
         SRDA  @08,32                                              0148 00363000
         D     @08,@CF00389                                        0148 00364000
         ST    @09,CTR                                             0148 00365000
*   STEPTR=SEGPTR+(CTR*LENGTH(SGTSTE));/* OBTAIN ADDR FIRST ENTRY    */ 00366000
         SLA   @09,2                                               0149 00367000
         AL    @09,SEGPTR                                          0149 00368000
         ST    @09,STEPTR                                          0149 00369000
*   DO I=1 TO NUMSEGS;              /* DO UNTIL ALL CSA SEGMENTS ARE    00370000
*                                      PROCESSED                     */ 00371000
         LA    @11,1                                               0150 00372000
         B     @DE00150                                            0150 00373000
@DL00150 DS    0H                                                  0151 00374000
*     DO;                           /* ILRMACCS (SENTRY) FROM(SGTSTE)*/ 00375000
*       RFY                                                        0152 00376000
*        (R0,                                                      0152 00377000
*         R1,                                                      0152 00378000
*         R14,                                                     0152 00379000
*         R15) RSTD;                                               0152 00380000
*       R0=ADDR(SGTSTE);                                           0153 00381000
         L     R0,STEPTR                                           0153 00382000
*       R1=ABDPLPTR;                /* POINT TO PARM LIST            */ 00383000
         LR    R1,ABDPLPTR                                         0154 00384000
*       CALL ACCESS;                /* ACCESS DATA                   */ 00385000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0155 00386000
         BALR  @14,@15                                             0155 00387000
*       R14=R0;                     /* CANNOT USE R0 AS BASE         */ 00388000
         LR    R14,R0                                              0156 00389000
*       IF R15=GOOD THEN            /* CHECK ACCESS                  */ 00390000
         LTR   R15,R15                                             0157 00391000
         BNZ   @RF00157                                            0157 00392000
*         SENTRY=CURRENT;           /* GET REQUESTED FIELD           */ 00393000
         MVC   SENTRY(4),CURRENT(R14)                              0158 00394000
*       ELSE                        /* ACCESS NO GOOD                */ 00395000
*         SENTRY=ZERO;              /* ZERO FIELD                    */ 00396000
         B     @RC00157                                            0159 00397000
@RF00157 SLR   @11,@11                                             0159 00398000
         ST    @11,SENTRY                                          0159 00399000
*       RFY                                                        0160 00400000
*        (R0,                                                      0160 00401000
*         R1,                                                      0160 00402000
*         R14,                                                     0160 00403000
*         R15) UNRSTD;                                             0160 00404000
@RC00157 DS    0H                                                  0161 00405000
*     END;                                                         0161 00406000
*     IF R15=ZERO THEN              /* ACCESS SUCCEEDED              */ 00407000
         LTR   R15,R15                                             0162 00408000
         BNZ   @RF00162                                            0162 00409000
*       DO;                                                        0163 00410000
*         RFY                                                      0164 00411000
*           SGTSTE BASED(ADDR(SENTRY));                            0164 00412000
*         IF SGTPAM=OFF THEN        /* SEGMENT IS VALID              */ 00413000
         LA    @11,SENTRY                                          0165 00414000
         TM    SGTPAM(@11),B'00000001'                             0165 00415000
         BNZ   @RF00165                                            0165 00416000
*           DO;                                                    0166 00417000
*             R2=SGTPTO;            /* REAL ADDRESS FOR TRANSLATION     00418000
*                                      PLUS 3 FLAG BITS              */ 00419000
         SLR   R2,R2                                               0167 00420000
         ICM   R2,7,SGTPTO(@11)                                    0167 00421000
*             RFY                                                  0168 00422000
*               SGTSTE BASED(STEPTR);                              0168 00423000
*             SRL(R2,3);            /* TO ZERO THE                   */ 00424000
         SRL   R2,3                                                0169 00425000
*             SLL(R2,3);            /* THREE FLAG BITS               */ 00426000
         SLL   R2,3                                                0170 00427000
*             CALL TRANSLAT;        /* CALL SEGMENT TO TRANSLATE REAL   00428000
*                                      TO VIRTUAL ADDR               */ 00429000
         BAL   @14,TRANSLAT                                        0171 00430000
*             IF R15=ZERO&R3^=ZERO THEN                            0172 00431000
         SLR   @11,@11                                             0172 00432000
         CR    R15,@11                                             0172 00433000
         BNE   @RF00172                                            0172 00434000
         CR    R3,@11                                              0172 00435000
         BE    @RF00172                                            0172 00436000
*               DO;                 /* TRANSLATION WAS SUCCESSFUL    */ 00437000
*                 PGTPTR=R3;        /* GET VIRTUAL ADDRESS           */ 00438000
         ST    R3,PGTPTR                                           0174 00439000
*                 CALL FMTPGXPT;    /* CALL SEGMENT TO FMT PGTES     */ 00440000
         BAL   @14,FMTPGXPT                                        0175 00441000
*               END;                                               0176 00442000
*             ELSE                                                 0177 00443000
*               CUMRC=CUMRC|ERROR;  /* TRANSLAT FAILED               */ 00444000
         B     @RC00172                                            0177 00445000
@RF00172 L     @11,CUMRC                                           0177 00446000
         O     @11,@CF00064                                        0177 00447000
         ST    @11,CUMRC                                           0177 00448000
*           END;                                                   0178 00449000
*       END;                        /* END SEGMENT PROCESSING        */ 00450000
*     ELSE                                                         0180 00451000
*       DO;                         /* ACCESS FAILED                 */ 00452000
         B     @RC00162                                            0180 00453000
@RF00162 DS    0H                                                  0181 00454000
*         CUMRC=CUMRC|ERROR;        /* OR IN ERROR                   */ 00455000
         L     @11,CUMRC                                           0181 00456000
         O     @11,@CF00064                                        0181 00457000
         ST    @11,CUMRC                                           0181 00458000
*         I=NUMSEGS;                /* FORCE END OF LOOP             */ 00459000
         MVC   I(4),NUMSEGS                                        0182 00460000
*       END;                                                       0183 00461000
*     STEPTR=STEPTR+LENGTH(SGTSTE); /* ADDR NEXT ENTRY               */ 00462000
@RC00162 LA    @11,4                                               0184 00463000
         AL    @11,STEPTR                                          0184 00464000
         ST    @11,STEPTR                                          0184 00465000
*     CTR=CTR+ONE;                  /* INCR VBN SS                   */ 00466000
         LA    @11,1                                               0185 00467000
         AL    @11,CTR                                             0185 00468000
         ST    @11,CTR                                             0185 00469000
*   END;                            /* END 1 TO NUMSEGS              */ 00470000
         LA    @11,1                                               0186 00471000
         AL    @11,I                                               0186 00472000
@DE00150 ST    @11,I                                               0186 00473000
         C     @11,NUMSEGS                                         0186 00474000
         BNH   @DL00150                                            0186 00475000
*   R14=RTNADDR1;                   /* RESTORE RETURN ADDRESS        */ 00476000
         L     R14,RTNADDR1                                        0187 00477000
*   END SEGFIND;                    /* RETURN TO CALLER              */ 00478000
@EL00002 DS    0H                                                  0188 00479000
@EF00002 DS    0H                                                  0188 00480000
@ER00002 BR    @14                                                 0188 00481000
*                                                                  0189 00482000
*/********************************************************************/ 00483000
*/*TRANSLAT:  CHART (SEQ)                                            */ 00484000
*/*TRANSLAT:  E TRANSLAT                                             */ 00485000
*/*           P SAVE RETURN ADDRESS                                  */ 00486000
*/*           L ACCESS:      PVTPFTP  PAGE FRAME TABLE               */ 00487000
*/*           D (YES,,NO,%ERRET) POINTER GOOD ?                      */ 00488000
*/*           P R3 = R2 - SAVE REAL ADDR                             */ 00489000
*/*           P SRL R2,12 - CHOP OFF DISPLACEMENT                    */ 00490000
*/*           P SLL R2,4 - LEFT JUSTIFY TO HALF WORD (OFFSET IN PFT) */ 00491000
*/*           P PFTEPTR = PFTPTR + R2                                */ 00492000
*/*           P ACCESS: PFTVBN FROM PFTEPTR                          */ 00493000
*/*           D (YES,,NO,%ERRET) ACCESS GOOD ?                       */ 00494000
*/*           P R2 = PFTVBN                                          */ 00495000
*/*           P SRL R2,4 - RIGHT JUSTIFY VBN                         */ 00496000
*/*           P SLL R3,20 - MOVE DISPLACEMENT NEXT TO VBN            */ 00497000
*/*           P SRDL R2,20 - GET VIRTUAL ADDRESS IN R3               */ 00498000
*/*%RETURN:   P RESTORE RETURN ADDRESS                               */ 00499000
*/*           R RETURN                                               */ 00500000
*/*%ERRET:    P (,%RETURN) MAINTAIN ERROR RETURN CODE TO PASS CALLER */ 00501000
*/*TRANSLAT:  END                                                    */ 00502000
*/********************************************************************/ 00503000
*                                                                  0189 00504000
*                                                                  0189 00505000
*/********************************************************************/ 00506000
*/*                                                                  */ 00507000
*/* TRANSLAT: SEGMENT TO TRANSLATE REAL ADDRESS TO VIRTUAL ADDRESS   */ 00508000
*/*                                                                  */ 00509000
*/* INPUT:    REAL ADDRESS IN REGISTER 2, PVTPTR                     */ 00510000
*/*                                                                  */ 00511000
*/* PROCESS:  ISOLATE SSP PASSED FROM CALLER,  GET CORRESPONDING     */ 00512000
*/*            VBN FROM PFTE, SHIFT UNTIL VIRTUAL ADDRESS IS IN      */ 00513000
*/*            REGISTER 3.                                           */ 00514000
*/*                                                                  */ 00515000
*/* OUTPUT:   VIRTUAL ADDRESS IN REGISTER 3                          */ 00516000
*/*                                                                  */ 00517000
*/********************************************************************/ 00518000
*                                                                  0189 00519000
*TRANSLAT:                                                         0189 00520000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0189 00521000
TRANSLAT DS    0H                                                  0190 00522000
*   RTNADDR2=R14;                   /* SAVE RETURN ADDRESS           */ 00523000
         ST    R14,RTNADDR2                                        0190 00524000
*   DO;                             /* ILRMACCS (PFTPTR)           0191 00525000
*                                      FROM(PVTPFTP)                 */ 00526000
*     RFY                                                          0192 00527000
*      (R0,                                                        0192 00528000
*       R1,                                                        0192 00529000
*       R14,                                                       0192 00530000
*       R15) RSTD;                                                 0192 00531000
*     R0=ADDR(PVTPFTP);                                            0193 00532000
         L     @11,PVTPTR                                          0193 00533000
         LA    R0,PVTPFTP(,@11)                                    0193 00534000
*     R1=ABDPLPTR;                  /* POINT TO PARM LIST            */ 00535000
         LR    R1,ABDPLPTR                                         0194 00536000
*     CALL ACCESS;                  /* ACCESS DATA                   */ 00537000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0195 00538000
         BALR  @14,@15                                             0195 00539000
*     R14=R0;                       /* CANNOT USE R0 AS BASE         */ 00540000
         LR    R14,R0                                              0196 00541000
*     IF R15=GOOD THEN              /* CHECK ACCESS                  */ 00542000
         LTR   R15,R15                                             0197 00543000
         BNZ   @RF00197                                            0197 00544000
*       PFTPTR=CURRENT;             /* GET REQUESTED FIELD           */ 00545000
         MVC   PFTPTR(4),CURRENT(R14)                              0198 00546000
*     ELSE                          /* ACCESS NO GOOD                */ 00547000
*       PFTPTR=ZERO;                /* ZERO FIELD                    */ 00548000
         B     @RC00197                                            0199 00549000
@RF00197 SLR   @11,@11                                             0199 00550000
         ST    @11,PFTPTR                                          0199 00551000
*     RFY                                                          0200 00552000
*      (R0,                                                        0200 00553000
*       R1,                                                        0200 00554000
*       R14,                                                       0200 00555000
*       R15) UNRSTD;                                               0200 00556000
@RC00197 DS    0H                                                  0201 00557000
*   END;                                                           0201 00558000
*   IF PFTPTR^=ZERO THEN            /* ACCESS WAS GOOD               */ 00559000
         L     @11,PFTPTR                                          0202 00560000
         LTR   @11,@11                                             0202 00561000
         BZ    @RF00202                                            0202 00562000
*     DO;                           /* CONVERT REAL ADDRESS TO     0203 00563000
*                                      VIRTUAL                       */ 00564000
*       R3=R2;                      /* SAVE REAL ADDRESS             */ 00565000
         LR    R3,R2                                               0204 00566000
*       SLL(R2,8);                  /* REMOVE HIGH ORDER BYTE        */ 00567000
         SLL   R2,8                                                0205 00568000
*       SRL(R2,20);                 /* CHOP OFF DISPLACEMENT         */ 00569000
         SRL   R2,20                                               0206 00570000
*       SLL(R2,4);                  /* LEFT JUSTIFY TO HALFWORD      */ 00571000
         SLL   R2,4                                                0207 00572000
*       PFTEPTR=PFTPTR+R2;          /* CALCULATE PFT ENTRY FOR THIS     00573000
*                                      ADDR                          */ 00574000
         ALR   @11,R2                                              0208 00575000
         ST    @11,PFTEPTR                                         0208 00576000
*       R0=ADDR(PFTVBN);                                           0209 00577000
         LA    R0,PFTVBN(,@11)                                     0209 00578000
*       R1=ABDPLPTR;                /* POINT TO PARM LIST            */ 00579000
         LR    R1,ABDPLPTR                                         0210 00580000
*       CALL ACCESS;                /* ACCESS DATA                   */ 00581000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0211 00582000
         BALR  @14,@15                                             0211 00583000
*       R14=R0;                     /* CANNOT USE R0 AS BASE         */ 00584000
         LR    R14,R0                                              0212 00585000
*       IF R15=ZERO THEN            /* ACCESS WAS GOOD               */ 00586000
         LTR   R15,R15                                             0213 00587000
         BNZ   @RF00213                                            0213 00588000
*         DO;                       /* COMPLETE REAL ADDR CONVERSION */ 00589000
*           VBN=CURRHALF;           /* GET REQUESTED FIELD           */ 00590000
         SLR   @11,@11                                             0215 00591000
         ICM   @11,3,CURRHALF(R14)                                 0215 00592000
         STH   @11,VBN                                             0215 00593000
*           R2=VBN;                 /* PUT VBN IN A REGISTER         */ 00594000
         LR    R2,@11                                              0216 00595000
*           SRL(R2,4);              /* RIGHT JUSTIFY VBN             */ 00596000
         SRL   R2,4                                                0217 00597000
*           SLL(R3,20);             /* MOVE DISPLACEMENT NEXT TO VBN */ 00598000
         SLL   R3,20                                               0218 00599000
*           SRDL(R2,20);            /* GET VIRTUAL ADDR IN REG3      */ 00600000
         SRDL  R2,20                                               0219 00601000
*         END;                      /* END SHIFT OF VBN              */ 00602000
*     END;                          /* END CONVERSION OF REAL ADDRESS*/ 00603000
*   ELSE                                                           0222 00604000
*     R15=ERROR;                    /* ACCESS FAILED                 */ 00605000
         B     @RC00202                                            0222 00606000
@RF00202 LA    R15,4                                               0222 00607000
*   R14=RTNADDR2;                   /* RESTORE RETURN ADDRESS        */ 00608000
@RC00202 L     R14,RTNADDR2                                        0223 00609000
*   END TRANSLAT;                   /* RETURN TO CALLER              */ 00610000
@EL00003 DS    0H                                                  0224 00611000
@EF00003 DS    0H                                                  0224 00612000
@ER00003 BR    @14                                                 0224 00613000
*                                                                  0225 00614000
*/********************************************************************/ 00615000
*/*FMTPGXPT:  CHART (SEQ)                                            */ 00616000
*/*FMTPGXPT:  E FMTPGXPT                                             */ 00617000
*/*           P SAVE RETURN ADDRESS                                  */ 00618000
*/*           L LINE - FORMAT PATTERNS                               */ 00619000
*/*           P SET UP HEADER                                        */ 00620000
*/*           P PRINT HEADER                                         */ 00621000
*/*           P CALCULATE XPTPTR FROM PGTPTR                         */ 00622000
*/*           N PROCESS 16 PGTES/XPTES                               */ 00623000
*/*%PAGE:     D (NO,,YES,%RETURN) ALL ENTRES DONE (I=16) ?           */ 00624000
*/*           P PUT PAGE TABLE ENTRY INTO LINE                       */ 00625000
*/*           P PUT EXTERNAL PAGE TABLE INTO LINE                    */ 00626000
*/*           L FORMAT: FORMAT AND PRINT LINE                        */ 00627000
*/*%ERROR2:   P OR IN RETURN CODE                                    */ 00628000
*/*           P CALCULATE ADDR NEXT XPTE                             */ 00629000
*/*           P CALCULATE ADDR NEXT PGTE                             */ 00630000
*/*           P (,%PAGE) INCR I                                      */ 00631000
*/*%RETURN:   P RESTORE RETURN ADDRESS                               */ 00632000
*/*           R RETURN                                               */ 00633000
*/********************************************************************/ 00634000
*                                                                  0225 00635000
*                                                                  0225 00636000
*/********************************************************************/ 00637000
*/*                                                                  */ 00638000
*/* FMTPGXPT: SEGMENT TO PRINT PAGE AND EXTERNAL PAGE TABLES         */ 00639000
*/*                                                                  */ 00640000
*/* INPUT:    VIRTUAL ADDRESS IN PGT, CTR IS SS PART OF A VBN        */ 00641000
*/*                                                                  */ 00642000
*/* PROCESS:  PUT PAGE TABLE DATA INTO OUTPUT LINE.  CALCULATE       */ 00643000
*/*            ADDRESS OF ASSOCIATED XPT AND PUT XPT DATA INTO       */ 00644000
*/*            OUTPUT LINE. FORMAT AND PRINT THE OUTPUT LINE.        */ 00645000
*/*            REPEAT THIS PROCESS UNTIL 16 PAGE TABLES HAVE BEEN    */ 00646000
*/*            FORMATTED.                                            */ 00647000
*/*                                                                  */ 00648000
*/* OUTPUT:   THE CUMULATIVE RETURN CODE                             */ 00649000
*/*                                                                  */ 00650000
*/********************************************************************/ 00651000
*                                                                  0225 00652000
*FMTPGXPT:                                                         0225 00653000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0225 00654000
FMTPGXPT DS    0H                                                  0226 00655000
*                                                                  0226 00656000
*   /*****************************************************************/ 00657000
*   /*                                                               */ 00658000
*   /* XPT1:ILRMLINE (PGTE,XPTE,(LGN,NOLABEL),(LPN,NOLABEL)) OF(XPTE)*/ 00659000
*   /* LABEL VARIABLES REFERENCED IN FORMAT PATTERNS                 */ 00660000
*   /*                                                               */ 00661000
*   /*****************************************************************/ 00662000
*                                                                  0226 00663000
*   DCL                                                            0226 00664000
*     XPT1LB1 CHAR(LENGTH('PGTE')) INIT('PGTE');                   0226 00665000
*   DCL                                                            0227 00666000
*     XPT1LB2 CHAR(LENGTH('XPTE')) INIT('XPTE');                   0227 00667000
*                                                                  0227 00668000
*   /*****************************************************************/ 00669000
*   /*                                                               */ 00670000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 00671000
*   /*                                                               */ 00672000
*   /*****************************************************************/ 00673000
*                                                                  0228 00674000
*   DCL                                                            0228 00675000
*     1 XPT1PTS BDY(WORD),                                         0228 00676000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00677000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00678000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(XPTPGTE)-1+16*LENGTH('PGTE') 00679000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 00680000
*       3 * FIXED(8) INIT(5),                                      0228 00681000
*       3 * FIXED(8) INIT(11),                                     0228 00682000
*       3 * PTR(31) INIT(ADDR(XPT1LB1)),/* SEE PTTLBPTR              */ 00683000
*       3 XPT1DT1 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 00684000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00685000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00686000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(XPTXPTE)-1+16*LENGTH('XPTE') 00687000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 00688000
*       3 * FIXED(8) INIT(15+8),                                   0228 00689000
*       3 * FIXED(8) INIT(21+8),                                   0228 00690000
*       3 * PTR(31) INIT(ADDR(XPT1LB2)),/* SEE PTTLBPTR              */ 00691000
*       3 XPT1DT2 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 00692000
*      2 * CHAR(8) BDY(WORD),       /* FORMAT PATTERN SEE PTTRNS     */ 00693000
*       3 * BIT(8) BDY(BYTE) INIT('00001100'B),/* SEE PTTFLGS        */ 00694000
*       3 * FIXED(8) BDY(BYTE) INIT(16*0+LENGTH(XPTLGN)-1),        0228 00695000
*                                                                  0228 00696000
*     /***************************************************************/ 00697000
*     /*                                                             */ 00698000
*     /* SEE PTTLBLGH                                                */ 00699000
*     /*                                                             */ 00700000
*     /***************************************************************/ 00701000
*                                                                  0228 00702000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 00703000
*       3 * FIXED(8) INIT(25+8+8),                                 0228 00704000
*       3 XPT1DT3 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 00705000
*      2 * CHAR(8) BDY(WORD),       /* FORMAT PATTERN SEE PTTRNS     */ 00706000
*       3 * BIT(8) BDY(BYTE) INIT('00001100'B),/* SEE PTTFLGS        */ 00707000
*       3 * FIXED(8) BDY(BYTE) INIT(16*0+LENGTH(XPTLPN)-1),        0228 00708000
*                                                                  0228 00709000
*     /***************************************************************/ 00710000
*     /*                                                             */ 00711000
*     /* SEE PTTLBLGH                                                */ 00712000
*     /*                                                             */ 00713000
*     /***************************************************************/ 00714000
*                                                                  0228 00715000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 00716000
*       3 * FIXED(8) INIT(29+8+8+8),                               0228 00717000
*       3 XPT1DT4 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 00718000
*      2 * FIXED(31) INIT(0);       /* END OF FORMAT PATTERNS        */ 00719000
*                                                                  0229 00720000
*   /*****************************************************************/ 00721000
*   /*                                                               */ 00722000
*   /* START OF EXECUTABLE CODE OF FMTPGXPT ROUTINE                  */ 00723000
*   /*                                                               */ 00724000
*   /*****************************************************************/ 00725000
*                                                                  0229 00726000
*   RTNADDR2=R14;                   /* SAVE RETURN ADDRESS           */ 00727000
         ST    R14,RTNADDR2                                        0229 00728000
*   ANCHTXT='FOR        ';                                         0230 00729000
         L     @11,ADPLBUF(,ABDPLPTR)                              0230 00730000
         MVC   ANCHTXT(11,@11),@CC01987                            0230 00731000
*   R0=ADDR(PGTPTR);                /* PTR TO BE FORMATTED           */ 00732000
         LA    R0,PGTPTR                                           0231 00733000
*   R1=ADDR(PGTNAM);                /* LABEL FOR POINTER             */ 00734000
         LA    R1,PGTNAM                                           0232 00735000
*   CALL ILRFPRTH;                  /* FORMAT AND PRINT HEADING LINE */ 00736000
         L     @15,@CV01768                                        0233 00737000
         BALR  @14,@15                                             0233 00738000
*   VSA=CTR*65536;                  /* OBTAIN VSA FROM VBN SS        */ 00739000
         L     @11,CTR                                             0234 00740000
         SLA   @11,16                                              0234 00741000
         ST    @11,VSA                                             0234 00742000
*   R0=PGTPTR;                      /* INPUT TO ACCESS       @ZA11398*/ 00743000
         L     R0,PGTPTR                                           0235 00744000
*   R1=ABDPLPTR;                    /* INPUT TO ACCESS       @ZA11398*/ 00745000
         LR    R1,ABDPLPTR                                         0236 00746000
*   CALL ACCESS;                    /* ACCESS PGTPTR         @ZA11398*/ 00747000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0237 00748000
         BALR  @14,@15                                             0237 00749000
*   R0=ADDR(VSA);                   /* PTR TO BE FORMATTED           */ 00750000
         LA    R0,VSA                                              0238 00751000
*   R1=ADDR(VSANAM);                /* LABEL FOR POINTER             */ 00752000
         LA    R1,VSANAM                                           0239 00753000
*   IF R15^=ZERO THEN                                              0240 00754000
         LTR   R15,R15                                             0240 00755000
         BZ    @RF00240                                            0240 00756000
*     DO;                           /* NO FORMATTING?        @ZA11398*/ 00757000
*       BUFFER(18:29)='INACCESSIBLE';/* MSG CHANGE           @ZA11398*/ 00758000
         L     @11,ADPLBUF(,ABDPLPTR)                              0242 00759000
         MVC   BUFFER+17(12,@11),@CC01991                          0242 00760000
*       CALL ILRFPRTH;              /* PRINT HEADING         @ZA11398*/ 00761000
         L     @15,@CV01768                                        0243 00762000
         BALR  @14,@15                                             0243 00763000
*     END;                          /* NO FORMATTING         @ZA11398*/ 00764000
*   ELSE                                                           0245 00765000
*     DO;                           /* FORMAT PGT/XPT        @ZA11398*/ 00766000
         B     @RC00240                                            0245 00767000
@RF00240 DS    0H                                                  0246 00768000
*       CALL ILRFPRTH;              /* FORMAT AND PRINT SECOND     0246 00769000
*                                      HEADING LINE                  */ 00770000
         L     @15,@CV01768                                        0246 00771000
         BALR  @14,@15                                             0246 00772000
*       PTEPTR=PGTPTR;              /* OBTAIN ADDR FIRST PAGE TABLE     00773000
*                                      ENTRY                         */ 00774000
         L     @11,PGTPTR                                          0247 00775000
         ST    @11,PTEPTR                                          0247 00776000
*       XPTEPTR=PTEPTR+16*LENGTH(PGTPTE);/* CALCULATE FIRST XPTE   0248 00777000
*                                      ADDRESS                       */ 00778000
         AH    @11,@CH00090                                        0248 00779000
         ST    @11,XPTEPTR                                         0248 00780000
*       R15=ZERO;                   /* CLEAR RETURN CODE             */ 00781000
         SLR   R15,R15                                             0249 00782000
*       DO J=1 TO 16 WHILE R15=ZERO;/* PROCESS 16 PGTES/XPTES        */ 00783000
         LA    @11,1                                               0250 00784000
         ST    @11,J                                               0250 00785000
@DL00250 SLR   @11,@11                                             0250 00786000
         CR    R15,@11                                             0250 00787000
         BNE   @DC00250                                            0250 00788000
*         DO;                       /* ILRMFRMT (XPT) LINES(XPT1)    */ 00789000
*           FORMRC=0;               /* ZERO THE RETURN CODE          */ 00790000
         ST    @11,FORMRC                                          0252 00791000
*           XPT1DT1=ADDR(XPTPGTE);                                 0253 00792000
         MVC   XPT1DT1(4),PTEPTR                                   0253 00793000
*           XPT1DT2=ADDR(XPTXPTE);                                 0254 00794000
         L     @11,XPTEPTR                                         0254 00795000
         ST    @11,XPT1DT2                                         0254 00796000
*           XPT1DT3=ADDR(XPTLGN);                                  0255 00797000
         LA    @10,XPTLGN(,@11)                                    0255 00798000
         ST    @10,XPT1DT3                                         0255 00799000
*           XPT1DT4=ADDR(XPTLPN);                                  0256 00800000
         LA    @11,XPTLPN(,@11)                                    0256 00801000
         ST    @11,XPT1DT4                                         0256 00802000
*           RFY                                                    0257 00803000
*            (R0,                                                  0257 00804000
*             R1) RSTD;                                            0257 00805000
*           R0=ADDR(XPT1PTS);                                      0258 00806000
         LA    R0,XPT1PTS                                          0258 00807000
*           R1=ABDPLPTR;            /* GET PARM ADDR                 */ 00808000
         LR    R1,ABDPLPTR                                         0259 00809000
*           CALL FORMAT;            /* FORMAT THE LINE               */ 00810000
         L     @15,ADPLFRMT(,ABDPLPTR)                             0260 00811000
         BALR  @14,@15                                             0260 00812000
*           RFY                                                    0261 00813000
*             R15 RSTD;                                            0261 00814000
*           FORMRC=FORMRC|R15;      /* SAVE RETURN CODE              */ 00815000
         L     @11,FORMRC                                          0262 00816000
         OR    @11,R15                                             0262 00817000
         ST    @11,FORMRC                                          0262 00818000
*           RFY                                                    0263 00819000
*             R15 UNRSTD;                                          0263 00820000
*           CALL PRINT;             /* CALL PRINT ROUTINE            */ 00821000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0264 00822000
         BALR  @14,@15                                             0264 00823000
*           RFY                                                    0265 00824000
*            (R0,                                                  0265 00825000
*             R1) UNRSTD;                                          0265 00826000
*           RFY                                                    0266 00827000
*             R15 RSTD;                                            0266 00828000
*           R15=FORMRC;             /* SAVE RETURN CODE FROM FORMAT  */ 00829000
         L     R15,FORMRC                                          0267 00830000
*           RFY                                                    0268 00831000
*             R15 UNRSTD;                                          0268 00832000
*         END;                      /* ILRMFRMT (XPT) LINES(XPT1)    */ 00833000
*         CUMRC=CUMRC|R15;          /* OR IN RETURN CODE             */ 00834000
         L     @11,CUMRC                                           0270 00835000
         OR    @11,R15                                             0270 00836000
         ST    @11,CUMRC                                           0270 00837000
*         XPTEPTR=XPTEPTR+LENGTH(XPTE);/* CALCULATE ADDR OF NEXT XPTE*/ 00838000
         LA    @11,12                                              0271 00839000
         AL    @11,XPTEPTR                                         0271 00840000
         ST    @11,XPTEPTR                                         0271 00841000
*         PTEPTR=PTEPTR+LENGTH(PGTPTE);/* CLACULATE ADDR OF NEXT PGTE*/ 00842000
         LA    @11,2                                               0272 00843000
         AL    @11,PTEPTR                                          0272 00844000
         ST    @11,PTEPTR                                          0272 00845000
*       END;                        /* END 1 TO 16                   */ 00846000
         LA    @11,1                                               0273 00847000
         AL    @11,J                                               0273 00848000
         ST    @11,J                                               0273 00849000
         CH    @11,@CH00076                                        0273 00850000
         BNH   @DL00250                                            0273 00851000
@DC00250 DS    0H                                                  0274 00852000
*     END;                          /* FORMAT PGT/XPT        @ZA11398*/ 00853000
*   CALL PRINT;                     /* TO SPACE ONE LINE             */ 00854000
@RC00240 L     @15,ADPLPRNT(,ABDPLPTR)                             0275 00855000
         BALR  @14,@15                                             0275 00856000
*   R14=RTNADDR2;                   /* RESTORE RETURN ADDRESS        */ 00857000
         L     R14,RTNADDR2                                        0276 00858000
*   END FMTPGXPT;                   /* RETURN TO CALLER              */ 00859000
@EL00004 DS    0H                                                  0277 00860000
@EF00004 DS    0H                                                  0277 00861000
@ER00004 BR    @14                                                 0277 00862000
*                                                                  0278 00863000
*/********************************************************************/ 00864000
*/*ILRFMTH:   CHART (SEQ)                                            */ 00865000
*/*ILRFMTH:   E HEADER SEGMENT                                       */ 00866000
*/*           P SAVE CALLERS REGS, ESTABLISH ADDRESSIBILITY          */ 00867000
*/*           P OBTAIN ABDUMP PARM LIST PTR (R1)                     */ 00868000
*/*           P OBTAIN RSMHD PTR (R3)                                */ 00869000
*/*           P OBTAIN CVT PTR FROM PARM LIST                        */ 00870000
*/*           P CLEAR CUMULATIVE RETURN CODE                         */ 00871000
*/*           S FMTRSMHD: CALL ROUTINE TO FORMAT RSMHD AND SPCT      */ 00872000
*/*           P UPDATE ADDRESS TO POINT TO ASMHD                     */ 00873000
*/*           S FMTASMHD: CALL ROUTINE TO FORMAT ASMHD AND ITS AIAS  */ 00874000
*/*           D (YES,,NO,%SEGS) FIRST TIME HEADER ENTERED  ?         */ 00875000
*/*           L ACCESS:      PSASTOR MASTER SGT                      */ 00876000
*/*           D (YES,,NO,%ERRET2) POINTER GOOD ?                     */ 00877000
*/*           L ACCESS: PVT PTR FROM CVTPVTP                         */ 00878000
*/*           D (YES,,NO,%ERRET3) POINTER GOOD ?                     */ 00879000
*/*           L ACCESS: PVTLPRIV AND PVTLCSA                         */ 00880000
*/*           D (YES,,NO,%ERRET3) RETURN CODE GOOD ?                 */ 00881000
*/*           P STARTVBN = PVTLPRIV FIRST VBN OF USER PIRVATE AREA   */ 00882000
*/*           P CALCULATE NUMBER SEGMENTS (LPRIV - LCSA) / 16        */ 00883000
*/*           S SEGFIND: CALL ROUTINE TO FIND SGTES                  */ 00884000
*/*           P  OR IN RETURN CODE                                   */ 00885000
*/*%RETURN:   P RESTORE CALLERS REGS                                 */ 00886000
*/*           R RETURN                                               */ 00887000
*/*%ERRET2:   S (,%RETURN) ERRMSG: R1=10, MASTER SGT INACCESSIBLE    */ 00888000
*/*%ERRET3:   S (,%RETURN) ERRMSG: RC=6, PRINTS PVT INACCESSIBLE     */ 00889000
*/*%ERROR3:   P (,%RETURN) OR IN RETURN CODE                         */ 00890000
*/*%SEGS:     L ACCESS:     RSMSPCT                                  */ 00891000
*/*           D (YES,,NO,%ERROR3) ACCESS GOOD                        */ 00892000
*/*           L ACCESS:      SPCTSSEG, ACTIVE SEGES                  */ 00893000
*/*           D (YES,,NO,%ERROR3) ACCESS GOOD ?                      */ 00894000
*/*           P ENTRYCTR = ZERO                                      */ 00895000
*/*           P CALCULATE ADDR OF FIRST SEGMENT ENTRY (SEGADDR)      */ 00896000
*/*%CTR:      D (NO,,YES,%RETURN) ENTRYCTR = SPCTSSEG                */ 00897000
*/*           L ACCESS:      SEGSPTR VIA SEGADDR                     */ 00898000
*/*           D (YES,,NO,%ERROR4) ACCESS GOOD ?                      */ 00899000
*/*           P PGTADDR = SPCTPGT, PAGE TABLE ADDRESS                */ 00900000
*/*           S FMTPGXPT: CALL RTN TO FORMAT PGT/XPT'S               */ 00901000
*/*%ERROR4:   P OR IN RETURN CODE                                    */ 00902000
*/*           P ENTRYCTR = ENTRYCTR + ONE                            */ 00903000
*/*           P (,%CTR) CALCULATE ADDR NEXT SEGMENT ENTRY (SEGADDR)  */ 00904000
*/*ILRFMTH:   END                                                    */ 00905000
*/********************************************************************/ 00906000
*                                                                  0278 00907000
*                                                                  0278 00908000
*/********************************************************************/ 00909000
*/*                                                                  */ 00910000
*/* HEADER:  SEGMENT TO FORMAT RSM, ASM HEADERS AND PRIVATE AREA PAGE*/ 00911000
*/*           AND EXTERNAL PAGE TABLES FOR EACH ADDRESS SPACE        */ 00912000
*/*                                                                  */ 00913000
*/* INPUT:   VIRTUAL ADDRESS OF RSMHD IN R3                          */ 00914000
*/*           ABDUMP PARM LIST PTR IN R1                             */ 00915000
*/*           ADPLASID IN ABDUMP PARM LIST                           */ 00916000
*/*                                                                  */ 00917000
*/* PROCESS: (1) CALL SEGMENT TO FORMAT RSMHD AND SPCT               */ 00918000
*/*          (2) CALL SEGMENT TO FORMAT ASMHD                        */ 00919000
*/*          (3) GET SEGMENT TABLE ADDRESS, VBN OF FIRST PRIVATE AREA*/ 00920000
*/*          (4) CALL SEGMENT THAT LOCATES SEGMENTS AND CAUSES THEIR */ 00921000
*/*               PAGES TABLES TO BE FORMATTED                       */ 00922000
*/*          (5) GET SEGMENT ENTRY AND SEGMENT ADDRESS IN SPCT       */ 00923000
*/*          (6) CALL SEGMENT TO FORMAT PAGE AND EXTERNAL PAGE TABLES*/ 00924000
*/*                                                                  */ 00925000
*/* OUTPUT:  THE CUMULATIVE RETURN CODE IN R15 (0 OR 4)              */ 00926000
*/*                                                                  */ 00927000
*/********************************************************************/ 00928000
*                                                                  0278 00929000
*ILRFMTH:                                                          0278 00930000
*   ENTRY;                          /* FORMAT HEADER ENTRY           */ 00931000
@EP00278 DS    0H                                                  0279 00932000
*   ABDPLPTR=R1;                    /* GET ADDR OF PARM LIST         */ 00933000
         LR    ABDPLPTR,R1                                         0279 00934000
*   CVTPTR=ADPLCVT;                 /* GET CVT PTR FROM PARM LIST    */ 00935000
