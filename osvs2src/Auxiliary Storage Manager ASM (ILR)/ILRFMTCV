*               CALL FORMAT;        /* FORMAT LINE                   */ 02796000
         L     @15,ADPLFRMT(,ABDPLPTR)                             0727 02797000
         BALR  @14,@15                                             0727 02798000
*               CUMRC=CUMRC|R15;    /* OR IN RETURN CODE             */ 02799000
