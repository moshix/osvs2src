*     CALL FORMAT;                  /* FORMAT THE LINE               */ 02685000
         L     @15,ADPLFRMT(,ABDPLPTR)                             0704 02686000
         BALR  @14,@15                                             0704 02687000
*     RFY                                                          0705 02688000
*       R15 RSTD;                                                  0705 02689000
*     FORMRC=FORMRC|R15;            /* SAVE RETURN CODE              */ 02690000
         OR    FORMRC,R15                                          0706 02691000
*     RFY                                                          0707 02692000
*       R15 UNRSTD;                                                0707 02693000
*     CALL PRINT;                   /* CALL PRINT ROUTINE            */ 02694000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0708 02695000
         BALR  @14,@15                                             0708 02696000
*     RFY                                                          0709 02697000
*      (R0,                                                        0709 02698000
*       R1) UNRSTD;                                                0709 02699000
*     RFY                                                          0710 02700000
*       R15 RSTD;                                                  0710 02701000
*     R15=FORMRC;                   /* SAVE RETURN CODE FROM FORMAT  */ 02702000
         LR    R15,FORMRC                                          0711 02703000
*     RFY                                                          0712 02704000
*       R15 UNRSTD;                                                0712 02705000
*   END;                            /* ILRMFRMT (PAT) LINES(PAT1)    */ 02706000
*   IF R15^=ZERO THEN               /* IF PAT HEADER FORMATTING BLEW    02707000
*                                      UP,                           */ 02708000
         LTR   R15,R15                                             0714 02709000
         BZ    @RF00714                                            0714 02710000
*     CUMPATRC=4;                   /* SET BAD RETURN CODE AND SKIP     02711000
*                                      PAT MAPS THIS PAT             */ 02712000
         MVC   CUMPATRC(4),@CF00139                                0715 02713000
*                                                                  0716 02714000
*   /*****************************************************************/ 02715000
*   /*                                                               */ 02716000
*   /* FOLLOWING SECTION FORMATS THE PATMAPS IF FORMAT OF HEADER     */ 02717000
*   /* SECTION WAS SUCCESSFUL                                        */ 02718000
*   /*                                                               */ 02719000
*   /*****************************************************************/ 02720000
*                                                                  0716 02721000
*   ELSE                            /* OTHERWISE GO AHEAD AND FORMAT    02722000
*                                      PAT MAPS FOR THIS PAT         */ 02723000
*     DO;                                                          0716 02724000
         B     @RC00714                                            0716 02725000
@RF00714 DS    0H                                                  0717 02726000
*       BUFFER(1:LENGTH(MAPNAM))=MAPNAM;/* SET HEADING FOR PAT MAPS  */ 02727000
         L     @14,ADPLBUF(,ABDPLPTR)                              0717 02728000
         MVC   BUFFER(7,@14),MAPNAM                                0717 02729000
*       RFY                                                        0718 02730000
*         R1 RSTD;                                                 0718 02731000
*       R1=ABDPLPTR;                /* SET UP FOR PRINT              */ 02732000
         LR    R1,ABDPLPTR                                         0719 02733000
*       CALL PRINT;                 /* PRINT HEADER FOR PAT MAPS     */ 02734000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0720 02735000
         BALR  @14,@15                                             0720 02736000
*       RFY                                                        0721 02737000
*         R1 UNRSTD;                                               0721 02738000
*       DO;                         /* ILRMACCS (CYLS) FROM(PATCYLNO)*/ 02739000
*         RFY                                                      0723 02740000
