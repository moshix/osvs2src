*   ANCHNAME=ANCHPAT;               /* NAME OF ANCHOR                */ 02642000
         MVC   ANCHNAME(8,@14),ANCHPAT                             0687 02643000
*   RFY                                                            0688 02644000
*    (R0,                                                          0688 02645000
*     R1) RSTD;                                                    0688 02646000
*   R0=ADDR(PATPTR);                /* PTR TO BE FORMATTED           */ 02647000
         LA    R0,PATPTR                                           0689 02648000
*   R1=ADDR(PATNAM);                /* LABEL                         */ 02649000
         LA    R1,PATNAM                                           0690 02650000
*   CALL ILRFPRTH;                  /* FORMAT AND PRINT HEADING      */ 02651000
         L     @15,@CV00488                                        0691 02652000
         BALR  @14,@15                                             0691 02653000
*   RFY                                                            0692 02654000
*    (R0,                                                          0692 02655000
*     R1) UNRSTD;                                                  0692 02656000
*   DO;                             /* ILRMFRMT (PAT) LINES(PAT1)    */ 02657000
*     FORMRC=0;                     /* ZERO THE RETURN CODE          */ 02658000
         SLR   FORMRC,FORMRC                                       0694 02659000
*     PAT1DT1=ADDR(PATIDENT);                                      0695 02660000
