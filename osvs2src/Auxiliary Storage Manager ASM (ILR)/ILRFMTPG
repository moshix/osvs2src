*             ;                     /* THE QUEUE WAS EMPTY           */ 00429000
@RF00180 DS    0H                                                  0187 00430000
*           CUMRC=CUMRC|R15;        /* OR IN APPROPRIATE RET CODE    */ 00431000
         L     @14,CUMRC                                           0187 00432000
         OR    @14,R15                                             0187 00433000
         ST    @14,CUMRC                                           0187 00434000
*                                                                  0188 00435000
*/********************************************************************/ 00436000
*/* THIS SECTION PROCESSES THE PART ENTRY:                           */ 00437000
*/*   1. GET THE NUMBER OF PART ENTRIES                              */ 00438000
*/*   2. INITIALIZE:  -DATA SET NAME LIST ADDRESS                    */ 00439000
*/*                   -PART ENTRY COUNTER                            */ 00440000
*/*                   -ADDRESS OF FIRST ENTRY                        */ 00441000
*/*  FOR EACH ENTRY:                                                 */ 00442000
*/*   3. CALL SEGMENT TO FORMAT PARTE                                */ 00443000
*/*   4. INCREMENT COUNTER                                           */ 00444000
*/*   5. CALCULATE NEXT ENTRY ADDRESS AND NEXT DSNAME ADDRESS        */ 00445000
*/********************************************************************/ 00446000
*                                                                  0188 00447000
*           DO;                     /* ILRMACCS (NUMPARTE)         0188 00448000
*                                      FROM(PARTSIZE)                */ 00449000
*             RFY                                                  0189 00450000
*              (R0,                                                0189 00451000
*               R1,                                                0189 00452000
*               R14,                                               0189 00453000
*               R15) RSTD;                                         0189 00454000
*             R0=ADDR(PARTSIZE);                                   0190 00455000
         L     @12,PARTPTR                                         0190 00456000
         LA    R0,PARTSIZE(,@12)                                   0190 00457000
*             R1=ABDPLPTR;          /* POINT TO PARM LIST            */ 00458000
         LR    R1,ABDPLPTR                                         0191 00459000
*             CALL ACCESS;          /* ACCESS DATA                   */ 00460000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0192 00461000
         BALR  @14,@15                                             0192 00462000
*             R14=R0;               /* CANNOT USE R0 AS BASE         */ 00463000
         LR    R14,R0                                              0193 00464000
*             IF R15=GOOD THEN      /* CHECK ACCESS                  */ 00465000
         LTR   R15,R15                                             0194 00466000
         BNZ   @RF00194                                            0194 00467000
*               NUMPARTE=CURRENT;   /* GET REQUESTED FIELD           */ 00468000
         MVC   NUMPARTE(4),CURRENT(R14)                            0195 00469000
*             ELSE                  /* ACCESS NO GOOD                */ 00470000
*               NUMPARTE=ZERO;      /* ZERO FIELD                    */ 00471000
         B     @RC00194                                            0196 00472000
@RF00194 SLR   @12,@12                                             0196 00473000
         ST    @12,NUMPARTE                                        0196 00474000
*             RFY                                                  0197 00475000
*              (R0,                                                0197 00476000
*               R1,                                                0197 00477000
*               R14,                                               0197 00478000
*               R15) UNRSTD;                                       0197 00479000
@RC00194 DS    0H                                                  0198 00480000
*           END;                    /* GET NUM PART ENTRIES          */ 00481000
*           IF NUMPARTE^=ZERO THEN  /* ACCESS WAS GOOD               */ 00482000
         ICM   @14,15,NUMPARTE                                     0199 00483000
         BZ    @RF00199                                            0199 00484000
*             DO;                                                  0200 00485000
*               DO;                 /* ILRMACCS (DSNLPARM)         0201 00486000
*                                      FROM(PARTDSNL)                */ 00487000
*                 RFY                                              0202 00488000
*                  (R0,                                            0202 00489000
*                   R1,                                            0202 00490000
*                   R14,                                           0202 00491000
*                   R15) RSTD;                                     0202 00492000
*                 R0=ADDR(PARTDSNL);                               0203 00493000
         L     @12,PARTPTR                                         0203 00494000
         LA    R0,PARTDSNL(,@12)                                   0203 00495000
*                 R1=ABDPLPTR;      /* POINT TO PARM LIST            */ 00496000
         LR    R1,ABDPLPTR                                         0204 00497000
*                 CALL ACCESS;      /* ACCESS DATA                   */ 00498000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0205 00499000
         BALR  @14,@15                                             0205 00500000
*                 R14=R0;           /* CANNOT USE R0 AS BASE         */ 00501000
         LR    R14,R0                                              0206 00502000
*                 IF R15=GOOD THEN  /* CHECK ACCESS                  */ 00503000
         LTR   R15,R15                                             0207 00504000
         BNZ   @RF00207                                            0207 00505000
*                   DSNLPARM=CURRENT;/* GET REQUESTED FIELD          */ 00506000
         L     DSNLPARM,CURRENT(,R14)                              0208 00507000
*                 ELSE              /* ACCESS NO GOOD                */ 00508000
*                   DSNLPARM=ZERO;  /* ZERO FIELD                    */ 00509000
         B     @RC00207                                            0209 00510000
@RF00207 SLR   DSNLPARM,DSNLPARM                                   0209 00511000
*                 RFY                                              0210 00512000
*                  (R0,                                            0210 00513000
*                   R1,                                            0210 00514000
*                   R14,                                           0210 00515000
*                   R15) UNRSTD;                                   0210 00516000
@RC00207 DS    0H                                                  0211 00517000
*               END;                                               0211 00518000
*               IF DSNLPARM^=NULLPTR THEN                          0212 00519000
         LTR   DSNLPARM,DSNLPARM                                   0212 00520000
         BZ    @RF00212                                            0212 00521000
*                 DO;               /* DSN TABLE EXIST?      @ZA11398*/ 00522000
*                   RFY                                            0214 00523000
*                    (R0,                                          0214 00524000
*                     R1,                                          0214 00525000
*                     R15) RSTD;    /*                       @ZA11398*/ 00526000
*                   R0=DSNLPARM;    /* INPUT TO ACCESS       @ZA11398*/ 00527000
         LR    R0,DSNLPARM                                         0215 00528000
*                   R1=ABDPLPTR;    /* INPUT TO ACCESS       @ZA11398*/ 00529000
         LR    R1,ABDPLPTR                                         0216 00530000
*                   CALL ACCESS;    /* DSN TABLE ACCESSIBLE? @ZA11398*/ 00531000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0217 00532000
         BALR  @14,@15                                             0217 00533000
*                   IF R15^=GOOD THEN                              0218 00534000
         LTR   R15,R15                                             0218 00535000
         BZ    @RF00218                                            0218 00536000
*                     DSNLPARM=NULLPTR;/* NO                 @ZA11398*/ 00537000
         SLR   DSNLPARM,DSNLPARM                                   0219 00538000
*                   RFY                                            0220 00539000
*                    (R0,                                          0220 00540000
*                     R1,                                          0220 00541000
*                     R15) UNRSTD;  /*                       @ZA11398*/ 00542000
@RF00218 DS    0H                                                  0221 00543000
*                 END;              /* DSN TABLE EXIST       @ZA11398*/ 00544000
*               CUMRC=CUMRC|R15;    /* OR IN ACCESS RET CODE         */ 00545000
@RF00212 L     @14,CUMRC                                           0222 00546000
         OR    @14,R15                                             0222 00547000
         ST    @14,CUMRC                                           0222 00548000
*/* PARTDSNL COULD BE ZERO - FMTPARTE WILL CHECK FOR THAT            */ 00549000
*                                                                  0223 00550000
*               PAREPTR=ADDR(PARTENTS);/* ADDR OF FIRST PART ENTRY   */ 00551000
         L     @14,PARTPTR                                         0223 00552000
         LA    @14,PARTENTS(,@14)                                  0223 00553000
         ST    @14,PAREPTR                                         0223 00554000
*               DO PARTECTR=1 TO NUMPARTE;/* UNTIL ALL ENTRIES     0224 00555000
*                                      PROCESSED                     */ 00556000
         LA    @14,1                                               0224 00557000
         B     @DE00224                                            0224 00558000
@DL00224 DS    0H                                                  0225 00559000
*                 CALL FMTPARTE;    /* CALL SEGMENT OT FMT PARTE     */ 00560000
         BAL   @14,FMTPARTE                                        0225 00561000
*                 CUMRC=CUMRC|R15;  /* OR IN RETURN CODE             */ 00562000
         L     @14,CUMRC                                           0226 00563000
         OR    @14,R15                                             0226 00564000
         ST    @14,CUMRC                                           0226 00565000
*                 IF DSNLPARM^=NULLPTR THEN/* DSNAME EXISTS          */ 00566000
         LTR   DSNLPARM,DSNLPARM                                   0227 00567000
         BZ    @RF00227                                            0227 00568000
*                   DSNLPARM=DSNLPARM+LENGTH(DSNAME);              0228 00569000
         AH    DSNLPARM,@CH00607                                   0228 00570000
*                 ELSE                                             0229 00571000
*                   ;               /* DSNLPARM WAS ZERO             */ 00572000
@RF00227 DS    0H                                                  0230 00573000
*                 PAREPTR=PAREPTR+LENGTH(PARTENT);/* INCR TO NEXT  0230 00574000
*                                      PARTE ADDR                    */ 00575000
         LA    @14,64                                              0230 00576000
         AL    @14,PAREPTR                                         0230 00577000
         ST    @14,PAREPTR                                         0230 00578000
*               END;                /* END 1 TO PARTECNT             */ 00579000
         LA    @14,1                                               0231 00580000
         AL    @14,PARTECTR                                        0231 00581000
@DE00224 ST    @14,PARTECTR                                        0231 00582000
         C     @14,NUMPARTE                                        0231 00583000
         BNH   @DL00224                                            0231 00584000
*             END;                  /* END FORMAT PARTES- ACCESS GOOD   00585000
*                                      FOR PART SIZE                 */ 00586000
*           ELSE                    /* ACCESS FAILED FOR PART SIZE   */ 00587000
*             CUMRC=4;              /* SET BAD RET CODE              */ 00588000
         B     @RC00199                                            0233 00589000
@RF00199 MVC   CUMRC(4),@CF00139                                   0233 00590000
*         END;                      /* END PART FORMATTING           */ 00591000
*       ELSE                                                       0235 00592000
*         CUMRC=8;                  /* PART NOT FORMATTED          0235 00593000
*                                      SUCCESSFULLY                  */ 00594000
         B     @RC00072                                            0235 00595000
@RF00072 MVC   CUMRC(4),@CF00040                                   0235 00596000
*     END;                          /* END PART PROCESSING           */ 00597000
*   ELSE                            /* ACCESS FAILED FOR APRT        */ 00598000
*     CUMRC=8;                                                     0237 00599000
         B     @RC00069                                            0237 00600000
@RF00069 MVC   CUMRC(4),@CF00040                                   0237 00601000
*   RETURN CODE(CUMRC);             /* RETURN TO CALLER              */ 00602000
@RC00069 L     @15,CUMRC                                           0238 00603000
         L     @13,4(,@13)                                         0238 00604000
         L     @14,12(,@13)                                        0238 00605000
         LM    @00,@12,20(@13)                                     0238 00606000
         BR    @14                                                 0238 00607000
*                                                                  0239 00608000
*/********************************************************************/ 00609000
*/*                                                                  */ 00610000
*/* FMTPART: SEGMENT TO FORMAT THE PAGE ACTIVITY REFERENCE TABLE     */ 00611000
*/*                                                                  */ 00612000
*/* INPUT:   VIRTUAL ADDRESS OF PART IN PARTPTR                      */ 00613000
*/*                                                                  */ 00614000
*/* PROCESS: FORMAT FOUR LINES OF PART USING ILRMLINE AND ILRMFRMT.  */ 00615000
*/*                                                                  */ 00616000
*/* OUTPUT:  PART CONTROL BLOCK FORMATTED.                           */ 00617000
*/*                                                                  */ 00618000
*/* RETURN CODES: 0 - PART FORMATTED SUCCESSFULLY                    */ 00619000
*/*               4 - AT LEAST 1 ERROR OCCURRED WHILE FORMATTING     */ 00620000
*/*                     THE PART.                                    */ 00621000
*/*                                                                  */ 00622000
*/********************************************************************/ 00623000
*                                                                  0239 00624000
*FMTPART:                                                          0239 00625000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0239 00626000
FMTPART  DS    0H                                                  0240 00627000
*                                                                  0240 00628000
*   /*****************************************************************/ 00629000
*   /*                                                               */ 00630000
*   /* PAR1:ILRMLINE ((TIDEN,ASIS),TSIZE,TEUSE,TAIAE,TCIR1) OF(PART) */ 00631000
*   /* LABEL VARIABLES REFERENCED IN FORMAT PATTERNS                 */ 00632000
*   /*                                                               */ 00633000
*   /*****************************************************************/ 00634000
*                                                                  0240 00635000
*   DCL                                                            0240 00636000
*     PAR1LB1 CHAR(LENGTH('TIDEN')) INIT('TIDEN');                 0240 00637000
*   DCL                                                            0241 00638000
*     PAR1LB2 CHAR(LENGTH('TSIZE')) INIT('TSIZE');                 0241 00639000
*   DCL                                                            0242 00640000
*     PAR1LB3 CHAR(LENGTH('TEUSE')) INIT('TEUSE');                 0242 00641000
*   DCL                                                            0243 00642000
*     PAR1LB4 CHAR(LENGTH('TAIAE')) INIT('TAIAE');                 0243 00643000
*   DCL                                                            0244 00644000
*     PAR1LB5 CHAR(LENGTH('TCIR1')) INIT('TCIR1');                 0244 00645000
*                                                                  0244 00646000
*   /*****************************************************************/ 00647000
*   /*                                                               */ 00648000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 00649000
*   /*                                                               */ 00650000
*   /*****************************************************************/ 00651000
*                                                                  0245 00652000
*   DCL                                                            0245 00653000
*     1 PAR1PTS BDY(WORD),                                         0245 00654000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00655000
*       3 * BIT(8) BDY(BYTE) INIT('00101111'B),/* SEE PTTFLGS        */ 00656000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PARTIDEN)-1+16*LENGTH('TIDEN 00657000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 00658000
*       3 * FIXED(8) INIT(5),                                      0245 00659000
*       3 * FIXED(8) INIT(11),                                     0245 00660000
*       3 * PTR(31) INIT(ADDR(PAR1LB1)),/* SEE PTTLBPTR              */ 00661000
*       3 PAR1DT1 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 00662000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00663000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00664000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PARTSIZE)-1+16*LENGTH('TSIZE 00665000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 00666000
*       3 * FIXED(8) INIT(15+8),                                   0245 00667000
*       3 * FIXED(8) INIT(21+8),                                   0245 00668000
*       3 * PTR(31) INIT(ADDR(PAR1LB2)),/* SEE PTTLBPTR              */ 00669000
*       3 PAR1DT2 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 00670000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00671000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00672000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PARTEUSE)-1+16*LENGTH('TEUSE 00673000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 00674000
*       3 * FIXED(8) INIT(25+8+8),                                 0245 00675000
*       3 * FIXED(8) INIT(31+8+8),                                 0245 00676000
*       3 * PTR(31) INIT(ADDR(PAR1LB3)),/* SEE PTTLBPTR              */ 00677000
*       3 PAR1DT3 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 00678000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00679000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00680000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(PARTAIAE)-1+16*LENGTH('TAIAE 00681000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 00682000
