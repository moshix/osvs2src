         SLR   @14,@14                                             0685 02637000
         ST    @14,CUMPATRC                                        0685 02638000
*   ANCHTXT=ANCH;                   /* ANCHORED TEXT                 */ 02639000
         L     @14,ADPLBUF(,ABDPLPTR)                              0686 02640000
