*             R1=ABDPLPTR;          /* POINT TO PARM LIST            */ 00093000
         LR    R1,ABDPLPTR                                         0077 00094000
*             CALL ACCESS;          /* ACCESS DATA                   */ 00095000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0078 00096000
         BALR  @14,@15                                             0078 00097000
*             R14=R0;               /* CANNOT USE R0 AS BASE         */ 00098000
         LR    R14,R0                                              0079 00099000
*             IF R15=GOOD THEN      /* CHECK ACCESS                  */ 00100000
         LTR   R15,R15                                             0080 00101000
         BNZ   @RF00080                                            0080 00102000
*               PCTPTR=CURRENT;     /* GET REQUESTED FIELD           */ 00103000
         MVC   PCTPTR(4),CURRENT(R14)                              0081 00104000
*             ELSE                  /* ACCESS NO GOOD                */ 00105000
*               PCTPTR=ZERO;        /* ZERO FIELD                    */ 00106000
         B     @RC00080                                            0082 00107000
@RF00080 SLR   @12,@12                                             0082 00108000
         ST    @12,PCTPTR                                          0082 00109000
*             RFY                                                  0083 00110000
*              (R0,                                                0083 00111000
*               R1,                                                0083 00112000
*               R14,                                               0083 00113000
*               R15) UNRSTD;                                       0083 00114000
@RC00080 DS    0H                                                  0084 00115000
*           END;                    /* GET ADDR OF PCT Q             */ 00116000
*           IF PCTPTR^=NULLPTR THEN /* ACCESS WAS SUCCESSFUL         */ 00117000
         ICM   @14,15,PCTPTR                                       0085 00118000
         BZ    @RF00085                                            0085 00119000
*             CALL FMTPCT;          /* CALL SEGMENT TO FMT PCT       */ 00120000
         BAL   @14,FMTPCT                                          0086 00121000
*           ELSE                                                   0087 00122000
*             ;                     /* PCTPTR WAS NULL               */ 00123000
*                                                                  0087 00124000
@RF00085 DS    0H                                                  0088 00125000
*           /*********************************************************/ 00126000
*           /*                                                       */ 00127000
*           /* NOTE: THE FOLLOWING INSTRUCTION TO OR IN THE RETURN   */ 00128000
*           /* CODE ORS IN EITHER ACCESS RC OR FMTPCTS RETURN CODE   */ 00129000
*           /* DEPENDING ON THE CONTENTS OF PCTPTR. THIS METHOD IS   */ 00130000
*           /* USED THROUGOUT THE MODULE.                            */ 00131000
*           /*                                                       */ 00132000
*           /*********************************************************/ 00133000
*                                                                  0088 00134000
*           CUMRC=CUMRC|R15;        /* OR IN RET CODE                */ 00135000
         L     @14,CUMRC                                           0088 00136000
         OR    @14,R15                                             0088 00137000
         ST    @14,CUMRC                                           0088 00138000
*                                                                  0089 00139000
*/********************************************************************/ 00140000
*/* ACCESS AND FORMAT THE AIA ERROR QUEUE.  IF THE ACCESS IS GOOD    */ 00141000
*/*  AND THE QUEUE POINTER IS NOT ZERO, SET UP ANCHOR, CALL SEGMENT  */ 00142000
*/*  TO FORMAT AIA QUEUE AND OR IN RETURN CODE.   IF THE QUEUE WAS   */ 00143000
*/*  EMPTY, SKIP THE PROCESSING.  IF THE ACCESS FAILED, SKIP THE     */ 00144000
*/* PROCESSING AND SET BAD RETURN CODE.                              */ 00145000
*/********************************************************************/ 00146000
*                                                                  0089 00147000
*           DO;                     /* ILRMACCS (AIAPTR)           0089 00148000
