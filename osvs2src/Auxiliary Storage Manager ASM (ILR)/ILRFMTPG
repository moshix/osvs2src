         TITLE 'ILRFMTPG   ASM PAGE CONTROL-BLOCK FORMATTING MODULE    *00001000
                        '                                               00002000
ILRFMTPG CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRFMTPG  76.170'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @09,0                                               0001 00011000
@PSTART  LA    @10,4095(,@09)                                      0001 00012000
         USING @PSTART,@09                                         0001 00013000
         USING @PSTART+4095,@10                                    0001 00014000
         ST    @13,@SA00001+4                                      0001 00015000
         LA    @14,@SA00001                                        0001 00016000
         ST    @14,8(,@13)                                         0001 00017000
         LR    @13,@14                                             0001 00018000
*   ABDPLPTR=R1;                    /* SET PRINTDUMP PARM LIST PTR 0049 00019000
*                                      FROM INPUT REG                */ 00020000
         LR    ABDPLPTR,R1                                         0049 00021000
*   ASMVTPTR=R3;                    /* SET ASMVT BASE FROM INPUT   0050 00022000
*                                      REGISTER                      */ 00023000
         LR    ASMVTPTR,R3                                         0050 00024000
*   RFY                                                            0051 00025000
*    (R1,                                                          0051 00026000
*     R3) UNRSTD;                                                  0051 00027000
*   DO;                             /* ILRMACCS (PART) VIA(ASMPART)  */ 00028000
*     RFY                                                          0053 00029000
*      (R0,                                                        0053 00030000
*       R1,                                                        0053 00031000
*       R14,                                                       0053 00032000
*       R15) RSTD;                                                 0053 00033000
*     R1=ABDPLPTR;                                                 0054 00034000
         LR    R1,ABDPLPTR                                         0054 00035000
*     R0=ADDR(ASMPART);             /* ADDR OF CELL WITH PTR         */ 00036000
         LA    R0,ASMPART(,ASMVTPTR)                               0055 00037000
*     CALL ACCESS;                  /* ACCESS PTR TO PART            */ 00038000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0056 00039000
         BALR  @14,@15                                             0056 00040000
*     IF R15=GOOD THEN                                             0057 00041000
         LTR   R15,R15                                             0057 00042000
         BNZ   @RF00057                                            0057 00043000
*       DO;                         /* FOUND PTR - NOW FIND BLOCK    */ 00044000
*         R14=R0;                   /* CANNOT USE R0 AS BASE REG     */ 00045000
         LR    R14,R0                                              0059 00046000
*         R0=CURRENT;               /* POINTER ITSELF                */ 00047000
         L     R0,CURRENT(,R14)                                    0060 00048000
*         PARTPTR=R0;               /* SAVE POINTER                  */ 00049000
         ST    R0,PARTPTR                                          0061 00050000
*         CALL ACCESS;              /* ACCESS PART ITSELF            */ 00051000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0062 00052000
         BALR  @14,@15                                             0062 00053000
*         IF R15^=GOOD THEN                                        0063 00054000
         SLR   @12,@12                                             0063 00055000
         CR    R15,@12                                             0063 00056000
         BE    @RF00063                                            0063 00057000
*           PARTPTR=NULLPTR;                                       0064 00058000
         ST    @12,PARTPTR                                         0064 00059000
*       END;                                                       0065 00060000
*     ELSE                                                         0066 00061000
*       PARTPTR=NULLPTR;                                           0066 00062000
         B     @RC00057                                            0066 00063000
@RF00057 SLR   @12,@12                                             0066 00064000
         ST    @12,PARTPTR                                         0066 00065000
*     RFY                                                          0067 00066000
*      (R0,                                                        0067 00067000
*       R1,                                                        0067 00068000
*       R14,                                                       0067 00069000
*       R15) UNRSTD;                                               0067 00070000
@RC00057 DS    0H                                                  0068 00071000
*   END;                            /* GET ADDRESS OF PART   @ZA11398*/ 00072000
*   IF PARTPTR^=NULLPTR THEN        /* ACCESS WAS SUCCESSFUL         */ 00073000
         ICM   @14,15,PARTPTR                                      0069 00074000
         BZ    @RF00069                                            0069 00075000
*     DO;                                                          0070 00076000
*       CALL FMTPART;               /* CALL SEGMENT TO FORMAT PART   */ 00077000
         BAL   @14,FMTPART                                         0071 00078000
*       IF R15=ZERO THEN            /* SUCCESSFULLY FORMAT THE PART? */ 00079000
         LTR   R15,R15                                             0072 00080000
         BNZ   @RF00072                                            0072 00081000
*         DO;                       /* CONTINUE                      */ 00082000
*           DO;                     /* ILRMACCS (PCTPTR)           0074 00083000
*                                      FROM(PARTPCTQ)                */ 00084000
*             RFY                                                  0075 00085000
*              (R0,                                                0075 00086000
*               R1,                                                0075 00087000
*               R14,                                               0075 00088000
*               R15) RSTD;                                         0075 00089000
*             R0=ADDR(PARTPCTQ);                                   0076 00090000
         L     @12,PARTPTR                                         0076 00091000
