*       RFY                                                        1058 04225000
*        (R0,                                                      1058 04226000
*         R1,                                                      1058 04227000
*         R14,                                                     1058 04228000
*         R15) UNRSTD;                                             1058 04229000
@RC01055 DS    0H                                                  1059 04230000
*     END;                                                         1059 04231000
*     RFY                                                          1060 04232000
*       R15 RSTD;                                                  1060 04233000
*     IORRC=IORRC|R15;              /* SAVE THE RETURN CODE          */ 04234000
         OR    IORRC,R15                                           1061 04235000
*     IF R15=GOOD THEN              /* WAS ACCESS SUCCESSFUL         */ 04236000
         LTR   R15,R15                                             1062 04237000
         BNZ   @RF01062                                            1062 04238000
*       DO;                                                        1063 04239000
*         RFY                                                      1064 04240000
*           R15 UNRSTD;                                            1064 04241000
*                                                                  1064 04242000
*/*   THE FIRST LINE OF THE IORB IS FORMATTED DIFFERENTLY            */ 04243000
*/*   DEPENDING IF THE IORB IS FOR SWAP OR PAGE. CHECK THE           */ 04244000
*/*   FLAGS TO DETERMINE WHAT TYPE AND FORMAT THE FIRST LINE.        */ 04245000
*                                                                  1065 04246000
*         IF IORBSWAP='0'B THEN     /* IS THIS A SWAP IORB??         */ 04247000
         TM    IORBSWAP,B'00001000'                                1065 04248000
         BNZ   @RF01065                                            1065 04249000
*           DO;                                                    1066 04250000
*             DO;                   /* ILRMFRMT (IORB) LINES(IOR1)   */ 04251000
*               FORMRC=0;           /* ZERO THE RETURN CODE          */ 04252000
         SLR   FORMRC,FORMRC                                       1068 04253000
*               IOR1DT1=ADDR(IORID);                               1069 04254000
         L     @12,IORBPTR                                         1069 04255000
         ST    @12,IOR1DT1                                         1069 04256000
*               IOR1DT2=ADDR(IORNUM);                              1070 04257000
         LA    @08,IORNUM(,@12)                                    1070 04258000
         ST    @08,IOR1DT2                                         1070 04259000
*               IOR1DT3=ADDR(IORRQSZ);                             1071 04260000
         LA    @08,IORRQSZ(,@12)                                   1071 04261000
         ST    @08,IOR1DT3                                         1071 04262000
*               IOR1DT4=ADDR(IORFLGS);                             1072 04263000
         LA    @08,IORFLGS(,@12)                                   1072 04264000
         ST    @08,IOR1DT4                                         1072 04265000
*               IOR1DT5=ADDR(IORIORB);                             1073 04266000
         LA    @08,IORIORB(,@12)                                   1073 04267000
