         ST    @03,SCCW1DT4                                        0732 02806000
*       SCCW1DT5=ADDR(SCCWAIA);                                    0733 02807000
         LA    @03,SCCWAIA(,@12)                                   0733 02808000
         ST    @03,SCCW1DT5                                        0733 02809000
*       SCCW1DT6=ADDR(SCCWIORB);                                   0734 02810000
         LA    @12,SCCWIORB(,@12)                                  0734 02811000
         ST    @12,SCCW1DT6                                        0734 02812000
*       RFY                                                        0735 02813000
*        (R0,                                                      0735 02814000
*         R1) RSTD;                                                0735 02815000
*       R0=ADDR(SCCW1PTS);                                         0736 02816000
         LA    R0,SCCW1PTS                                         0736 02817000
*       R1=ABDPLPTR;                /* GET PARM ADDR                 */ 02818000
         L     @12,ABDPLPTR                                        0737 02819000
         LR    R1,@12                                              0737 02820000
*       CALL FORMAT;                /* FORMAT THE LINE               */ 02821000
         L     @15,ADPLFRMT(,@12)                                  0738 02822000
         BALR  @14,@15                                             0738 02823000
*       RFY                                                        0739 02824000
*         R15 RSTD;                                                0739 02825000
*       FORMRC=FORMRC|R15;          /* SAVE RETURN CODE              */ 02826000
         OR    FORMRC,R15                                          0740 02827000
*       RFY                                                        0741 02828000
*         R15 UNRSTD;                                              0741 02829000
*       CALL PRINT;                 /* CALL PRINT ROUTINE            */ 02830000
