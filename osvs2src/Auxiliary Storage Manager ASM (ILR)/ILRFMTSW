*/********************************************************************/ 04150000
*/* HOUSEKEEPING AND INITIALIZATIONS.  PROCESS IORBS UNTIL THE END   */ 04151000
*/*  OF QUEUE IS REACHED, THE MAXIMUM IS EXCEEDED OR AN ACCESS FAILS */ 04152000
*/*  THEN GET IOSB PTR AND CALL SEGMENT TO FMT IOSB                  */ 04153000
*/********************************************************************/ 04154000
*                                                                  1037 04155000
*   RFY                                                            1037 04156000
*     R14 RSTD;                                                    1037 04157000
*   RTNADDR2=R14;                   /* SAVE RETURN ADDRESS           */ 04158000
         ST    R14,RTNADDR2                                        1038 04159000
*   RFY                                                            1039 04160000
*     R14 UNRSTD;                                                  1039 04161000
*   IORRC=0;                        /* ZERO THE RETURN CODE          */ 04162000
         SLR   IORRC,IORRC                                         1040 04163000
*   IORCTR=0;                       /* INITIALIZE COUNTER            */ 04164000
         SLR   @12,@12                                             1041 04165000
         ST    @12,IORCTR                                          1041 04166000
*   DO WHILE(IORBPTR^=NULLPTR&      /* UNTIL ACCESS FAILS -OR- NEXT     04167000
*                                      IORB = 0 -OR-                 */ 04168000
*         IORCTR<MAXIORBS);         /* UNTIL MAX EXCEEDED            */ 04169000
         B     @DE01042                                            1042 04170000
@DL01042 DS    0H                                                  1043 04171000
*     IORCTR=IORCTR+1;              /* INCREMENT THE LOOP COUNTER    */ 04172000
         LA    @12,1                                               1043 04173000
         AL    @12,IORCTR                                          1043 04174000
         ST    @12,IORCTR                                          1043 04175000
*     RFY                                                          1044 04176000
*      (R0,                                                        1044 04177000
*       R1) RSTD;                                                  1044 04178000
*     R0=ADDR(IORBPTR);             /* GET ADDRESS OF THE BLOCK      */ 04179000
         LA    R0,IORBPTR                                          1045 04180000
*     R1=ADDR(IORBNAM);             /* GET ADDRESS OF THE NAME       */ 04181000
         LA    R1,IORBNAM                                          1046 04182000
*     CALL ILRFPRTH;                /* GO TO PRINT HEADING           */ 04183000
         L     @15,@CV00676                                        1047 04184000
         BALR  @14,@15                                             1047 04185000
*     RFY                                                          1048 04186000
*      (R0,                                                        1048 04187000
*       R1) UNRSTD;                                                1048 04188000
*                                                                  1048 04189000
*     /***************************************************************/ 04190000
*     /*                                                             */ 04191000
*     /* GET THE IORB FLAGS TO DETERMINE IF THIS IS FOR A PAGE OR A  */ 04192000
*     /* SWAP. IF THE ACCESS IS GOOD DETERMINE IF THE IORB REPRESENTS*/ 04193000
*     /* A PAGE OR A SWAP AND FORMAT ACCORDINGLY.                    */ 04194000
*     /*                                                             */ 04195000
*     /***************************************************************/ 04196000
*                                                                  1049 04197000
*     DO;                           /* ILRMACCS (IORBTYPE)         1049 04198000
*                                      FROM(IORID)                   */ 04199000
*       RFY                                                        1050 04200000
*        (R0,                                                      1050 04201000
*         R1,                                                      1050 04202000
*         R14,                                                     1050 04203000
*         R15) RSTD;                                               1050 04204000
*       R0=ADDR(IORID);                                            1051 04205000
         L     R0,IORBPTR                                          1051 04206000
*       R1=ABDPLPTR;                /* POINT TO PARM LIST            */ 04207000
         L     @12,ABDPLPTR                                        1052 04208000
         LR    R1,@12                                              1052 04209000
*       CALL ACCESS;                /* ACCESS DATA                   */ 04210000
         L     @15,ADPLMEMA(,@12)                                  1053 04211000
         BALR  @14,@15                                             1053 04212000
*       R14=R0;                     /* CANNOT USE R0 AS BASE         */ 04213000
         LR    R14,R0                                              1054 04214000
*       IF R15=GOOD THEN            /* CHECK ACCESS                  */ 04215000
         LTR   R15,R15                                             1055 04216000
         BNZ   @RF01055                                            1055 04217000
*         IORBTYPE=CURRENT;         /* GET REQUESTED FIELD           */ 04218000
         MVC   IORBTYPE(4),CURRENT(R14)                            1056 04219000
*       ELSE                        /* ACCESS NO GOOD                */ 04220000
*         IORBTYPE=ZERO;            /* ZERO FIELD                    */ 04221000
         B     @RC01055                                            1057 04222000
@RF01055 SLR   @12,@12                                             1057 04223000
