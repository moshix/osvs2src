         LR    R1,@12                                              0294 00790000
*     CALL FORMAT;                  /* FORMAT THE LINE               */ 00791000
         L     @15,ADPLFRMT(,@12)                                  0295 00792000
         BALR  @14,@15                                             0295 00793000
*     RFY                                                          0296 00794000
*       R15 RSTD;                                                  0296 00795000
*     FORMRC=FORMRC|R15;            /* SAVE RETURN CODE              */ 00796000
         OR    FORMRC,R15                                          0297 00797000
*     RFY                                                          0298 00798000
*       R15 UNRSTD;                                                0298 00799000
*     CALL PRINT;                   /* CALL PRINT ROUTINE            */ 00800000
         L     @12,ABDPLPTR                                        0299 00801000
         L     @15,ADPLPRNT(,@12)                                  0299 00802000
         BALR  @14,@15                                             0299 00803000
*     RFY                                                          0300 00804000
*      (R0,                                                        0300 00805000
*       R1) UNRSTD;                                                0300 00806000
*     RFY                                                          0301 00807000
*       R1 RSTD;                                                   0301 00808000
*     R1=ABDPLPTR;                  /* GET PARM ADDR                 */ 00809000
         L     @12,ABDPLPTR                                        0302 00810000
         LR    R1,@12                                              0302 00811000
*     CALL PRINT;                   /* CALL PRINT ROUTINE            */ 00812000
         L     @15,ADPLPRNT(,@12)                                  0303 00813000
         BALR  @14,@15                                             0303 00814000
*     RFY                                                          0304 00815000
*       R1 UNRSTD;                                                 0304 00816000
*     RFY                                                          0305 00817000
*       R15 RSTD;                                                  0305 00818000
*     R15=FORMRC;                   /* SAVE RETURN CODE FROM FORMAT  */ 00819000
         LR    R15,FORMRC                                          0306 00820000
*     RFY                                                          0307 00821000
*       R15 UNRSTD;                                                0307 00822000
*   END;                            /* ILRMFRMT (SAR)              0308 00823000
*                                      LINES(SAR1,SAR2,SAR3)SPACE  0308 00824000
*                                      FORMAT AND PRINT SART         */ 00825000
*   RFY                                                            0309 00826000
*     R14 RSTD;                                                    0309 00827000
*   R14=RTNADDR1;                   /* RESTORE RETURN ADDRESS        */ 00828000
         L     R14,RTNADDR1                                        0310 00829000
*   RFY                                                            0311 00830000
*     R14 UNRSTD;                                                  0311 00831000
*   END FMTSART;                    /* RETURN TO CALLER              */ 00832000
@EL00002 DS    0H                                                  0312 00833000
@EF00002 DS    0H                                                  0312 00834000
@ER00002 BR    @14                                                 0312 00835000
*                                                                  0313 00836000
*/********************************************************************/ 00837000
*/*                                                                  */ 00838000
*/* FMTSDCT: SEGMENT TO FORMAT SDCT AND SDCT ENTRIES                 */ 00839000
*/*                                                                  */ 00840000
*/* INPUT:   VIRTUAL ADDRESS OF SDCT IN SCDTPTR                      */ 00841000
*/*                                                                  */ 00842000
*/* PROCESS: FORMAT SDCT AND ONE ENTRY, SET COUNTER FOR NUMBER       */ 00843000
*/*           OF ENTRIES AND LOOP UNTIL ALL ENTRIES FORMATTED        */ 00844000
*/*                                                                  */ 00845000
*/* OUTPUT:  RETURN CODE = 0  IF FORMATS WERE SUCCESSFUL,            */ 00846000
*/*           OTHERWISE, RETURN CODE = FOUR                          */ 00847000
*/*                                                                  */ 00848000
*/********************************************************************/ 00849000
*                                                                  0313 00850000
*FMTSDCT:                                                          0313 00851000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0313 00852000
FMTSDCT  DS    0H                                                  0314 00853000
*                                                                  0314 00854000
*   /*****************************************************************/ 00855000
*   /*                                                               */ 00856000
*   /* SDCT1:ILRMLINE ((ID,ASIS),SIZE,RSV1) OF(SDCT) LABEL VARIABLES */ 00857000
*   /* REFERENCED IN FORMAT PATTERNS                                 */ 00858000
*   /*                                                               */ 00859000
*   /*****************************************************************/ 00860000
*                                                                  0314 00861000
*   DCL                                                            0314 00862000
*     SDCT1LB1 CHAR(LENGTH('ID')) INIT('ID');                      0314 00863000
*   DCL                                                            0315 00864000
*     SDCT1LB2 CHAR(LENGTH('SIZE')) INIT('SIZE');                  0315 00865000
*   DCL                                                            0316 00866000
*     SDCT1LB3 CHAR(LENGTH('RSV1')) INIT('RSV1');                  0316 00867000
*                                                                  0316 00868000
*   /*****************************************************************/ 00869000
*   /*                                                               */ 00870000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 00871000
*   /*                                                               */ 00872000
*   /*****************************************************************/ 00873000
*                                                                  0317 00874000
*   DCL                                                            0317 00875000
*     1 SDCT1PTS BDY(WORD),                                        0317 00876000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00877000
*       3 * BIT(8) BDY(BYTE) INIT('00101111'B),/* SEE PTTFLGS        */ 00878000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SDCID)-1+16*LENGTH('ID')-16) 00879000
*         ,                         /* SEE PTTLBLGH AND PTTDTLGH     */ 00880000
*       3 * FIXED(8) INIT(5),                                      0317 00881000
*       3 * FIXED(8) INIT(11),                                     0317 00882000
*       3 * PTR(31) INIT(ADDR(SDCT1LB1)),/* SEE PTTLBPTR             */ 00883000
*       3 SDCT1DT1 PTR(31) INIT(0), /* SEE PTTDTPTR                  */ 00884000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00885000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00886000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SDCSIZE)-1+16*LENGTH('SIZE') 00887000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 00888000
*       3 * FIXED(8) INIT(15+8),                                   0317 00889000
*       3 * FIXED(8) INIT(21+8),                                   0317 00890000
*       3 * PTR(31) INIT(ADDR(SDCT1LB2)),/* SEE PTTLBPTR             */ 00891000
*       3 SDCT1DT2 PTR(31) INIT(0), /* SEE PTTDTPTR                  */ 00892000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00893000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00894000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SDCRSV1)-1+16*LENGTH('RSV1') 00895000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 00896000
*       3 * FIXED(8) INIT(25+8+8),                                 0317 00897000
*       3 * FIXED(8) INIT(31+8+8),                                 0317 00898000
*       3 * PTR(31) INIT(ADDR(SDCT1LB3)),/* SEE PTTLBPTR             */ 00899000
*       3 SDCT1DT3 PTR(31) INIT(0), /* SEE PTTDTPTR                  */ 00900000
*      2 * FIXED(31) INIT(0);       /* END OF FORMAT PATTERNS        */ 00901000
*                                                                  0318 00902000
*   /*****************************************************************/ 00903000
*   /*                                                               */ 00904000
*   /* SDCT2:ILRMLINE ((DEVTP,ASIS),DTYPX,SLTRK,CYLSZ,CMASK,SSECT)   */ 00905000
*   /* LABEL VARIABLES REFERENCED IN FORMAT PATTERNS                 */ 00906000
*   /*                                                               */ 00907000
*   /*****************************************************************/ 00908000
*                                                                  0318 00909000
*   DCL                                                            0318 00910000
*     SDCT2LB1 CHAR(LENGTH('DEVTP')) INIT('DEVTP');                0318 00911000
*   DCL                                                            0319 00912000
*     SDCT2LB2 CHAR(LENGTH('DTYPX')) INIT('DTYPX');                0319 00913000
*   DCL                                                            0320 00914000
*     SDCT2LB3 CHAR(LENGTH('SLTRK')) INIT('SLTRK');                0320 00915000
*   DCL                                                            0321 00916000
*     SDCT2LB4 CHAR(LENGTH('CYLSZ')) INIT('CYLSZ');                0321 00917000
*   DCL                                                            0322 00918000
*     SDCT2LB5 CHAR(LENGTH('CMASK')) INIT('CMASK');                0322 00919000
*   DCL                                                            0323 00920000
*     SDCT2LB6 CHAR(LENGTH('SSECT')) INIT('SSECT');                0323 00921000
*                                                                  0323 00922000
*   /*****************************************************************/ 00923000
*   /*                                                               */ 00924000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 00925000
*   /*                                                               */ 00926000
*   /*****************************************************************/ 00927000
*                                                                  0324 00928000
*   DCL                                                            0324 00929000
*     1 SDCT2PTS BDY(WORD),                                        0324 00930000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00931000
*       3 * BIT(8) BDY(BYTE) INIT('00101111'B),/* SEE PTTFLGS        */ 00932000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SDCDEVTP)-1+16*LENGTH('DEVTP 00933000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 00934000
*       3 * FIXED(8) INIT(5),                                      0324 00935000
*       3 * FIXED(8) INIT(11),                                     0324 00936000
*       3 * PTR(31) INIT(ADDR(SDCT2LB1)),/* SEE PTTLBPTR             */ 00937000
*       3 SDCT2DT1 PTR(31) INIT(0), /* SEE PTTDTPTR                  */ 00938000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00939000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00940000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SDCDTYPX)-1+16*LENGTH('DTYPX 00941000
