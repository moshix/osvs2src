         LR    R15,FORMRC                                          0659 02341000
*       RFY                                                        0660 02342000
*         R15 UNRSTD;                                              0660 02343000
*     END;                          /* ILRMFRMT ARRAY(SAT3)          */ 02344000
*     RFY                                                          0662 02345000
*       R15 RSTD;                                                  0662 02346000
*     SATRC=SATRC|R15;              /* SAVE THE RETURN CODE          */ 02347000
         OR    SATRC,R15                                           0663 02348000
*     RFY                                                          0664 02349000
*       R15 UNRSTD;                                                0664 02350000
*     MAPPTR=MAPPTR+8*LENGTH(SATCYLMP);/* GET ADDR OF NEXT 8 WORDS   */ 02351000
         AL    MAPPTR,@CF00136                                     0665 02352000
*   END;                                                           0666 02353000
*                                                                  0666 02354000
         AH    I,@CH00041                                          0666 02355000
@DE00638 C     I,CTR                                               0666 02356000
         BNH   @DL00638                                            0666 02357000
*   /*****************************************************************/ 02358000
*   /*                                                               */ 02359000
*   /* IF THERE ARE ANY MAP ENTRIES LEFT OVER THEY MUST BE FORMATTED */ 02360000
*   /* AND THE PATTERNS MUST BE CHANGED SO A FULL LINE IS NOT        */ 02361000
*   /* PRINTED.                                                      */ 02362000
*   /*                                                               */ 02363000
*   /*****************************************************************/ 02364000
*                                                                  0667 02365000
*   IF REM^=0 THEN                  /* IS THERE A PARTIAL LINE TO  0667 02366000
*                                      FORMAT                        */ 02367000
         LTR   REM,REM                                             0667 02368000
         BZ    @RF00667                                            0667 02369000
*     DO;                                                          0668 02370000
*       PPTRNPTR=ADDR(SAT3PTS);     /* GET THE ADDRESS OF THE PATTERN*/ 02371000
         LA    PPTRNPTR,SAT3PTS                                    0669 02372000
*       RFY                                                        0670 02373000
*         PTTRNS BASED(PPTRNPTR);   /* MAP THE NAMES TO PATTERN      */ 02374000
*       DO WHILE(REM>0);            /* LOOP UNTIL ENTIRE ARRAY     0671 02375000
*                                      COMPLETE                      */ 02376000
         B     @DE00671                                            0671 02377000
@DL00671 DS    0H                                                  0672 02378000
*         PTTLBPTR=MAPPTR;          /* PUT ADDRESS OF DATA IN PATTERN*/ 02379000
         ST    MAPPTR,PTTLBPTR(,PPTRNPTR)                          0672 02380000
*         PPTRNPTR=PPTRNPTR+LENGTH(PTTSTOP)+LENGTH(PTTDTPTR);/* NEXT */ 02381000
         AH    PPTRNPTR,@CH00062                                   0673 02382000
*         MAPPTR=MAPPTR+LENGTH(SATCYLMP);/* NEXT * WORDS             */ 02383000
         LA    @12,4                                               0674 02384000
         ALR   MAPPTR,@12                                          0674 02385000
*         REM=REM-4;                /* DECREMENT THE COUNT           */ 02386000
         SLR   REM,@12                                             0675 02387000
*       END;                                                       0676 02388000
@DE00671 SLR   @12,@12                                             0676 02389000
         CR    REM,@12                                             0676 02390000
         BH    @DL00671                                            0676 02391000
*       SAVPTR=PTTSTOP;             /* SAVE THE NEXT PATTERN ENTRY   */ 02392000
         L     SAVPTR,PTTSTOP(,PPTRNPTR)                           0677 02393000
*       PTTSTOP=ZERO;               /* ZERO PATTERN TO INDICATE END  */ 02394000
         ST    @12,PTTSTOP(,PPTRNPTR)                              0678 02395000
*       RFY                                                        0679 02396000
*        (R0,                                                      0679 02397000
