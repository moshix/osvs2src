*       RFY                                                        0837 03158000
*        (R0,                                                      0837 03159000
*         R1,                                                      0837 03160000
*         R14,                                                     0837 03161000
*         R15) UNRSTD;                                             0837 03162000
@RC00834 DS    0H                                                  0838 03163000
*     END;                                                         0838 03164000
*     RFY                                                          0839 03165000
*       R15 RSTD;                                                  0839 03166000
*     SCCWRC=SCCWRC|R15;            /* SAVE THE RETURN CODE          */ 03167000
         OR    SCCWRC,R15                                          0840 03168000
*     RFY                                                          0841 03169000
*       R15 UNRSTD;                                                0841 03170000
*   END;                            /* END SCCW PROCESSING           */ 03171000
@DE00720 ICM   @12,15,SCCWPTR                                      0842 03172000
         BZ    @DC00720                                            0842 03173000
         L     @12,MAXCTR                                          0842 03174000
         C     @12,MAXSCCWS                                        0842 03175000
         BL    @DL00720                                            0842 03176000
@DC00720 DS    0H                                                  0843 03177000
*   IF SCCWPTR^=NULLPTR THEN        /* WAS THE MAX EXCEEDED          */ 03178000
         ICM   @12,15,SCCWPTR                                      0843 03179000
         BZ    @RF00843                                            0843 03180000
*     DO;                                                          0844 03181000
*       SCCWRC=4;                   /* SET THE RETURN CODE           */ 03182000
         LA    SCCWRC,4                                            0845 03183000
*       RFY                                                        0846 03184000
*         R1 RSTD;                                                 0846 03185000
*       R1=15;                      /* CODE OF MESSAGE               */ 03186000
         LA    R1,15                                               0847 03187000
*       RFY                                                        0848 03188000
*         R1 UNRSTD;                                               0848 03189000
*       CALL ILRFMSG;               /* GO TO MESSAGE SEGMENT         */ 03190000
         L     @15,@CV00674                                        0849 03191000
         BALR  @14,@15                                             0849 03192000
*     END;                                                         0850 03193000
*   RFY                                                            0851 03194000
*    (R14,                                                         0851 03195000
*     R15) RSTD;                                                   0851 03196000
@RF00843 DS    0H                                                  0852 03197000
*   R15=SCCWRC;                     /* PUT THE RETURN CODE IN REG 15 */ 03198000
         LR    R15,SCCWRC                                          0852 03199000
*   R14=RTNADDR3;                   /* RESTORE RETURN ADDRESS        */ 03200000
         LR    R14,RTNADDR3                                        0853 03201000
*   RFY                                                            0854 03202000
*    (R14,                                                         0854 03203000
*     R15) UNRSTD;                                                 0854 03204000
