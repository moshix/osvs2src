         BALR  @14,@15                                             1082 04292000
*               RFY                                                1083 04293000
*                (R0,                                              1083 04294000
*                 R1) UNRSTD;                                      1083 04295000
*               RFY                                                1084 04296000
*                 R15 RSTD;                                        1084 04297000
*               R15=FORMRC;         /* SAVE RETURN CODE FROM FORMAT  */ 04298000
         LR    R15,FORMRC                                          1085 04299000
*               RFY                                                1086 04300000
*                 R15 UNRSTD;                                      1086 04301000
*             END;                  /* ILRMFRMT (IORB) LINES(IOR1)   */ 04302000
*           END;                                                   1088 04303000
*         ELSE                                                     1089 04304000
*           DO;                                                    1089 04305000
         B     @RC01065                                            1089 04306000
@RF01065 DS    0H                                                  1090 04307000
*             DO;                   /* ILRMFRMT LINES(IOR2)          */ 04308000
*               FORMRC=0;           /* ZERO THE RETURN CODE          */ 04309000
         SLR   FORMRC,FORMRC                                       1091 04310000
