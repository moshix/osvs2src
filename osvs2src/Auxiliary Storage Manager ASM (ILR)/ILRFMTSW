*       3 * FIXED(8) INIT(15+8),                                   0324 00943000
*       3 * FIXED(8) INIT(21+8),                                   0324 00944000
*       3 * PTR(31) INIT(ADDR(SDCT2LB2)),/* SEE PTTLBPTR             */ 00945000
*       3 SDCT2DT2 PTR(31) INIT(0), /* SEE PTTDTPTR                  */ 00946000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00947000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00948000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SDCSLTRK)-1+16*LENGTH('SLTRK 00949000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 00950000
*       3 * FIXED(8) INIT(25+8+8),                                 0324 00951000
