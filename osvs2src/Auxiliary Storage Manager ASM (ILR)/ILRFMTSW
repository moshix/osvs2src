*       3 * PTR(31) INIT(ADDR(SDCT2LB3)),/* SEE PTTLBPTR             */ 00953000
*       3 SDCT2DT3 PTR(31) INIT(0), /* SEE PTTDTPTR                  */ 00954000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00955000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00956000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SDCCYLSZ)-1+16*LENGTH('CYLSZ 00957000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 00958000
*       3 * FIXED(8) INIT(35+8+8+8),                               0324 00959000
*       3 * FIXED(8) INIT(41+8+8+8),                               0324 00960000
*       3 * PTR(31) INIT(ADDR(SDCT2LB4)),/* SEE PTTLBPTR             */ 00961000
*       3 SDCT2DT4 PTR(31) INIT(0), /* SEE PTTDTPTR                  */ 00962000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00963000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00964000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SDCCMASK)-1+16*LENGTH('CMASK 00965000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 00966000
*       3 * FIXED(8) INIT(45+8+8+8+8),                             0324 00967000
*       3 * FIXED(8) INIT(51+8+8+8+8),                             0324 00968000
*       3 * PTR(31) INIT(ADDR(SDCT2LB5)),/* SEE PTTLBPTR             */ 00969000
*       3 SDCT2DT5 PTR(31) INIT(0), /* SEE PTTDTPTR                  */ 00970000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 00971000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 00972000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SDCSSECT)-1+16*LENGTH('SSECT 00973000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 00974000
*       3 * FIXED(8) INIT(55+8+8+8+8+8),                           0324 00975000
*       3 * FIXED(8) INIT(61+8+8+8+8+8),                           0324 00976000
*       3 * PTR(31) INIT(ADDR(SDCT2LB6)),/* SEE PTTLBPTR             */ 00977000
*       3 SDCT2DT6 PTR(31) INIT(0), /* SEE PTTDTPTR                  */ 00978000
*      2 * FIXED(31) INIT(0);       /* END OF FORMAT PATTERNS        */ 00979000
*   RFY                                                            0325 00980000
*     R14 RSTD;                                                    0325 00981000
*   RTNADDR1=R14;                   /* SAVE RETURN ADDRESS           */ 00982000
         ST    R14,RTNADDR1                                        0326 00983000
*   RFY                                                            0327 00984000
*     R14 UNRSTD;                                                  0327 00985000
*   ANCHTXT=ANCH;                   /* PUT LABEL IN BUFFER           */ 00986000
         L     @12,ABDPLPTR                                        0328 00987000
         L     @12,ADPLBUF(,@12)                                   0328 00988000
         MVC   ANCHTXT(11,@12),ANCH                                0328 00989000
*   ANCHNAME=ANCHSDCT;              /* PUT NAME IN THE BUFFER        */ 00990000
         MVC   ANCHNAME(8,@12),ANCHSDCT                            0329 00991000
*   RFY                                                            0330 00992000
*    (R0,                                                          0330 00993000
*     R1) RSTD;                                                    0330 00994000
*   R0=ADDR(SDCTPTR);               /* GET ADDR OF CONTROL BLOCK     */ 00995000
         LA    R0,SDCTPTR                                          0331 00996000
*   R1=ADDR(SDCTNAM);               /* GET NAME OF CONTROL BLOCK     */ 00997000
         LA    R1,SDCTNAM                                          0332 00998000
*   CALL ILRFPRTH;                  /* PRINT THE HEADING             */ 00999000
         L     @15,@CV00676                                        0333 01000000
         BALR  @14,@15                                             0333 01001000
*   RFY                                                            0334 01002000
*    (R0,                                                          0334 01003000
*     R1) UNRSTD;                                                  0334 01004000
*                                                                  0334 01005000
*   /*****************************************************************/ 01006000
*   /*                                                               */ 01007000
*   /* FORMAT THE SDCT                                               */ 01008000
*   /*                                                               */ 01009000
*   /*****************************************************************/ 01010000
*                                                                  0335 01011000
*   DO;                             /* ILRMFRMT (SDCT) LINES(SDCT1)  */ 01012000
*     FORMRC=0;                     /* ZERO THE RETURN CODE          */ 01013000
         SLR   FORMRC,FORMRC                                       0336 01014000
*     SDCT1DT1=ADDR(SDCID);                                        0337 01015000
         L     @12,SDCTPTR                                         0337 01016000
         ST    @12,SDCT1DT1                                        0337 01017000
*     SDCT1DT2=ADDR(SDCSIZE);                                      0338 01018000
         LA    @08,SDCSIZE(,@12)                                   0338 01019000
         ST    @08,SDCT1DT2                                        0338 01020000
*     SDCT1DT3=ADDR(SDCRSV1);                                      0339 01021000
         LA    @12,SDCRSV1(,@12)                                   0339 01022000
         ST    @12,SDCT1DT3                                        0339 01023000
*     RFY                                                          0340 01024000
*      (R0,                                                        0340 01025000
*       R1) RSTD;                                                  0340 01026000
*     R0=ADDR(SDCT1PTS);                                           0341 01027000
         LA    R0,SDCT1PTS                                         0341 01028000
*     R1=ABDPLPTR;                  /* GET PARM ADDR                 */ 01029000
         L     @12,ABDPLPTR                                        0342 01030000
         LR    R1,@12                                              0342 01031000
*     CALL FORMAT;                  /* FORMAT THE LINE               */ 01032000
         L     @15,ADPLFRMT(,@12)                                  0343 01033000
         BALR  @14,@15                                             0343 01034000
*     RFY                                                          0344 01035000
*       R15 RSTD;                                                  0344 01036000
*     FORMRC=FORMRC|R15;            /* SAVE RETURN CODE              */ 01037000
         OR    FORMRC,R15                                          0345 01038000
*     RFY                                                          0346 01039000
*       R15 UNRSTD;                                                0346 01040000
*     CALL PRINT;                   /* CALL PRINT ROUTINE            */ 01041000
         L     @12,ABDPLPTR                                        0347 01042000
         L     @15,ADPLPRNT(,@12)                                  0347 01043000
         BALR  @14,@15                                             0347 01044000
*     RFY                                                          0348 01045000
*      (R0,                                                        0348 01046000
*       R1) UNRSTD;                                                0348 01047000
*     RFY                                                          0349 01048000
*       R15 RSTD;                                                  0349 01049000
*     R15=FORMRC;                   /* SAVE RETURN CODE FROM FORMAT  */ 01050000
         LR    R15,FORMRC                                          0350 01051000
*     RFY                                                          0351 01052000
*       R15 UNRSTD;                                                0351 01053000
*   END;                            /* ILRMFRMT (SDCT) LINES(SDCT1)     01054000
*                                      FMT & PRINT HDR SECTION       */ 01055000
*   RFY                                                            0353 01056000
*     R15 RSTD;                                                    0353 01057000
*   SDCTRC=R15;                     /* SAVE THE RETURN CODE LOCALLY  */ 01058000
         LR    SDCTRC,R15                                          0354 01059000
*   RFY                                                            0355 01060000
*     R15 UNRSTD;                                                  0355 01061000
*                                                                  0356 01062000
*   /*****************************************************************/ 01063000
*   /*                                                               */ 01064000
*   /* GET THE NUMBER OF SDCT ENTRIES TO FORMAT THEM                 */ 01065000
*   /*                                                               */ 01066000
*   /*****************************************************************/ 01067000
*                                                                  0356 01068000
*   RFY                                                            0356 01069000
*    (R0,                                                          0356 01070000
*     R1,                                                          0356 01071000
*     R14,                                                         0356 01072000
*     R15) RSTD;                                                   0356 01073000
*   R0=ADDR(SDCSIZE);               /* ADDRESS OF THE DATA           */ 01074000
         L     @12,SDCTPTR                                         0357 01075000
         LA    R0,SDCSIZE(,@12)                                    0357 01076000
*   R1=ABDPLPTR;                    /* ADDRESS OF PARM LIST          */ 01077000
         L     @12,ABDPLPTR                                        0358 01078000
         LR    R1,@12                                              0358 01079000
*   CALL ACCESS;                    /* CALL THE ACCESS ROUTINE       */ 01080000
         L     @15,ADPLMEMA(,@12)                                  0359 01081000
         BALR  @14,@15                                             0359 01082000
*   R14=R0;                                                        0360 01083000
         LR    R14,R0                                              0360 01084000
*   IF R15=GOOD THEN                /* IS RETURN CODE GOOD           */ 01085000
         LTR   R15,R15                                             0361 01086000
         BNZ   @RF00361                                            0361 01087000
*     ENTRYCNT=HALFCURR;            /* PICK UP HALF WORD             */ 01088000
         LH    @12,HALFCURR(,R14)                                  0362 01089000
         ST    @12,ENTRYCNT                                        0362 01090000
*   ELSE                            /* RETURN CODE NOT GOOD          */ 01091000
*     ENTRYCNT=ZERO;                /* ZERO NOT AVAILABLE            */ 01092000
         B     @RC00361                                            0363 01093000
@RF00361 SLR   @12,@12                                             0363 01094000
         ST    @12,ENTRYCNT                                        0363 01095000
*   RFY                                                            0364 01096000
*    (R0,                                                          0364 01097000
*     R1,                                                          0364 01098000
*     R14,                                                         0364 01099000
*     R15) UNRSTD;                                                 0364 01100000
@RC00361 DS    0H                                                  0365 01101000
*   IF ENTRYCNT^=NULLPTR THEN       /* ACCESS WAS GOOD               */ 01102000
         ICM   @12,15,ENTRYCNT                                     0365 01103000
         BZ    @RF00365                                            0365 01104000
*     DO;                                                          0366 01105000
*       SDCTEPTR=SDCTPTR+LENGTH(SDCHDR);/* FIRST ENTRY               */ 01106000
         LA    SDCTEPTR,8                                          0367 01107000
         AL    SDCTEPTR,SDCTPTR                                    0367 01108000
*       DO I=1 TO ENTRYCNT;         /* UNTIL ALL ENTRIES FORMATTED   */ 01109000
         LA    I,1                                                 0368 01110000
         B     @DE00368                                            0368 01111000
@DL00368 DS    0H                                                  0369 01112000
*         DO;                       /* ILRMFRMT LINES(SDCT2)         */ 01113000
*           FORMRC=0;               /* ZERO THE RETURN CODE          */ 01114000
         SLR   FORMRC,FORMRC                                       0370 01115000
*           SDCT2DT1=ADDR(SDCDEVTP);                               0371 01116000
         ST    SDCTEPTR,SDCT2DT1                                   0371 01117000
*           SDCT2DT2=ADDR(SDCDTYPX);                               0372 01118000
         LA    @08,SDCDTYPX(,SDCTEPTR)                             0372 01119000
         ST    @08,SDCT2DT2                                        0372 01120000
*           SDCT2DT3=ADDR(SDCSLTRK);                               0373 01121000
         LA    @08,SDCSLTRK(,SDCTEPTR)                             0373 01122000
         ST    @08,SDCT2DT3                                        0373 01123000
*           SDCT2DT4=ADDR(SDCCYLSZ);                               0374 01124000
         LA    @08,SDCCYLSZ(,SDCTEPTR)                             0374 01125000
         ST    @08,SDCT2DT4                                        0374 01126000
*           SDCT2DT5=ADDR(SDCCMASK);                               0375 01127000
         LA    @08,SDCCMASK(,SDCTEPTR)                             0375 01128000
         ST    @08,SDCT2DT5                                        0375 01129000
*           SDCT2DT6=ADDR(SDCSSECT);                               0376 01130000
         LA    @08,SDCSSECT(,SDCTEPTR)                             0376 01131000
         ST    @08,SDCT2DT6                                        0376 01132000
*           RFY                                                    0377 01133000
*            (R0,                                                  0377 01134000
*             R1) RSTD;                                            0377 01135000
*           R0=ADDR(SDCT2PTS);                                     0378 01136000
         LA    R0,SDCT2PTS                                         0378 01137000
*           R1=ABDPLPTR;            /* GET PARM ADDR                 */ 01138000
         L     @08,ABDPLPTR                                        0379 01139000
