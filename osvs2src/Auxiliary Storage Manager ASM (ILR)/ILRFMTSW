*                                                                  0443 01473000
*   /*****************************************************************/ 01474000
*   /*                                                               */ 01475000
*   /* FORMAT THE SARTE                                              */ 01476000
*   /*                                                               */ 01477000
*   /*****************************************************************/ 01478000
*                                                                  0443 01479000
*   DO;                             /* ILRMFRMT (SRE)              0443 01480000
*                                      LINES(SRE1,SRE2,SRE3)SPACE    */ 01481000
*     FORMRC=0;                     /* ZERO THE RETURN CODE          */ 01482000
         SLR   FORMRC,FORMRC                                       0444 01483000
*     SRE1DT1=ADDR(SRENEXT);                                       0445 01484000
