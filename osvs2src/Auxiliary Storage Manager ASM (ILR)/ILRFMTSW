*         IORRC=4;                  /* SET A BAD RETURN CODE         */ 04602000
         LA    IORRC,4                                             1192 04603000
*         IORBPTR=0;                /* ZERO PTR TO TERMINATE LOOP    */ 04604000
         SLR   @12,@12                                             1193 04605000
         ST    @12,IORBPTR                                         1193 04606000
*       END;                                                       1194 04607000
*   END;                            /* END IORB PROCESSING           */ 04608000
@RC01062 DS    0H                                                  1195 04609000
