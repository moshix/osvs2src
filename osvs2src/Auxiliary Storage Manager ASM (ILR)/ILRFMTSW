*       END;                                                       0824 03112000
*     RFY                                                          0825 03113000
*       R15 RSTD;                                                  0825 03114000
@RF00815 DS    0H                                                  0826 03115000
*     SCCWRC=SCCWRC|R15;            /* SAVE THE RETURN CODE          */ 03116000
         OR    SCCWRC,R15                                          0826 03117000
*     RFY                                                          0827 03118000
*      (R0,                                                        0827 03119000
*       R1,                                                        0827 03120000
*       R2,                                                        0827 03121000
*       R15) UNRSTD;                                               0827 03122000
*                                                                  0827 03123000
*     /***************************************************************/ 03124000
*     /*                                                             */ 03125000
*     /* TRY TO GET THE NEXT SCCW ADDRESS IN THE CHAIN TO FORMAT IT. */ 03126000
*     /*                                                             */ 03127000
*     /***************************************************************/ 03128000
*                                                                  0828 03129000
*     DO;                           /* ILRMACCS (SCCWPTR)          0828 03130000
*                                      FROM(SCCWSCCW)                */ 03131000
*       RFY                                                        0829 03132000
*        (R0,                                                      0829 03133000
*         R1,                                                      0829 03134000
*         R14,                                                     0829 03135000
*         R15) RSTD;                                               0829 03136000
*       R0=ADDR(SCCWSCCW);                                         0830 03137000
         L     @12,SCCWPTR                                         0830 03138000
         LA    R0,SCCWSCCW(,@12)                                   0830 03139000
*       R1=ABDPLPTR;                /* POINT TO PARM LIST            */ 03140000
         L     @12,ABDPLPTR                                        0831 03141000
         LR    R1,@12                                              0831 03142000
*       CALL ACCESS;                /* ACCESS DATA                   */ 03143000
         L     @15,ADPLMEMA(,@12)                                  0832 03144000
         BALR  @14,@15                                             0832 03145000
*       R14=R0;                     /* CANNOT USE R0 AS BASE         */ 03146000
         LR    R14,R0                                              0833 03147000
*       IF R15=GOOD THEN            /* CHECK ACCESS                  */ 03148000
         LTR   R15,R15                                             0834 03149000
         BNZ   @RF00834                                            0834 03150000
*         SCCWPTR=CURRENT;          /* GET REQUESTED FIELD           */ 03151000
         MVC   SCCWPTR(4),CURRENT(R14)                             0835 03152000
*       ELSE                        /* ACCESS NO GOOD                */ 03153000
*         SCCWPTR=ZERO;             /* ZERO FIELD                    */ 03154000
         B     @RC00834                                            0836 03155000
@RF00834 SLR   @12,@12                                             0836 03156000
