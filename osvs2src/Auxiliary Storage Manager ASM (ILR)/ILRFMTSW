         LR    R15,FORMRC                                          0490 01610000
*     RFY                                                          0491 01611000
*       R15 UNRSTD;                                                0491 01612000
*   END;                            /* ILRMFRMT (SRE)              0492 01613000
*                                      LINES(SRE1,SRE2,SRE3)SPACE    */ 01614000
*   RFY                                                            0493 01615000
*     R15 RSTD;                                                    0493 01616000
*   SARTERC=SARTERC|R15;            /* SAVE THE RETURN CODE          */ 01617000
         L     @12,SARTERC                                         0494 01618000
         OR    @12,R15                                             0494 01619000
         ST    @12,SARTERC                                         0494 01620000
*   RFY                                                            0495 01621000
*     R15 UNRSTD;                                                  0495 01622000
*                                                                  0496 01623000
*   /*****************************************************************/ 01624000
*   /*                                                               */ 01625000
*   /* ISSUE ACCESS TO PICK UP THE ADDRESS OF THE SAT AND IF THE     */ 01626000
*   /* ACCESS IS SUCCESSFUL CALL FMTSAT TO FORMAT IT.                */ 01627000
*   /*                                                               */ 01628000
*   /*****************************************************************/ 01629000
*                                                                  0496 01630000
*   DO;                             /* ILRMACCS (SATPTR) FROM(SRESAT)*/ 01631000
*     RFY                                                          0497 01632000
*      (R0,                                                        0497 01633000
*       R1,                                                        0497 01634000
*       R14,                                                       0497 01635000
*       R15) RSTD;                                                 0497 01636000
*     R0=ADDR(SRESAT);                                             0498 01637000
         L     @12,SAREPTR                                         0498 01638000
         LA    R0,SRESAT(,@12)                                     0498 01639000
*     R1=ABDPLPTR;                  /* POINT TO PARM LIST            */ 01640000
         L     @12,ABDPLPTR                                        0499 01641000
         LR    R1,@12                                              0499 01642000
*     CALL ACCESS;                  /* ACCESS DATA                   */ 01643000
         L     @15,ADPLMEMA(,@12)                                  0500 01644000
         BALR  @14,@15                                             0500 01645000
*     R14=R0;                       /* CANNOT USE R0 AS BASE         */ 01646000
         LR    R14,R0                                              0501 01647000
*     IF R15=GOOD THEN              /* CHECK ACCESS                  */ 01648000
         LTR   R15,R15                                             0502 01649000
         BNZ   @RF00502                                            0502 01650000
*       SATPTR=CURRENT;             /* GET REQUESTED FIELD           */ 01651000
         MVC   SATPTR(4),CURRENT(R14)                              0503 01652000
*     ELSE                          /* ACCESS NO GOOD                */ 01653000
*       SATPTR=ZERO;                /* ZERO FIELD                    */ 01654000
         B     @RC00502                                            0504 01655000
@RF00502 SLR   @12,@12                                             0504 01656000
         ST    @12,SATPTR                                          0504 01657000
*     RFY                                                          0505 01658000
*      (R0,                                                        0505 01659000
*       R1,                                                        0505 01660000
*       R14,                                                       0505 01661000
*       R15) UNRSTD;                                               0505 01662000
@RC00502 DS    0H                                                  0506 01663000
*   END;                                                           0506 01664000
*   IF SATPTR^=NULLPTR THEN         /* ACCESS WAS GOOD               */ 01665000
