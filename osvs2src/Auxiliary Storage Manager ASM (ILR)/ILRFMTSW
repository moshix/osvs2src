         ST    @03,SCCW1DT2                                        0730 02800000
*       SCCW1DT3=ADDR(SCCWFLAG);                                   0731 02801000
         LA    @03,SCCWFLAG(,@12)                                  0731 02802000
         ST    @03,SCCW1DT3                                        0731 02803000
*       SCCW1DT4=ADDR(SCCWSCCW);                                   0732 02804000
