*     SRE2DT2=ADDR(SRERRCNT);                                      0461 01530000
         LA    @12,SRERRCNT(,@15)                                  0461 01531000
         ST    @12,SRE2DT2                                         0461 01532000
*     SRE2DT3=ADDR(SREIORB);                                       0462 01533000
         LA    @12,SREIORB(,@15)                                   0462 01534000
         ST    @12,SRE2DT3                                         0462 01535000
*     SRE2DT4=ADDR(SRESAT);                                        0463 01536000
         LA    @12,SRESAT(,@15)                                    0463 01537000
         ST    @12,SRE2DT4                                         0463 01538000
*     SRE2DT5=ADDR(SRESDCTE);                                      0464 01539000
         LA    @12,SRESDCTE(,@15)                                  0464 01540000
         ST    @12,SRE2DT5                                         0464 01541000
*     SRE2DT6=ADDR(SREEDB);                                        0465 01542000
         LA    @15,SREEDB(,@15)                                    0465 01543000
         ST    @15,SRE2DT6                                         0465 01544000
*     RFY                                                          0466 01545000
*      (R0,                                                        0466 01546000
*       R1) RSTD;                                                  0466 01547000
*     R0=ADDR(SRE2PTS);                                            0467 01548000
         LA    R0,SRE2PTS                                          0467 01549000
*     R1=ABDPLPTR;                  /* GET PARM ADDR                 */ 01550000
         L     @15,ABDPLPTR                                        0468 01551000
