*           CALL FORMAT;            /* FORMAT THE LINE               */ 01141000
         L     @15,ADPLFRMT(,@08)                                  0380 01142000
         BALR  @14,@15                                             0380 01143000
*           RFY                                                    0381 01144000
*             R15 RSTD;                                            0381 01145000
*           FORMRC=FORMRC|R15;      /* SAVE RETURN CODE              */ 01146000
         OR    FORMRC,R15                                          0382 01147000
*           RFY                                                    0383 01148000
*             R15 UNRSTD;                                          0383 01149000
*           CALL PRINT;             /* CALL PRINT ROUTINE            */ 01150000
         L     @08,ABDPLPTR                                        0384 01151000
         L     @15,ADPLPRNT(,@08)                                  0384 01152000
         BALR  @14,@15                                             0384 01153000
*           RFY                                                    0385 01154000
*            (R0,                                                  0385 01155000
*             R1) UNRSTD;                                          0385 01156000
*           RFY                                                    0386 01157000
*             R15 RSTD;                                            0386 01158000
*           R15=FORMRC;             /* SAVE RETURN CODE FROM FORMAT  */ 01159000
         LR    R15,FORMRC                                          0387 01160000
*           RFY                                                    0388 01161000
*             R15 UNRSTD;                                          0388 01162000
*         END;                      /* ILRMFRMT LINES(SDCT2) FORMAT &   01163000
*                                      PRINT ENTRY                   */ 01164000
*         RFY                                                      0390 01165000
*           R15 RSTD;                                              0390 01166000
*         SDCTRC=SDCTRC|R15;        /* SAVE THE RETURN CODE LOCALLY  */ 01167000
         OR    SDCTRC,R15                                          0391 01168000
*         RFY                                                      0392 01169000
*           R15 UNRSTD;                                            0392 01170000
*         SDCTEPTR=SDCTEPTR+LENGTH(SDCTE);/* CALC ADDR OF NEXT ENTRY */ 01171000
         AH    SDCTEPTR,@CH00184                                   0393 01172000
*       END;                                                       0394 01173000
         AH    I,@CH00041                                          0394 01174000
@DE00368 C     I,ENTRYCNT                                          0394 01175000
         BNH   @DL00368                                            0394 01176000
*     END;                                                         0395 01177000
*   ELSE                                                           0396 01178000
*     SDCTRC=4;                     /* ACCESS FAILED                 */ 01179000
         B     @RC00365                                            0396 01180000
@RF00365 LA    SDCTRC,4                                            0396 01181000
*   RFY                                                            0397 01182000
*     R1 RSTD;                                                     0397 01183000
@RC00365 DS    0H                                                  0398 01184000
*   R1=ABDPLPTR;                    /* GET THE ADDR OF THE PARM LIST */ 01185000
         L     @12,ABDPLPTR                                        0398 01186000
         LR    R1,@12                                              0398 01187000
*   CALL PRINT;                     /* PRINT A BLANK LINE            */ 01188000
         L     @15,ADPLPRNT(,@12)                                  0399 01189000
         BALR  @14,@15                                             0399 01190000
*   RFY                                                            0400 01191000
*     R1 UNRSTD;                                                   0400 01192000
*   RFY                                                            0401 01193000
*    (R14,                                                         0401 01194000
*     R15) RSTD;                                                   0401 01195000
*   R15=SDCTRC;                     /* PUT RETURN CODE IN REG 15     */ 01196000
         LR    R15,SDCTRC                                          0402 01197000
*   R14=RTNADDR1;                   /* RESTORE RETURN ADDRESS        */ 01198000
         L     R14,RTNADDR1                                        0403 01199000
*   RFY                                                            0404 01200000
*    (R14,                                                         0404 01201000
*     R15) UNRSTD;                                                 0404 01202000
*   END FMTSDCT;                    /* RETURN TO CALLER              */ 01203000
@EL00003 DS    0H                                                  0405 01204000
@EF00003 DS    0H                                                  0405 01205000
@ER00003 BR    @14                                                 0405 01206000
*                                                                  0406 01207000
*/********************************************************************/ 01208000
*/*                                                                  */ 01209000
*/* FMTSARTE:  SEGMENT TO FORMAT A SART ENTRY AND ITS ASSOCIATED     */ 01210000
*/*             CONTROL BLOCKS                                       */ 01211000
*/*                                                                  */ 01212000
*/* INPUT:     VIRTUAL ADDRESS OF SARTE IN SAREPTR                   */ 01213000
*/*                                                                  */ 01214000
*/* PROCESS:   IF A DATA SET NAME EXISTS FOR THIS ENTRY, PUT IT      */ 01215000
*/*             INTO THE HEADER.  THEN, FORMAT THE ENTRY.  NEXT,     */ 01216000
*/*             CALL SEGMENTS TO FORMAT THE SAT, SCCW QUEUE AND      */ 01217000
*/*             IORB QUEUE (WHICH INCLUDES IOSB AND SCCW QUEUE).     */ 01218000
*/*                                                                  */ 01219000
*/* OUTPUT:    THE CUMULATIVE RETURN CODE FROM ALL SEGMENTS CALLED   */ 01220000
*/*                                                                  */ 01221000
*/********************************************************************/ 01222000
*                                                                  0406 01223000
*FMTSARTE:                                                         0406 01224000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0406 01225000
FMTSARTE DS    0H                                                  0407 01226000
*                                                                  0407 01227000
*   /*****************************************************************/ 01228000
*   /*                                                               */ 01229000
*   /* SRE1:ILRMLINE (NEXT,LOCK,FLG,NN,SCCW,TOTSL) OF(SRE) LABEL     */ 01230000
*   /* VARIABLES REFERENCED IN FORMAT PATTERNS                       */ 01231000
*   /*                                                               */ 01232000
*   /*****************************************************************/ 01233000
*                                                                  0407 01234000
*   DCL                                                            0407 01235000
*     SRE1LB1 CHAR(LENGTH('NEXT')) INIT('NEXT');                   0407 01236000
*   DCL                                                            0408 01237000
*     SRE1LB2 CHAR(LENGTH('LOCK')) INIT('LOCK');                   0408 01238000
*   DCL                                                            0409 01239000
*     SRE1LB3 CHAR(LENGTH('FLG')) INIT('FLG');                     0409 01240000
*   DCL                                                            0410 01241000
*     SRE1LB4 CHAR(LENGTH('NN')) INIT('NN');                       0410 01242000
*   DCL                                                            0411 01243000
*     SRE1LB5 CHAR(LENGTH('SCCW')) INIT('SCCW');                   0411 01244000
*   DCL                                                            0412 01245000
*     SRE1LB6 CHAR(LENGTH('TOTSL')) INIT('TOTSL');                 0412 01246000
*                                                                  0412 01247000
*   /*****************************************************************/ 01248000
*   /*                                                               */ 01249000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 01250000
*   /*                                                               */ 01251000
*   /*****************************************************************/ 01252000
*                                                                  0413 01253000
*   DCL                                                            0413 01254000
*     1 SRE1PTS BDY(WORD),                                         0413 01255000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01256000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01257000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SRENEXT)-1+16*LENGTH('NEXT') 01258000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 01259000
*       3 * FIXED(8) INIT(5),                                      0413 01260000
*       3 * FIXED(8) INIT(11),                                     0413 01261000
*       3 * PTR(31) INIT(ADDR(SRE1LB1)),/* SEE PTTLBPTR              */ 01262000
*       3 SRE1DT1 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01263000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01264000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01265000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SRELOCK)-1+16*LENGTH('LOCK') 01266000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 01267000
*       3 * FIXED(8) INIT(15+8),                                   0413 01268000
*       3 * FIXED(8) INIT(21+8),                                   0413 01269000
*       3 * PTR(31) INIT(ADDR(SRE1LB2)),/* SEE PTTLBPTR              */ 01270000
*       3 SRE1DT2 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01271000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01272000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01273000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SREFLG)-1+16*LENGTH('FLG')-  01274000
