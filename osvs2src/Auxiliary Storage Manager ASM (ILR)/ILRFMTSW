*         RFY                                                      0819 03098000
*          (R0,                                                    0819 03099000
*           R1,                                                    0819 03100000
*           R2) RSTD;                                              0819 03101000
*         R0=AIAPTR;                /* GET THE ADDRESS OF THE FIRST     03102000
*                                      AIA                           */ 03103000
         LR    R0,AIAPTR                                           0820 03104000
*         R1=ADDR(AIANAM);          /* GET THE ADDRESS OF THE NAME   */ 03105000
         LA    R1,AIANAM                                           0821 03106000
*         R2=3;                     /* SET CODE TO CHAIN BOTH QUEUES */ 03107000
         LA    R2,3                                                0822 03108000
*         CALL ILRFAIA;             /* CALL SEGMENT TO FMT AIA Q     */ 03109000
         L     @15,@CV00675                                        0823 03110000
