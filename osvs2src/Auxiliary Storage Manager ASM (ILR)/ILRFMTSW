         BALR  @14,@15                                             0802 03042000
*     RFY                                                          0803 03043000
*      (R0,                                                        0803 03044000
*       R1) UNRSTD;                                                0803 03045000
*                                                                  0804 03046000
*     /***************************************************************/ 03047000
*     /*                                                             */ 03048000
*     /* GET THE ADDRESS OF ANY POSSIBLE AIA'S FROM THE SCCW. IF     */ 03049000
*     /* THERE ARE ANY SET UP THE ANCHOR AND CALL ILRFAIA.           */ 03050000
*     /*                                                             */ 03051000
*     /***************************************************************/ 03052000
*                                                                  0804 03053000
*     DO;                           /* ILRMACCS (AIAPTR)           0804 03054000
*                                      FROM(SCCWAIA)                 */ 03055000
*       RFY                                                        0805 03056000
*        (R0,                                                      0805 03057000
*         R1,                                                      0805 03058000
*         R14,                                                     0805 03059000
*         R15) RSTD;                                               0805 03060000
*       R0=ADDR(SCCWAIA);                                          0806 03061000
         L     @12,SCCWPTR                                         0806 03062000
         LA    R0,SCCWAIA(,@12)                                    0806 03063000
*       R1=ABDPLPTR;                /* POINT TO PARM LIST            */ 03064000
         L     @12,ABDPLPTR                                        0807 03065000
         LR    R1,@12                                              0807 03066000
*       CALL ACCESS;                /* ACCESS DATA                   */ 03067000
         L     @15,ADPLMEMA(,@12)                                  0808 03068000
         BALR  @14,@15                                             0808 03069000
*       R14=R0;                     /* CANNOT USE R0 AS BASE         */ 03070000
         LR    R14,R0                                              0809 03071000
*       IF R15=GOOD THEN            /* CHECK ACCESS                  */ 03072000
         LTR   R15,R15                                             0810 03073000
         BNZ   @RF00810                                            0810 03074000
*         AIAPTR=CURRENT;           /* GET REQUESTED FIELD           */ 03075000
         L     AIAPTR,CURRENT(,R14)                                0811 03076000
*       ELSE                        /* ACCESS NO GOOD                */ 03077000
*         AIAPTR=ZERO;              /* ZERO FIELD                    */ 03078000
         B     @RC00810                                            0812 03079000
@RF00810 SLR   AIAPTR,AIAPTR                                       0812 03080000
*       RFY                                                        0813 03081000
*        (R0,                                                      0813 03082000
*         R1,                                                      0813 03083000
*         R14,                                                     0813 03084000
*         R15) UNRSTD;                                             0813 03085000
@RC00810 DS    0H                                                  0814 03086000
*     END;                                                         0814 03087000
*     IF AIAPTR^=NULLPTR THEN       /* THE Q HAS ELEMENTS            */ 03088000
         LTR   AIAPTR,AIAPTR                                       0815 03089000
         BZ    @RF00815                                            0815 03090000
*       DO;                                                        0816 03091000
*         ANCHTXT=ANCH;             /* PUT IN LABEL                  */ 03092000
         L     @01,ABDPLPTR                                        0817 03093000
