*     CALL FORMAT;                  /* FORMAT THE LINE               */ 01510000
         L     @15,ADPLFRMT(,@15)                                  0454 01511000
         BALR  @14,@15                                             0454 01512000
*     RFY                                                          0455 01513000
*       R15 RSTD;                                                  0455 01514000
*     FORMRC=FORMRC|R15;            /* SAVE RETURN CODE              */ 01515000
         OR    FORMRC,R15                                          0456 01516000
*     RFY                                                          0457 01517000
*       R15 UNRSTD;                                                0457 01518000
*     CALL PRINT;                   /* CALL PRINT ROUTINE            */ 01519000
         L     @12,ABDPLPTR                                        0458 01520000
         L     @15,ADPLPRNT(,@12)                                  0458 01521000
         BALR  @14,@15                                             0458 01522000
*     RFY                                                          0459 01523000
*      (R0,                                                        0459 01524000
*       R1) UNRSTD;                                                0459 01525000
*     SRE2DT1=ADDR(SREAVLSL);                                      0460 01526000
         L     @15,SAREPTR                                         0460 01527000
         LA    @12,SREAVLSL(,@15)                                  0460 01528000
