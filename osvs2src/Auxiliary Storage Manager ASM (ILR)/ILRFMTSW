         TITLE 'ILRFMTSW   ASM SWAP CONTROL-BLOCK FORMATTING MODULE    *00001000
                        '                                               00002000
ILRFMTSW CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRFMTSW  76.169'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @09,0                                               0001 00011000
@PSTART  LA    @10,4095(,@09)                                      0001 00012000
         USING @PSTART,@09                                         0001 00013000
         USING @PSTART+4095,@10                                    0001 00014000
         ST    @13,@SA00001+4                                      0001 00015000
         LA    @14,@SA00001                                        0001 00016000
         ST    @14,8(,@13)                                         0001 00017000
         LR    @13,@14                                             0001 00018000
*                                                                  0106 00019000
*/********************************************************************/ 00020000
*/*                                                                  */ 00021000
*/* SWAP:     SEGMENT TO FORMAT THE SART AND ALL ASSOCIATED SWAPPING */ 00022000
*/*            CONTROL BLOCKS                                        */ 00023000
*/*                                                                  */ 00024000
*/* INPUT:    VIRTUAL ADDRESS OF THE SART IN SARTPTR                 */ 00025000
*/*                                                                  */ 00026000
*/* PROCESS:  (1) CALL SEGMENT TO FORMAT THE SART                    */ 00027000
*/*           (2) ACCESS SDCT PTR AND CALL SEGMENT TO FORMAT SDCT    */ 00028000
*/*           (3) ACCESS AND FORMAT AIA'S ON THE SART WAIT QUEUE     */ 00029000
*/*                IF THERE ARE ANY                                  */ 00030000
*/*           (4) PUT CORRESPONDING DATA SET NAME INTO HEADER WITH   */ 00031000
*/*                SART ENTRY, UNLESS THAT ENTRY IS ZERO.            */ 00032000
*/*           (5) CALL THE SEGMENT TO FORMAT THE SART ENTRY          */ 00033000
*/*           (6) DO (4) AND (5) FOR EACH ENTRY                      */ 00034000
*/*                                                                  */ 00035000
*/* OUTPUT:   THE CUMULATIVE RETURN CODE FROM ALL SEGMENTS CALLED    */ 00036000
*/*            NON-ZERO INDICATES AT LEAST ONE ERROR HAS OCCURRED IN */ 00037000
*/*            THE CHAIN OF CALLS                                    */ 00038000
*/*                                                                  */ 00039000
*/********************************************************************/ 00040000
*                                                                  0106 00041000
*   CUMRC=ZERO;                     /* INITIALIZE CUMRC TO ZERO    0106 00042000
*                                                            @ZM43066*/ 00043000
         SLR   CUMRC,CUMRC                                         0106 00044000
*   RFY                                                            0107 00045000
*    (R1,                                                          0107 00046000
*     R3) RSTD;                                                    0107 00047000
*   ASMVTPTR=R3;                    /* GET THE ADDR OF THE ASMVT     */ 00048000
         ST    R3,ASMVTPTR                                         0108 00049000
*   ABDPLPTR=R1;                    /* GET ADDR OF THE PARM LIST     */ 00050000
         ST    R1,ABDPLPTR                                         0109 00051000
*   RFY                                                            0110 00052000
*    (R1,                                                          0110 00053000
*     R3) UNRSTD;                                                  0110 00054000
*                                                                  0110 00055000
*   /*****************************************************************/ 00056000
*   /*                                                               */ 00057000
*   /* GET THE ADDRESS OF THE SART FROM THE ASMVT                    */ 00058000
*   /*                                                               */ 00059000
*   /*****************************************************************/ 00060000
*                                                                  0111 00061000
*   DO;                             /* ILRMACCS (SART) VIA(ASMSART)  */ 00062000
*     RFY                                                          0112 00063000
*      (R0,                                                        0112 00064000
*       R1,                                                        0112 00065000
*       R14,                                                       0112 00066000
*       R15) RSTD;                                                 0112 00067000
*     R1=ABDPLPTR;                                                 0113 00068000
         L     @12,ABDPLPTR                                        0113 00069000
         LR    R1,@12                                              0113 00070000
*     R0=ADDR(ASMSART);             /* ADDR OF CELL WITH PTR         */ 00071000
         L     @08,ASMVTPTR                                        0114 00072000
         LA    R0,ASMSART(,@08)                                    0114 00073000
*     CALL ACCESS;                  /* ACCESS PTR TO SART            */ 00074000
         L     @15,ADPLMEMA(,@12)                                  0115 00075000
         BALR  @14,@15                                             0115 00076000
*     IF R15=GOOD THEN                                             0116 00077000
         LTR   R15,R15                                             0116 00078000
         BNZ   @RF00116                                            0116 00079000
*       DO;                         /* FOUND PTR - NOW FIND BLOCK    */ 00080000
*         R14=R0;                   /* CANNOT USE R0 AS BASE REG     */ 00081000
         LR    R14,R0                                              0118 00082000
*         R0=CURRENT;               /* POINTER ITSELF                */ 00083000
         L     R0,CURRENT(,R14)                                    0119 00084000
*         SARTPTR=R0;               /* SAVE POINTER                  */ 00085000
         ST    R0,SARTPTR                                          0120 00086000
*         CALL ACCESS;              /* ACCESS SART ITSELF            */ 00087000
         L     @12,ABDPLPTR                                        0121 00088000
         L     @15,ADPLMEMA(,@12)                                  0121 00089000
         BALR  @14,@15                                             0121 00090000
*         IF R15^=GOOD THEN                                        0122 00091000
         SLR   @12,@12                                             0122 00092000
         CR    R15,@12                                             0122 00093000
         BE    @RF00122                                            0122 00094000
*           SARTPTR=NULLPTR;                                       0123 00095000
         ST    @12,SARTPTR                                         0123 00096000
*       END;                                                       0124 00097000
*     ELSE                                                         0125 00098000
*       SARTPTR=NULLPTR;                                           0125 00099000
         B     @RC00116                                            0125 00100000
@RF00116 SLR   @12,@12                                             0125 00101000
         ST    @12,SARTPTR                                         0125 00102000
*     RFY                                                          0126 00103000
*      (R0,                                                        0126 00104000
*       R1,                                                        0126 00105000
*       R14,                                                       0126 00106000
*       R15) UNRSTD;                                               0126 00107000
@RC00116 DS    0H                                                  0127 00108000
*   END;                            /* GET ADDR OF SART      @ZA11398*/ 00109000
*                                                                  0128 00110000
*   /*****************************************************************/ 00111000
*   /*                                                               */ 00112000
*   /* CHECK TO SEE IF A POINTER WAS RETURNED. IF NOT THE SART IS NOT*/ 00113000
*   /* ACCESSIBLE AND A MESSAGE WILL BE PRINTED. IF THE SART IS      */ 00114000
*   /* AVAILABLE CALL FMTSART TO FORMAT IT.                          */ 00115000
*   /*                                                               */ 00116000
*   /*****************************************************************/ 00117000
*                                                                  0128 00118000
*   IF SARTPTR^=NULLPTR THEN        /* ACCESS WAS SUCCESSFUL         */ 00119000
         ICM   @12,15,SARTPTR                                      0128 00120000
         BZ    @RF00128                                            0128 00121000
*     DO;                           /* CONTINUE                      */ 00122000
*       CALL FMTSART;               /* CALL SEGMENT TO FMT SART      */ 00123000
         BAL   @14,FMTSART                                         0130 00124000
*       RFY                                                        0131 00125000
*         R15 RSTD;                                                0131 00126000
*       IF R15^=0 THEN              /* IS RETURN CODE GOOD           */ 00127000
         LTR   R15,R15                                             0132 00128000
         BNZ   @RT00132                                            0132 00129000
*         GO TO NOSART;             /* NO PROCESS MESSAGE            */ 00130000
*       RFY                                                        0134 00131000
*         R15 UNRSTD;                                              0134 00132000
*                                                                  0134 00133000
*       /*************************************************************/ 00134000
*       /*                                                           */ 00135000
*       /* GET THE ADDRESS OF THE SDCT AND IF IT IS AVAILABLE CALL   */ 00136000
*       /* FMTSDCT FOR FORMATTING.                                   */ 00137000
*       /*                                                           */ 00138000
*       /*************************************************************/ 00139000
*                                                                  0135 00140000
*       DO;                         /* ILRMACCS (SDCTPTR)          0135 00141000
*                                      FROM(SARSDCT)                 */ 00142000
*         RFY                                                      0136 00143000
*          (R0,                                                    0136 00144000
*           R1,                                                    0136 00145000
*           R14,                                                   0136 00146000
*           R15) RSTD;                                             0136 00147000
*         R0=ADDR(SARSDCT);                                        0137 00148000
         L     @12,SARTPTR                                         0137 00149000
         LA    R0,SARSDCT(,@12)                                    0137 00150000
*         R1=ABDPLPTR;              /* POINT TO PARM LIST            */ 00151000
         L     @12,ABDPLPTR                                        0138 00152000
         LR    R1,@12                                              0138 00153000
*         CALL ACCESS;              /* ACCESS DATA                   */ 00154000
         L     @15,ADPLMEMA(,@12)                                  0139 00155000
         BALR  @14,@15                                             0139 00156000
*         R14=R0;                   /* CANNOT USE R0 AS BASE         */ 00157000
         LR    R14,R0                                              0140 00158000
*         IF R15=GOOD THEN          /* CHECK ACCESS                  */ 00159000
         LTR   R15,R15                                             0141 00160000
         BNZ   @RF00141                                            0141 00161000
*           SDCTPTR=CURRENT;        /* GET REQUESTED FIELD           */ 00162000
         MVC   SDCTPTR(4),CURRENT(R14)                             0142 00163000
*         ELSE                      /* ACCESS NO GOOD                */ 00164000
*           SDCTPTR=ZERO;           /* ZERO FIELD                    */ 00165000
         B     @RC00141                                            0143 00166000
@RF00141 SLR   @12,@12                                             0143 00167000
         ST    @12,SDCTPTR                                         0143 00168000
*         RFY                                                      0144 00169000
*          (R0,                                                    0144 00170000
*           R1,                                                    0144 00171000
*           R14,                                                   0144 00172000
*           R15) UNRSTD;                                           0144 00173000
@RC00141 DS    0H                                                  0145 00174000
*       END;                                                       0145 00175000
*       IF SDCTPTR^=NULLPTR THEN    /* ACCESS WAS SUCCESSFUL         */ 00176000
         ICM   @12,15,SDCTPTR                                      0146 00177000
         BZ    @RF00146                                            0146 00178000
*         CALL FMTSDCT;             /* CALL SEGMENT TO FMT SDCT      */ 00179000
         BAL   @14,FMTSDCT                                         0147 00180000
*       RFY                                                        0148 00181000
*         R15 RSTD;                                                0148 00182000
@RF00146 DS    0H                                                  0149 00183000
*       CUMRC=CUMRC|R15;            /* SAVE RETURN CODE              */ 00184000
         OR    CUMRC,R15                                           0149 00185000
*       RFY                                                        0150 00186000
*         R15 UNRSTD;                                              0150 00187000
*                                                                  0150 00188000
*       /*************************************************************/ 00189000
*       /*                                                           */ 00190000
*       /* GET THE FIRST AIA FROM THE SARWAITQ. IF ACCESS WAS        */ 00191000
*       /* SUCCESSFUL CHECK IF ANYTHING WAS ON THE QUEUE. IF THERE IS*/ 00192000
*       /* AN AIA SET UP THE ANCHOR AND CALL ILRFAIA.                */ 00193000
*       /*                                                           */ 00194000
*       /*************************************************************/ 00195000
*                                                                  0151 00196000
*       DO;                         /* ILRMACCS (AIAPTR)           0151 00197000
*                                      FROM(SARWAITF)                */ 00198000
*         RFY                                                      0152 00199000
*          (R0,                                                    0152 00200000
*           R1,                                                    0152 00201000
*           R14,                                                   0152 00202000
*           R15) RSTD;                                             0152 00203000
*         R0=ADDR(SARWAITF);                                       0153 00204000
         L     @12,SARTPTR                                         0153 00205000
         LA    R0,SARWAITF(,@12)                                   0153 00206000
*         R1=ABDPLPTR;              /* POINT TO PARM LIST            */ 00207000
         L     @12,ABDPLPTR                                        0154 00208000
         LR    R1,@12                                              0154 00209000
*         CALL ACCESS;              /* ACCESS DATA                   */ 00210000
         L     @15,ADPLMEMA(,@12)                                  0155 00211000
         BALR  @14,@15                                             0155 00212000
*         R14=R0;                   /* CANNOT USE R0 AS BASE         */ 00213000
         LR    R14,R0                                              0156 00214000
*         IF R15=GOOD THEN          /* CHECK ACCESS                  */ 00215000
         LTR   R15,R15                                             0157 00216000
         BNZ   @RF00157                                            0157 00217000
*           AIAPTR=CURRENT;         /* GET REQUESTED FIELD           */ 00218000
         L     AIAPTR,CURRENT(,R14)                                0158 00219000
*         ELSE                      /* ACCESS NO GOOD                */ 00220000
*           AIAPTR=ZERO;            /* ZERO FIELD                    */ 00221000
         B     @RC00157                                            0159 00222000
@RF00157 SLR   AIAPTR,AIAPTR                                       0159 00223000
*         RFY                                                      0160 00224000
*          (R0,                                                    0160 00225000
*           R1,                                                    0160 00226000
*           R14,                                                   0160 00227000
*           R15) UNRSTD;                                           0160 00228000
@RC00157 DS    0H                                                  0161 00229000
*       END;                                                       0161 00230000
*       IF AIAPTR^=NULLPTR THEN     /* THE QUEUE IS NOT EMPTY        */ 00231000
         LTR   AIAPTR,AIAPTR                                       0162 00232000
         BZ    @RF00162                                            0162 00233000
*         DO;                       /* CONTINUE                      */ 00234000
*           ANCHTXT=ANCH;           /* PUT LABEL IN BUFFER           */ 00235000
         L     @08,ABDPLPTR                                        0164 00236000
         L     @08,ADPLBUF(,@08)                                   0164 00237000
         MVC   ANCHTXT(11,@08),ANCH                                0164 00238000
*           ANCHNAME=ANCHAIA4;      /* PUT ANCHOR LOCATION IN BUFFER */ 00239000
         MVC   ANCHNAME(8,@08),ANCHAIA4                            0165 00240000
*           RFY                                                    0166 00241000
*            (R0,                                                  0166 00242000
*             R1,                                                  0166 00243000
*             R2) RSTD;                                            0166 00244000
*           R0=AIAPTR;              /* GET THE ADDRESS OF THE AIA    */ 00245000
         LR    R0,AIAPTR                                           0167 00246000
*           R1=ADDR(AIANAM);        /* ADDRESS OF THE NAME OF BLOCK  */ 00247000
         LA    R1,AIANAM                                           0168 00248000
*           R2=3;                   /* CODE FOR THE AIA ROUTINE      */ 00249000
         LA    R2,3                                                0169 00250000
*           RFY                                                    0170 00251000
*            (R0,                                                  0170 00252000
*             R1,                                                  0170 00253000
*             R2) UNRSTD;                                          0170 00254000
*           CALL ILRFAIA;           /* CALL SEGMENT TO FMT AIA Q     */ 00255000
         L     @15,@CV00675                                        0171 00256000
         BALR  @14,@15                                             0171 00257000
*         END;                                                     0172 00258000
*       RFY                                                        0173 00259000
*         R15 RSTD;                                                0173 00260000
@RF00162 DS    0H                                                  0174 00261000
*       CUMRC=CUMRC|R15;            /* SAVE THE RETURN CODE          */ 00262000
         OR    CUMRC,R15                                           0174 00263000
*       RFY                                                        0175 00264000
*         R15 UNRSTD;                                              0175 00265000
*                                                                  0175 00266000
*       /*************************************************************/ 00267000
*       /*                                                           */ 00268000
*       /* GET THE NUMBER OF SARTE'S FOR A LOOP. IF THE ACCESS IS    */ 00269000
*       /* GOOD GET THE ADDRESS OF THE FIRST SARTE AND DO AN ACCESS. */ 00270000
*       /* FOR THE DSNAME LIST POINTER. IF THE LIST IS NOT AVAILABLE */ 00271000
*       /* DSNPTR WILL HAVE A 0 IN IT AND FMTSARTE WILL NOT PRINT OUT*/ 00272000
*       /* THE DATA SET NAME.                                        */ 00273000
*       /*                                                           */ 00274000
*       /*************************************************************/ 00275000
*                                                                  0176 00276000
*       DO;                         /* ILRMACCS (NUMSARTE)         0176 00277000
*                                      FROM(SARSIZE)                 */ 00278000
*         RFY                                                      0177 00279000
*          (R0,                                                    0177 00280000
*           R1,                                                    0177 00281000
*           R14,                                                   0177 00282000
*           R15) RSTD;                                             0177 00283000
*         R0=ADDR(SARSIZE);                                        0178 00284000
         L     @12,SARTPTR                                         0178 00285000
         LA    R0,SARSIZE(,@12)                                    0178 00286000
*         R1=ABDPLPTR;              /* POINT TO PARM LIST            */ 00287000
         L     @12,ABDPLPTR                                        0179 00288000
         LR    R1,@12                                              0179 00289000
*         CALL ACCESS;              /* ACCESS DATA                   */ 00290000
         L     @15,ADPLMEMA(,@12)                                  0180 00291000
         BALR  @14,@15                                             0180 00292000
*         R14=R0;                   /* CANNOT USE R0 AS BASE         */ 00293000
         LR    R14,R0                                              0181 00294000
*         IF R15=GOOD THEN          /* CHECK ACCESS                  */ 00295000
         LTR   R15,R15                                             0182 00296000
         BNZ   @RF00182                                            0182 00297000
*           NUMSARTE=CURRENT;       /* GET REQUESTED FIELD           */ 00298000
         MVC   NUMSARTE(4),CURRENT(R14)                            0183 00299000
*         ELSE                      /* ACCESS NO GOOD                */ 00300000
*           NUMSARTE=ZERO;          /* ZERO FIELD                    */ 00301000
         B     @RC00182                                            0184 00302000
@RF00182 SLR   @12,@12                                             0184 00303000
         ST    @12,NUMSARTE                                        0184 00304000
*         RFY                                                      0185 00305000
*          (R0,                                                    0185 00306000
*           R1,                                                    0185 00307000
*           R14,                                                   0185 00308000
*           R15) UNRSTD;                                           0185 00309000
@RC00182 DS    0H                                                  0186 00310000
*       END;                        /* GET NUM SART ENTRIES          */ 00311000
*       RFY                                                        0187 00312000
*         R15 RSTD;                                                0187 00313000
*       CUMRC=CUMRC|R15;            /* SAVE THE RETURN CODE          */ 00314000
         OR    CUMRC,R15                                           0188 00315000
*       RFY                                                        0189 00316000
*         R15 UNRSTD;                                              0189 00317000
*       IF NUMSARTE^=NULLPTR THEN   /* ACCESS WAS GOOD               */ 00318000
         ICM   @12,15,NUMSARTE                                     0190 00319000
         BZ    @RF00190                                            0190 00320000
*         DO;                                                      0191 00321000
*           SAREPTR=ADDR(SARENTS);  /* ADDR OF FIRST SART ENTRY      */ 00322000
         L     @12,SARTPTR                                         0192 00323000
         LA    @08,SARENTS(,@12)                                   0192 00324000
         ST    @08,SAREPTR                                         0192 00325000
*           DO;                     /* ILRMACCS (DSNPTR)           0193 00326000
*                                      FROM(SARDSNL)                 */ 00327000
*             RFY                                                  0194 00328000
*              (R0,                                                0194 00329000
*               R1,                                                0194 00330000
*               R14,                                               0194 00331000
*               R15) RSTD;                                         0194 00332000
*             R0=ADDR(SARDSNL);                                    0195 00333000
         LA    R0,SARDSNL(,@12)                                    0195 00334000
*             R1=ABDPLPTR;          /* POINT TO PARM LIST            */ 00335000
         L     @12,ABDPLPTR                                        0196 00336000
         LR    R1,@12                                              0196 00337000
*             CALL ACCESS;          /* ACCESS DATA                   */ 00338000
         L     @15,ADPLMEMA(,@12)                                  0197 00339000
         BALR  @14,@15                                             0197 00340000
*             R14=R0;               /* CANNOT USE R0 AS BASE         */ 00341000
         LR    R14,R0                                              0198 00342000
*             IF R15=GOOD THEN      /* CHECK ACCESS                  */ 00343000
         LTR   R15,R15                                             0199 00344000
         BNZ   @RF00199                                            0199 00345000
*               DSNPTR=CURRENT;     /* GET REQUESTED FIELD           */ 00346000
         MVC   DSNPTR(4),CURRENT(R14)                              0200 00347000
*             ELSE                  /* ACCESS NO GOOD                */ 00348000
*               DSNPTR=ZERO;        /* ZERO FIELD                    */ 00349000
         B     @RC00199                                            0201 00350000
@RF00199 SLR   @12,@12                                             0201 00351000
         ST    @12,DSNPTR                                          0201 00352000
*             RFY                                                  0202 00353000
*              (R0,                                                0202 00354000
*               R1,                                                0202 00355000
*               R14,                                               0202 00356000
*               R15) UNRSTD;                                       0202 00357000
@RC00199 DS    0H                                                  0203 00358000
*           END;                    /* GET DATA SET NAME             */ 00359000
*           IF DSNPTR^=NULLPTR THEN                                0204 00360000
         L     @12,DSNPTR                                          0204 00361000
         LTR   @12,@12                                             0204 00362000
         BZ    @RF00204                                            0204 00363000
*             DO;                   /* DSN TABLE EXIST?      @ZA11398*/ 00364000
*               RFY                                                0206 00365000
*                (R0,                                              0206 00366000
*                 R1,                                              0206 00367000
*                 R15) RSTD;        /*                       @ZA11398*/ 00368000
*               R0=DSNPTR;          /* INPUT TO ACCESS       @ZA11398*/ 00369000
         LR    R0,@12                                              0207 00370000
*               R1=ABDPLPTR;        /* INPUT TO ACCESS       @ZA11398*/ 00371000
         L     @14,ABDPLPTR                                        0208 00372000
         LR    R1,@14                                              0208 00373000
*               CALL ACCESS;        /* DSN TABLE ACCESSIBLE? @ZA11398*/ 00374000
         L     @15,ADPLMEMA(,@14)                                  0209 00375000
         BALR  @14,@15                                             0209 00376000
*               IF R15^=GOOD THEN                                  0210 00377000
         SLR   @14,@14                                             0210 00378000
         CR    R15,@14                                             0210 00379000
         BE    @RF00210                                            0210 00380000
*                 DSNPTR=NULLPTR;   /* INACCESSIBLE          @ZA11398*/ 00381000
         ST    @14,DSNPTR                                          0211 00382000
*               RFY                                                0212 00383000
*                (R0,                                              0212 00384000
*                 R1) UNRSTD;       /*                       @ZA11398*/ 00385000
@RF00210 DS    0H                                                  0213 00386000
*             END;                  /* DSN TABLE EXIST       @ZA11398*/ 00387000
*           CUMRC=CUMRC|R15;                                       0214 00388000
@RF00204 OR    CUMRC,R15                                           0214 00389000
*           RFY                                                    0215 00390000
*             R15 UNRSTD;                                          0215 00391000
*                                                                  0215 00392000
*           /*********************************************************/ 00393000
*           /*                                                       */ 00394000
*           /* LOOP UNTIL ALL THE SARTE'S ARE FORMATTED AND SAVE THE */ 00395000
*           /* RETURN CODE.                                          */ 00396000
*           /*                                                       */ 00397000
*           /*********************************************************/ 00398000
*                                                                  0216 00399000
*           DO J=1 TO NUMSARTE;     /* UNTIL ALL ENTRIES PROCESSED   */ 00400000
         LA    @12,1                                               0216 00401000
         B     @DE00216                                            0216 00402000
@DL00216 DS    0H                                                  0217 00403000
*             CALL FMTSARTE;        /* CALL SEGMENT TO FMT SARTE     */ 00404000
         BAL   @14,FMTSARTE                                        0217 00405000
*             RFY                                                  0218 00406000
*               R15 RSTD;                                          0218 00407000
*             CUMRC=CUMRC|R15;      /* SAVE RETURN CODE              */ 00408000
         OR    CUMRC,R15                                           0219 00409000
*             RFY                                                  0220 00410000
*               R15 UNRSTD;                                        0220 00411000
*             IF DSNPTR^=0 THEN     /* WAS ACCESS SUCCESSFUL         */ 00412000
         L     @12,DSNPTR                                          0221 00413000
         LTR   @12,@12                                             0221 00414000
         BZ    @RF00221                                            0221 00415000
*               DSNPTR=DSNPTR+LENGTH(DSNAME);/* CALCULATE ADDRESS OF    00416000
*                                      NEXT DSNAME                   */ 00417000
         AH    @12,@CH00787                                        0222 00418000
         ST    @12,DSNPTR                                          0222 00419000
*             SAREPTR=SAREPTR+LENGTH(SARTE);/* GET NEXT SARTE      0223 00420000
*                                      CALCULATE SARTE ADDR          */ 00421000
@RF00221 LA    @12,48                                              0223 00422000
         AL    @12,SAREPTR                                         0223 00423000
         ST    @12,SAREPTR                                         0223 00424000
*           END;                    /* END SARTE PROCESSING          */ 00425000
         LA    @12,1                                               0224 00426000
         AL    @12,J                                               0224 00427000
@DE00216 ST    @12,J                                               0224 00428000
         C     @12,NUMSARTE                                        0224 00429000
         BNH   @DL00216                                            0224 00430000
*         END;                      /* END WHEN GOT SART NUM         */ 00431000
*       ELSE                        /* ACCESS FAILED FOR SARTE       */ 00432000
*         CUMRC=4;                  /* SET THE RETURN CODE-SARTE   0226 00433000
*                                      ACCESS                        */ 00434000
         B     @RC00190                                            0226 00435000
@RF00190 LA    CUMRC,4                                             0226 00436000
*     END;                          /* END SART PROCESSING           */ 00437000
*   ELSE                            /* ACCESS FAILED FOR SART        */ 00438000
*NOSART:                                                           0228 00439000
