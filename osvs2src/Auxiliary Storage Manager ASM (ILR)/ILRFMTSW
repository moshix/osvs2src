         ICM   @12,15,IORBPTR                                      0545 01794000
         BZ    @RF00545                                            0545 01795000
*     DO;                                                          0546 01796000
*       ANCHTXT=ANCH;               /* PUT LABEL IN BUFFER           */ 01797000
         L     @12,ABDPLPTR                                        0547 01798000
         L     @12,ADPLBUF(,@12)                                   0547 01799000
         MVC   ANCHTXT(11,@12),ANCH                                0547 01800000
*       ANCHNAME=ANCHIOR2;          /* PUT ANCHOR NAME IN BUFFER     */ 01801000
         MVC   ANCHNAME(8,@12),ANCHIOR2                            0548 01802000
*       CALL FMTIORB;               /* CALL SEGMENT TO FMT IORB Q    */ 01803000
         BAL   @14,FMTIORB                                         0549 01804000
*     END;                                                         0550 01805000
*   RFY                                                            0551 01806000
*     R15 RSTD;                                                    0551 01807000
@RF00545 DS    0H                                                  0552 01808000
*   SARTERC=SARTERC|R15;            /* SAVE THE RETURN CODE          */ 01809000
         L     @14,SARTERC                                         0552 01810000
         OR    @14,R15                                             0552 01811000
         ST    @14,SARTERC                                         0552 01812000
*   RFY                                                            0553 01813000
*     R15 UNRSTD;                                                  0553 01814000
*   RFY                                                            0554 01815000
*    (R14,                                                         0554 01816000
*     R15) RSTD;                                                   0554 01817000
*   R15=SARTERC;                    /* PUT THE RETURN CODE IN REG 15 */ 01818000
         L     @12,SARTERC                                         0555 01819000
         LR    R15,@12                                             0555 01820000
*   R14=RTNADDR1;                   /* RESTORE RETURN ADDRESS        */ 01821000
         L     R14,RTNADDR1                                        0556 01822000
*   RFY                                                            0557 01823000
*    (R14,                                                         0557 01824000
*     R15) UNRSTD;                                                 0557 01825000
*   END FMTSARTE;                   /* RETURN TO CALLER              */ 01826000
@EL00004 DS    0H                                                  0558 01827000
@EF00004 DS    0H                                                  0558 01828000
@ER00004 BR    @14                                                 0558 01829000
*                                                                  0559 01830000
*/********************************************************************/ 01831000
*/*                                                                  */ 01832000
*/* FMTSAT:  SEGMENT TO FORMAT THE SAT                               */ 01833000
*/*                                                                  */ 01834000
*/* INPUT:   VIRTUAL ADDRESS IN SATPTR                               */ 01835000
*/*                                                                  */ 01836000
*/* PROCESS: FORMAT THE SAT HEADER SECTION, CALCULATE THE SIZE       */ 01837000
*/*           FOR THE SATMAP AND FORMAT THE PATMAP.                  */ 01838000
*/*                                                                  */ 01839000
*/* OUTPUT:  RETURN CODE = 0 IF ALL FORMATS SUCCEED, OTHERWISE,      */ 01840000
*/*           RETURN CODE > ZERO.                                    */ 01841000
*/*                                                                  */ 01842000
*/********************************************************************/ 01843000
*                                                                  0559 01844000
*FMTSAT:                                                           0559 01845000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0559 01846000
FMTSAT   DS    0H                                                  0560 01847000
*                                                                  0560 01848000
*   /*****************************************************************/ 01849000
*   /*                                                               */ 01850000
*   /* SAT1:ILRMLINE ((ID,ASIS),SARTE,MAPLN,BYTCL,OFFST,MASK) OF(SAT)*/ 01851000
*   /* LABEL VARIABLES REFERENCED IN FORMAT PATTERNS                 */ 01852000
*   /*                                                               */ 01853000
*   /*****************************************************************/ 01854000
*                                                                  0560 01855000
*   DCL                                                            0560 01856000
*     SAT1LB1 CHAR(LENGTH('ID')) INIT('ID');                       0560 01857000
*   DCL                                                            0561 01858000
*     SAT1LB2 CHAR(LENGTH('SARTE')) INIT('SARTE');                 0561 01859000
*   DCL                                                            0562 01860000
*     SAT1LB3 CHAR(LENGTH('MAPLN')) INIT('MAPLN');                 0562 01861000
*   DCL                                                            0563 01862000
*     SAT1LB4 CHAR(LENGTH('BYTCL')) INIT('BYTCL');                 0563 01863000
*   DCL                                                            0564 01864000
*     SAT1LB5 CHAR(LENGTH('OFFST')) INIT('OFFST');                 0564 01865000
*   DCL                                                            0565 01866000
*     SAT1LB6 CHAR(LENGTH('MASK')) INIT('MASK');                   0565 01867000
*                                                                  0565 01868000
*   /*****************************************************************/ 01869000
*   /*                                                               */ 01870000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 01871000
*   /*                                                               */ 01872000
*   /*****************************************************************/ 01873000
*                                                                  0566 01874000
*   DCL                                                            0566 01875000
*     1 SAT1PTS BDY(WORD),                                         0566 01876000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01877000
*       3 * BIT(8) BDY(BYTE) INIT('00101111'B),/* SEE PTTFLGS        */ 01878000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SATID)-1+16*LENGTH('ID')-16) 01879000
*         ,                         /* SEE PTTLBLGH AND PTTDTLGH     */ 01880000
*       3 * FIXED(8) INIT(5),                                      0566 01881000
*       3 * FIXED(8) INIT(11),                                     0566 01882000
*       3 * PTR(31) INIT(ADDR(SAT1LB1)),/* SEE PTTLBPTR              */ 01883000
*       3 SAT1DT1 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01884000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01885000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01886000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SATSARTE)-1+16*LENGTH('SARTE 01887000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 01888000
*       3 * FIXED(8) INIT(15+8),                                   0566 01889000
*       3 * FIXED(8) INIT(21+8),                                   0566 01890000
*       3 * PTR(31) INIT(ADDR(SAT1LB2)),/* SEE PTTLBPTR              */ 01891000
*       3 SAT1DT2 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01892000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01893000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01894000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SATMAPLN)-1+16*LENGTH('MAPLN 01895000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 01896000
*       3 * FIXED(8) INIT(25+8+8),                                 0566 01897000
*       3 * FIXED(8) INIT(31+8+8),                                 0566 01898000
*       3 * PTR(31) INIT(ADDR(SAT1LB3)),/* SEE PTTLBPTR              */ 01899000
*       3 SAT1DT3 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01900000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01901000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01902000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SATBYTCL)-1+16*LENGTH('BYTCL 01903000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 01904000
*       3 * FIXED(8) INIT(35+8+8+8),                               0566 01905000
*       3 * FIXED(8) INIT(41+8+8+8),                               0566 01906000
*       3 * PTR(31) INIT(ADDR(SAT1LB4)),/* SEE PTTLBPTR              */ 01907000
*       3 SAT1DT4 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01908000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01909000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01910000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SATOFFST)-1+16*LENGTH('OFFST 01911000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 01912000
*       3 * FIXED(8) INIT(45+8+8+8+8),                             0566 01913000
*       3 * FIXED(8) INIT(51+8+8+8+8),                             0566 01914000
*       3 * PTR(31) INIT(ADDR(SAT1LB5)),/* SEE PTTLBPTR              */ 01915000
*       3 SAT1DT5 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01916000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01917000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01918000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SATMASK)-1+16*LENGTH('MASK') 01919000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 01920000
*       3 * FIXED(8) INIT(55+8+8+8+8+8),                           0566 01921000
*       3 * FIXED(8) INIT(61+8+8+8+8+8),                           0566 01922000
*       3 * PTR(31) INIT(ADDR(SAT1LB6)),/* SEE PTTLBPTR              */ 01923000
*       3 SAT1DT6 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01924000
*      2 * FIXED(31) INIT(0);       /* END OF FORMAT PATTERNS        */ 01925000
*                                                                  0567 01926000
*   /*****************************************************************/ 01927000
*   /*                                                               */ 01928000
*   /* SAT2:ILRMLINE (SCAN,SLTNO,ASGN) LABEL VARIABLES REFERENCED IN */ 01929000
*   /* FORMAT PATTERNS                                               */ 01930000
*   /*                                                               */ 01931000
*   /*****************************************************************/ 01932000
*                                                                  0567 01933000
*   DCL                                                            0567 01934000
*     SAT2LB1 CHAR(LENGTH('SCAN')) INIT('SCAN');                   0567 01935000
*   DCL                                                            0568 01936000
*     SAT2LB2 CHAR(LENGTH('SLTNO')) INIT('SLTNO');                 0568 01937000
*   DCL                                                            0569 01938000
*     SAT2LB3 CHAR(LENGTH('ASGN')) INIT('ASGN');                   0569 01939000
*                                                                  0569 01940000
*   /*****************************************************************/ 01941000
*   /*                                                               */ 01942000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */ 01943000
*   /*                                                               */ 01944000
*   /*****************************************************************/ 01945000
*                                                                  0570 01946000
*   DCL                                                            0570 01947000
*     1 SAT2PTS BDY(WORD),                                         0570 01948000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01949000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01950000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SATSCAN)-1+16*LENGTH('SCAN') 01951000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 01952000
*       3 * FIXED(8) INIT(5),                                      0570 01953000
*       3 * FIXED(8) INIT(11),                                     0570 01954000
*       3 * PTR(31) INIT(ADDR(SAT2LB1)),/* SEE PTTLBPTR              */ 01955000
*       3 SAT2DT1 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01956000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01957000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01958000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SATSLTNO)-1+16*LENGTH('SLTNO 01959000
*             )-16),                /* SEE PTTLBLGH AND PTTDTLGH     */ 01960000
*       3 * FIXED(8) INIT(15+8),                                   0570 01961000
*       3 * FIXED(8) INIT(21+8),                                   0570 01962000
*       3 * PTR(31) INIT(ADDR(SAT2LB2)),/* SEE PTTLBPTR              */ 01963000
*       3 SAT2DT2 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01964000
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */ 01965000
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */ 01966000
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SATASGN)-1+16*LENGTH('ASGN') 01967000
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */ 01968000
*       3 * FIXED(8) INIT(25+8+8),                                 0570 01969000
*       3 * FIXED(8) INIT(31+8+8),                                 0570 01970000
*       3 * PTR(31) INIT(ADDR(SAT2LB3)),/* SEE PTTLBPTR              */ 01971000
*       3 SAT2DT3 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01972000
*      2 * FIXED(31) INIT(0);       /* END OF FORMAT PATTERNS        */ 01973000
*                                                                  0571 01974000
*   /*****************************************************************/ 01975000
*   /*                                                               */ 01976000
*   /* SAT3:ILRMARRY (CYLMP,8) FORMAT PATTERNS FOR ONE LINE OF OUTPUT*/ 01977000
*   /*                                                               */ 01978000
*   /*****************************************************************/ 01979000
*                                                                  0571 01980000
*   DCL                                                            0571 01981000
*     1 SAT3PTS BDY(WORD),                                         0571 01982000
*      2 * CHAR(8) BDY(WORD),       /* ONE FORMAT PATTERN            */ 01983000
*       3 * BIT(8) INIT('00001100'B),/* SEE PTTFLGS                  */ 01984000
*         5 * BIT(2),                                              0571 01985000
*         5 * BIT(6),                                              0571 01986000
*       3 * BIT(8) INIT(16*0+LENGTH(SATCYLMP)-1),/* SEE PTTLBLGH AND    01987000
*                                      PTTDTLGH                      */ 01988000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 01989000
*       3 * FIXED(8) INIT(5),       /* SEE PTTDTOFF                  */ 01990000
*       3 SAT3DT1 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 01991000
*      2 * CHAR(8) BDY(WORD),       /* ONE FORMAT PATTERN            */ 01992000
*       3 * BIT(8) INIT('00001100'B),/* SEE PTTFLGS                  */ 01993000
*         5 * BIT(2),                                              0571 01994000
*         5 * BIT(6),                                              0571 01995000
*       3 * BIT(8) INIT(16*0+LENGTH(SATCYLMP)-1),/* SEE PTTLBLGH AND    01996000
*                                      PTTDTLGH                      */ 01997000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 01998000
*       3 * FIXED(8) INIT(17),      /* SEE PTTDTOFF                  */ 01999000
*       3 SAT3DT2 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 02000000
*      2 * CHAR(8) BDY(WORD),       /* ONE FORMAT PATTERN            */ 02001000
*       3 * BIT(8) INIT('00001100'B),/* SEE PTTFLGS                  */ 02002000
*         5 * BIT(2),                                              0571 02003000
*         5 * BIT(6),                                              0571 02004000
*       3 * BIT(8) INIT(16*0+LENGTH(SATCYLMP)-1),/* SEE PTTLBLGH AND    02005000
*                                      PTTDTLGH                      */ 02006000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 02007000
*       3 * FIXED(8) INIT(29),      /* SEE PTTDTOFF                  */ 02008000
*       3 SAT3DT3 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 02009000
*      2 * CHAR(8) BDY(WORD),       /* ONE FORMAT PATTERN            */ 02010000
*       3 * BIT(8) INIT('00001100'B),/* SEE PTTFLGS                  */ 02011000
*         5 * BIT(2),                                              0571 02012000
*         5 * BIT(6),                                              0571 02013000
*       3 * BIT(8) INIT(16*0+LENGTH(SATCYLMP)-1),/* SEE PTTLBLGH AND    02014000
*                                      PTTDTLGH                      */ 02015000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 02016000
*       3 * FIXED(8) INIT(41),      /* SEE PTTDTOFF                  */ 02017000
*       3 SAT3DT4 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 02018000
*      2 * CHAR(8) BDY(WORD),       /* ONE FORMAT PATTERN            */ 02019000
*       3 * BIT(8) INIT('00001100'B),/* SEE PTTFLGS                  */ 02020000
*         5 * BIT(2),                                              0571 02021000
*         5 * BIT(6),                                              0571 02022000
*       3 * BIT(8) INIT(16*0+LENGTH(SATCYLMP)-1),/* SEE PTTLBLGH AND    02023000
*                                      PTTDTLGH                      */ 02024000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 02025000
*       3 * FIXED(8) INIT(53),      /* SEE PTTDTOFF                  */ 02026000
*       3 SAT3DT5 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 02027000
*      2 * CHAR(8) BDY(WORD),       /* ONE FORMAT PATTERN            */ 02028000
*       3 * BIT(8) INIT('00001100'B),/* SEE PTTFLGS                  */ 02029000
*         5 * BIT(2),                                              0571 02030000
*         5 * BIT(6),                                              0571 02031000
*       3 * BIT(8) INIT(16*0+LENGTH(SATCYLMP)-1),/* SEE PTTLBLGH AND    02032000
*                                      PTTDTLGH                      */ 02033000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 02034000
*       3 * FIXED(8) INIT(65),      /* SEE PTTDTOFF                  */ 02035000
*       3 SAT3DT6 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 02036000
*      2 * CHAR(8) BDY(WORD),       /* ONE FORMAT PATTERN            */ 02037000
*       3 * BIT(8) INIT('00001100'B),/* SEE PTTFLGS                  */ 02038000
*         5 * BIT(2),                                              0571 02039000
*         5 * BIT(6),                                              0571 02040000
*       3 * BIT(8) INIT(16*0+LENGTH(SATCYLMP)-1),/* SEE PTTLBLGH AND    02041000
*                                      PTTDTLGH                      */ 02042000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 02043000
*       3 * FIXED(8) INIT(77),      /* SEE PTTDTOFF                  */ 02044000
*       3 SAT3DT7 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 02045000
*      2 * CHAR(8) BDY(WORD),       /* ONE FORMAT PATTERN            */ 02046000
*       3 * BIT(8) INIT('00001100'B),/* SEE PTTFLGS                  */ 02047000
*         5 * BIT(2),                                              0571 02048000
*         5 * BIT(6),                                              0571 02049000
*       3 * BIT(8) INIT(16*0+LENGTH(SATCYLMP)-1),/* SEE PTTLBLGH AND    02050000
*                                      PTTDTLGH                      */ 02051000
*       3 * FIXED(8) INIT(0),       /* SEE PTTLBOFF                  */ 02052000
*       3 * FIXED(8) INIT(89),      /* SEE PTTDTOFF                  */ 02053000
*       3 SAT3DT8 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */ 02054000
*      2 * FIXED(31) INIT(0);       /* END OF FORMAT PATTERNS        */ 02055000
*   RFY                                                            0572 02056000
*     R14 RSTD;                                                    0572 02057000
*   RTNADDR2=R14;                   /* SAVE RETURN ADDRESS           */ 02058000
         ST    R14,RTNADDR2                                        0573 02059000
*   RFY                                                            0574 02060000
*     R14 UNRSTD;                                                  0574 02061000
*                                                                  0574 02062000
*   /*****************************************************************/ 02063000
*   /*                                                               */ 02064000
*   /* SET UP AND PRINT THE HEADER LINE FOR THE SAT                  */ 02065000
*   /*                                                               */ 02066000
*   /*****************************************************************/ 02067000
*                                                                  0575 02068000
*   ANCHTXT=ANCH;                   /* SET UP HEADER                 */ 02069000
         L     @12,ABDPLPTR                                        0575 02070000
         L     @12,ADPLBUF(,@12)                                   0575 02071000
         MVC   ANCHTXT(11,@12),ANCH                                0575 02072000
*   ANCHNAME=ANCHSAT;               /* NAME OF BLOCK                 */ 02073000
         MVC   ANCHNAME(8,@12),ANCHSAT                             0576 02074000
*   RFY                                                            0577 02075000
*    (R0,                                                          0577 02076000
*     R1) RSTD;                                                    0577 02077000
*   R0=ADDR(SATPTR);                /* ADDRESS OF BLOCK              */ 02078000
         LA    R0,SATPTR                                           0578 02079000
*   R1=ADDR(SATNAM);                /* NAME OF BLOCK                 */ 02080000
         LA    R1,SATNAM                                           0579 02081000
*   CALL ILRFPRTH;                  /* PRINT HEADER                  */ 02082000
         L     @15,@CV00676                                        0580 02083000
         BALR  @14,@15                                             0580 02084000
*   RFY                                                            0581 02085000
*    (R0,                                                          0581 02086000
*     R1) UNRSTD;                                                  0581 02087000
*                                                                  0581 02088000
*   /*****************************************************************/ 02089000
*   /*                                                               */ 02090000
*   /* FORMAT THE FIRST TWO LINES OF THE SAT                         */ 02091000
*   /*                                                               */ 02092000
*   /*****************************************************************/ 02093000
*                                                                  0582 02094000
*   DO;                             /* ILRMFRMT (SAT)              0582 02095000
*                                      LINES(SAT1,SAT2)              */ 02096000
*     FORMRC=0;                     /* ZERO THE RETURN CODE          */ 02097000
         SLR   FORMRC,FORMRC                                       0583 02098000
*     SAT1DT1=ADDR(SATID);                                         0584 02099000
         L     @12,SATPTR                                          0584 02100000
         ST    @12,SAT1DT1                                         0584 02101000
*     SAT1DT2=ADDR(SATSARTE);                                      0585 02102000
         LA    @03,SATSARTE(,@12)                                  0585 02103000
         ST    @03,SAT1DT2                                         0585 02104000
*     SAT1DT3=ADDR(SATMAPLN);                                      0586 02105000
         LA    @03,SATMAPLN(,@12)                                  0586 02106000
         ST    @03,SAT1DT3                                         0586 02107000
*     SAT1DT4=ADDR(SATBYTCL);                                      0587 02108000
         LA    @03,SATBYTCL(,@12)                                  0587 02109000
         ST    @03,SAT1DT4                                         0587 02110000
*     SAT1DT5=ADDR(SATOFFST);                                      0588 02111000
         LA    @03,SATOFFST(,@12)                                  0588 02112000
         ST    @03,SAT1DT5                                         0588 02113000
*     SAT1DT6=ADDR(SATMASK);                                       0589 02114000
         LA    @12,SATMASK(,@12)                                   0589 02115000
         ST    @12,SAT1DT6                                         0589 02116000
*     RFY                                                          0590 02117000
*      (R0,                                                        0590 02118000
*       R1) RSTD;                                                  0590 02119000
*     R0=ADDR(SAT1PTS);                                            0591 02120000
         LA    R0,SAT1PTS                                          0591 02121000
*     R1=ABDPLPTR;                  /* GET PARM ADDR                 */ 02122000
         L     @12,ABDPLPTR                                        0592 02123000
         LR    R1,@12                                              0592 02124000
*     CALL FORMAT;                  /* FORMAT THE LINE               */ 02125000
         L     @15,ADPLFRMT(,@12)                                  0593 02126000
         BALR  @14,@15                                             0593 02127000
*     RFY                                                          0594 02128000
*       R15 RSTD;                                                  0594 02129000
*     FORMRC=FORMRC|R15;            /* SAVE RETURN CODE              */ 02130000
         OR    FORMRC,R15                                          0595 02131000
*     RFY                                                          0596 02132000
*       R15 UNRSTD;                                                0596 02133000
*     CALL PRINT;                   /* CALL PRINT ROUTINE            */ 02134000
         L     @12,ABDPLPTR                                        0597 02135000
         L     @15,ADPLPRNT(,@12)                                  0597 02136000
         BALR  @14,@15                                             0597 02137000
*     RFY                                                          0598 02138000
*      (R0,                                                        0598 02139000
*       R1) UNRSTD;                                                0598 02140000
*     SAT2DT1=ADDR(SATSCAN);                                       0599 02141000
         L     @12,SATPTR                                          0599 02142000
         LA    @03,SATSCAN(,@12)                                   0599 02143000
         ST    @03,SAT2DT1                                         0599 02144000
*     SAT2DT2=ADDR(SATSLTNO);                                      0600 02145000
         LA    @03,SATSLTNO(,@12)                                  0600 02146000
         ST    @03,SAT2DT2                                         0600 02147000
*     SAT2DT3=ADDR(SATASGN);                                       0601 02148000
         LA    @12,SATASGN(,@12)                                   0601 02149000
         ST    @12,SAT2DT3                                         0601 02150000
*     RFY                                                          0602 02151000
*      (R0,                                                        0602 02152000
*       R1) RSTD;                                                  0602 02153000
*     R0=ADDR(SAT2PTS);                                            0603 02154000
         LA    R0,SAT2PTS                                          0603 02155000
*     R1=ABDPLPTR;                  /* GET PARM ADDR                 */ 02156000
         L     @12,ABDPLPTR                                        0604 02157000
         LR    R1,@12                                              0604 02158000
*     CALL FORMAT;                  /* FORMAT THE LINE               */ 02159000
         L     @15,ADPLFRMT(,@12)                                  0605 02160000
         BALR  @14,@15                                             0605 02161000
*     RFY                                                          0606 02162000
*       R15 RSTD;                                                  0606 02163000
*     FORMRC=FORMRC|R15;            /* SAVE RETURN CODE              */ 02164000
         OR    FORMRC,R15                                          0607 02165000
*     RFY                                                          0608 02166000
*       R15 UNRSTD;                                                0608 02167000
*     CALL PRINT;                   /* CALL PRINT ROUTINE            */ 02168000
         L     @12,ABDPLPTR                                        0609 02169000
         L     @15,ADPLPRNT(,@12)                                  0609 02170000
         BALR  @14,@15                                             0609 02171000
*     RFY                                                          0610 02172000
*      (R0,                                                        0610 02173000
*       R1) UNRSTD;                                                0610 02174000
*     RFY                                                          0611 02175000
*       R15 RSTD;                                                  0611 02176000
*     R15=FORMRC;                   /* SAVE RETURN CODE FROM FORMAT  */ 02177000
         LR    R15,FORMRC                                          0612 02178000
*     RFY                                                          0613 02179000
*       R15 UNRSTD;                                                0613 02180000
*   END;                            /* ILRMFRMT (SAT)              0614 02181000
*                                      LINES(SAT1,SAT2)              */ 02182000
*   RFY                                                            0615 02183000
*     R15 RSTD;                                                    0615 02184000
*   SATRC=R15;                      /* ADD IN RETURN CODE            */ 02185000
         LR    SATRC,R15                                           0616 02186000
*   RFY                                                            0617 02187000
*     R15 UNRSTD;                                                  0617 02188000
*                                                                  0618 02189000
*   /*****************************************************************/ 02190000
*   /*                                                               */ 02191000
*   /* PUT THE HEADER IN THE BUFFER FOR THE SATMAP                   */ 02192000
*   /*                                                               */ 02193000
*   /*****************************************************************/ 02194000
*                                                                  0618 02195000
*   BUFFER(1:6)=SATMAP;             /* MOVE 'SATMAP' TO BUFFER       */ 02196000
         L     @12,ABDPLPTR                                        0618 02197000
         L     @01,ADPLBUF(,@12)                                   0618 02198000
         MVC   BUFFER(6,@01),SATMAP                                0618 02199000
*   CALL PRINT;                     /* PRINT THE HEADING             */ 02200000
*                                                                  0619 02201000
         L     @15,ADPLPRNT(,@12)                                  0619 02202000
         BALR  @14,@15                                             0619 02203000
*   /*****************************************************************/ 02204000
*   /*                                                               */ 02205000
*   /* GET THE SIZE OF THE SATMAP AND USE ILRMARRY TO FORMAT IT.     */ 02206000
*   /*                                                               */ 02207000
*   /*****************************************************************/ 02208000
*                                                                  0620 02209000
*   RFY                                                            0620 02210000
*    (R0,                                                          0620 02211000
*     R1,                                                          0620 02212000
*     R14,                                                         0620 02213000
*     R15) RSTD;                                                   0620 02214000
*   R0=ADDR(SATMAPLN);              /* GET THE ADDR OF THE DATA      */ 02215000
         L     @12,SATPTR                                          0621 02216000
         LA    R0,SATMAPLN(,@12)                                   0621 02217000
*   R1=ABDPLPTR;                    /* GET ADDR OF PARM LIST         */ 02218000
         L     @12,ABDPLPTR                                        0622 02219000
         LR    R1,@12                                              0622 02220000
*   CALL ACCESS;                    /* HECK IF DATA IS AVAILABLE     */ 02221000
         L     @15,ADPLMEMA(,@12)                                  0623 02222000
         BALR  @14,@15                                             0623 02223000
*   R14=R0;                         /* GET ADDR OF THE DATA          */ 02224000
         LR    R14,R0                                              0624 02225000
*   IF R15=GOOD THEN                /* WAS THE ACCESS SUCCESSFUL     */ 02226000
         LTR   R15,R15                                             0625 02227000
         BNZ   @RF00625                                            0625 02228000
*     MAPSIZE=HALFCURR;             /* GET THE DATA                  */ 02229000
         LH    @12,HALFCURR(,R14)                                  0626 02230000
         ST    @12,MAPSIZE                                         0626 02231000
*   ELSE                                                           0627 02232000
*     MAPSIZE=ZERO;                 /* ZERO THE DATA POINTER         */ 02233000
         B     @RC00625                                            0627 02234000
@RF00625 SLR   @12,@12                                             0627 02235000
         ST    @12,MAPSIZE                                         0627 02236000
*   RFY                                                            0628 02237000
*    (R0,                                                          0628 02238000
*     R1,                                                          0628 02239000
*     R14,                                                         0628 02240000
*     R15) UNRSTD;                                                 0628 02241000
@RC00625 DS    0H                                                  0629 02242000
*   IF MAPSIZE=0 THEN               /* NO MAP SOMETHING WRONG        */ 02243000
         ICM   @12,15,MAPSIZE                                      0629 02244000
         BNZ   @RF00629                                            0629 02245000
*     DO;                           /* ERROR RETURN                  */ 02246000
*       SATRC=4;                    /* SET ERROR RETURN CODE         */ 02247000
         LA    SATRC,4                                             0631 02248000
*       GO TO SATRTN;               /* RETURN TO CALLER              */ 02249000
         B     SATRTN                                              0632 02250000
*     END;                                                         0633 02251000
*                                                                  0633 02252000
*   /*****************************************************************/ 02253000
*   /*                                                               */ 02254000
*   /* THE SATMAP IS PRINTED 8 WORDS PER LINE AND THE LENGTH IS GIVEN*/ 02255000
*   /* IN BYTES. TO DETERMINE # OF LINES DIVIDE BY 32(4 BYTES/WORD   */ 02256000
*   /* AND 8 WORDS/LINE.                                             */ 02257000
*   /*                                                               */ 02258000
*   /*****************************************************************/ 02259000
*                                                                  0634 02260000
*   CTR=MAPSIZE/32;                 /* DETERMINE NUMBER OF LINES     */ 02261000
@RF00629 L     @12,MAPSIZE                                         0634 02262000
         LR    @00,@12                                             0634 02263000
         SRDA  @00,32                                              0634 02264000
         D     @00,@CF00136                                        0634 02265000
         ST    @01,CTR                                             0634 02266000
*   REM=MAPSIZE//32;                /* OF SATMAP TO PRINT            */ 02267000
         LR    @00,@12                                             0635 02268000
         SRDA  @00,32                                              0635 02269000
         D     @00,@CF00136                                        0635 02270000
         LR    REM,@00                                             0635 02271000
*   MAPPTR=ADDR(SATMAPS);           /* GET THE ADDRESS OF THE ARRAY  */ 02272000
         L     MAPPTR,SATPTR                                       0636 02273000
         LA    MAPPTR,SATMAPS(,MAPPTR)                             0636 02274000
*   RFY                                                            0637 02275000
*     SATCYLMP BASED(MAPPTR);       /* ADDRESSABILITY FOR MAPWORDS   */ 02276000
*   DO I=1 TO CTR;                                                 0638 02277000
         LA    I,1                                                 0638 02278000
         B     @DE00638                                            0638 02279000
@DL00638 DS    0H                                                  0639 02280000
*     DO;                           /* ILRMFRMT ARRAY(SAT3)          */ 02281000
*       FORMRC=0;                   /* ZERO THE RETURN CODE          */ 02282000
         SLR   FORMRC,FORMRC                                       0640 02283000
*       SAT3DT1=ADDR(SATCYLMP)+00*LENGTH(SATCYLMP);                0641 02284000
         ST    MAPPTR,SAT3DT1                                      0641 02285000
*       SAT3DT2=ADDR(SATCYLMP)+01*LENGTH(SATCYLMP);                0642 02286000
         LA    @03,4                                               0642 02287000
         ALR   @03,MAPPTR                                          0642 02288000
         ST    @03,SAT3DT2                                         0642 02289000
*       SAT3DT3=ADDR(SATCYLMP)+02*LENGTH(SATCYLMP);                0643 02290000
