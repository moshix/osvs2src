*       3 * FIXED(8) INIT(LENGTH(PCCWTIC)-1+16*LENGTH('TIC')-16),  0874 03420000
*       3 * FIXED(8) INIT(83),                                     0874 03421000
*       3 * FIXED(8) INIT(89),                                     0874 03422000
*       3 * PTR(31) INIT(ADDR(PCCW3LB4)),                          0874 03423000
*      2 * FIXED(31) INIT(0);                                      0874 03424000
*                                                                  0874 03425000
*   /*****************************************************************/ 03426000
*   /*                                                               */ 03427000
*   /* PCCW4: (RW,NOP) LABEL VARIABLES REFERENCED IN FMT PTTRNS      */ 03428000
*   /*                                                               */ 03429000
*   /*****************************************************************/ 03430000
*                                                                  0875 03431000
*   DCL                                                            0875 03432000
*     PCCW4LB1 CHAR(LENGTH('R/W')) INIT('R/W');                    0875 03433000
*   DCL                                                            0876 03434000
*     PCCW4LB2 CHAR(LENGTH('NOP')) INIT('NOP');                    0876 03435000
*                                                                  0877 03436000
*   /*****************************************************************/ 03437000
*   /*                                                               */ 03438000
*   /* FORMAT PATTERNS FOR LINE OF OUTPUT (HAND CODED)               */ 03439000
*   /*                                                               */ 03440000
*   /*****************************************************************/ 03441000
*                                                                  0877 03442000
*   DCL                                                            0877 03443000
*     1 PCCW4PTS BDY(WORD),         /* FORMAT PATTERNS - SEE PTTRNS  */ 03444000
*      2 * CHAR(8) BDY(WORD),                                      0877 03445000
*       3 * BIT(8) BDY(BYTE) INIT('00000111'B),                    0877 03446000
*       3 * FIXED(8) INIT(LENGTH(PCCWRW)-1+16*LENGTH('R/W')-16),   0877 03447000
*       3 * FIXED(8) INIT(5),                                      0877 03448000
*       3 * FIXED(8) INIT(11),                                     0877 03449000
*       3 * PTR(31) INIT(ADDR(PCCW4LB1)),                          0877 03450000
*      2 * CHAR(8) BDY(WORD),                                      0877 03451000
*       3 * BIT(8) BDY(BYTE) INIT('00000111'B),                    0877 03452000
*       3 * FIXED(8) INIT(LENGTH(PCCWNOP)-1+16*LENGTH('NOP')-16),  0877 03453000
*       3 * FIXED(8) INIT(31),                                     0877 03454000
*       3 * FIXED(8) INIT(37),                                     0877 03455000
*       3 * PTR(31) INIT(ADDR(PCCW4LB2)),                          0877 03456000
*      2 * FIXED(31) INIT(0);                                      0877 03457000
*   RFY                                                            0878 03458000
*     R14 RSTD;                                                    0878 03459000
*   RTNADDR3=R14;                   /* SAVE RETURN ADDRESS           */ 03460000
         LR    RTNADDR3,R14                                        0879 03461000
*   RFY                                                            0880 03462000
*     R14 UNRSTD;                                                  0880 03463000
*   PCCWRC=0;                       /* ZERO THE RETURN CODE          */ 03464000
*                                                                  0881 03465000
         SLR   PCCWRC,PCCWRC                                       0881 03466000
*   /*****************************************************************/ 03467000
*   /*                                                               */ 03468000
*   /* GET THE NUMBER OF PCCW'S AND IF IT IS NOT AVAILABLE SET IT TO */ 03469000
*   /* 150.                                                          */ 03470000
*   /*                                                               */ 03471000
*   /*****************************************************************/ 03472000
*                                                                  0882 03473000
*   DO;                             /* ILRMACCS (MAXNUM)           0882 03474000
*                                      FROM(ASMPCCWN)                */ 03475000
*     RFY                                                          0883 03476000
*      (R0,                                                        0883 03477000
*       R1,                                                        0883 03478000
*       R14,                                                       0883 03479000
*       R15) RSTD;                                                 0883 03480000
*     R0=ADDR(ASMPCCWN);                                           0884 03481000
         L     @12,ASMVTPTR                                        0884 03482000
         LA    R0,ASMPCCWN(,@12)                                   0884 03483000
*     R1=ABDPLPTR;                  /* POINT TO PARM LIST            */ 03484000
         L     @12,ABDPLPTR                                        0885 03485000
         LR    R1,@12                                              0885 03486000
*     CALL ACCESS;                  /* ACCESS DATA                   */ 03487000
         L     @15,ADPLMEMA(,@12)                                  0886 03488000
         BALR  @14,@15                                             0886 03489000
*     R14=R0;                       /* CANNOT USE R0 AS BASE         */ 03490000
         LR    R14,R0                                              0887 03491000
*     IF R15=GOOD THEN              /* CHECK ACCESS                  */ 03492000
         LTR   R15,R15                                             0888 03493000
         BNZ   @RF00888                                            0888 03494000
*       MAXNUM=CURRENT;             /* GET REQUESTED FIELD           */ 03495000
         MVC   MAXNUM(4),CURRENT(R14)                              0889 03496000
*     ELSE                          /* ACCESS NO GOOD                */ 03497000
*       MAXNUM=ZERO;                /* ZERO FIELD                    */ 03498000
         B     @RC00888                                            0890 03499000
@RF00888 SLR   @12,@12                                             0890 03500000
         ST    @12,MAXNUM                                          0890 03501000
*     RFY                                                          0891 03502000
*      (R0,                                                        0891 03503000
*       R1,                                                        0891 03504000
*       R14,                                                       0891 03505000
*       R15) UNRSTD;                                               0891 03506000
@RC00888 DS    0H                                                  0892 03507000
*   END;                            /* GET NUMBER OF PCCWS           */ 03508000
*   IF MAXNUM^=NULLPTR THEN         /* ACCESS WAS GOOD               */ 03509000
         L     @12,MAXNUM                                          0893 03510000
         LTR   @12,@12                                             0893 03511000
         BZ    @RF00893                                            0893 03512000
*     MAXPCCWS=MAXNUM;              /* SET MAX TO THAT NUMBER        */ 03513000
         ST    @12,MAXPCCWS                                        0894 03514000
*   ELSE                            /* COUNLD NOT GET MAX NUM        */ 03515000
*     MAXPCCWS=150;                 /* SET A MAX NUM                 */ 03516000
         B     @RC00893                                            0895 03517000
@RF00893 MVC   MAXPCCWS(4),@CF01731                                0895 03518000
*   MAXCTR=0;                       /* INITIALIZE COUNTER            */ 03519000
@RC00893 SLR   @12,@12                                             0896 03520000
         ST    @12,MAXCTR                                          0896 03521000
*   ANCHTXT=ANCH;                   /* SET UP THE HEADER LABEL       */ 03522000
         L     @12,ABDPLPTR                                        0897 03523000
         L     @12,ADPLBUF(,@12)                                   0897 03524000
         MVC   ANCHTXT(11,@12),ANCH                                0897 03525000
*   ANCHNAME=ANCHPCCW;              /* PUT IN SOURCE NAME            */ 03526000
*                                                                  0898 03527000
         MVC   ANCHNAME(8,@12),ANCHPCCW                            0898 03528000
*   /*****************************************************************/ 03529000
*   /*                                                               */ 03530000
*   /* LOOP UNTIL ALL PCCW'S ARE PRINTED                             */ 03531000
*   /*                                                               */ 03532000
*   /*****************************************************************/ 03533000
*                                                                  0899 03534000
*   DO WHILE(PCCWPTR^=NULLPTR&      /* UNTIL ACCESS FAILS, Q ENDED   */ 03535000
*         MAXCTR<MAXPCCWS);         /* OR MAXEXCEEDED                */ 03536000
         B     @DE00899                                            0899 03537000
@DL00899 DS    0H                                                  0900 03538000
*     MAXCTR=MAXCTR+1;              /* INCREMENT LOOP COUNT          */ 03539000
         LA    @12,1                                               0900 03540000
         AL    @12,MAXCTR                                          0900 03541000
         ST    @12,MAXCTR                                          0900 03542000
*     RFY                                                          0901 03543000
*      (R0,                                                        0901 03544000
*       R1) RSTD;                                                  0901 03545000
*     R0=ADDR(PCCWPTR);             /* GET PCCW PTR                  */ 03546000
         LA    R0,PCCWPTR                                          0902 03547000
*     R1=ADDR(PCCWNAM);             /* GET ADDRESS OF NAME           */ 03548000
         LA    R1,PCCWNAM                                          0903 03549000
*     CALL ILRFPRTH;                /* CALL TO PRINT HEADING         */ 03550000
         L     @15,@CV00676                                        0904 03551000
         BALR  @14,@15                                             0904 03552000
*     RFY                                                          0905 03553000
*      (R0,                                                        0905 03554000
*       R1) UNRSTD;                                                0905 03555000
*                                                                  0905 03556000
*     /***************************************************************/ 03557000
*     /*                                                             */ 03558000
*     /* FORMAT THE FIRST TWO LINES OF THE PCCW                      */ 03559000
*     /*                                                             */ 03560000
*     /***************************************************************/ 03561000
*                                                                  0906 03562000
*     DO;                           /* ILRMFRMT (PCCW)             0906 03563000
*                                      LINES(PCCW1,PCCW2)            */ 03564000
*       FORMRC=0;                   /* ZERO THE RETURN CODE          */ 03565000
         SLR   FORMRC,FORMRC                                       0907 03566000
*       PCCW1DT1=ADDR(PCCWID);                                     0908 03567000
         L     @12,PCCWPTR                                         0908 03568000
         ST    @12,PCCW1DT1                                        0908 03569000
*       PCCW1DT2=ADDR(PCCWSECT);                                   0909 03570000
         LA    @03,PCCWSECT(,@12)                                  0909 03571000
         ST    @03,PCCW1DT2                                        0909 03572000
*       PCCW1DT3=ADDR(PCCWFLGS);                                   0910 03573000
         LA    @03,PCCWFLGS(,@12)                                  0910 03574000
         ST    @03,PCCW1DT3                                        0910 03575000
*       PCCW1DT4=ADDR(PCCWPCCW);                                   0911 03576000
         LA    @03,PCCWPCCW(,@12)                                  0911 03577000
         ST    @03,PCCW1DT4                                        0911 03578000
*       PCCW1DT5=ADDR(PCCWAIA);                                    0912 03579000
         LA    @12,PCCWAIA(,@12)                                   0912 03580000
         ST    @12,PCCW1DT5                                        0912 03581000
*       RFY                                                        0913 03582000
*        (R0,                                                      0913 03583000
*         R1) RSTD;                                                0913 03584000
*       R0=ADDR(PCCW1PTS);                                         0914 03585000
         LA    R0,PCCW1PTS                                         0914 03586000
*       R1=ABDPLPTR;                /* GET PARM ADDR                 */ 03587000
         L     @12,ABDPLPTR                                        0915 03588000
         LR    R1,@12                                              0915 03589000
*       CALL FORMAT;                /* FORMAT THE LINE               */ 03590000
         L     @15,ADPLFRMT(,@12)                                  0916 03591000
         BALR  @14,@15                                             0916 03592000
*       RFY                                                        0917 03593000
*         R15 RSTD;                                                0917 03594000
*       FORMRC=FORMRC|R15;          /* SAVE RETURN CODE              */ 03595000
         OR    FORMRC,R15                                          0918 03596000
*       RFY                                                        0919 03597000
*         R15 UNRSTD;                                              0919 03598000
*       CALL PRINT;                 /* CALL PRINT ROUTINE            */ 03599000
         L     @12,ABDPLPTR                                        0920 03600000
         L     @15,ADPLPRNT(,@12)                                  0920 03601000
         BALR  @14,@15                                             0920 03602000
*       RFY                                                        0921 03603000
*        (R0,                                                      0921 03604000
*         R1) UNRSTD;                                              0921 03605000
*       PCCW2DT1=ADDR(PCCWIORB);                                   0922 03606000
         L     @12,PCCWPTR                                         0922 03607000
         LA    @03,PCCWIORB(,@12)                                  0922 03608000
         ST    @03,PCCW2DT1                                        0922 03609000
*       PCCW2DT2=ADDR(PCCWM);                                      0923 03610000
         LA    @03,PCCWM(,@12)                                     0923 03611000
         ST    @03,PCCW2DT2                                        0923 03612000
*       PCCW2DT3=ADDR(PCCWBB);                                     0924 03613000
         LA    @03,PCCWBB(,@12)                                    0924 03614000
         ST    @03,PCCW2DT3                                        0924 03615000
*       PCCW2DT4=ADDR(PCCWCC);                                     0925 03616000
         LA    @03,PCCWCC(,@12)                                    0925 03617000
         ST    @03,PCCW2DT4                                        0925 03618000
*       PCCW2DT5=ADDR(PCCWHH);                                     0926 03619000
         LA    @03,PCCWHH(,@12)                                    0926 03620000
         ST    @03,PCCW2DT5                                        0926 03621000
*       PCCW2DT6=ADDR(PCCWR);                                      0927 03622000
         LA    @12,PCCWR(,@12)                                     0927 03623000
         ST    @12,PCCW2DT6                                        0927 03624000
*       RFY                                                        0928 03625000
*        (R0,                                                      0928 03626000
*         R1) RSTD;                                                0928 03627000
*       R0=ADDR(PCCW2PTS);                                         0929 03628000
         LA    R0,PCCW2PTS                                         0929 03629000
*       R1=ABDPLPTR;                /* GET PARM ADDR                 */ 03630000
         L     @12,ABDPLPTR                                        0930 03631000
         LR    R1,@12                                              0930 03632000
*       CALL FORMAT;                /* FORMAT THE LINE               */ 03633000
         L     @15,ADPLFRMT(,@12)                                  0931 03634000
         BALR  @14,@15                                             0931 03635000
*       RFY                                                        0932 03636000
*         R15 RSTD;                                                0932 03637000
*       FORMRC=FORMRC|R15;          /* SAVE RETURN CODE              */ 03638000
         OR    FORMRC,R15                                          0933 03639000
*       RFY                                                        0934 03640000
*         R15 UNRSTD;                                              0934 03641000
*       CALL PRINT;                 /* CALL PRINT ROUTINE            */ 03642000
         L     @12,ABDPLPTR                                        0935 03643000
         L     @15,ADPLPRNT(,@12)                                  0935 03644000
         BALR  @14,@15                                             0935 03645000
*       RFY                                                        0936 03646000
*        (R0,                                                      0936 03647000
*         R1) UNRSTD;                                              0936 03648000
*       RFY                                                        0937 03649000
*         R15 RSTD;                                                0937 03650000
*       R15=FORMRC;                 /* SAVE RETURN CODE FROM FORMAT  */ 03651000
         LR    R15,FORMRC                                          0938 03652000
*       RFY                                                        0939 03653000
*         R15 UNRSTD;                                              0939 03654000
*     END;                          /* ILRMFRMT (PCCW)             0940 03655000
*                                      LINES(PCCW1,PCCW2)            */ 03656000
*     RFY                                                          0941 03657000
*       R15 RSTD;                                                  0941 03658000
*     PCCWRC=PCCWRC|R15;                                           0942 03659000
         OR    PCCWRC,R15                                          0942 03660000
*     RFY                                                          0943 03661000
*       R15 UNRSTD;                                                0943 03662000
*                                                                  0944 03663000
*     /***************************************************************/ 03664000
*     /*                                                             */ 03665000
*     /* FORMAT THE REST OF THE PCCW WITH HARDCODED CALLS TO FORMAT  */ 03666000
*     /* AND PRINT.                                                  */ 03667000
*     /*                                                             */ 03668000
*     /***************************************************************/ 03669000
*                                                                  0944 03670000
*     PCCW3DT1=ADDR(PCCWSEEK);                                     0944 03671000
         L     @12,PCCWPTR                                         0944 03672000
         LA    @12,PCCWSEEK(,@12)                                  0944 03673000
         ST    @12,PCCW3DT1                                        0944 03674000
*     RFY                                                          0945 03675000
*      (R0,                                                        0945 03676000
*       R1) RSTD;                                                  0945 03677000
*     R0=ADDR(PCCW3PTS);                                           0946 03678000
         LA    R0,PCCW3PTS                                         0946 03679000
*     R1=ABDPLPTR;                                                 0947 03680000
         L     @12,ABDPLPTR                                        0947 03681000
         LR    R1,@12                                              0947 03682000
*     CALL FORMAT;                                                 0948 03683000
         L     @15,ADPLFRMT(,@12)                                  0948 03684000
         BALR  @14,@15                                             0948 03685000
*     RFY                                                          0949 03686000
*       R15 RSTD;                                                  0949 03687000
*     PCCWRC=PCCWRC|R15;            /* SAVE RETURN CODE              */ 03688000
         OR    PCCWRC,R15                                          0950 03689000
*     RFY                                                          0951 03690000
*       R15 UNRSTD;                                                0951 03691000
*     CALL PRINT;                                                  0952 03692000
         L     @12,ABDPLPTR                                        0952 03693000
         L     @15,ADPLPRNT(,@12)                                  0952 03694000
         BALR  @14,@15                                             0952 03695000
*     RFY                                                          0953 03696000
*      (R0,                                                        0953 03697000
*       R1) RSTD;                                                  0953 03698000
*     R0=ADDR(PCCW4PTS);                                           0954 03699000
         LA    R0,PCCW4PTS                                         0954 03700000
*     R1=ABDPLPTR;                                                 0955 03701000
         L     @12,ABDPLPTR                                        0955 03702000
         LR    R1,@12                                              0955 03703000
*     CALL FORMAT;                                                 0956 03704000
         L     @15,ADPLFRMT(,@12)                                  0956 03705000
         BALR  @14,@15                                             0956 03706000
*     RFY                                                          0957 03707000
*       R15 RSTD;                                                  0957 03708000
*     PCCWRC=PCCWRC|R15;            /* SAVE RETURN CODE              */ 03709000
         OR    PCCWRC,R15                                          0958 03710000
*     RFY                                                          0959 03711000
*       R15 UNRSTD;                                                0959 03712000
*     CALL PRINT;                                                  0960 03713000
         L     @12,ABDPLPTR                                        0960 03714000
         L     @15,ADPLPRNT(,@12)                                  0960 03715000
         BALR  @14,@15                                             0960 03716000
*     CALL PRINT;                                                  0961 03717000
         L     @12,ABDPLPTR                                        0961 03718000
         L     @15,ADPLPRNT(,@12)                                  0961 03719000
         BALR  @14,@15                                             0961 03720000
*     RFY                                                          0962 03721000
*      (R0,                                                        0962 03722000
*       R1) UNRSTD;                                                0962 03723000
*                                                                  0963 03724000
*     /***************************************************************/ 03725000
*     /*                                                             */ 03726000
*     /* GET THE ADDRESS OF THE AIA AND IF ONE IS AVAILABLE CALL     */ 03727000
*     /* ILRFAIA TO FORMAT IT.                                       */ 03728000
*     /*                                                             */ 03729000
*     /***************************************************************/ 03730000
*                                                                  0963 03731000
*     DO;                           /* ILRMACCS (AIAPTR)           0963 03732000
*                                      FROM(PCCWAIA)                 */ 03733000
*       RFY                                                        0964 03734000
*        (R0,                                                      0964 03735000
*         R1,                                                      0964 03736000
*         R14,                                                     0964 03737000
*         R15) RSTD;                                               0964 03738000
*       R0=ADDR(PCCWAIA);                                          0965 03739000
         L     @12,PCCWPTR                                         0965 03740000
         LA    R0,PCCWAIA(,@12)                                    0965 03741000
*       R1=ABDPLPTR;                /* POINT TO PARM LIST            */ 03742000
         L     @12,ABDPLPTR                                        0966 03743000
         LR    R1,@12                                              0966 03744000
*       CALL ACCESS;                /* ACCESS DATA                   */ 03745000
         L     @15,ADPLMEMA(,@12)                                  0967 03746000
         BALR  @14,@15                                             0967 03747000
*       R14=R0;                     /* CANNOT USE R0 AS BASE         */ 03748000
         LR    R14,R0                                              0968 03749000
*       IF R15=GOOD THEN            /* CHECK ACCESS                  */ 03750000
         LTR   R15,R15                                             0969 03751000
         BNZ   @RF00969                                            0969 03752000
*         AIAPTR=CURRENT;           /* GET REQUESTED FIELD           */ 03753000
         L     AIAPTR,CURRENT(,R14)                                0970 03754000
*       ELSE                        /* ACCESS NO GOOD                */ 03755000
*         AIAPTR=ZERO;              /* ZERO FIELD                    */ 03756000
         B     @RC00969                                            0971 03757000
@RF00969 SLR   AIAPTR,AIAPTR                                       0971 03758000
*       RFY                                                        0972 03759000
*        (R0,                                                      0972 03760000
*         R1,                                                      0972 03761000
*         R14,                                                     0972 03762000
*         R15) UNRSTD;                                             0972 03763000
@RC00969 DS    0H                                                  0973 03764000
*     END;                                                         0973 03765000
*     IF AIAPTR^=NULLPTR THEN       /* QUEUE HAS ELEMENTS            */ 03766000
         LTR   AIAPTR,AIAPTR                                       0974 03767000
         BZ    @RF00974                                            0974 03768000
*       DO;                                                        0975 03769000
*         ANCHTXT=ANCH;             /* PUT LABEL IN BUFFER           */ 03770000
         L     @01,ABDPLPTR                                        0976 03771000
         L     @15,ADPLBUF(,@01)                                   0976 03772000
         MVC   ANCHTXT(11,@15),ANCH                                0976 03773000
*         ANCHNAME=ANCHAIA6;        /* ANCHOR LOCATION               */ 03774000
         MVC   ANCHNAME(8,@15),ANCHAIA6                            0977 03775000
*         RFY                                                      0978 03776000
*          (R0,                                                    0978 03777000
*           R1,                                                    0978 03778000
*           R2) RSTD;                                              0978 03779000
*         R0=AIAPTR;                /* GET THE ADDR OF THE FIRST AIA */ 03780000
         LR    R0,AIAPTR                                           0979 03781000
*         R1=ADDR(AIANAM);          /* GET ADDR OF NAME OF BLOCK     */ 03782000
         LA    R1,AIANAM                                           0980 03783000
*         R2=0;                     /* SET CODE FOR ILRFAIA          */ 03784000
         SLR   R2,R2                                               0981 03785000
*         CALL ILRFAIA;             /* CALL SEGMENT TO FMT AIA Q     */ 03786000
         L     @15,@CV00675                                        0982 03787000
         BALR  @14,@15                                             0982 03788000
*       END;                                                       0983 03789000
*     RFY                                                          0984 03790000
*       R15 RSTD;                                                  0984 03791000
@RF00974 DS    0H                                                  0985 03792000
*     PCCWRC=PCCWRC|R15;            /* SAVE RETURN CODE              */ 03793000
         OR    PCCWRC,R15                                          0985 03794000
*     RFY                                                          0986 03795000
*      (R0,                                                        0986 03796000
*       R1,                                                        0986 03797000
*       R2,                                                        0986 03798000
*       R15) UNRSTD;                                               0986 03799000
*                                                                  0986 03800000
*     /***************************************************************/ 03801000
*     /*                                                             */ 03802000
*     /* GET THE ADDRESS OF THE NEXT PCCW. IF THERE IS NONE THE LOOP */ 03803000
*     /* WILL END.                                                   */ 03804000
*     /*                                                             */ 03805000
*     /***************************************************************/ 03806000
*                                                                  0987 03807000
*     DO;                           /* ILRMACCS (PCCWPTR)          0987 03808000
*                                      FROM(PCCWPCCW)                */ 03809000
*       RFY                                                        0988 03810000
*        (R0,                                                      0988 03811000
*         R1,                                                      0988 03812000
*         R14,                                                     0988 03813000
*         R15) RSTD;                                               0988 03814000
*       R0=ADDR(PCCWPCCW);                                         0989 03815000
         L     @12,PCCWPTR                                         0989 03816000
         LA    R0,PCCWPCCW(,@12)                                   0989 03817000
*       R1=ABDPLPTR;                /* POINT TO PARM LIST            */ 03818000
         L     @12,ABDPLPTR                                        0990 03819000
         LR    R1,@12                                              0990 03820000
*       CALL ACCESS;                /* ACCESS DATA                   */ 03821000
         L     @15,ADPLMEMA(,@12)                                  0991 03822000
         BALR  @14,@15                                             0991 03823000
*       R14=R0;                     /* CANNOT USE R0 AS BASE         */ 03824000
         LR    R14,R0                                              0992 03825000
*       IF R15=GOOD THEN            /* CHECK ACCESS                  */ 03826000
         LTR   R15,R15                                             0993 03827000
         BNZ   @RF00993                                            0993 03828000
*         PCCWPTR=CURRENT;          /* GET REQUESTED FIELD           */ 03829000
         MVC   PCCWPTR(4),CURRENT(R14)                             0994 03830000
*       ELSE                        /* ACCESS NO GOOD                */ 03831000
*         PCCWPTR=ZERO;             /* ZERO FIELD                    */ 03832000
         B     @RC00993                                            0995 03833000
@RF00993 SLR   @12,@12                                             0995 03834000
         ST    @12,PCCWPTR                                         0995 03835000
*       RFY                                                        0996 03836000
*        (R0,                                                      0996 03837000
*         R1,                                                      0996 03838000
*         R14,                                                     0996 03839000
*         R15) UNRSTD;                                             0996 03840000
@RC00993 DS    0H                                                  0997 03841000
*     END;                                                         0997 03842000
*     RFY                                                          0998 03843000
*       R15 RSTD;                                                  0998 03844000
*     PCCWRC=PCCWRC|R15;            /* SAVE THE RETURN CODE          */ 03845000
         OR    PCCWRC,R15                                          0999 03846000
*     RFY                                                          1000 03847000
*       R15 UNRSTD;                                                1000 03848000
*   END;                            /* END PCCW PROCESSING           */ 03849000
*                                                                  1001 03850000
@DE00899 ICM   @12,15,PCCWPTR                                      1001 03851000
         BZ    @DC00899                                            1001 03852000
         L     @12,MAXCTR                                          1001 03853000
         C     @12,MAXPCCWS                                        1001 03854000
         BL    @DL00899                                            1001 03855000
@DC00899 DS    0H                                                  1002 03856000
*   /*****************************************************************/ 03857000
*   /*                                                               */ 03858000
*   /* IF THE LOOP COUNTER HAS EXCEEDED THE NUMBER OF ITERATIONS     */ 03859000
*   /* REQUIRED A MESSAGE WILL BE PUT OUT.                           */ 03860000
*   /*                                                               */ 03861000
*   /*****************************************************************/ 03862000
*                                                                  1002 03863000
*   IF PCCWPTR^=NULLPTR THEN        /* MAX WAS EXCEEDED              */ 03864000
         ICM   @12,15,PCCWPTR                                      1002 03865000
         BZ    @RF01002                                            1002 03866000
*     DO;                                                          1003 03867000
*       PCCWRC=4;                   /* SET THE BAD RETURN CODE       */ 03868000
