*   /*****************************************************************/ 02734000
*   /*                                                               */ 02735000
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT (HAND CODED)           */ 02736000
*   /*                                                               */ 02737000
*   /*****************************************************************/ 02738000
*                                                                  0714 02739000
*   DCL                                                            0714 02740000
*     1 SCCWIPTS BDY(WORD),                                        0714 02741000
*      2 * CHAR(8) BDY(WORD),                                      0714 02742000
*       3 * BIT(8) BDY(BYTE) INIT('00000111'B),                    0714 02743000
*       3 * FIXED(8) INIT(LENGTH(SCCWLTIC)-1+16*LENGTH('LTIC')-16),     02744000
*       3 * FIXED(8) INIT(5),                                      0714 02745000
*       3 * FIXED(8) INIT(11),                                     0714 02746000
*       3 * PTR(31) INIT(ADDR(SCCWILB1)),                          0714 02747000
*      2 * FIXED(31) INIT(0);                                      0714 02748000
*   RFY                                                            0715 02749000
*     R14 RSTD;                                                    0715 02750000
*   RTNADDR3=R14;                   /* SAVE RETURN ADDRESS           */ 02751000
         LR    RTNADDR3,R14                                        0716 02752000
*   RFY                                                            0717 02753000
*     R14 UNRSTD;                                                  0717 02754000
*   SCCWRC=0;                       /* ZERO THE RETURN CODE          */ 02755000
         SLR   SCCWRC,SCCWRC                                       0718 02756000
*   MAXCTR=0;                       /* INITIALIZE MAXIMUM COUNTER    */ 02757000
         SLR   @12,@12                                             0719 02758000
         ST    @12,MAXCTR                                          0719 02759000
*   DO WHILE(SCCWPTR^=NULLPTR&      /* UNTIL ACCESS FAILS, Q ENDED   */ 02760000
*         MAXCTR<MAXSCCWS);         /* OR MAX EXCEEDED               */ 02761000
         B     @DE00720                                            0720 02762000
@DL00720 DS    0H                                                  0721 02763000
*     MAXCTR=MAXCTR+1;              /* INCREMENT THE LOOP COUNT      */ 02764000
         LA    @12,1                                               0721 02765000
         AL    @12,MAXCTR                                          0721 02766000
         ST    @12,MAXCTR                                          0721 02767000
*     RFY                                                          0722 02768000
*      (R0,                                                        0722 02769000
*       R1) RSTD;                                                  0722 02770000
*     R0=ADDR(SCCWPTR);             /* GET THE ADDRESS OF THE CONTROL   02771000
*                                      BLOCK                         */ 02772000
         LA    R0,SCCWPTR                                          0723 02773000
*     R1=ADDR(SCCWNAM);             /* GET THE NAME OF THE CONTROL 0724 02774000
*                                      BLOCK                         */ 02775000
         LA    R1,SCCWNAM                                          0724 02776000
*     CALL ILRFPRTH;                /* PRINT THE HEADING             */ 02777000
         L     @15,@CV00676                                        0725 02778000
         BALR  @14,@15                                             0725 02779000
*     RFY                                                          0726 02780000
*      (R0,                                                        0726 02781000
*       R1) UNRSTD;                                                0726 02782000
*                                                                  0726 02783000
*     /***************************************************************/ 02784000
*     /*                                                             */ 02785000
*     /* FORMAT THE FIRST LINE OF THE SCCW. THE REST OF THE LINES ARE*/ 02786000
*     /* FORMATTED WITH HARDCODED LINES TO FORMAT ROUTINE.           */ 02787000
*     /*                                                             */ 02788000
*     /***************************************************************/ 02789000
*                                                                  0727 02790000
*     DO;                           /* ILRMFRMT (SCCW)             0727 02791000
*                                      LINES(SCCW1)OF(SCCW)          */ 02792000
*       FORMRC=0;                   /* ZERO THE RETURN CODE          */ 02793000
         SLR   FORMRC,FORMRC                                       0728 02794000
*       SCCW1DT1=ADDR(SCCWID);                                     0729 02795000
         L     @12,SCCWPTR                                         0729 02796000
         ST    @12,SCCW1DT1                                        0729 02797000
*       SCCW1DT2=ADDR(SCCWSECT);                                   0730 02798000
