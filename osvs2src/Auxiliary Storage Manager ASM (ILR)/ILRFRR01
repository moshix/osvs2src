*   ENTRY;                          /* SARWAITQ VERIFICATION ROUTINE */ 00303000
@EP00177 DS    0H                                                  0178 00304000
*   RFY                                                            0178 00305000
*    (R2,                                                          0178 00306000
*     ASMVTPTR,                                                    0178 00307000
*     R4) RSTD;                     /* RESTRICT REGS THAT SHOULD NOT    00308000
*                                      BE DESTROYED BY VERIFICATION     00309000
*                                      RTN                           */ 00310000
*   RFY                                                            0179 00311000
*     R14 RSTD;                     /* RESTRICT REG CONTAINING RETURN   00312000
*                                      ADDRESS                       */ 00313000
*   WAREG->WORKA=''B;               /* CLEAR WORKAREA                */ 00314000
         XC    WORKA(72,WAREG),WORKA(WAREG)                        0180 00315000
*   MAINRTRN=R14;                   /* SAVE RETURN ADDRESS           */ 00316000
         ST    R14,MAINRTRN                                        0181 00317000
*   RFY                                                            0182 00318000
*     R14 UNRSTD;                   /* RELEASE REG                   */ 00319000
*   QVPLEVR=ADDR(ILRVAIA);          /* INIT ADDR OF ELEMENT        0183 00320000
*                                      VERIFICATION ROUTINE          */ 00321000
         L     @15,@CA02870                                        0183 00322000
         ST    @15,QVPLEVR                                         0183 00323000
*   QVPLHDR=ADDR(SARWAITF);         /* INIT ADDR OF QUEUE HEADER     */ 00324000
         L     @15,SARTPTR(,ASMVTPTR)                              0184 00325000
         LA    @14,SARWAITF(,@15)                                  0184 00326000
         ST    @14,QVPLHDR                                         0184 00327000
*   QVPLTRLR=ADDR(SARWAITL);        /* INIT ADDR OF QUEUE TRAILER    */ 00328000
         LA    @15,SARWAITL(,@15)                                  0185 00329000
         ST    @15,QVPLTRLR                                        0185 00330000
*   RFY                                                            0186 00331000
*     AIAPTR RSTD;                  /* RESTRICT REG TO POINT TO AIA  */ 00332000
*   QVPLFPTR=ADDR(AIANXAIA)-ADDR(AIA);/* INIT OFFSET OF FORWARD PTR  */ 00333000
         MVC   QVPLFPTR(2),@CH01945                                0187 00334000
*   RFY                                                            0188 00335000
*     AIAPTR UNRSTD;                /* RELEASE REG                   */ 00336000
*   QTYPE=TWO;                      /* SET QTYPE FOR SINGLE-THREADED,   00337000
*                                      DOUBLE-HEADED QUEUE           */ 00338000
         MVC   QTYPE(4),@CF00133                                   0189 00339000
*   CALL COMQRTN;                   /* INTERFACE WITH QUEUE VERIFIER */ 00340000
*                                                                  0190 00341000
         BAL   @14,COMQRTN                                         0190 00342000
*   /*****************************************************************/ 00343000
*   /*                                                               */ 00344000
*   /* SET UP TO VERIFY LATERAL AIA'S.                               */ 00345000
*   /*                                                               */ 00346000
*   /*****************************************************************/ 00347000
*                                                                  0191 00348000
*   RFY                                                            0191 00349000
*    (AIAPTR,                                                      0191 00350000
*     RETCODE) RSTD;                /* RESTRICT REG TO POINT TO AIA     00351000
*                                      AND REG USED FOR RETURN CODE  */ 00352000
*   AIAPTR=SARWAITF;                /* INIT ADDR OF FIRST AIA        */ 00353000
         L     @14,SARTPTR(,ASMVTPTR)                              0192 00354000
         L     AIAPTR,SARWAITF(,@14)                               0192 00355000
*   GOTO VERLAIAS;                  /* VERIFY LATERAL AIA'S          */ 00356000
         B     VERLAIAS                                            0193 00357000
*   RFY                                                            0194 00358000
*    (R2,                                                          0194 00359000
*     ASMVTPTR,                                                    0194 00360000
*     R4,                                                          0194 00361000
*     AIAPTR,                                                      0194 00362000
*     RETCODE) UNRSTD;              /* RELEASE REGS                  */ 00363000
*                                                                  0195 00364000
*/********************************************************************/ 00365000
*/*                                                                  */ 00366000
