@EP00001 DS    0H                                                  0002 00121000
*                                                                  0127 00122000
*/********************************************************************/ 00123000
*/*                                                                  */ 00124000
*/*  ILRVASGQ IS THE ASMSTAGQ VERIFICATION ROUTINE.                  */ 00125000
*/*                                                                  */ 00126000
*/*  INPUT - REG 1 CONTAINS THE ADDRESS OF THE SDWA                  */ 00127000
*/*          REG 3 CONTAINS THE ADDRESS OF THE ASMVT                 */ 00128000
*/*          REG 8 CONTAINS THE ADDRESS OF THE WORKAREA              */ 00129000
*/*          REG 13 CONTAINS THE ADDRESS OF A REG-SAVE AREA          */ 00130000
*/*                                                                  */ 00131000
*/********************************************************************/ 00132000
*                                                                  0127 00133000
*ILRVASGQ:                                                         0127 00134000
*   ENTRY;                          /* ASMSTAGQ VERIFICATION ROUTINE */ 00135000
@EP00127 DS    0H                                                  0128 00136000
*   RFY                                                            0128 00137000
*    (R2,                                                          0128 00138000
*     ASMVTPTR,                                                    0128 00139000
*     R4) RSTD;                     /* RESTRICT REGS THAT SHOULD NOT    00140000
*                                      BE DESTROYED OVER VERIFICATION   00141000
*                                      ROUTINE                       */ 00142000
*   RFY                                                            0129 00143000
*     R14 RSTD;                     /* RESTRICT REG CONTAINING RETURN   00144000
*                                      ADDRESS                       */ 00145000
*   GEN REFS(WAREG)(USING @DATD,WAREG);/* GET ADDRESSABILITY TO    0130 00146000
*                                      WORKAREA                      */ 00147000
         USING @DATD,WAREG                                              00148000
*   WAREG->WORKA=''B;               /* CLEAR WORKAREA                */ 00149000
         XC    WORKA(72,WAREG),WORKA(WAREG)                        0131 00150000
*   MAINRTRN=R14;                   /* SAVE RETURN ADDRESS           */ 00151000
         ST    R14,MAINRTRN                                        0132 00152000
*   RFY                                                            0133 00153000
*     R14 UNRSTD;                   /* RELEASE REG                   */ 00154000
*   QVPLEVR=ADDR(ILRVAIA);          /* INIT ADDR OF ELEMENT        0134 00155000
*                                      VERIFICATION ROUTINE          */ 00156000
         L     @15,@CA02870                                        0134 00157000
         ST    @15,QVPLEVR                                         0134 00158000
*   QVPLHDR=ADDR(ASMSTAGF);         /* INIT ADDR OF QUEUE HEADER     */ 00159000
         LA    @15,ASMSTAGF(,ASMVTPTR)                             0135 00160000
         ST    @15,QVPLHDR                                         0135 00161000
*   QVPLTRLR=ADDR(ASMSTAGL);        /* INIT ADDR OF QUEUE TRAILER    */ 00162000
         LA    @15,ASMSTAGL(,ASMVTPTR)                             0136 00163000
         ST    @15,QVPLTRLR                                        0136 00164000
*   RFY                                                            0137 00165000
*     AIAPTR RSTD;                  /* RESTRICT REG TO POINT TO AIA  */ 00166000
*   QVPLFPTR=ADDR(AIANXAIA)-ADDR(AIA);/* INIT OFFSET OF FORWARD PTR  */ 00167000
         MVC   QVPLFPTR(2),@CH01945                                0138 00168000
*   RFY                                                            0139 00169000
*     AIAPTR UNRSTD;                /* RELEASE REG                   */ 00170000
*   QTYPE=TWO;                      /* SET QTYPE FOR SINGLE-THREADED,   00171000
*                                      DOUBLE-HEADED QUEUE           */ 00172000
         MVC   QTYPE(4),@CF00133                                   0140 00173000
*   CALL COMQRTN;                   /* INTERFACE WITH QUEUE VERIFIER */ 00174000
         BAL   @14,COMQRTN                                         0141 00175000
*   RFY                                                            0142 00176000
*     R14 RSTD;                     /* RESTRICT REG FOR RETURN ADDR  */ 00177000
*   R14=MAINRTRN;                   /* RESTORE RETURN ADDRESS        */ 00178000
         L     R14,MAINRTRN                                        0143 00179000
*   GEN(BR    R14);                 /* RETURN TO CALLER              */ 00180000
         BR    R14                                                      00181000
*   RFY                                                            0145 00182000
*    (R2,                                                          0145 00183000
*     ASMVTPTR,                                                    0145 00184000
*     R4,                                                          0145 00185000
*     R14) UNRSTD;                  /* RELEASE REGS                  */ 00186000
*                                                                  0146 00187000
*/********************************************************************/ 00188000
*/*                                                                  */ 00189000
*/*  ILRVLPRQ IS THE LGEPROCQ VERIFICATION ROUTINE.                  */ 00190000
*/*                                                                  */ 00191000
*/*  INPUT - REG 0 CONTAINS THE ADDRESS OF THE LGE                   */ 00192000
*/*          REG 1 CONTAINS THE ADDRESS OF THE SDWA                  */ 00193000
*/*          REG 8 CONTAINS THE ADDRESS OF THE WORKAREA              */ 00194000
*/*          REG 13 CONTAINS THE ADDRESS OF A REG-SAVE AREA          */ 00195000
*/*                                                                  */ 00196000
*/********************************************************************/ 00197000
*                                                                  0146 00198000
*ILRVLPRQ:                                                         0146 00199000
*   ENTRY;                          /* LGEPROCQ VERIFICATION ROUTINE */ 00200000
@EP00146 DS    0H                                                  0147 00201000
*   RFY                                                            0147 00202000
*    (R2,                                                          0147 00203000
*     R3,                                                          0147 00204000
*     R4) RSTD;                     /* RESTRICT REGS THAT SHOULD NOT    00205000
*                                      BE DESTROYED BY VERIFICATION     00206000
*                                      ROUTINE                       */ 00207000
*   RFY                                                            0148 00208000
*     R14 RSTD;                     /* RESTRICT REG CONTAINING RETURN   00209000
*                                      ADDRESS                       */ 00210000
*   WAREG->WORKA=''B;               /* CLEAR WORKAREA                */ 00211000
         XC    WORKA(72,WAREG),WORKA(WAREG)                        0149 00212000
*   MAINRTRN=R14;                   /* SAVE RETURN ADDRESS           */ 00213000
         ST    R14,MAINRTRN                                        0150 00214000
*   RFY                                                            0151 00215000
*     R14 UNRSTD;                   /* RELEASE REG                   */ 00216000
*   RFY                                                            0152 00217000
*     R7 RSTD;                      /* RESTRICT REG FOR STM          */ 00218000
*   STM(R2,R7,REGSAVE);             /* SAVE REGS DESTROYED BY CALL TO   00219000
*                                      ILRVLGE                       */ 00220000
         STM   R2,R7,REGSAVE(SAVEPTR)                              0153 00221000
*   CALL ILRVLGE;                   /* VERIFY LGE                    */ 00222000
         L     @15,@CA02877                                        0154 00223000
         BALR  @14,@15                                             0154 00224000
*   LM(R2,R7,REGSAVE);              /* RESTORE REGS DESTROYED BY CALL   00225000
*                                      TO ILRVLGE                    */ 00226000
         LM    R2,R7,REGSAVE(SAVEPTR)                              0155 00227000
*   RFY                                                            0156 00228000
*     R7 UNRSTD;                    /* RELEASE REG                   */ 00229000
*   RFY                                                            0157 00230000
*     RETCODE RSTD;                 /* RESTRICT REG CONTAINING RETURN   00231000
*                                      CODE                          */ 00232000
*   IF RETCODE=ZERO THEN            /* TEST FOR RETURN CODE OF 0 - 0158 00233000
*                                      INDICATING A VALID LGE        */ 00234000
         LTR   RETCODE,RETCODE                                     0158 00235000
         BNZ   @RF00158                                            0158 00236000
*     DO;                           /* CONTINUE ONLY IF VALID LGE    */ 00237000
*       RFY                                                        0160 00238000
*         RETCODE UNRSTD;           /* RELEASE REG                   */ 00239000
*       QVPLEVR=ADDR(ILRVAIAC);     /* INIT ADDR OF ELEMENT        0161 00240000
*                                      VERIFICATION ROUTINE          */ 00241000
         L     @15,@CA02879                                        0161 00242000
         ST    @15,QVPLEVR                                         0161 00243000
*       QVPLHDR=ADDR(INPARM->LGEPROCF);/* INIT ADDR OF QUEUE HEADER  */ 00244000
         ST    INPARM,QVPLHDR                                      0162 00245000
*       QVPLTRLR=ADDR(INPARM->LGEPROCL);/* INIT ADDR OF QUEUE TRLR   */ 00246000
         LR    @15,INPARM                                          0163 00247000
         LA    @15,LGEPROCL(,@15)                                  0163 00248000
         ST    @15,QVPLTRLR                                        0163 00249000
*       RFY                                                        0164 00250000
*         AIAPTR RSTD;              /* RESTRICT REG TO POINT TO AIA  */ 00251000
*       QVPLFPTR=ADDR(AIAFQPA)-ADDR(AIA);/* INIT OFFSET OF FORWARD 0165 00252000
*                                      POINTER                       */ 00253000
         LR    @06,AIAPTR                                          0165 00254000
         SLR   @06,AIAPTR                                          0165 00255000
         STH   @06,QVPLFPTR                                        0165 00256000
*       QVPLBPTR=ADDR(AIABQPA)-ADDR(AIA);/* INIT OFFSET OF BACKWARD     00257000
*                                      POINTER                       */ 00258000
         MVC   QVPLBPTR(2),@CH00145                                0166 00259000
*       RFY                                                        0167 00260000
*         AIAPTR UNRSTD;            /* RELEASE REG                   */ 00261000
*       QTYPE=THREE;                /* SET QTYPE FOR DOUBLE-THREADED,   00262000
*                                      DOUBLE-THREADED QUEUE         */ 00263000
         MVC   QTYPE(4),@CF00083                                   0168 00264000
*       CALL COMQRTN;               /* INTERFACE WITH QUEUE VERIFIER */ 00265000
         BAL   @14,COMQRTN                                         0169 00266000
*       RFY                                                        0170 00267000
*         RETCODE RSTD;             /* RESTRICT REG USED FOR RETURN     00268000
*                                      CODE                          */ 00269000
*     END;                          /* END OF SECTION TO PROCESS A 0171 00270000
*                                      VALID LGE                     */ 00271000
*   ELSE                            /* WHEN LGE IS NOT VALID         */ 00272000
*     RETCODE=INVPARM;              /* SET RETURN CODE TO INDICATE A    00273000
*                                      BAD PARAMETER WAS PASSED      */ 00274000
         B     @RC00158                                            0172 00275000
@RF00158 LA    RETCODE,24                                          0172 00276000
*   RFY                                                            0173 00277000
*     R14 RSTD;                     /* RESTRICT REG FOR RETURN ADDR  */ 00278000
@RC00158 DS    0H                                                  0174 00279000
*   R14=MAINRTRN;                   /* RESTORE RETURN ADDRESS        */ 00280000
         L     R14,MAINRTRN                                        0174 00281000
*   GEN(BR    R14);                 /* RETURN TO CALLER              */ 00282000
         BR    R14                                                      00283000
*   RFY                                                            0176 00284000
*    (R2,                                                          0176 00285000
*     R3,                                                          0176 00286000
*     R4,                                                          0176 00287000
*     R14,                                                         0176 00288000
*     RETCODE) UNRSTD;              /* RELEASE REGS                  */ 00289000
*                                                                  0177 00290000
*/********************************************************************/ 00291000
*/*                                                                  */ 00292000
*/*  ILRVSWTQ IS THE SARWAITQ VERIFICATION ROUTINE.                  */ 00293000
*/*                                                                  */ 00294000
