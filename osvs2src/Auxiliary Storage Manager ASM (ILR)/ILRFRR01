         L     @15,@CV02859                                        0462 01263000
         BALR  @14,@15                                             0462 01264000
*       RFY                                                        0463 01265000
*         R3 UNRSTD;                /* RELEASE REG                   */ 01266000
*       IF RETCODE=ZERO THEN        /* IF RETCODE IS 0, LGE IS     0464 01267000
*                                      ADDRESSABLE                   */ 01268000
         LTR   RETCODE,RETCODE                                     0464 01269000
         BNZ   @RF00464                                            0464 01270000
*         DO;                       /* CONTINUE VERIFICATION         */ 01271000
*           RFY                                                    0466 01272000
*            (ASMVTPTR,                                            0466 01273000
*             LGVTPTR,                                             0466 01274000
*             LGVTEPTR) RSTD;       /* RESTRICT REGS TO POINT TO THE    01275000
*                                      ASMVT, THE LGVT, AND AN LGVTE */ 01276000
*           ASMVTPTR=CVTASMVT;      /* GET ASMVT ADDR FROM CVT       */ 01277000
         L     @14,CVTPTR                                          0467 01278000
         L     ASMVTPTR,CVTASMVT(,@14)                             0467 01279000
*           LGVTPTR=ASMLGVT;        /* GET LGVT ADDR FROM ASMVT      */ 01280000
         L     LGVTPTR,ASMLGVT(,ASMVTPTR)                          0468 01281000
*           IF LGELGID<=LGVMAXLG THEN/* IF THIS LGID IS ALLOWABLE    */ 01282000
         L     @14,LGELGID(,LGEPTR)                                0469 01283000
         C     @14,LGVMAXLG(,LGVTPTR)                              0469 01284000
         BH    @RF00469                                            0469 01285000
*             DO;                   /* CONTINUE VERIFICATION         */ 01286000
*               LGVTEPTR=ADDR(LGVENTRS(LGELGID+ONE));/* CALCULATE  0471 01287000
*                                      LGVTE ADDRESS                 */ 01288000
         SLA   @14,3                                               0471 01289000
         LA    @14,LGVENTRS(@14,LGVTPTR)                           0471 01290000
         LR    LGVTEPTR,@14                                        0471 01291000
*               IF LGVELGEP^=LGEPTR THEN/* IF LGVTE DOES NOT POINT TO   01292000
*                                      LGE                           */ 01293000
         C     LGEPTR,LGVELGEP(,LGVTEPTR)                          0472 01294000
         BE    @RF00472                                            0472 01295000
*                 RETCODE=INVCB;    /* SET RETURN CODE TO INDICATE 0473 01296000
*                                      INVALID CB                    */ 01297000
         LA    RETCODE,8                                           0473 01298000
*               RFY                                                0474 01299000
*                (ASMVTPTR,                                        0474 01300000
*                 LGVTPTR,                                         0474 01301000
*                 LGVTEPTR) UNRSTD; /* RELEASE REGS                  */ 01302000
@RF00472 DS    0H                                                  0475 01303000
*             END;                                                 0475 01304000
*           ELSE                    /* IF LGID IS NOT ALLOWABLE      */ 01305000
*             RETCODE=INVCB;        /* SET RETURN CODE TO INDICATE 0476 01306000
*                                      INVALID CB                    */ 01307000
         B     @RC00469                                            0476 01308000
@RF00469 LA    RETCODE,8                                           0476 01309000
*         END;                                                     0477 01310000
*       ELSE                        /* IF RETURN CODE IS NOT 0, LGE     01311000
*                                      IS NOT ADDRESSABLE            */ 01312000
*         RETCODE=INVCB;            /* SET RETURN CODE TO INDICATE 0478 01313000
*                                      INVALID CB                    */ 01314000
         B     @RC00464                                            0478 01315000
@RF00464 LA    RETCODE,8                                           0478 01316000
*       INPARM=LGEPTR;              /* RESTORE VALUE OF INPUT R0     */ 01317000
@RC00464 LR    INPARM,LGEPTR                                       0479 01318000
*       RFY                                                        0480 01319000
*         R14 RSTD;                 /* RESTRICT REG FOR RETURN ADDR  */ 01320000
*       R14=RETURN2;                /* RESTORE RETURN ADDRESS        */ 01321000
         LR    R14,RETURN2                                         0481 01322000
*     END;                          /* END OF PROCESSING NON-0 INPUT */ 01323000
*   ELSE                            /* IF INPUT VALUE IS 0           */ 01324000
*     RETCODE=INVCB;                /* SET RETURN CODE TO INDICATE 0483 01325000
*                                      INVALID CB                    */ 01326000
         B     @RC00455                                            0483 01327000
@RF00455 LA    RETCODE,8                                           0483 01328000
*   GEN(BR    R14);                 /* RETURN TO CALLER              */ 01329000
@RC00455 DS    0H                                                  0484 01330000
         BR    R14                                                      01331000
*   RFY                                                            0485 01332000
*    (LGEPTR,                                                      0485 01333000
*     RETURN2,                                                     0485 01334000
*     R6,                                                          0485 01335000
*     R7,                                                          0485 01336000
