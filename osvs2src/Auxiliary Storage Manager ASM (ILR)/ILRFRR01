*VERLAIAS:                          /* LABEL TO ALLOW ILRVSWTQ TO  0244 00541000
*                                      VERIFY LATERAL AIA'S          */ 00542000
*   RTRNCODE=RETCODE;               /* SAVE RETURN CODE              */ 00543000
VERLAIAS ST    RETCODE,RTRNCODE                                    0244 00544000
*   RFY                                                            0245 00545000
*     RETCODE UNRSTD;               /* RELEASE REG                   */ 00546000
*   QVPLFPTR=ADDR(AIAFQPA)-ADDR(AIA);/* CHANGE OFFSET OF FORWARD PTR */ 00547000
         LR    @06,AIAPTR                                          0246 00548000
