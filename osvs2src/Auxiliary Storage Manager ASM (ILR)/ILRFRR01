*/*                                                                  */ 01341000
*/*  ILRVSCCW IS THE ROUTINE TO VERIFY A POTENTIAL SCCW              */ 01342000
*/*                                                                  */ 01343000
*/*  INPUT - REG 0 CONTAINS THE ADDRESS OF THE POTENTIAL SCCW        */ 01344000
*/*          REG 1 CONTAINS THE ADDRESS OF THE SDWA                  */ 01345000
*/*                                                                  */ 01346000
*/********************************************************************/ 01347000
*                                                                  0486 01348000
*ILRVSCCW:                                                         0486 01349000
*   ENTRY;                          /* SCCW VERIFICATION ROUTINE     */ 01350000
@EP00486 DS    0H                                                  0487 01351000
*   RFY                                                            0487 01352000
*    (RETURN1,                                                     0487 01353000
*     SCCWPTR,                                                     0487 01354000
*     R14) RSTD;                    /* RESTRICT REGS TO SAVE RETURN     01355000
*                                      ADDRESS AND INPUT R0 AND REG     01356000
*                                      CONTAINING RETURN ADDRESS     */ 01357000
*   IF INPARM^=ZERO THEN            /* TEST FOR INPUT VALUE OF 0     */ 01358000
         LTR   INPARM,INPARM                                       0488 01359000
         BZ    @RF00488                                            0488 01360000
*     DO;                           /* CONTINUE PROCESSING IF NOT 0  */ 01361000
*       RETURN1=R14;                /* SAVE RETURN ADDRESS           */ 01362000
         LR    RETURN1,R14                                         0490 01363000
*       RFY                                                        0491 01364000
*         R14 UNRSTD;               /* RELEASE REG                   */ 01365000
*       SCCWPTR=INPARM;             /* INIT ADDR OF CB TO BE VERIFIED*/ 01366000
         LR    SCCWPTR,INPARM                                      0492 01367000
*       INPARM=LENGTH(SCCW);        /* INIT LENGTH OF CB TO BE     0493 01368000
*                                      VERIFIED                      */ 01369000
         LA    INPARM,400                                          0493 01370000
*       RFY                                                        0494 01371000
*        (R3,                                                      0494 01372000
*         RETCODE) RSTD;            /* RESTRICT REG DESTROYED BY CALL   01373000
*                                      TO ADDRESS VERIFIER AND REG 0494 01374000
*                                      FOR RETURN CODE               */ 01375000
*       CALL IEAVEADV;              /* CALL ADDRESS VERIFIER         */ 01376000
         L     @15,@CV02859                                        0495 01377000
         BALR  @14,@15                                             0495 01378000
*       RFY                                                        0496 01379000
*         R3 UNRSTD;                /* RELEASE REG                   */ 01380000
*       IF RETCODE=ZERO&            /* IF RETCODE IS 0, SCCW IS    0497 01381000
*                                      ADDRESSABLE                   */ 01382000
*           (SCCWPTR+LENGTH(SCCW))<CVTNUCB&/* AND IF SCCW IS IN THE     01383000
*                                      NUCLEUS BUFFER AREA           */ 01384000
*           SCCWID=IDSCCW THEN      /* AND IF SCCW IDENTIFIER IS   0497 01385000
*                                      PRESENT                       */ 01386000
         LTR   RETCODE,RETCODE                                     0497 01387000
         BNZ   @RF00497                                            0497 01388000
         LA    @14,400                                             0497 01389000
         ALR   @14,SCCWPTR                                         0497 01390000
         L     @07,CVTPTR                                          0497 01391000
         C     @14,CVTNUCB(,@07)                                   0497 01392000
         BNL   @RF00497                                            0497 01393000
         CLI   SCCWID(SCCWPTR),X'87'                               0497 01394000
         BNE   @RF00497                                            0497 01395000
*         DO;                       /* CHECK FOR BAD DATA            */ 01396000
*           IF SCCWSKOP^=SEEKCODE|  /* IF SCCWSEEK DOES NOT CONTAIN     01397000
*                                      THE SEEK OPCODE               */ 01398000
*               SCCWSSOP^=SSECCODE| /* OR IF SCCWSSEC DOES NOT     0499 01399000
*                                      CONTAIN THE SET SECTOR OPCODE */ 01400000
*               SCCWSSAD^=ADDR(SCCWSECT) THEN/* OR IF SCCWSSEC DOES     01401000
*                                      NOT POINT TO SCCWSECT         */ 01402000
         CLI   SCCWSKOP(SCCWPTR),X'0B'                             0499 01403000
         BNE   @RT00499                                            0499 01404000
         CLI   SCCWSSOP(SCCWPTR),X'23'                             0499 01405000
         BNE   @RT00499                                            0499 01406000
         LA    @14,SCCWSECT(,SCCWPTR)                              0499 01407000
         SLR   @07,@07                                             0499 01408000
         ICM   @07,7,SCCWSSAD(SCCWPTR)                             0499 01409000
         CR    @14,@07                                             0499 01410000
         BE    @RF00499                                            0499 01411000
@RT00499 DS    0H                                                  0500 01412000
*             RETCODE=BADDATA;      /* SET RETURN CODE TO INDICATE 0500 01413000
*                                      BAD DATA                      */ 01414000
         LA    RETCODE,4                                           0500 01415000
*         END;                      /* END OF CHECK FOR BAD DATA     */ 01416000
*       ELSE                        /* IF ANY CB VERIFICATION FAILS  */ 01417000
*         RETCODE=INVCB;            /* SET RETURN CODE TO INDICATE 0502 01418000
*                                      INVALID CB                    */ 01419000
         B     @RC00497                                            0502 01420000
@RF00497 LA    RETCODE,8                                           0502 01421000
*       INPARM=SCCWPTR;             /* RESTORE VALUE OF INPUT R0     */ 01422000
@RC00497 LR    INPARM,SCCWPTR                                      0503 01423000
*       RFY                                                        0504 01424000
*         R14 RSTD;                 /* RESTRICT REG FOR RETURN ADDR  */ 01425000
*       R14=RETURN1;                /* RESTORE RETURN ADDRESS        */ 01426000
         LR    R14,RETURN1                                         0505 01427000
*     END;                          /* END OF PROCESSING NON-0 INPUT */ 01428000
*   ELSE                            /* IF INPUT VALUE IS 0           */ 01429000
*     RETCODE=INVCB;                /* SET RETURN CODE TO INDICATE 0507 01430000
*                                      INVALID CB                    */ 01431000
         B     @RC00488                                            0507 01432000
@RF00488 LA    RETCODE,8                                           0507 01433000
*   GEN(BR    R14);                 /* RETURN TO CALLER              */ 01434000
@RC00488 DS    0H                                                  0508 01435000
         BR    R14                                                      01436000
*   RFY                                                            0509 01437000
