*       RFY                                                        0571 01669000
*         CBPTR RSTD;               /* RESTRICT REG TO POINT TO CB TO   01670000
*                                      BE VERIFIED                   */ 01671000
*       CBPTR=INPARM;               /* INIT ADDR OF CB TO BE VERIFIED*/ 01672000
         LR    CBPTR,INPARM                                        0572 01673000
*       INPARM=LENGTH(IORB)+        /* INIT LENGTH OF CB TO BE     0573 01674000
*                                      VERIFIED - LENGTH OF IORB     */ 01675000
*           SALENGTH+               /* PLUS LENGTH OF A STANDARD SA  */ 01676000
*           ((LENGTH(IOSB)+7)/8*8)+ /* PLUS LENGTH OF IOSB           */ 01677000
*           ((LENGTH(SRB)+7)/8*8);  /* PLUS LENGTH OF SRB            */ 01678000
         LA    INPARM,272                                          0573 01679000
*       RFY                                                        0574 01680000
*        (R3,                                                      0574 01681000
*         RETCODE) RSTD;            /* RESTRICT REG DESTROYED BY CALL   01682000
*                                      TO ADDRESS VERIFIER AND REG 0574 01683000
*                                      FOR RETURN CODE               */ 01684000
*       CALL IEAVEADV;              /* CALL ADDRESS VERIFIER         */ 01685000
         L     @15,@CV02859                                        0575 01686000
         BALR  @14,@15                                             0575 01687000
*       IF RETCODE=ZERO&            /* IF RETURN CODE IS 0, IORB+ IS    01688000
*                                      ADDRESSABLE                   */ 01689000
*           IORID=IDIORB THEN       /* AND THE IORB IDENTIFIER IS  0576 01690000
*                                      PRESENT                       */ 01691000
         LTR   RETCODE,RETCODE                                     0576 01692000
         BNZ   @RF00576                                            0576 01693000
         CLI   IORID(IORBPTR),X'88'                                0576 01694000
         BNE   @RF00576                                            0576 01695000
*         DO;                       /* CONTINUE VERIFICATION         */ 01696000
*           CBPTR=IORPARTE;         /* INIT ADDR OF CB TO BE       0578 01697000
*                                      VERIFIED, NOW THE PARTE/SARTE */ 01698000
         L     CBPTR,IORPARTE(,IORBPTR)                            0578 01699000
*           INPARM=LENGTH(SARTE);   /* INIT LENGTH OF CB TO BE     0579 01700000
*                                      VERIFIED, VERIFY STORAGE FOR     01701000
*                                      THE LENGTH OF THE SARTE SINCE    01702000
*                                      IT IS THE SHORTER CB          */ 01703000
         LA    INPARM,48                                           0579 01704000
*           CALL IEAVEADV;          /* CALL ADDRESS VERIFIER         */ 01705000
         L     @15,@CV02859                                        0580 01706000
         BALR  @14,@15                                             0580 01707000
*           RFY                                                    0581 01708000
*             R3 UNRSTD;            /* RELEASE REG                   */ 01709000
*           IF RETCODE=ZERO THEN    /* IF RETURN CODE IS 0,        0582 01710000
*                                      PARTE/SARTE IS ADDRESSABLE    */ 01711000
         LTR   RETCODE,RETCODE                                     0582 01712000
         BNZ   @RF00582                                            0582 01713000
*             DO;                   /* CONTINUE VERIFICATION         */ 01714000
*               RFY                                                0584 01715000
*                (ENTRYPTR,                                        0584 01716000
*                 ASMVTPTR) RSTD;   /* RESTRICT REGS TO POINT TO   0584 01717000
*                                      PARTE/SARTE AND TO ASMVT      */ 01718000
*               ENTRYPTR=CBPTR->PARENN+ONE;/* CALCULATE ENTRY NUM    */ 01719000
         LA    @14,1                                               0585 01720000
         AH    @14,PARENN(,CBPTR)                                  0585 01721000
         LR    ENTRYPTR,@14                                        0585 01722000
*               RFY                                                0586 01723000
*                 CBPTR UNRSTD;     /* RELEASE REG                   */ 01724000
*               ASMVTPTR=CVTASMVT;  /* GET ASMVT ADDR FROM CVT       */ 01725000
         L     @14,CVTPTR                                          0587 01726000
         L     ASMVTPTR,CVTASMVT(,@14)                             0587 01727000
*               IF IORSWAP=OFF THEN /* IF THIS IORB IS NOT FOR A SWAP   01728000
*                                      DATA SET                      */ 01729000
         TM    IORSWAP(IORBPTR),B'00001000'                        0588 01730000
         BNZ   @RF00588                                            0588 01731000
*                 ENTRYPTR=ADDR(PARTENTS(ENTRYPTR));/* CALCULATE ADDR   01732000
*                                      OF PARTE                      */ 01733000
         LR    @14,ENTRYPTR                                        0589 01734000
         SLA   @14,6                                               0589 01735000
         L     @04,PARTPTR(,ASMVTPTR)                              0589 01736000
         LA    @14,PARTENTS-64(@14,@04)                            0589 01737000
         LR    ENTRYPTR,@14                                        0589 01738000
*               ELSE                /* IF IORB IS FOR A SWAP DATA SET*/ 01739000
*                 ENTRYPTR=ADDR(SARENTS(ENTRYPTR));/* CALCULATE ADDR    01740000
*                                      OF SARTE                      */ 01741000
         B     @RC00588                                            0590 01742000
@RF00588 LR    @14,ENTRYPTR                                        0590 01743000
         MH    @14,@CH00457                                        0590 01744000
         L     @04,SARTPTR(,ASMVTPTR)                              0590 01745000
         LA    @14,SARENTS-48(@14,@04)                             0590 01746000
         LR    ENTRYPTR,@14                                        0590 01747000
*               RFY                                                0591 01748000
*                 ASMVTPTR UNRSTD;  /* RELEASE REG                   */ 01749000
@RC00588 DS    0H                                                  0592 01750000
*               RFY                                                0592 01751000
*                 IOSBPTR RSTD;     /* RESTRICT REG TO POINT TO IOSB */ 01752000
*               IOSBPTR=IORBPTR+LENGTH(IORB)+SALENGTH;/* CALCULATE 0593 01753000
*                                      IOSB ADDRESS                  */ 01754000
         LA    IOSBPTR,112                                         0593 01755000
         ALR   IOSBPTR,IORBPTR                                     0593 01756000
*               IF IORPARTE=ENTRYPTR&/* IF IORPARTE IS THE SAME AS 0594 01757000
*                                      THE CALCULATED PARTE ADDR     */ 01758000
*                   IOSDVRID=IOSMISID&/* AND THE DRIVER ID IN THE  0594 01759000
*                                      IOSB IS THE MISC ID           */ 01760000
*                   IOSASID=MASTERID THEN/* AND THE ASID IN THE IOSB    01761000
*                                      IS MASTER'S ASID              */ 01762000
         C     ENTRYPTR,IORPARTE(,IORBPTR)                         0594 01763000
         BNE   @RF00594                                            0594 01764000
         CLI   IOSDVRID(IOSBPTR),X'01'                             0594 01765000
         BNE   @RF00594                                            0594 01766000
         CLC   IOSASID(2,IOSBPTR),@CH00055                         0594 01767000
         BNE   @RF00594                                            0594 01768000
*                 DO;               /* REFRESH IORB, IOSB, SRB FIELDS*/ 01769000
*                   IF ENTRYPTR->PAREUCBP->UCBRPS=ON THEN/* IF UCB 0596 01770000
*                                      INDICATES DEVICE IS AN RPS  0596 01771000
*                                      DEVICE                        */ 01772000
         L     @14,PAREUCBP(,ENTRYPTR)                             0596 01773000
         TM    UCBRPS(@14),B'00010000'                             0596 01774000
         BNO   @RF00596                                            0596 01775000
*                     IORFRPS=ON;   /* TURN ON RPS FLAG IN IORB      */ 01776000
         OI    IORFRPS(IORBPTR),B'01000000'                        0597 01777000
*                   RFY                                            0598 01778000
*                     ENTRYPTR UNRSTD;/* RELEASE REG                 */ 01779000
@RF00596 DS    0H                                                  0599 01780000
*                   IORSAVE=IORBPTR+LENGTH(IORB);/* REFRESH ADDR OF     01781000
*                                      SA IN IORB                    */ 01782000
         LA    @14,40                                              0599 01783000
         ALR   @14,IORBPTR                                         0599 01784000
         ST    @14,IORSAVE(,IORBPTR)                               0599 01785000
*                   IORIOSB=IOSBPTR;/* REFRESH ADDR OF IOSB IN IORB  */ 01786000
         ST    IOSBPTR,IORIOSB(,IORBPTR)                           0600 01787000
*                   IOSCC3WE=ON;    /* REFRESH FLAG IN IOSB TO     0601 01788000
*                                      REQUEST POST FOR SIO CC3      */ 01789000
         OI    IOSCC3WE(IOSBPTR),B'00100000'                       0601 01790000
*                   IOSIDR=ON;      /* REFRESH FLAG IN IOSB TO     0602 01791000
*                                      REQUEST ASYNCHRONOUS        0602 01792000
*                                      SCHEDULING                    */ 01793000
         OI    IOSIDR(IOSBPTR),B'00000100'                         0602 01794000
*                   IOSPSLL=ON;     /* REFRESH FLAG IN IOSB TO     0603 01795000
*                                      INDICATE LOCAL LOCK NOT WANTED   01796000
*                                      FOR POST STATUS PROCESSING    */ 01797000
*                   IOSTSLL=ON;     /* REFRESH FLAG IN IOSB TO     0604 01798000
*                                      INDICATE LOCAL LOCK NOT WANTED   01799000
*                                      BY TERMINATION ROUTINE        */ 01800000
         OI    IOSPSLL(IOSBPTR),B'00010100'                        0604 01801000
*                   IOSSRB=IOSBPTR+((LENGTH(IOSB)+7)/8*8);/* REFRESH    01802000
*                                      ADDR OF SRB IN IOSB           */ 01803000
         LA    @14,112                                             0605 01804000
         ALR   @14,IOSBPTR                                         0605 01805000
         ST    @14,IOSSRB(,IOSBPTR)                                0605 01806000
*                   IOSUSE=IORBPTR; /* REFRESH ADDR OF IORB IN IOSB  */ 01807000
         ST    IORBPTR,IOSUSE(,IOSBPTR)                            0606 01808000
*                   IOSFMSK=FILEMASK;/* REFRESH FILE MASK IN IOSB    */ 01809000
         MVI   IOSFMSK(IOSBPTR),X'88'                              0607 01810000
*                   RFY                                            0608 01811000
*                     ASMVTPTR RSTD;/* RESTRICT REG TO POINT TO ASMVT*/ 01812000
*                   ASMVTPTR=CVTASMVT;/* GET ASMVT ADDR FROM CVT     */ 01813000
         L     @02,CVTPTR                                          0609 01814000
         L     ASMVTPTR,CVTASMVT(,@02)                             0609 01815000
*                   IOSPGAD=ASMPCMP;/* REFRESH ADDR OF ILRCMP IN IOSB*/ 01816000
         MVC   IOSPGAD(4,IOSBPTR),ASMPCMP(ASMVTPTR)                0610 01817000
*                   IOSNRM=ASMPCMPN;/* REFRESH ADDR OF ILRCMPNE IN 0611 01818000
*                                      IOSB                          */ 01819000
         MVC   IOSNRM(4,IOSBPTR),ASMPCMPN(ASMVTPTR)                0611 01820000
*                   IOSABN=ASMPCMPA;/* REFRESH ADDR OF ILRCMPAE IN 0612 01821000
*                                      IOSB                          */ 01822000
         MVC   IOSABN(4,IOSBPTR),ASMPCMPA(ASMVTPTR)                0612 01823000
*                   IOSDIE=ASMPCMPD;/* REFRESH ADDR OF ILRCMPDI IN 0613 01824000
*                                      IOSB - HIGH ORDER BIT IS ON   */ 01825000
         MVC   IOSDIE(4,IOSBPTR),ASMPCMPD(ASMVTPTR)                0613 01826000
*                   RFY                                            0614 01827000
*                     ASMVTPTR UNRSTD;/* RELEASE REG                 */ 01828000
*                   RFY                                            0615 01829000
*                     SRBPTR RSTD;  /* RESTRICT REG TO POINT TO SRB  */ 01830000
*                   SRBPTR=IOSSRB;  /* GET SRB ADDR JUST CALCULATED  */ 01831000
         LR    SRBPTR,@14                                          0616 01832000
*                   SRBID=IDSRB;    /* REFRESH SRB IDENTIFIER        */ 01833000
         MVC   SRBID(4,SRBPTR),@CC02853                            0617 01834000
*                   SRBPARM=IOSBPTR;/* REFRESH ADDR OF IOSB IN SRB   */ 01835000
         ST    IOSBPTR,SRBPARM(,SRBPTR)                            0618 01836000
*                   SRBPRIOR=NONQUIES;/* REFRESH SRB PRIORITY FIELD  */ 01837000
         MVI   SRBPRIOR(SRBPTR),X'04'                              0619 01838000
*                   RFY                                            0620 01839000
*                    (SRBPTR,                                      0620 01840000
*                     IOSBPTR) UNRSTD;/* RELEASE REGS                */ 01841000
*                 END;              /* END OF REFRESH CODE           */ 01842000
*               ELSE                /* IF ANY VERIFICATION FAILS     */ 01843000
*                 RETCODE=INVCB;    /* SET RETURN CODE TO INDICATE 0622 01844000
*                                      INVALID CB                    */ 01845000
         B     @RC00594                                            0622 01846000
@RF00594 LA    RETCODE,8                                           0622 01847000
*             END;                                                 0623 01848000
*           ELSE                    /* IF RETURN CODE IS NOT 0,    0624 01849000
*                                      PARTE/SARTE IS NOT ADDRESSABLE*/ 01850000
*             RETCODE=INVCB;        /* SET RETURN CODE TO INDICATE 0624 01851000
*                                      INVALID CB                    */ 01852000
         B     @RC00582                                            0624 01853000
@RF00582 LA    RETCODE,8                                           0624 01854000
*         END;                                                     0625 01855000
*       ELSE                        /* IF EITHER IORB VERIFICATION 0626 01856000
*                                      FAILS                         */ 01857000
*         RETCODE=INVCB;            /* SET RETURN CODE TO INDICATE 0626 01858000
*                                      INVALID CB                    */ 01859000
         B     @RC00576                                            0626 01860000
@RF00576 LA    RETCODE,8                                           0626 01861000
*       INPARM=IORBPTR;             /* RESTORE VALUE OF INPUT R0     */ 01862000
@RC00576 LR    INPARM,IORBPTR                                      0627 01863000
*       RFY                                                        0628 01864000
*         R14 RSTD;                 /* RESTRICT REG FOR RETURN ADDR  */ 01865000
*       R14=RETURN1;                /* RESTORE RETURN ADDRESS        */ 01866000
         LR    R14,RETURN1                                         0629 01867000
*     END;                          /* END OF PROCESSING NON-0 INPUT */ 01868000
*   ELSE                            /* IF INPUT VALUE IS 0           */ 01869000
*     RETCODE=INVCB;                /* SET RETURN CODE TO INDICATE 0631 01870000
*                                      INVALID CB                    */ 01871000
         B     @RC00566                                            0631 01872000
@RF00566 LA    RETCODE,8                                           0631 01873000
*   GEN(BR    R14);                 /* RETURN TO CALLER              */ 01874000
@RC00566 DS    0H                                                  0632 01875000
         BR    R14                                                      01876000
*   RFY                                                            0633 01877000
*    (RETURN1,                                                     0633 01878000
*     IORBPTR,                                                     0633 01879000
*     R14,                                                         0633 01880000
*     RETCODE) UNRSTD;              /* RELEASE REGS                  */ 01881000
*                                                                  0634 01882000
*/********************************************************************/ 01883000
*/*                                                                  */ 01884000
*/*  ILRPSRMT IS THE RESOURCE MANAGER TERMINATION ROUTINE (RMTR)     */ 01885000
*/*           FOR PART MONITOR'S SRB AND SWAP DRIVER'S SRB.          */ 01886000
*/*                                                                  */ 01887000
*/*  INPUT - REG 1 CONTAINS THE ADDRESS OF THE DEQUEUED SRB          */ 01888000
*/*                                                                  */ 01889000
*/********************************************************************/ 01890000
*                                                                  0634 01891000
*ILRPSRMT:                                                         0634 01892000
*   ENTRY;                          /* RMTR FOR PTM AND SWPDR SRB'S  */ 01893000
@EP00634 DS    0H                                                  0635 01894000
*   RFY                                                            0635 01895000
*    (R1,                                                          0635 01896000
*     R14) RSTD;                    /* RESTRICT REG CONTAINING ADDR     01897000
*                                      OF DEQUEUED SRB AND REG     0635 01898000
*                                      CONTAINING RETURN ADDR        */ 01899000
*   GEN(SCHEDULE SRB=(1),SCOPE=GLOBAL)                             0636 01900000
*   REFS(CVTGSMQ,SRBFLNK,SRBSECT);  /* RESCHEDULE DEQUEUED SRB       */ 01901000
         SCHEDULE SRB=(1),SCOPE=GLOBAL                                  01902000
*   RETURN;                         /* RETURN TO PURGEDQ             */ 01903000
@EL00001 DS    0H                                                  0637 01904000
@EF00001 DS    0H                                                  0637 01905000
@ER00001 BR    @14                                                 0637 01906000
*   RFY                                                            0638 01907000
*    (R1,                                                          0638 01908000
*     R14) UNRSTD;                  /* RELEASE REGS                  */ 01909000
*                                                                  0639 01910000
*/********************************************************************/ 01911000
*/*                                                                  */ 01912000
*/*  COMQRTN IS AN INTERNAL ROUTINE THAT INTERFACES WITH THE GENERAL */ 01913000
*/*          SUPERVISOR QUEUE VERIFICATION ROUTINES.                 */ 01914000
*/*                                                                  */ 01915000
*/*  INPUT - REG 0 CONTAINS AN INPUT PARAMETER THAT MUST BE SAVED    */ 01916000
*/*          REG 1 CONTAINS THE ADDRESS OF THE SDWA                  */ 01917000
*/*          REG 8 CONTAINS THE ADDRESS OF THE WORKAREA              */ 01918000
*/*          REG 13 CONTAINS THE ADDRESS OF REG-SAVE AREA            */ 01919000
*/*                                                                  */ 01920000
*/*  OUTPUT - REG 15 CONTAINS A RETURN CODE                          */ 01921000
*/*                                                                  */ 01922000
*/********************************************************************/ 01923000
*                                                                  0639 01924000
*COMQRTN:                                                          0639 01925000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0639 01926000
COMQRTN  DS    0H                                                  0640 01927000
*   RFY                                                            0640 01928000
*    (R2,                                                          0640 01929000
*     R3,                                                          0640 01930000
*     R4,                                                          0640 01931000
*     R7) RSTD;                     /* RESTRICT REGS NOT TO BE     0640 01932000
*                                      DESTROYED BY THIS RTN         */ 01933000
*   RFY                                                            0641 01934000
*    (RETURN1,                                                     0641 01935000
*     R14) RSTD;                    /* RESTRICT REG TO SAVE RETURN 0641 01936000
*                                      ADDRESS AND REG CONTAINING  0641 01937000
*                                      RETURN ADDRESS                */ 01938000
*   RETURN1=R14;                    /* SAVE RETURN ADDRESS           */ 01939000
         LR    RETURN1,R14                                         0642 01940000
*   RFY                                                            0643 01941000
*     R14 UNRSTD;                   /* RELEASE REG                   */ 01942000
*                                                                  0643 01943000
*   /*****************************************************************/ 01944000
*   /*                                                               */ 01945000
*   /* PROCESS EMPTY DOUBLE-HEADED QUEUES.                           */ 01946000
*   /*                                                               */ 01947000
*   /*****************************************************************/ 01948000
*                                                                  0644 01949000
*   IF QTYPE>ONE&                   /* IF QUEUE IS DOUBLE-HEADED     */ 01950000
*       QVPLHDR->ELEMENT=ZERO THEN  /* AND EMPTY, DO NOT CALL QUEUE     01951000
*                                      VERIFIER ROUTINE              */ 01952000
         CLC   QTYPE(4),@CF00055                                   0644 01953000
         BNH   @RF00644                                            0644 01954000
         SLR   @15,@15                                             0644 01955000
         L     @14,QVPLHDR                                         0644 01956000
         C     @15,ELEMENT(,@14)                                   0644 01957000
         BNE   @RF00644                                            0644 01958000
*     DO;                                                          0645 01959000
*       QVPLTRLR->ELEMENT=ZERO;     /* INSURE LAST ELEMENT IS 0      */ 01960000
         L     @14,QVPLTRLR                                        0646 01961000
         ST    @15,ELEMENT(,@14)                                   0646 01962000
*       RFY                                                        0647 01963000
*         RETCODE RSTD;             /* RESTRICT REG USED FOR RETURN     01964000
*                                      CODE                          */ 01965000
*       RETCODE=ZERO;               /* SET SUCCESSFUL RETURN CODE    */ 01966000
         SLR   RETCODE,RETCODE                                     0648 01967000
*       RFY                                                        0649 01968000
*         RETCODE UNRSTD;           /* RELEASE REG                   */ 01969000
*     END;                          /* END OF SECTION TO PROCESS   0650 01970000
*                                      EMPTY DOUBLE-HEADED QUEUE     */ 01971000
*                                                                  0650 01972000
*   /*****************************************************************/ 01973000
*   /*                                                               */ 01974000
*   /* PROCESS ALL REMAINING QUEUES.                                 */ 01975000
*   /*                                                               */ 01976000
*   /*****************************************************************/ 01977000
*                                                                  0651 01978000
*   ELSE                            /* QUEUE IS NOT DOUBLE-HEADED OR    01979000
*                                      EMPTY                         */ 01980000
*     DO;                                                          0651 01981000
         B     @RC00644                                            0651 01982000
@RF00644 DS    0H                                                  0652 01983000
*       QVPLWKA=ADDR(QVWA);         /* INIT ADDR OF WORKAREA FOR   0652 01984000
*                                      QUEUE VERIFIER                */ 01985000
         LA    @15,QVWA                                            0652 01986000
         ST    @15,QVPLWKA                                         0652 01987000
*       QVPLODA=ADDR(SDWARA);       /* INIT ADDR OF OUTPUT DATA AREA */ 01988000
         LA    @15,SDWARA(,SDWAPTR)                                0653 01989000
         ST    @15,QVPLODA                                         0653 01990000
*       RFY                                                        0654 01991000
*         QVERPTR RSTD;             /* RESTRICT REG FOR ADDR OF QUEUE   01992000
*                                      VERIFIER                      */ 01993000
*       IF QTYPE=ONE THEN           /* IF QUEUE IS SINGLE-THREADED 0655 01994000
*                                      AND SINGLE-HEADED             */ 01995000
         CLC   QTYPE(4),@CF00055                                   0655 01996000
         BNE   @RF00655                                            0655 01997000
*         QVERPTR=ADDR(IEAVEQV1);   /* CALL QUEUE VERIFIER ENTRY 1   */ 01998000
         L     QVERPTR,@CV02860                                    0656 01999000
*       ELSE                                                       0657 02000000
*         IF QTYPE=TWO THEN         /* IF QUEUE IS SINGLE-THREADED 0657 02001000
*                                      AND DOUBLE-HEADED             */ 02002000
         B     @RC00655                                            0657 02003000
@RF00655 CLC   QTYPE(4),@CF00133                                   0657 02004000
         BNE   @RF00657                                            0657 02005000
*           QVERPTR=ADDR(IEAVEQV2); /* CALL QUEUE VERIFIER ENTRY 2   */ 02006000
         L     QVERPTR,@CV02861                                    0658 02007000
*         ELSE                      /* QUEUE MUST BE DOUBLE-THREADED    02008000
*                                      AND DOUBLE-HEADED             */ 02009000
*           QVERPTR=ADDR(IEAVEQV3); /* CALL QUEUE VERIFIER ENTRY 3   */ 02010000
         B     @RC00657                                            0659 02011000
@RF00657 L     QVERPTR,@CV02862                                    0659 02012000
*       SAVERG=INPARM;              /* SAVE VALUE OF INPUT REG 0     */ 02013000
@RC00657 DS    0H                                                  0660 02014000
@RC00655 ST    INPARM,SAVERG                                       0660 02015000
*       INPARM=SDWAPTR;             /* SET REG 0 TO ADDRESS OF SDWA,    02016000
*                                      PARM FOR EVR                  */ 02017000
         LR    INPARM,SDWAPTR                                      0661 02018000
*       PARMLPTR=ADDR(QVPL);        /* SET REG 1 TO ADDR OF QVPL     */ 02019000
         LA    PARMLPTR,QVPL                                       0662 02020000
*       CALL QVERIFY;               /* CALL APPROPRIATE Q VERIFIER   */ 02021000
         BALR  @14,@15                                             0663 02022000
*       RFY                                                        0664 02023000
*         QVERPTR UNRSTD;           /* RELEASE REG                   */ 02024000
*       RFY                                                        0665 02025000
*         RETCODE RSTD;             /* RESTRICT REG USED FOR RETURN     02026000
*                                      CODE                          */ 02027000
*       SDWAPTR=INPARM;             /* RESTORE ADDR OF SDWA          */ 02028000
         LR    SDWAPTR,INPARM                                      0666 02029000
*       INPARM=SAVERG;              /* RESTORE VALUE OF INPUT REG 0  */ 02030000
         L     INPARM,SAVERG                                       0667 02031000
*       RETCODE=RETCODE&RCMASK;     /* EXTRACT RETURN CODE FROM R15  */ 02032000
         N     RETCODE,@CF02834                                    0668 02033000
*     END;                          /* END OF SECTION TO CALL QUEUE     02034000
*                                      VERIFIER                      */ 02035000
*   RFY                                                            0670 02036000
*     R14 RSTD;                     /* RESTRICT REG FOR RETURN ADDR  */ 02037000
@RC00644 DS    0H                                                  0671 02038000
*   R14=RETURN1;                    /* RESTORE RETURN ADDRESS        */ 02039000
         LR    R14,RETURN1                                         0671 02040000
*   RFY                                                            0672 02041000
*    (R2,                                                          0672 02042000
*     R3,                                                          0672 02043000
*     R4,                                                          0672 02044000
*     RETURN1,                                                     0672 02045000
*     R7,                                                          0672 02046000
*     R14,                                                         0672 02047000
*     RETCODE) UNRSTD;              /* RELEASE REGS                  */ 02048000
*   END COMQRTN;                                                   0673 02049000
@EL00002 DS    0H                                                  0673 02050000
@EF00002 DS    0H                                                  0673 02051000
@ER00002 BR    @14                                                 0673 02052000
*   END ILRFRR01                                                   0674 02053000
*                                                                  0674 02054000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02055000
