*   RFY                                                            0655 02479000
*     R14 UNRSTD;                                                  0655 02480000
         EJECT                                                          02481000
         EJECT                                                          02482000
*                                                                  0656 02483000
*/********************************************************************/ 02484000
*/*                                                                  */ 02485000
*/*  AFSFREE:  FREE ASPCT EXTN OF STORAGE                            */ 02486000
*/*                                                                  */ 02487000
*/*  INPUT:  ADDRESS OF EXTN IN REGISTER 1                           */ 02488000
*/*                                                                  */ 02489000
*/*  PROCESS:  SET UP REGS, ISSUE FREEMAIN MACRO TO FREE ASPCT EXTN. */ 02490000
*/*                                                                  */ 02491000
*/*  OUTPUT:  RETURN CODE IN REGISTER 15                             */ 02492000
*/*             00 - NORMAL RETURN                                   */ 02493000
*/*             44 - FREEMAIN RETURNED ERROR CODE                    */ 02494000
*/*                                                                  */ 02495000
*/********************************************************************/ 02496000
*/*AFSFREE: CHART (AREF)                                             */ 02497000
*/*HEADER                                                          0656 02498000
*/*  ILRAFS00                                                      0656 02499000
*/* AFSFREE - FREEMAIN THE ASPCT                                     */ 02500000
*/*AFSFREE: E  ENTRY                                                 */ 02501000
*/*         P  SAVE THE RETURN ADDRESS                               */ 02502000
*/*         N  THIS NEXT TEST AND RESULTING LEGS OF CODE, IS A MACRO */ 02503000
*/*         N  PASS TEST AND EXPANSION.                              */ 02504000
*/*         D  (YES,,NO,%AFS07) SVC OK?                              */ 02505000
*/*         P  LOAD REG 14 WITH SUBPOOL ID                           */ 02506000
*/*         P  LOAD REG 0 WITH LENGTH                                */ 02507000
*/*         P  LOAD REG 1 WITH ADDRESS OF AREA TO BE FREED           */ 02508000
*/*         S  (,%AFS08) FREEMAIN: FREE THE ASPCT                    */ 02509000
*/*%AFS07:  P  LOAD REG 0 WITH LENTH                                 */ 02510000
*/*         P  LOAD REG 1 WITH ADDRESS OF AREA TO BE FREED           */ 02511000
*/*         P  LOAD REG 4 WITH CURRENT TCB ADDRESS                   */ 02512000
*/*         P  LOAD REG 7 WITH CURRENT ASCB ADDRESS                  */ 02513000
*/*         P  LOAD REG 3 WITH SUBPOOL ID                            */ 02514000
*/*         S  IEAVGM00: FREE THE ASPCT                              */ 02515000
*/*%AFS08:  D  (YES,%AFS09,NO,) ZERO RETURN CODE?                    */ 02516000
*/*         P  SAVE FREEMAINS RETURN CODE                            */ 02517000
*/*         P  SET RETURN CODE TO 68                                 */ 02518000
*/*%AFS09:  R  RETURN TO MAINLINE                                    */ 02519000
*                                                                  0656 02520000
         EJECT                                                          02521000
*AFSFREE:                           /* FREEMAIN AN EXTN OF ASPCT     */ 02522000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0656 02523000
AFSFREE  DS    0H                                                  0657 02524000
*   RFY                                                            0657 02525000
*     R14 RSTD;                                                    0657 02526000
*   AFSRET2=R14;                    /* SAVE THE RETURN ADDRESS       */ 02527000
         ST    R14,AFSRET2                                         0658 02528000
*   RFY                                                            0659 02529000
*    (R0,                                                          0659 02530000
*     R1,                                                          0659 02531000
*     R3,                                                          0659 02532000
*     R4,                                                          0659 02533000
*     R7) RSTD;                                                    0659 02534000
*   R0=AFSLNGTH;                    /* GET THE LENGTH OF AREA TO BE     02535000
*                                      FREED                         */ 02536000
         LR    R0,AFSLNGTH                                         0660 02537000
*   R1=AFSWORK;                     /* GET ADDRESS OF AREA TO FREE   */ 02538000
         LR    R1,AFSWORK                                          0661 02539000
*   R4=PSATOLD;                     /* GET CURRENT TCB ADDRESS       */ 02540000
         L     R4,PSATOLD                                          0662 02541000
*   R7=PSAAOLD;                     /* GET CURRENT ASCB ADDRESS      */ 02542000
         L     R7,PSAAOLD                                          0663 02543000
*   R3=AFSSPID;                     /* GET SUBPOOL ID                */ 02544000
         L     R3,AFSSPID                                          0664 02545000
*   GENERATE REFS(R0,R1,R3,R4,R7,CVTCRMN,CVTMAP);                  0665 02546000
        FREEMAIN RC,A=(1),LV=(0),SP=(3),BRANCH=YES                      02547000
*   RFY                                                            0666 02548000
*    (R0,                                                          0666 02549000
*     R1,                                                          0666 02550000
*     R3,                                                          0666 02551000
*     R4,                                                          0666 02552000
*     R7,                                                          0666 02553000
*     R14) UNRSTD;                                                 0666 02554000
*   RFY                                                            0667 02555000
*    (R14,                                                         0667 02556000
*     R15) RSTD;                                                   0667 02557000
*   IF R15^=AFSZERO                 /* FREEMAIN SUCCESSFUL?          */ 02558000
*     THEN                          /* NO                            */ 02559000
         LTR   R15,R15                                             0668 02560000
         BZ    @RF00668                                            0668 02561000
*     R15=AFS44;                    /* SET RETURN CODE TO 44         */ 02562000
         LA    R15,44                                              0669 02563000
*   R14=AFSRET2;                    /* GET RETURN ADDRESS            */ 02564000
@RF00668 L     R14,AFSRET2                                         0670 02565000
*   RFY                                                            0671 02566000
*    (R14,                                                         0671 02567000
*     R15) UNRSTD;                                                 0671 02568000
*   END AFSFREE;                                                   0672 02569000
@EL00005 DS    0H                                                  0672 02570000
@EF00005 DS    0H                                                  0672 02571000
@ER00005 BR    @14                                                 0672 02572000
*   END ILRAFS00;                                                  0673 02573000
*                                                                  0674 02574000
*/*  MACCOMP Y-4SC1CW/PZD81                                          */ 02575000
*/*  MACSTAT Y-475082/415304                                         */ 02576000
*/********************************************************************/ 02577000
*/*                                                                  */ 02578000
*/*  NAME:  ILRALS00                                                 */ 02579000
*/*                                                                  */ 02580000
*/*  STATUS:  OS/VS2 RELEASE 4, LEVEL 0                              */ 02581000
*/*                                                                  */ 02582000
*/*  ENTRY POINT:  ILRALS00                                          */ 02583000
*/*                                                                  */ 02584000
*/*  FUNCTION:  THIS SUBROUTINE SEARCHES THE LPME SECTORS OF A GIVEN */ 02585000
*/*             ASPCT AND CALLS A PROCESSING ROUTINE (ALSPROC) FOR   */ 02586000
*/*             EVERY VALID LSID IT FINDS.                           */ 02587000
*/*                                                                  */ 02588000
*/*  INPUT:  ADDRESS OF ASPCT IN REGISTER 0.                         */ 02589000
*/*                                                                  */ 02590000
*/*  PROCESS:  FOR EACH LPME EXTENSION, FIND ALL LPMES CONTAINING    */ 02591000
*/*            VALID LSIDS AND SEND THEIR ADDRESSES, ONE AT A TIME   */ 02592000
*/*            (ALONG WITH THE INPUT CONTROL INFORMATION), TO ALSPROC*/ 02593000
*/*              ALSPROC INPUT -                                     */ 02594000
*/*                REGISTER 0 = RPN OF LPME PASSED                   */ 02595000
*/*                REGISTER 1 = ADDRESS OF LPME                      */ 02596000
*/*                                                                  */ 02597000
*/*  OUTPUT:  NONE                                                   */ 02598000
*/*                                                                  */ 02599000
*/*  EXIT-NORMAL:  RETURN TO CALLER                                  */ 02600000
*/*                                                                  */ 02601000
*/*  EXIT-ERROR:  NONE                                               */ 02602000
*/*                                                                  */ 02603000
*/*  EXTERNAL REFERENCES:  NONE                                      */ 02604000
*/*                                                                  */ 02605000
*/*  REGISTERS CLOBBERED:  0 AND 1, 14 AND 15                        */ 02606000
*/*                                                                  */ 02607000
*/*  MAPPING MACROS USED:  ILRASPCT                                  */ 02608000
*/*                                                                  */ 02609000
*/*   NOTES:  DUE TO PLS OPTIMIZATION, RESTRICT ALL NECESSARY        */ 02610000
*/*          REGISTERS AROUND THIS ROUTINE.                          */ 02611000
*/*          ALL LABELS AND DATA AREAS CREATED BY THIS ROUTINE BEGIN */ 02612000
*/*          WITH THE THREE CHARACTERS 'ALS'.  THE CALLING MODULE    */ 02613000
*/*          MUST INCLUDE SUFFICIENT WORKSPACE FOR THIS ROUTINE'S    */ 02614000
*/*          DATA AREAS.  THIS AMOUNT IS COMPILER DEPENDENT WITH A   */ 02615000
*/*          MAXIMUM REQUIREMENT OF 44 BYTES.  THIS ROUTINE ALSO     */ 02616000
*/*          REQUIRES THAT THE CALLING MODULE DECLARES REGISTERS     */ 02617000
*/*          R0-R15.                                                 */ 02618000
*/*          MAPPING MACROS LISTED ABOVE MUST BE INCLUDED IN THE     */ 02619000
*/*          CALLING MODULE.  THE PROCEDURE ALSPROC MUST BE DEFINED  */ 02620000
*/*          BY THE CALLING MODULE.  THE PL/S MACRO FACILITY CAN BE  */ 02621000
*/*          USED TO CHANGE THIS NAME, IF DESIRED.                   */ 02622000
*/*                                                                  */ 02623000
*/*  CHANGE-ACTIVITY:  LINE 052480 DELETED FOR APAR OZ15336          */ 02624000
*/*                                                                  */ 02625000
*/********************************************************************/ 02626000
*/*ILRALS00: CHART (AREF)                                            */ 02627000
*/*HEADER                                                          0674 02628000
*/*           ILRALS00 - FIND ALL VALID LSIDS (& RPNS) IN AN ASPCT   */ 02629000
*/*ILRALS00: E ENTRY                                                 */ 02630000
*/*         P  SAVE THE RETURN ADDRESS                               */ 02631000
*/*         P  SAVE PTR TO ASPCT BASE                                */ 02632000
*/*         P  INITIALIZE BASE ARRAY SUBSCRIPT TO ONE                */ 02633000
*/*         P  INITIALIZE ASST EXT ARRAY SUBSCRIPT TO TWO            */ 02634000
*/*         P  INITIALIZE LPME EXT ARRAY SUBSCRIPT TO ONE            */ 02635000
*/*         P  INITIALIZE RPN VALUE TO ZERO                          */ 02636000
*/*         P  GET THE NUMBER OF LPME EXTS FROM BASE                 */ 02637000
*/*         P  GET PTR TO BASE LPME EXTENSION                        */ 02638000
*/*         P  GET PTR TO FIRST LPME                                 */ 02639000
*/*%ALS01:  D  (YES,%ALS02,NO,) LPME VALID?                          */ 02640000
*/*         D  (YES,,NO,%ALS03) RELEASED AFTER SAVE?                 */ 02641000
*/*%ALS02:  P  LOAD REG 0 WITH RPN                                   */ 02642000
*/*         P  LOAD REG 1 WITH PTR TO LPME                           */ 02643000
*/*         S  ALSPROC: CALL USER'S ROUTINE                          */ 02644000
*/*%ALS03:  P  INCREMENT THE RPN                                     */ 02645000
*/*         P  INCREMENT THE LPME EXT ARRAY SUBSCRIPT                */ 02646000
*/*         P  GET PTR TO NEXT LPME                                  */ 02647000
*/*         D  (YES,%ALS04,NO,) REACHED END OF ARRAY?                */ 02648000
*/*         D  (YES,,NO,%ALS01) LPME ZERO?                           */ 02649000
*/*%ALS04:  P  RESET LPME EXT SUBSCRIPT TO ONE                       */ 02650000
*/*         D  (YES,%ALS05,NO,) ARE THERE ANY EXTENSIONS?            */ 02651000
*/*         R  RETURN TO CALLER                                      */ 02652000
*/*%ALS05:  D  (YES,,NO,%ALS07) REACHED END OF ASST EXT ARRAY?       */ 02653000
*/*         P  RESET ASST EXT ARRAY SUBSCRIPT                        */ 02654000
*/*         P  INCREMENT THE BASE ARRAY SUBSCRIPT                    */ 02655000
*/*         D  (YES,%ALS06,NO,) REACHED END OF BASE ARRAY            */ 02656000
*/*         D  (YES,%ALS07,NO,) ANYMORE ASST EXTS?                   */ 02657000
*/*%ALS06:  R  RETURN TO CALLER                                      */ 02658000
*/*%ALS07:  P  GET PTR TO NEXT ASST EXTENSION                        */ 02659000
*/*         P  GET PTR TO NEXT LPME EXTENSION                        */ 02660000
*/*         P  GET PTR TO FIRST LPME IN EXTENSION                    */ 02661000
*/*         P  (,%ALS01) INCREMENT ASST EXT ARRAY SUBSCRIPT          */ 02662000
*                                                                  0674 02663000
*ILRALS00:                          /* LPME SEARCH                   */ 02664000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0674 02665000
*                                                                  0674 02666000
ILRALS00 DS    0H                                                  0675 02667000
*/*A-000000-999999                                           @Z40WPWH*/ 02668000
*                                                                  0675 02669000
*   DCL                                                            0675 02670000
*     ALSRETRN FIXED(31),           /* RETURN ADDRESS SAVE AREA      */ 02671000
*     ALSASPCT PTR(31),             /* PTR TO ASPCT BASE             */ 02672000
*     ALSEXPTR PTR(31),             /* PTR TO ASST EXTENSION         */ 02673000
*     ALSLEPTR PTR(31),             /* PTR TO LPME EXTENSION         */ 02674000
*     ALSLPME PTR(31),              /* PTR TO INDIVIDUAL LPME        */ 02675000
*     ALSRPN FIXED(31),             /* RELATIVE PAGE NUMBER          */ 02676000
*     ALSA FIXED(31),               /* ASST ARRAY SUBSCRIPT          */ 02677000
*     ALSL FIXED(31),               /* LPME ARRAY SUBSCRIPT          */ 02678000
*     ALSB FIXED(31),               /* BASE ARRAY SUBSCRIPT          */ 02679000
*     ALSCNTL FIXED(8);             /* LOOP CONTROLLER               */ 02680000
*   DCL                                                            0676 02681000
*     ALSZERO FIXED(31) CONSTANT(0),                               0676 02682000
*     ALSONE FIXED(31) CONSTANT(1),                                0676 02683000
*     ALSTWO FIXED(31) CONSTANT(2),                                0676 02684000
*     ALSFIVE FIXED(31) CONSTANT(5),                               0676 02685000
*     ALS257 FIXED(31) CONSTANT(257),                              0676 02686000
*     ALSLPMEL FIXED(31) INIT(LENGTH(ASPLPME)),                    0676 02687000
*     ALSON BIT(1) CONSTANT('1'B);                                 0676 02688000
*   RFY                                                            0677 02689000
*    (R0,                                                          0677 02690000
*     R14) RSTD;                                                   0677 02691000
*   ALSRETRN=R14;                   /* SAVE RETURN ADDRESS           */ 02692000
         ST    R14,ALSRETRN                                        0678 02693000
*   ALSASPCT=R0;                    /* SAVE PTR TO ASPCT BASE        */ 02694000
         LR    ALSASPCT,R0                                         0679 02695000
*   RFY                                                            0680 02696000
*    (R0,                                                          0680 02697000
*     R14) UNRSTD;                                                 0680 02698000
*   ALSB=ALSONE;                    /* INIT BASE SUBSCRIPT TO FIRST     02699000
*                                      ASST EXTENSION                */ 02700000
         LA    @12,1                                               0681 02701000
         LR    ALSB,@12                                            0681 02702000
*   ALSA=ALSTWO;                    /* INIT ASST SUBSCRIPT TO FIRST     02703000
*                                      LPME EXTENSION                */ 02704000
         LA    ALSA,2                                              0682 02705000
*   ALSL=ALSONE;                    /* INIT LPME SUBSCRIPT TO FIRST     02706000
*                                      LPME                          */ 02707000
         LR    ALSL,@12                                            0683 02708000
*   ALSRPN=ALSZERO;                 /* INIT THE RPN                  */ 02709000
         SLR   ALSRPN,ALSRPN                                       0684 02710000
*   ALSCNTL=ALSONE;                 /* INIT LOOP CONTROLLER GET PTR     02711000
*                                      TO LPME'S IN BASE             */ 02712000
         LR    ALSCNTL,@12                                         0685 02713000
*   ALSLPME=ADDR(ALSASPCT->ASPLPMES(ALSL));                        0686 02714000
         LR    @12,ALSL                                            0686 02715000
         SLA   @12,2                                               0686 02716000
         LA    ALSLPME,ASPLPMES-4(@12,ALSASPCT)                    0686 02717000
*   DO WHILE ALSCNTL^=ALSZERO;                                     0687 02718000
         B     @DE00687                                            0687 02719000
@DL00687 DS    0H                                                  0688 02720000
*     DO WHILE ALSL<ALS257;         /* IS THE LPME VALID OR HAS IT 0688 02721000
*                                      BEEN RELEASED AFTER SAVE?     */ 02722000
         B     @DE00688                                            0688 02723000
@DL00688 DS    0H                                                  0689 02724000
*       IF ALSLPME->ASPLVALD=ALSON|ALSLPME->ASPLSVRP=ALSON THEN    0689 02725000
         TM    ASPLVALD(ALSLPME),B'10001000'                       0689 02726000
         BZ    @RF00689                                            0689 02727000
*         DO;                       /* YES                           */ 02728000
*           RFY                                                    0691 02729000
*            (R0,                                                  0691 02730000
*             R1) RSTD;                                            0691 02731000
*           R0=ALSRPN;              /* LOAD REG0 WITH CURRENT RPN    */ 02732000
         LR    R0,ALSRPN                                           0692 02733000
*           R1=ALSLPME;             /* LOAD REG1 WITH PTR TO LPME    */ 02734000
         LR    R1,ALSLPME                                          0693 02735000
*           CALL ALSPROC;           /* CALL USER'S PROC TO PERFORM   */ 02736000
         BAL   @14,ALSPROC                                         0694 02737000
*           RFY                                                    0695 02738000
*            (R0,                                                  0695 02739000
*             R1) UNRSTD;           /* USER'S FUNCTION               */ 02740000
*         END;                                                     0696 02741000
*       ELSE                                                       0697 02742000
*         ;                                                        0697 02743000
@RF00689 DS    0H                                                  0698 02744000
*       ALSRPN=ALSRPN+ALSONE;       /* INCREMENT RPN FOR NEXT LPME   */ 02745000
         LA    @12,1                                               0698 02746000
         ALR   ALSRPN,@12                                          0698 02747000
*       ALSLPME=ALSLPME+ALSLPMEL;   /* INCREMENT LPME PTR TO NEXT  0699 02748000
*                                      LPME                          */ 02749000
         AL    ALSLPME,ALSLPMEL                                    0699 02750000
*       ALSL=ALSL+ALSONE;           /* INCREMENT LPME SUBSCRIPT      */ 02751000
         ALR   ALSL,@12                                            0700 02752000
*     END;                          /* ARE THERE ANY EXTENSIONS?     */ 02753000
@DE00688 CH    ALSL,@CH02288                                       0701 02754000
         BL    @DL00688                                            0701 02755000
*     IF ALSASPCT->ASPAEXCT=ALSZERO THEN/* NO                        */ 02756000
         ICM   @12,3,ASPAEXCT(ALSASPCT)                            0702 02757000
         BNZ   @RF00702                                            0702 02758000
*       ALSCNTL=ALSZERO;            /* FORCE LOOP EXIT               */ 02759000
         SLR   ALSCNTL,ALSCNTL                                     0703 02760000
*     ELSE                                                         0704 02761000
*       DO;                         /* YES, PROCESS ALL LPME'S IN  0704 02762000
*                                      EXTENSIONS                    */ 02763000
         B     @RC00702                                            0704 02764000
@RF00702 DS    0H                                                  0705 02765000
*         ALSL=ALSONE;              /* RESET LPME SUBSCRIPT          */ 02766000
         LA    ALSL,1                                              0705 02767000
*         IF ALSA<ALS257            /* REACHED END OF ASST ARRAY     */ 02768000
*           THEN                                                   0706 02769000
         CH    ALSA,@CH02288                                       0706 02770000
         BNL   @RF00706                                            0706 02771000
*           DO;                     /* NO ANYMORE ASST EXTS?         */ 02772000
*             IF ALSASPCT->ASPASSTP(ALSB)=ALSZERO THEN/* NO          */ 02773000
         LR    @12,ALSB                                            0708 02774000
         SLA   @12,2                                               0708 02775000
         L     @12,ASPASSTP-4(@12,ALSASPCT)                        0708 02776000
         LTR   @12,@12                                             0708 02777000
         BNZ   @RF00708                                            0708 02778000
*               ALSCNTL=ALSZERO;    /* FORCE LOOP EXIT               */ 02779000
         SLR   ALSCNTL,ALSCNTL                                     0709 02780000
*             ELSE                                                 0710 02781000
*               DO;                 /* YES GET PTR TO NEXT ASST EXT  */ 02782000
         B     @RC00708                                            0710 02783000
@RF00708 DS    0H                                                  0711 02784000
*ALSLOOP:                                                          0711 02785000
*                 ALSEXPTR=ALSASPCT->ASPASSTP(ALSB);               0711 02786000
*                                                                  0711 02787000
ALSLOOP  LR    @11,ALSB                                            0711 02788000
         SLA   @11,2                                               0711 02789000
         L     ALSEXPTR,ASPASSTP-4(@11,ALSASPCT)                   0711 02790000
*                 /***************************************************/ 02791000
*                 /*                                                 */ 02792000
*                 /* IF THERE ARE ANYMORE LPME EXTENSIONS, GET THE   */ 02793000
*                 /* POINTER TO THE LPME EXTENSION, POINTER TO THE   */ 02794000
*                 /* LPME, AND INCREMENT THE ASST ARRAY SUBSCRIPT    */ 02795000
*                 /*                                                 */ 02796000
*                 /***************************************************/ 02797000
*                                                                  0712 02798000
*                 IF ALSEXPTR->ASPSECTA(ALSA)^=ALSZERO THEN        0712 02799000
         LR    @11,ALSA                                            0712 02800000
         SLA   @11,2                                               0712 02801000
         L     @11,ASPSECTA-4(@11,ALSEXPTR)                        0712 02802000
         LTR   @11,@11                                             0712 02803000
         BZ    @RF00712                                            0712 02804000
*                   DO;                                            0713 02805000
         ST    @11,@TF00001                                        0713 02806000
*                     ALSLEPTR=ALSEXPTR->ASPSECTA(ALSA);           0714 02807000
         L     ALSLEPTR,@TF00001                                   0714 02808000
*                     ALSLPME=ADDR(ALSLEPTR->ASPLPMEE(ALSL));      0715 02809000
         LR    @12,ALSL                                            0715 02810000
         SLA   @12,2                                               0715 02811000
         LA    ALSLPME,ASPLPMEE-4(@12,ALSLEPTR)                    0715 02812000
*                     ALSA=ALSA+ALSONE;                            0716 02813000
         AH    ALSA,@CH00055                                       0716 02814000
*                   END;                                           0717 02815000
*                 ELSE                                             0718 02816000
*                   ALSCNTL=ALSZERO;                               0718 02817000
         B     @RC00712                                            0718 02818000
@RF00712 SLR   ALSCNTL,ALSCNTL                                     0718 02819000
*               END;                                               0719 02820000
*           END;                                                   0720 02821000
*         ELSE                                                     0721 02822000
*           DO;                                                    0721 02823000
         B     @RC00706                                            0721 02824000
@RF00706 DS    0H                                                  0722 02825000
*             ALSA=ALSONE;          /* RESET ASST SUBSCRIPT          */ 02826000
         LA    @12,1                                               0722 02827000
         LR    ALSA,@12                                            0722 02828000
*             ALSB=ALSB+ALSONE;     /* INCREMENT BASE SUBSCRIPT ARE     02829000
*                                      THERE ANYMORE ASST EXT        */ 02830000
         ALR   ALSB,@12                                            0723 02831000
*             IF ALSB<ALSFIVE&ALSASPCT->ASPASSTP(ALSB)^=ALSZERO THEN    02832000
         CH    ALSB,@CH01287                                       0724 02833000
         BNL   @RF00724                                            0724 02834000
         LR    @12,ALSB                                            0724 02835000
         SLA   @12,2                                               0724 02836000
         L     @12,ASPASSTP-4(@12,ALSASPCT)                        0724 02837000
         LTR   @12,@12                                             0724 02838000
         BNZ   @RT00724                                            0724 02839000
*                                   /* YES                           */ 02840000
*               GOTO ALSLOOP;       /* CONTINUE LOOPING              */ 02841000
*             ELSE                  /* NO                            */ 02842000
*               ALSCNTL=ALSZERO;    /* FORCE LOOP EXIT               */ 02843000
@RF00724 SLR   ALSCNTL,ALSCNTL                                     0726 02844000
*           END;                                                   0727 02845000
*       END;                                                       0728 02846000
@RC00706 DS    0H                                                  0729 02847000
*   END;                                                           0729 02848000
@RC00702 DS    0H                                                  0729 02849000
@DE00687 CH    ALSCNTL,@CH00088                                    0729 02850000
         BNE   @DL00687                                            0729 02851000
*   RFY                                                            0730 02852000
*     R14 RSTD;                                                    0730 02853000
*   R14=ALSRETRN;                                                  0731 02854000
         L     R14,ALSRETRN                                        0731 02855000
*   RFY                                                            0732 02856000
*     R14 UNRSTD;                                                  0732 02857000
*   END ILRALS00;                                                  0733 02858000
@EL00006 DS    0H                                                  0733 02859000
@EF00006 DS    0H                                                  0733 02860000
@ER00006 BR    @14                                                 0733 02861000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 02862000
*     GPR00P PTR(31) REG(0),                                       0734 02863000
*     GPR01P PTR(31) REG(1),                                       0734 02864000
*     GPR14P PTR(31) REG(14),                                      0734 02865000
*     GPR15P PTR(31) REG(15);                                      0734 02866000
*   DECLARE                         /* COMMON VARIABLES              */ 02867000
*     I256C CHAR(256) BASED,                                       0735 02868000
*     I031F FIXED(31) BASED,                                       0735 02869000
*     I031P PTR(31) BASED,                                         0735 02870000
*     I015F FIXED(15) BASED,                                       0735 02871000
*     I015P PTR(15) BASED,                                         0735 02872000
*     I008P PTR(8) BASED,                                          0735 02873000
*     I001C CHAR(1) BASED;          /* FOR PLS MACROS                */ 02874000
*   END ILRGOS01                    /* END OF RECOVERY ROUTINE -   0736 02875000
*                                      RETURN TO RTM                 */ 02876000
*                                                                  0736 02877000
