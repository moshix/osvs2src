*       FIXECB=ZERO;                /* INITIALIZE ECB TO ZERO        */ 00447000
         SLR   @12,@12                                             0167 00448000
         ST    @12,FIXECB(,DATARG)                                 0167 00449000
*       RFY                                                        0168 00450000
*         ENDADDR RSTD;             /* RESTRICT REG TO CONTAIN ADDR     00451000
*                                      OF SDWA END                   */ 00452000
*       ENDADDR=SAVERG+LENGTH(SDWA);/* CALCULATE ADDR OF SDWA END, 0169 00453000
*                                      SAVERG CONTAINS SDWA ADDR     */ 00454000
