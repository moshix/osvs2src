         TITLE 'ILRGOS01- GROUP OPERATORS RECOVERY ROUTINE             *00001000
                        '                                               00002000
ILRGOS01 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRGOS01  76.315'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @10,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@10                                         0001 00012000
*                                      RETURN ADDRESS                */ 00013000
*   DATARG=R0;                      /* SET UP ADDRESS OF DATA AREA   */ 00014000
         LR    DATARG,R0                                           0070 00015000
*   RFY                                                            0071 00016000
*    (R0) UNRSTD;                   /* RELEASE REG 0                 */ 00017000
*   SDWAPARM->ATAWORD1=R14;         /* SAVE REG14 THROUGHOUT MODULE  */ 00018000
         L     @15,SDWAPARM(,SDWAPTR)                              0072 00019000
         ST    R14,ATAWORD1(,@15)                                  0072 00020000
*   RFY                                                            0073 00021000
*     R14 UNRSTD;                   /* RELEASE REG 14                */ 00022000
*   SDWACOMP=INDFRR;                /* SET FRR INDICATOR IN SDWA     */ 00023000
         SLR   @12,@12                                             0074 00024000
         ST    @12,SDWACOMP(,SDWAPTR)                              0074 00025000
*   DO;                             /* SETRP                       0075 00026000
*                                      FRELOCK(ASM(ASHLOCK),SALLOC)  */ 00027000
*     RESPECIFY                                                    0076 00028000
*      (GPR00P,                                                    0076 00029000
*       GPR01P,                                                    0076 00030000
*       GPR14P,                                                    0076 00031000
*       GPR15P) RSTD;                                              0076 00032000
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */ 00033000
         MVI   SDWARCDE(GPR01P),X'00'                              0077 00034000
*     GPR01P->SDWAAPLW=ADDR(ASHLOCK);/* INITIALIZE ASMPAT LOCKWORD   */ 00035000
         L     @12,ASCBPTR                                         0078 00036000
         L     @12,RSMHDPTR(,@12)                                  0078 00037000
         LA    @12,ASHLOCK(,@12)                                   0078 00038000
         ST    @12,SDWAAPLW(,GPR01P)                               0078 00039000
*     GPR01P->SDWAACF3=GPR01P->SDWAACF3|'00001100'B;/* INITIALIZE  0079 00040000
*                                      LOCK FLAGS                    */ 00041000
         OI    SDWAACF3(GPR01P),B'00001100'                        0079 00042000
*     RESPECIFY                                                    0080 00043000
*      (GPR00P,                                                    0080 00044000
*       GPR01P,                                                    0080 00045000
*       GPR14P,                                                    0080 00046000
*       GPR15P) UNRSTD;                                            0080 00047000
*   END;                            /* INDICATE FREE SALLOC AND ASM     00048000
*                                      CLASS LOCK                    */ 00049000
*   GOTO COMMON;                    /* SKIP ESTAE ENTRY CODE AND   0082 00050000
*                                      START COMMON PROCESSING       */ 00051000
         B     COMMON                                              0082 00052000
*                                                                  0083 00053000
*/********************************************************************/ 00054000
*/*                                                                  */ 00055000
*/*       ILRCGOSE: ESTAE ENTRY POINT                                */ 00056000
*/*                                                                  */ 00057000
*/*       INPUT: REG 0= INDICATOR IF SDWA OBTAINED                   */ 00058000
*/*                  12 IF NO SDWA OBTAINED                          */ 00059000
*/*              REG 1= ADDRESS OF SDWA, IF R0 DOES NOT CONTAIN A 12 */ 00060000
*/*                     COMPLETION CODE IF NO SDWA OBTAINED          */ 00061000
*/*              REG 2= UNPREDICTABLE IF SDWA OBTAINED               */ 00062000
*/*                     ADDRESS OF ATA IF SDWA NOT OBTAINED          */ 00063000
*/*              REG13= ADDRESS OF 72 BYTE SAVEAREA IF SDWA OBTAINED */ 00064000
*/*                     UNPREDICTABLE IF NO SDWA OBTAINED            */ 00065000
*/*              REG14= RETURN ADDRESS                               */ 00066000
*/*              REG15= ILRCGOSE ENTRY POINT ADDRESS                 */ 00067000
*/*                                                                  */ 00068000
*/*       PROCESS: HANDLE ERRORS FOR SAVE OR ACTIVATE REQUESTS       */ 00069000
*/*                                                                  */ 00070000
*/*       NOTE: ENTRY CODE GENERATED BECAUSE INTERFACE IS            */ 00071000
*/*             DIFFERENT FROM FRR ENTRY AND PLS MUST NOT USE        */ 00072000
*/*             ANY REGISTERS BEFORE REGISTERS ARE SAVED.            */ 00073000
*/*             COMMON MODULE ADDRESSABILITY MUST BE SET UP          */ 00074000
*/*             BEFORE ALLOWING PLS TO GENERATE INSTRUCTIONS.        */ 00075000
*/*                                                                  */ 00076000
*/********************************************************************/ 00077000
*                                                                  0083 00078000
*   RFY                                                            0083 00079000
*     SAVEPTR RSTD;                 /* RESTRICT CRITICAL REGISTER- 0083 00080000
*                                      REG13                         */ 00081000
*   RFY                                                            0084 00082000
*    (R0,                                                          0084 00083000
*     R12,                                                         0084 00084000
*     R14,                                                         0084 00085000
*     R15) RSTD;                    /* RESTRICT INTERFACE REGS       */ 00086000
*   GENERATE DEFS(ILRCGOSE,GOTSDWA) /* ESTAE ENTRY POINT             */ 00087000
*       REFS(NOSDWA,BASEADDR,RESTORE,R0,R10,R12,SAVEPTR,R14,R15);  0085 00088000
ILRCGOSE DS    0H                ESTAE ENTRY POINT                      00089000
         ENTRY ILRCGOSE                                                 00090000
         USING *,R15              SET UP TEMPORARY BASE REGISTER        00091000
         C     R0,NOSDWA           SDWA OBTAINED                        00092000
         BNE   GOTSDWA             YES, BRANCH TO CONTINUE              00093000
         LA    R15,0              NO, INDICATE CONT WITH TERMINATION    00094000
         BR    R14                RETURN TO RTM                         00095000
GOTSDWA  DS    0H                 SDWA OBTAINED, CAN ATTEMPT RECOVERY   00096000
         STM   R14,R12,RESTORE(SAVEPTR) SAVE RTM REGISTERS              00097000
         L     R10,BASEADDR       PICK UP COMMON BASE REG VALUE         00098000
         DROP  R15                STOP TEMPORARY ADDRESSABILITY         00099000
*   RFY                                                            0086 00100000
*    (R0,                                                          0086 00101000
*     R12,                                                         0086 00102000
*     R14,                                                         0086 00103000
*     R15) UNRSTD;                  /* RELEASE REGS                  */ 00104000
*   SDWACOMP=SAVEPTR;               /* SAVE REG13 THROUGHOUT MODULE  */ 00105000
         ST    SAVEPTR,SDWACOMP(,SDWAPTR)                          0087 00106000
*   RFY                                                            0088 00107000
*     SAVEPTR UNRSTD;               /* RELEASE REG 13- PTR HAS BEEN     00108000
*                                      SAVED                         */ 00109000
*                                                                  0089 00110000
*   /*****************************************************************/ 00111000
*   /*                                                               */ 00112000
*   /* COMMON PROCESSING FOR ESTAE AND FRR ENTRY POINTS              */ 00113000
*   /*                                                               */ 00114000
*   /*****************************************************************/ 00115000
*                                                                  0089 00116000
*COMMON:                                                           0089 00117000
*   ;                                                              0089 00118000
COMMON   DS    0H                                                  0090 00119000
*   DO;                             /* SETRP                       0090 00120000
*                                      RC(0)RECPARM(MODID)RECORD(YES)*/ 00121000
*     RESPECIFY                                                    0091 00122000
*      (GPR00P,                                                    0091 00123000
*       GPR01P,                                                    0091 00124000
*       GPR14P,                                                    0091 00125000
*       GPR15P) RSTD;                                              0091 00126000
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */ 00127000
         MVI   SDWARCDE(GPR01P),X'00'                              0092 00128000
*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */ 00129000
         OI    SDWARCRD(GPR01P),B'10000000'                        0093 00130000
*     GPR15P=ADDR(MODID);           /* ACCESS RECORD PARAMETER LIST  */ 00131000
         LA    GPR15P,MODID                                        0094 00132000
*     GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD PARAMETERS */ 00133000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   0095 00134000
*     RESPECIFY                                                    0096 00135000
*      (GPR00P,                                                    0096 00136000
*       GPR01P,                                                    0096 00137000
*       GPR14P,                                                    0096 00138000
*       GPR15P) UNRSTD;                                            0096 00139000
*   END;                            /* RECORD DEFAULT PARMS IN SDWA-    00140000
*                                      RECORD ERROR, CONTINUE WITH 0097 00141000
*                                      TERMINATION, RECORD MODULE  0097 00142000
*                                      NAMES                         */ 00143000
*   RFY                                                            0098 00144000
*     ATAPTR RSTD;                  /* RESTRICT ATA REGISTER         */ 00145000
*   ATAPTR=SDWAPARM;                /* SET UP ADDRESS OF ATA         */ 00146000
         L     ATAPTR,SDWAPARM(,SDWAPTR)                           0099 00147000
*   SDWAVRA(1:LENGTH(ATA))=ATA;     /* RECORD ATA IN SDWA            */ 00148000
         MVC   SDWAVRA(24,SDWAPTR),ATA(ATAPTR)                     0100 00149000
*   SDWAURAL=LENGTH(ATA);           /* UPDATE LENGTH OF DATA IN    0101 00150000
*                                      RECORDING AREA                */ 00151000
         MVI   SDWAURAL(SDWAPTR),X'18'                             0101 00152000
*   IF ATAEPATH=ZERO                /* EPATH CHECKPOINTED?           */ 00153000
*     THEN                                                         0102 00154000
         SLR   @12,@12                                             0102 00155000
         L     @11,ATAEPATH(,ATAPTR)                               0102 00156000
         CR    @11,@12                                             0102 00157000
         BE    @RT00102                                            0102 00158000
*     GOTO EXIT;                    /* NO, RETURN TO RTM. NO RECOVERY   00159000
*                                      CAN BE DONE                   */ 00160000
*   RFY                                                            0104 00161000
*     EPAPTR RSTD;                  /* RESTRICT EPATH REGISTER       */ 00162000
*   EPAPTR=ATAEPATH;                /* SET UP POINTER TO EPATH       */ 00163000
         LR    EPAPTR,@11                                          0105 00164000
*   RFY                                                            0106 00165000
*     ATAPTR UNRSTD;                /* RELEASE ATA REGISTER          */ 00166000
*   RFY                                                            0107 00167000
*     RECPTR RSTD;                  /* RESTRICT REG                  */ 00168000
*   RECPTR=ADDR(SDWAVRA)+URAL;      /* SET UP POINTER TO AVAILABLE 0108 00169000
*                                      SPACE IN RECORDING AREA       */ 00170000
         LA    RECPTR,SDWAVRA(,SDWAPTR)                            0108 00171000
         SLR   @11,@11                                             0108 00172000
         IC    @11,URAL(,SDWAPTR)                                  0108 00173000
         ALR   RECPTR,@11                                          0108 00174000
*   RECAREA(1:LENGTH(EPATH))=EPATH; /* RECORD EPATH IN SDWA          */ 00175000
         MVC   RECAREA(52,RECPTR),EPATH(EPAPTR)                    0109 00176000
*   RFY                                                            0110 00177000
*     RECPTR UNRSTD;                /* RELEASE REG                   */ 00178000
*   URAL=URAL+LENGTH(EPATH);        /* UPDATE LENGTH OF DATA IN    0111 00179000
*                                      RECORDING AREA                */ 00180000
         AH    @11,@CH00376                                        0111 00181000
         STC   @11,URAL(,SDWAPTR)                                  0111 00182000
*                                                                  0112 00183000
*   /*****************************************************************/ 00184000
*   /*                                                               */ 00185000
*   /* RECORD APPROPRIATE MODULE NAMES IN SDWA                       */ 00186000
*   /*                                                               */ 00187000
*   /*****************************************************************/ 00188000
*                                                                  0112 00189000
*   IF SDWACOMP^=INDFRR             /* ESTAE ENTRY POINT ?           */ 00190000
*     THEN                                                         0112 00191000
         C     @12,SDWACOMP(,SDWAPTR)                              0112 00192000
         BE    @RF00112                                            0112 00193000
*     SDWAREXN=NGOSE;               /* CHANGE RECOVERY NAME TO     0113 00194000
*                                      ILRCGOSE                      */ 00195000
         MVC   SDWAREXN(8,SDWAPTR),NGOSE                           0113 00196000
*   IF EPAVSAMI=ON                  /* ILRVSAMI IN CONTROL ?         */ 00197000
*     THEN                                                         0114 00198000
@RF00112 TM    EPAVSAMI(EPAPTR),B'10000000'                        0114 00199000
         BNO   @RF00114                                            0114 00200000
*     SDWACSCT=NVSAMI;              /* INDICATE ILRVSAMI IN CONTROL     00201000
*                                      AT TIME OF ERROR              */ 00202000
         MVC   SDWACSCT(8,SDWAPTR),NVSAMI                          0115 00203000
*   ELSE                                                           0116 00204000
*     IF EPASAVE=ON                 /* ILRSAV IN CONTROL ?           */ 00205000
*       THEN                                                       0116 00206000
         B     @RC00114                                            0116 00207000
@RF00114 TM    EPASAVE(EPAPTR),B'00100000'                         0116 00208000
         BNO   @RF00116                                            0116 00209000
*       SDWACSCT=NSAVE;             /* INDICATE ILRSAV IN CONTROL AT    00210000
*                                      TIME OF ERROR                 */ 00211000
         MVC   SDWACSCT(8,SDWAPTR),NSAVE                           0117 00212000
*     ELSE                                                         0118 00213000
*       IF EPAACT=ON                /* ILRACT IN CONTROL ?           */ 00214000
*         THEN                                                     0118 00215000
         B     @RC00116                                            0118 00216000
@RF00116 TM    EPAACT(EPAPTR),B'00010000'                          0118 00217000
         BNO   @RF00118                                            0118 00218000
*         SDWACSCT=NACT;            /* INDICATE ILRACT IN CONTROL AT    00219000
*                                      TIME OF ERROR                 */ 00220000
         MVC   SDWACSCT(8,SDWAPTR),NACT                            0119 00221000
*       ELSE                                                       0120 00222000
*         IF EPARLG=ON              /* ILRRLG IN CONTROL?            */ 00223000
*           THEN                                                   0120 00224000
         B     @RC00118                                            0120 00225000
@RF00118 TM    EPARLG(EPAPTR),B'01000000'                          0120 00226000
         BNO   @RF00120                                            0120 00227000
*           SDWACSCT=NRLG;          /* INDICATE ILRRLG IN CONTROL AT    00228000
*                                      TIME OF ERROR                 */ 00229000
         MVC   SDWACSCT(8,SDWAPTR),NRLG                            0121 00230000
*         ELSE                                                     0122 00231000
*           ;                       /* ILRGOS IN CONTROL, NAME     0122 00232000
*                                      ALREADY SET IN SDWA           */ 00233000
@RF00120 DS    0H                                                  0123 00234000
*                                                                  0123 00235000
*   /*****************************************************************/ 00236000
*   /*                                                               */ 00237000
*   /* PROCESS RECORD ONLY ABEND                                     */ 00238000
*   /*                                                               */ 00239000
*   /*****************************************************************/ 00240000
*                                                                  0123 00241000
*   RFY                                                            0123 00242000
*     COMPCODE RSTD;                /* RESTRICT REG                  */ 00243000
@RC00118 DS    0H                                                  0123 00244000
@RC00116 DS    0H                                                  0123 00245000
@RC00114 DS    0H                                                  0124 00246000
*   COMPCODE=SDWAABCC&COMPCLR;      /* ISOLATE COMPLETION CODE       */ 00247000
         L     COMPCODE,SDWAABCC(,SDWAPTR)                         0124 00248000
         N     COMPCODE,COMPCLR                                    0124 00249000
*   IF(SDWACLUP^=ON)&               /* ABLE TO RETRY AND RECORD ONLY    00250000
*                                      ABEND?                        */ 00251000
*       ((COMPCODE=C085)|           /* X'085' ABEND ?                */ 00252000
*       (COMPCODE=C086)|            /* OR X'086' ABEND?              */ 00253000
*       (COMPCODE=C087)|            /* OR X'087' ABEND?              */ 00254000
*       (COMPCODE=CC0D))            /* OR X'C0D' ABEND?              */ 00255000
*     THEN                                                         0125 00256000
         TM    SDWACLUP(SDWAPTR),B'10000000'                       0125 00257000
         BO    @RF00125                                            0125 00258000
         CL    COMPCODE,@CF02138                                   0125 00259000
         BE    @RT00125                                            0125 00260000
         CL    COMPCODE,@CF02140                                   0125 00261000
         BE    @RT00125                                            0125 00262000
         CL    COMPCODE,@CF02142                                   0125 00263000
         BE    @RT00125                                            0125 00264000
         CL    COMPCODE,@CF02144                                   0125 00265000
         BNE   @RF00125                                            0125 00266000
@RT00125 DS    0H                                                  0126 00267000
*     DO;                           /* RECORD ONLY ABEND, RECORD & 0126 00268000
*                                      RETRY                         */ 00269000
*       SDWASRSV(1:RECRGLN)=RECRGS; /* SET UP RETRY IN SDWA FROM DATA   00270000
*                                      IN EPATH                      */ 00271000
         L     @12,EPARTYRG(,EPAPTR)                               0127 00272000
         MVC   SDWASRSV(60,SDWAPTR),RECRGS(@12)                    0127 00273000
*       DO;                         /* SETRP RC(4)RETREGS(YES)RETADDR   00274000
*                                      (RETRYLBL)                    */ 00275000
*         RESPECIFY                                                0129 00276000
*          (GPR00P,                                                0129 00277000
*           GPR01P,                                                0129 00278000
*           GPR14P,                                                0129 00279000
*           GPR15P) RSTD;                                          0129 00280000
*         GPR01P->SDWARCDE=4;       /* STORE RC INTO SDWA            */ 00281000
         MVI   SDWARCDE(GPR01P),X'04'                              0130 00282000
*         GPR01P->SDWARTYA=ADDR(RETRYLBL);/* SAVE RETRY ADDRESS      */ 00283000
         MVC   SDWARTYA(4,GPR01P),SDWANXT1(SDWAPTR)                0131 00284000
*         GPR01P->SDWAUPRG='1'B;    /* TURN ON RETREGS INDICATOR     */ 00285000
         OI    SDWAUPRG(GPR01P),B'00001000'                        0132 00286000
*         RESPECIFY                                                0133 00287000
*          (GPR00P,                                                0133 00288000
*           GPR01P,                                                0133 00289000
*           GPR14P,                                                0133 00290000
*           GPR15P) UNRSTD;                                        0133 00291000
*       END;                        /* INDICATE RETRY AT NEXT      0134 00292000
*                                      SEQUENTIAL INSTRUCTION,     0134 00293000
*                                      SPECIFY REGS FOR RETRY, AND 0134 00294000
*                                      INDICATE RETRY                */ 00295000
*       GOTO EXIT;                  /* RETURN TO RTM                 */ 00296000
         B     EXIT                                                0135 00297000
*     END;                          /* END OF PROCESSING FOR RECORD     00298000
*                                      ONLY ABEND                    */ 00299000
*   RFY                                                            0137 00300000
*     COMPCODE UNRSTD;              /* RELEASE REG                   */ 00301000
@RF00125 DS    0H                                                  0138 00302000
*                                                                  0138 00303000
*   /*****************************************************************/ 00304000
*   /*                                                               */ 00305000
*   /* FRR ENTRY POINT- DETERMINE IF ASM CLASS LOCK ALREADY HELD     */ 00306000
*   /*                                                               */ 00307000
*   /*****************************************************************/ 00308000
*                                                                  0138 00309000
*   IF SDWACOMP=INDFRR              /* FRR ENTRY POINT ?             */ 00310000
*     THEN                                                         0138 00311000
         ICM   @12,15,SDWACOMP(SDWAPTR)                            0138 00312000
         BNZ   @RF00138                                            0138 00313000
*     DO;                           /* FRR ENTRY POINT               */ 00314000
*       RFY                                                        0140 00315000
*        (R11,                                                     0140 00316000
*         R12,                                                     0140 00317000
*         LOCKCODE,                                                0140 00318000
*         R14) RSTD;                /* RESTRICT INTERFACE REGS       */ 00319000
*       GENERATE REFS(PSALITA,GETSALLC);                           0141 00320000
TESTASM  SETLOCK TEST,TYPE=ASM,BRANCH=(NOTHELD,GETSALLC),             **00321000
               RELATED=(VIO-FRR-ONLY,ILRGOS01)                          00322000
COBTSALC SETLOCK OBTAIN,TYPE=SALLOC,MODE=COND,                        **00323000
               RELATED=(VIO-FRR-ONLY,IEAVTRTS)                          00324000
*       IF LOCKCODE=NOOBT           /* UNABLE TO OBTAIN SALLOC WHILE    00325000
*                                      HOLDING ASM CLASS LOCK?       */ 00326000
*         THEN                      /* YES, RETURN TO RTM- NO      0142 00327000
*                                      RECOVERY                      */ 00328000
         CH    LOCKCODE,@CH00035                                   0142 00329000
         BE    @RT00142                                            0142 00330000
*         GOTO EXIT;                /* RETURN TO RTM                 */ 00331000
*       RFY                                                        0144 00332000
*        (R11,                                                     0144 00333000
*         R12,                                                     0144 00334000
*         LOCKCODE,                                                0144 00335000
*         R14) UNRSTD;              /* RELEASE INTERFACE REGS        */ 00336000
*     END;                          /* END OF SPECIAL PROCESSING FOR    00337000
*                                      FRR                           */ 00338000
*                                                                  0146 00339000
*   /*****************************************************************/ 00340000
*   /*                                                               */ 00341000
*   /* ESTAE ENTRY POINT- MUST GET 200 BYTE WORKAREA & LOCAL LOCK    */ 00342000
*   /*                                                               */ 00343000
*   /*****************************************************************/ 00344000
*                                                                  0146 00345000
*   ELSE                                                           0146 00346000
*     DO;                           /* ESTAE ENTRY POINT             */ 00347000
         B     @RC00138                                            0146 00348000
@RF00138 DS    0H                                                  0147 00349000
*       RFY                                                        0147 00350000
*         SAVERG RSTD;              /* RESTRICT SPECIAL REG          */ 00351000
*       SAVERG=SDWAPTR;             /* SAVE SDWA ADDRESS ACROSS    0148 00352000
*                                      GETMAIN                       */ 00353000
         LR    SAVERG,SDWAPTR                                      0148 00354000
*       RFY                                                        0149 00355000
*        (R0,                                                      0149 00356000
*         R1,                                                      0149 00357000
*         R14,                                                     0149 00358000
*         RETCODE) RSTD;            /* RESTRICT INTERFACE REGS       */ 00359000
*       GENERATE;                                                  0150 00360000
*                                        /* ISSUE SVC GETMAIN */        00361000
         GETMAIN RC,LV=200,SP=245,                                    **00362000
               RELATED=(SP245-200,ILRCGOSE)                             00363000
*       IF RETCODE^=ZERO            /* GETMAIN SUCCESSFUL?           */ 00364000
*         THEN                                                     0151 00365000
         LTR   RETCODE,RETCODE                                     0151 00366000
         BNZ   @RT00151                                            0151 00367000
*         GOTO PREEXIT;             /* NO, RETURN TO RTM- CANNOT   0152 00368000
*                                      RECOVER                       */ 00369000
*       DATARG=R1;                  /* SAVE ADDRESS OF DATA AREA     */ 00370000
*                                                                  0153 00371000
         LR    DATARG,R1                                           0153 00372000
*       /*************************************************************/ 00373000
*       /*                                                           */ 00374000
*       /* ADDRESSABILITY TO AUTOMATIC DATA AREA NOT SET UP UNTIL    */ 00375000
*       /* COMMON ADDRESSABILITY SET UP FOR BOTH ESTAE AND FRR ENTRY */ 00376000
*       /* POINTS                                                    */ 00377000
*       /*                                                           */ 00378000
*       /*************************************************************/ 00379000
*                                                                  0154 00380000
*       RFY                                                        0154 00381000
*        (R0,                                                      0154 00382000
*         R1,                                                      0154 00383000
*         R14,                                                     0154 00384000
*         RETCODE) UNRSTD;          /* RELEASE INTERFACE REGS        */ 00385000
*                                                                  0155 00386000
*       /*************************************************************/ 00387000
*       /*                                                           */ 00388000
*       /* - ILRVSAMI IN CONTROL - MUST DETERMINE IF ENDREQ TO BE    */ 00389000
*       /* ISSUED AND ISSUE REQUEST BEFORE ANY LOCKS ARE OBTAINED    */ 00390000
*       /*                                                           */ 00391000
*       /*************************************************************/ 00392000
*                                                                  0155 00393000
*       RFY                                                        0155 00394000
*         IFGRPL BASED(EPAVWKA);    /* SET UP ADDRESSABILITY TO RPL  */ 00395000
*       IF EPAVSAMI=ON&             /* ILRVSAMI IN CONTROL AT TIME OF   00396000
*                                      ERROR AND                     */ 00397000
*           EPARPLB=ON&             /* RPL ALREADY SET UP AND        */ 00398000
*           SAVERG->SDWAPERC=ON     /* PERCOLATED TO FROM ANOTHER RTN   00399000
*                                      ?                             */ 00400000
*         THEN                                                     0156 00401000
         TM    EPAVSAMI(EPAPTR),B'10000001'                        0156 00402000
         BNO   @RF00156                                            0156 00403000
         TM    SDWAPERC(SAVERG),B'00010000'                        0156 00404000
         BNO   @RF00156                                            0156 00405000
*         DO;                       /* YES, ISSUE ENDREQ             */ 00406000
*           RFY                                                    0158 00407000
*            (R0,                                                  0158 00408000
*             R1,                                                  0158 00409000
*             R14,                                                 0158 00410000
*             R15) RSTD;            /* RESTRICT INTERFACE REGS       */ 00411000
*           RFY                                                    0159 00412000
*             SAVEPTR RSTD;         /* RESTRICT PTR TO SAVEAREA    0159 00413000
*                                                            @ZM43073*/ 00414000
*           SAVEPTR=DATARG;         /* USE BEGINNING OF WORKAREA FOR    00415000
*                                      SAVEAREA              @ZM43073*/ 00416000
         LR    SAVEPTR,DATARG                                      0160 00417000
*           R1=EPAVWKA;             /* SET UP ADDRESS OF RPL         */ 00418000
         L     R1,EPAVWKA(,EPAPTR)                                 0161 00419000
*           GEN( ENDREQ RPL=(1));   /* CLEAN UP VSAM RESOURCES       */ 00420000
          ENDREQ RPL=(1)                                                00421000
*           RFY                                                    0163 00422000
*            (R0,                                                  0163 00423000
*             R1,                                                  0163 00424000
*             R14,                                                 0163 00425000
*             R15) UNRSTD;          /* RELEASE REGS                  */ 00426000
*           RFY                                                    0164 00427000
*             SAVEPTR UNRSTD;       /* RELEASE REG FOR SAVEAREA    0164 00428000
*                                                            @ZM43073*/ 00429000
*         END;                      /* END OF ILRVSAMI SPECIAL     0165 00430000
*                                      PROCESSING                    */ 00431000
*                                                                  0166 00432000
*       /*************************************************************/ 00433000
*       /*                                                           */ 00434000
*       /* BEFORE PERFORMING ESTAE RECOVERY, WHICH WILL OBTAIN THE   */ 00435000
*       /* SALLOC AND ASM LOCKS, THE SDWA MUST BE PAGEFIXED TO       */ 00436000
*       /* PREVENT GOS01 FROM SUFFERING A PAGE FAULT WHILE HOLDING A */ 00437000
*       /* GLOBAL LOCK. IF AN ERROR OCCURS DURING THE PAGEFIX        */ 00438000
*       /* PROCESSING, NO FURTHER RECOVERY WILL TAKE PLACE.          */ 00439000
*       /*                                                           */ 00440000
*       /*************************************************************/ 00441000
*                                                                  0166 00442000
*       RFY                                                        0166 00443000
*         FIXECB BASED(DATARG);     /* USE FIRST WORD OF WORKAREA FOR   00444000
*                                      ECB                           */ 00445000
