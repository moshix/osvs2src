*                   AFSEXPTR=AFSZERO;/* FORCE LOOP EXIT              */ 02445000
         B     @RC00642                                            0644 02446000
@RF00642 SLR   AFSEXPTR,AFSEXPTR                                   0644 02447000
*               END;                                               0645 02448000
*             ELSE                  /* YES, REACHED END OF ARRAY     */ 02449000
*               AFSEXPTR=AFSZERO;   /* FORCE LOOP EXIT               */ 02450000
         B     @RC00639                                            0646 02451000
@RF00639 SLR   AFSEXPTR,AFSEXPTR                                   0646 02452000
*           END;                                                   0647 02453000
@RC00639 DS    0H                                                  0648 02454000
*       END;                        /* PHYSICAL END OF DO WHILE LOOP    02455000
*                                      FOR ASST EXTENSIONS           */ 02456000
