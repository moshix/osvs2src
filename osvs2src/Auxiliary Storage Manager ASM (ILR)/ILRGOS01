*                 IF AFSEXPTR->ASPEIDNT='ASST' THEN/* YES            */ 02439000
         CLC   ASPEIDNT(4,AFSEXPTR),@CC02295                       0642 02440000
         BNE   @RF00642                                            0642 02441000
*                   AFSA=AFSONE;    /* RESET ASST SUBSCRIPT          */ 02442000
