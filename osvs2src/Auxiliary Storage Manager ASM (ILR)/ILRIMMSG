         TITLE 'ILRIMMSG -- ASM RIM MESSAGE MODULE                     *00001000
                        '                                               00002000
ILRIMMSG CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRIMMSG  76.087'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*                                                                       00018000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00019000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00020000
* WILL ASSEMBLE PROPERLY.                                               00021000
*   MSGPLPTR=R1;                    /* GET ADDR OF MESSAGE PARM LIST */ 00022000
         LR    MSGPLPTR,R1                                         0153 00023000
*   R2=NVTPTR;                      /* GET NVT ADDRESS               */ 00024000
         L     R2,NVTPTR(,MSGPLPTR)                                0154 00025000
*   R3=CVTPTR;                      /* GET CVT ADDRESS               */ 00026000
         L     R3,CVTPTR(,MSGPLPTR)                                0155 00027000
*   MSGOPREF=IEA;                   /* SET MSG ID PREFIX             */ 00028000
         MVC   MSGOPREF(3),IEA                                     0156 00029000
*   MSGOID=MSGID;                   /* MOVE MSG ID TO MSG AREA       */ 00030000
         MVC   MSGOID(4),MSGID(MSGPLPTR)                           0157 00031000
*   MSGOTEXT=BLANK;                 /* BLANK MESSAGE TEXT AREA       */ 00032000
         MVI   MSGOTEXT+1,C' '                                     0158 00033000
         MVC   MSGOTEXT+2(62),MSGOTEXT+1                           0158 00034000
         MVI   MSGOTEXT,C' '                                       0158 00035000
*   VALFLGS=''B;                    /* INITLZ VALID REPLY FLGS (USED    00036000
*                                      WITH 'D' TYPE MSGS)           */ 00037000
*                                                                  0159 00038000
         XC    VALFLGS(4),VALFLGS                                  0159 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* LOOP THROUGH BRANCH TABLE TO GET MESSAGE SUBROUTINE           */ 00042000
*   /* CORRESPONDING TO MESSAGE ID RECEIVED                          */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0160 00046000
*   DO WRKCTR=DIM(MSGBTENT) TO ONE BY-1;                           0160 00047000
*                                                                  0160 00048000
         LA    WRKCTR,24                                           0160 00049000
@DL00160 DS    0H                                                  0161 00050000
*     /***************************************************************/ 00051000
*     /*                                                             */ 00052000
*     /* TEST FOR MSG ID MATCH                                       */ 00053000
*     /*                                                             */ 00054000
*     /***************************************************************/ 00055000
*                                                                  0161 00056000
*     IF MSGID=MSGBTID(WRKCTR) THEN /* MSG IDS MATCH ?               */ 00057000
         LR    @07,WRKCTR                                          0161 00058000
         SLA   @07,3                                               0161 00059000
         LA    @04,MSGBTID-8(@07)                                  0161 00060000
         CLC   MSGID(4,MSGPLPTR),0(@04)                            0161 00061000
         BNE   @RF00161                                            0161 00062000
*       DO;                                                        0162 00063000
*                                                                  0162 00064000
*         /***********************************************************/ 00065000
*         /*                                                         */ 00066000
*         /* INVOKE PROPER SUBROUTINE TO BUILD MESSAGE               */ 00067000
*         /*                                                         */ 00068000
*         /***********************************************************/ 00069000
*                                                                  0163 00070000
*         MSGSPTR=MSGBTSUB(WRKCTR); /* GET MSG SUBRTN ADDRESS        */ 00071000
         L     MSGSPTR,MSGBTSUB-8(@07)                             0163 00072000
*         CALL MSGNNNT;             /* BUILD MESSAGE                 */ 00073000
         LR    @15,MSGSPTR                                         0164 00074000
         BALR  @14,@15                                             0164 00075000
*         WRKCTR=ONE;               /* END LOOP                      */ 00076000
         LA    WRKCTR,1                                            0165 00077000
*       END;                                                       0166 00078000
*     ELSE                                                         0167 00079000
*       ;                           /* NO MATCH ?                    */ 00080000
@RF00161 DS    0H                                                  0168 00081000
*   END;                            /* END LOOP THRU BRANCH TABLE    */ 00082000
*                                                                  0168 00083000
         BCTR  WRKCTR,0                                            0168 00084000
         LTR   WRKCTR,WRKCTR                                       0168 00085000
         BP    @DL00160                                            0168 00086000
*   /*****************************************************************/ 00087000
*   /*                                                               */ 00088000
*   /* WTO OR WTOR MESSAGE, DEPENDING ON MESSAGE ID TYPE CODE. IF 'W'*/ 00089000
*   /* TYPE WTO MESSAGE, THE SYSTEM IS PUT INTO A WAIT CONDITION     */ 00090000
*   /*                                                               */ 00091000
*   /*****************************************************************/ 00092000
*                                                                  0169 00093000
*   IF MSGIDTYP=DECISION THEN       /* WTOR TYPE MESSAGE ?           */ 00094000
         CLI   MSGIDTYP(MSGPLPTR),C'D'                             0169 00095000
         BNE   @RF00169                                            0169 00096000
*     DO;                                                          0170 00097000
*                                                                  0170 00098000
*       /*************************************************************/ 00099000
*       /*                                                           */ 00100000
*       /* LOOP UNTIL VALID REPLY RECEIVED FROM OPERATOR             */ 00101000
*       /*                                                           */ 00102000
*       /*************************************************************/ 00103000
*                                                                  0171 00104000
*       VALREPLY=NO;                /* INITLZ LOOP CONTROL VBL       */ 00105000
         NI    VALREPLY,B'10111111'                                0171 00106000
*       DO WHILE(VALREPLY=NO);                                     0172 00107000
*                                                                  0172 00108000
         B     @DE00172                                            0172 00109000
@DL00172 DS    0H                                                  0173 00110000
*         /***********************************************************/ 00111000
*         /*                                                         */ 00112000
*         /* USE NIP SERVICE TO WTOR MESSAGE                         */ 00113000
*         /*                                                         */ 00114000
*         /***********************************************************/ 00115000
*                                                                  0173 00116000
*         GEN(IEAPMNIP TYPE=WTOR,PARAM=NWTORPL)                    0173 00117000
*         REFS(NVTWTOR,NWTORPL);                                   0173 00118000
         IEAPMNIP TYPE=WTOR,PARAM=NWTORPL                               00119000
*         REPLYPTR=R1;              /* GET ADDR OF OPER REPLY        */ 00120000
         LR    REPLYPTR,R1                                         0174 00121000
*         VNP04R1=REPLYPTR;         /* RETURN ADDR OF OPERATOR REPLY    00122000
*                                      AREA                          */ 00123000
         L     @06,VNP04SAP(,R13)                                  0175 00124000
         ST    REPLYPTR,VNP04R1(,@06)                              0175 00125000
*         CALL SYNCHK;              /* CHECK FOR VALID REPLY         */ 00126000
         BAL   @14,SYNCHK                                          0176 00127000
*       END;                        /* END WTOR LOOP                 */ 00128000
@DE00172 TM    VALREPLY,B'01000000'                                0177 00129000
         BZ    @DL00172                                            0177 00130000
*     END;                          /* END WTOR PROCESSING           */ 00131000
*   ELSE                                                           0179 00132000
*                                                                  0179 00133000
*     /***************************************************************/ 00134000
*     /*                                                             */ 00135000
*     /* WTO TYPE MESSAGE PROCESSING                                 */ 00136000
*     /*                                                             */ 00137000
*     /***************************************************************/ 00138000
*                                                                  0179 00139000
*     DO;                                                          0179 00140000
*                                                                  0179 00141000
         B     @RC00169                                            0179 00142000
@RF00169 DS    0H                                                  0180 00143000
*       /*************************************************************/ 00144000
*       /*                                                           */ 00145000
*       /* USE NIP SERVICE TO WTO MESSAGE                            */ 00146000
*       /*                                                           */ 00147000
*       /*************************************************************/ 00148000
*                                                                  0180 00149000
*       GEN(IEAPMNIP TYPE=WTO,PARAM=NWTOMHDR)                      0180 00150000
*       REFS(NVTWTO,NWTOMHDR);                                     0180 00151000
*                                                                  0180 00152000
         IEAPMNIP TYPE=WTO,PARAM=NWTOMHDR                               00153000
*       /*************************************************************/ 00154000
*       /*                                                           */ 00155000
*       /* PUT SYSTEM INTO WAIT STATE IF MESSAGE ID SO INDICATES     */ 00156000
*       /*                                                           */ 00157000
*       /*************************************************************/ 00158000
*                                                                  0181 00159000
*       IF MSGIDTYP=SYSWAIT|MSGID=NORSTID THEN                     0181 00160000
         CLI   MSGIDTYP(MSGPLPTR),C'W'                             0181 00161000
         BE    @RT00181                                            0181 00162000
         CLC   MSGID(4,MSGPLPTR),NORSTID                           0181 00163000
         BNE   @RF00181                                            0181 00164000
@RT00181 DS    0H                                                  0182 00165000
*         GEN(IEAPMNIP TYPE=SWAIT)                                 0182 00166000
*         REFS(NVTSWAIT);                                          0182 00167000
         IEAPMNIP TYPE=SWAIT                                            00168000
*       ELSE                                                       0183 00169000
*         ;                                                        0183 00170000
@RF00181 DS    0H                                                  0184 00171000
*     END;                                                         0184 00172000
*RIMMEXIT:                                                         0185 00173000
*                                                                  0185 00174000
*   /*****************************************************************/ 00175000
*   /*                                                               */ 00176000
*   /* EXIT -- RETURN TO CALLER                                      */ 00177000
*   /*                                                               */ 00178000
*   /*****************************************************************/ 00179000
*                                                                  0185 00180000
*   RETURN;                                                        0185 00181000
@EL00001 L     @13,4(,@13)                                         0185 00182000
@EF00001 DS    0H                                                  0185 00183000
@ER00001 LM    @14,@12,12(@13)                                     0185 00184000
         BR    @14                                                 0185 00185000
*                                                                  0186 00186000
*/********************************************************************/ 00187000
*/*                                                                  */ 00188000
*/* SYNCHK:    ROUTINE OF ILRIMMSG TO VALIDATE OPERATOR REPLY TO MSG */ 00189000
*/*                                                                  */ 00190000
*/*                                                                  */ 00191000
*/* INPUT:     ARRAY OF VALID OPERATOR REPLIES                       */ 00192000
*/*            SET OF FLAGS CORRESPONDING TO REPLY ARRAY ENTRIES     */ 00193000
*/*                                                                  */ 00194000
*/* PROCESS:   LOOP COMPARES OPERATOR REPLY WITH EACH ENTRY IN THE   */ 00195000
*/*            REPLY ARRAY FOR WHICH THE CORRESPONDING FLAG IS SET   */ 00196000
*/*            ON (SET BY MSGNNND SUBROUTINE).                       */ 00197000
*/*            IF A VALID MATCH IS FOUND, THE WTOR MESSAGE CONDITION */ 00198000
*/*            IS SET OFF.  OTHERWISE, A MESSAGE OF 'INVALID REPLY'  */ 00199000
*/*            IS MOVED TO THE MESSAGE BUFFER WHICH THE WTOR LOOP    */ 00200000
*/*            IN THE ILRIMMSG SEGMENT WILL CAUSE TO HAVE WRITTEN.   */ 00201000
*/*                                                                  */ 00202000
*/* OUTPUT:    RESET WTOR MESSAGE CONDITION, IF VALID REPLY OBTAINED */ 00203000
*/*            ALTERED MESSAGE BUFFER, IF VALID REPLY NOT OBTAINED   */ 00204000
*/*                                                                  */ 00205000
*/********************************************************************/ 00206000
*/*SYNCHK:   CHART                                                   */ 00207000
*/* FOOTING                                                        0186 00208000
*/*                         ILRIMMSG           OS/VS 2 RELEASE 2:  0186 00209000
*/*                                                JANUARY 28, 1974  */ 00210000
*/*SYNCHK:   E    ENTRY TO VALIDATE OPERATOR REPLY                   */ 00211000
*/*          N    LOOP THRU VALID REPLY ARRAY                        */ 00212000
*/*%SCK10:   D (YES,,NO,%SCK50) CONTINUE LOOPING ?                   */ 00213000
*/*          N    LOOP CONTINUES UNTIL 1) END OF ARRAY IS REACHED OR */ 00214000
*/*          N    2) UNTIL A VALID MATCH IS FOUND (ARRAY AND       0186 00215000
*/*               OPER REPLY                                         */ 00216000
*/*          D (YES,,NO,%SCK30) TRY TO MATCH THIS ENTRY ?            */ 00217000
*/*          D (YES,,NO,%SCK30) OPER REPLY MATCH ENTRY ?             */ 00218000
*/*          P    INDICATE VALID REPLY RECEIVED                      */ 00219000
*/*%SCK30:   N (,%SCK10) END OF LOOP THRU VALID REPLY ARRAY          */ 00220000
*/*%SCK50:   D (YES,,NO,%SCK90) WTOR MSG COND STILL ON ?             */ 00221000
*/*          P    MOVE 'INVALID REPLY' TO MESSAGE BUFFER             */ 00222000
*/*%SCK90:   R    RETURN TO ILRIMMSG                                 */ 00223000
*/*SYNCHK:   END                                                     */ 00224000
*                                                                  0186 00225000
*SYNCHK:                            /* VALIDATE OPERATOR REPLY       */ 00226000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0186 00227000
         B     @EL00001                                            0186 00228000
SYNCHK   DS    0H                                                  0187 00229000
*   RFY                                                            0187 00230000
*    (R2,                                                          0187 00231000
*     R3,                                                          0187 00232000
*     R14) RSTD;                    /* RESTRICT NVT,CVT AND RETURN 0187 00233000
*                                      ADDRESS REGISTERS             */ 00234000
*                                                                  0187 00235000
*   /*****************************************************************/ 00236000
*   /*                                                               */ 00237000
*   /* LOOP THROUGH VALID REPLY ARRAY, TEST REPLY IF CORRESPONDING   */ 00238000
*   /* REPLY FLAG IS SET.                                            */ 00239000
*   /*                                                               */ 00240000
*   /*****************************************************************/ 00241000
*                                                                  0188 00242000
*   VALMASK=WORDBIT1;               /* INITLZ VAL REPLY FLAG MASK    */ 00243000
         L     VALMASK,WORDBIT1                                    0188 00244000
*   DO WRKCTR=ONE TO DIM(VALRTENT) WHILE(VALREPLY=NO);             0189 00245000
         LA    WRKCTR,1                                            0189 00246000
@DL00189 TM    VALREPLY,B'01000000'                                0189 00247000
         BNZ   @DC00189                                            0189 00248000
*     IF VALFLGS=(VALFLGS|VALMASK) THEN/* TEST THIS VALID REPLY ?    */ 00249000
*                                                                  0190 00250000
         L     @15,VALFLGS                                         0190 00251000
         LR    @04,@15                                             0190 00252000
         OR    @04,VALMASK                                         0190 00253000
         CLR   @15,@04                                             0190 00254000
         BNE   @RF00190                                            0190 00255000
*       /*************************************************************/ 00256000
*       /*                                                           */ 00257000
*       /* TEST FOR VALID REPLY                                      */ 00258000
*       /*                                                           */ 00259000
*       /*************************************************************/ 00260000
*                                                                  0191 00261000
*       IF MSGREPLY(ONE:VALREPLS(WRKCTR))=VALREPS(WRKCTR) THEN     0191 00262000
         LR    @15,WRKCTR                                          0191 00263000
         SLA   @15,3                                               0191 00264000
         SLR   @04,@04                                             0191 00265000
         IC    @04,VALREPLS-8(@15)                                 0191 00266000
         BCTR  @04,0                                               0191 00267000
         LA    @01,VALREPS-8(@15)                                  0191 00268000
         EX    @04,@SC00854                                        0191 00269000
         BNE   @RF00191                                            0191 00270000
*         VALREPLY=YES;             /* INDICATE MATCH FOUND          */ 00271000
         OI    VALREPLY,B'01000000'                                0192 00272000
*       ELSE                                                       0193 00273000
*         ;                                                        0193 00274000
@RF00191 DS    0H                                                  0194 00275000
*     ELSE                                                         0194 00276000
*       ;                                                          0194 00277000
@RF00190 DS    0H                                                  0195 00278000
*     VALMASK=VALMASK/SHIFTR1;      /* SHIFT MASK TO TEST NEXT FLG   */ 00279000
         SRL   VALMASK,1                                           0195 00280000
*   END;                                                           0196 00281000
*                                                                  0196 00282000
         AL    WRKCTR,@CF00055                                     0196 00283000
         C     WRKCTR,@CF00067                                     0196 00284000
         BNH   @DL00189                                            0196 00285000
@DC00189 DS    0H                                                  0197 00286000
*   /*****************************************************************/ 00287000
*   /*                                                               */ 00288000
*   /* IF INVALID REPLY, MOVE MESSAGE TO OUTPUT BUFFER               */ 00289000
*   /*                                                               */ 00290000
*   /*****************************************************************/ 00291000
*                                                                  0197 00292000
*   IF VALREPLY=NO THEN             /* NO VALID REPLY FOUND ?        */ 00293000
         TM    VALREPLY,B'01000000'                                0197 00294000
         BNZ   @RF00197                                            0197 00295000
*     MSGOBUF=BADREPLY;             /* MOVE 'INVALID REPLY' TO OUTPUT   00296000
*                                      BUFFER                        */ 00297000
         MVI   MSGOBUF+33,C' '                                     0198 00298000
         MVC   MSGOBUF+34(38),MSGOBUF+33                           0198 00299000
         MVC   MSGOBUF(33),BADREPLY                                0198 00300000
*   ELSE                                                           0199 00301000
*     ;                                                            0199 00302000
@RF00197 DS    0H                                                  0200 00303000
*   END SYNCHK;                     /* END OF SUBROUTINE SYNCHK      */ 00304000
@EL00002 DS    0H                                                  0200 00305000
@EF00002 DS    0H                                                  0200 00306000
@ER00002 BR    @14                                                 0200 00307000
*                                                                  0201 00308000
*/********************************************************************/ 00309000
*/*                                                                  */ 00310000
*/* MSGNNNT: GENERIC FORMAT OF MESSAGE BUILDING SUBROUTINE           */ 00311000
*/*                                                                  */ 00312000
*/*                                                                  */ 00313000
*/* INPUT:   PARAMETER LIST INPUT TO ILRIMMSG MAIN SEGMENT           */ 00314000
*/*                                                                  */ 00315000
*/* PROCESS: SINCE THERE IS A UNIQUE SUBROUTINE FOR ALL MESSAGES,    */ 00316000
*/*          NO TESTS ARE NECESSARY, ONLY A SERIES OF                */ 00317000
*/*          MOVES OF MSG DATA TO THE MESSAGE BUFFER.                */ 00318000
*/*                                                                  */ 00319000
*/*          MOST OF ANY MESSAGE IS FIXED.  HOWEVER, IN SOME CASES   */ 00320000
*/*          A DATA SET NAME OR OTHER VARIABLE MAY BE REQUIRED. FOR  */ 00321000
*/*          THIS REASON, THE POINTERS CONTAINED IN THE PARAMETER    */ 00322000
*/*          LIST FROM THE CALLER WILL POINT TO VARYING DATA         */ 00323000
*/*          DEPENDING ON THE SPECIFIC MESSAGE.  THESE POINTERS      */ 00324000
*/*          ADDRESS THE ADDITIONAL INFORMATION, LOCATED IN THE      */ 00325000
*/*          CALLING MODULE, REQUIRED TO COMPLETE BUILDING THE       */ 00326000
*/*          MESSAGE.                                                */ 00327000
*/*                                                                  */ 00328000
*/*          IF THE MESSAGE IS MULTIPLE LINES, THE FIRST LINE(S)     */ 00329000
*/*          IS (ARE) WRITTEN VIA NIPWTO (SEE SUBROUTINE WTOMSGC).   */ 00330000
*/*          THE FINAL LINE OF THE MESSAGE IS SET UP TO BE WRITTEN   */ 00331000
*/*          FROM THE MAIN ROUTINE (ILRIMMSG).                       */ 00332000
*/*                                                                  */ 00333000
*/*          IF THE MESSAGE THE SUBROUTINE BUILDS IS A TYPE 'D'      */ 00334000
*/*          THE SUBROUTINE ALSO SETS A SERIES OF FLAGS              */ 00335000
*/*          CORRESPONDING TO ENTRIES IN A 'VALID REPLY' ARRAY.      */ 00336000
*/*          THIS IS DONE SO THAT THE SYNCHK SUBROUTINE WILL KNOW    */ 00337000
*/*          WHICH VALID REPLIES ARE DESIRED FROM THE OPERATOR.      */ 00338000
*/*                                                                  */ 00339000
*/*          IF THE MESSAGE THE SUBROUTINE BUILDS IS A TYPE 'W'      */ 00340000
*/*          MESSAGE, THE WAIT STATE CODE CORRESPONDING TO THE       */ 00341000
*/*          ERROR CONDITION IS PLACED INTO THE NVT PRIOR TO         */ 00342000
*/*          RETURNING TO ILRIMMSG.                                  */ 00343000
*/*                                                                  */ 00344000
*/* OUTPUT:  NONE SPECIFICALLY TO ILRIMMSG, MESSAGE BUFFER NOW       */ 00345000
*/*          CONTAINS MESSAGE.                                       */ 00346000
*/*                                                                  */ 00347000
*/********************************************************************/ 00348000
*/*MSGNNNT:  CHART                                                   */ 00349000
*/* HEADER                                                         0201 00350000
*/*                        MSGNNNT: GENERIC FORMAT OF AN ILRIMMSG ME    00351000
*/*SSAGE BUILDING SUBROUTINE                                PAGE #1  */ 00352000
*/* FOOTING                                                        0201 00353000
*/*                         ILRIMMSG           OS/VS 2 RELEASE 2:  0201 00354000
*/*                                                JANUARY 28, 1974  */ 00355000
*/*MSGNNNT:  E    ENTRY TO BUILD MESSAGE                             */ 00356000
*/*          N    NEXT 2 STEPS ARE REPEATED FOR 1ST LINE(S) OF     0201 00357000
*/*               MULTIPLE LINE MSGS                                 */ 00358000
*/*          P    MOVE REQUIRED MSG TEXT FROM DATA AREA TO MSG     0201 00359000
*/*               BUFFER                                             */ 00360000
*/*          S WTOMSGC: WTO LINE, BLANK MSG AREA                     */ 00361000
*/*          P    MOVE MSG (OR FINAL LINE, IF MULTIPLE) TO MSGAREA   */ 00362000
*/*          N    INCLUDE ANY DATA ADDRESSED IN PARAMETER LIST       */ 00363000
*/*          N    ALSO SET VALID REPLY FLAGS FOR SYNCHK IF 'D'     0201 00364000
*/*               TYPE MESSAGE                                       */ 00365000
*/*          N    ALSO MOVE ASSOCIATED WAIT STATE CODE TO NVT IF 'W'    00366000
*/*               TYPE MSG                                           */ 00367000
*/*          R    RETURN TO ILRIMMSG TO WTO(R) MESSAGE               */ 00368000
*/*MSGNNNT:  END                                                     */ 00369000
*                                                                  0201 00370000
*                                                                  0201 00371000
*/********************************************************************/ 00372000
*/*                                                                  */ 00373000
*/* MSG340I: SUBROUTINE TO BUILD FOLLOWING MESSAGE:                  */ 00374000
*/*   IEA340I REAL OR VIRTUAL STORAGE NOT AVAILABLE FOR ASM          */ 00375000
*/*                                                                  */ 00376000
*/* INPUT:    NVT                                                    */ 00377000
*/*                                                                  */ 00378000
*/* PROCESS:  MOVE MESSAGE TO OUTPUT AREA.                           */ 00379000
*/*           MOVE WAIT STATE CODE INTO NVT                          */ 00380000
*/*                                                                  */ 00381000
*/* OUTPUT:   MESSAGE IN OUTPUT AREA                                 */ 00382000
*/*           WAIT STATE CODE IN NVT                                 */ 00383000
*/*                                                                  */ 00384000
*/********************************************************************/ 00385000
*                                                                  0201 00386000
*MSG340I:                           /* BUILD MSG IEA340I             */ 00387000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0201 00388000
*                                                                  0201 00389000
MSG340I  DS    0H                                                  0202 00390000
*   /*****************************************************************/ 00391000
*   /*                                                               */ 00392000
*   /* BUILD MESSAGE IN OUTPUT AREA                                  */ 00393000
*   /*                                                               */ 00394000
*   /*****************************************************************/ 00395000
*                                                                  0202 00396000
*   MSGOTEXT(1:41)=BDY340I;                                        0202 00397000
         MVC   MSGOTEXT(41),BDY340I                                0202 00398000
*   MSGOTEXT(42:45)=FUNC340I;                                      0203 00399000
*                                                                  0203 00400000
         MVC   MSGOTEXT+41(4),FUNC340I                             0203 00401000
*   /*****************************************************************/ 00402000
*   /*                                                               */ 00403000
*   /* MOVE WAIT STATE CODE TO NVT                                   */ 00404000
*   /*                                                               */ 00405000
*   /*****************************************************************/ 00406000
*                                                                  0204 00407000
*   NVTFLWSC=NVTFLWRS;                                             0204 00408000
         MVI   NVTFLWSC(RNVT),X'38'                                0204 00409000
*   END MSG340I;                    /* END OF MSG340I                */ 00410000
@EL00003 DS    0H                                                  0205 00411000
@EF00003 DS    0H                                                  0205 00412000
@ER00003 BR    @14                                                 0205 00413000
*                                                                  0206 00414000
*/********************************************************************/ 00415000
*/*                                                                  */ 00416000
*/* MSG915E: SUBROUTINE TO BUILD FOLLOWING MESSAGE:                  */ 00417000
*/*   IEA915E SYNTAX ERROR IN IEASYSNN CONTENTS, PROMPT FOLLOWS      */ 00418000
*/*                                                                  */ 00419000
*/* INPUT:    NONE                                                   */ 00420000
*/*                                                                  */ 00421000
*/* PROCESS:  MOVE MESSAGE TO OUTPUT AREA.                           */ 00422000
*/*                                                                  */ 00423000
*/* OUTPUT:   MESSAGE IN OUTPUT AREA                                 */ 00424000
*/*                                                                  */ 00425000
*/********************************************************************/ 00426000
*                                                                  0206 00427000
*MSG915E:                           /* BUILD MESSAGE IEA915E         */ 00428000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0206 00429000
*                                                                  0206 00430000
MSG915E  DS    0H                                                  0207 00431000
*   /*****************************************************************/ 00432000
*   /*                                                               */ 00433000
*   /* BUILD MESSAGE IN OUTPUT AREA                                  */ 00434000
*   /*                                                               */ 00435000
*   /*****************************************************************/ 00436000
*                                                                  0207 00437000
*   MSGOTEXT(1:LENGTH(TXT915E))=TXT915E;                           0207 00438000
         MVC   MSGOTEXT(49),TXT915E                                0207 00439000
*   END MSG915E;                    /* END OF SUBROUTINE MSG915E     */ 00440000
@EL00004 DS    0H                                                  0208 00441000
@EF00004 DS    0H                                                  0208 00442000
