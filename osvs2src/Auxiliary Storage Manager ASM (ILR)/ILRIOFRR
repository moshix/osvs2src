         TITLE 'ILRIOFRR, I/O CONTROL RECOVERY                         *00001000
                        '                                               00002000
ILRIOFRR CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ILRIOFRR  76.091'                                 0001 00009000
ILRCQIOE DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00572-ILRCQIOE)                                   00013000
         ENTRY ILRCQIOE                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  BALR  @10,0                                               0001 00016000
@PSTART  DS    0H                                                  0001 00017000
         USING @PSTART,@10                                         0001 00018000
         AH    @15,4(,@15)                                         0001 00019000
         BR    @15                                                 0001 00020000
@EP00001 DS    0H                                                  0002 00021000
*                                      AREA ADDR AND RETURN ADDRESS  */ 00022000
*   WORKADDR=R0;                    /* SAVE WORKAREA ADDR            */ 00023000
         LR    WORKADDR,R0                                         0071 00024000
*   RFY                                                            0072 00025000
*     R0 UNRSTD;                    /* RELEASE REG                   */ 00026000
*   WORKADDR->FRRWORKA=''B;         /* CLEAR WORKAREA                */ 00027000
         XC    FRRWORKA(200,WORKADDR),FRRWORKA(WORKADDR)           0073 00028000
*   WAREG=WORKADDR+LENGTH(WORKAREA);/* GET ADDR OF PORTION OF WORK 0074 00029000
*                                      AREA USED BY RECOVERY SUBRTNS */ 00030000
         LA    WAREG,100                                           0074 00031000
         ALR   WAREG,WORKADDR                                      0074 00032000
*   GEN REFS(WAREG)(USING @DATD,WAREG);/* GET ADDRESSABILITY TO THIS    00033000
*                                      PORTION OF WORKAREA           */ 00034000
         USING @DATD,WAREG                                              00035000
*   RTMRTRN=R14;                    /* SAVE RETURN ADDR              */ 00036000
         ST    R14,RTMRTRN(,WORKADDR)                              0076 00037000
*   RFY                                                            0077 00038000
*     R14 UNRSTD;                   /* RELEASE REG                   */ 00039000
*   BASEADDR=BASEREG;               /* SAVE VALUE OF BASE REG        */ 00040000
         ST    BASEREG,BASEADDR(,WORKADDR)                         0078 00041000
*   ASMVTPTR=CVTASMVT;              /* INIT PTR TO ASMVT             */ 00042000
         L     @12,CVTPTR                                          0079 00043000
         L     ASMVTPTR,CVTASMVT(,@12)                             0079 00044000
*   ATAPTR=SDWAPARM;                /* INIT PTR TO ATA               */ 00045000
*                                                                  0080 00046000
         L     ATAPTR,SDWAPARM(,SDWAPTR)                           0080 00047000
*   /*****************************************************************/ 00048000
*   /*                                                               */ 00049000
*   /* INITIALIZE PARAMETERS FOR RECORDING.                          */ 00050000
*   /*                                                               */ 00051000
*   /*****************************************************************/ 00052000
*                                                                  0081 00053000
*   SDWAMODN=MODIDS(ATAMODID);      /* PLACE MODULE NAME IN SDWA FOR    00054000
*                                      RECORDING                     */ 00055000
