         BNZ   @RF00089                                            0089 00096000
*     DO;                           /* DO COMMON VERIFICATION        */ 00097000
*       RFY                                                        0091 00098000
*        (R5,                                                      0091 00099000
*         R6,                                                      0091 00100000
*         R7) RSTD;                 /* RESTRICT REGS DESTROYED BY  0091 00101000
*                                      CALL TO Q VERIFICATION RTN    */ 00102000
*       CALL ILRVASGQ;              /* VERIFY ASMSTAGQ - SALLOC LOCK    00103000
*                                      MUST BE HELD                  */ 00104000
         L     @15,@CV02316                                        0092 00105000
         BALR  @14,@15                                             0092 00106000
*       RFY                                                        0093 00107000
*        (R5,                                                      0093 00108000
*         R6,                                                      0093 00109000
*         R7) UNRSTD;               /* RELEASE REGS                  */ 00110000
*       IF ATAMODID=ATAMPGCM THEN   /* CONTINUE VERIFICATIONS IF   0094 00111000
*                                      ERROR OCCURRED DURING PAGE  0094 00112000
*                                      COMPLETION PROCESSING         */ 00113000
         CLI   ATAMODID(ATAPTR),2                                  0094 00114000
         BNE   @RF00094                                            0094 00115000
*         DO;                                                      0095 00116000
*           RFY                                                    0096 00117000
*             PARMPTR RSTD;         /* RESTRICT REG USED AS PARM TO     00118000
*                                      VERIFICATIONS RTNS            */ 00119000
*           PARMPTR=ADDR(ASM3PIOP); /* LOCATE QUEUE OF AIA'S TO BE 0097 00120000
*                                      RETURNED TO PIOP              */ 00121000
