         ST    @12,ATAAIA(,ATAPTR)                                 0275 00760000
*                                                                  0276 00761000
*           /*********************************************************/ 00762000
*           /*                                                       */ 00763000
*           /* SCHEDULE THE SRB FOR SRB CONTROLLER, IF IT IS NOT     */ 00764000
*           /* ALREADY SCHEDULED.                                    */ 00765000
*           /*                                                       */ 00766000
*           /*********************************************************/ 00767000
*                                                                  0276 00768000
*           IF ASHSCHED=OFF THEN    /* TEST IF SRB IS SCHEDULED      */ 00769000
@RF00274 TM    ASHSCHED(RSMHDPTR),B'10000000'                      0276 00770000
         BNZ   @RF00276                                            0276 00771000
*             DO;                   /* IF NOT, SCHEDULE THE SRB      */ 00772000
*               ASHSCHED=ON;        /* INDICATE THAT SRB IS SCHEDULED*/ 00773000
         OI    ASHSCHED(RSMHDPTR),B'10000000'                      0278 00774000
*               RFY                                                0279 00775000
*                 R1 RSTD;          /* RESTRICT REG TO POINT TO SRB  */ 00776000
*               R1=ASHVSRBP;        /* GET ADDR OF SRB CONTROLLER SRB*/ 00777000
         L     R1,ASHVSRBP(,RSMHDPTR)                              0280 00778000
*               RFY                                                0281 00779000
*                 RSMHDPTR UNRSTD;  /* RELEASE REG                   */ 00780000
*               GEN(SCHEDULE SRB=(1),SCOPE=GLOBAL)                 0282 00781000
*               REFS(CVTGSMQ,SRBFLNK,SRBSECT);/* SCHEDULE SRB FOR SRB   00782000
*                                      CONTROLLER                    */ 00783000
