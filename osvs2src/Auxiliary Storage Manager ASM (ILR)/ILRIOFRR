*       RFY                                                        0219 00563000
*        (R5,                                                      0219 00564000
*         R6,                                                      0219 00565000
*         R7) UNRSTD;               /* RELEASE REGS                  */ 00566000
*       IF ATAAIA^=ZERO THEN        /* TEST IF THERE IS A VALID AIA     00567000
*                                      CHECKPOINTED IN THE ATA       */ 00568000
         L     @12,ATAAIA(,ATAPTR)                                 0220 00569000
         LTR   @12,@12                                             0220 00570000
         BZ    @RF00220                                            0220 00571000
*         DO;                       /* PROCESS THIS AIA              */ 00572000
*           PCBPTR=ATAAIA-(LENGTH(PCB)-LENGTH(AIA));/* CALCULATE THE    00573000
*                                      ADDR OF THE RELATED PCB       */ 00574000
         L     @11,ATAAIA(,ATAPTR)                                 0222 00575000
         SH    @11,@CH02178                                        0222 00576000
         LR    PCBPTR,@11                                          0222 00577000
*           ASCBPTR=PCBASCB;        /* GET THE ADDR OF THE ASCB      */ 00578000
         L     ASCBPTR,PCBASCB(,PCBPTR)                            0223 00579000
*           RFY                                                    0224 00580000
*             RSMHDPTR RSTD;        /* RESTRICT REG TO POINT TO RSMHD*/ 00581000
*           RSMHDPTR=ASCBRSM;       /* GET ADDR OF RSMHD             */ 00582000
*                                                                  0225 00583000
         L     RSMHDPTR,ASCBRSM(,ASCBPTR)                          0225 00584000
*           /*********************************************************/ 00585000
*           /*                                                       */ 00586000
*           /* UNCONDITIONALLY REQUEST THE ASM LOCK FOR THIS ADDRESS */ 00587000
*           /* SPACE. EITHER THIS LOCK WAS ALREADY HELD BY ILRVIOCM, */ 00588000
*           /* OR IT WILL BE OBTAINED NOW. IN EITHER CASE, THE LOCK  */ 00589000
*           /* WILL BE FREED BY THIS ROUTINE, SO THERE IS NO TEST TO */ 00590000
*           /* SEE WHETHER THE LOCK WAS JUST OBTAINED.               */ 00591000
*           /*                                                       */ 00592000
*           /*********************************************************/ 00593000
*                                                                  0226 00594000
*           RFY                                                    0226 00595000
*            (R11,                                                 0226 00596000
*             R12,                                                 0226 00597000
*             R14) RSTD;            /* RESTRICT REGS USED BY SETLOCK */ 00598000
*           R11=ADDR(ASHLOCK);      /* POINT TO THE LOCKWORD FOR THE    00599000
*                                      ASM LOCK OF THIS ADDR SPACE   */ 00600000
         LA    R11,ASHLOCK(,RSMHDPTR)                              0227 00601000
*           GEN REFS(PSALITA);                                     0228 00602000
*                                  /* OBTAIN ASM LOCK FOR ADDR SPACE*/  00603000
GTASM1   SETLOCK OBTAIN,TYPE=ASM,ADDR=(11),MODE=UNCOND,DISABLED,      **00604000
               RELATED=(VIO,ILRIOFRR(FRASM1))                           00605000
*           RFY                                                    0229 00606000
*            (R11,                                                 0229 00607000
*             R12,                                                 0229 00608000
*             R14) UNRSTD;          /* RELEASE REGS                  */ 00609000
*           SAVEPTR=ADDR(ASMSAVE);  /* RESTORE ADDR OF THE REG SAVE     00610000
*                                      AREA, DESTROYED BY SETLOCK    */ 00611000
         LA    SAVEPTR,ASMSAVE(,ASMVTPTR)                          0230 00612000
*           RFY                                                    0231 00613000
*             RETCODE RSTD;         /* RESTRICT REG USED FOR RETURN     00614000
