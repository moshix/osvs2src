*           IF RETCODE=ZERO THEN    /* TEST FOR A RETURN CODE OF 0 -    00409000
*                                      INDICATING A VALID ASCB       */ 00410000
         LTR   RETCODE,RETCODE                                     0171 00411000
         BNZ   @RF00171                                            0171 00412000
*             DO;                   /* SINCE THE ERROR OCURRED WHILE    00413000
*                                      EITHER A SWAP OUT OR A SWAP IN   00414000
*                                      WAS IN PROCESS, SET UP TO   0172 00415000
*                                      TERMINATE THE MEMORY  @ZM43070*/ 00416000
*               RFY                                                0173 00417000
*                 RETCODE UNRSTD;   /* RELEASE REG                   */ 00418000
*               ASCBPTR=PARMPTR;    /* POINT TO ASCB TO BE TERMINATED*/ 00419000
         LR    ASCBPTR,PARMPTR                                     0174 00420000
*               RFY                                                0175 00421000
*                 RSMHDPTR RSTD;    /* RESTRICT REG TO POINT TO RSMHD*/ 00422000
*               RSMHDPTR=ASCBRSM;   /* GET ADDR OF RSMHD             */ 00423000
         L     RSMHDPTR,ASCBRSM(,ASCBPTR)                          0176 00424000
*               IF SWAPIN=ON THEN   /* IF ERROR OCCURRED DURING A  0177 00425000
*                                      SWAP IN REQUEST       @ZM43070*/ 00426000
         TM    SWAPIN,B'00100000'                                  0177 00427000
         BNO   @RF00177                                            0177 00428000
*                 RSMFAIL=ON;       /* TURN ON RSMFAIL TO INDICATE 0178 00429000
*                                      LSQA IS NOT AVAILABLE @ZM43070*/ 00430000
         OI    RSMFAIL(RSMHDPTR),B'00000010'                       0178 00431000
*               ELSE                /* IF ERROR OCCURRED DURING A  0179 00432000
*                                      SWAP OUT REQUEST      @ZM43070*/ 00433000
*                 DO;               /* PERFORM FURTHER RECOVERY FOR A   00434000
*                                      SWAP OUT REQUEST      @ZM43070*/ 00435000
         B     @RC00177                                            0179 00436000
@RF00177 DS    0H                                                  0180 00437000
*                   PARMPTR=ADDR(ASHCAPQ);/* POINT TO THE CAPTURE Q     00438000
*                                      OF THE ADDRESS SPACE          */ 00439000
         LA    PARMPTR,ASHCAPQ(,RSMHDPTR)                          0180 00440000
*                   RFY                                            0181 00441000
*                    (R5,                                          0181 00442000
*                     R6,                                          0181 00443000
*                     R7) RSTD;     /* RESTRICT REGS DESTROYED BY  0181 00444000
*                                      CALL TO Q VERIFICATION RTN    */ 00445000
*                   CALL ILRVSPAQ;  /* VERIFY THIS QUEUE             */ 00446000
         L     @15,@CV02318                                        0182 00447000
         BALR  @14,@15                                             0182 00448000
*                   RFY                                            0183 00449000
*                    (R5,                                          0183 00450000
*                     R6,                                          0183 00451000
*                     R7) UNRSTD;   /* RELEASE REGS                  */ 00452000
*                   RFY                                            0184 00453000
*                     PARMPTR UNRSTD;/* RELEASE REG                  */ 00454000
*                   IF ATAAIA^=ZERO THEN/* TEST IF THERE IS A VALID     00455000
*                                      AIA CHECKPOINTED IN THE ATA   */ 00456000
         L     @11,ATAAIA(,ATAPTR)                                 0185 00457000
         LTR   @11,@11                                             0185 00458000
         BZ    @RF00185                                            0185 00459000
*                     DO;           /* PROCESS THIS AIA              */ 00460000
*                       ATAAIA->AIAERROR=ON;/* TURN ON THE         0187 00461000
*                                      INDETERMINATE ERROR FLAG IN 0187 00462000
*                                      THIS AIA                      */ 00463000
         OI    AIAERROR(@11),B'00000010'                           0187 00464000
*                       ATAAIA->AIANXAIA=ASHCAPQ;/* CHAIN THE CAPTURE   00465000
*                                      Q TO THIS AIA                 */ 00466000
         MVC   AIANXAIA(4,@11),ASHCAPQ(RSMHDPTR)                   0188 00467000
*                       ASHCAPQ=ATAAIA;/* PLACE THIS AIA AT THE HEAD    00468000
*                                      OF THE CAPTURE Q              */ 00469000
         ST    @11,ASHCAPQ(,RSMHDPTR)                              0189 00470000
*                     END;          /* END OF PROCESSING ATAAIA      */ 00471000
*                   ASHPERME=ON;    /* TURN ON THE PERMANENT ERROR 0191 00472000
*                                      FLAG FOR THIS ADDRESS SPACE   */ 00473000
@RF00185 OI    ASHPERME(RSMHDPTR),B'00100000'                      0191 00474000
*                 END;              /* END OF FURTHER RECOVERY FOR A    00475000
*                                      SWAP OUT REQUEST      @ZM43070*/ 00476000
*               RFY                                                0193 00477000
*                 RSMHDPTR UNRSTD;  /* RELEASE REG                   */ 00478000
@RC00177 DS    0H                                                  0194 00479000
*               RFY                                                0194 00480000
*                 WORK1 RSTD;       /* RESTRICT REG USED FOR CALLRTM */ 00481000
*               WORK1=ASCBASID;     /* GET THE ASID OF THE ADDR    0195 00482000
*                                      SPACE, NEEDED FOR THE CALLRTM */ 00483000
         SLR   WORK1,WORK1                                         0195 00484000
         ICM   WORK1,3,ASCBASID(ASCBPTR)                           0195 00485000
*               GEN REFS(CVTBTERM);                                0196 00486000
*                                  /* TERMINATE THE ADDRESS SPACE   */  00487000
         CALLRTM TYPE=MEMTERM,COMPCOD=X'028',ASID=(WORK1)               00488000
*               RFY                                                0197 00489000
*                 WORK1 UNRSTD;     /* RELEASE REG                   */ 00490000
*               SDWAPTR=SAVESDWA;   /* RESTORE ADDR OF SDWA          */ 00491000
         L     SDWAPTR,SAVESDWA                                    0198 00492000
*             END;                  /* END OF SECTION TO TERMINATE 0199 00493000
*                                      THE ADDRESS SPACE             */ 00494000
*         END;                      /* END OF SECTION TO PROCESS A 0200 00495000
*                                      SWAP OUT REQUEST, AND TO    0200 00496000
*                                      TERMINATE THE MEMORY FOR A  0200 00497000
*                                      SWAP IN REQUEST       @ZM43070*/ 00498000
@RF00171 DS    0H                                                  0201 00499000
*       ATAASCB=ZERO;               /* ZERO ASCB ADDRESS IN THE ATA  */ 00500000
@RF00163 SLR   @12,@12                                             0201 00501000
         ST    @12,ATAASCB(,ATAPTR)                                0201 00502000
*       ATAAIA=ASM3TMPA;            /* UPDATE THE AIA PTR IN THE ATA    00503000
*                                      TO THE FIRST UNPROCESSED AIA  */ 00504000
         MVC   ATAAIA(4,ATAPTR),ASM3TMPA(ASMVTPTR)                 0202 00505000
*       GOTO DONE;                  /* RESUME MAINLINE PROCESSING    */ 00506000
         B     DONE                                                0203 00507000
*     END;                          /* END OF SWAPCOMP RECOVERY      */ 00508000
*                                                                  0205 00509000
*   /*****************************************************************/ 00510000
*   /*                                                               */ 00511000
*   /* IF ILRVIOCM WAS IN CONTROL AT THE TIME OF ERROR, PERFORM      */ 00512000
*   /* ILRVIOCM RECOVERY.                                            */ 00513000
*   /*                                                               */ 00514000
*   /*****************************************************************/ 00515000
*                                                                  0205 00516000
*   IF ATAVIOCM=ON THEN                                            0205 00517000
@RF00123 TM    ATAVIOCM(ATAPTR),B'00010000'                        0205 00518000
         BNO   @RF00205                                            0205 00519000
*     DO;                           /* VIOCM RECOVERY                */ 00520000
*       SDWACSCT=VCMPNAME;          /* PLACE VIOCM NAME IN SDWA FOR     00521000
*                                      RECORDING                     */ 00522000
         MVC   SDWACSCT(8,SDWAPTR),VCMPNAME                        0207 00523000
*       DO;                         /* ILRTRAS BACK                  */ 00524000
*         RFY                                                      0209 00525000
*           R15 RSTD;                                              0209 00526000
*         R15=PSAAOLD;                                             0210 00527000
         L     R15,PSAAOLD                                         0210 00528000
*         GEN(LCTL  1,1,ASCBSTOR(15))                              0211 00529000
*         REFS(ASCBSTOR);                                          0211 00530000
         LCTL  1,1,ASCBSTOR(15)                                         00531000
*         PSALCCAV->LCCAASID=0;                                    0212 00532000
         L     @14,PSALCCAV                                        0212 00533000
         SLR   @12,@12                                             0212 00534000
         ST    @12,LCCAASID(,@14)                                  0212 00535000
*         RFY                                                      0213 00536000
*           R15 UNRSTD;                                            0213 00537000
*       END;                        /* ILRTRAS BACK TRAS BACK TO   0214 00538000
*                                      CURRENT ADDRESS SPACE IN CASE    00539000
*                                      ERROR OCCURRED WHILE TRAS'D,     00540000
*                                      IF NOT, OPERATION IS A NO-OP  */ 00541000
*                                                                  0215 00542000
*       /*************************************************************/ 00543000
*       /*                                                           */ 00544000
*       /* PROCESS THE AIA CHECKPOINTED IN THE ATA.                  */ 00545000
*       /*                                                           */ 00546000
*       /*************************************************************/ 00547000
*                                                                  0215 00548000
*       RFY                                                        0215 00549000
*         PARMPTR RSTD;             /* RESTRICT REG USED FOR PARM TO    00550000
