*           SAVEPTR=ADDR(ASMSAVE);  /* USE ASMSAVE AS REG SAVE AREA     01681000
*                                      SINCE SALLOC LOCK IS HELD     */ 01682000
         LA    SAVEPTR,ASMSAVE(,ASMVTPTR)                          0520 01683000
*           IF ATASLSQA=ON THEN     /* IF ERROR OCCURRED DURING SLSQA   01684000
*                                      PROCESSING                    */ 01685000
         TM    ATASLSQA(ATAPTR),B'01000000'                        0521 01686000
         BNO   @RF00521                                            0521 01687000
*             ATAAIA=ASM3TMPA;      /* RESTORE ATAAIA FROM WHERE IT     01688000
*                                      WAS SAVED BY PAGECOMP RTN OR     01689000
*                                      SWAPCOMP RTN                  */ 01690000
         MVC   ATAAIA(4,ATAPTR),ASM3TMPA(ASMVTPTR)                 0522 01691000
*           RFY                                                    0523 01692000
*             PARMPTR RSTD;         /* RESTRICT REG USED BY        0523 01693000
*                                      VERIFICATION RTN              */ 01694000
@RF00521 DS    0H                                                  0524 01695000
*           PARMPTR=ADDR(ATAAIA);   /* LOCATE QUEUE OF AIA'S IN ATA  */ 01696000
         LA    PARMPTR,ATAAIA(,ATAPTR)                             0524 01697000
*           RFY                                                    0525 01698000
*            (R5,                                                  0525 01699000
*             R6,                                                  0525 01700000
*             R7) RSTD;             /* RESTRICT REGS DESTROYED BY  0525 01701000
*                                      CALL TO Q VERIFICATION RTN    */ 01702000
*           CALL ILRVAIAQ;          /* VERIFY QUEUE OF AIA'S         */ 01703000
         L     @15,@CV02317                                        0526 01704000
         BALR  @14,@15                                             0526 01705000
*           RFY                                                    0527 01706000
*            (R5,                                                  0527 01707000
*             R6,                                                  0527 01708000
*             R7) UNRSTD;           /* RELEASE REGS                  */ 01709000
*           RFY                                                    0528 01710000
*             PARMPTR UNRSTD;       /* RELEASE REG                   */ 01711000
*                                                                  0529 01712000
*           /*********************************************************/ 01713000
*           /*                                                       */ 01714000
*           /* IF THERE ARE ANY UNPROCESSED AIA'S, QUEUE THEM TO THE */ 01715000
*           /* PART AIA ERROR QUEUE AND SCHEDULE THE SRB FOR PART    */ 01716000
*           /* MONITOR. THIS WILL CAUSE ANOTHER INVOCATION OF        */ 01717000
*           /* ILRPAGCM TO PROCESS THESE AIA'S.                      */ 01718000
*           /*                                                       */ 01719000
*           /*********************************************************/ 01720000
*                                                                  0529 01721000
*           IF ATAAIA^=ZERO|        /* IF THERE ARE AIA'S IN THE ATA */ 01722000
*               ASM3SWPQ^=ZERO|     /* OR THERE ARE AIA'S FOR THE  0529 01723000
*                                      SWAPCOMP RTN                  */ 01724000
*               ASM3GENQ^=ZERO THEN /* OR THERE ARE AIA'S FOR THE  0529 01725000
*                                      PAGECOMP RTN                  */ 01726000
         SLR   @12,@12                                             0529 01727000
         C     @12,ATAAIA(,ATAPTR)                                 0529 01728000
         BNE   @RT00529                                            0529 01729000
         C     @12,ASM3SWPQ(,ASMVTPTR)                             0529 01730000
         BNE   @RT00529                                            0529 01731000
         C     @12,ASM3GENQ(,ASMVTPTR)                             0529 01732000
         BE    @RF00529                                            0529 01733000
@RT00529 DS    0H                                                  0530 01734000
*             DO;                   /* QUEUE THESE UNPROCESSED AIA'S    01735000
*                                      FOR LATER PROCESSING          */ 01736000
*               RFY                                                0531 01737000
*                 AIAPTR RSTD;      /* RESTRICT REG USED TO CONTAIN     01738000
*                                      AIA PTR FOR SUBRTN            */ 01739000
*               AIAPTR=ATAAIA;      /* POINT TO AIA'S STILL IN ATA   */ 01740000
         L     AIAPTR,ATAAIA(,ATAPTR)                              0532 01741000
*               ATAAIA=ZERO;        /* ZERO AIA PTR IN ATA           */ 01742000
         SLR   @12,@12                                             0533 01743000
         ST    @12,ATAAIA(,ATAPTR)                                 0533 01744000
*               CALL QTOPARTQ;      /* QUEUE AIA'S TO PARTAIAE       */ 01745000
         BAL   @14,QTOPARTQ                                        0534 01746000
*               AIAPTR=ASM3SWPQ;    /* POINT TO AIA'S FOR SWAPCOMP 0535 01747000
*                                      SUBRTN                        */ 01748000
         L     AIAPTR,ASM3SWPQ(,ASMVTPTR)                          0535 01749000
*               ASM3SWPQ=ZERO;      /* ZERO PTR TO AIA'S FOR SWAPCOMP   01750000
*                                      SUBRTN - IN ASMVT WA          */ 01751000
         SLR   @12,@12                                             0536 01752000
         ST    @12,ASM3SWPQ(,ASMVTPTR)                             0536 01753000
*               CALL QTOPARTQ;      /* QUEUE AIA'S TO PARTAIAE       */ 01754000
         BAL   @14,QTOPARTQ                                        0537 01755000
*               AIAPTR=ASM3GENQ;    /* POINT TO AIA'S FOR PAGECOMP 0538 01756000
*                                      SUBRTN                        */ 01757000
         L     AIAPTR,ASM3GENQ(,ASMVTPTR)                          0538 01758000
*               ASM3GENQ=ZERO;      /* ZERO PTR TO AIA'S FOR PAGECOMP   01759000
*                                      SUBRTN - IN ASMVT WA          */ 01760000
         SLR   @12,@12                                             0539 01761000
         ST    @12,ASM3GENQ(,ASMVTPTR)                             0539 01762000
*               CALL QTOPARTQ;      /* QUEUE AIA'S TO PARTAIAE       */ 01763000
         BAL   @14,QTOPARTQ                                        0540 01764000
*               RFY                                                0541 01765000
*                 AIAPTR UNRSTD;    /* RELEASE REG                   */ 01766000
*               CALL PMSCHEDL;      /* SCHEDULE THE SRB FOR PART   0542 01767000
*                                      MONITOR, IF IT IS NOT ALREADY    01768000
*                                      SCHEDULED                     */ 01769000
         BAL   @14,PMSCHEDL                                        0542 01770000
*               SDWAPTR=SAVESDWA;   /* RESTORE ADDR OF SDWA,       0543 01771000
*                                      DESTROYED BY SCHEDULE IN    0543 01772000
*                                      PMSCHEDL                      */ 01773000
         L     SDWAPTR,SAVESDWA                                    0543 01774000
*             END;                  /* END OF SECTION TO QUEUE     0544 01775000
*                                      UNPROCESSED AIA'S             */ 01776000
*                                                                  0545 01777000
*           /*********************************************************/ 01778000
*           /*                                                       */ 01779000
*           /* IF THERE ARE ANY COMPLETED AIA'S TO BE RETURNED TO    */ 01780000
*           /* RSM, ATTEMPT TO RETRY TO RETURN THEM TO RSM. IF RETRY */ 01781000
*           /* IS NOT ALLOWED, MAKE ATTEMPT TO RETURN THEM TO RSM    */ 01782000
*           /* FROM FRR. THIS IS NOT ALWAYS DONE BECAUSE SUCH A CALL */ 01783000
*           /* MIGHT CAUSE A RECURSIVE PROBLEM.                      */ 01784000
*           /*                                                       */ 01785000
*           /*********************************************************/ 01786000
*                                                                  0545 01787000
*           IF ASM3PIOP^=ZERO THEN  /* TEST IF THERE ARE ANY       0545 01788000
*                                      COMPLETED AIA'S FOR RETURN TO    01789000
*                                      RSM                           */ 01790000
@RF00529 ICM   @12,15,ASM3PIOP(ASMVTPTR)                           0545 01791000
         BZ    @RF00545                                            0545 01792000
*             IF SDWACLUP=ON|       /* TEST IF RETRY IS ALLOWED      */ 01793000
*                 ATARCRF1=ON THEN  /* OR IF ILRPAGCM RECURSION FLAG    01794000
*                                      IS SET                        */ 01795000
         TM    SDWACLUP(SDWAPTR),B'10000000'                       0546 01796000
         BO    @RT00546                                            0546 01797000
         TM    ATARCRF1(ATAPTR),B'10000000'                        0546 01798000
         BNO   @RF00546                                            0546 01799000
@RT00546 DS    0H                                                  0547 01800000
*               DO;                 /* ATTEMPT TO RETURN AIA'S TO RSM   01801000
*                                      FROM FRR                      */ 01802000
*                 R1=ASM3PIOP;      /* POINT TO COMPLETED AIA'S      */ 01803000
         L     R1,ASM3PIOP(,ASMVTPTR)                              0548 01804000
*                 ASM3PIOP=ZERO;    /* ZERO PTR TO COMPLETED AIA'S   */ 01805000
         SLR   @12,@12                                             0549 01806000
         ST    @12,ASM3PIOP(,ASMVTPTR)                             0549 01807000
*                 CALL IEAVPIOP;    /* RETURN COMPLETED AIA'S TO RSM */ 01808000
         L     @15,@CV02312                                        0550 01809000
         BALR  @14,@15                                             0550 01810000
*                 SDWAPTR=SAVESDWA; /* RESTORE ADDR OF SDWA,       0551 01811000
*                                      DESTROYED BY CALL TO PIOP     */ 01812000
         L     SDWAPTR,SAVESDWA                                    0551 01813000
*                 RFY                                              0552 01814000
*                   SAVEPTR UNRSTD; /* RELEASE REG USED TO POINT TO     01815000
*                                      REG SAVE AREA - NO LONGER   0552 01816000
*                                      NEEDED                        */ 01817000
*               END;                /* END OF SECTION TO RETURN AIA'S   01818000
*                                      TO RSM                        */ 01819000
*             ELSE                  /* IF RETRY IS ALLOWED AND NO  0554 01820000
*                                      RECURSION IS INDICATED, SET UP   01821000
*                                      TO RETRY TO CALL PIOP         */ 01822000
*               DO;                 /* SET UP FOR RETRY INTO ILRPAGCM*/ 01823000
         B     @RC00546                                            0554 01824000
@RF00546 DS    0H                                                  0555 01825000
*                 SDWARTYA=ADDR(ILRCRPC1);/* SET RETRY ADDR TO 1ST 0555 01826000
*                                      RETRY POINT IN ILRPAGCM       */ 01827000
         L     @12,@CV02326                                        0555 01828000
         ST    @12,SDWARTYA(,SDWAPTR)                              0555 01829000
*                 ATARCRF1=ON;      /* TURN ON ILRPAGCM RECURSION  0556 01830000
*                                      FLAG IN ATA                   */ 01831000
         OI    ATARCRF1(ATAPTR),B'10000000'                        0556 01832000
*                 RFY                                              0557 01833000
*                  (R0,                                            0557 01834000
*                   R15) RSTD;      /* RESTRICT REGS FOR STM         */ 01835000
*                 STM(R0,R15,SDWASRSV);/* INSURE THAT RETRY REGS 3-4    01836000
*                                      ARE VALID                     */ 01837000
         STM   R0,R15,SDWASRSV(SDWAPTR)                            0558 01838000
*                 RFY                                              0559 01839000
*                  (R0,                                            0559 01840000
*                   R15) UNRSTD;    /* RELEASE REGS                  */ 01841000
*                 SDWASR08=ADDR(ILRCBPGM);/* INSURE ILRPAGCM BASE REG   01842000
*                                      IS VALID FOR RETRY            */ 01843000
         L     @12,@CV02332                                        0560 01844000
         ST    @12,SDWASR08(,SDWAPTR)                              0560 01845000
*                 ATASFLGS=''B;     /* CLEAR ALL SECTION FLAGS IN ATA   01846000
*                                      TO PREPARE FOR RETRY          */ 01847000
         XC    ATASFLGS(3,ATAPTR),ATASFLGS(ATAPTR)                 0561 01848000
*                 ATAPAGCM=ON;      /* INSURE THAT ONLY ILRPAGCM   0562 01849000
*                                      SECTION FLAG IS ON WHEN RETRY    01850000
*                                      IS ATTEMPTED                  */ 01851000
         OI    ATAPAGCM(ATAPTR),B'00000001'                        0562 01852000
*               END;                /* END OF SECTION TO SET UP FOR     01853000
*                                      RETRY                         */ 01854000
*         END;                      /* END OF CLEAN-UP PROCESSING    */ 01855000
@RC00546 DS    0H                                                  0564 01856000
@RF00545 DS    0H                                                  0565 01857000
*     END;                                                         0565 01858000
@RF00516 DS    0H                                                  0566 01859000
*                                                                  0566 01860000
*   /*****************************************************************/ 01861000
*   /*                                                               */ 01862000
*   /* COMMON EXIT PROCESSING. FIRST COMPLETE RETRY SET UP.          */ 01863000
*   /*                                                               */ 01864000
*   /*****************************************************************/ 01865000
*                                                                  0566 01866000
*   IF SDWARTYA^=ZERO THEN          /* TEST IF RETRY WAS REQUESTED   */ 01867000
@RF00514 ICM   @12,15,SDWARTYA(SDWAPTR)                            0566 01868000
         BZ    @RF00566                                            0566 01869000
*     SDWARCDE=FOUR;                /* IF RETRY IS ALLOWED AND     0567 01870000
*                                      REQUESTED, SET RETRY RETURN 0567 01871000
