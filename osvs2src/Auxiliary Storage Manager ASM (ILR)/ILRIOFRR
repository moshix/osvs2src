*   SDWAVRA(1:LENGTH(ATA))=ATA;     /* COPY ATA INTO VARIABLE      0084 00067000
*                                      RECORDING AREA                */ 00068000
         MVC   SDWAVRA(24,SDWAPTR),ATA(ATAPTR)                     0084 00069000
*   SDWAURAL=LENGTH(ATA);           /* INIT COUNT OF USED PORTION OF    00070000
*                                      VARIABLE RECORDING AREA       */ 00071000
         MVI   SDWAURAL(SDWAPTR),X'18'                             0085 00072000
*   SDWARCRD=ON;                    /* REQUEST RECORDING IN ALL    0086 00073000
*                                      CASES, EVEN IF PERCOLATED TO  */ 00074000
         OI    SDWARCRD(SDWAPTR),B'10000000'                       0086 00075000
*                                                                  0087 00076000
*   /*****************************************************************/ 00077000
*   /*                                                               */ 00078000
*   /* PERFORM COMMON VERIFICATIONS.                                 */ 00079000
*   /*                                                               */ 00080000
*   /*****************************************************************/ 00081000
*                                                                  0087 00082000
*   RFY                                                            0087 00083000
*     SAVEPTR RSTD;                 /* RESTRICT REG USED TO POINT TO    00084000
*                                      REG SAVE AREA                 */ 00085000
*   SAVEPTR=ADDR(ASMSAVE);          /* REG SAVE AREA ONLY USED WHEN     00086000
*                                      SALLOC LOCK IS HELD SO USE  0088 00087000
*                                      ASMSAVE - INITIALIZED HERE FOR   00088000
*                                      USE BY NON-EXTERNAL RECOVERY     00089000
*                                      ROUTINES                      */ 00090000
         LA    SAVEPTR,ASMSAVE(,ASMVTPTR)                          0088 00091000
*   IF ATASWPDR=OFF THEN            /* TEST IF SWAP DRIVER WAS IN  0089 00092000
*                                      CONTROL - IF NOT THE SALLOC 0089 00093000
*                                      LOCK IS HELD                  */ 00094000
