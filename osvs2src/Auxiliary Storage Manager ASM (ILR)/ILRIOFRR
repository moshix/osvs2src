*         RETCODE) UNRSTD;          /* RELEASE REGS                  */ 00277000
@RF00137 DS    0H                                                  0140 00278000
*                                                                  0140 00279000
*       /*************************************************************/ 00280000
*       /*                                                           */ 00281000
*       /* RECOVER FROM AN ERROR DURING SWAPCOMP'S PROCESSING OF A   */ 00282000
*       /* SWAP IN REQUEST.                                          */ 00283000
*       /*                                                           */ 00284000
*       /*************************************************************/ 00285000
*                                                                  0140 00286000
*       IF ATAASCB=ZERO THEN        /* IF ATAASCB IS 0, SWAPCOMP WAS    00287000
*                                      PROCESSING A SWAP IN REQUEST  */ 00288000
         SLR   @12,@12                                             0140 00289000
         C     @12,ATAASCB(,ATAPTR)                                0140 00290000
         BNE   @RF00140                                            0140 00291000
*         DO;                       /* RECOVERY FOR SWAP IN REQUEST  */ 00292000
*           IF ATAAIA^=ZERO THEN    /* TEST IF THERE IS A VALID AIA     00293000
*                                      CHECKPOINTED IN THE ATA       */ 00294000
         L     @11,ATAAIA(,ATAPTR)                                 0142 00295000
         CR    @11,@12                                             0142 00296000
         BE    @RF00142                                            0142 00297000
*             DO;                   /* PROCESS THIS AIA              */ 00298000
*               PCBPTR=ATAAIA-(LENGTH(PCB)-LENGTH(AIA));/* CALCULATE    00299000
*                                      RELATED PCB ADDR      @ZM43070*/ 00300000
         LR    PCBPTR,@11                                          0144 00301000
         SH    PCBPTR,@CH02178                                     0144 00302000
*               ATAASCB=PCBASCB;    /* GET THE ADDR OF THE ASCB BEING   00303000
*                                      SWAPPED IN            @ZM43070*/ 00304000
         MVC   ATAASCB(4,ATAPTR),PCBASCB(PCBPTR)                   0145 00305000
*               SWAPIN=ON;          /* SET INTERNAL FLAG TO INDICATE    00306000
*                                      MEMTERM IS FOR FAILURE DURING    00307000
*                                      SWAP-IN PROCESSING    @ZM43070*/ 00308000
         OI    SWAPIN,B'00100000'                                  0146 00309000
*               ATAAIA->AIAERROR=ON;/* TURN ON THE INDETERMINATE   0147 00310000
*                                      ERROR FLAG IN THIS AIA        */ 00311000
         OI    AIAERROR(@11),B'00000010'                           0147 00312000
*               RFY                                                0148 00313000
*                 AIAPTR RSTD;      /* RESTRICT REG TO POINT TO AIA  */ 00314000
*               AIAPTR=ASM3PIOP;    /* POINT TO THE FIRST AIA ON THE    00315000
*                                      CHAIN OF AIA'S TO BE RETURNED    00316000
*                                      TO RSM                        */ 00317000
         L     AIAPTR,ASM3PIOP(,ASMVTPTR)                          0149 00318000
*               DO WHILE(AIAPTR^=ZERO&AIAPTR^=ATAAIA);/* LOOP THROUGH   00319000
*                                      THIS CHAIN UNTIL THE END OF 0150 00320000
*                                      THE CHAIN IS REACHED OR ATAAIA   00321000
*                                      IS FOUND ON THE CHAIN         */ 00322000
