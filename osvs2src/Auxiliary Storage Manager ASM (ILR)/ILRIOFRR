*   RFY                                                            0568 01874000
*     R14 RSTD;                     /* RESTRICT REG FOR RETURN ADDR  */ 01875000
@RF00566 DS    0H                                                  0569 01876000
*   R14=RTMRTRN;                    /* RESTORE RTM'S RETURN ADDRESS  */ 01877000
         L     R14,RTMRTRN(,WORKADDR)                              0569 01878000
*   RETURN;                         /* RETURN TO RTM                 */ 01879000
@EL00001 DS    0H                                                  0570 01880000
@EF00001 DS    0H                                                  0570 01881000
@ER00001 BR    @14                                                 0570 01882000
*   RFY                                                            0571 01883000
*     R14 UNRSTD;                   /* RELEASE REG                   */ 01884000
*                                                                  0572 01885000
*/********************************************************************/ 01886000
*/*                                                                  */ 01887000
*/*  ILRCQIOE IS THE RECOVERY ROUTINE FOR ILRQIOE'S PROCESSING.  IT  */ 01888000
*/*           IS INVOKED BY THE FRR'S ILRIOFRR AND ILRSRB01.         */ 01889000
*/*                                                                  */ 01890000
*/*  INPUT - REG 1 CONTAINS THE ADDR OF THE SDWA                     */ 01891000
*/*          REG 3 CONTAINS THE ADDR OF THE ASMVT                    */ 01892000
*/*          REG 4 CONTAINS THE ADDR OF THE ATA                      */ 01893000
*/*          REG 8 CONTAINS THE ADDR OF A 92 BYTE WORKAREA TO BE     */ 01894000
*/*                USED BY THE VERIFICATION ROUTINES                 */ 01895000
*/*          REG 9 CONTAINS THE ADDR OF A XX BYTE WORKAREA           */ 01896000
*/*          REG 14 CONTAINS THE RETURN ADDRESS                      */ 01897000
*/*                                                                  */ 01898000
*/********************************************************************/ 01899000
*                                                                  0572 01900000
*ILRCQIOE:                                                         0572 01901000
*   ENTRY;                          /* QIOE RECOVERY SUBRTN          */ 01902000
@EP00572 DS    0H                                                  0573 01903000
*   RFY                                                            0573 01904000
*    (SAVEPTR,                                                     0573 01905000
*     R14) RSTD;                    /* RESTRICT REG CONTAINING RETURN   01906000
*                                      ADDRESS AND REG USED TO POINT    01907000
*                                      TO REG SAVE AREA              */ 01908000
*   RETURN1=R14;                    /* SAVE RETURN ADDRESS           */ 01909000
         ST    R14,RETURN1                                         0574 01910000
*   RFY                                                            0575 01911000
*     R14 UNRSTD;                   /* RELEASE REG                   */ 01912000
*   SAVEPTR=ADDR(ASMSAVE);          /* USE ASMSAVE AS REG SAVE AREA     01913000
*                                      SINCE SALLOC LOCK IS HELD     */ 01914000
         LA    SAVEPTR,ASMSAVE(,ASMVTPTR)                          0576 01915000
*   RFY                                                            0577 01916000
*    (R5,                                                          0577 01917000
*     R6,                                                          0577 01918000
*     R7) RSTD;                     /* RESTRICT REGS DESTROYED BY  0577 01919000
*                                      CALL TO Q VERIFICATION RTN    */ 01920000
*   CALL ILRVASGQ;                  /* VERIFY ASMSTAGQ               */ 01921000
         L     @15,@CV02316                                        0578 01922000
         BALR  @14,@15                                             0578 01923000
*   RFY                                                            0579 01924000
*    (R5,                                                          0579 01925000
*     R6,                                                          0579 01926000
*     R7) UNRSTD;                   /* RELEASE REGS                  */ 01927000
*                                                                  0580 01928000
*   /*****************************************************************/ 01929000
*   /*                                                               */ 01930000
*   /* PROCESS THE AIA CHECKPOINTED IN THE ATA.                      */ 01931000
*   /*                                                               */ 01932000
*   /*****************************************************************/ 01933000
*                                                                  0580 01934000
*   STM(ASMVTPTR,ATAPTR,REGSAVE);   /* SAVE VALUES OF REGS 3 AND 4,     01935000
*                                      THESE REGS ARE DESTROYED BY 0580 01936000
*                                      THE ROUTINE ILRVAIA           */ 01937000
         STM   ASMVTPTR,ATAPTR,REGSAVE                             0580 01938000
*   RFY                                                            0581 01939000
*    (PARMPTR,                                                     0581 01940000
*     RETCODE) RSTD;                /* RESTRICT REGS USED FOR CALL TO   01941000
*                                      ILRVAIA                       */ 01942000
*   PARMPTR=ATAAIA;                 /* POINT TO AIA TO BE VERIFIED   */ 01943000
         L     PARMPTR,ATAAIA(,ATAPTR)                             0582 01944000
*   RFY                                                            0583 01945000
*    (R2,                                                          0583 01946000
*     R3,                                                          0583 01947000
*     R4,                                                          0583 01948000
*     R5,                                                          0583 01949000
*     R6,                                                          0583 01950000
*     R7) RSTD;                     /* RESTRICT REGS DESTROYED BY  0583 01951000
*                                      CALL TO ELEMENT VERIFICATION     01952000
*                                      RTN                           */ 01953000
*   CALL ILRVAIA;                   /* VERIFY THE AIA                */ 01954000
         L     @15,@CV02322                                        0584 01955000
         BALR  @14,@15                                             0584 01956000
*   RFY                                                            0585 01957000
*    (R2,                                                          0585 01958000
*     R3,                                                          0585 01959000
*     R4,                                                          0585 01960000
*     R5,                                                          0585 01961000
*     R6,                                                          0585 01962000
*     R7) UNRSTD;                   /* RELEASE REGS                  */ 01963000
*   LM(ASMVTPTR,ATAPTR,REGSAVE);    /* RESTORE REGS DESTROYED BY CALL   01964000
*                                      TO ILRVAIA                    */ 01965000
         LM    ASMVTPTR,ATAPTR,REGSAVE                             0586 01966000
*   IF RETCODE=ZERO THEN            /* TEST FOR A RETURN CODE OF 0 -    01967000
*                                      INDICATING A VALID AIA        */ 01968000
         SLR   @14,@14                                             0587 01969000
         CR    RETCODE,@14                                         0587 01970000
         BNE   @RF00587                                            0587 01971000
*     DO;                           /* PROCESS A VALID ATAAIA        */ 01972000
*       RFY                                                        0589 01973000
*         AIAPTR RSTD;              /* RESTRICT REG TO POINT TO AIA  */ 01974000
*       AIAPTR=PARMPTR;             /* POINT TO ATAAIA               */ 01975000
         LR    AIAPTR,PARMPTR                                      0590 01976000
*       RFY                                                        0591 01977000
*        (PARMPTR,                                                 0591 01978000
*         RETCODE) UNRSTD;          /* RELEASE REGS                  */ 01979000
*       IF AIAPTR^=ASMSTAGF THEN    /* TEST IF THE AIA IN THE ATA IS    01980000
*                                      ON THE ASMSTAGQ - IF IT IS, IT   01981000
*                                      IS EQUAL TO ASMSTAGF          */ 01982000
         C     AIAPTR,ASMSTAGF(,ASMVTPTR)                          0592 01983000
         BE    @RF00592                                            0592 01984000
*         DO;                       /* IF THE AIA IN THE ATA IS NOT     01985000
*                                      ON THE ASMSTAGQ, PLACE IT ON     01986000
*                                      THE PART AIA ERROR QUEUE      */ 01987000
*           AIAERROR=ON;            /* FIRST MARK THE AIA WITH THE 0594 01988000
*                                      INDETERMINATE ERROR FLAG      */ 01989000
         OI    AIAERROR(AIAPTR),B'00000010'                        0594 01990000
*           AIANXAIA=ZERO;          /* INSURE THAT THERE IS ONLY ONE    01991000
*                                      AIA IN PROCESS                */ 01992000
         ST    @14,AIANXAIA(,AIAPTR)                               0595 01993000
*           CALL QTOPARTQ;          /* QUEUE AIA TO PARTAIAE         */ 01994000
         BAL   @14,QTOPARTQ                                        0596 01995000
*           RFY                                                    0597 01996000
*             AIAPTR UNRSTD;        /* RELEASE REG                   */ 01997000
*         END;                                                     0598 01998000
*     END;                          /* END OF SECTION TO PROCESS A 0599 01999000
*                                      VALID ATAAIA                  */ 02000000
@RF00592 DS    0H                                                  0600 02001000
*   ATAAIA=ZERO;                    /* ZERO ATAAIA                   */ 02002000
@RF00587 SLR   @12,@12                                             0600 02003000
         ST    @12,ATAAIA(,ATAPTR)                                 0600 02004000
*                                                                  0601 02005000
*   /*****************************************************************/ 02006000
*   /*                                                               */ 02007000
*   /* VERIFY ANY PROCESSED IOE'S.                                   */ 02008000
*   /*                                                               */ 02009000
*   /*****************************************************************/ 02010000
*                                                                  0601 02011000
*   RFY                                                            0601 02012000
*     PARMPTR RSTD;                 /* RESTRICT REG USED IN CALL TO     02013000
*                                      ILRVIOEQ                      */ 02014000
*   PARMPTR=ADDR(ASM2PAQ);          /* LOCATE QUEUE OF PRIVATE AREA     02015000
*                                      IOE'S                         */ 02016000
         LA    PARMPTR,ASM2PAQ(,ASMVTPTR)                          0602 02017000
*   RFY                                                            0603 02018000
*    (R5,                                                          0603 02019000
*     R6,                                                          0603 02020000
*     R7) RSTD;                     /* RESTRICT REGS DESTROYED BY  0603 02021000
*                                      CALL TO Q VERIFICATION RTNS   */ 02022000
*   CALL ILRVIOEQ;                  /* VERIFY THIS QUEUE             */ 02023000
         L     @15,@CV02319                                        0604 02024000
         BALR  @14,@15                                             0604 02025000
*   PARMPTR=ADDR(ASM2CAQ);          /* LOCATE QUEUE OF COMMON AREA 0605 02026000
*                                      IOE'S                         */ 02027000
         LA    PARMPTR,ASM2CAQ(,ASMVTPTR)                          0605 02028000
*   CALL ILRVIOEQ;                  /* VERIFY THIS QUEUE             */ 02029000
         L     @15,@CV02319                                        0606 02030000
         BALR  @14,@15                                             0606 02031000
*   PARMPTR=ADDR(ASM2DUPQ);         /* LOCATE QUEUE OF DUPLEXED IOE'S*/ 02032000
         LA    PARMPTR,ASM2DUPQ(,ASMVTPTR)                         0607 02033000
*   CALL ILRVIOEQ;                  /* VERIFY THIS QUEUE             */ 02034000
         L     @15,@CV02319                                        0608 02035000
         BALR  @14,@15                                             0608 02036000
*   RFY                                                            0609 02037000
*    (R5,                                                          0609 02038000
*     R6,                                                          0609 02039000
*     R7) UNRSTD;                   /* RELEASE REGS                  */ 02040000
*   RFY                                                            0610 02041000
*    (PARMPTR,                                                     0610 02042000
*     SAVEPTR) UNRSTD;              /* RELEASE REGS                  */ 02043000
*                                                                  0611 02044000
*   /*****************************************************************/ 02045000
*   /*                                                               */ 02046000
*   /* IF THERE ARE ANY IOE'S ON THE TEMPORARY WRITE QUEUES, OBTAIN  */ 02047000
*   /* THE ASM LOCK THAT SERIALIZES THE PART WRITE QUEUES AND QUEUE  */ 02048000
*   /* THE IOE'S TO THE PART WRITE QUEUES.                           */ 02049000
*   /*                                                               */ 02050000
*   /*****************************************************************/ 02051000
*                                                                  0611 02052000
*   IF ASM2PAQ^=ZERO|               /* TEST IF THERE ARE ANY PRIVATE    02053000
*                                      AREA IOE'S                    */ 02054000
*       ASM2CAQ^=ZERO|              /* TEST IF THERE ARE ANY COMMON     02055000
*                                      AREA IOE'S                    */ 02056000
*       ASM2DUPQ^=ZERO THEN         /* TEST IF THERE ARE ANY DUPLEXED   02057000
*                                      IOE'S                         */ 02058000
         SLR   @12,@12                                             0611 02059000
         C     @12,ASM2PAQ(,ASMVTPTR)                              0611 02060000
         BNE   @RT00611                                            0611 02061000
         C     @12,ASM2CAQ(,ASMVTPTR)                              0611 02062000
         BNE   @RT00611                                            0611 02063000
         C     @12,ASM2DUPQ(,ASMVTPTR)                             0611 02064000
         BE    @RF00611                                            0611 02065000
@RT00611 DS    0H                                                  0612 02066000
*     DO;                           /* PROCESS THESE IOE'S           */ 02067000
*                                                                  0612 02068000
*       /*************************************************************/ 02069000
*       /*                                                           */ 02070000
*       /* UNCONDITIONALLY REQUEST THE ASM LOCK THAT SERIALIZES THE  */ 02071000
*       /* PART WRITE QUEUES. EITHER THIS LOCK WAS ALREADY HELD BY   */ 02072000
*       /* ILRQIOE, OR IT WILL BE OBTAINED NOW. IN EITHER CASE, THE  */ 02073000
*       /* LOCK WILL BE FREED BY THIS ROUTINE, SO THERE IS NO TEST TO*/ 02074000
*       /* SEE WHETHER THE LOCK WAS JUST OBTAINED.                   */ 02075000
*       /*                                                           */ 02076000
*       /*************************************************************/ 02077000
*                                                                  0613 02078000
*       RFY                                                        0613 02079000
*        (R11,                                                     0613 02080000
*         R12,                                                     0613 02081000
*         R13,                                                     0613 02082000
*         R14) RSTD;                /* RESTRICT REGS USED BY SETLOCK */ 02083000
*       R11=ADDR(PARTLOCK);         /* POINT TO THE LOCKWORD FOR THE    02084000
*                                      PART WRITE QUEUES             */ 02085000
         L     R11,PARTPTR(,ASMVTPTR)                              0614 02086000
         LA    R11,PARTLOCK(,R11)                                  0614 02087000
*       GEN REFS(PSALITA);                                         0615 02088000
*                                  /* OBTAIN ASM PART LOCK          */  02089000
GTASM3   SETLOCK OBTAIN,TYPE=ASM,ADDR=(11),MODE=UNCOND,DISABLED,      **02090000
               RELATED=(PART,ILRCQIOE(FRASM3))                          02091000
*       RFY                                                        0616 02092000
*        (R11,                                                     0616 02093000
*         R12,                                                     0616 02094000
*         R13,                                                     0616 02095000
*         R14) UNRSTD;              /* RELEASE REGS                  */ 02096000
*       RFY                                                        0617 02097000
*        (IOEPTR,                                                  0617 02098000
*         QUEUEPTR) RSTD;           /* RESTRICT REGS USED TO CONTAIN    02099000
*                                      PARAMETERS FOR QUEUEIOE SUBRTN*/ 02100000
*       IOEPTR=ASM2PAQ;             /* POINT TO PRIVATE AREA IOE'S   */ 02101000
         L     IOEPTR,ASM2PAQ(,ASMVTPTR)                           0618 02102000
*       ASM2PAQ=ZERO;               /* ZERO PTR TO PRIVATE AREA IOE'S*/ 02103000
         SLR   @12,@12                                             0619 02104000
         ST    @12,ASM2PAQ(,ASMVTPTR)                              0619 02105000
*       IF ASMNOLCL=OFF THEN        /* IF THERE ARE LOCAL DATA SETS  */ 02106000
         TM    ASMNOLCL(ASMVTPTR),B'00100000'                      0620 02107000
         BNZ   @RF00620                                            0620 02108000
*         QUEUEPTR=ADDR(PARTLOCQ);  /* USE THE PART LOCAL WRITE Q    */ 02109000
         L     QUEUEPTR,PARTPTR(,ASMVTPTR)                         0621 02110000
         LA    QUEUEPTR,PARTLOCQ(,QUEUEPTR)                        0621 02111000
*       ELSE                        /* WHEN NO LOCAL DATA SETS ARE 0622 02112000
*                                      AVAILABLE                     */ 02113000
*         QUEUEPTR=ADDR(PARTCOMQ);  /* USE THE PART COMMON WRITE Q   */ 02114000
         B     @RC00620                                            0622 02115000
@RF00620 L     QUEUEPTR,PARTPTR(,ASMVTPTR)                         0622 02116000
         LA    QUEUEPTR,PARTCOMQ(,QUEUEPTR)                        0622 02117000
*       CALL QUEUEIOE;              /* QUEUE IOE'S TO PART WRITE Q   */ 02118000
@RC00620 BAL   @14,QUEUEIOE                                        0623 02119000
*       IOEPTR=ASM2CAQ;             /* POINT TO COMMON AREA IOE'S    */ 02120000
         L     IOEPTR,ASM2CAQ(,ASMVTPTR)                           0624 02121000
*       ASM2CAQ=ZERO;               /* ZERO PTR TO COMMON AREA IOE'S */ 02122000
         SLR   @12,@12                                             0625 02123000
         ST    @12,ASM2CAQ(,ASMVTPTR)                              0625 02124000
*       QUEUEPTR=ADDR(PARTCOMQ);    /* USE THE PART COMMON WRITE Q   */ 02125000
         L     QUEUEPTR,PARTPTR(,ASMVTPTR)                         0626 02126000
         LA    QUEUEPTR,PARTCOMQ(,QUEUEPTR)                        0626 02127000
*       CALL QUEUEIOE;              /* QUEUE IOE'S TO PART WRITE Q   */ 02128000
         BAL   @14,QUEUEIOE                                        0627 02129000
*       IOEPTR=ASM2DUPQ;            /* POINT TO DUPLEXED IOE'S       */ 02130000
         L     IOEPTR,ASM2DUPQ(,ASMVTPTR)                          0628 02131000
*       ASM2DUPQ=ZERO;              /* ZERO PTR TO DUPLEXED IOE'S    */ 02132000
         SLR   @12,@12                                             0629 02133000
         ST    @12,ASM2DUPQ(,ASMVTPTR)                             0629 02134000
*       QUEUEPTR=ADDR(PARTDUPQ);    /* USE THE PART DUPLEX WRITE Q   */ 02135000
         L     QUEUEPTR,PARTPTR(,ASMVTPTR)                         0630 02136000
         LA    QUEUEPTR,PARTDUPQ(,QUEUEPTR)                        0630 02137000
*       CALL QUEUEIOE;              /* QUEUE IOE'S TO PART WRITE Q   */ 02138000
         BAL   @14,QUEUEIOE                                        0631 02139000
*       RFY                                                        0632 02140000
*        (IOEPTR,                                                  0632 02141000
*         QUEUEPTR) UNRSTD;         /* RELEASE REGS                  */ 02142000
*     END;                          /* END OF SECTION TO QUEUE IOE'S */ 02143000
*                                                                  0634 02144000
*   /*****************************************************************/ 02145000
*   /*                                                               */ 02146000
*   /* CLEAN-UP RESOURCES, SCHEDULE THE SRB FOR PART MONITOR, AND SET*/ 02147000
*   /* UP FOR RETRY.                                                 */ 02148000
*   /*                                                               */ 02149000
*   /*****************************************************************/ 02150000
*                                                                  0634 02151000
*   RFY                                                            0634 02152000
*    (R11,                                                         0634 02153000
*     R12,                                                         0634 02154000
*     R13,                                                         0634 02155000
*     R14) RSTD;                    /* RESTRICT REGS USED BY SETLOCK */ 02156000
@RF00611 DS    0H                                                  0635 02157000
*   R11=ADDR(PARTLOCK);             /* POINT TO THE LOCKWORD FOR THE    02158000
*                                      PART WRITE QUEUES             */ 02159000
*                                                                  0635 02160000
         L     R11,PARTPTR(,ASMVTPTR)                              0635 02161000
         LA    R11,PARTLOCK(,R11)                                  0635 02162000
*   /*****************************************************************/ 02163000
*   /*                                                               */ 02164000
*   /* FREE THE ASM LOCK THAT SERIALIZES THE PART WRITE QUEUES, IF IT*/ 02165000
*   /* IS HELD. THE LOCK IS FREED EVEN IF IT WAS HELD ON ENTRY TO THE*/ 02166000
*   /* FRR. IN THIS CASE, GLOBAL DISABLEMENT, AND HENCE SERIALIZATION*/ 02167000
*   /* OF RTM'S RESOURCES, IS STILL MAINTAINED SINCE THE SALLOC LOCK */ 02168000
*   /* IS HELD. THE ASM LOCK, IF HELD, IS ALWAYS FREED BY THE FRR    */ 02169000
*   /* SINCE IT MUST BE FREED BY THE FRR IF RETRY IS TO BE REQUESTED.*/ 02170000
*   /*                                                               */ 02171000
*   /*****************************************************************/ 02172000
*                                                                  0636 02173000
*   GEN REFS(PSALITA);                                             0636 02174000
*                                  /* FREE ASM PART LOCK            */  02175000
FRASM3   SETLOCK RELEASE,TYPE=ASM,DISABLED,ADDR=(11),                 **02176000
               RELATED=(PART,ILRQIOE(GETLOCK),ILRCQIOE(GTASM3))         02177000
*   RFY                                                            0637 02178000
*    (R11,                                                         0637 02179000
*     R12,                                                         0637 02180000
*     R13,                                                         0637 02181000
*     R14) UNRSTD;                  /* RELEASE REGS                  */ 02182000
*                                                                  0637 02183000
*   /*****************************************************************/ 02184000
*   /*                                                               */ 02185000
*   /* SCHEDULE THE SRB FOR PART MONITOR AND SET UP FOR RETRY.       */ 02186000
*   /*                                                               */ 02187000
*   /*****************************************************************/ 02188000
*                                                                  0638 02189000
*   SAVESDWA=SDWAPTR;               /* SAVE ADDR OF SDWA, IT MAY BE     02190000
*                                      DESTROYED BY PMSCHEDL SUBRTN  */ 02191000
         ST    SDWAPTR,SAVESDWA                                    0638 02192000
*   CALL PMSCHEDL;                  /* SCHEDULE THE SRB FOR PART   0639 02193000
*                                      MONITOR, IF IT IS NOT ALREADY    02194000
*                                      SCHEDULED                     */ 02195000
         BAL   @14,PMSCHEDL                                        0639 02196000
*   SDWAPTR=SAVESDWA;               /* RESTORE ADDR OF SDWA, MAYBE 0640 02197000
*                                      DESTROYED BY PMSCHEDL         */ 02198000
         L     SDWAPTR,SAVESDWA                                    0640 02199000
*   IF ATARCRF7=OFF THEN            /* TEST IF ILRQIOE RECURSION FLAG   02200000
*                                      IS SET                        */ 02201000
         TM    ATARCRF7(ATAPTR),B'00000010'                        0641 02202000
         BNZ   @RF00641                                            0641 02203000
*     DO;                           /* IF NOT, SET UP FOR RETRY      */ 02204000
*       ATARCRF7=ON;                /* TURN ON ILRQIOE RECURSION FLAG   02205000
*                                      IN ATA                        */ 02206000
         OI    ATARCRF7(ATAPTR),B'00000010'                        0643 02207000
*       SDWARTYA=ADDR(ILRCRQI1);    /* SET RETRY ADDR TO QIOE RETRY     02208000
*                                      POINT                         */ 02209000
         L     @12,@CV02330                                        0644 02210000
         ST    @12,SDWARTYA(,SDWAPTR)                              0644 02211000
*       SDWASR03=ASMVTPTR;          /* INSURE ASMVT REG IS VALID FOR    02212000
*                                      RETRY                         */ 02213000
         ST    ASMVTPTR,SDWASR03(,SDWAPTR)                         0645 02214000
*     END;                          /* END OF SECTION TO SET UP FOR     02215000
*                                      RETRY                         */ 02216000
*   RFY                                                            0647 02217000
*     R14 RSTD;                     /* RESTRICT REG FOR RETURN ADDR  */ 02218000
@RF00641 DS    0H                                                  0648 02219000
*   R14=RETURN1;                    /* RESTORE RETURN ADDRESS        */ 02220000
         L     R14,RETURN1                                         0648 02221000
*   RETURN;                         /* RETURN TO CALLER              */ 02222000
         B     @EL00001                                            0649 02223000
*   RFY                                                            0650 02224000
*     R14 UNRSTD;                   /* RELEASE REG                   */ 02225000
*                                                                  0651 02226000
*/********************************************************************/ 02227000
*/*                                                                  */ 02228000
*/*  QTOPARTQ IS AN INTERNAL ROUTINE THAT QUEUES A CHAIN OF AIA'S TO */ 02229000
*/*           THE PART AIA ERROR QUEUE.                              */ 02230000
*/*                                                                  */ 02231000
*/*  INPUT - AIAPTR CONTAINS THE PTR TO THE CHAIN OF AIA'S OR 0      */ 02232000
*/*                                                                  */ 02233000
*/*  PROCESS - IF AIAPTR IS NOT 0, INDICATING THAT THERE ARE AIA'S   */ 02234000
*/*            ON THE CHAIN, THE WHOLE CHAIN IS ADDED TO THE PART    */ 02235000
*/*            AIA ERROR QUEUE (VIA CS).                             */ 02236000
*/*                                                                  */ 02237000
*/********************************************************************/ 02238000
*                                                                  0651 02239000
*QTOPARTQ:                                                         0651 02240000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0651 02241000
QTOPARTQ DS    0H                                                  0652 02242000
*   RFY                                                            0652 02243000
*    (AIAPTR,                                                      0652 02244000
*     R14) RSTD;                    /* RESTRICT REGS CONTAINING INPUT   02245000
*                                      PARAMETER AND RETURN ADDRESS  */ 02246000
*   RFY                                                            0653 02247000
*     R13 RSTD;                     /* RESTRICT REG RESTRICTED ACROSS   02248000
*                                      CALL TO THIS RTN              */ 02249000
*   IF AIAPTR^=ZERO THEN            /* TEST IF THERE ARE ANY AIA'S ON   02250000
*                                      THE INPUT CHAIN               */ 02251000
         LTR   AIAPTR,AIAPTR                                       0654 02252000
         BZ    @RF00654                                            0654 02253000
*     DO;                           /* IF THERE ARE, QUEUE THEM TO 0655 02254000
*                                      THE PART AIA ERROR QUEUE      */ 02255000
*       LASTAIA=AIAPTR;             /* INIT PTR TO LAST AIA          */ 02256000
         LR    LASTAIA,AIAPTR                                      0656 02257000
*       DO WHILE(LASTAIA->AIANXAIA^=ZERO);/* LOOP THROUGH INPUT CHAIN   02258000
*                                      TO FIND LAST AIA              */ 02259000
         B     @DE00657                                            0657 02260000
@DL00657 DS    0H                                                  0658 02261000
*         LASTAIA=LASTAIA->AIANXAIA;/* UPDATE PTR TO LAST AIA        */ 02262000
         L     LASTAIA,AIANXAIA(,LASTAIA)                          0658 02263000
*       END;                        /* UPON COMPLETION OF THIS LOOP,    02264000
*                                      LASTAIA WILL POINT TO THE LAST   02265000
*                                      AIA ON THE CHAIN              */ 02266000
@DE00657 ICM   @15,15,AIANXAIA(LASTAIA)                            0659 02267000
         BNZ   @DL00657                                            0659 02268000
*       RFY                                                        0660 02269000
*         WORK1 RSTD;               /* RESTRICT REG USED FOR CS      */ 02270000
*       WORK1=PARTAIAE;             /* GET PTR TO AIA ERROR QUEUE    */ 02271000
         L     @15,PARTPTR(,ASMVTPTR)                              0661 02272000
         L     WORK1,PARTAIAE(,@15)                                0661 02273000
*REDOAIAE:                          /* RETRY POINT FOR CS            */ 02274000
*       LASTAIA->AIANXAIA=WORK1;    /* PLACE INPUT CHAIN ON AIA ERROR   02275000
*                                      QUEUE                         */ 02276000
REDOAIAE ST    WORK1,AIANXAIA(,LASTAIA)                            0662 02277000
*       CS(WORK1,AIAPTR,PARTAIAE);  /* TRY TO UPDATE AIA ERROR QUEUE    02278000
*                                      IN PART                       */ 02279000
         L     @15,PARTPTR(,ASMVTPTR)                              0663 02280000
         CS    WORK1,@11,PARTAIAE(@15)                             0663 02281000
*       BC(NE,REDOAIAE);            /* IF CS FAILED, TRY IT AGAIN    */ 02282000
         BC    7,REDOAIAE                                          0664 02283000
*       RFY                                                        0665 02284000
*        (AIAPTR,                                                  0665 02285000
*         WORK1,                                                   0665 02286000
*         R13,                                                     0665 02287000
*         R14) UNRSTD;              /* RELEASE REGS                  */ 02288000
*     END;                          /* END OF SECTION TO PROCESS   0666 02289000
*                                      NON-ZERO INPUT CHAIN          */ 02290000
*   END QTOPARTQ;                                                  0667 02291000
@EL00002 DS    0H                                                  0667 02292000
@EF00002 DS    0H                                                  0667 02293000
@ER00002 BR    @14                                                 0667 02294000
*                                                                  0668 02295000
*/********************************************************************/ 02296000
*/*                                                                  */ 02297000
*/*  PMSCHEDL IS AN INTERNAL ROUTINE THAT SCHEDULES THE SRB FOR PART */ 02298000
*/*           MONITOR IF IT IS NOT ALREADY SCHEDULED.                */ 02299000
*/*                                                                  */ 02300000
*/*  INPUT - NONE                                                    */ 02301000
*/*                                                                  */ 02302000
*/*  PROCESS - IF ASMSRBCT INDICATES THAT THE SRB FOR PART MONITOR   */ 02303000
*/*            IS NOT SCHEDULED, SET ASMSRBCT (VIA CS) AND SCHEDULE  */ 02304000
*/*            THE SRB (NOTE: THE SCHEDULE DESTROYS REGS 0 & 1)      */ 02305000
*/*                                                                  */ 02306000
*/********************************************************************/ 02307000
*                                                                  0668 02308000
*PMSCHEDL:                                                         0668 02309000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0668 02310000
PMSCHEDL DS    0H                                                  0669 02311000
*   RFY                                                            0669 02312000
*     R14 RSTD;                     /* RESTRICT REG CONTAINING RETURN   02313000
*                                      ADDRESS                       */ 02314000
*   RFY                                                            0670 02315000
*     R13 RSTD;                     /* RESTRICT REG RESTRICTED ACROSS   02316000
*                                      CALL TO THIS RTN              */ 02317000
*   IF ASMSRBCT=ZERO THEN           /* IF SRB IS NOT SCHEDULED       */ 02318000
         ICM   @15,15,ASMSRBCT(ASMVTPTR)                           0671 02319000
         BNZ   @RF00671                                            0671 02320000
*     DO;                           /* SCHEDULE SRB AND INCREMENT  0672 02321000
*                                      COUNT IN ASMVT                */ 02322000
*       RFY                                                        0673 02323000
*        (WORK1,                                                   0673 02324000
*         WORK2) RSTD;              /* RESTRICT REGS USED FOR CS     */ 02325000
*       WORK1=ZERO;                 /* SRB COUNT SHOULD BE 0         */ 02326000
         SLR   WORK1,WORK1                                         0674 02327000
*       WORK2=ONE;                  /* TRY TO SET SRB COUNT TO 1     */ 02328000
         LA    WORK2,1                                             0675 02329000
*       CS(WORK1,WORK2,ASMSRBCT);   /* TRY TO SET SRB COUNT IN ASMVT */ 02330000
         CS    WORK1,@06,ASMSRBCT(ASMVTPTR)                        0676 02331000
*       BC(NE,NOSCHED);             /* IF CS FAILED, DO NOT RETRY IT */ 02332000
         BC    7,NOSCHED                                           0677 02333000
*       RFY                                                        0678 02334000
*        (WORK1,                                                   0678 02335000
*         WORK2) UNRSTD;            /* RELEASE REGS                  */ 02336000
*       RFY                                                        0679 02337000
*         R1 RSTD;                  /* RESTRICT REG TO POINT TO SRB  */ 02338000
*       R1=ASMPSRB;                 /* GET ADDR OF PART MONITOR SRB  */ 02339000
         L     R1,ASMPSRB(,ASMVTPTR)                               0680 02340000
*       GEN(SCHEDULE SRB=(1),SCOPE=GLOBAL)                         0681 02341000
*       REFS(CVTGSMQ,SRBFLNK,SRBSECT);/* SCHEDULE SRB FOR PART     0681 02342000
*                                      MONITOR                       */ 02343000
         SCHEDULE SRB=(1),SCOPE=GLOBAL                                  02344000
*       RFY                                                        0682 02345000
*         R1 UNRSTD;                /* RELEASE REG                   */ 02346000
*     END;                                                         0683 02347000
*NOSCHED:                           /* LABEL FOR BRANCH FOLLOWING  0684 02348000
*                                      UNSUCCESSFUL CS               */ 02349000
*   RFY                                                            0684 02350000
*    (R13,                                                         0684 02351000
*     R14) UNRSTD;                  /* RELEASE REGS                  */ 02352000
*   END PMSCHEDL;                                                  0685 02353000
@EL00003 DS    0H                                                  0685 02354000
@EF00003 DS    0H                                                  0685 02355000
@ER00003 BR    @14                                                 0685 02356000
*                                                                  0686 02357000
*/********************************************************************/ 02358000
*/*                                                                  */ 02359000
*/*  QUEUEIOE IS AN INTERNAL ROUTINE THAT QUEUES A CHAIN OF IOE'S TO */ 02360000
*/*           ONE OF THE PART WRITE QUEUES.                          */ 02361000
*/*                                                                  */ 02362000
*/*  INPUT - IOEPTR CONTAINS THE PTR TO THE CHAIN OF IOE'S OR 0      */ 02363000
*/*          QUEUEPTR CONTAINS THE PTR TO THE APPROPRIATE PART WRITE */ 02364000
*/*                   QUEUE                                          */ 02365000
*/*                                                                  */ 02366000
*/*  PROCESS - IF IOEPTR IS NOT 0, INDICATING THAT THERE ARE IOE'S   */ 02367000
*/*            ON THE CHAIN, THE WHOLE CHAIN IS ADDED TO THE PART    */ 02368000
*/*            WRITE QUEUE.                                          */ 02369000
*/*                                                                  */ 02370000
*/********************************************************************/ 02371000
*                                                                  0686 02372000
*QUEUEIOE:                                                         0686 02373000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0686 02374000
QUEUEIOE DS    0H                                                  0687 02375000
*   RFY                                                            0687 02376000
*    (IOEPTR,                                                      0687 02377000
*     QUEUEPTR,                                                    0687 02378000
*     R14) RSTD;                    /* RESTRICT REGS CONTAINING INPUT   02379000
*                                      PARAMETERS AND RETURN ADDRESS */ 02380000
*   RFY                                                            0688 02381000
*     QUEUE BASED(QUEUEPTR);        /* USE PART WRITE QUEUE POINTED     02382000
*                                      TO BY QUEUEPTR                */ 02383000
*   IF IOEPTR^=ZERO THEN            /* TEST IF THERE ARE ANY IOE'S ON   02384000
*                                      THE INPUT CHAIN               */ 02385000
         LTR   IOEPTR,IOEPTR                                       0689 02386000
         BZ    @RF00689                                            0689 02387000
*     DO;                           /* IF THERE ARE, QUEUE THEM TO 0690 02388000
*                                      THE PART WRITE QUEUE          */ 02389000
*       LASTIOE=IOEPTR;             /* INIT PTR TO LAST IOE          */ 02390000
         LR    LASTIOE,IOEPTR                                      0691 02391000
*       DO WHILE(LASTIOE->IOENXT^=ZERO);/* LOOP THROUGH INPUT CHAIN     02392000
*                                      TO FIND THE LAST IOE          */ 02393000
         B     @DE00692                                            0692 02394000
@DL00692 DS    0H                                                  0693 02395000
*         LASTIOE=LASTIOE->IOENXT;  /* UPDATE PTR TO LAST IOE        */ 02396000
         L     LASTIOE,IOENXT(,LASTIOE)                            0693 02397000
*       END;                        /* UPON COMPLETION TO THIS LOOP,    02398000
*                                      LASTIOE WILL POINT TO THE LAST   02399000
*                                      IOE ON THE CHAIN              */ 02400000
@DE00692 SLR   @15,@15                                             0694 02401000
         C     @15,IOENXT(,LASTIOE)                                0694 02402000
         BNE   @DL00692                                            0694 02403000
*       IF QUEUE1ST=ZERO THEN       /* IF THE QUEUE IS EMPTY, THAT 0695 02404000
*                                      IS, THE 1ST ELEMENT IS 0      */ 02405000
         C     @15,QUEUE1ST(,QUEUEPTR)                             0695 02406000
         BNE   @RF00695                                            0695 02407000
*         QUEUE1ST=IOEPTR;          /* SET THE 1ST ELEMENT TO INPUT     02408000
*                                      CHAIN PTR                     */ 02409000
         ST    IOEPTR,QUEUE1ST(,QUEUEPTR)                          0696 02410000
*       ELSE                        /* IF THE QUEUE IS NOT EMPTY     */ 02411000
*         QUEUELST->IOENXT=IOEPTR;  /* QUEUE THE INPUT CHAIN TO THE     02412000
*                                      LAST ELEMENT                  */ 02413000
         B     @RC00695                                            0697 02414000
@RF00695 L     @15,QUEUELST(,QUEUEPTR)                             0697 02415000
         ST    IOEPTR,IOENXT(,@15)                                 0697 02416000
*       QUEUELST=LASTIOE;           /* SET THE LAST ELEMENT OF THE 0698 02417000
*                                      QUEUE TO THE LAST IOE ON THE     02418000
*                                      INPUT CHAIN                   */ 02419000
@RC00695 ST    LASTIOE,QUEUELST(,QUEUEPTR)                         0698 02420000
*       RFY                                                        0699 02421000
*        (IOEPTR,                                                  0699 02422000
*         QUEUEPTR,                                                0699 02423000
*         R14) UNRSTD;              /* RELEASE REGS                  */ 02424000
*     END;                          /* SET OF SECTION TO PROCESS   0700 02425000
*                                      NON-ZERO INPUT CHAIN          */ 02426000
*   END QUEUEIOE;                                                  0701 02427000
@EL00004 DS    0H                                                  0701 02428000
@EF00004 DS    0H                                                  0701 02429000
@ER00004 BR    @14                                                 0701 02430000
*                                                                  0702 02431000
*/********************************************************************/ 02432000
*/*                                                                  */ 02433000
*/*  DEQPROCQ IS AN INTERNAL ROUTINE THAT DEQUEUES AN AIA OR AN ACE  */ 02434000
*/*           FROM THE CURRENT LGEPROCQ.                             */ 02435000
*/*                                                                  */ 02436000
*/*  INPUT - AIAPTR CONTAINS THE PTR TO THE AIA OR ACE               */ 02437000
*/*                                                                  */ 02438000
*/*  PROCESS - THE INPUT AIA OR ACE IS DEQUEUED FROM THE CURRENT     */ 02439000
*/*            LGEPROCQ (POINTED TO BY LGEPTR), IF IT IS ON THE      */ 02440000
*/*            LGEPROCQ.                                             */ 02441000
*/*                                                                  */ 02442000
*/*  OUTPUT - RETCODE CONTAINS A RETURN CODE INDICATING WHETHER OR   */ 02443000
*/*           NOT THE INPUT AIA OR ACE WAS ACTUALLY ON THE LGEPROCQ. */ 02444000
*/*           RETCODE IS ZERO IF THE AIA OR ACE IS FOUND ON THE      */ 02445000
*/*           LGEPROCQ AND IS FOUR IF THE AIA OR ACE IS NOT FOUND.   */ 02446000
*/*                                                                  */ 02447000
*/********************************************************************/ 02448000
*                                                                  0702 02449000
*DEQPROCQ:                                                         0702 02450000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0702 02451000
DEQPROCQ DS    0H                                                  0703 02452000
*   RFY                                                            0703 02453000
*    (AIAPTR,                                                      0703 02454000
*     R14,                                                         0703 02455000
*     RETCODE) RSTD;                /* RESTRICT REGS CONTAINING INPUT   02456000
*                                      PARAMETER AND RETURN ADDRESS     02457000
*                                      AND REG USED FOR RETURN CODE  */ 02458000
*   RFY                                                            0704 02459000
*     R2 RSTD;                      /* RESTRICT REG RESTRICTED ACROSS   02460000
*                                      CALL TO THIS RTN              */ 02461000
*   RETCODE=ZERO;                   /* INIT RETURN CODE              */ 02462000
         SLR   RETCODE,RETCODE                                     0705 02463000
*   IF AIAPTR=LGEPROCF THEN         /* TEST IF THE INPUT AIA IS THE     02464000
*                                      1ST ELEMENT OF THE LGEPROCQ   */ 02465000
         C     AIAPTR,LGEPROCF(,LGEPTR)                            0706 02466000
         BNE   @RF00706                                            0706 02467000
*     DO;                           /* IT IS LGEPROCF                */ 02468000
*       LGEPROCF=AIAFQPA;           /* RESET LGEPROCF                */ 02469000
         L     @05,AIAFQPA(,AIAPTR)                                0708 02470000
         ST    @05,LGEPROCF(,LGEPTR)                               0708 02471000
*       IF LGEPROCF=ZERO THEN       /* TEST IF QUEUE IS NOW EMPTY    */ 02472000
         SLR   @07,@07                                             0709 02473000
         CR    @05,@07                                             0709 02474000
         BNE   @RF00709                                            0709 02475000
*         LGEPROCL=ZERO;            /* IF IT IS, INSURE LGEPROCL IS     02476000
*                                      SET TO 0                      */ 02477000
         ST    @07,LGEPROCL(,LGEPTR)                               0710 02478000
*       ELSE                        /* IF THE QUEUE IS NOT EMPTY     */ 02479000
*         LGEPROCF->AIABQPA=ZERO;   /* ZERO THE BACK PTR OF THE FIRST   02480000
*                                      ELEMENT                       */ 02481000
         B     @RC00709                                            0711 02482000
@RF00709 L     @05,LGEPROCF(,LGEPTR)                               0711 02483000
         SLR   @00,@00                                             0711 02484000
         ST    @00,AIABQPA(,@05)                                   0711 02485000
*     END;                                                         0712 02486000
*   ELSE                            /* IF THE INPUT AIA IS NOT THE 0713 02487000
*                                      1ST ELEMENT OF THE LGEPROCQ   */ 02488000
*     IF AIAPTR=LGEPROCL THEN       /* TEST IF IT IS THE LAST ELEMENT   02489000
*                                      OF THE LGEPROCQ               */ 02490000
         B     @RC00706                                            0713 02491000
@RF00706 C     AIAPTR,LGEPROCL(,LGEPTR)                            0713 02492000
         BNE   @RF00713                                            0713 02493000
*       DO;                         /* IT IS LGEPROCL                */ 02494000
*         LGEPROCL=AIABQPA;         /* RESET LGEPROCL                */ 02495000
         L     @05,AIABQPA(,AIAPTR)                                0715 02496000
         ST    @05,LGEPROCL(,LGEPTR)                               0715 02497000
*         LGEPROCL->AIAFQPA=ZERO;   /* SET THE FORWARD PTR OF THE  0716 02498000
*                                      LAST ELEMENT TO 0             */ 02499000
         SLR   @00,@00                                             0716 02500000
         ST    @00,AIAFQPA(,@05)                                   0716 02501000
*       END;                                                       0717 02502000
*     ELSE                          /* THE INPUT AIA IS NEITHER THE     02503000
*                                      1ST NOR THE LAST ELEMENT OF 0718 02504000
*                                      THE LGEPROCQ                  */ 02505000
*       DO;                         /* SEARCH THE LGEPROCQ FOR THE 0718 02506000
*                                      INPUT AIA AND DEQUEUE IT IF 0718 02507000
*                                      FOUND                         */ 02508000
         B     @RC00713                                            0718 02509000
@RF00713 DS    0H                                                  0719 02510000
*         CURRAIA=LGEPROCF;         /* START SEARCH AT BEGINNING OF     02511000
*                                      LGEPROCQ                      */ 02512000
         L     CURRAIA,LGEPROCF(,LGEPTR)                           0719 02513000
*         FINISH=OFF;               /* INIT FLAG TO TERMINATE LOOP   */ 02514000
         NI    FINISH,B'10111111'                                  0720 02515000
*         DO WHILE(CURRAIA^=LGEPROCL&FINISH=OFF);/* LOOP THRU AIA'S     02516000
*                                      ON LGEPROCQ UNTIL END OF QUEUE   02517000
*                                      IS REACHED OR INPUT AIA IS  0721 02518000
*                                      FOUND                         */ 02519000
         B     @DE00721                                            0721 02520000
@DL00721 DS    0H                                                  0722 02521000
*           IF CURRAIA=AIAPTR THEN  /* IF INPUT AIA IS FOUND         */ 02522000
         CR    CURRAIA,AIAPTR                                      0722 02523000
         BNE   @RF00722                                            0722 02524000
*             DO;                   /* DEQUEUE IT FROM LGEPROCQ AND     02525000
*                                      TERMINATE LOOP                */ 02526000
*               AIAFQPA->AIABQPA=AIABQPA;/* RESET THE BACK PTR OF THE   02527000
*                                      NEXT ELEMENT                  */ 02528000
         L     @07,AIAFQPA(,AIAPTR)                                0724 02529000
         L     @06,AIABQPA(,AIAPTR)                                0724 02530000
         ST    @06,AIABQPA(,@07)                                   0724 02531000
*               AIABQPA->AIAFQPA=AIAFQPA;/* RESET THE FORWARD PTR OF    02532000
*                                      THE PREVIOUS ELEMENT          */ 02533000
         ST    @07,AIAFQPA(,@06)                                   0725 02534000
*               FINISH=ON;          /* FORCE LOOP TO TERMINATE       */ 02535000
         OI    FINISH,B'01000000'                                  0726 02536000
*             END;                                                 0727 02537000
*           ELSE                    /* IF INPUT AIA IS NOT FOUND     */ 02538000
*             CURRAIA=CURRAIA->AIAFQPA;/* UPDATE PTR TO CURRENT AIA  */ 02539000
         B     @RC00722                                            0728 02540000
@RF00722 L     CURRAIA,AIAFQPA(,CURRAIA)                           0728 02541000
*         END;                      /* END OF LOOP TO FIND INPUT AIA */ 02542000
@RC00722 DS    0H                                                  0729 02543000
@DE00721 C     CURRAIA,LGEPROCL(,LGEPTR)                           0729 02544000
         BE    @DC00721                                            0729 02545000
         TM    FINISH,B'01000000'                                  0729 02546000
         BZ    @DL00721                                            0729 02547000
@DC00721 DS    0H                                                  0730 02548000
*         IF FINISH=OFF THEN        /* IF FINISH IS NOT SET AND THE     02549000
*                                      END OF THE LOOP HAS BEEN    0730 02550000
*                                      REACHED, THE INPUT AIA WAS NOT   02551000
*                                      FOUND                         */ 02552000
         TM    FINISH,B'01000000'                                  0730 02553000
         BNZ   @RF00730                                            0730 02554000
*           RETCODE=FOUR;           /* SET AIA NOT FOUND RETURN CODE */ 02555000
         LA    RETCODE,4                                           0731 02556000
*       END;                                                       0732 02557000
@RF00730 DS    0H                                                  0733 02558000
*   IF RETCODE=ZERO THEN            /* TEST IF AIA WAS FOUND ON THE     02559000
*                                      LGEPROCQ                      */ 02560000
@RC00713 DS    0H                                                  0733 02561000
@RC00706 SLR   @12,@12                                             0733 02562000
         CR    RETCODE,@12                                         0733 02563000
         BNE   @RF00733                                            0733 02564000
*     DO;                                                          0734 02565000
*       AIAFQPA=ZERO;               /* 0 THE FORWARD PTR OF INPUT AIA*/ 02566000
         ST    @12,AIAFQPA(,AIAPTR)                                0735 02567000
*       AIABQPA=ZERO;               /* 0 THE BACK PTR OF INPUT AIA   */ 02568000
         ST    @12,AIABQPA(,AIAPTR)                                0736 02569000
*     END;                                                         0737 02570000
*   RFY                                                            0738 02571000
*    (AIAPTR,                                                      0738 02572000
*     R2,                                                          0738 02573000
*     R14) UNRSTD;                  /* RELEASE REGS                  */ 02574000
*   END DEQPROCQ;                                                  0739 02575000
@EL00005 DS    0H                                                  0739 02576000
@EF00005 DS    0H                                                  0739 02577000
@ER00005 BR    @14                                                 0739 02578000
*                                                                  0740 02579000
*/********************************************************************/ 02580000
*/*                                                                  */ 02581000
*/*  DEQSTAGQ IS AN INTERNAL ROUTINE THAT DEQUEUES AN AIA FROM THE   */ 02582000
*/*           ASMSTAGQ.                                              */ 02583000
*/*                                                                  */ 02584000
*/*  INPUT - AIAPTR CONTAINS THE PTR TO THE AIA                      */ 02585000
*/*                                                                  */ 02586000
*/*  PROCESS - THE INPUT AIA IS TESTED TO SEE IF IT IS ON THE        */ 02587000
*/*            ASMSTAGQ (IF IT IS, IT IS EQUAL TO ASMSTAGL).  IF     */ 02588000
*/*            THE AIA IS ON THE ASMSTAGQ, IT IS THEN DEQUEUED AND   */ 02589000
*/*            THE COUNT OF REQUESTS RECEIVED IS DECREMENTED.        */ 02590000
*/*                                                                  */ 02591000
*/********************************************************************/ 02592000
*                                                                  0740 02593000
*DEQSTAGQ:                                                         0740 02594000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0740 02595000
DEQSTAGQ DS    0H                                                  0741 02596000
*   RFY                                                            0741 02597000
*    (AIAPTR,                                                      0741 02598000
*     R14) RSTD;                    /* RESTRICT REGS CONTAINING INPUT   02599000
*                                      PARM AND RETURN ADDRESS       */ 02600000
*   IF AIAPTR=ASMSTAGL THEN         /* TEST IF INPUT AIA IS ON THE 0742 02601000
*                                      ASMSTAGQ                      */ 02602000
         C     AIAPTR,ASMSTAGL(,ASMVTPTR)                          0742 02603000
         BNE   @RF00742                                            0742 02604000
*     DO;                           /* DEQUEUE AIA FROM ASMSTAGQ     */ 02605000
*       IF AIAPTR=ASMSTAGF THEN     /* TEST IF INPUT AIA IS ALSO THE    02606000
*                                      FIRST AIA ON ASMSTAGQ         */ 02607000
         C     AIAPTR,ASMSTAGF(,ASMVTPTR)                          0744 02608000
         BNE   @RF00744                                            0744 02609000
*         DO;                       /* REMOVING INPUT AIA WILL LEAVE    02610000
*                                      AN EMPTY ASMSTAGQ             */ 02611000
*           ASMSTAGF=ZERO;          /* ZERO PTR TO FIRST AIA         */ 02612000
         SLR   @12,@12                                             0746 02613000
         ST    @12,ASMSTAGF(,ASMVTPTR)                             0746 02614000
*           ASMSTAGL=ZERO;          /* ZERO PTR TO LAST AIA          */ 02615000
         ST    @12,ASMSTAGL(,ASMVTPTR)                             0747 02616000
*         END;                                                     0748 02617000
*       ELSE                        /* OTHERWISE, THE INPUT AIA IS 0749 02618000
*                                      NOT THE ONLY AIA ON ASMSTAGQ  */ 02619000
*         DO;                       /* FIND THE AIA PRIOR TO THE   0749 02620000
*                                      INPUT AIA                     */ 02621000
         B     @RC00744                                            0749 02622000
@RF00744 DS    0H                                                  0750 02623000
*           LASTAIA=ASMSTAGF;       /* INIT PTR TO NEW LAST AIA      */ 02624000
         L     LASTAIA,ASMSTAGF(,ASMVTPTR)                         0750 02625000
*           DO WHILE(LASTAIA->AIANXAIA^=AIAPTR);/* LOOP THROUGH AIA'S   02626000
*                                      ON ASMSTAGQ TILL AIA PRIOR TO    02627000
*                                      INPUT AIA IS FOUND            */ 02628000
         B     @DE00751                                            0751 02629000
@DL00751 DS    0H                                                  0752 02630000
*             LASTAIA=LASTAIA->AIANXAIA;/* UPDATE PTR TO LAST AIA    */ 02631000
         L     LASTAIA,AIANXAIA(,LASTAIA)                          0752 02632000
*           END;                    /* END OF LOOP TO FIND PRIOR AIA */ 02633000
@DE00751 C     AIAPTR,AIANXAIA(,LASTAIA)                           0753 02634000
         BNE   @DL00751                                            0753 02635000
*           ASMSTAGL=LASTAIA;       /* SET PTR TO LAST AIA           */ 02636000
         ST    LASTAIA,ASMSTAGL(,ASMVTPTR)                         0754 02637000
*           ASMSTAGL->AIANXAIA=ZERO;/* DEQUEUE INPUT AIA             */ 02638000
         L     @12,ASMSTAGL(,ASMVTPTR)                             0755 02639000
         SLR   @05,@05                                             0755 02640000
         ST    @05,AIANXAIA(,@12)                                  0755 02641000
*         END;                                                     0756 02642000
*       ASMIORQR=ASMIORQR-ONE;      /* DECREMENT THE COUNT OF      0757 02643000
*                                      REQUESTS RECEIVED - DONE TO 0757 02644000
*                                      KEEP THIS COUNT ACCURATE, THE    02645000
*                                      COUNT WAS INCREMENTED BY    0757 02646000
*                                      ILRPAGIO AND BY ILRPOS BEFORE    02647000
*                                      THE AIA WAS ADDED TO THE    0757 02648000
*                                      ASMSTAGQ                      */ 02649000
@RC00744 L     @12,ASMIORQR(,ASMVTPTR)                             0757 02650000
         BCTR  @12,0                                               0757 02651000
         ST    @12,ASMIORQR(,ASMVTPTR)                             0757 02652000
*       IF AIAPRIV=ON|              /* IF AIA IS FOR A PRIVATE AREA     02653000
*                                      PAGE, OR                      */ 02654000
*           AIAVIO=ON|              /* AIA IS FOR A VIO PAGE, OR     */ 02655000
*           AIASWPFX=ON THEN        /* AIA IS FOR A PRIVATE AREA   0758 02656000
*                                      FIXED PAGE, THEN ASHIOCNT HAS    02657000
*                                      BEEN INCREMENTED BY ILRPAGIO     02658000
*                                      OR ILRPOS                     */ 02659000
         TM    AIAPRIV(AIAPTR),B'01100010'                         0758 02660000
         BZ    @RF00758                                            0758 02661000
*         DO;                       /* DECREMENT ASHIOCNT            */ 02662000
*           PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));/* CALCULATE THE    02663000
*                                      ASSOCIATED PCB                */ 02664000
         LR    PCBPTR,AIAPTR                                       0760 02665000
         SH    PCBPTR,@CH02178                                     0760 02666000
*           RFY                                                    0761 02667000
*             RSMHDPTR RSTD;        /* RESTRICT REG TO POINT TO RSMHD*/ 02668000
*           RSMHDPTR=PCBASCB->ASCBRSM;/* GET ADDR OF RSMHD           */ 02669000
         L     @12,PCBASCB(,PCBPTR)                                0762 02670000
         L     RSMHDPTR,ASCBRSM(,@12)                              0762 02671000
*           ASHIOCNT=ASHIOCNT-ONE;  /* DECREMENT THE COUNT OF PRIVATE   02672000
*                                      AREA I/O REQUESTS, INCREMENTED   02673000
*                                      BY ILRPAGIO AND BY ILRPOS   0763 02674000
*                                      BEFORE THE AIA WAS ADDED TO 0763 02675000
*                                      THE ASMSTAGQ                  */ 02676000
         L     @12,ASHIOCNT(,RSMHDPTR)                             0763 02677000
         BCTR  @12,0                                               0763 02678000
         ST    @12,ASHIOCNT(,RSMHDPTR)                             0763 02679000
*           RFY                                                    0764 02680000
*             RSMHDPTR UNRSTD;      /* RELEASE REG                   */ 02681000
*         END;                      /* END OF SECTION TO DECREMENT 0765 02682000
*                                      ASHIOCNT                      */ 02683000
*     END;                          /* END OF SECTION TO DEQUEUE   0766 02684000
*                                      INPUT AIA FROM ASMSTAGQ       */ 02685000
*   RFY                                                            0767 02686000
*    (AIAPTR,                                                      0767 02687000
*     R14) UNRSTD;                  /* RELEASE REGS                  */ 02688000
*   END DEQSTAGQ;                                                  0768 02689000
@EL00006 DS    0H                                                  0768 02690000
@EF00006 DS    0H                                                  0768 02691000
@ER00006 BR    @14                                                 0768 02692000
*                                                                  0769 02693000
*/********************************************************************/ 02694000
*/*                                                                  */ 02695000
*/*  FREEACE IS AN INTERNAL ROUTINE THAT FREES THE CELL USED FOR AN  */ 02696000
*/*          ACE.                                                    */ 02697000
*/*                                                                  */ 02698000
*/*  INPUT - CELLPTR CONTAINS THE PTR TO THE ACE                     */ 02699000
*/*                                                                  */ 02700000
*/*  PROCESS - THE ILRGMA MACRO IS USED TO FREE THE ACE (NOTE:  THE  */ 02701000
*/*            ILRGMA MACRO DESTROYS REG 1).                         */ 02702000
*/*                                                                  */ 02703000
*/********************************************************************/ 02704000
*                                                                  0769 02705000
*FREEACE:                                                          0769 02706000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0769 02707000
FREEACE  DS    0H                                                  0770 02708000
*   RFY                                                            0770 02709000
*    (CELLPTR,                                                     0770 02710000
*     R14) RSTD;                    /* RESTRICT REGS CONTAINING INPUT   02711000
*                                      PARM AND RETURN ADDRESS       */ 02712000
*   RETURN1=R14;                    /* SAVE RETURN ADDRESS           */ 02713000
         ST    R14,RETURN1                                         0771 02714000
*   RFY                                                            0772 02715000
*     R14 UNRSTD;                   /* RELEASE REG                   */ 02716000
*                                                                  0772 02717000
*   /*****************************************************************/ 02718000
*   /*                                                               */ 02719000
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */ 02720000
*   /*                                                               */ 02721000
*   /*****************************************************************/ 02722000
*                                                                  0773 02723000
*   DO;                             /* ILRGMA FREE                 0773 02724000
*                                      CELL(ACE)WORKREGS(WORK1,WORK2)*/ 02725000
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */ 02726000
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0774 02727000
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */ 02728000
*       1 GMACELL BASED BDY(WORD),                                 0775 02729000
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */ 02730000
*     RESPECIFY                                                    0776 02731000
*      (R0,                                                        0776 02732000
*       R1,                                                        0776 02733000
*       WORK1,                                                     0776 02734000
*       WORK2,                                                     0776 02735000
*       R14,                                                       0776 02736000
*       R15) RESTRICTED;                                           0776 02737000
*     RESPECIFY                                                    0777 02738000
*       ASMPOOLS BASED(WORK1);                                     0777 02739000
*     RESPECIFY                                                    0778 02740000
*       GMACELL BASED(R1);                                         0778 02741000
*     WORK1=ADDR(ASMACEPC);                                        0779 02742000
         LA    WORK1,ASMACEPC(,ASMVTPTR)                           0779 02743000
*     LM(R14,R15,ASMCPRSQ);         /* GET CONTENTS OF Q             */ 02744000
         LM    R14,R15,ASMCPRSQ(WORK1)                             0780 02745000
*GMA00031:                                                         0781 02746000
*     IF R14^=0 THEN                /* TEST RESERVE Q                */ 02747000
GMA00031 LTR   R14,R14                                             0781 02748000
         BZ    @RF00781                                            0781 02749000
*       DO;                         /* REPLENISH ACE RESERVE Q       */ 02750000
*         R0=R14-1;                 /* SET UP FOR COMPARE AND SWAP   */ 02751000
         LR    R0,R14                                              0783 02752000
         BCTR  R0,0                                                0783 02753000
*         GMANEXT=R15;              /* CELL TO FREE IN R1            */ 02754000
         ST    R15,GMANEXT(,R1)                                    0784 02755000
*         CDS(R14,R0,ASMCPRSQ);     /* ADD CELL TO RESERVE Q         */ 02756000
         CDS   R14,R0,ASMCPRSQ(WORK1)                              0785 02757000
*         BC(7,GMA00031);                                          0786 02758000
         BC    7,GMA00031                                          0786 02759000
*       END;                                                       0787 02760000
*     ELSE                                                         0788 02761000
*       DO;                         /* RETURN ACE TO NORMAL Q        */ 02762000
         B     @RC00781                                            0788 02763000
@RF00781 DS    0H                                                  0789 02764000
*         R14=ASMCPAVL;                                            0789 02765000
         L     R14,ASMCPAVL(,WORK1)                                0789 02766000
*GMA00032:                                                         0790 02767000
*         GMANEXT=R14;              /* PUT ADDR CURRENT FIRST IN   0790 02768000
*                                      INPUT CELL                    */ 02769000
GMA00032 ST    R14,GMANEXT(,R1)                                    0790 02770000
*         CS(R14,R1,ASMCPAVL);      /* ADD INPUT CELL TO POOL        */ 02771000
         CS    R14,@01,ASMCPAVL(WORK1)                             0791 02772000
*         BC(7,GMA00032);                                          0792 02773000
         BC    7,GMA00032                                          0792 02774000
*       END;                        /* ACE RETURNED TO NORMAL Q      */ 02775000
*     RESPECIFY                                                    0794 02776000
*      (R0,                                                        0794 02777000
*       R1,                                                        0794 02778000
*       WORK1,                                                     0794 02779000
*       WORK2,                                                     0794 02780000
*       R14,                                                       0794 02781000
*       R15) UNRESTRICTED;                                         0794 02782000
@RC00781 DS    0H                                                  0795 02783000
*   END;                            /* ILRGMA FREE                 0795 02784000
*                                      CELL(ACE)WORKREGS(WORK1,WORK2)*/ 02785000
*                                                                  0795 02786000
*   /*****************************************************************/ 02787000
*   /*                                                               */ 02788000
*   /* END ILRGMA MACRO EXPANSION                                    */ 02789000
*   /*                                                               */ 02790000
*   /*****************************************************************/ 02791000
*                                                                  0796 02792000
*                                   /* FREE CELL USED FOR ACE        */ 02793000
*   RFY                                                            0796 02794000
*     R14 RSTD;                     /* RESTRICT REG USED FOR RETURN     02795000
*                                      ADDRESS                       */ 02796000
*   R14=RETURN1;                    /* RESTORE RETURN ADDRESS        */ 02797000
         L     R14,RETURN1                                         0797 02798000
*   RFY                                                            0798 02799000
*    (CELLPTR,                                                     0798 02800000
*     R14) UNRSTD;                  /* RELEASE REGS                  */ 02801000
*   END FREEACE;                                                   0799 02802000
@EL00007 DS    0H                                                  0799 02803000
@EF00007 DS    0H                                                  0799 02804000
@ER00007 BR    @14                                                 0799 02805000
*   END ILRIOFRR                                                   0800 02806000
*                                                                  0800 02807000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02808000
*/*%INCLUDE SYSUT5  (FRRDCL  )                                       */ 02809000
*/*%INCLUDE SYSLIB  (ILRACE  )                                       */ 02810000
*/*%INCLUDE SYSLIB  (ILRAIA  )                                       */ 02811000
*/*%INCLUDE SYSLIB  (IHAASCB )                                       */ 02812000
*/*%INCLUDE SYSLIB  (ILRASMHD)                                       */ 02813000
*/*%INCLUDE SYSLIB  (ILRASMVT)                                       */ 02814000
*/*%INCLUDE SYSLIB  (ILRATA  )                                       */ 02815000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 02816000
*/*%INCLUDE SYSLIB  (ILRIOE  )                                       */ 02817000
*/*%INCLUDE SYSLIB  (IHALCCA )                                       */ 02818000
