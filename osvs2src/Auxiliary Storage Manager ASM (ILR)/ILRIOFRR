*               RFY                                                0283 00785000
*                 R1 UNRSTD;        /* RELEASE REG                   */ 00786000
*             END;                                                 0284 00787000
*           SDWAPTR=SAVESDWA;       /* RESTORE ADDR OF SDWA, MAY HAVE   00788000
*                                      BEEN DESTROYED BY ILRGMA FREE    00789000
*                                      OR BY SCHEDULE                */ 00790000
@RF00276 L     SDWAPTR,SAVESDWA                                    0285 00791000
*         END;                      /* END OF SECTION TO PROCESS A 0286 00792000
*                                      VALID AIA                     */ 00793000
*                                                                  0287 00794000
*       /*************************************************************/ 00795000
*       /*                                                           */ 00796000
*       /* CLEAN-UP RESOURCES AND CONTINUE RECOVERY PROCESSING.      */ 00797000
*       /*                                                           */ 00798000
*       /*************************************************************/ 00799000
*                                                                  0287 00800000
*       RFY                                                        0287 00801000
*        (WORK1,                                                   0287 00802000
*         RETCODE) RSTD;            /* RESTRICT REGS USED BY SETLOCK    00803000
*                                      TEST                          */ 00804000
@RF00220 DS    0H                                                  0288 00805000
*       WORK1=ASMLMASK;             /* SET MASK TO TEST IF ASM LOCK     00806000
*                                      IS HELD                       */ 00807000
         LA    WORK1,2048                                          0288 00808000
*       GEN;                                                       0289 00809000
*                                  /* TEST IF ASM LOCK IS HELD      */  00810000
         SETLOCK TEST,TYPE=(WORK1)                                      00811000
*       RFY                                                        0290 00812000
*        (R11,                                                     0290 00813000
*         R12,                                                     0290 00814000
*         R13,                                                     0290 00815000
*         R14) RSTD;                /* RESTRICT REGS USED BY SETLOCK */ 00816000
*                                                                  0290 00817000
*       /*************************************************************/ 00818000
*       /*                                                           */ 00819000
*       /* FREE THE ASM LOCK HELD ON THIS CPU, IF IT IS HELD. IF IT  */ 00820000
*       /* IS HELD, IT COULD ONLY BE THE ASM LOCK OBTAINED BY        */ 00821000
*       /* ILRVIOCM OR BY THIS ROUTINE. THIS IS BECAUSE THE SALLOC   */ 00822000
*       /* LOCK WAS ALREADY HELD WHEN ILRVIOCM WAS ENTERED. BECAUSE  */ 00823000
*       /* THE SALLOC LOCK IS HELD, GLOBAL DISABLEMENT, AND HENCE    */ 00824000
*       /* SERIALIZATION OF RTM'S RESOURCES, IS STILL MAINTAINED EVEN*/ 00825000
*       /* IF THE ASM LOCK IS FREED BY THE FRR.                      */ 00826000
*       /*                                                           */ 00827000
*       /*************************************************************/ 00828000
*                                                                  0291 00829000
*       IF RETCODE=ZERO THEN        /* IF THE ASM LOCK IS HELD, FREE    00830000
*                                      THIS LOCK                     */ 00831000
         LTR   RETCODE,RETCODE                                     0291 00832000
         BNZ   @RF00291                                            0291 00833000
*         GEN REFS(PSALITA,PSAMPSW);                               0292 00834000
*                                  /* FREE ASM LOCK                 */  00835000
FRASM1   SETLOCK RELEASE,TYPE=(WORK1),DISABLED,                       **00836000
               RELATED=(VIO,ILRVIOCM(SET),ILRIOFRR(GTASM1))             00837000
*         RFY                                                      0293 00838000
*          (WORK1,                                                 0293 00839000
*           R11,                                                   0293 00840000
*           R12,                                                   0293 00841000
*           R13,                                                   0293 00842000
*           R14,                                                   0293 00843000
*           RETCODE) UNRSTD;        /* RELEASE REGS                  */ 00844000
@RF00291 DS    0H                                                  0294 00845000
*       SAVEPTR=ADDR(ASMSAVE);      /* RESTORE ADDR OF REG SAVE AREA,   00846000
*                                      DESTROYED BY SETLOCK          */ 00847000
         LA    SAVEPTR,ASMSAVE(,ASMVTPTR)                          0294 00848000
*       ATAVIOCM=OFF;               /* TURN OFF VIOCM FLAG IN ATA    */ 00849000
         NI    ATAVIOCM(ATAPTR),B'11101111'                        0295 00850000
*       GOTO VERASCB;               /* CONTINUE RECOVERY PROCESSING     00851000
*                                      IN PAGECOMP RECOVERY          */ 00852000
         B     VERASCB                                             0296 00853000
*     END;                          /* END OF VIOCM RECOVERY         */ 00854000
*                                                                  0298 00855000
*   /*****************************************************************/ 00856000
*   /*                                                               */ 00857000
*   /* IF PAGECOMP SUBROUTINE OF ILRPAGCM WAS IN CONTROL AT THE TIME */ 00858000
*   /* OF ERROR, PERFORM PAGECOMP RECOVERY.                          */ 00859000
*   /*                                                               */ 00860000
*   /*****************************************************************/ 00861000
*                                                                  0298 00862000
*   IF ATAPCOMP=ON THEN                                            0298 00863000
