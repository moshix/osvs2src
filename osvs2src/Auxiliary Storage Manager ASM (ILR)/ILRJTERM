*             IF ACEPTR^=ZERO THEN                                 0100 00155000
         LTR   ACEPTR,ACEPTR                                       0100 00156000
         BZ    @RF00100                                            0100 00157000
*               DO;                 /* FOR GOOD RETURN ADDR FROM GMA */ 00158000
*                 ACE=ACE&&ACE;     /* ZERO OUT THE ACE              */ 00159000
