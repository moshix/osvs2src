*                                                                  0158 00349000
*/********************************************************************/ 00350000
*/*                                                                  */ 00351000
*/*                          ILRJTERM RECOVERY                       */ 00352000
*/*     INPUT TO JTM01 FROM RT/M WILL BE:                            */ 00353000
*/*       REGISTER 0 CONTAINS ADDR OF FRR WORKAREA                   */ 00354000
*/*       REGISTER 1 CONTAINS ADDR OF SDWA                           */ 00355000
*/*       REGISTER 14 CONTAINS RETURN ADDRESS                        */ 00356000
*/*       REGISTER 15 CONTAINS JTM01 EPA IN CVTJTERM                 */ 00357000
*/*     THE ATA POINTER IS ESTABLISHED, AND CERTAIN FIELDS IN SDWA   */ 00358000
*/*     ARE INITIALIZED.                                             */ 00359000
*/*                                                                  */ 00360000
*/********************************************************************/ 00361000
*                                                                  0158 00362000
*ILRJTM01:                                                         0158 00363000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0158 00364000
ILRJTM01 DS    0H                                                  0159 00365000
*   DCL                                                            0159 00366000
*     RECSDWA CHAR(LENGTH(ATA)) BASED(ADDR(SDWAVRA));              0159 00367000
*   DCL                                                            0160 00368000
*     IDLIST CHAR(24) CONSTANT('ILRJTERMILRJTERMILRJTM01');        0160 00369000
*   GEN REFS(10,11) SETS(10,11);                                   0161 00370000
         BALR  10,0               GET BASE REG FROM JTERM               00371000
BASE2    LA    11,BASE2-@PSTART   GET DIFFERENCE IN JTERM,JTM01         00372000
         SR    10,11              PUT BASE ADDR INTO REG 10             00373000
*   RFY                                                            0162 00374000
*    (SDWAPTR,                                                     0162 00375000
*     R14) RSTD;                    /* RESTRICT USE OF REGS 1,14     */ 00376000
*   SAVEADDR=R14;                   /* SAVE RETURN ADDRESS           */ 00377000
         LR    SAVEADDR,R14                                        0163 00378000
*   RFY                                                            0164 00379000
*     R14 UNRSTD;                   /* FREE REG 14 FOR USE           */ 00380000
*   ATAPTR=SDWAPARM;                /* ESTABLISH ADDR TO ATA         */ 00381000
         L     ATAPTR,SDWAPARM(,SDWAPTR)                           0165 00382000
*   RECSDWA=ATA;                    /* SAVE ATA                      */ 00383000
         MVC   RECSDWA+404(24,SDWAPTR),ATA(ATAPTR)                 0166 00384000
*   SDWAURAL=LENGTH(ATA);           /* SET LENGTH OF ATA             */ 00385000
         MVI   SDWAURAL(SDWAPTR),X'18'                             0167 00386000
*   DO;                             /* SETRP RC(4)RECPARM(IDLIST)RETA   00387000
*                                      DDR(ILRCRTR1)RECORD(YES)FRESDW   00388000
*                                      A(YES)                        */ 00389000
*     RESPECIFY                                                    0169 00390000
*      (GPR00P,                                                    0169 00391000
*       GPR01P,                                                    0169 00392000
*       GPR14P,                                                    0169 00393000
*       GPR15P) RSTD;                                              0169 00394000
*     GPR01P->SDWARCDE=4;           /* STORE RC INTO SDWA            */ 00395000
         MVI   SDWARCDE(GPR01P),X'04'                              0170 00396000
*     GPR01P->SDWARTYA=ADDR(ILRCRTR1);/* SAVE RETRY ADDRESS          */ 00397000
         LA    @12,ILRCRTR1                                        0171 00398000
         ST    @12,SDWARTYA(,GPR01P)                               0171 00399000
*     GPR01P->SDWAFREE='1'B;        /* SET FLAG TO FREE SDWA         */ 00400000
*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */ 00401000
         OI    SDWAFREE(GPR01P),B'10000100'                        0173 00402000
*     GPR15P=ADDR(IDLIST);          /* ACCESS RECORD PARAMETER LIST  */ 00403000
         LA    GPR15P,@CC01889                                     0174 00404000
*     GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD PARAMETERS */ 00405000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   0175 00406000
*     RESPECIFY                                                    0176 00407000
*      (GPR00P,                                                    0176 00408000
*       GPR01P,                                                    0176 00409000
*       GPR14P,                                                    0176 00410000
*       GPR15P) UNRSTD;                                            0176 00411000
*   END;                                                           0177 00412000
*                                                                  0178 00413000
*/********************************************************************/ 00414000
*/*                                                                  */ 00415000
*/*     IF THIS IS A RETRYABLE ERROR AND THERE HAS BEEN NO RECURSION */ 00416000
*/*     THEN GET THE ASM LOCK AND SET THE RECURSION FLAG IN THE ATA. */ 00417000
*/*     OTHERWISE SET LOCK FIELDS IN SDWA AND EXIT.                  */ 00418000
*/*                                                                  */ 00419000
*/********************************************************************/ 00420000
*                                                                  0178 00421000
*   IF SDWACLUP=OFF&ATARCRF1=OFF THEN                              0178 00422000
         TM    SDWACLUP(SDWAPTR),B'10000000'                       0178 00423000
         BNZ   @RF00178                                            0178 00424000
         TM    ATARCRF1(ATAPTR),B'10000000'                        0178 00425000
         BNZ   @RF00178                                            0178 00426000
*     DO;                                                          0179 00427000
*       RFY                                                        0180 00428000
*        (R11,                                                     0180 00429000
*         R12,                                                     0180 00430000
*         R13,                                                     0180 00431000
*         R14) RSTD;                                               0180 00432000
*       R11=ADDR(ASHLOCK);                                         0181 00433000
         L     @15,ASCBPTR                                         0181 00434000
         L     R11,RSMHDPTR(,@15)                                  0181 00435000
         LA    R11,ASHLOCK(,R11)                                   0181 00436000
*       GEN REFS(PSALITA,R11) SETS(R12,R13,R14);                   0182 00437000
RECSET   SETLOCK OBTAIN,TYPE=ASM,ADDR=(11),MODE=UNCOND,               XX00438000
               RELATED=(VIO,ILRJTERM(RELEASM))                          00439000
*       RFY                                                        0183 00440000
*        (R11,                                                     0183 00441000
*         R12,                                                     0183 00442000
*         R13,                                                     0183 00443000
*         R14) UNRSTD;                                             0183 00444000
*       ATARCRF1=ON;                /* SET RECURSION FLAG            */ 00445000
         OI    ATARCRF1(ATAPTR),B'10000000'                        0184 00446000
*       STM(R0,R15,SDWASRSV);       /* SAVE JTERM REGS IN SDWA       */ 00447000
         STM   R0,R15,SDWASRSV(SDWAPTR)                            0185 00448000
*     END;                                                         0186 00449000
*   ELSE                                                           0187 00450000
*     DO;                           /* SETRP FRELOCK(ASM(ASHLOCK),LOC   00451000
*                                      AL)RC(0)                      */ 00452000
         B     @RC00178                                            0187 00453000
@RF00178 DS    0H                                                  0188 00454000
*       RESPECIFY                                                  0188 00455000
*        (GPR00P,                                                  0188 00456000
*         GPR01P,                                                  0188 00457000
*         GPR14P,                                                  0188 00458000
*         GPR15P) RSTD;                                            0188 00459000
*       GPR01P->SDWARCDE=0;         /* SAVE RC VALUE                 */ 00460000
