         TITLE 'ILRJTERM - JOB TERMINATION RESOURCE MANAGER            *00001000
                        '                                               00002000
ILRJTERM CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRJTERM  76.087'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@10                                         0001 00013000
*   SAVEADDR=R13;                   /* SAVE SAVE AREA ADDRESS        */ 00014000
         LR    SAVEADDR,R13                                        0058 00015000
*   IF ASHLGEQ^=ZERO THEN                                          0059 00016000
         L     @12,ASCBPTR                                         0059 00017000
         L     @12,RSMHDPTR(,@12)                                  0059 00018000
         L     @12,ASHLGEQ(,@12)                                   0059 00019000
         LTR   @12,@12                                             0059 00020000
         BZ    @RF00059                                            0059 00021000
*     DO;                           /* IF LGE QUEUE NOT EMPTY        */ 00022000
*       RFY                                                        0061 00023000
*        (R11,                                                     0061 00024000
*         R12,                                                     0061 00025000
*         R14) RSTD;                /* THEN GET THE LOCAL LOCK       */ 00026000
*       GEN REFS(PSALITA) SETS(R11,R12,R13,R14);                   0062 00027000
GETLOC   SETLOCK OBTAIN,TYPE=LOCAL,RELATED=(VIO,ILRJTERM(RELELOC)),   XX00028000
               MODE=UNCOND                                              00029000
*       RFY                                                        0063 00030000
*        (R13,                                                     0063 00031000
*         R14) UNRSTD;                                             0063 00032000
*       RFY                                                        0064 00033000
*         R1 RSTD;                  /* RESTRICT USE OF REG 1         */ 00034000
*       R1=ADDR(ILRJTM01);          /* GET ADDR OF JTERM'S RECOVERY     00035000
*                                      RTN                           */ 00036000
         LA    R1,ILRJTM01                                         0065 00037000
*       GEN REFS(R1,PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRSELEN,   0066 00038000
*           FRRSPARM) SETS(FRRSFRRA,FRRSENTR,ATAPTR,R11,R12);      0066 00039000
