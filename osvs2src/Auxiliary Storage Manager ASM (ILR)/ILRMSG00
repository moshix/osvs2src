*               IF TYPE=PAGEDS THEN                                0262 00838000
         LTR   TYPE,TYPE                                           0262 00839000
         BNZ   @RF00262                                            0262 00840000
*                 DO;                                              0263 00841000
*                   IF PAREPLPA=OFF /* DEFAULT NAME IS PLPA          */ 00842000
*                     THEN                                         0264 00843000
         TM    PAREPLPA(ENTRYPTR),B'10000000'                      0264 00844000
         BNZ   @RF00264                                            0264 00845000
*                     IF PARECOMM=ON THEN                          0265 00846000
         TM    PARECOMM(ENTRYPTR),B'01000000'                      0265 00847000
         BNO   @RF00265                                            0265 00848000
*                       MSGNAME=COMMONDS;/* CHANGE DATA SET NAME FROM   00849000
*                                      PLPA TO COMMON                */ 00850000
         MVC   MSGNAME(6,@12),@CC02372                             0266 00851000
*                     ELSE                                         0267 00852000
*                       IF PARELOCL=ON THEN                        0267 00853000
         B     @RC00265                                            0267 00854000
@RF00265 TM    PARELOCL(ENTRYPTR),B'00010000'                      0267 00855000
         BNO   @RF00267                                            0267 00856000
*                         MSGNAME=LOCALDS;/* CHANGE DATA SET NAME  0268 00857000
*                                      FROM PLPA TO LOCAL            */ 00858000
         MVC   MSGNAME+4(6,BUFPTR),@CC02378                        0268 00859000
*                       ELSE                                       0269 00860000
*                         RETURN;   /* RETURN TO CALLER BECAUSE TYPE    00861000
*                                      OR ENTRYPTR IS INVALID        */ 00862000
*                 END;              /* ENTRY POINTER POINTS TO A PART   00863000
*                                      ENTRY                         */ 00864000
*               ELSE                /* ENTRY POINTER POINTS TO A SART   00865000
*                                      ENTRY                         */ 00866000
*                 DO;                                              0271 00867000
