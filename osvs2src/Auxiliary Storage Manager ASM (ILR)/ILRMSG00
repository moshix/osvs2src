@EL00003 DS    0H                                                  0335 01177000
@EF00003 DS    0H                                                  0335 01178000
@ER00003 BR    @14                                                 0335 01179000
         EJECT                                                          01180000
*                                                                  0336 01181000
*/********************************************************************/ 01182000
*/*                                                                  */ 01183000
*/*   ILRMSG01 - RECOVERY FOR TERMSYS                                */ 01184000
*/*                                                                  */ 01185000
*/*   INPUT =   R0 - PTR TO FRR WORK AREA                            */ 01186000
*/*             R5 - PTR TO SDWA                                     */ 01187000
*/*                                                                  */ 01188000
*/*   PROCESS = OBTAIN THE ADDRESS OF THE WAIT STATE PSW AND THEN    */ 01189000
*/*             LOAD THAT PSW.                                       */ 01190000
*/*                                                                  */ 01191000
*/*   OUTPUT =  NONE                                                 */ 01192000
*/*                                                                  */ 01193000
*/********************************************************************/ 01194000
*                                                                  0336 01195000
*ILRMSG01:                                                         0336 01196000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0336 01197000
ILRMSG01 DS    0H                                                  0337 01198000
*   RFY                                                            0337 01199000
*    (SDWAPTR) RSTD;                                               0337 01200000
*   RFY                                                            0338 01201000
*     FRRPARM BASED(SDWAPARM);                                     0338 01202000
*   LPSW(WAITPSW);                  /* ENTER WAIT STATE              */ 01203000
         L     @12,SDWAPARM(,SDWAPTR)                              0339 01204000
         L     @12,FRRWPSW@(,@12)                                  0339 01205000
         LPSW  WAITPSW(@12)                                        0339 01206000
*   RFY                                                            0340 01207000
*    (SDWAPTR) UNRSTD;                                             0340 01208000
*   END ILRMSG01;                                                  0341 01209000
@EL00004 DS    0H                                                  0341 01210000
@EF00004 DS    0H                                                  0341 01211000
@ER00004 BR    @14                                                 0341 01212000
*                                                                  0342 01213000
*/********************************************************************/ 01214000
*/*                                                                  */ 01215000
*/*   CLEARWTQ - MARK AIA'S, FREE IOE'S AND SCHEDULE ILRPTM          */ 01216000
*/*                                                                  */ 01217000
*/*   INPUT =    R3 - PTR TO ASMVT                                   */ 01218000
*/*              R5 - PART ENTRY POINTER                             */ 01219000
*/*                                                                  */ 01220000
*/*   PROCESS = OBTAIN THE ASM CLASS LOCK FOR THE PART WRITE QUEUES. */ 01221000
*/*             LOOP THRU THE IOE'S ON THE WRITE QUEUE ASSOCIATED    */ 01222000
*/*             WITH THE PART ENTRY PROVIDED. APPROPRIATELY MARK     */ 01223000
*/*             EACH AIA ASSOCIATED WITH THE PART ENTRY PROVIDED. IF */ 01224000
*/*             THE AIA COUNT OF OUTSTANDING WRITE OPERATIONS FOR A  */ 01225000
*/*             DUPLEXED WRITE OPERATION IS EQUAL TO TWO THEN        */ 01226000
*/*             DECREMENT THAT COUNT BY ONE. OTHERWISE, USE COMPARE  */ 01227000
*/*             AND SWAP LOGIC TO PLACE THAT AIA ON THE PART AIA     */ 01228000
*/*             ERROR QUEUE. IN EITHER CASE, RETURN EACH IOE TO THE  */ 01229000
*/*             IOE POOL, RELEASE THE ASM CLASS LOCK, AND THEN       */ 01230000
*/*             SCHEDULE ILRPTM IF IT IS NOT ALREADY SCHEDULED.      */ 01231000
*/*                                                                  */ 01232000
*/*   OUTPUT =  PART AIA ERROR QUEUE CONTAINS AIA'S APPROPRIATELY    */ 01233000
*/*             MARKED.                                              */ 01234000
*/*             THE IOE'S ON THE WRITE QUEUE ARE RETURNED TO THE     */ 01235000
*/*             IOE POOL.                                            */ 01236000
*/*                                                                  */ 01237000
*/*   NOTES  =  THIS ROUTINE IS ONLY CALLED IF THE SYSTEM IS         */ 01238000
*/*             DUPLEXING AND PLPA OR COMMON GOES BAD.               */ 01239000
*/*             THIS ROUTINE IS ONLY FUNCTIONAL FOR THE FOLLOWING    */ 01240000
*/*             THREE CASES: (1) PLPA GOES BAD, (2) COMMON GOES BAD, */ 01241000
*/*             OR DUPLEX GOES BAD.                                  */ 01242000
*/*                                                                  */ 01243000
*/********************************************************************/ 01244000
*                                                                  0342 01245000
*CLEARWTQ:                                                         0342 01246000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0342 01247000
CLEARWTQ DS    0H                                                  0343 01248000
         EJECT                                                          01249000
*                                                                  0343 01250000
*/********************************************************************/ 01251000
*/********************************************************************/ 01252000
*/*   OBTAIN ASM CLASS LOCK FOR THE PART WRITE QUEUES                */ 01253000
*/********************************************************************/ 01254000
*/********************************************************************/ 01255000
*                                                                  0343 01256000
*   RFY                                                            0343 01257000
*    (R11,                                                         0343 01258000
*     R12,                                                         0343 01259000
*     R14) RSTD;                                                   0343 01260000
*   STM(R13,R14,SAVREGS);           /* SAVE ADDRESS OF CALLERS SAVE     01261000
*                                      AREA AND THE RETURN ADDRESS   */ 01262000
         STM   R13,R14,SAVREGS+848(ASMVTPTR)                       0344 01263000
*   R11=ADDR(PARTLOCK);             /* OBTAIN LOCK WORD ADDRESS      */ 01264000
         L     R11,PARTPTR(,ASMVTPTR)                              0345 01265000
         LA    R11,PARTLOCK(,R11)                                  0345 01266000
*GTASMCLS:                                                         0346 01267000
*   GEN REFS(FLC,PSALITA) SETS(R11,R12,R13,R14);                   0346 01268000
GTASMCLS DS    0H                                                  0346 01269000
         SETLOCK OBTAIN,TYPE=ASM,ADDR=(11),MODE=UNCOND,               **01270000
               RELATED=(PART,ILRMSG00(RLASMCLS))                        01271000
*   RFY                                                            0347 01272000
*    (R11,                                                         0347 01273000
*     R12,                                                         0347 01274000
*     R14) UNRSTD;                                                 0347 01275000
         EJECT                                                          01276000
*                                                                  0348 01277000
*/********************************************************************/ 01278000
*/********************************************************************/ 01279000
*/*   APPROPRIATELY MARK EACH AIA                                    */ 01280000
*/********************************************************************/ 01281000
*/********************************************************************/ 01282000
*                                                                  0348 01283000
*   /*****************************************************************/ 01284000
*   /*                                                               */ 01285000
*   /* CLEAR THE IOE WRITE QUEUE THAT IS ASSOCIATED WITH THE BAD PART*/ 01286000
*   /* ENTRY.                                                        */ 01287000
*   /*                                                               */ 01288000
*   /*****************************************************************/ 01289000
*                                                                  0348 01290000
*   IOEPTR=PAREWTQE->WRITEQF;       /* REMOVE IOE'S FROM THE       0348 01291000
*                                      APPROPRIATE WRITE QUEUE       */ 01292000
         L     @12,PAREWTQE(,ENTRYPTR)                             0348 01293000
         L     IOEPTR,WRITEQF(,@12)                                0348 01294000
*   PAREWTQE->WRITEQ=''B;           /* CLEAR THE WRITE QUEUE         */ 01295000
*                                                                  0349 01296000
         XC    WRITEQ(8,@12),WRITEQ(@12)                           0349 01297000
*   /*****************************************************************/ 01298000
*   /*                                                               */ 01299000
*   /* FOR EACH IOE ON THE WRITE QUEUE: (1) MARK THE CORRESPONDING   */ 01300000
*   /* AIA APPROPRIATELY, (2) IF NECESSARY, CHAIN THE AIA ONTO THE   */ 01301000
*   /* PART AIA ERROR QUEUE, AND (3) RETURN THE IOE TO THE IOE POOL. */ 01302000
*   /*                                                               */ 01303000
*   /*****************************************************************/ 01304000
*                                                                  0350 01305000
*   DO WHILE(IOEPTR^=ZERO);                                        0350 01306000
         B     @DE00350                                            0350 01307000
@DL00350 DS    0H                                                  0351 01308000
*     AIAPTR=IOEAIA;                /* OBTAIN AIA POINTER FROM THE 0351 01309000
*                                      IOE                           */ 01310000
*                                                                  0351 01311000
         L     AIAPTR,IOEAIA(,IOEPTR)                              0351 01312000
*     /***************************************************************/ 01313000
*     /*                                                             */ 01314000
*     /* IF THE IOE IS ON THE DUPLEX WIRTE QUEUE (I.E., A SECONDARY  */ 01315000
*     /* WRITE), TURN ON THE SECONDARY ERROR FLAG IN THE AIA.        */ 01316000
*     /* OTHERWISE, THE AIA IS ASSUMED TO BE A PRIMARY WRITE SO ZERO */ 01317000
*     /* OUT THE LSID FIELD IN THE AIA.                              */ 01318000
*     /*                                                             */ 01319000
*     /***************************************************************/ 01320000
*                                                                  0352 01321000
*     IF PAREDPLX=ON THEN                                          0352 01322000
         TM    PAREDPLX(ENTRYPTR),B'00100000'                      0352 01323000
         BNO   @RF00352                                            0352 01324000
*       AIASECER=ON;                /* INDICATE TO ILRPAGCM WHAT TO     01325000
*                                      DO WITH THE AIA               */ 01326000
         OI    AIASECER(AIAPTR),B'00000100'                        0353 01327000
*     ELSE                                                         0354 01328000
*       AIALSID=ZERO;               /* INDICATE TO ILRPAGCM WHAT TO     01329000
*                                      DO WITH THE AIA               */ 01330000
*                                                                  0354 01331000
         B     @RC00352                                            0354 01332000
@RF00352 SLR   @12,@12                                             0354 01333000
         ST    @12,AIALSID(,AIAPTR)                                0354 01334000
*     /***************************************************************/ 01335000
*     /*                                                             */ 01336000
*     /* IF THERE ARE TWO OUTSTANDING WRITE OPERATIONS FOR THE AIA   */ 01337000
*     /* ASSOCIATED WITH THE IOE TO BE FREED, THEN DECREMENT THE     */ 01338000
*     /* COUNT OF OUTSTANDING WRITE OPERATIONS BY ONE AND THEN FREE  */ 01339000
*     /* THAT IOE. OTHERWISE, CHAIN THE AIA TO THE PART AIA ERROR    */ 01340000
*     /* QUEUE AND THEN FREE THAT IOE.                               */ 01341000
*     /*                                                             */ 01342000
*     /***************************************************************/ 01343000
*                                                                  0355 01344000
*     IF AIADPXCT=TWO THEN                                         0355 01345000
@RC00352 L     @12,AIADPXCT(,AIAPTR)                               0355 01346000
         C     @12,@CF00081                                        0355 01347000
         BNE   @RF00355                                            0355 01348000
*       AIADPXCT=AIADPXCT-ONE;      /* DECREMENT COUNT OF OUTSTANDING   01349000
*                                      WRITE OPERATIONS FOR A      0356 01350000
*                                      DUPLEXED WIRTE OPERATION      */ 01351000
         BCTR  @12,0                                               0356 01352000
         ST    @12,AIADPXCT(,AIAPTR)                               0356 01353000
*     ELSE                                                         0357 01354000
*       DO;                                                        0357 01355000
         B     @RC00355                                            0357 01356000
@RF00355 DS    0H                                                  0358 01357000
*         RFY                                                      0358 01358000
*          (R0,                                                    0358 01359000
*           R1) RSTD;                                              0358 01360000
*         R1=AIAPTR;                /* OBTAIN POINTER OF THE AIA TO     01361000
*                                      BE PLACED ON THE PART AIA   0359 01362000
*                                      ERROR QUEUE                   */ 01363000
         LR    R1,AIAPTR                                           0359 01364000
*         IOEAIA=ZERO;              /* ZERO AIA POINTER IN THE IOE   */ 01365000
         SLR   @12,@12                                             0360 01366000
         ST    @12,IOEAIA(,IOEPTR)                                 0360 01367000
*         R0=PARTAIAE;              /* OBTAIN POINTER TO THE PART AIA   01368000
*                                      ERROR QUEUE                   */ 01369000
         L     @12,PARTPTR(,ASMVTPTR)                              0361 01370000
         L     R0,PARTAIAE(,@12)                                   0361 01371000
*TRYAGAIN:                                                         0362 01372000
*         R1->AIANXAIA=R0;          /* CHAIN EXISTING AIA'S TO NEW 0362 01373000
*                                      AIA                           */ 01374000
TRYAGAIN ST    R0,AIANXAIA(,R1)                                    0362 01375000
*         CS(R0,R1,PARTAIAE);       /* UPDATE THE AIA ERROR QUEUE  0363 01376000
*                                      WITH NEW AIA                  */ 01377000
         L     @12,PARTPTR(,ASMVTPTR)                              0363 01378000
         CS    R0,@01,PARTAIAE(@12)                                0363 01379000
*         BC(NE,TRYAGAIN);          /* IF UNSUCCESSFUL, TRY AGAIN    */ 01380000
         BC    7,TRYAGAIN                                          0364 01381000
*         RFY                                                      0365 01382000
*          (R0) UNRSTD;                                            0365 01383000
*       END;                                                       0366 01384000
         EJECT                                                          01385000
*                                                                  0367 01386000
*/********************************************************************/ 01387000
*/********************************************************************/ 01388000
*/*   RETURN THE IOE TO ITS POOL                                     */ 01389000
*/********************************************************************/ 01390000
*/********************************************************************/ 01391000
*                                                                  0367 01392000
*     R1=IOEPTR;                    /* SET UP TO FREE THIS IOE       */ 01393000
@RC00355 LR    R1,IOEPTR                                           0367 01394000
*     IOEPTR=IOENXT;                /* OBTAIN POINTER TO NEXT IOE    */ 01395000
*                                                                  0368 01396000
         L     IOEPTR,IOENXT(,IOEPTR)                              0368 01397000
*     /***************************************************************/ 01398000
*     /*                                                             */ 01399000
*     /* BEGIN ILRGMA MACRO EXPANSION                                */ 01400000
*     /*                                                             */ 01401000
*     /***************************************************************/ 01402000
*                                                                  0369 01403000
*     DO;                           /* ILRGMA FREE                 0369 01404000
*                                      CELL(IOE)WORKREGS(R11,R12)    */ 01405000
*       DECLARE                     /* ASMVT VECTOR TO ILRPEX        */ 01406000
*         ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                     0370 01407000
*       DECLARE                     /* MAP FOR CELL CONTROL WORDS    */ 01408000
*         1 GMACELL BASED BDY(WORD),                               0371 01409000
*          2 GMANEXT PTR(31);       /* PTR NEXT FREE CELL            */ 01410000
*       RESPECIFY                                                  0372 01411000
*        (R0,                                                      0372 01412000
*         R1,                                                      0372 01413000
*         R11,                                                     0372 01414000
*         R12,                                                     0372 01415000
*         R14,                                                     0372 01416000
*         R15) RESTRICTED;                                         0372 01417000
*       RESPECIFY                                                  0373 01418000
*         ASMPOOLS BASED(R11);                                     0373 01419000
*       RESPECIFY                                                  0374 01420000
*         GMACELL BASED(R1);                                       0374 01421000
*       R11=ADDR(ASMIOEPC);                                        0375 01422000
         LA    R11,ASMIOEPC(,ASMVTPTR)                             0375 01423000
*       R14=ASMCPAVL;                                              0376 01424000
         L     R14,ASMCPAVL(,R11)                                  0376 01425000
*GMA00132:                                                         0377 01426000
*       GMANEXT=R14;                /* PUT ADDR CURRENT FIRST IN   0377 01427000
*                                      INPUT CELL                    */ 01428000
GMA00132 ST    R14,GMANEXT(,R1)                                    0377 01429000
*       CS(R14,R1,ASMCPAVL);        /* ADD INPUT CELL TO POOL        */ 01430000
         CS    R14,@01,ASMCPAVL(R11)                               0378 01431000
*       BC(7,GMA00132);                                            0379 01432000
         BC    7,GMA00132                                          0379 01433000
*       RESPECIFY                                                  0380 01434000
*        (R0,                                                      0380 01435000
*         R1,                                                      0380 01436000
*         R11,                                                     0380 01437000
*         R12,                                                     0380 01438000
*         R14,                                                     0380 01439000
*         R15) UNRESTRICTED;                                       0380 01440000
*     END;                          /* ILRGMA FREE                 0381 01441000
*                                      CELL(IOE)WORKREGS(R11,R12)    */ 01442000
*                                                                  0381 01443000
*     /***************************************************************/ 01444000
*     /*                                                             */ 01445000
*     /* END ILRGMA MACRO EXPANSION                                  */ 01446000
*     /*                                                             */ 01447000
*     /***************************************************************/ 01448000
*                                                                  0382 01449000
*     RFY                                                          0382 01450000
*      (R1) UNRSTD;                                                0382 01451000
*   END;                            /* ANY MORE IOE'S                */ 01452000
@DE00350 LTR   IOEPTR,IOEPTR                                       0383 01453000
         BNZ   @DL00350                                            0383 01454000
         EJECT                                                          01455000
*                                                                  0384 01456000
*/********************************************************************/ 01457000
*/********************************************************************/ 01458000
*/*   RELEASE ASM CLASS LOCK FOR THE PART WRITE QUEUES               */ 01459000
*/********************************************************************/ 01460000
*/********************************************************************/ 01461000
*                                                                  0384 01462000
*   RFY                                                            0384 01463000
*    (R11,                                                         0384 01464000
*     R12,                                                         0384 01465000
*     R14) RSTD;                                                   0384 01466000
