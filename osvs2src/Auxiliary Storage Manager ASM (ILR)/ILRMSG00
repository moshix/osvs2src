@RF00262 DS    0H                                                  0272 00869000
*                   MSGNAME=BLANKS; /* SET DATA SET NAME TO BLANKS   */ 00870000
         LA    @12,4                                               0272 00871000
         ALR   @12,BUFPTR                                          0272 00872000
         MVC   MSGNAME(6,@12),@CC02370                             0272 00873000
*                   MSGTYPE=SWAP;   /* CHANGE DATA SET TYPE FROM PAGE   00874000
*                                      TO SWAP                       */ 00875000
         MVC   MSGTYPE(4,@12),@CC02391                             0273 00876000
*                 END;              /* ENTRY POINTER POINTS TO A SART   00877000
*                                      ENTRY                         */ 00878000
*             END;                  /* MESSAGE NUMBER IS 9           */ 00879000
         EJECT                                                          00880000
*                                                                  0276 00881000
*/********************************************************************/ 00882000
*/********************************************************************/ 00883000
*/*   MESSAGE 10 ?                                                   */ 00884000
*/********************************************************************/ 00885000
*/********************************************************************/ 00886000
*                                                                  0276 00887000
*           ELSE                    /* MESSAGE NUMBER IS NOT 9       */ 00888000
*                                                                  0276 00889000
*             /*******************************************************/ 00890000
*             /*                                                     */ 00891000
*             /* IF THE MESSAGE NUMBER IS 10, THEN DETERMINE IF THE  */ 00892000
*             /* DATA SET IS PLPA OR COMMON OR IF THE DATA SET IS BAD*/ 00893000
*             /* OR FULL AND CHANGE THE BASE MESSAGE IF NECESSARY    */ 00894000
*             /*                                                     */ 00895000
*             /*******************************************************/ 00896000
*                                                                  0276 00897000
*             IF MSGNUM=TEN THEN                                   0276 00898000
         B     @RC00259                                            0276 00899000
@RF00259 C     MSGNUM,@CF00032                                     0276 00900000
         BNE   @RF00276                                            0276 00901000
*               DO;                                                0277 00902000
*                 IF PAREPLPA=OFF   /* DEFAULT NAME IS PLPA          */ 00903000
*                   THEN                                           0278 00904000
         TM    PAREPLPA(ENTRYPTR),B'10000000'                      0278 00905000
         BNZ   @RF00278                                            0278 00906000
*                   IF PARECOMM=ON THEN                            0279 00907000
         TM    PARECOMM(ENTRYPTR),B'01000000'                      0279 00908000
         BNO   @RF00279                                            0279 00909000
*                     MSGNAME=COMMONDS;/* CHANGE DATA SET NAME FROM     00910000
*                                      PLPA TO COMMON                */ 00911000
         MVC   MSGNAME+4(6,BUFPTR),@CC02372                        0280 00912000
*                   ELSE                                           0281 00913000
*                     RETURN;       /* RETURN TO CALLER BECAUSE    0281 00914000
*                                      ENTRYPTR IS INVALID           */ 00915000
*                 IF FULLFLAG=OFF THEN                             0282 00916000
@RF00278 C     FULLFLAG,@CF02383                                   0282 00917000
         BNE   @RF00282                                            0282 00918000
*                   MSGDISP=BAD;    /* CHANGE DATA SET DISPOSITION 0283 00919000
*                                      FROM FULL TO BAD              */ 00920000
         MVC   MSGDISP+4(4,BUFPTR),@CC02368                        0283 00921000
*               END;                /* MESSAGE NUMBER IS 10          */ 00922000
*             ELSE                  /* MESSAGE NUMBER IS NOT 10      */ 00923000
*               RETURN;             /* RETURN TO CALLER BECAUSE    0285 00924000
*                                      MSGNUM IS INVALID             */ 00925000
*         END;                      /* MESSAGE NUMBER IS 9 OR 10     */ 00926000
@RC00276 DS    0H                                                  0286 00927000
@RC00259 DS    0H                                                  0287 00928000
*     END;                          /* MESSAGE NUMBER IS 7, 9, OR 10 */ 00929000
@RC00254 DS    0H                                                  0288 00930000
         EJECT                                                          00931000
*                                                                  0288 00932000
*/********************************************************************/ 00933000
*/********************************************************************/ 00934000
*/*   WRITE THE MESSAGE TO THE OPERATOR AND RETURN                   */ 00935000
*/********************************************************************/ 00936000
*/********************************************************************/ 00937000
*                                                                  0288 00938000
*   /*****************************************************************/ 00939000
*   /*                                                               */ 00940000
*   /* USE THE ASYNCHRONOUS RECORDING FACILITY TO WRITE THE MESSAGE  */ 00941000
*   /* TO THE OPERATOR AND THEN RETURN TO THE CALLER                 */ 00942000
*   /*                                                               */ 00943000
*   /*****************************************************************/ 00944000
*                                                                  0288 00945000
*   RFY                                                            0288 00946000
*    (R11,                                                         0288 00947000
*     R12,                                                         0288 00948000
*     R14) RSTD;                                                   0288 00949000
@RC00246 DS    0H                                                  0289 00950000
*   R11=R13;                        /* SAVE CALLER'S SAVE AREA     0289 00951000
*                                      ADDRESS                       */ 00952000
         LR    R11,R13                                             0289 00953000
*   R12=R14;                        /* SAVE RETURN ADDRESS           */ 00954000
         LR    R12,R14                                             0290 00955000
*   R13=ADDR(ASMWKSA7);             /* PROVIDE A SAVE AREA FOR RECORD   00956000
*                                      FACILITY                      */ 00957000
         LA    R13,ASMWKSA7(,ASMVTPTR)                             0291 00958000
*   GEN REFS(BUFPTR,CVTMAP,CVTRECRQ,FLCCVT);                       0292 00959000
         RECORD TYPE=WTO,RCVRY=SETFRR,DATAADR=(BUFPTR),LENGTH=80,     XX00960000
               BUFFER=OPT                                               00961000
*   R13=R11;                        /* RESTORE SAVE AREA ADDRESS     */ 00962000
         LR    R13,R11                                             0293 00963000
*   R14=R12;                        /* RESTORE RETURN ADDRESS        */ 00964000
         LR    R14,R12                                             0294 00965000
*   RFY                                                            0295 00966000
*    (R11,                                                         0295 00967000
*     R12,                                                         0295 00968000
*     R14) UNRSTD;                                                 0295 00969000
*   RETURN;                         /* RETURN TO CALLER              */ 00970000
@EL00002 DS    0H                                                  0296 00971000
@EF00002 DS    0H                                                  0296 00972000
@ER00002 BR    @14                                                 0296 00973000
*   END WRITEMSG;                                                  0297 00974000
         B     @EL00002                                            0297 00975000
         EJECT                                                          00976000
*                                                                  0298 00977000
*/********************************************************************/ 00978000
*/*                                                                  */ 00979000
*/*   TERMSYS - TERMINATE THE SYSTEM                                 */ 00980000
*/*                                                                  */ 00981000
*/*   INPUT =    R3 - PTR TO ASMVT                                   */ 00982000
*/*              R5 - ENTRY POINTER                                  */ 00983000
*/*                                                                  */ 00984000
*/*   PROCESS = ISSUE A SETFRR TO ESTABLISH RECOVERY FOR THIS        */ 00985000
*/*             TERMINATION ROUTINE IN ORDER TO GIVE ASM A SECOND    */ 00986000
*/*             CHANCE TO TERMINATE THE SYSTEM. IF THE DSBD FLAG     */ 00987000
*/*             OR THE NUSE FLAG OF THE PARTE IS ON, THEN SET THE    */ 00988000
*/*             WAIT STATE CODE TO X'02E'. OTHERWISE, SET IT TO      */ 00989000
*/*             X'03C'. PLACE WAIT STATE PSW INTO THE FRRPARM AREA.  */ 00990000
*/*             MOVE BASE MESSAGE INTO THE TERMINATION BUFFER AND    */ 00991000
*/*             THEN FILL IN THE VARIABLE INFORMATION. INITIALIZE    */ 00992000
*/*             LOG REC BUFFER. PLACE REAL ADDRESS OF THE UPDATED    */ 00993000
*/*             WTO MESSAGE AND OF THE LOG REC BUFFER INTO THE       */ 00994000
*/*             SYSTEM TERMINATION PARAMETER LIST. PLACE THE REAL    */ 00995000
*/*             ADDRESS OF THE PARAMETER LIST INTO REG 1 AND THEN    */ 00996000
*/*             ENTER IGFPTERM VIA A LPSW IN ORDER TO TERMINATE      */ 00997000
*/*             THE SYSTEM. A LPSW IS USED IN ORDER TO TURN OFF THE  */ 00998000
*/*             DAT BIT.                                             */ 00999000
*/*                                                                  */ 01000000
*/*   OUTPUT =  NONE                                                 */ 01001000
*/*                                                                  */ 01002000
*/********************************************************************/ 01003000
*                                                                  0298 01004000
*TERMSYS:                                                          0298 01005000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0298 01006000
TERMSYS  DS    0H                                                  0299 01007000
         EJECT                                                          01008000
*                                                                  0299 01009000
*/********************************************************************/ 01010000
*/********************************************************************/ 01011000
*/*   ESTABLISH RECOVERY                                             */ 01012000
*/********************************************************************/ 01013000
*/********************************************************************/ 01014000
*                                                                  0299 01015000
*   /*****************************************************************/ 01016000
*   /*                                                               */ 01017000
*   /* ESTABLISH RECOVERY FOR THIS ROUTINE IN ORDER TO GIVE ASM A    */ 01018000
*   /* SECOND CHANCE TO TERMINATE THE SYSTEM                         */ 01019000
*   /*                                                               */ 01020000
*   /*****************************************************************/ 01021000
*                                                                  0299 01022000
*   RFY                                                            0299 01023000
*    (R11,                                                         0299 01024000
*     R12,                                                         0299 01025000
*     FRRADDR,                                                     0299 01026000
*     FRRPARM@) RSTD;                                              0299 01027000
*   FRRADDR=ADDR(ILRMSG01);                                        0300 01028000
         LA    FRRADDR,ILRMSG01                                    0300 01029000
*   GEN REFS(FRRADDR,FRRSCURR,FRRSELEN,FRRSENTR,FRRSFRRA,FRRSLAST, 0301 01030000
*       FRRSPARM,PSACSTK,PSALSFCC) SETS(R11,R12,FRRPARM@);         0301 01031000
          SETFRR A,FRRAD=(FRRADDR),PARMAD=(FRRPARM@),WRKREGS=(R11,R12)  01032000
*   RFY                                                            0302 01033000
*    (R11,                                                         0302 01034000
*     R12,                                                         0302 01035000
*     FRRADDR) UNRSTD;                                             0302 01036000
*                                                                  0302 01037000
*   /*****************************************************************/ 01038000
*   /*                                                               */ 01039000
*   /* PLACE THE ADDRESS OF THE APPROPRIATE WAIT STATE PSW INTO THE  */ 01040000
*   /* FRRPARM AREA.                                                 */ 01041000
*   /*                                                               */ 01042000
*   /*****************************************************************/ 01043000
*                                                                  0303 01044000
*   IF PARENUSE=ON|PAREDSBD=ON THEN                                0303 01045000
         TM    PARENUSE(ENTRYPTR),B'11000000'                      0303 01046000
         BZ    @RF00303                                            0303 01047000
*     DO;                                                          0304 01048000
*       FRRWPSW@=ADDR(WPSW02E);     /* PLACE THE ADDRESS OF THE PSW     01049000
*                                      WITH WAIT STATE 02E INTO THE     01050000
*                                      FRRPARM AREA                  */ 01051000
         LA    @12,WPSW02E                                         0305 01052000
         ST    @12,FRRWPSW@(,FRRPARM@)                             0305 01053000
*       WAITCODE=WCODE02E;                                         0306 01054000
         L     WAITCODE,WCODE02E                                   0306 01055000
*     END;                                                         0307 01056000
*   ELSE                                                           0308 01057000
*     DO;                                                          0308 01058000
         B     @RC00303                                            0308 01059000
@RF00303 DS    0H                                                  0309 01060000
*       FRRWPSW@=ADDR(WPSW03C);     /* PLACE THE ADDRESS OF THE PSW     01061000
*                                      WITH WAIT STATE 03C INTO THE     01062000
*                                      FRRPARM AREA                  */ 01063000
         LA    @12,WPSW03C                                         0309 01064000
         ST    @12,FRRWPSW@(,FRRPARM@)                             0309 01065000
*       WAITCODE=WCODE03C;                                         0310 01066000
         L     WAITCODE,WCODE03C                                   0310 01067000
*     END;                                                         0311 01068000
*   RFY                                                            0312 01069000
*    (FRRPARM@) UNRSTD;                                            0312 01070000
@RC00303 DS    0H                                                  0313 01071000
         EJECT                                                          01072000
*                                                                  0313 01073000
*/********************************************************************/ 01074000
*/********************************************************************/ 01075000
*/*   BUILD TERMINATION MESSAGE                                      */ 01076000
*/********************************************************************/ 01077000
*/********************************************************************/ 01078000
*                                                                  0313 01079000
*   BUFPTR=ADDR(TERMMSG);                                          0313 01080000
         L     @12,ASMMSGBF(,ASMVTPTR)                             0313 01081000
         LA    BUFPTR,TERMMSG(,@12)                                0313 01082000
*   TERMMSG=WTO08W;                 /* MOVE BASE TERMINATION MESSAGE    01083000
*                                      INTO TERMINATION BUFFER       */ 01084000
         MVC   TERMMSG(80,@12),WTO08W                              0314 01085000
*   MSGVOLID=UCBVOLI;               /* PLACE VOLUME ID INTO BASE   0315 01086000
*                                      MESSAGE                       */ 01087000
         LA    @12,4                                               0315 01088000
         ALR   @12,BUFPTR                                          0315 01089000
         L     @11,UCBPTR(,ENTRYPTR)                               0315 01090000
         MVC   MSGVOLID(6,@12),UCBVOLI(@11)                        0315 01091000
*   IF PAREPLPA=OFF                 /* DEFAULT NAME IS PLPA          */ 01092000
*     THEN                                                         0316 01093000
         TM    PAREPLPA(ENTRYPTR),B'10000000'                      0316 01094000
         BNZ   @RF00316                                            0316 01095000
*     IF PARECOMM=ON THEN                                          0317 01096000
         TM    PARECOMM(ENTRYPTR),B'01000000'                      0317 01097000
         BNO   @RF00317                                            0317 01098000
*       MSGNAME=COMMONDS;           /* CHANGE DATA SET NAME FROM PLPA   01099000
*                                      TO COMMON                     */ 01100000
         MVC   MSGNAME(6,@12),@CC02372                             0318 01101000
*     ELSE                                                         0319 01102000
*       IF PAREDPLX=ON THEN                                        0319 01103000
         B     @RC00317                                            0319 01104000
@RF00317 TM    PAREDPLX(ENTRYPTR),B'00100000'                      0319 01105000
         BNO   @RF00319                                            0319 01106000
*         MSGNAME=DUPLEXDS;         /* CHANGE DATA SET NAME FROM PLPA   01107000
*                                      TO DUPLEX                     */ 01108000
         MVC   MSGNAME+4(6,BUFPTR),@CC02374                        0320 01109000
*       ELSE                                                       0321 01110000
*         MSGNAME=LOCALDS;          /* CHANGE DATA SET NAME FROM PLPA   01111000
*                                      TO LOCAL                      */ 01112000
         B     @RC00319                                            0321 01113000
@RF00319 MVC   MSGNAME+4(6,BUFPTR),@CC02378                        0321 01114000
         EJECT                                                          01115000
*                                                                  0322 01116000
*/********************************************************************/ 01117000
*/********************************************************************/ 01118000
*/*   BUILD SYSTEM TERMINATION LOG REC BUFFER                        */ 01119000
*/********************************************************************/ 01120000
*/********************************************************************/ 01121000
*                                                                  0322 01122000
*   LRBPTR=ADDR(SYSTLRB);           /* INITIALIZE LRB POINTER        */ 01123000
@RC00319 DS    0H                                                  0322 01124000
@RC00317 DS    0H                                                  0322 01125000
@RF00316 L     @12,ASMMSGBF(,ASMVTPTR)                             0322 01126000
         LA    LRBPTR,SYSTLRB(,@12)                                0322 01127000
*   LRB=''B;                        /* CLEAR LRB                     */ 01128000
         XC    LRB(24,LRBPTR),LRB(LRBPTR)                          0323 01129000
*   LRBHTYPE=LRBHTER;               /* INIDICATE LRB IS A SYSTEM   0324 01130000
*                                      TERMINATION RECORD            */ 01131000
         MVI   LRBHTYPE(LRBPTR),X'81'                              0324 01132000
*   LRBTLNH=LENGTH(SYSTLRB);        /* SET LENGTH FIELD              */ 01133000
         MVC   LRBTLNH(4,LRBPTR),@CF00083                          0325 01134000
*   LRBTWSC=WAITCODE;               /* FILL IN WAIT STATE CODE       */ 01135000
         ST    WAITCODE,LRBTWSC(,LRBPTR)                           0326 01136000
         EJECT                                                          01137000
*                                                                  0327 01138000
*/********************************************************************/ 01139000
*/********************************************************************/ 01140000
*/*   BUILD SYSTEM TERMINATION PARAMETER LIST -- ENTER IGFPTERM      */ 01141000
*/********************************************************************/ 01142000
*/********************************************************************/ 01143000
*                                                                  0327 01144000
*   RFY                                                            0327 01145000
*    (R0,                                                          0327 01146000
*     R1,                                                          0327 01147000
*     R15) RSTD;                                                   0327 01148000
*   R15=ASMMSGBF;                                                  0328 01149000
         LR    R15,@12                                             0328 01150000
*   GEN REFS(R15,TERMMSG,SYSTLRB) SETS(R0,R1);                     0329 01151000
         LRA   R0,TERMMSG(,R15) /* OBTAIN REAL ADDRESS OF TERMINATION XX01152000
                                   MESSAGE                          */  01153000
         LRA   R1,SYSTLRB(,R15) /* OBTAIN REAL ADDRESS OF TERMINATION XX01154000
                                   LRB                              */  01155000
*   STWTO@=R0;                      /* PLACE REAL ADDRESS OF       0330 01156000
*                                      TERMINATION MESSAGE INTO    0330 01157000
*                                      SYSTEM TERMINATION PARAMETER     01158000
*                                      LIST                          */ 01159000
         L     @14,ASMMSGBF(,ASMVTPTR)                             0330 01160000
         ST    R0,STWTO@(,@14)                                     0330 01161000
*   STLRB@=R1;                      /* PLACE REAL ADDRESS OF       0331 01162000
*                                      TERMINATION LRB INTO SYSTEM 0331 01163000
*                                      TERMINATION PARAMETER LIST    */ 01164000
         ST    R1,STLRB@(,@14)                                     0331 01165000
*   GEN REFS(R15,SYSTERPL) SETS(R1);                               0332 01166000
         LRA   R1,SYSTERPL(,R15) /* PLACE REAL REAL ADDRESS OF SYSTEM XX01167000
                                    TERMINATION PARAMETER LIST INTO   XX01168000
                                    REG 1                           */  01169000
*   LPSW(TERMPSW);                  /* ENTER IGFPTERM                */ 01170000
