         TITLE 'ILRMSG00 - ASM''S MESSAGE MODULE                       *00001000
                         '                                              00002000
ILRMSG00 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ILRMSG00  76.087'                                 0001 00009000
ILRMSGSP DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00182-ILRMSGSP)                                   00013000
         ENTRY ILRMSGSP                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @10,0                                               0001 00017000
@PSTART  DS    0H                                                  0001 00018000
         USING @PSTART,@10                                         0001 00019000
         AH    @15,4(,@15)                                         0001 00020000
         BR    @15                                                 0001 00021000
@EP00001 DS    0H                                                  0002 00022000
         SLR   R15,R15                                                  00023000
         ST    R15,16(R13)       /* SET RETURN CODE TO ZERO         */  00024000
*   MSGNUM=PARMNUM;                 /* OBTAIN MESSAGE NUMBER FROM  0113 00025000
*                                      INPUT PARAMETER LIST          */ 00026000
         LH    MSGNUM,PARMNUM(,R1)                                 0113 00027000
         N     MSGNUM,@CF02608                                     0113 00028000
*   TYPE=PARMTYPE;                  /* OBTAIN ENTRY TYPE FROM INPUT     00029000
*                                      PARAMETER LIST                */ 00030000
         LH    TYPE,PARMTYPE(,R1)                                  0114 00031000
         N     TYPE,@CF02608                                       0114 00032000
*   ENTRYPTR=PARMENTP;              /* OBTAIN ENTRY POINTER FROM   0115 00033000
*                                      INPUT PARAMETER LIST          */ 00034000
         L     ENTRYPTR,PARMENTP(,R1)                              0115 00035000
*   RFY                                                            0116 00036000
*    (R1) UNRSTD;                                                  0116 00037000
*                                                                  0116 00038000
*   /*****************************************************************/ 00039000
*   /*                                                               */ 00040000
*   /* OBTAIN A POINTER TO THE ASMVT. INITIALIZE FULL FLAG           */ 00041000
*   /*                                                               */ 00042000
*   /*****************************************************************/ 00043000
*                                                                  0117 00044000
*   ASMVTPTR=CVTASMVT;              /* OBTAIN POINTER TO ASMVT       */ 00045000
         L     @12,CVTPTR                                          0117 00046000
         L     ASMVTPTR,CVTASMVT(,@12)                             0117 00047000
*   FULLFLAG=OFF;                   /* SET FULL FLAG OFF INDICATING     00048000
*                                      THAT THE DATA SET IN QUESTION    00049000
*                                      IS BAD INSTEAD OF FULL        */ 00050000
         SLR   FULLFLAG,FULLFLAG                                   0118 00051000
         EJECT                                                          00052000
*                                                                  0119 00053000
*/********************************************************************/ 00054000
*/********************************************************************/ 00055000
*/*   MESSAGE NUMBER IS NOT PROVIDED                                 */ 00056000
*/********************************************************************/ 00057000
*/********************************************************************/ 00058000
*                                                                  0119 00059000
*   IF MSGNUM=ZERO THEN                                            0119 00060000
         LTR   MSGNUM,MSGNUM                                       0119 00061000
         BNZ   @RF00119                                            0119 00062000
*     DO;                                                          0120 00063000
*                                                                  0120 00064000
*       /*************************************************************/ 00065000
*       /*                                                           */ 00066000
*       /* CHECK THE NUSE FLAG AND DSBD FLAG OF THE PART/SART ENTRY  */ 00067000
*       /* PROVIDED AS INPUT. IF BOTH FLAGS ARE ON, THEN THIS MODULE */ 00068000
*       /* HAS ALREADY BEEN CALLED CONCERNING THIS PART/SART ENTRY.  */ 00069000
*       /* HENCE, RETURN TO THE CALLER. IF NEITHER FLAG IS ON, THEN  */ 00070000
*       /* RETURN TO THE CALLER SINCE A MESSAGE NUMBER SHOULD HAVE   */ 00071000
*       /* BEEN PROVIDED. IF EITHER FLAG IS ON, THEN DETERMINE       */ 00072000
*       /* WHETHER THE ENTRY POINTER POINTS TO A PART ENTRY OR A SART*/ 00073000
*       /* ENTRY.                                                    */ 00074000
*       /*                                                           */ 00075000
*       /*************************************************************/ 00076000
*                                                                  0121 00077000
*       IF PARENUSE=ON&PAREDSBD=ON THEN                            0121 00078000
         TM    PARENUSE(ENTRYPTR),B'11000000'                      0121 00079000
         BO    @RT00121                                            0121 00080000
*         RETURN;                   /* RETURN TO THE CALLER          */ 00081000
*       IF PARENUSE=OFF&PAREDSBD=OFF THEN                          0123 00082000
         TM    PARENUSE(ENTRYPTR),B'11000000'                      0123 00083000
         BZ    @RT00123                                            0123 00084000
*         RETURN;                   /* RETURN TO THE CALLER          */ 00085000
*                                                                  0124 00086000
*       /*************************************************************/ 00087000
*       /*                                                           */ 00088000
*       /* IF THE ENTRY POINTER POINTS TO A PART ENTRY, THEN         */ 00089000
*       /* DETERMINE WHICH PAGE DATA SET THE ENTRY REPRESENTS.       */ 00090000
*       /* OTHERWISE, THE ENTRY POINTER IS ASSUMED TO POINT TO A SART*/ 00091000
*       /* ENTRY.                                                    */ 00092000
*       /*                                                           */ 00093000
*       /*************************************************************/ 00094000
*                                                                  0125 00095000
*       IF TYPE=PAGEDS THEN                                        0125 00096000
@RF00123 LTR   TYPE,TYPE                                           0125 00097000
         BNZ   @RF00125                                            0125 00098000
*         DO;                                                      0126 00099000
         EJECT                                                          00100000
*                                                                  0127 00101000
*/********************************************************************/ 00102000
*/********************************************************************/ 00103000
*/*   PLPA IS BAD OR COMMON IS BAD                                   */ 00104000
*/********************************************************************/ 00105000
*/********************************************************************/ 00106000
*                                                                  0127 00107000
*           /*********************************************************/ 00108000
*           /*                                                       */ 00109000
*           /* IF THE PART ENTRY IS PLPA OR COMMON AND IF DUPLEXING  */ 00110000
*           /* IS NOT AVAILABLE, THEN TERMINATE THE SYSTEM. HOWEVER, */ 00111000
*           /* IF DUPLEXING IS AVAILABLE, THEN WRITE MESSAGES 9 AND  */ 00112000
*           /* 10 TO THE OPERATOR. ALSO TURN ON THE NO COMMON WRITE  */ 00113000
*           /* QUEUE FLAG IN THE ASMVT AS WELL AS ZERO THE WRITE     */ 00114000
*           /* QUEUE POINTER OF THE PART ENTRY.                      */ 00115000
*           /*                                                       */ 00116000
*           /*********************************************************/ 00117000
*                                                                  0127 00118000
*           IF PAREPLPA=ON|PARECOMM=ON THEN                        0127 00119000
         TM    PAREPLPA(ENTRYPTR),B'11000000'                      0127 00120000
         BZ    @RF00127                                            0127 00121000
*             DO;                                                  0128 00122000
*               IF ASMDUPLX=OFF THEN                               0129 00123000
         TM    ASMDUPLX(ASMVTPTR),B'10000000'                      0129 00124000
         BNZ   @RF00129                                            0129 00125000
*                 CALL TERMSYS;     /* TERMINATE THE SYSTEM          */ 00126000
         BAL   @14,TERMSYS                                         0130 00127000
*               MSGNUM=NINE;        /* SET MESSAGE NUMBER TO NINE    */ 00128000
@RF00129 LA    MSGNUM,9                                            0131 00129000
*               ASMNOCWQ=ON;        /* TURN NO COMMON WRITE QUEUE  0132 00130000
*                                      FLAG ON                       */ 00131000
         OI    ASMNOCWQ(ASMVTPTR),B'01000000'                      0132 00132000
*               CALL CLEARWTQ;      /* CLEAN UP THE IOE'S ON THE   0133 00133000
*                                      WRITE QUEUE POINTED TO BY THE    00134000
*                                      WRITE QUEUE POINTER IN THE  0133 00135000
*                                      PART ENTRY                    */ 00136000
         BAL   @14,CLEARWTQ                                        0133 00137000
*               PAREWTQE=ZERO;      /* ZERO THE PART ENTRY WRITE   0134 00138000
*                                      QUEUE POINTER IN ORDER TO   0134 00139000
*                                      PREVENT PART MONITOR (ILRPTM)    00140000
*                                      FROM WRITING TO PLPA OR COMMON*/ 00141000
         SLR   @12,@12                                             0134 00142000
         ST    @12,PAREWTQE(,ENTRYPTR)                             0134 00143000
*               CALL WRITEMSG;      /* BUILD MESSAGE 9 AND WRITE IT     00144000
*                                      TO THE OPERATOR               */ 00145000
         BAL   @14,WRITEMSG                                        0135 00146000
*               MSGNUM=TEN;         /* SET MESSAGE NUMBER TO 10      */ 00147000
         LA    MSGNUM,10                                           0136 00148000
*             END;                  /* PLPA OR COMMON IS BAD         */ 00149000
         EJECT                                                          00150000
*                                                                  0138 00151000
*/********************************************************************/ 00152000
*/********************************************************************/ 00153000
*/*   DUPLEX IS BAD                                                  */ 00154000
*/********************************************************************/ 00155000
*/********************************************************************/ 00156000
*                                                                  0138 00157000
*           ELSE                    /* PLPA AND COMMON ARE NOT BAD   */ 00158000
*             DO;                                                  0138 00159000
*                                                                  0138 00160000
         B     @RC00127                                            0138 00161000
@RF00127 DS    0H                                                  0139 00162000
*               /*****************************************************/ 00163000
*               /*                                                   */ 00164000
*               /* IF THE PART ENTRY IS DUPLEX AND IF PLPA AND COMMON*/ 00165000
*               /* ARE BOTH FULL OR ONE OF THEM IS UNUSABLE, THEN    */ 00166000
*               /* TERMINATE THE SYSTEM. HOWEVER IF PLPA AND COMMON  */ 00167000
*               /* ARE NOT BOTH FULL AND BOTH ARE USABLE, THEN WRITE */ 00168000
*               /* MESSAGE 7 TO THE OPERATOR. ALSO TURN OFF THE ASMVT*/ 00169000
*               /* DUPLEX OPTION FLAG AS WELL AS TURN ON THE ASMVT   */ 00170000
*               /* DUPLEXING SUSPENDED FLAG.                         */ 00171000
*               /*                                                   */ 00172000
*               /*****************************************************/ 00173000
*                                                                  0139 00174000
*               IF PAREDPLX=ON THEN                                0139 00175000
         TM    PAREDPLX(ENTRYPTR),B'00100000'                      0139 00176000
         BNO   @RF00139                                            0139 00177000
*                 DO;                                              0140 00178000
*                   PLPAPTR=ADDR(PARTENTS(PARTPLPN+1));/* OBTAIN A 0141 00179000
*                                      POINTER TO THE PART ENTRY FOR    00180000
*                                      PLPA                          */ 00181000
         L     @12,PARTPTR(,ASMVTPTR)                              0141 00182000
         LA    PLPAPTR,PARTENTS(,@12)                              0141 00183000
*                   COMMPTR=ADDR(PARTENTS(PARTCOMN+1));/* OBTAIN A 0142 00184000
*                                      POINTER TO THE PART ENTRY FOR    00185000
*                                      COMMON                        */ 00186000
         LA    COMMPTR,PARTENTS+64(,@12)                           0142 00187000
*                   IF(ASMPLPAF=ON&ASMCOMMF=ON)|(PLPAPTR->PARENUSE=ON|  00188000
*                       PLPAPTR->PAREDSBD=ON)|(COMMPTR->PARENUSE=ON|    00189000
*                       COMMPTR->PAREDSBD=ON) THEN                 0143 00190000
         TM    ASMPLPAF(ASMVTPTR),B'00000110'                      0143 00191000
         BO    @RT00143                                            0143 00192000
         TM    PARENUSE(PLPAPTR),B'11000000'                       0143 00193000
         BNZ   @RT00143                                            0143 00194000
         TM    PARENUSE(COMMPTR),B'11000000'                       0143 00195000
         BZ    @RF00143                                            0143 00196000
@RT00143 DS    0H                                                  0144 00197000
*                     CALL TERMSYS; /* TERMINATE THE SYSTEM          */ 00198000
         BAL   @14,TERMSYS                                         0144 00199000
*                   MSGNUM=SEVEN;   /* SET MESSAGE NUMBER TO 7       */ 00200000
@RF00143 LA    MSGNUM,7                                            0145 00201000
*                   ASMDUPLX=OFF;   /* TURN DUPLEX OPTION FLAG OFF   */ 00202000
*                   ASMNODPX=ON;    /* TURN DUPLEXING SUSPENDED FLAG    00203000
*                                      ON                            */ 00204000
         OI    ASMNODPX(ASMVTPTR),B'00001000'                      0147 00205000
         NI    ASMDUPLX(ASMVTPTR),B'01111111'                      0147 00206000
*                 END;              /* DUPLEX IS BAD                 */ 00207000
         EJECT                                                          00208000
*                                                                  0149 00209000
*/********************************************************************/ 00210000
*/********************************************************************/ 00211000
*/*   A LOCAL IS BAD                                                 */ 00212000
*/********************************************************************/ 00213000
*/********************************************************************/ 00214000
*                                                                  0149 00215000
*               ELSE                /* PLPA, COMMON, AND DUPLEX ARE     00216000
*                                      NOT BAD                       */ 00217000
*                 DO;                                              0149 00218000
*                                                                  0149 00219000
         B     @RC00139                                            0149 00220000
@RF00139 DS    0H                                                  0150 00221000
*                   /*************************************************/ 00222000
*                   /*                                               */ 00223000
*                   /* IF THE PART ENTRY IS A LOCAL AND IT HAPPENS TO*/ 00224000
*                   /* BE THE LAST ACTIVE LOCAL PAGE DATA SET, THEN  */ 00225000
*                   /* TERMINATE THE SYSTEM. HOWEVER IF IT IS NOT THE*/ 00226000
*                   /* LAST ACTIVE LOCAL, THEN WRITE MESSAGE 9 TO THE*/ 00227000
*                   /* OPERATOR. ALSO DECREMENT THE ASMVT UNRESERVED */ 00228000
*                   /* SLOT COUNT BY THE NUMBER OF SLOTS MADE        */ 00229000
*                   /* AVAILABLE BY THIS LOCAL AS WELL AS DECREMENT  */ 00230000
*                   /* THE COUNT OF ACTIVE LOCAL PAGE DATA SETS BY   */ 00231000
*                   /* ONE. IF THE PART ENTRY IS NOT A LOCAL, THEN   */ 00232000
*                   /* RETURN TO THE CALLER SINCE THE INPUT APPEARS  */ 00233000
*                   /* TO BE BAD.                                    */ 00234000
*                   /*                                               */ 00235000
*                   /*************************************************/ 00236000
*                                                                  0150 00237000
*                   IF PARELOCL=ON THEN                            0150 00238000
         TM    PARELOCL(ENTRYPTR),B'00010000'                      0150 00239000
         BNO   @RF00150                                            0150 00240000
*                     DO;                                          0151 00241000
*                       IF PARTLCNT<=ONE THEN                      0152 00242000
         L     @12,PARTPTR(,ASMVTPTR)                              0152 00243000
         LH    @12,PARTLCNT(,@12)                                  0152 00244000
         C     @12,@CF00042                                        0152 00245000
         BH    @RF00152                                            0152 00246000
*                         CALL TERMSYS;/* TERMINATE THE SYSTEM       */ 00247000
         BAL   @14,TERMSYS                                         0153 00248000
*                       MSGNUM=NINE;/* SET MESSAGE NUMBER TO 9       */ 00249000
@RF00152 LA    MSGNUM,9                                            0154 00250000
*                       RFY                                        0155 00251000
*                        (R0,                                      0155 00252000
*                         R1) RSTD;                                0155 00253000
*                       R0=ASMBKSLT;/* OBTAIN THE ASMVT UNRESERVED 0156 00254000
*                                      SLOT COUNT                    */ 00255000
         L     R0,ASMBKSLT(,ASMVTPTR)                              0156 00256000
*SETBKSLT:                                                         0157 00257000
*                       R1=R0-PARESLTA;/* DECREMENT COUNT            */ 00258000
SETBKSLT LR    R1,R0                                               0157 00259000
         SL    R1,PARESLTA(,ENTRYPTR)                              0157 00260000
*                       CS(R0,R1,ASMBKSLT);/* UPDATE ASMVT UNRESERVED   00261000
*                                      SLOT COUNT FIELD              */ 00262000
         CS    R0,@01,ASMBKSLT(ASMVTPTR)                           0158 00263000
*                       BC(NE,SETBKSLT);/* IF UNSUCCESSFUL, TRY AGAIN*/ 00264000
         BC    7,SETBKSLT                                          0159 00265000
*                       RFY                                        0160 00266000
*                        (R0,                                      0160 00267000
*                         R1) UNRSTD;                              0160 00268000
*                       PAREWTQE=ZERO;/* ZERO THE PART ENTRY WRITE 0161 00269000
*                                      QUEUE POINTER IN ORDER TO   0161 00270000
*                                      PREVENT PART MONITOR (ILRPTM)    00271000
*                                      FROM WRITING TO THIS LOCAL    */ 00272000
         SLR   @12,@12                                             0161 00273000
         ST    @12,PAREWTQE(,ENTRYPTR)                             0161 00274000
*                       PARTLCNT=PARTLCNT-1;/* DECREMENT THE COUNT OF   00275000
*                                      ACTIVE LOCAL PAGE DATA SETS BY   00276000
*                                      ONE                           */ 00277000
         L     @12,PARTPTR(,ASMVTPTR)                              0162 00278000
         LH    @11,PARTLCNT(,@12)                                  0162 00279000
         BCTR  @11,0                                               0162 00280000
         STH   @11,PARTLCNT(,@12)                                  0162 00281000
*                     END;          /* LOCAL IS BAD                  */ 00282000
*                   ELSE            /* NO PAGE DATA SET IS BAD       */ 00283000
*                     RETURN;                                      0164 00284000
*                 END;              /* PLPA, COMMON, OR DUPLEX ARE 0165 00285000
*                                      NOT BAD                       */ 00286000
*             END;                  /* PLPA OR COMMON ARE NOT BAD    */ 00287000
*         END;                      /* ENTRY POINTER POINTS TO A PART   00288000
*                                      ENTRY                         */ 00289000
         EJECT                                                          00290000
*                                                                  0168 00291000
*/********************************************************************/ 00292000
*/********************************************************************/ 00293000
*/*   A SWAP DATA SET IS BAD                                         */ 00294000
*/********************************************************************/ 00295000
*/********************************************************************/ 00296000
*                                                                  0168 00297000
*       /*************************************************************/ 00298000
*       /*                                                           */ 00299000
*       /* SINCE THE ENTRY POINTER DOES NOT POINT TO A PART ENTRY, IT*/ 00300000
*       /* IS ASSUMED TO POINT TO A SART ENTRY. THEREFORE, WRITE     */ 00301000
*       /* MESSAGE 9 TO THE OPERATOR. ALSO DECREMENT THE TOTAL NUMBER*/ 00302000
*       /* OF AVAILABLE SWAP SETS BY THE SWAP SET COUNT FOR THE BAD  */ 00303000
*       /* SWAP DATA SET AS WELL AS ZERO THE SWAP SET COUNT FOR THE  */ 00304000
*       /* BAD SWAP DATA SET.                                        */ 00305000
*       /*                                                           */ 00306000
*       /*************************************************************/ 00307000
*                                                                  0168 00308000
*       ELSE                        /* ENTRY POINTER POINTS TO A SART   00309000
*                                      ENTRY                         */ 00310000
*         DO;                                                      0168 00311000
         B     @RC00125                                            0168 00312000
@RF00125 DS    0H                                                  0169 00313000
*           MSGNUM=NINE;            /* SET MESSAGE NUMBER TO NINE    */ 00314000
         LA    MSGNUM,9                                            0169 00315000
*           SARSETCT=SARSETCT-SREAVLSL;/* DECREMENT THE TOTAL NUMBER    00316000
*                                      OF AVAILABLE SWAP SETS BY THE    00317000
*                                      NUMBER OF SWAP SETS PROVIDED     00318000
*                                      BY THE BAD SWAP DATA SET      */ 00319000
         L     @12,SARTPTR(,ASMVTPTR)                              0170 00320000
         L     @11,SARSETCT(,@12)                                  0170 00321000
         SL    @11,SREAVLSL(,ENTRYPTR)                             0170 00322000
         ST    @11,SARSETCT(,@12)                                  0170 00323000
*           SREAVLSL=ZERO;          /* ZERO THE AVAILABLE SWAP SET 0171 00324000
*                                      COUNT FOR THE BAD DATA SET    */ 00325000
         SLR   @12,@12                                             0171 00326000
         ST    @12,SREAVLSL(,ENTRYPTR)                             0171 00327000
*         END;                      /* A SWAP DATA SET IS BAD        */ 00328000
*     END;                          /* A MESSAGE NUMBER WAS NOT    0173 00329000
*                                      PROVIDED                      */ 00330000
         EJECT                                                          00331000
*                                                                  0174 00332000
*/********************************************************************/ 00333000
*/********************************************************************/ 00334000
*/*   A MESSAGE NUMBER IS PROVIDED                                   */ 00335000
*/********************************************************************/ 00336000
*/********************************************************************/ 00337000
*                                                                  0174 00338000
*   /*****************************************************************/ 00339000
*   /*                                                               */ 00340000
*   /* IF THE MESSAGE NUMBER PROVIDED IS EIGHT, THEN TERMINATE THE   */ 00341000
*   /* SYSTEM. IF THE MESSAGE NUMBER PROVIDED IS TEN, THEN THE       */ 00342000
*   /* INDICATED DATA SET (PLPA OR COMMON) IS ASSUMED TO BE FULL.    */ 00343000
*   /*                                                               */ 00344000
*   /*****************************************************************/ 00345000
*                                                                  0174 00346000
*   ELSE                            /* A MESSAGE NUMBER IS PROVIDED  */ 00347000
*     DO;                                                          0174 00348000
         B     @RC00119                                            0174 00349000
@RF00119 DS    0H                                                  0175 00350000
*       IF MSGNUM=EIGHT THEN                                       0175 00351000
         C     MSGNUM,@CF00039                                     0175 00352000
         BNE   @RF00175                                            0175 00353000
*         CALL TERMSYS;             /* TERMINATE THE SYSTEM          */ 00354000
         BAL   @14,TERMSYS                                         0176 00355000
*       IF MSGNUM=TEN THEN                                         0177 00356000
@RF00175 C     MSGNUM,@CF00032                                     0177 00357000
         BNE   @RF00177                                            0177 00358000
*         FULLFLAG=ON;              /* SET FULL FLAG ON INDICATING 0178 00359000
*                                      THAT THE DATA SET IN QUESTION    00360000
*                                      IS FULL INSTEAD OF BAD        */ 00361000
         LA    FULLFLAG,1                                          0178 00362000
*     END;                          /* A MESSAGE NUMBER PROVIDED     */ 00363000
@RF00177 DS    0H                                                  0180 00364000
         EJECT                                                          00365000
*                                                                  0180 00366000
*/********************************************************************/ 00367000
*/********************************************************************/ 00368000
*/*   WRITE MESSAGE TO THE OPERATOR AND THEN RETURN TO CALLER        */ 00369000
*/********************************************************************/ 00370000
*/********************************************************************/ 00371000
*                                                                  0180 00372000
*   CALL WRITEMSG;                  /* BUILD THE MESSAGE AND WRITE IT   00373000
*                                      TO THE OPERATOR               */ 00374000
@RC00119 BAL   @14,WRITEMSG                                        0180 00375000
*   RETURN;                         /* RETURN TO THE CALLER          */ 00376000
@EL00001 DS    0H                                                  0181 00377000
@EF00001 DS    0H                                                  0181 00378000
@ER00001 LM    @14,@12,12(@13)                                     0181 00379000
         BR    @14                                                 0181 00380000
         EJECT                                                          00381000
*                                                                  0182 00382000
*/********************************************************************/ 00383000
*/********************************************************************/ 00384000
*/*   ENTRY POINT FOR ILRTMI00                                       */ 00385000
*/********************************************************************/ 00386000
*/********************************************************************/ 00387000
*                                                                  0182 00388000
*ILRMSGSP:                                                         0182 00389000
*   ENTRY;                                                         0182 00390000
*                                                                  0182 00391000
@EP00182 DS    0H                                                  0183 00392000
*   /*****************************************************************/ 00393000
*   /*                                                               */ 00394000
*   /* OBTAIN POINTER TO THE ASMVT, PLPA PART ENTRY AND COMMON PART  */ 00395000
*   /* ENTRY.                                                        */ 00396000
*   /*                                                               */ 00397000
*   /*****************************************************************/ 00398000
*                                                                  0183 00399000
*   ASMVTPTR=CVTASMVT;              /* OBTAIN POINTER TO ASMVT       */ 00400000
         L     @12,CVTPTR                                          0183 00401000
         L     ASMVTPTR,CVTASMVT(,@12)                             0183 00402000
*   PLPAPTR=ADDR(PARTENTS(PARTPLPN+1));/* OBTAIN A POINTER TO THE  0184 00403000
*                                      PART ENTRY FOR PLPA           */ 00404000
         L     @12,PARTPTR(,ASMVTPTR)                              0184 00405000
         LA    PLPAPTR,PARTENTS(,@12)                              0184 00406000
*   COMMPTR=ADDR(PARTENTS(PARTCOMN+1));/* OBTAIN A POINTER TO THE  0185 00407000
*                                      PART ENTRY FOR COMMON         */ 00408000
*                                                                  0185 00409000
         LA    COMMPTR,PARTENTS+64(,@12)                           0185 00410000
*   /*****************************************************************/ 00411000
*   /*                                                               */ 00412000
*   /* CHECK THE FOLLOWING FLAGS: ASMNODPX, ASMPLPAF, ASMCOMMF,      */ 00413000
*   /* PARENUSE AND PAREDSBD OF THE PART ENTRIES FOR PLPA AND COMMON.*/ 00414000
*   /* IF ANY OF THE ABOVE FLAGS ARE ON, THEN A MESSAGE NEEDS TO BE  */ 00415000
*   /* ISSUED TO THE OPERATOR. OTHERWISE, NO MESSAGE IS NEEDED SO    */ 00416000
*   /* RETURN TO ILRTMI00.                                           */ 00417000
*   /*                                                               */ 00418000
*   /*****************************************************************/ 00419000
*                                                                  0186 00420000
*   IF(ASMNODPX=ON|ASMPLPAF=ON|ASMCOMMF=ON)|(PLPAPTR->PARENUSE=ON| 0186 00421000
*       PLPAPTR->PAREDSBD=ON)|(COMMPTR->PARENUSE=ON|COMMPTR->PAREDSBD=  00422000
*       ON) THEN                                                   0186 00423000
         TM    ASMNODPX(ASMVTPTR),B'00001110'                      0186 00424000
         BNZ   @RT00186                                            0186 00425000
         TM    PARENUSE(PLPAPTR),B'11000000'                       0186 00426000
         BNZ   @RT00186                                            0186 00427000
         TM    PARENUSE(COMMPTR),B'11000000'                       0186 00428000
         BZ    @RF00186                                            0186 00429000
@RT00186 DS    0H                                                  0187 00430000
*     DO;                                                          0187 00431000
*       TYPE=PAGEDS;                /* SET TYPE FOR A PAGE DATA SET  */ 00432000
         SLR   TYPE,TYPE                                           0188 00433000
*       FULLFLAG=ON;                /* SET FULL FLAG ON INDICATING 0189 00434000
*                                      THAT PLPA OR COMMON IS FULL 0189 00435000
*                                      INSTEAD OF BAD                */ 00436000
         LA    FULLFLAG,1                                          0189 00437000
         EJECT                                                          00438000
*                                                                  0190 00439000
*/********************************************************************/ 00440000
*/********************************************************************/ 00441000
*/*   OBTAIN SALLOC                                                  */ 00442000
*/********************************************************************/ 00443000
*/********************************************************************/ 00444000
*                                                                  0190 00445000
*       RFY                                                        0190 00446000
*        (R11,                                                     0190 00447000
*         R12,                                                     0190 00448000
*         R14,                                                     0190 00449000
*         R15) RSTD;                                               0190 00450000
*       R15=R13;                    /* SAVE ADDRESS OF ILRTMI00'S  0191 00451000
*                                      SAVEAREA                      */ 00452000
         LR    R15,R13                                             0191 00453000
*       GEN REFS(PSALITA) SETS(R11,R12,R13,R14);                   0192 00454000
GTSALLOC SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                      XX00455000
               RELATED=(ASMBUFFERS,ILRMSG00(RLSALLOC))                  00456000
*       R13=R15;                    /* RESTORE REG 13 WITH ILRTMI00'S   00457000
*                                      SAVE AREA ADDRESS             */ 00458000
         LR    R13,R15                                             0193 00459000
*       RFY                                                        0194 00460000
*        (R11,                                                     0194 00461000
