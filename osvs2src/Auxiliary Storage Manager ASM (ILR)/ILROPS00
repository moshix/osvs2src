         ALR   @01,@07                                             0417 00951000
         ALR   @01,@05                                             0417 00952000
         ALR   @01,@15                                             0417 00953000
         MH    @01,STRNGNO                                         0417 00954000
         ALR   @14,@01                                             0417 00955000
         STH   @14,CORESIZE                                        0417 00956000
*       DO;                         /* GETMAIN (RC) LV(CORESIZE)   0418 00957000
*                                      SP(245)                       */ 00958000
*         RESPECIFY                                                0419 00959000
*          (GPR01F,                                                0419 00960000
*           GPR15F,                                                0419 00961000
*           GPR00F) RESTRICTED;                                    0419 00962000
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */ 00963000
         SLR   GPR01F,GPR01F                                       0420 00964000
*         GPR15F=0;                 /* RC-TYPE GETMAIN               */ 00965000
