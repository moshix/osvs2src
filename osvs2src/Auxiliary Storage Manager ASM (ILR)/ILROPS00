         CLC   RTCODE(4),@CF00053                                  0387 00769000
         BE    @RT00387                                            0387 00770000
*         GO TO OPSEXIT;            /* ERROR EXIT-RETURN TO CALLER   */ 00771000
*     END;                          /* END OF DYNALLOC               */ 00772000
*                                                                  0390 00773000
*/********************************************************************/ 00774000
*/*                                                                  */ 00775000
*/* GETCORE:   SUBSECTION TO ACQUIRE STORAGE IN SQA AND IN THE       */ 00776000
*/*            NUCLEUS BUFFER AREA FOR I/O CONTROL BLOCKS.           */ 00777000
*/*                                                                  */ 00778000
*/* INPUT:     DEVICE TYPE                                           */ 00779000
*/*            NUMBER OF EXTENTS                                     */ 00780000
*/*                                                                  */ 00781000
*/* PROCESS:   CALCULATE THE AMOUNT OF SQA AND NUCLEUS BUFFER AREA   */ 00782000
*/*            REQUIRED TO BUILD THE I/O CONTROL BLOCKS.             */ 00783000
*/*            THE DEVICE TYPE IS USED TO DETERMINE BOTH THE NUMBER  */ 00784000
*/*            OF IORB-IOSB-SRB STRINGS: 1 FOR DISK OR 4 FOR DRUM,   */ 00785000
*/*            AND THE NUMBER OF PCCWS REQUIRED:                     */ 00786000
*/*                 DEVIDE THE DESIRED I/O RESPONSE TIME (BURST TIME */ 00787000
*/*                 = CONSTANT OF 50000 MICRO SECS.) BY THE TIME TO  */ 00788000
*/*                 PROCESS I/O FOR ONE SLOT ON THIS DEVICE.         */ 00789000
*/*            ISSUE A CONDITIONAL GETMAIN FOR THE SQA AND CHECK FOR */ 00790000
*/*            AVAILABLE NUCLEUS STORAGE. ZERO BOTH AREAS OF STORAGE.*/ 00791000
*/*                                                                  */ 00792000
*/* OUTPUT:    SUCCESSFUL - ZEROED AVAILABLE STORAGE                 */ 00793000
*/*                                                                  */ 00794000
*/********************************************************************/ 00795000
*/*GETCORE:  CHART                                                   */ 00796000
*/*GETCORE:  N    ACQUIRE CORE FOR ASM I/O CONTROL BLOCKS            */ 00797000
*/*          N    LOOP TO FIND MATCHING DEVICE TYPE                  */ 00798000
*/*          M    SET DEVICE MATCH NOT FOUND                         */ 00799000
*/*%GET10:   P    GET A DEVICE TYPE ENTRY FROM THE TABLE             */ 00800000
*/*          D    (YES,,NO,%GET30) THIS DEVICE MATCH TABLE ENTRY?    */ 00801000
*/*          D    (YES,%GET35,NO,) DOES THE DEVICE IS 2314 FOR SWAP? */ 00802000
*/*          P    CALCULATE BURST NUMBER FOR THIS DEVICE             */ 00803000
*/*          P    INDICATE DEVICE MATCH FOUND                        */ 00804000
*/*          P    COMPUTE NBR OF IORBS REQUIRED                      */ 00805000
*/*          D   (YES,%GET20,NO,) DEVICE MATCH FOUND?                */ 00806000
*/*%GET30:  D   (YES,%GET10,NO,%GET35) MORE ENTRIES IN DEVICE TABLE  */ 00807000
*/*%GET20:   P    COMPUTE SIZE OF ALL SQA CONTROL BLOCKS             */ 00808000
*/*          P    CONDITIONAL SQA GETMAIN FOR TOTAL LENGTH           */ 00809000
*/*          D    (YES,,NO,%GET40) GETMAIN SUCCESSFUL ?              */ 00810000
*/*          P    SAVE GETMAIN PTR AND LENGTH IN OUTPUT PARM         */ 00811000
*/*          P    ZERO ACQUIRED SQA STORAGE                          */ 00812000
*/*          D   (YES,,NO,%GET70) IS DATASET A PAGE SPACE & AT NIP   */ 00813000
*/*          P    CALCULATE CORE LENGTH NEEDED FOR PCCWS             */ 00814000
*/*          P    CALCULATE SIZE OF NUCLEUS BUFFER AREA AVAILABLE    */ 00815000
*/*          D    (YES,,NO,%GET50) ENOUGH CORE FOR PCCWS ?           */ 00816000
*/*          P    SAVE POINTER TO NUCLEUS BUFFER AREA CORE           */ 00817000
*/*          P    UPDATE POINTER TO TOP OF NUCLEUS BUFFER AREA       */ 00818000
*/*          P    ADJUST AMOUNT OF BUFFER SPACE REMAINING            */ 00819000
*/*          P    (,%GET70) CLEAR NUCLEUS BUFFER AREA                */ 00820000
*/*%GET35:   R    RETURN: ERROR EXIT - R15 = ERROR CODE(12)          */ 00821000
*/*%GET40:   R    RETURN: ERROR EXIT - R15 = ERROR CODE(16)          */ 00822000
*/*%GET50:   R    RETURN: ERROR EXIT - R15 = ERROR CODE(20)          */ 00823000
*/*%GET70:   R    END OF ACQUIRE STORAGE SECTION                     */ 00824000
*/*GETCORE:  END                                                     */ 00825000
*                                                                  0390 00826000
*                                                                  0390 00827000
*/* GETCORE-ACQUIRE NEEDED STORAGE                                   */ 00828000
*                                                                  0390 00829000
*   IF NOCONBK=ON THEN                                             0390 00830000
@RC00319 TM    NOCONBK(PARMPTR),B'00001000'                        0390 00831000
         BO    @RT00390                                            0390 00832000
*     GOTO OPSEXIT;                 /* SHORT PATH EXIT               */ 00833000
*   DEVTBPTR=ADDR(DEVIDTB);         /* SET DEVICE ID TABLE PTR       */ 00834000
         LA    @15,DEVIDTB                                         0392 00835000
         LR    DEVTBPTR,@15                                        0392 00836000
*   DEVTBEND=ADDR(DEVIDTB)+LENGTH(DEVIDTB);/* SET END OF DEVID TABLE    00837000
*                                      ADDRESS                       */ 00838000
         AL    @15,@CF00368                                        0393 00839000
         LR    DEVTBEND,@15                                        0393 00840000
*   DEVMATCH=NO;                    /* SET MATCH NOT FOUND FLAG      */ 00841000
         NI    DEVMATCH,B'01111111'                                0394 00842000
*   DO WHILE(DEVMATCH=NO&DEVTBPTR<DEVTBEND);/* SEARCH DEVID TABLE    */ 00843000
         B     @DE00395                                            0395 00844000
@DL00395 DS    0H                                                  0396 00845000
*     IF DEVID=DEVTYP(THREE:FOUR) THEN/* DEVICE TYPES MATCH ?        */ 00846000
         L     @01,FPL3DPTR                                        0396 00847000
         CLC   DEVID(2,DEVTBPTR),DEVTYP+2(@01)                     0396 00848000
         BNE   @RF00396                                            0396 00849000
*       DO;                                                        0397 00850000
*         DEVMATCH=YES;             /* DEVICE TYPE FOUND             */ 00851000
         OI    DEVMATCH,B'10000000'                                0398 00852000
*         BURSTNO=ASMBURST/SLOTTIME;/* CALC BURST NBR FOR DEV TYPE   */ 00853000
         L     @01,ASMVTPTR                                        0399 00854000
         L     @00,ASMBURST(,@01)                                  0399 00855000
         SRDA  @00,32                                              0399 00856000
         LH    @15,SLOTTIME(,DEVTBPTR)                             0399 00857000
         DR    @00,@15                                             0399 00858000
         STH   @01,BURSTNO                                         0399 00859000
*         DVTYPE=DEVID;             /* SAVE DEVICE TYPE FOR CALLER   */ 00860000
         MVC   DVTYPE(2,PARMPTR),DEVID(DEVTBPTR)                   0400 00861000
*         IF DEVID=D2305M1|         /* DEVICE A DRUM ?               */ 00862000
*             DEVID=D2305M2 THEN                                   0401 00863000
         CLC   DEVID(2,DEVTBPTR),D2305M1                           0401 00864000
         BE    @RT00401                                            0401 00865000
         CLC   DEVID(2,DEVTBPTR),D2305M2                           0401 00866000
         BNE   @RF00401                                            0401 00867000
@RT00401 DS    0H                                                  0402 00868000
*           STRNGNO=DRUMSTRS;       /* SET NBR IORBS FOR DRUM        */ 00869000
         MVC   STRNGNO(2),@CH00095                                 0402 00870000
*         ELSE                                                     0403 00871000
*           IF PAGESP=OFF THEN      /* SWAP SPACE?                   */ 00872000
         B     @RC00401                                            0403 00873000
@RF00401 TM    PAGESP(PARMPTR),B'10000000'                         0403 00874000
         BNZ   @RF00403                                            0403 00875000
*             STRNGNO=DISKSTRS;     /* SET NBR IORBS TO BE CREATED   */ 00876000
         MVC   STRNGNO(2),@CH00260                                 0404 00877000
*           ELSE                    /* NO-PAGE SPACE                 */ 00878000
*             STRNGNO=ONE;          /* SET NRB IORBS FOR PAGE-DISK   */ 00879000
         B     @RC00403                                            0405 00880000
@RF00403 MVC   STRNGNO(2),@CH00036                                 0405 00881000
*       END;                                                       0406 00882000
*     ELSE                                                         0407 00883000
*       ;                                                          0407 00884000
@RF00396 DS    0H                                                  0408 00885000
*     DEVTBPTR=DEVTBPTR+LENGTH(DEVENT);/* GET NEXT DEVICE TABLE ENTRY*/ 00886000
@RC00396 AL    DEVTBPTR,@CF00095                                   0408 00887000
*   END;                            /* END OF SEARCH DEVICE TYPE   0409 00888000
*                                      TABLE                         */ 00889000
@DE00395 TM    DEVMATCH,B'10000000'                                0409 00890000
         BNZ   @DC00395                                            0409 00891000
         CR    DEVTBPTR,DEVTBEND                                   0409 00892000
         BL    @DL00395                                            0409 00893000
@DC00395 DS    0H                                                  0410 00894000
*   IF DEVMATCH=NO|(DEVTYP(THREE:FOUR)=D2314&PAGESP=OFF) THEN/* VALID   00895000
*                                      DEVICE TYPE FOUND?            */ 00896000
         TM    DEVMATCH,B'10000000'                                0410 00897000
         BZ    @RT00410                                            0410 00898000
         L     @01,FPL3DPTR                                        0410 00899000
         CLC   DEVTYP+2(2,@01),D2314                               0410 00900000
         BNE   @RF00410                                            0410 00901000
         TM    PAGESP(PARMPTR),B'10000000'                         0410 00902000
         BNZ   @RF00410                                            0410 00903000
@RT00410 DS    0H                                                  0411 00904000
*     RTCODE=LOCFAILD;              /* NO-LOCATEFAIL                 */ 00905000
         MVC   RTCODE(4),@CF00488                                  0411 00906000
*   ELSE                            /* DEVICE TYPE FOUND             */ 00907000
*     DO;                                                          0412 00908000
         B     @RC00410                                            0412 00909000
@RF00410 DS    0H                                                  0413 00910000
*       IOSBLGH=(LENGTH(IOSB)+SEVEN)/EIGHT*EIGHT;/* ROUND TO DBL WORD*/ 00911000
         LA    @05,112                                             0413 00912000
         STC   @05,IOSBLGH                                         0413 00913000
*       SRBLGH=(LENGTH(SRB)+SEVEN)/EIGHT*EIGHT;/* ROUND TO DOUBLE  0414 00914000
*                                      WORD                          */ 00915000
         LA    @15,48                                              0414 00916000
         STC   @15,SRBLGH                                          0414 00917000
*       LPMBLGH=(LENGTH(IDALPMB)+SEVEN)/EIGHT*EIGHT;               0415 00918000
         LA    @14,32                                              0415 00919000
         STC   @14,LPMBLGH                                         0415 00920000
*       TOSIZEDB=((EDBLGH*NOEXTNT+EDBHDLGH)+SEVEN)/EIGHT*EIGHT;    0416 00921000
*                                                                  0416 00922000
         L     @01,FPL4DPTR                                        0416 00923000
         SLR   @07,@07                                             0416 00924000
         IC    @07,NOEXTNT(,@01)                                   0416 00925000
         SLR   TOSIZEDB,TOSIZEDB                                   0416 00926000
         IC    TOSIZEDB,EDBLGH                                     0416 00927000
         MR    @06,TOSIZEDB                                        0416 00928000
         LR    @00,@07                                             0416 00929000
         SLR   TOSIZEDB,TOSIZEDB                                   0416 00930000
         IC    TOSIZEDB,EDBHDLGH                                   0416 00931000
         ALR   @00,TOSIZEDB                                        0416 00932000
         AL    @00,@CF00076                                        0416 00933000
         SRDA  @00,32                                              0416 00934000
         D     @00,@CF00053                                        0416 00935000
         SLA   @01,3                                               0416 00936000
         LR    TOSIZEDB,@01                                        0416 00937000
*       /*************************************************************/ 00938000
*       /*                                                           */ 00939000
*       /* CALCULATE AMOUNT OF SQA REQUIRED FOR I/O CONTROL BLOCKS   */ 00940000
*       /*                                                           */ 00941000
*       /*************************************************************/ 00942000
*                                                                  0417 00943000
*       CORESIZE=LPMBLGH+TOSIZEDB+STRNGNO*(IORBLGH+SAVALGH+IOSBLGH+     00944000
*           SRBLGH);                /* SQA SIZE NEEDED               */ 00945000
         ALR   @14,TOSIZEDB                                        0417 00946000
