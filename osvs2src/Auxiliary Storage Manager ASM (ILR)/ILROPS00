*       NMNTDSA=ADDR(WKDSNAME);     /* DSNAME PTR TO PARM LIST       */ 00537000
         LA    @15,WKDSNAME                                        0325 00538000
         STCM  @15,15,NMNTDSA(@14)                                 0325 00539000
*       RFY                                                        0326 00540000
*        (R1,                                                      0326 00541000
*         R15) RSTD;                                               0326 00542000
*       GEN REFS(NVTMOUNT) SETS(R1);                               0327 00543000
*                                   /*                                  00544000
