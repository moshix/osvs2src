*       LPMBPTRK=LPMBLKSZ*LPMBLKTR; /* BYTES PER TRACK               */ 01432000
         L     @12,LPMBLKSZ(,LPMBPTR)                              0520 01433000
         MH    @12,LPMBLKTR(,LPMBPTR)                              0520 01434000
         ST    @12,LPMBPTRK(,LPMBPTR)                              0520 01435000
*     END;                                                         0521 01436000
*   IF UCBRPS=ON THEN               /* RPS DEVICE ?                  */ 01437000
@RC00511 L     @12,UCBPTR                                          0522 01438000
         TM    UCBRPS(@12),B'00010000'                             0522 01439000
         BNO   @RF00522                                            0522 01440000
*     LPMBRPS=ON;                   /* YES - SO INDICATE             */ 01441000
         OI    LPMBRPS(LPMBPTR),B'10000000'                        0523 01442000
*   ELSE                                                           0524 01443000
*     ;                                                            0524 01444000
@RF00522 DS    0H                                                  0525 01445000
*   COREADDR=COREADDR+LPMBLGH;      /* UPDATE NEXT AVAIL STORAGE     */ 01446000
         SLR   @12,@12                                             0525 01447000
         IC    @12,LPMBLGH                                         0525 01448000
         ALR   COREADDR,@12                                        0525 01449000
*                                                                  0526 01450000
*/********************************************************************/ 01451000
*/*                                                                  */ 01452000
*/* INITEDB:   SUBSECTION TO INITIALIZE THE EXTENT DEFINITION BLOCK  */ 01453000
*/*                                                                  */ 01454000
*/* INPUT:     POINTER TO NEXT AVAILABLE STORAGE                     */ 01455000
*/*            CATALOG INFORMATION                                   */ 01456000
*/*                                                                  */ 01457000
*/* PROCESS:   THE EDB IS INITIALIZED WITH INFORMATION ACQUIRED FROM */ 01458000
*/*            THE CATALOG LOCATE. IF THERE ARE MULTIPLE EXTENTS, AN */ 01459000
*/*            EDB HAS TO BE INITIALIZED FOR EACH EXTENT. IN THE     */ 01460000
*/*            CASE OF MULTIPLE EXTENTS, THERE IS A SORT BY RELATIVE */ 01461000
*/*            STARTING TRACK ADDRESS IN ORDER THAT ILRSRT00 WILL BE */ 01462000
*/*            ABLE TO CHOOSE THE 'BEST' SLOT. THE AMS UTILITY       */ 01463000
*/*            PICKS EXTENTS BY SIZE RATHER THAN BY LOCATION.        */ 01464000
*/*                                                                  */ 01465000
*/*            THE SORT LOOPS THROUGH THE CATALOG PARAMETER LISTS    */ 01466000
*/*            SEARCHING FOR THE LOWEST EXTENT STARTING TRACK WHICH  */ 01467000
*/*            HAS NOT BEEN USED. WHEN FOUND, THE NEXT EDB ENTRY IS  */ 01468000
*/*            INITIALIZED WITH THE CATALOG INFORMATION.             */ 01469000
*/*                                                                  */ 01470000
*/* OUTPUT:    A COMPLETED EDB                                       */ 01471000
*/*                                                                  */ 01472000
*/********************************************************************/ 01473000
*/*INITEDB:  CHART                                                   */ 01474000
*/*INITEDB:  N    INITIALIZE THE EDB                                 */ 01475000
*/*          P    INITIALIZE EDBHDR PTR TO NEXT AVAILABLE STORAGE    */ 01476000
*/*          P    INIT EDB IDENTIFIER                                */ 01477000
*/*          P    INIT EDB LENGTH IN HEADER                          */ 01478000
*/*          P    INIT NUMBER OF EDBS IN HEADER                      */ 01479000
*/*          P    SET EDB LPMB POINTER IN HEADER                     */ 01480000
*/*          N    SORT EDB ENTRIES, IF NECESSARY                     */ 01481000
*/*          P    INIT PTR TO FIRST EDB ENTRY                        */ 01482000
*/*          P    INIT VALUE OF LAST USED HIRBA TO ZERO              */ 01483000
*/*%EDB10:   D    (YES,,NO,%EDB20) ANOTHER EDB TO PROCESS ?          */ 01484000
*/*          P    INITIALIZE CURRENT SORT COMPARE VALUE              */ 01485000
*/*          N    LOOP THROUGH LOCATE LIST EXTENT ENTRIES            */ 01486000
*/*%EDB50:   D    (YES,,NO,%EDB30) ANOTHER ENTRY IN LOCATE LIST ?    */ 01487000
*/*          D    (YES,,NO,%EDB40) EDBSTTRK < CURRENT & ENTRY ^ USED */ 01488000
*/*          P    SET CURRENT VALUE TO THIS STARTING TRACK           */ 01489000
*/*          P    SAVE INDEX TO THIS LIST ENTRY                      */ 01490000
*/*%EDB40:   P    (,%EDB50) UPDATE POINTER TO NEXT LOCATE LIST ENTRY */ 01491000
*/*%EDB30:   P    SET EDB LPMB POINTER                               */ 01492000
*/*          P    INITIALIZE NUMBER OF THIS EXTENT                   */ 01493000
*/*          P    INITIALIZE STARTING TRACK OF EXTENT                */ 01494000
*/*          P    INIT LOW RBA LIMIT                                 */ 01495000
*/*          P    INIT HIGH RBA LIMIT                                */ 01496000
*/*          D    (YES,,NO,%EDB60) TRACK OVERFLOW DEVICE ?           */ 01497000
*/*          P    INIT TRACK OVERFLOW FLAG FOR THIS ENTRY            */ 01498000
*/*%EDB60:   P    MARK EXTENT ENTRY USED FLAG FOR THIS ENTRY         */ 01499000
*/*          P    (,%EDB10) UPDATE PTR TO NEXT EDB ENTRY             */ 01500000
*/*%EDB20:   P    SET CATALOG HIRBA TO EDB HIGH RBA                  */ 01501000
*/*          R    END OF INITIALIZE EDB                              */ 01502000
*/*INITEDB:  END                                                     */ 01503000
*                                                                  0526 01504000
*                                                                  0526 01505000
*/*INITEDB-INITIALIZE EDB                                            */ 01506000
*                                                                  0526 01507000
*   RFY                                                            0526 01508000
*     IDAEDB BASED(EDBPTR);                                        0526 01509000
*   RFY                                                            0527 01510000
*     IDAEDBHD BASED(EDBHDPTR);                                    0527 01511000
*   RFY                                                            0528 01512000
*     CTEXTENT BASED(WRKPTR);                                      0528 01513000
*   EDBHDPTR=COREADDR;              /* NEXT AVAILABLE STORAGE        */ 01514000
         LR    EDBHDPTR,COREADDR                                   0529 01515000
*   OUEDBPR=EDBHDPTR;               /* SET EDB ADDRESS IN OUTPUT PARM*/ 01516000
         ST    EDBHDPTR,OUEDBPR(,PARMPTR)                          0530 01517000
*   EDBID=EDBIDC;                   /* INIT IDENTIFIER FIELD         */ 01518000
         IC    @12,EDBIDC                                          0531 01519000
         STC   @12,EDBID(,EDBHDPTR)                                0531 01520000
*   EDBLEN=EDBLGH;                  /* EDB ENTRY LENGTH              */ 01521000
         SLR   @12,@12                                             0532 01522000
         IC    @12,EDBLGH                                          0532 01523000
         STH   @12,EDBLEN(,EDBHDPTR)                               0532 01524000
*   EDBNO=NOEXTNT;                  /* NBR OF EDB ENTRIES            */ 01525000
         L     @12,FPL4DPTR                                        0533 01526000
         IC    @06,NOEXTNT(,@12)                                   0533 01527000
         STC   @06,EDBNO(,EDBHDPTR)                                0533 01528000
*   EDBLPMBC=LPMBPTR;               /* LPMB ADDRESS                  */ 01529000
         ST    LPMBPTR,EDBLPMBC(,EDBHDPTR)                         0534 01530000
*   EDBPTR=COREADDR+EDBHDLGH;       /* ADDRESS FIRST EDB ENTRY       */ 01531000
         LR    EDBPTR,COREADDR                                     0535 01532000
         SLR   @12,@12                                             0535 01533000
         IC    @12,EDBHDLGH                                        0535 01534000
         ALR   EDBPTR,@12                                          0535 01535000
*   LOEDBVAL=ZERO;                  /* VALUE OF LAST HIRBA USED      */ 01536000
         SLR   LOEDBVAL,LOEDBVAL                                   0536 01537000
*   PROCFLG=''B;                    /* INIT EDB USED FLAG            */ 01538000
         XC    PROCFLG(5),PROCFLG                                  0537 01539000
*   DO I=1 TO EDBNO;                /* LOOP THROUGH EDB ENTRIES      */ 01540000
         LA    @12,1                                               0538 01541000
         B     @DE00538                                            0538 01542000
@DL00538 DS    0H                                                  0539 01543000
*     LOCHECK=SORTMASK;             /* SORT COMPARE VALUE            */ 01544000
         LH    @12,SORTMASK                                        0539 01545000
         STH   @12,LOCHECK                                         0539 01546000
*     DO J=1 TO EDBNO;              /* LOOP THROUGH LOCATE LIST      */ 01547000
         LA    J,1                                                 0540 01548000
         B     @DE00540                                            0540 01549000
@DL00540 DS    0H                                                  0541 01550000
*       WRKPTR=ADDR(EXTENT(J));     /* BASE FOR LOCATE DATA          */ 01551000
         LR    @15,J                                               0541 01552000
         MH    @15,@CH00362                                        0541 01553000
         L     WRKPTR,FPL4DPTR                                     0541 01554000
         LA    WRKPTR,EXTENT-20(@15,WRKPTR)                        0541 01555000
*       IF ENTUSEDF(J)^=ON&         /* ENTRY NOT CHOSEN YET ?        */ 01556000
*           CTCCBEG<LOCHECK THEN    /* AND LOWER THAN COMP VALUE     */ 01557000
         LA    @01,ENTUSEDF-1(J)                                   0542 01558000
         TM    0(@01),B'10000000'                                  0542 01559000
         BO    @RF00542                                            0542 01560000
         ICM   @15,12,CTCCBEG(WRKPTR)                              0542 01561000
         SRA   @15,16                                              0542 01562000
         CH    @15,LOCHECK                                         0542 01563000
         BNL   @RF00542                                            0542 01564000
*         DO;                                                      0543 01565000
*           LOCHECK=CTCCBEG;        /* SWAP SORT COMPARE VALUE       */ 01566000
         STH   @15,LOCHECK                                         0544 01567000
*           K=J;                    /* REMEMBER THIS LOCATE ENTRY    */ 01568000
         STC   J,K                                                 0545 01569000
*         END;                                                     0546 01570000
*       ELSE                                                       0547 01571000
*         ;                                                        0547 01572000
@RF00542 DS    0H                                                  0548 01573000
*     END;                                                         0548 01574000
         AL    J,@CF00036                                          0548 01575000
@DE00540 SLR   @06,@06                                             0548 01576000
         IC    @06,EDBNO(,EDBHDPTR)                                0548 01577000
         CR    J,@06                                               0548 01578000
         BNH   @DL00540                                            0548 01579000
*     WRKPTR=ADDR(EXTENT(K));       /* BASE FOR LOCATE DATA          */ 01580000
         SLR   @12,@12                                             0549 01581000
         IC    @12,K                                               0549 01582000
         MH    @12,@CH00362                                        0549 01583000
         L     WRKPTR,FPL4DPTR                                     0549 01584000
         LA    WRKPTR,EXTENT-20(@12,WRKPTR)                        0549 01585000
*     EDBLPMBA=LPMBPTR;             /* INIT LPMB ADDR IN EDB ENTRY   */ 01586000
         ST    LPMBPTR,EDBLPMBA(,EDBPTR)                           0550 01587000
*     EDBM=I-ONE;                   /* EDB EXTENT NBR (BASE ZERO)    */ 01588000
         SLR   @12,@12                                             0551 01589000
         IC    @12,I                                               0551 01590000
         BCTR  @12,0                                               0551 01591000
         STC   @12,EDBM(,EDBPTR)                                   0551 01592000
*     EDBSTTRK=CTCCBEG*LPMTPC+CTHHBEG;/* STARTING TRACK OF EXTENT    */ 01593000
         ICM   @12,12,CTCCBEG(WRKPTR)                              0552 01594000
         SRA   @12,16                                              0552 01595000
         MH    @12,LPMTPC(,LPMBPTR)                                0552 01596000
         ICM   @15,12,CTHHBEG(WRKPTR)                              0552 01597000
         SRA   @15,16                                              0552 01598000
         ALR   @12,@15                                             0552 01599000
         ST    @12,EDBSTTRK(,EDBPTR)                               0552 01600000
*     EDBLORBA=LOEDBVAL;            /* TOP OF LAST EXTENT            */ 01601000
         ST    LOEDBVAL,EDBLORBA(,EDBPTR)                          0553 01602000
*     EDBHIRBA=EDBLORBA+(CTRBAEND-CTRBABEG);/* INIT HIGH RBA         */ 01603000
         L     @12,EDBLORBA(,EDBPTR)                               0554 01604000
         ICM   @15,15,CTRBAEND(WRKPTR)                             0554 01605000
         ICM   @14,15,CTRBABEG(WRKPTR)                             0554 01606000
         SLR   @15,@14                                             0554 01607000
         ALR   @12,@15                                             0554 01608000
         ST    @12,EDBHIRBA(,EDBPTR)                               0554 01609000
*     LOEDBVAL=EDBHIRBA+ONE;        /* NEW LO VALUE FOR NEXT ENTRY   */ 01610000
         AL    @12,@CF00036                                        0555 01611000
         LR    LOEDBVAL,@12                                        0555 01612000
*     IF TRACKOVL=YES THEN          /* TRACK OVERFLOW DEVICE ?       */ 01613000
         L     @12,FPL5DPTR                                        0556 01614000
         TM    TRACKOVL(@12),B'00000001'                           0556 01615000
         BNO   @RF00556                                            0556 01616000
*       EDBTOFLW=ON;                /* TRACK OVERFLOW FLAG           */ 01617000
         OI    EDBTOFLW(EDBPTR),B'01000000'                        0557 01618000
*     ELSE                                                         0558 01619000
*       ;                                                          0558 01620000
@RF00556 DS    0H                                                  0559 01621000
*     EDBPSDS=ON;                   /* EDB DESCRIBES A PAGE SPACE    */ 01622000
         OI    EDBPSDS(EDBPTR),B'00100000'                         0559 01623000
*     ENTUSEDF(K)=ON;               /* LOCATE LIST ENTRY USED        */ 01624000
         SLR   @12,@12                                             0560 01625000
         IC    @12,K                                               0560 01626000
         LA    @06,ENTUSEDF-1(@12)                                 0560 01627000
         OI    0(@06),B'10000000'                                  0560 01628000
*     EDBPTR=EDBPTR+EDBLGH;         /* ADDR OF NEXT EDB ENTRY        */ 01629000
         SLR   @12,@12                                             0561 01630000
         IC    @12,EDBLGH                                          0561 01631000
         ALR   EDBPTR,@12                                          0561 01632000
*   END;                                                           0562 01633000
         LA    @12,1                                               0562 01634000
         SLR   @06,@06                                             0562 01635000
         IC    @06,I                                               0562 01636000
         ALR   @12,@06                                             0562 01637000
@DE00538 STC   @12,I                                               0562 01638000
         SLR   @06,@06                                             0562 01639000
         IC    @06,EDBNO(,EDBHDPTR)                                0562 01640000
         CR    @12,@06                                             0562 01641000
         BNH   @DL00538                                            0562 01642000
*   HARBA=LOEDBVAL;                 /* INSURE HIRBA VALUES MATCH     */ 01643000
         L     @12,FPL4DPTR                                        0563 01644000
         STCM  LOEDBVAL,15,HARBA(@12)                              0563 01645000
*   SLOTNBR=HARBA/PAGESIZE;         /* SET SLOT NUMBER FOR PAGE/SWAP    01646000
*                                      SPACE IN OUTPUT PARM          */ 01647000
         L     @12,FPL4DPTR                                        0564 01648000
         ICM   @10,15,HARBA(@12)                                   0564 01649000
         SRL   @10,12                                              0564 01650000
         ST    @10,SLOTNBR(,PARMPTR)                               0564 01651000
*                                                                  0565 01652000
*/********************************************************************/ 01653000
*/*                                                                  */ 01654000
*/*   ENDOPS00:                                                      */ 01655000
*/*      RESTORE CALLER REGISTERS AND RETURN TO CALLER               */ 01656000
*/*                                                                  */ 01657000
*/********************************************************************/ 01658000
*                                                                  0565 01659000
*OPSEXIT:                                                          0565 01660000
*   RETURN CODE(RTCODE);            /* RETURN TO CALLER              */ 01661000
OPSEXIT  L     @12,RTCODE                                          0565 01662000
         L     @13,4(,@13)                                         0565 01663000
         L     @00,@SIZDATD                                        0565 01664000
         LR    @01,@09                                             0565 01665000
         FREEMAIN R,LV=(0),A=(1)                                        01666000
         LR    @15,@12                                             0565 01667000
         L     @14,12(,@13)                                        0565 01668000
         LM    @00,@12,20(@13)                                     0565 01669000
         BR    @14                                                 0565 01670000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 01671000
*     GPR00F FIXED(31) REG(0),                                     0566 01672000
*     GPR01F FIXED(31) REG(1),                                     0566 01673000
*     GPR15F FIXED(31) REG(15);                                    0566 01674000
*   DECLARE                         /* COMMON VARIABLES              */ 01675000
*     I256C CHAR(256) BASED,                                       0567 01676000
*     I031F FIXED(31) BASED,                                       0567 01677000
*     I031P PTR(31) BASED,                                         0567 01678000
*     I015F FIXED(15) BASED,                                       0567 01679000
*     I015P PTR(15) BASED,                                         0567 01680000
*     I008P PTR(8) BASED,                                          0567 01681000
*     I001C CHAR(1) BASED;                                         0567 01682000
*                                                                  0568 01683000
*/********************************************************************/ 01684000
*/*                                                                  */ 01685000
*/* INITIOSB:  PROCEDURE TO INITIALIZE IOSB-SRB PAIR ASSOCIATED      */ 01686000
*/*            WITH THE I/O REQUEST BLOCK(IORB).                     */ 01687000
*/*                                                                  */ 01688000
*/* INPUT:     POINTER TO END OF USED STORAGE                        */ 01689000
*/*            POINTER TO IORB                                       */ 01690000
*/*            CATALOG INFORMATION                                   */ 01691000
*/*            PSAAOLD  (ASCB OLD PTR IN PSA)                        */ 01692000
*/*            ASMVT    (FOR I/O TERMINATION,NORMAL,ABNORMAL ADDRESS)*/ 01693000
*/*                                                                  */ 01694000
*/* PROCESS:   INITIALIZE A IOSB-SRB IN THE NEXT AVAILABLE AREA OF   */ 01695000
*/*            STORAGE. GENERALLY THIS ENTAILS MOVING DATA FROM THE  */ 01696000
*/*            INPUT SOURCES TO THE IOSB OR SRB.                     */ 01697000
*/*                                                                  */ 01698000
*/* OUTPUT:    AN INITIALIZED IOSB-SRB PAIR                          */ 01699000
*/*                                                                  */ 01700000
*/********************************************************************/ 01701000
*/*INITIOSB: CHART                                                   */ 01702000
*/*INITIOSB: E    INITIALIZE THE IOSB                                */ 01703000
*/*          P    SAVE RETURN REGISTER                               */ 01704000
*/*          P    INIT IOSBPTR TO NEXT AVAILABLE STORAGE             */ 01705000
*/*          P    INIT IOSB DRIVER ID FIELD                          */ 01706000
*/*          P    INIT IOSB FILE MASK                                */ 01707000
*/*          P    INIT ASCB ADDRESS-MASTER SCHEDULER                 */ 01708000
*/*          P    INIT IOSB ASID                                     */ 01709000
*/*          P    INIT IORB POINTER IN IOSB                          */ 01710000
*/*          P    INIT I/O TERMINATION ADDRESS                       */ 01711000
*/*          P    INIT POINTER TO NORMAL END APPENDAGE               */ 01712000
*/*          P    INIT POINTER TO ABNORMAL END APPENDAGE             */ 01713000
*/*          P    INIT IOS DIE ADDRESS                               */ 01714000
*/*          P    INIT ASYNCHRONOUS SCHEDULING FLAG                  */ 01715000
*/*          P    SET POST ON CONDITION CODE 3 FLAG                  */ 01716000
*/*          P    INDICATE POST STATUS TO RUN WITHOUT LOCAL LOCK     */ 01717000
*/*          P    UPDATE POINTER TO NEXT AVAILABLE STORAGE           */ 01718000
*/*          P    INIT INTERNAL SRB POINTER                          */ 01719000
*/*          P    INIT SRB ADDRESS IN IOSB                           */ 01720000
*/*          N    INITIALIZE SRB FIELDS                              */ 01721000
*/*          P    INITIALIZE SRB IDENTIFIER                          */ 01722000
*/*          P    SET SRB PARAMETER TO IOSB POINTER                  */ 01723000
*/*          P    SET SRB PRIORITY LEVEL (LOCAL)                     */ 01724000
*/*          P    UPDATE POINTER TO NEXT AVAILABLE STORAGE           */ 01725000
*/*          P    RESTORE RETURN REGISTER                            */ 01726000
*/*          R    RETURN TO INITIORB                                 */ 01727000
*/*INITIOSB: END                                                     */ 01728000
*                                                                  0568 01729000
*INITIOSB:                          /* INITIALIZE IOSB               */ 01730000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0568 01731000
INITIOSB DS    0H                                                  0569 01732000
*   RFY                                                            0569 01733000
*     R14 RSTD;                                                    0569 01734000
*   SAVEPTR=R14;                    /* SAVE REG 14                   */ 01735000
         LR    SAVEPTR,R14                                         0570 01736000
*   RFY                                                            0571 01737000
*     R14 UNRSTD;                                                  0571 01738000
*   IOSBPTR=COREADDR;               /* NEXT AVAILABLE STORAGE        */ 01739000
*                                                                  0572 01740000
         LR    IOSBPTR,COREADDR                                    0572 01741000
*   /*****************************************************************/ 01742000
*   /*                                                               */ 01743000
*   /* INITIALIZE IOSB FIELDS F0R CURRENT IORB.                      */ 01744000
*   /*                                                               */ 01745000
*   /*****************************************************************/ 01746000
*                                                                  0573 01747000
*   IOSDVRID=IOSMISID;              /* MISCELLANEOUS DRVR ID('01')   */ 01748000
         MVI   IOSDVRID(IOSBPTR),X'01'                             0573 01749000
*   IOSFMSK=FILEMASK;               /* PAGE SPACE FILE MASK          */ 01750000
         MVC   IOSFMSK(1,IOSBPTR),FILEMASK                         0574 01751000
*   ASCBPTR=PSAAOLD;                /* SET ADDRESSABILITY TO ASCB    */ 01752000
         L     ASCBPTR,PSAAOLD                                     0575 01753000
*   IOSASID=ASCBASID;               /* ASM'S ASID                    */ 01754000
         MVC   IOSASID(2,IOSBPTR),ASCBASID(ASCBPTR)                0576 01755000
*   IOSUSE=IORBPTR;                 /* IORB ADDRESS                  */ 01756000
         ST    IORBPTR,IOSUSE(,IOSBPTR)                            0577 01757000
*   IOSPGAD=ASMPCMP;                /* I/O TERMINATION ADDRESS       */ 01758000
         L     @07,ASMVTPTR                                        0578 01759000
         L     @15,ASMPCMP(,@07)                                   0578 01760000
         ST    @15,IOSPGAD(,IOSBPTR)                               0578 01761000
*   IOSNRM=ASMPCMPN;                /* NORMAL END APPENDAGE          */ 01762000
         L     @15,ASMPCMPN(,@07)                                  0579 01763000
         ST    @15,IOSNRM(,IOSBPTR)                                0579 01764000
*   IOSABN=ASMPCMPA;                /* ABNORMAL END APPENDAGE        */ 01765000
         L     @15,ASMPCMPA(,@07)                                  0580 01766000
         ST    @15,IOSABN(,IOSBPTR)                                0580 01767000
*   IOSIDR=ON;                      /* ASYNCHRONOUS SCHEDULE FLAG    */ 01768000
         OI    IOSIDR(IOSBPTR),B'00000100'                         0581 01769000
*   IOSCC3WE=ON;                    /* CC3 POST FLAG                 */ 01770000
         OI    IOSCC3WE(IOSBPTR),B'00100000'                       0582 01771000
*   IOSOPT=NOLOCLK;                 /* NO LOCAL LOCK INDICATOR       */ 01772000
         MVC   IOSOPT(1,IOSBPTR),NOLOCLK                           0583 01773000
*   IOSDIE=ASMPCMPD;                /* SET IOS DIE ADDRESS           */ 01774000
         L     @07,ASMPCMPD(,@07)                                  0584 01775000
         ST    @07,IOSDIE(,IOSBPTR)                                0584 01776000
*   IOSNOTRS=ON;                    /* SET NOTRAS FLAG       @ZA25907*/ 01777000
         OI    IOSNOTRS(IOSBPTR),B'00100000'                       0585 01778000
*   COREADDR=COREADDR+IOSBLGH;      /* UPDATE FREE STG ADDRESS       */ 01779000
         SLR   @07,@07                                             0586 01780000
         IC    @07,IOSBLGH                                         0586 01781000
         ALR   COREADDR,@07                                        0586 01782000
*   SRBPTR=COREADDR;                /* NEXT AVAILABLE STORAGE        */ 01783000
         LR    SRBPTR,COREADDR                                     0587 01784000
*   IOSSRB=SRBPTR;                  /* SRB ADDRESS INTO IOSB         */ 01785000
*                                                                  0588 01786000
         ST    SRBPTR,IOSSRB(,IOSBPTR)                             0588 01787000
*   /*****************************************************************/ 01788000
*   /*                                                               */ 01789000
*   /* INITIALIZE SRB FIELDS F0R CURRENT IORB-IOSB.                  */ 01790000
*   /*                                                               */ 01791000
*   /*****************************************************************/ 01792000
*                                                                  0589 01793000
*   SRBID=SRBIDC;                   /* SRB IDENTIFIER FIELD          */ 01794000
         MVC   SRBID(4,SRBPTR),SRBIDC                              0589 01795000
*   SRBPARM=IOSBPTR;                /* ASSOCIATED IOSB AS PARM       */ 01796000
         ST    IOSBPTR,SRBPARM(,SRBPTR)                            0590 01797000
*   SRBPRIOR=NQUISCFG;              /* NON-QUIESCEABLE PRIORITY      */ 01798000
         IC    @07,NQUISCFG                                        0591 01799000
         STC   @07,SRBPRIOR(,SRBPTR)                               0591 01800000
*   COREADDR=COREADDR+SRBLGH;       /* UPDATE FREE STG ADDRESS       */ 01801000
         SLR   @04,@04                                             0592 01802000
         IC    @04,SRBLGH                                          0592 01803000
         ALR   COREADDR,@04                                        0592 01804000
*   RFY                                                            0593 01805000
*     R14 RSTD;                                                    0593 01806000
*   R14=SAVEPTR;                    /* RESTORE REG 14                */ 01807000
         LR    R14,SAVEPTR                                         0594 01808000
*   RFY                                                            0595 01809000
*     R14 UNRSTD;                                                  0595 01810000
*   END INITIOSB;                                                  0596 01811000
@EL00002 DS    0H                                                  0596 01812000
@EF00002 DS    0H                                                  0596 01813000
@ER00002 BR    @14                                                 0596 01814000
*   END ILROPS00                                                   0597 01815000
