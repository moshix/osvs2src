*             END;                                                 0452 01078000
*           ELSE                                                   0453 01079000
*             RTCODE=NORCBSTG;      /* NO NUCLEUR CORE AVAILABLE     */ 01080000
         B     @RC00443                                            0453 01081000
@RF00443 MVC   RTCODE(4),@CF00362                                  0453 01082000
*         END;                      /* END OF OBTAIN NUCLEUS BUFFER  */ 01083000
@RC00443 DS    0H                                                  0455 01084000
*     END;                          /* END OF PROCESS CORRECT DEVICE    01085000
*                                      TYPE                          */ 01086000
@RF00439 DS    0H                                                  0456 01087000
