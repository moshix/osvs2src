*           MVCL(R0,R14);           /* MOVE ZERO TO CLEAR BUFFER AREA*/ 01006000
         MVCL  R0,R14                                              0435 01007000
*         END;                                                     0436 01008000
*       ELSE                                                       0437 01009000
*         RTCODE=NOVCBSTG;          /* SET NO SPACE IN SQA ERROR     */ 01010000
         B     @RC00428                                            0437 01011000
@RF00428 MVC   RTCODE(4),@CF00107                                  0437 01012000
*       RFY                                                        0438 01013000
*        (R0,                                                      0438 01014000
*         R1,                                                      0438 01015000
