         TITLE 'ILROPS00:  ASM OPEN ROUTINE                            *00001000
                        '                                               00002000
ILROPS00 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILROPS00  77.272'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@11                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @09,@01                                             0001 00016000
         USING @DATD,@09                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @09,8(,@13)                                         0001 00020000
         LR    @13,@09                                             0001 00021000
*                                                                       00022000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00023000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00024000
* WILL ASSEMBLE PROPERLY.                                               00025000
JSCBDISP EQU 188                                                        00026000
JSCBAOS1 EQU  72                                                        00027000
JSCBAOS2 EQU 192                                                        00028000
*   PARMPTR=RPARM;                  /* SAVE PARAMETER LIST POINTER   */ 00029000
         LR    PARMPTR,RPARM                                       0228 00030000
*   RFY                                                            0229 00031000
*     RPARM UNRSTD;                                                0229 00032000
*   SVDSNAME=DSNAME;                /* SAVE INPUT DSNAME             */ 00033000
         MVC   SVDSNAME(44),DSNAME(PARMPTR)                        0230 00034000
*   OUDSNAM=''B;                    /* ZERO OUTPUT DSNAME FIELD FOR     00035000
*                                      ILRPGEXP ESTAE                */ 00036000
         XC    OUDSNAM(44,PARMPTR),OUDSNAM(PARMPTR)                0231 00037000
*   RTCODE=SUCCESS;                 /* INIT RETURN CODE FOR CALLER   */ 00038000
         SLR   @15,@15                                             0232 00039000
         ST    @15,RTCODE                                          0232 00040000
*   IF AFTERNIP=ON THEN             /* PROCESSING AFTER NIPTIME      */ 00041000
         TM    AFTERNIP(PARMPTR),B'01000000'                       0233 00042000
         BNO   @RF00233                                            0233 00043000
*     CVTPTR=FLCCVT;                /* SET CVT PTR                   */ 00044000
         L     CVTPTR,FLCCVT                                       0234 00045000
*   ASMVTPTR=CVTASMVT;              /* INIT ASMVT PTR                */ 00046000
@RF00233 L     @15,CVTASMVT(,CVTPTR)                               0235 00047000
         ST    @15,ASMVTPTR                                        0235 00048000
*                                                                  0236 00049000
*/********************************************************************/ 00050000
*/*                                                                  */ 00051000
*/* LOCPREP:   SECTION  TO PREPARE THE CATALOG LOCATE PARAMETER      */ 00052000
*/*            LISTS.                                                */ 00053000
*/*                                                                  */ 00054000
*/* INPUT:     DATA SET NAME TO BE OPENED                            */ 00055000
*/*                                                                  */ 00056000
*/* PROCESS:   ESSENTIAL CONSTANT FIELDS IN THE CATALOG PARAMETER    */ 00057000
*/*            LIST (CTGPL) AND CATALOG FIELD PARAMETER LISTS        */ 00058000
*/*            (CTGFLS) ARE INITIALIZED FOR BOTH THE CLUSTER         */ 00059000
*/*            LOCATE ON DATA SET  NAME AND THE DATA OBJECT          */ 00060000
*/*            LOCATE ON DATA CONTROL INTERVAL NUMBER.               */ 00061000
*/*                                                                  */ 00062000
*/* OUTPUT:    INITIALIZED CATALOG LOCATE PARAMETER LISTS            */ 00063000
*/*                                                                  */ 00064000
*/********************************************************************/ 00065000
*/*LOCPREP:    CHART                                                 */ 00066000
*/*LOCPREP:  N    SECTION TO PREPARE LOCATE PARM LISTS               */ 00067000
*/*          N    INITIALIZE CATALOG PARM LIST FOR DATA CI LOCATE    */ 00068000
*/*          P    INITIALIZE WORK AREA LENGTH IN REPLY AREA 2        */ 00069000
*/*          P    INIT ADDRESS OF  FIELD PARAMETER ENTRIES           */ 00070000
*/*          P    INDICATE PASSWORD CHECKING NOT WANTED              */ 00071000
*/*          P    INDICATE CATALOG NAME FIELD IS ZERO                */ 00072000
*/*          P    INDICATE LOCATE REQUEST                            */ 00073000
*/*          P    INDICATE VSAM REQUEST                              */ 00074000
*/*          P    SET ADDRESS OF LOCATE REPLY AREA                   */ 00075000
*/*          P    INITIALIZE COUNT OF FIELD PARAMETER LISTS          */ 00076000
*/*          N    INITIALIZE CATALOG PARM LIST FOR CLUSTER LOCATE    */ 00077000
*/*          P    INITIALIZE WORK AREA LENGTH IN REPLY AREA 1        */ 00078000
*/*          P    INIT ADDRESS OF  FIELD PARAMETER ENTRIES           */ 00079000
*/*          P    COPY CATALOG PARM LIST INITIALIZED ABOVE           */ 00080000
*/*          P    COPY DSNAME INTO DSNAME WORK AREA                  */ 00081000
*/*          P    INDICATE LOCATE ON DATA SET NAME                   */ 00082000
*/*          P    SET ADDRESS OF DATA SET NAME FIELD                 */ 00083000
*/*          P    SET ADDRESS OF LOCATE REPLY AREA                   */ 00084000
*/*          P    INITIALIZE COUNT OF FIELD PARAMETER LISTS          */ 00085000
*/*          N    INITIALIZE FIELD PARAMETER LISTS NOW...            */ 00086000
*/*          P    SET NAMEDS COMBINATION TO BE LOCATED IN FIRST    0236 00087000
*/*               FIELD PARM LIST                                    */ 00088000
*/*          P    SET CATTR  TO BE LOCATED IN SECOND FIELD PARM LIST */ 00089000
*/*          P    SET VOLDEV COMBINATION TO BE LOCATED IN THIRD    0236 00090000
*/*               FIELD PARM LIST                                    */ 00091000
*/*          P    SET VOLPHY COMBINATION TO BE LOCATED IN FOURTH   0236 00092000
*/*               FIELD PARM LIST                                    */ 00093000
*/*          P    SET DSATTR COMBINATION TO BE LOCATED IN FIFTH    0236 00094000
*/*               FIELD PARM LIST                                    */ 00095000
*/*          P    SET AMDSB  TO BE LOCATED IN SIXTH FIELD PARM LIST  */ 00096000
*/*          R    END OF LOCATE PREPARATION SECTION                  */ 00097000
*/*LOCPREP:  END                                                     */ 00098000
*/*  LOCPREP-PREPARE LOCATE PARM LIST                                */ 00099000
*/*  CLEAR CORE FOR CATALOG PARAMETER LISTS                          */ 00100000
*                                                                  0236 00101000
*   RFY                                                            0236 00102000
*    (R0,                                                          0236 00103000
*     R1,                                                          0236 00104000
*     R14,                                                         0236 00105000
*     R15) RSTD;                    /* RESERVE REGISTERS             */ 00106000
*   R0=ADDR(ZEROLIST);              /* ADDRESS OF CORE TO CLEAR      */ 00107000
         LA    R0,ZEROLIST                                         0237 00108000
*   R1=LENGTH(ZEROLIST);            /* LENGTH OF CORE TO CLEAR       */ 00109000
         LA    R1,496                                              0238 00110000
*   R15=ZERO;                       /* PADDING CHAR OF ZERO          */ 00111000
         SLR   R15,R15                                             0239 00112000
*   MVCL(R0,R14);                   /* MOVE ZERO TO PARM LIST        */ 00113000
         MVCL  R0,R14                                              0240 00114000
*   RFY                                                            0241 00115000
*    (R0,                                                          0241 00116000
*     R1,                                                          0241 00117000
*     R14,                                                         0241 00118000
*     R15) UNRSTD;                  /* RELEASE REGISTERS             */ 00119000
*                                                                  0241 00120000
*   /*****************************************************************/ 00121000
*   /*                                                               */ 00122000
*   /* INITIALIZE CATALOG PARAMETER LIST (CTGPL) FOR DATA CI LOCATE  */ 00123000
*   /*                                                               */ 00124000
*   /*****************************************************************/ 00125000
*                                                                  0242 00126000
*   RFY                                                            0242 00127000
*     CTGPL BASED(ADDR(RIMCPL02));  /* ESTAB CATALOG PARM LIST     0242 00128000
*                                      ADDRESSABILITY                */ 00129000
*   CWA2LEN=LENGTH(CWKAREA2);       /* INIT WORK AREA LENGTH         */ 00130000
         MVC   CWA2LEN(2),@CH03830                                 0243 00131000
*   FPL3PTR=ADDR(FPL3);             /* ADDR OF 3RD FIELD PARM        */ 00132000
         LA    @15,FPL3                                            0244 00133000
         ST    @15,FPL3PTR                                         0244 00134000
*   FPL4PTR=ADDR(FPL4);             /* ADDR OF 4TH FIELD PARM        */ 00135000
         LA    @14,FPL4                                            0245 00136000
         ST    @14,FPL4PTR                                         0245 00137000
*   FPL5PTR=ADDR(FPL5);             /* ADDR OF 5TH FIELD PARM        */ 00138000
         LA    @07,FPL5                                            0246 00139000
         ST    @07,FPL5PTR                                         0246 00140000
*   FPL6PTR=ADDR(FPL6);             /* ADDR OF 6TH FIELD PARM        */ 00141000
         LA    @06,FPL6                                            0247 00142000
         ST    @06,FPL6PTR                                         0247 00143000
*   CTGBYPSS=YES;                   /* INDICATE NO PASSWORD CHECK    */ 00144000
*   CTGCNAME=YES;                   /* INDICATE CATALOG NAME FIELD IS   00145000
*                                      ZERO                          */ 00146000
         LA    @05,RIMCPL02                                        0249 00147000
         OI    CTGBYPSS(@05),B'10000010'                           0249 00148000
*   CTGFUNC=CTGLOC;                 /* INDICATE LOCATE REQUEST       */ 00149000
         NI    CTGFUNC(@05),B'00111111'                            0250 00150000
         OI    CTGFUNC(@05),B'00100000'                            0250 00151000
*   CTGAM0=YES;                     /* INDICATE VSAM REQUEST         */ 00152000
         OI    CTGAM0(@05),B'00000001'                             0251 00153000
*   CTGWKA=ADDR(CWKAREA2);          /* SET ADDR OF LOCATE REPLY AREA */ 00154000
         LA    @04,CWKAREA2                                        0252 00155000
         ST    @04,CTGWKA(,@05)                                    0252 00156000
*   CTGNOFLD=FPL2CTR;               /* INDICATE FOUR FIELD PARM      */ 00157000
*                                                                  0253 00158000
         MVI   CTGNOFLD(@05),X'04'                                 0253 00159000
*   /*****************************************************************/ 00160000
*   /*                                                               */ 00161000
*   /* INITIALIZE CATALOG PARAMETER LIST (CTGPL) FOR CLUSTER LOCATE  */ 00162000
*   /*                                                               */ 00163000
*   /*****************************************************************/ 00164000
*                                                                  0254 00165000
*   RFY                                                            0254 00166000
*     CTGPL BASED(ADDR(RIMCPL01));  /* ESTAB CATALOG PARM LIST     0254 00167000
*                                      ADDRESSABILITY                */ 00168000
*   CWA1LEN=LENGTH(CWKAREA1);       /* INIT WORK AREA LENGTH         */ 00169000
         MVC   CWA1LEN(2),@CH00362                                 0255 00170000
*   FPL1PTR=ADDR(FPL1);             /* ADDR OF 1ST FIELD PARM        */ 00171000
         LA    @05,FPL1                                            0256 00172000
         ST    @05,FPL1PTR                                         0256 00173000
*   FPL2PTR=ADDR(FPL2);             /* ADDR OF 2ND FIELD PARM        */ 00174000
         LA    @04,FPL2                                            0257 00175000
         ST    @04,FPL2PTR                                         0257 00176000
*   CPL1FXD=CPL2FXD;                /* COPY PARMLIST JUST INITLZED   */ 00177000
         MVC   CPL1FXD(28),CPL2FXD                                 0258 00178000
*   WKDSNAME=SVDSNAME;              /* GET PAGE OR SWAP DSNAME       */ 00179000
         MVC   WKDSNAME(44),SVDSNAME                               0259 00180000
*   CTGNAME=YES;                    /* INDICATE LOCATE ON DSNAME     */ 00181000
         LA    @01,RIMCPL01                                        0260 00182000
         OI    CTGNAME(@01),B'00000100'                            0260 00183000
*   CTGENT=ADDR(WKDSNAME);          /* SET ADDR OF DSNAME FIELD      */ 00184000
         LA    @00,WKDSNAME                                        0261 00185000
         ST    @00,CTGENT(,@01)                                    0261 00186000
*   CTGWKA=ADDR(CWKAREA1);          /* SET ADDR OF LOCATE REPLY AREA */ 00187000
         LA    @00,CWKAREA1                                        0262 00188000
         ST    @00,CTGWKA(,@01)                                    0262 00189000
*   CTGNOFLD=FPLCTR;                /* INDICATE TWO FIELD PARM       */ 00190000
*                                                                  0263 00191000
         MVI   CTGNOFLD(@01),X'02'                                 0263 00192000
*   /*****************************************************************/ 00193000
*   /*                                                               */ 00194000
*   /* INITIALIZE THE FIELD PARAMETER LISTS                          */ 00195000
*   /*                                                               */ 00196000
*   /*****************************************************************/ 00197000
*                                                                  0264 00198000
*   FPL1PTR->CTGFLDNM=ADDR(NAMEDSC);/* SET NAMEDS TO BE LOCATED      */ 00199000
         LA    @00,NAMEDSC                                         0264 00200000
         ST    @00,CTGFLDNM(,@05)                                  0264 00201000
*   FPL1PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */ 00202000
         MVI   CTGFLDNO(@05),X'01'                                 0265 00203000
*   FPL2PTR->CTGFLDNM=ADDR(CATTRC); /* SET CATTR TO BE LOCATED       */ 00204000
         LA    @05,CATTRC                                          0266 00205000
         ST    @05,CTGFLDNM(,@04)                                  0266 00206000
*   FPL2PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */ 00207000
         MVI   CTGFLDNO(@04),X'01'                                 0267 00208000
*   FPL3PTR->CTGFLDNM=ADDR(TODEVC); /* SET TOVOLDEV TO BE LOCATED    */ 00209000
         LA    @00,TODEVC                                          0268 00210000
         ST    @00,CTGFLDNM(,@15)                                  0268 00211000
*   FPL3PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */ 00212000
         MVI   CTGFLDNO(@15),X'01'                                 0269 00213000
*   FPL4PTR->CTGFLDNM=ADDR(VOLPHYC);/* SET VOLPHY TO BE LOCATED      */ 00214000
         LA    @15,VOLPHYC                                         0270 00215000
         ST    @15,CTGFLDNM(,@14)                                  0270 00216000
*   FPL4PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */ 00217000
         MVI   CTGFLDNO(@14),X'01'                                 0271 00218000
*   FPL5PTR->CTGFLDNM=ADDR(DSATTRC);/* SET DSATTR TO BE LOCATED      */ 00219000
         LA    @00,DSATTRC                                         0272 00220000
         ST    @00,CTGFLDNM(,@07)                                  0272 00221000
*   FPL5PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */ 00222000
         MVI   CTGFLDNO(@07),X'01'                                 0273 00223000
*   FPL6PTR->CTGFLDNM=ADDR(AMDSBC); /* SET AMDSB TO BE LOCATED       */ 00224000
         LA    @07,AMDSBC                                          0274 00225000
         ST    @07,CTGFLDNM(,@06)                                  0274 00226000
*   FPL6PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */ 00227000
         MVI   CTGFLDNO(@06),X'01'                                 0275 00228000
*   IF PAGESP=ON THEN               /* INPUT IS A PAGE SPACE?        */ 00229000
         TM    PAGESP(PARMPTR),B'10000000'                         0276 00230000
         BNO   @RF00276                                            0276 00231000
*                                                                  0277 00232000
*/********************************************************************/ 00233000
*/*  LOCPAGE: ISSUE LOCATES FOR A SINGLE PAGE SPACE                  */ 00234000
*/*                                                                  */ 00235000
*/*  INPUT: CATALOG PARAMETER LISTS AND FIELD PARAMETER LISTS ARE    */ 00236000
*/*         PREVIOUSLY SET UP BY 'LOCPREP'.                          */ 00237000
*/*                                                                  */ 00238000
*/*  PROCESS: AN ATTEMPT IS MADE TO LOCATE A PAGING SPACE NAMED      */ 00239000
*/*           IN THE INPUT PARAMETER LIST.                           */ 00240000
*/*                                                                  */ 00241000
*/*           FIRST LOCATE CLUSTER BY NAME AND VERIFY IT IS A PAGE   */ 00242000
*/*           SPACE. IF IT IS SUCCESSFUL,A SECOND LOCATE IS ISSUED   */ 00243000
*/*           ON THE DATA CONTROL INTERVAL NUMBER TO GET THE PAGE    */ 00244000
*/*           SPACE  VOLUME SERIAL,DEVICE TYPE,DSATTR AND AMDSB.     */ 00245000
*/*                                                                  */ 00246000
*/*           IF EITHER LOCATE IS UNSUCCESSFUL,OR THE DATA SET IS    */ 00247000
*/*           NOT A VALID PAGE SPACE A NONZERO RETURN CODE WILL BE   */ 00248000
*/*           RETURNED TO THE CALLER.PROCESSING IS TERMINATED.       */ 00249000
*/*                                                                  */ 00250000
*/*  OUTPUT:  THE REQUIRED INFORMATION TO OPEN  A  PAGE SPACE        */ 00251000
*/*                                                                  */ 00252000
*/********************************************************************/ 00253000
*/*LOCPAGE:    CHART                                                 */ 00254000
*/*            N    SECTION TO ISSUE LOCATE FOR A PAGE SPACE         */ 00255000
*/*            P    SET ADDRESS OF CATALOG PARM LIST IN REG 1        */ 00256000
*/*            L    SVC26: LOCATE CLUSTER BY DSNAME                  */ 00257000
*/*            D    (YES,,NO,%LOCFAIL) LOCATE CLUSTER SUCCESSFUL?    */ 00258000
*/*            D    (YES,,NO,%LOCFAIL) DSNAME NOT IN CVOL?           */ 00259000
*/*            D    (YES,,NO,%LOCFAIL) DATA SET REALLY A PAGE SPACE? */ 00260000
*/*            D  (YES,,NO,%LOCFAIL) DATASET HAS ONLY A 'DATA' TYPE? */ 00261000
*/*            P  SET ADDR OF DATA CI IN CATLOG PARM LIST            */ 00262000
*/*            L    SVC26:LOCATE INFO BY CI NUMBER                   */ 00263000
*/*            D    (YES,,NO,%LOCFAIL) LOCATE INFO SUCCESSFUL?       */ 00264000
*/*            D     (YES,%LOCFAIL,NO,) NOT USABLE FLAG ON?          */ 00265000
*/*            R    END OF LOCATE PAGE SECTION                       */ 00266000
*/*%LOCFAIL:   P    SET RETURN CODE(12)-LOCATE FAIL                  */ 00267000
*/*            R    RETURN TO CALLER                                 */ 00268000
*/*LOCPAGE:    END                                                   */ 00269000
*                                                                  0277 00270000
*                                                                  0277 00271000
*/*LOCPGSP-LOCATE PAGE SPACE                                         */ 00272000
*                                                                  0277 00273000
*     DO;                                                          0277 00274000
*       RFY                                                        0278 00275000
*        (R1,                                                      0278 00276000
*         R15) RSTD;                                               0278 00277000
*                                                                  0278 00278000
*       /*************************************************************/ 00279000
*       /*                                                           */ 00280000
*       /* SET UP FOR CLUSTER LOCATE (GET DATA CI NUMBER AND ENTRY   */ 00281000
*       /* TYPE)                                                     */ 00282000
*       /*                                                           */ 00283000
*       /*************************************************************/ 00284000
*                                                                  0279 00285000
*       R1=ADDR(RIMCPL01);          /* ADDR OF CATALOG PARM LIST     */ 00286000
         LA    @14,RIMCPL01                                        0279 00287000
         LR    R1,@14                                              0279 00288000
*       SVC(26);                    /* LOCATE THE PAGING SPACE       */ 00289000
*                                                                  0280 00290000
         SVC   26                                                  0280 00291000
*       /*************************************************************/ 00292000
*       /*                                                           */ 00293000
*       /* IF THE CLUSTER LOCATE WAS SUCCESSFUL, VERIFY THAT THE     */ 00294000
*       /* DATASET IS INDEED A PAGE SPACE. ISSUE LOCATE FOR PAGE     */ 00295000
*       /* SPACE VOLUME SERIAL, DEVICE TYPE, ATTR1, AND AMDSB USING  */ 00296000
*       /* THE DATA CONTROL INTERVAL NUMBER FOUND IN THE LOCATE      */ 00297000
*       /* ABOVE. IF EITHER LOCATE FAILS, TAKE AN ERROR EXIT.        */ 00298000
*       /*                                                           */ 00299000
*       /*************************************************************/ 00300000
*                                                                  0281 00301000
*       IF R15=SUCCESS              /* CHECK 1ST LOCATE SUCCESSFUL   */ 00302000
*           &FPL1PTR->CTGFLPT(ONE)^=ZERO&FPL2PTR->CTGFLPT(ONE)^=ZERO    00303000
*                                   /* CHECK DSNAME NOT IN CVOL      */ 00304000
*           &(PSPACEDS=YES&SWPAGE=NO)&(DSTYPED=DATACHAR) THEN/* CHECK   00305000
*                                      PAGE SPACE AND CHECK DATA TYPE*/ 00306000
         SLR   @14,@14                                             0281 00307000
         CR    R15,@14                                             0281 00308000
         BNE   @RF00281                                            0281 00309000
         L     @07,FPL1PTR                                         0281 00310000
         C     @14,CTGFLPT(,@07)                                   0281 00311000
         BE    @RF00281                                            0281 00312000
         L     @07,FPL2PTR                                         0281 00313000
         C     @14,CTGFLPT(,@07)                                   0281 00314000
         BE    @RF00281                                            0281 00315000
         L     @14,FPL2DPTR                                        0281 00316000
         TM    PSPACEDS(@14),B'00000001'                           0281 00317000
         BNO   @RF00281                                            0281 00318000
         TM    SWPAGE(@14),B'00000010'                             0281 00319000
         BNZ   @RF00281                                            0281 00320000
         L     @14,FPL1DPTR                                        0281 00321000
         CLI   DSTYPED(@14),C'D'                                   0281 00322000
         BNE   @RF00281                                            0281 00323000
*         DO;                       /* FIRST LOCATE SUCCESSFUL       */ 00324000
*           RFY                                                    0283 00325000
*             CTGPL BASED(ADDR(RIMCPL02));/* SET ADDRESSABILITY      */ 00326000
*           CTGENT=ADDR(DATACI);    /* GET CI NUMBER                 */ 00327000
         LA    @07,RIMCPL02                                        0284 00328000
         LA    @14,DATACI(,@14)                                    0284 00329000
         ST    @14,CTGENT(,@07)                                    0284 00330000
*           R1=ADDR(RIMCPL02);      /* GET CATALOG PARMLIST ADDR     */ 00331000
         LR    R1,@07                                              0285 00332000
*           SVC(26);                                               0286 00333000
         SVC   26                                                  0286 00334000
*           IF R15^=SUCCESS|NOTUSE=ON THEN/* 2ND LOCATE SUCCESSFUL?  */ 00335000
         LTR   R15,R15                                             0287 00336000
         BNZ   @RT00287                                            0287 00337000
         L     @14,FPL5DPTR                                        0287 00338000
         TM    NOTUSE(@14),B'00000001'                             0287 00339000
         BNO   @RF00287                                            0287 00340000
@RT00287 DS    0H                                                  0288 00341000
*             RTCODE=LOCFAILD;      /* SET LOCATE FAILED             */ 00342000
         MVC   RTCODE(4),@CF00488                                  0288 00343000
*         END;                                                     0289 00344000
*       ELSE                                                       0290 00345000
*         RTCODE=LOCFAILD;          /* FIRST LOCATE FAILED           */ 00346000
         B     @RC00281                                            0290 00347000
@RF00281 MVC   RTCODE(4),@CF00488                                  0290 00348000
*       RFY                                                        0291 00349000
*        (R1,                                                      0291 00350000
*         R15) UNRSTD;              /* RELEASE REG1 AND REG15        */ 00351000
@RC00281 DS    0H                                                  0292 00352000
*       IF RTCODE^=SUCCESS THEN     /* A LOCATE FAILED               */ 00353000
         L     @15,RTCODE                                          0292 00354000
         LTR   @15,@15                                             0292 00355000
         BNZ   @RT00292                                            0292 00356000
*         GOTO OPSEXIT;             /* ERROR EXIT-RETURN TO CALLER   */ 00357000
*     END;                                                         0294 00358000
*   ELSE                            /* LOCATE A SWAP SPACE           */ 00359000
*                                                                  0295 00360000
*/********************************************************************/ 00361000
*/*                                                                  */ 00362000
*/*  LOCSWAP: ISSUE LOCATES FOR A SWAP DATA SET                      */ 00363000
*/*                                                                  */ 00364000
*/*  INPUT: CATALOG PARAMETER LISTS AND FIELD PARAMETER LISTS ARE    */ 00365000
*/*         PREVIOUSLY SET UP BY 'LOCPREP'.                          */ 00366000
*/*                                                                  */ 00367000
*/*  PROCESS: AN ATTEMP IS MADE TO LOCATE A SWAP DATA SET NAMED      */ 00368000
*/*           IN THE INPUT PARAMETER LIST.                           */ 00369000
*/*                                                                  */ 00370000
*/*           FIRST LOCATE CLUSTER BY NAME AND VERIFY IT IS A SWAP   */ 00371000
*/*           DATA SET. IF IT IS SUCCESSFUL,A SECOND LOCATE IS ISSUED*/ 00372000
*/*           ON THE DATA CONTROL INTERVAL NUMBER TO GET THE VOLUME  */ 00373000
*/*           SERIAL NUMBER,DEVICE TYPE,DSATTR AND AMDSB FOR THE SWAP*/ 00374000
*/*           DATA SET.                                              */ 00375000
*/*                                                                  */ 00376000
*/*           IF EITHER LOCATE IS UNSUCCESSFUL,OR THE DATA SET IS    */ 00377000
*/*           NOT A VALID SWAP SPACE A NON-ZERO RETURN CODE WILL BE  */ 00378000
*/*           RETURNED TO THE CALLER. PROCESSING IS TERMINATED       */ 00379000
*/*                                                                  */ 00380000
*/*  OUTPUT:  THE REQUIRED INFORMATION TO OPEN  A  SWAP SPACE        */ 00381000
*/*                                                                  */ 00382000
*/********************************************************************/ 00383000
*/*LOCSWAP:    CHART                                                 */ 00384000
*/*            N    SECTION TO ISSUE LOCATE FOR A SWAP SPACE         */ 00385000
*/*            P    SET ADDRESS OF CATALOG PARM LIST IN REG 1        */ 00386000
*/*            L    SVC26:LOCATE CLUSTER BY DSNAME                   */ 00387000
*/*            D    (YES,,NO,%LOSFAIL) LOCATE CLUSTER SUCCESSFUL?    */ 00388000
*/*            D    (YES,,NO,%LOSFAIL) DSNAME NOT IN CVOL?           */ 00389000
*/*            D    (YES,,NO,%LOSFAIL) DATA SET REALLY A SWAP SPACE? */ 00390000
*/*            D    (YES,,NO,%LOSFAIL) DATA SET IS A 'DATA' TYPE?    */ 00391000
*/*            P    SET ADDR OF DATA CI IN CATLOG PARM LIST          */ 00392000
*/*            L    SVC26:LOCATE INFO BY CI NUMBER                   */ 00393000
*/*            D    (YES,,NO,%LOSFAIL) LOCATE INFO SUCCESSFUL?       */ 00394000
*/*            D    (YES,,NO,%LOSFAIL)  NO TRACK OVERFLOW?           */ 00395000
*/*            D     (YES,%LOSFAIL,NO,) NOT USABLE FLAG ON?          */ 00396000
*/*            R    END OF LOCATE SWAP SECTION                       */ 00397000
*/*%LOSFAIL:   P    SET RETURN CODE(12)-LOCATE FAIL                  */ 00398000
*/*            R    RETURN TO CALLER                                 */ 00399000
*/*LOCSWAP:    END                                                   */ 00400000
*                                                                  0295 00401000
*     DO;                           /* LOCATE A SWAP SPACE           */ 00402000
         B     @RC00276                                            0295 00403000
