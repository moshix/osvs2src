         ST    @07,SQALEN(,PARMPTR)                                0432 01001000
*           R0=COREADDR;            /* SET GETMAIN ADDRESS           */ 01002000
         LR    R0,COREADDR                                         0433 01003000
*           R1=CORESIZE;            /* SET GETMAIN LENGTH            */ 01004000
