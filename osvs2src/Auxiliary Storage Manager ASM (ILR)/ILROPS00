*         GPR15F=GPR15F|((245)*256);/* SP IN BYTE 2                  */ 00969000
         O     GPR15F,@CF03836                                     0423 00970000
*         SVC(120);                 /* RC/RU-FORM OF GETMAIN         */ 00971000
         SVC   120                                                 0424 00972000
*         RESPECIFY                                                0425 00973000
*          (GPR01F,                                                0425 00974000
*           GPR15F,                                                0425 00975000
*           GPR00F) UNRESTRICTED;                                  0425 00976000
*       END;                        /* GETMAIN (RC) LV(CORESIZE)   0426 00977000
*                                      SP(245)                       */ 00978000
*                                                                  0426 00979000
*       /*************************************************************/ 00980000
*       /*                                                           */ 00981000
*       /* TEST FOR SUCCESSFUL GETMAIN                               */ 00982000
*       /*                                                           */ 00983000
*       /*************************************************************/ 00984000
*                                                                  0427 00985000
*       RFY                                                        0427 00986000
*        (R0,                                                      0427 00987000
*         R1,                                                      0427 00988000
*         R14,                                                     0427 00989000
*         R15) RSTD;                /* RESTRICT REG0,1,14,AND 15     */ 00990000
*       IF R15=SUCCESS THEN         /* GETMAIN SUCCESSFUL ?          */ 00991000
         LTR   R15,R15                                             0428 00992000
         BNZ   @RF00428                                            0428 00993000
*         DO;                       /* YES - GETMAIN SUCCESSFUL      */ 00994000
*           COREADDR=R1;            /* SAVE PTR TO SQA CORE          */ 00995000
         LR    COREADDR,R1                                         0430 00996000
*           SQAPTR=R1;              /* SAVE PTR TO SQA CORE IN OUTPUT*/ 00997000
