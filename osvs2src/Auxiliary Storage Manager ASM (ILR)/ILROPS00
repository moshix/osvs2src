         ALR   @05,@14                                             0508 01398000
         LH    @05,DEVTABTC(,@05)                                  0508 01399000
         STH   @05,LPMTPC(,LPMBPTR)                                0508 01400000
*   LPMBLKTR=NOBLKTRK;              /* BLOCKS PER TRACK              */ 01401000
         LH    @05,NOBLKTRK(,@12)                                  0509 01402000
         STH   @05,LPMBLKTR(,LPMBPTR)                              0509 01403000
*   LPMBLKSZ=PHYBLKSZ;              /* BLOCK SIZE                    */ 01404000
         L     @05,PHYBLKSZ(,@12)                                  0510 01405000
         ST    @05,LPMBLKSZ(,LPMBPTR)                              0510 01406000
*   IF TRACKOVL=YES THEN            /* TRACK OVERFLOW DEVICE ?       */ 01407000
         L     @05,FPL5DPTR                                        0511 01408000
         TM    TRACKOVL(@05),B'00000001'                           0511 01409000
         BNO   @RF00511                                            0511 01410000
*     DO;                           /* YES - TRACK OVERFLOW          */ 01411000
*       LPMAUSZ=NOBYTAU;            /* BYTES PER ALLOCATED UNIT      */ 01412000
         L     @05,NOBYTAU(,@12)                                   0513 01413000
         ST    @05,LPMAUSZ(,LPMBPTR)                               0513 01414000
*       LPMBPTRK=NOBYTTRK;          /* BYTES PER TRACK               */ 01415000
         L     @12,NOBYTTRK(,@12)                                  0514 01416000
         ST    @12,LPMBPTRK(,LPMBPTR)                              0514 01417000
*       LPMBTOFL=ON;                /* INDICATE TRACK OVERFLOW       */ 01418000
         OI    LPMBTOFL(LPMBPTR),B'00010000'                       0515 01419000
*     END;                                                         0516 01420000
*   ELSE                                                           0517 01421000
*     DO;                           /* NOT TRACK OVERFLOW DEVICE     */ 01422000
         B     @RC00511                                            0517 01423000
@RF00511 DS    0H                                                  0518 01424000
*       RFY                                                        0518 01425000
*         IDAAMDSB BASED(FPL6DPTR); /* SET PARM FOR AMDSB            */ 01426000
*       LPMAUSZ=AMDCINV*AMDCIPCA;   /* BYTES PER ALLOCATED UNIT      */ 01427000
         L     @12,FPL6DPTR                                        0519 01428000
