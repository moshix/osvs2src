*        (R1,                                                      0296 00408000
*         R15) RSTD;                /* RESTRICT REG 1 AND REG15      */ 00409000
*       R1=ADDR(RIMCPL01);          /* SET CATALOG PARM LIST         */ 00410000
         LA    R1,RIMCPL01                                         0297 00411000
*       SVC(26);                    /* ISSUE 1ST LOCATE-CLUSTER      */ 00412000
         SVC   26                                                  0298 00413000
*       IF R15=SUCCESS&FPL1PTR->CTGFLPT(ONE)^=ZERO&/* 1ST LOCATE   0299 00414000
