         L     @12,ASM3TMPA(,ASMVTPTR)                             0398 01175000
         ST    @12,ATAAIA(,ATAPTR)                                 0398 01176000
*               END;                /* SETUP TO START LSQA           */ 01177000
*           END;                    /* CHECK FOR P.A. COMPLETION     */ 01178000
@RF00393 DS    0H                                                  0401 01179000
*       END;                        /* CHECK FOR P.A. COMPLETE       */ 01180000
