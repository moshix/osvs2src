*     GO TO ENDUP;                  /* GO TO QUEUE AIA FOR PIOP      */ 01085000
         B     ENDUP                                               0379 01086000
*     RFY                                                          0380 01087000
*       XPTEPTR UNRSTD;             /* RELEASE REG FOR USE           */ 01088000
*                                                                  0381 01089000
*     /***************************************************************/ 01090000
*     /*                                                             */ 01091000
*     /* ALL CASES WHEN THE AIA MUST BE QUEUED TO THE ASMSTAGQ WILL  */ 01092000
*     /* COME THROUGH THIS COMMON PROCESSING. THE ATA IS UPDATED AND */ 01093000
*     /* THE STAGEIT SUBROUTINE IS CALLED. CONTROL THEN PASSED TO    */ 01094000
*     /* CHKMORE.                                                    */ 01095000
*     /*                                                             */ 01096000
*     /***************************************************************/ 01097000
*                                                                  0381 01098000
*STAGEAIA:                                                         0381 01099000
*     ATAAIA=AIANXAIA;              /* GET THE NEXT AIA IN THE ATA   */ 01100000
STAGEAIA L     @12,AIANXAIA(,AIAPTR)                               0381 01101000
         ST    @12,ATAAIA(,ATAPTR)                                 0381 01102000
*     CALL STAGEIT;                 /* CALL TO PUT ON STAGE QUEUE    */ 01103000
         BAL   @14,STAGEIT                                         0382 01104000
*     GO TO CHKMORE;                /* GO TO GET ANOTHER AIA         */ 01105000
         B     CHKMORE                                             0383 01106000
*                                                                  0384 01107000
*     /***************************************************************/ 01108000
*     /*                                                             */ 01109000
*     /* THIS SECTION OF CODE COMPLETES PAGE COMPLETION PROCESSING.  */ 01110000
*     /* ANY AIA'S WHICH HAVE NOT BEEN PUT ON THE ASMSTAGQ ARE PUT ON*/ 01111000
*     /* AN INTERNAL QUEUE FOR IEAVPIOP.                             */ 01112000
*     /*                                                             */ 01113000
*     /***************************************************************/ 01114000
*                                                                  0384 01115000
*ENDUP:                                                            0384 01116000
*     ATAAIA=AIANXAIA;              /* GET THE NEXT AIA POINTER      */ 01117000
*                                                                  0384 01118000
