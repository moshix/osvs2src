@DE00132 L     @12,ATAAIA(,ATAPTR)                                 0402 01187000
         LTR   @12,@12                                             0402 01188000
         BNZ   @DL00132                                            0402 01189000
*   R14=ASM3R14A;                   /* RESTORE RETURN ADDRESS        */ 01190000
         L     R14,ASM3R14A(,ASMVTPTR)                             0403 01191000
*   ATAPCOMP='0'B;                  /* RESET RECOVERY POINTER        */ 01192000
         NI    ATAPCOMP(ATAPTR),B'11110111'                        0404 01193000
*   END PAGECOMP;                                                  0405 01194000
@EL00002 DS    0H                                                  0405 01195000
@EF00002 DS    0H                                                  0405 01196000
@ER00002 BR    @14                                                 0405 01197000
*SWAPCOMP:                                                         0406 01198000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0406 01199000
*                                                                  0406 01200000
SWAPCOMP DS    0H                                                  0407 01201000
*   /*****************************************************************/ 01202000
*   /*                                                               */ 01203000
*   /* THIS PROCEDURE WILL HANDLE ALL COMPLETIONS FOR LSQA PAGES.    */ 01204000
*   /* THEY CAN BE SENT BACK TO RSM OR CAN BE REDRIVEN.              */ 01205000
*   /*                                                               */ 01206000
*   /*****************************************************************/ 01207000
*                                                                  0407 01208000
*   ATASCOMP='1'B;                  /* SET FOOTPRINT FOR RECOVERY    */ 01209000
         OI    ATASCOMP(ATAPTR),B'00100000'                        0407 01210000
*   ASM3R14A=R14;                   /* SAVE RETURN ADDRESS           */ 01211000
         ST    R14,ASM3R14A(,ASMVTPTR)                             0408 01212000
*   RFY                                                            0409 01213000
*     SARTPTR RSTD;                 /* RESTRICT USE OF REG           */ 01214000
*   SARTPTR=ASMSART;                /* GET ADDRESS OF THE SART       */ 01215000
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0410 01216000
*   DO WHILE(ATAAIA^=0);            /* CYCLE THROUGH AIA'S           */ 01217000
         B     @DE00411                                            0411 01218000
@DL00411 DS    0H                                                  0412 01219000
*     AIAPTR=ATAAIA;                /* GET POINTER TO THE AIA        */ 01220000
         L     AIAPTR,ATAAIA(,ATAPTR)                              0412 01221000
*     ASM3TMPA=AIANXAIA;            /* SAVE NEXT AIA POINTER         */ 01222000
*                                                                  0413 01223000
         L     @12,AIANXAIA(,AIAPTR)                               0413 01224000
         ST    @12,ASM3TMPA(,ASMVTPTR)                             0413 01225000
*     /***************************************************************/ 01226000
*     /*                                                             */ 01227000
*     /* PRIOR TO DOING ANY READ OR WRITE PROCESSING THE TERMINATION */ 01228000
*     /* FLAG IS TESTED. IF ON NO PROCESSING CAN BE DONE EXCEPT      */ 01229000
*     /* FREEING ALLOCATED SLOTS. THE AIA'S ARE PUT ON THE INTERNAL  */ 01230000
*     /* QUEUE FOR IEAVPIOP.                                         */ 01231000
*     /*                                                             */ 01232000
*     /***************************************************************/ 01233000
*                                                                  0414 01234000
*     IF AIATERMR='1'B THEN         /* IS AIA FOR TERMINATION        */ 01235000
         TM    AIATERMR(AIAPTR),B'01000000'                        0414 01236000
         BNO   @RF00414                                            0414 01237000
*       DO;                         /* TERMR PROCESSING              */ 01238000
*         IF AIAPRIER='0'B&         /* WAS THERE AN I/O ERROR        */ 01239000
*             AIABADID='0'B&        /* OR ANY OTHER ERROR            */ 01240000
*             AIAERROR='0'B THEN                                   0416 01241000
         TM    AIAPRIER(AIAPTR),B'00001011'                        0416 01242000
         BNZ   @RF00416                                            0416 01243000
*           DO;                     /* NO-SLOTS CAN BE FREED         */ 01244000
*             IF AIAPAGDS='1'B THEN /* IS SLOT ON SWAP OR PAGE DS    */ 01245000
         TM    AIAPAGDS(AIAPTR),B'00000100'                        0418 01246000
         BNO   @RF00418                                            0418 01247000
*               DO;                                                0419 01248000
*                 CALL FREESLT1;    /* FREE PAGE DATA SET SLOT       */ 01249000
         BAL   @14,FREESLT1                                        0420 01250000
*                 ASMNVSC=ASMNVSC-1;/* DECREMENT ALLOCATED COUNT     */ 01251000
         L     @12,ASMNVSC(,ASMVTPTR)                              0421 01252000
         BCTR  @12,0                                               0421 01253000
         ST    @12,ASMNVSC(,ASMVTPTR)                              0421 01254000
*               END;                                               0422 01255000
*             ELSE                  /* MUST BE ON SWAP DATA SET      */ 01256000
*               CALL FREESWAP;      /* FREE THE SWAP SET             */ 01257000
         B     @RC00418                                            0423 01258000
@RF00418 BAL   @14,FREESWAP                                        0423 01259000
*           END;                    /* END-SLOTS CAN BE FREED        */ 01260000
*         ELSE                      /* AN ERROR OCCURRED             */ 01261000
*           IF AIAPRIER='1'B&       /* IF AN I/O ERROR DID OCCUR     */ 01262000
*               AIAPAGDS='1'B THEN  /* AND IT WAS A PAGE DATA SET    */ 01263000
         B     @RC00416                                            0425 01264000
@RF00416 TM    AIAPRIER(AIAPTR),B'00001000'                        0425 01265000
         BNO   @RF00425                                            0425 01266000
         TM    AIAPAGDS(AIAPTR),B'00000100'                        0425 01267000
         BNO   @RF00425                                            0425 01268000
*             ASMNVSC=ASMNVSC-1;    /* DECREMENT COUNT BECAUSE IT IS    01269000
*                                      NOW COUNTED IN ASMERRS        */ 01270000
         L     @12,ASMNVSC(,ASMVTPTR)                              0426 01271000
         BCTR  @12,0                                               0426 01272000
         ST    @12,ASMNVSC(,ASMVTPTR)                              0426 01273000
*         GO TO RECHAIN;            /* GO TO REQUEUE AIA'S           */ 01274000
         B     RECHAIN                                             0427 01275000
*       END;                        /* END-TERMR PROCESSING          */ 01276000
*                                                                  0429 01277000
*     /***************************************************************/ 01278000
*     /*                                                             */ 01279000
*     /* THE AIA IS NOT FOR TERMINATION SO NORMAL PROCESSING         */ 01280000
*     /* CONTINUES AND THE WRITE CASE IS HANDLED FIRST.              */ 01281000
*     /*                                                             */ 01282000
*     /***************************************************************/ 01283000
*                                                                  0429 01284000
*     IF AIAWRITE='1'B THEN         /* IS THIS A SWAP OUT?           */ 01285000
@RF00414 TM    AIAWRITE(AIAPTR),B'10000000'                        0429 01286000
         BNO   @RF00429                                            0429 01287000
*       DO;                         /* DO-WRITE PROCESSING           */ 01288000
*         PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));                 0431 01289000
         LR    PCBPTR,AIAPTR                                       0431 01290000
         SL    PCBPTR,@CF01923                                     0431 01291000
*         RSMHDPTR=ASCBRSM;         /* GET RSMHD ADDRESS             */ 01292000
         L     @01,ASCBPTR(,PCBPTR)                                0432 01293000
         L     RSMHDPTR,ASCBRSM(,@01)                              0432 01294000
*         ATAASCB=RSMASCB;          /* ASCB ADDR FOR RECOVERY        */ 01295000
*                                                                  0433 01296000
         L     @12,RSMASCB(,RSMHDPTR)                              0433 01297000
         ST    @12,ATAASCB(,ATAPTR)                                0433 01298000
*         /***********************************************************/ 01299000
*         /*                                                         */ 01300000
*         /* THE SWAP OUT COULD HAVE GONE TO A PAGE DATA SET OR A    */ 01301000
*         /* SWAP DATA SET. PROCESSING IS SEPARATE FOR EACH CASE. THE*/ 01302000
*         /* PAGE DATA SET IS HANDLED FIRST. IF A RETRY IS SPECIFIED */ 01303000
*         /* THE AIA IS QUEUED TO THE ASMSTAGQ FOR FOR REDRIVE.      */ 01304000
*         /*                                                         */ 01305000
*         /***********************************************************/ 01306000
*                                                                  0434 01307000
*         IF AIAPAGDS='1'B THEN     /* PAGE DATA SET USED            */ 01308000
         TM    AIAPAGDS(AIAPTR),B'00000100'                        0434 01309000
         BNO   @RF00434                                            0434 01310000
*           DO;                     /* PAGE DATA SET PROCESSING      */ 01311000
*             IF AIAPRIER='0'B&     /* NO I/O ERRORS                 */ 01312000
*                 AIAIORTY='0'B&    /* NO RETRY SITUATION            */ 01313000
*                 AIAERROR='0'B THEN/* NO MISCELLANEOUS ERROR        */ 01314000
         TM    AIAPRIER(AIAPTR),B'00101010'                        0436 01315000
         BNZ   @RF00436                                            0436 01316000
*               DO;                 /* DO-NO ERROR COMPLETION        */ 01317000
*                 SPCTSSID=AIASSID; /* GET THE LSID FROM AIA         */ 01318000
         L     @12,SWAPPTR(,PCBPTR)                                0438 01319000
         MVC   SPCTSSID(3,@12),AIASSID(AIAPTR)                     0438 01320000
*                 SPCTLVAL='1'B;    /* VALIDATE SPCTE                */ 01321000
         OI    SPCTLVAL(@12),B'10000000'                           0439 01322000
*               END;                /* END-NO ERROR COMPLETION       */ 01323000
*                                                                  0441 01324000
*             /*******************************************************/ 01325000
*             /*                                                     */ 01326000
*             /* ONE OF THE SPECIAL BITS WAS ON AND THEY WILL BE     */ 01327000
*             /* CHECKED INDIVIDUALLY AND THE PROPER ACTION TAKEN.   */ 01328000
*             /*                                                     */ 01329000
*             /*******************************************************/ 01330000
*                                                                  0441 01331000
*             ELSE                  /* SOME TYPE OF ERROR FOUND      */ 01332000
*               DO;                 /* CHECK SPECIAL CASES           */ 01333000
         B     @RC00436                                            0441 01334000
@RF00436 DS    0H                                                  0442 01335000
*                 IF AIAIORTY='1'B THEN/* RETRY SPECIFIED?           */ 01336000
         TM    AIAIORTY(AIAPTR),B'00100000'                        0442 01337000
         BNO   @RF00442                                            0442 01338000
*                   DO;             /* WRITE RETRY TO PAGE DS        */ 01339000
*                     AIAIORTY='0'B;/* RESET RETRY FLAG              */ 01340000
         NI    AIAIORTY(AIAPTR),B'11011111'                        0444 01341000
*                     CALL FREESLT1;/* FREE ALLOCATED SLOT           */ 01342000
         BAL   @14,FREESLT1                                        0445 01343000
*                     AIALSID=''B;  /* ZERO THE LSID IN AIA          */ 01344000
         SLR   @12,@12                                             0446 01345000
         ST    @12,AIALSID(,AIAPTR)                                0446 01346000
*                     ATAAIA=0;     /* ZERO THE ATA POINTER          */ 01347000
         SLR   @12,@12                                             0447 01348000
         ST    @12,ATAAIA(,ATAPTR)                                 0447 01349000
*                     CALL STAGEIT; /* QUEUE TO STAGE QUEUE          */ 01350000
         BAL   @14,STAGEIT                                         0448 01351000
*                     GO TO CKDONE; /* GO TO GET NEXT AIA            */ 01352000
         B     CKDONE                                              0449 01353000
*                   END;            /* END-WRITE RETRY TO PAGE DS    */ 01354000
*                                                                  0450 01355000
*                 /***************************************************/ 01356000
*                 /*                                                 */ 01357000
*                 /* IF THERE WAS AN I/O ERROR IT CAN BE REDRIVEN    */ 01358000
*                 /* (PUT ON THE ASMSTAGQ) IF THERE ARE NO PREVIOUS  */ 01359000
*                 /* MISCELLANEOUS ERRORS(ASHPERME). IF THERE WAS A  */ 01360000
*                 /* MISCELLANEOUS ERROR WE GO TO CHECK IF THE SWAP  */ 01361000
*                 /* OUT IS COMPLETE.                                */ 01362000
*                 /*                                                 */ 01363000
*                 /***************************************************/ 01364000
*                                                                  0451 01365000
*                 IF AIAPRIER='1'B THEN/* WAS THERE AN I/O ERROR     */ 01366000
@RF00442 TM    AIAPRIER(AIAPTR),B'00001000'                        0451 01367000
         BNO   @RF00451                                            0451 01368000
*                   DO;             /* YES-CHECK IF WE CAN REDRIVE   */ 01369000
*                     IF ASHPERME='0'B THEN/* PERMANENT ERROR?       */ 01370000
         TM    ASHPERME(RSMHDPTR),B'00100000'                      0453 01371000
         BNZ   @RF00453                                            0453 01372000
*                       DO;         /* NO-AIA CAN BE REDRIVEN        */ 01373000
*                         AIAPRIER='0'B;/* TURN OFF ERROR FLAG       */ 01374000
         NI    AIAPRIER(AIAPTR),B'11110111'                        0455 01375000
*                         AIALSID=''B;/* ZERO THE LSID               */ 01376000
         SLR   @12,@12                                             0456 01377000
         ST    @12,AIALSID(,AIAPTR)                                0456 01378000
*                         ATAAIA=0; /* ZERO THE ATA POINTER          */ 01379000
         SLR   @12,@12                                             0457 01380000
         ST    @12,ATAAIA(,ATAPTR)                                 0457 01381000
*                         CALL STAGEIT;/* QUEUE AIA TO STAGE QUE     */ 01382000
         BAL   @14,STAGEIT                                         0458 01383000
*                         GO TO CKDONE;/* GO TO GET NEXT AIA         */ 01384000
         B     CKDONE                                              0459 01385000
*                       END;        /* END-AIA CAN BE REDRIVEN       */ 01386000
*                   END;            /* END-CHECK IF WE CAN REDRIVE   */ 01387000
*                                                                  0461 01388000
*                 /***************************************************/ 01389000
*                 /*                                                 */ 01390000
*                 /* THERE WAS NO I/O ERROR ON THIS AIA SO WE CHECK  */ 01391000
*                 /* FOR A MISCELLANEOUS ERROR AND IF THERE IS ONE   */ 01392000
*                 /* THE PERMANENT ERROR FLAG IS TURNED ON IN THE    */ 01393000
*                 /* ASMHD.                                          */ 01394000
*                 /*                                                 */ 01395000
*                 /***************************************************/ 01396000
*                                                                  0462 01397000
*                 ELSE              /* NO I/O ERROR                  */ 01398000
*                   DO;             /* CHECK FOR MISCELLANEOUS ERR   */ 01399000
         B     @RC00451                                            0462 01400000
@RF00451 DS    0H                                                  0463 01401000
*                     IF AIAERROR='1'B THEN/* MISCELLANEOUS ERROR    */ 01402000
         TM    AIAERROR(AIAPTR),B'00000010'                        0463 01403000
         BNO   @RF00463                                            0463 01404000
*                       ASHPERME='1'B;/* YES-SET PERMANENT ERROR FLAG*/ 01405000
         OI    ASHPERME(RSMHDPTR),B'00100000'                      0464 01406000
*                   END;            /* END-CHK FOR MISC ERROR        */ 01407000
@RF00463 DS    0H                                                  0466 01408000
*               END;                /* END-                          */ 01409000
@RC00451 DS    0H                                                  0467 01410000
*                                                                  0467 01411000
*             /*******************************************************/ 01412000
*             /*                                                     */ 01413000
*             /* DECREMENT THE SWAP COUNT FOR THIS AIA AND IF IT HAS */ 01414000
*             /* GONE TO ZERO(SWAP IS COMPLETE) SEND THE AIA'S FROM  */ 01415000
*             /* THE ASHCAPQ TO IEAVSWPC.                            */ 01416000
*             /*                                                     */ 01417000
*             /*******************************************************/ 01418000
*                                                                  0467 01419000
*             ASHSWPCT=ASHSWPCT-1;  /* DECREMENT SWAP COUNT          */ 01420000
@RC00436 LH    @12,ASHSWPCT(,RSMHDPTR)                             0467 01421000
         BCTR  @12,0                                               0467 01422000
         STH   @12,ASHSWPCT(,RSMHDPTR)                             0467 01423000
*             ATAAIA=0;             /* ZERO RECOVERY POINTER         */ 01424000
         SLR   @07,@07                                             0468 01425000
         ST    @07,ATAAIA(,ATAPTR)                                 0468 01426000
*             AIANXAIA=ASHCAPQ;     /* PICK UP CAPTURE QUEUE         */ 01427000
         L     @05,ASHCAPQ(,RSMHDPTR)                              0469 01428000
         ST    @05,AIANXAIA(,AIAPTR)                               0469 01429000
*             ASMIORQC=ASMIORQC+1;  /* UPDATE COUNT                  */ 01430000
         LA    @05,1                                               0470 01431000
         AL    @05,ASMIORQC(,ASMVTPTR)                             0470 01432000
         ST    @05,ASMIORQC(,ASMVTPTR)                             0470 01433000
*             IF ASHSWPCT=0 THEN    /* IS THE SWAP OUT COMP          */ 01434000
         CR    @12,@07                                             0471 01435000
         BNE   @RF00471                                            0471 01436000
*               DO;                 /* YES-SEND CHAIN BACK TO RSM    */ 01437000
*                 RFY                                              0473 01438000
*                   R0 RSTD;        /* RESTRICT USE OF REG           */ 01439000
*                 IF ASHPERME='1'B THEN/* PERM ERRORS                */ 01440000
         TM    ASHPERME(RSMHDPTR),B'00100000'                      0474 01441000
         BNO   @RF00474                                            0474 01442000
*                   R0=4;           /* SET ERROR CODE FOR IEAVSWPC   */ 01443000
         LA    R0,4                                                0475 01444000
*                 ELSE              /* NO PERMENENT ERROR            */ 01445000
*                   R0=0;           /* SET GOOD COMPLETION CODE      */ 01446000
         B     @RC00474                                            0476 01447000
@RF00474 SLR   R0,R0                                               0476 01448000
*                 ASHCAPQ=0;        /* ZERO THE CAPTURE QUEUE        */ 01449000
@RC00474 SLR   @12,@12                                             0477 01450000
         ST    @12,ASHCAPQ(,RSMHDPTR)                              0477 01451000
*                 ASHSWPOT='0'B;    /* TURN OFF SWAP OUT BIT         */ 01452000
*                 ASHPERME='0'B;    /* RESET PERMANENT ERROR FLAG    */ 01453000
         NI    ASHSWPOT(RSMHDPTR),B'01011111'                      0479 01454000
*                 RFY                                              0480 01455000
*                   R1 RSTD;        /* RESTRICT USE OF REG           */ 01456000
*                 R1=AIAPTR;        /* GET AIA CHAIN INTO REG 1      */ 01457000
         LR    R1,AIAPTR                                           0481 01458000
*                 CALL IEAVSWPC;    /* CALL SWAP COMPLETE            */ 01459000
         L     @15,@CV01861                                        0482 01460000
         BALR  @14,@15                                             0482 01461000
*                 RFY                                              0483 01462000
*                  (R0,                                            0483 01463000
*                   R1) UNRSTD;     /* RELEASE REGS                  */ 01464000
*               END;                /* END-SEND CHAIN BACK TO RSM    */ 01465000
*             ELSE                  /* SWAP COUNT IS NOT ZERO        */ 01466000
*               ASHCAPQ=AIAPTR;     /* PUT AIA AT TOP OF CAPTURE   0485 01467000
*                                      QUEUE                         */ 01468000
         B     @RC00471                                            0485 01469000
@RF00471 ST    AIAPTR,ASHCAPQ(,RSMHDPTR)                           0485 01470000
*           END;                    /* END-PAGE DS PROCESSING        */ 01471000
*                                                                  0487 01472000
*         /***********************************************************/ 01473000
*         /*                                                         */ 01474000
*         /* WE NOW KNOW THAT THE AIA IS FROM A SWAP DATA SET SO THE */ 01475000
*         /* AIA'S ARE CHAINED BY GROUPS AND HAVE TO BE RE- CHAINED  */ 01476000
*         /* BEFORE BEING RETURNED TO RSM. THE FIRST CHECK IS TO SEE */ 01477000
*         /* IF THE AIA IS FOR RETRY(AIAIORTY) IF FOR RETRY THE AIA'S*/ 01478000
*         /* ARE PUT ON THE SARWAITQ AS THEY ARE.                    */ 01479000
*         /*                                                         */ 01480000
*         /***********************************************************/ 01481000
*                                                                  0487 01482000
*         ELSE                      /* NOT A PAGE DATA SET           */ 01483000
*           DO;                     /* SWAP DATA SET PROCESSING      */ 01484000
         B     @RC00434                                            0487 01485000
@RF00434 DS    0H                                                  0488 01486000
*             IF AIAIORTY='1'B THEN /* I/O RETRY SPECIFIED           */ 01487000
         TM    AIAIORTY(AIAPTR),B'00100000'                        0488 01488000
         BNO   @RF00488                                            0488 01489000
*               DO;                 /* RETRY PROCESSING              */ 01490000
*                 AIAIORTY='0'B;    /* RESET RETRY FLAG              */ 01491000
         NI    AIAIORTY(AIAPTR),B'11011111'                        0490 01492000
*                 ATAAIA=0;         /* ZERO RECOVERY POINTER         */ 01493000
         SLR   @12,@12                                             0491 01494000
         ST    @12,ATAAIA(,ATAPTR)                                 0491 01495000
*                 AIANXAIA=SARWAITF;/* PICK UP QUEUE                 */ 01496000
         L     @07,SARWAITF(,SARTPTR)                              0492 01497000
         ST    @07,AIANXAIA(,AIAPTR)                               0492 01498000
*                 SARWAITF=AIAPTR;  /* PUT AIA ON QUEUE              */ 01499000
         ST    AIAPTR,SARWAITF(,SARTPTR)                           0493 01500000
*                 IF SARWAITL=0 THEN/* IS QUEUE EMPTY                */ 01501000
         C     @12,SARWAITL(,SARTPTR)                              0494 01502000
         BNE   @RF00494                                            0494 01503000
*                   SARWAITL=AIAPTR;/* YES-ALSO MAKE AIA LAST        */ 01504000
         ST    AIAPTR,SARWAITL(,SARTPTR)                           0495 01505000
*                 GO TO CKDONE;     /* GET ANOTHER AIA               */ 01506000
         B     CKDONE                                              0496 01507000
*               END;                /* END RETRY PROCESSING          */ 01508000
*                                                                  0497 01509000
*             /*******************************************************/ 01510000
*             /*                                                     */ 01511000
*             /* IF THERE WAS AN I/O ERROR (AIAPRIER=1) THE ASHCAPER */ 01512000
*             /* FLAG IS TURNED ON TO INDICATE THAT THERE IS AN ERROR*/ 01513000
*             /* AIA ON THE CAPTURE QUEUE(ASHCAPQ).                  */ 01514000
*             /*                                                     */ 01515000
*             /*******************************************************/ 01516000
*                                                                  0498 01517000
*             IF AIAPRIER='1'B THEN /* WAS THERE AN I/O ERROR        */ 01518000
@RF00488 TM    AIAPRIER(AIAPTR),B'00001000'                        0498 01519000
         BNO   @RF00498                                            0498 01520000
*               ASHCAPER='1'B;      /* TURN ON CAP Q ERROR FLAG      */ 01521000
         OI    ASHCAPER(RSMHDPTR),B'01000000'                      0499 01522000
*                                                                  0500 01523000
*             /*******************************************************/ 01524000
*             /*                                                     */ 01525000
*             /* IF A MISCELLANEOUS ERROR IS FOUND TURN ON THE       */ 01526000
*             /* PERMANENT ERROR FLAG(ASHPERME). THESE AIA'S WILL NOT*/ 01527000
*             /* BE REDRIVEN.                                        */ 01528000
*             /*                                                     */ 01529000
*             /*******************************************************/ 01530000
*                                                                  0500 01531000
*             ELSE                  /* NO I/O ERROR                  */ 01532000
*               DO;                 /* MISCELLANEOUS ERROR CHECK     */ 01533000
         B     @RC00498                                            0500 01534000
@RF00498 DS    0H                                                  0501 01535000
*                 IF AIAERROR='1'B THEN/* MISCELLANEOUS ERR?         */ 01536000
         TM    AIAERROR(AIAPTR),B'00000010'                        0501 01537000
         BNO   @RF00501                                            0501 01538000
*                   ASHPERME='1'B;  /* YES TURN ON PERMANENT ERROR 0502 01539000
*                                      FLAG                          */ 01540000
         OI    ASHPERME(RSMHDPTR),B'00100000'                      0502 01541000
*               END;                                               0503 01542000
*                                                                  0503 01543000
@RF00501 DS    0H                                                  0504 01544000
*             /*******************************************************/ 01545000
*             /*                                                     */ 01546000
*             /* THE NEXT CHECK IS TO SEE IF THE SWAP HAS COMPLETED  */ 01547000
*             /* AND IF SO THE AIA'S WHICH WERE PUT ON THE CAPTURE   */ 01548000
*             /* QUEUE ARE PROCESSED.                                */ 01549000
*             /*                                                     */ 01550000
*             /*******************************************************/ 01551000
*                                                                  0504 01552000
*             ASHSWPCT=ASHSWPCT-AIAGRPSZ;/* DECREMENT SWP CNT        */ 01553000
@RC00498 LH    @12,ASHSWPCT(,RSMHDPTR)                             0504 01554000
         SL    @12,AIAGRPSZ(,AIAPTR)                               0504 01555000
         STH   @12,ASHSWPCT(,RSMHDPTR)                             0504 01556000
*             ATAAIA=0;             /* ZERO THE RECOVERY POINTER     */ 01557000
         SLR   @07,@07                                             0505 01558000
         ST    @07,ATAAIA(,ATAPTR)                                 0505 01559000
*             AIANXAIA=ASHCAPQ;     /* PICK UP CAPTURE QUEUE         */ 01560000
         L     @05,ASHCAPQ(,RSMHDPTR)                              0506 01561000
         ST    @05,AIANXAIA(,AIAPTR)                               0506 01562000
*             ASHCAPQ=AIAPTR;       /* PUT AIA ON TOP OF CAPTURE   0507 01563000
*                                      QUEUE                         */ 01564000
         ST    AIAPTR,ASHCAPQ(,RSMHDPTR)                           0507 01565000
*             IF ASHSWPCT=0 THEN    /* IS SWAP COMPLETE              */ 01566000
         CR    @12,@07                                             0508 01567000
         BNE   @RF00508                                            0508 01568000
*               DO;                 /* YES-PROCESS CAPTURE QUEUE     */ 01569000
*                 DO WHILE(AIAPTR^=0);/* CYCLE AIA'S ON CAP Q        */ 01570000
         B     @DE00510                                            0510 01571000
@DL00510 DS    0H                                                  0511 01572000
*                   ASM3GRPA=AIANXAIA;/* SAVE NEXT GROUP             */ 01573000
         L     @12,AIANXAIA(,AIAPTR)                               0511 01574000
         ST    @12,ASM3GRPA(,ASMVTPTR)                             0511 01575000
*                                                                  0512 01576000
*                   /*************************************************/ 01577000
*                   /*                                               */ 01578000
*                   /* IF THERE WAS A PREVIOUS ERROR IT MAY HAVE TO  */ 01579000
*                   /* BE REDRIVEN. ASHCAPER MEANS IT CAN BE REDRIVEN*/ 01580000
*                   /* WHILE ASHPERME CANNOT.                        */ 01581000
*                   /*                                               */ 01582000
*                   /*************************************************/ 01583000
*                                                                  0512 01584000
*                   IF ASHCAPER='1'B|/* I/O ERROR?                   */ 01585000
*                       ASHPERME='1'B THEN/* MISC. ERROR             */ 01586000
         TM    ASHCAPER(RSMHDPTR),B'01100000'                      0512 01587000
         BZ    @RF00512                                            0512 01588000
*                     DO;           /* YES-CHECK THIS AIA            */ 01589000
*                                                                  0513 01590000
*                       /*********************************************/ 01591000
*                       /*                                           */ 01592000
*                       /* IF THIS AIA HAS AN ERROR THE I/O ERROR    */ 01593000
*                       /* FLAG IS TURNED OFF IF THERE WAS NO        */ 01594000
*                       /* PREVIOUS PERMANENT ERROR. ALL WRITE I/O   */ 01595000
*                       /* ERRORS ARE REDRIVEN.                      */ 01596000
*                       /*                                           */ 01597000
*                       /*********************************************/ 01598000
*                                                                  0514 01599000
*                       IF AIAERROR='1'B|/* MISC. ERROR?             */ 01600000
*                           AIAPRIER='1'B THEN/* I/O ERROR?          */ 01601000
         TM    AIAERROR(AIAPTR),B'00001010'                        0514 01602000
         BZ    @RF00514                                            0514 01603000
*                         DO;       /* TURN OFF I/O ERROR FLAG IF NO    01604000
*                                      PERMANENENT ERROR             */ 01605000
*                           IF ASHPERME='0'B THEN/* PERM ERR?        */ 01606000
         TM    ASHPERME(RSMHDPTR),B'00100000'                      0516 01607000
         BNZ   @RF00516                                            0516 01608000
*                             AIAPRIER='0'B;/* NO-TURN OFF           */ 01609000
         NI    AIAPRIER(AIAPTR),B'11110111'                        0517 01610000
*                         END;      /* END-TURN OFF I/O ERROR FLAG   */ 01611000
*                                                                  0518 01612000
*                       /*********************************************/ 01613000
*                       /*                                           */ 01614000
*                       /* THIS SWAP SET CAN BE FREED SINCE THERE IS */ 01615000
*                       /* NO ERROR AND THE SLOTS WILL NOT BE USED   */ 01616000
*                       /* BECAUSE THERE WAS SOME OTHER TYPE OF      */ 01617000
*                       /* ERROR.                                    */ 01618000
*                       /*                                           */ 01619000
*                       /*********************************************/ 01620000
*                                                                  0519 01621000
*                       ELSE        /* NO ERRORS ON THIS AIA         */ 01622000
*                         CALL FREESWAP;/* FREE ALLOCATED SET        */ 01623000
         B     @RC00514                                            0519 01624000
@RF00514 BAL   @14,FREESWAP                                        0519 01625000
*                                                                  0520 01626000
*                       /*********************************************/ 01627000
*                       /*                                           */ 01628000
*                       /* THE HORIZONTAL CHAIN OF AIA'S IS RECHAINED*/ 01629000
*                       /* BY AIANXAIA AND THE AIALSID AND AIAGRPSZ  */ 01630000
*                       /* ARE SET TO ZERO.                          */ 01631000
*                       /*                                           */ 01632000
*                       /*********************************************/ 01633000
*                                                                  0520 01634000
*                       AIALSID=0;  /* ZERO LSID                     */ 01635000
@RC00514 SLR   @12,@12                                             0520 01636000
         ST    @12,AIALSID(,AIAPTR)                                0520 01637000
*                       AIAGRPSZ=0; /* ZERO THE GROUP SIZE           */ 01638000
         ST    @12,AIAGRPSZ(,AIAPTR)                               0521 01639000
*                       DO WHILE(AIAFQPA^=0);/* CYCLE HORIZONTAL   0522 01640000
*                                      CHAIN                         */ 01641000
         B     @DE00522                                            0522 01642000
@DL00522 DS    0H                                                  0523 01643000
*                         AIANXAIA=AIAFQPA;/* RECHAIN BY NEXT        */ 01644000
         L     @12,AIAFQPA(,AIAPTR)                                0523 01645000
         ST    @12,AIANXAIA(,AIAPTR)                               0523 01646000
*                         AIAFQPA=0;/* ZERO HORIZONTAL PTR           */ 01647000
         SLR   @07,@07                                             0524 01648000
         ST    @07,AIAFQPA(,AIAPTR)                                0524 01649000
*                         AIAPTR=AIANXAIA;/* GET NEXT AIA            */ 01650000
         LR    AIAPTR,@12                                          0525 01651000
*                         AIALSID=0;/* ZERO NEXT AIA                 */ 01652000
         ST    @07,AIALSID(,AIAPTR)                                0526 01653000
*                       END;        /* END-CYCLE HORIZONTAL CHAIN    */ 01654000
@DE00522 L     @12,AIAFQPA(,AIAPTR)                                0527 01655000
         LTR   @12,@12                                             0527 01656000
         BNZ   @DL00522                                            0527 01657000
*                     END;          /* END-CHECK THIS AIA            */ 01658000
*                                                                  0529 01659000
*                   /*************************************************/ 01660000
*                   /*                                               */ 01661000
*                   /* WE NOW KNOW THERE WERE NO ERRORS ON THE SWAP  */ 01662000
*                   /* DATA SET COMPLETION. THE AIA'S ARE RECHAINED  */ 01663000
*                   /* BY AIANXAIA AND THE LSID IS MOVED FROM THE AIA*/ 01664000
*                   /* TO THE SPCTE.                                 */ 01665000
*                   /*                                               */ 01666000
*                   /*************************************************/ 01667000
*                                                                  0529 01668000
*                   ELSE            /* NO PREVIOUS ERRORS            */ 01669000
*                     DO;                                          0529 01670000
         B     @RC00512                                            0529 01671000
@RF00512 DS    0H                                                  0530 01672000
*                       PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));   0530 01673000
         LR    PCBPTR,AIAPTR                                       0530 01674000
         SL    PCBPTR,@CF01923                                     0530 01675000
*                       SPCTSSID=AIASSID;/* MOVE LSID                */ 01676000
         L     @12,SWAPPTR(,PCBPTR)                                0531 01677000
         MVC   SPCTSSID(3,@12),AIASSID(AIAPTR)                     0531 01678000
*                       SPCTLVAL='1'B;/* VALIDATE SPCTE              */ 01679000
         OI    SPCTLVAL(@12),B'10000000'                           0532 01680000
*                       DO WHILE(AIAFQPA^=0);/* CYCLE CHAIN          */ 01681000
         B     @DE00533                                            0533 01682000
@DL00533 DS    0H                                                  0534 01683000
*                         AIANXAIA=AIAFQPA;/* RECHAIN BY NEXT        */ 01684000
         L     @12,AIAFQPA(,AIAPTR)                                0534 01685000
         ST    @12,AIANXAIA(,AIAPTR)                               0534 01686000
*                         AIAFQPA=0;/* ZERO HORIZONTAL PTR           */ 01687000
         SLR   @07,@07                                             0535 01688000
         ST    @07,AIAFQPA(,AIAPTR)                                0535 01689000
*                         AIAPTR=AIANXAIA;/* GET NEXT AIA            */ 01690000
         LR    AIAPTR,@12                                          0536 01691000
*                         PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA)); 0537 01692000
         LR    PCBPTR,AIAPTR                                       0537 01693000
         SL    PCBPTR,@CF01923                                     0537 01694000
*                         SPCTSSID=AIASSID;                        0538 01695000
         L     @12,SWAPPTR(,PCBPTR)                                0538 01696000
         MVC   SPCTSSID(3,@12),AIASSID(AIAPTR)                     0538 01697000
*                         SPCTLVAL='1'B;                           0539 01698000
         OI    SPCTLVAL(@12),B'10000000'                           0539 01699000
*                       END;                                       0540 01700000
@DE00533 L     @12,AIAFQPA(,AIAPTR)                                0540 01701000
         LTR   @12,@12                                             0540 01702000
         BNZ   @DL00533                                            0540 01703000
*                     END;                                         0541 01704000
*                                                                  0541 01705000
*                   /*************************************************/ 01706000
*                   /*                                               */ 01707000
*                   /* PICK UP THE ADDRESS OF THE NEXT GROUP OF AIA'S*/ 01708000
*                   /* AND PROCESS THEM.                             */ 01709000
*                   /*                                               */ 01710000
*                   /*************************************************/ 01711000
*                                                                  0542 01712000
*                   AIANXAIA=ASM3GRPA;/* NEXT GROUP ADDRESS          */ 01713000
@RC00512 L     @12,ASM3GRPA(,ASMVTPTR)                             0542 01714000
         ST    @12,AIANXAIA(,AIAPTR)                               0542 01715000
*                   AIAPTR=AIANXAIA;/* GET NEXT GROUP                */ 01716000
         LR    AIAPTR,@12                                          0543 01717000
*                 END;              /* END-CYCLE AIA'S ON CAP Q      */ 01718000
@DE00510 SLR   @12,@12                                             0544 01719000
         CR    AIAPTR,@12                                          0544 01720000
         BNE   @DL00510                                            0544 01721000
*                 RFY                                              0545 01722000
*                   R1 RSTD;        /* RESTRICT USE OF REG           */ 01723000
*                 R1=ASHCAPQ;       /* PICK UP CAPTURE QUE           */ 01724000
         L     R1,ASHCAPQ(,RSMHDPTR)                               0546 01725000
*                 ASHCAPQ=0;        /* ZERO THE QUEUE                */ 01726000
         ST    @12,ASHCAPQ(,RSMHDPTR)                              0547 01727000
*                 ATAAIA=0;         /* ZERO RECOVERY POINTER         */ 01728000
*                                                                  0548 01729000
         ST    @12,ATAAIA(,ATAPTR)                                 0548 01730000
*                 /***************************************************/ 01731000
*                 /*                                                 */ 01732000
*                 /* IF THERE WAS A PERMANENT ERROR THE AIA'S ARE    */ 01733000
*                 /* SENT TO IEAVSWPC.                               */ 01734000
*                 /*                                                 */ 01735000
*                 /***************************************************/ 01736000
*                                                                  0549 01737000
*                 IF ASHPERME='1'B THEN/* PERMANENT ERROR            */ 01738000
         TM    ASHPERME(RSMHDPTR),B'00100000'                      0549 01739000
         BNO   @RF00549                                            0549 01740000
*                   DO;             /* YES-MUST GO BACK TO RSM       */ 01741000
*                     ASHCAPER='0'B;/* RESET ERROR BIT               */ 01742000
*                     ASHSWPOT='0'B;/* RESET SWAP OUT BIT            */ 01743000
*                     ASHPERME='0'B;/* RESET ERROR BIT               */ 01744000
         NI    ASHCAPER(RSMHDPTR),B'00011111'                      0553 01745000
*                     RFY                                          0554 01746000
*                       R0 RSTD;    /* RESTRICT USE OF REG           */ 01747000
*                     R0=4;         /* SET ERROR COMPLETION CODE     */ 01748000
         LA    R0,4                                                0555 01749000
*                     CALL IEAVSWPC;/* RETURN AIA'S TO IEAVSWPC      */ 01750000
         L     @15,@CV01861                                        0556 01751000
         BALR  @14,@15                                             0556 01752000
*                     RFY                                          0557 01753000
*                       R0 UNRSTD;  /* RELEASE REG FOR USE           */ 01754000
*                   END;            /* END-MUST GO BACK TO RSM       */ 01755000
*                                                                  0558 01756000
*                 /***************************************************/ 01757000
*                 /*                                                 */ 01758000
*                 /* IF THERE ARE NO ERRORS THE AIA'S ARE SENT TO    */ 01759000
*                 /* IEAVSWPC.                                       */ 01760000
*                 /*                                                 */ 01761000
*                 /***************************************************/ 01762000
*                                                                  0559 01763000
*                 ELSE              /* NO PERMANENT ERROR            */ 01764000
*                   DO;                                            0559 01765000
         B     @RC00549                                            0559 01766000
@RF00549 DS    0H                                                  0560 01767000
*                     IF ASHCAPER='0'B THEN/* PREVIOUS I/O ERRS      */ 01768000
         TM    ASHCAPER(RSMHDPTR),B'01000000'                      0560 01769000
         BNZ   @RF00560                                            0560 01770000
*                       DO;         /* NO ERROR SWAP COMP            */ 01771000
*                         RFY                                      0562 01772000
*                           R0 RSTD;/* RESTRICT USE OF REG           */ 01773000
*                         R0=0;     /* SET GOOD COMPLETION CODE      */ 01774000
         SLR   R0,R0                                               0563 01775000
*                         ASHSWPOT='0'B;/* RESET SWAP BIT            */ 01776000
         NI    ASHSWPOT(RSMHDPTR),B'01111111'                      0564 01777000
*                         CALL IEAVSWPC;/* RETURN GOOD ONES          */ 01778000
         L     @15,@CV01861                                        0565 01779000
         BALR  @14,@15                                             0565 01780000
*                         RFY                                      0566 01781000
*                           R0 UNRSTD;/* RELEASE REG FOR USE         */ 01782000
*                       END;        /* END-NO ERROR SWAP COMP        */ 01783000
*                                                                  0567 01784000
*                     /***********************************************/ 01785000
*                     /*                                             */ 01786000
*                     /* HOWEVER, THE ASHCAPER FLAG MEANS THERE IS A */ 01787000
*                     /* RETRIABLE ERROR AND THE AIA'S ARE PUT ON THE*/ 01788000
*                     /* ASHSWAPQ AND SENT TO ILRSLSQA FOR REDRIVE.  */ 01789000
*                     /*                                             */ 01790000
*                     /***********************************************/ 01791000
*                                                                  0568 01792000
*                     ELSE          /* ERROR IS RETRYABLE            */ 01793000
*                       DO;         /* RETRY ENTIRE CHAIN            */ 01794000
         B     @RC00560                                            0568 01795000
@RF00560 DS    0H                                                  0569 01796000
*                         ASHSWAPQ=R1;/* PUT ENTRE CHAIN ON Q        */ 01797000
         ST    R1,ASHSWAPQ(,RSMHDPTR)                              0569 01798000
*                         ASHCAPER='0'B;/* RESET ERROR FLAG          */ 01799000
         NI    ASHCAPER(RSMHDPTR),B'10111111'                      0570 01800000
*                         CALL ILRSLSQA;/* GO TO REDRIVE             */ 01801000
         L     @15,@CV01863                                        0571 01802000
         BALR  @14,@15                                             0571 01803000
*                         RFY                                      0572 01804000
*                           R1 UNRSTD;/* RELEASE REG FOR USE         */ 01805000
*                       END;        /* END-RETRY ENTIRE CHAIN        */ 01806000
*                   END;                                           0574 01807000
*               END;                /* END PROCESS CAPTURE QUEUE     */ 01808000
*           END;                    /* END-SWAP DATA SET PROCESS     */ 01809000
*       END;                        /* END-WRITE PROCESSING          */ 01810000
*                                                                  0578 01811000
*     /***************************************************************/ 01812000
*     /*                                                             */ 01813000
*     /* THIS BEGINS THE CODE FOR SWAP IN COMPLETIONS OF LSQA PAGES  */ 01814000
*     /* THE FIRST TEST IS TO SEE IF AN ERROR OCCURRED DURING        */ 01815000
*     /* PROCESSING.                                                 */ 01816000
*     /*                                                             */ 01817000
*     /***************************************************************/ 01818000
*                                                                  0578 01819000
*     ELSE                          /* NOT WRITE-MUST BE READ        */ 01820000
*       DO;                         /* DO ERROR CHECK                */ 01821000
         B     @RC00429                                            0578 01822000
@RF00429 DS    0H                                                  0579 01823000
*         IF AIAERROR='1'B|         /* DID ERROR HAPPEN?             */ 01824000
*             AIABADID='1'B THEN    /*                               */ 01825000
*                                                                  0579 01826000
         TM    AIAERROR(AIAPTR),B'00000011'                        0579 01827000
         BZ    @RF00579                                            0579 01828000
*           /*********************************************************/ 01829000
*           /*                                                       */ 01830000
*           /* NEXT THE AIA THAT WE ARE PROCESSING MUST BE PUT ON THE*/ 01831000
*           /* INTERNAL QUEUE FOR IEAVPIOP. IF THE AIA'S ARE QUEUED  */ 01832000
*           /* BY SWAP SETS THEY HAVE TO BE REQUEUED BY THE AIANXAIA */ 01833000
*           /* POINTER.                                              */ 01834000
*           /*                                                       */ 01835000
*           /*********************************************************/ 01836000
*                                                                  0580 01837000
*RECHAIN:                                                          0580 01838000
*           DO UNTIL(AIAPTR=ASM3PIOP);/* CYCLE THROUGH AIA'S         */ 01839000
RECHAIN  DS    0H                                                  0580 01840000
@DL00580 DS    0H                                                  0581 01841000
*             AIANXAIA=ASM3PIOP;    /* ADD QUEUE TO THIS AIA         */ 01842000
         L     @12,ASM3PIOP(,ASMVTPTR)                             0581 01843000
         ST    @12,AIANXAIA(,AIAPTR)                               0581 01844000
*             ASM3PIOP=AIAPTR;      /* PUT THIS AIA AT TOP OF QUE    */ 01845000
         ST    AIAPTR,ASM3PIOP(,ASMVTPTR)                          0582 01846000
*             IF AIAPAGDS='1'B THEN /* IS THIS FROMGPAGE DS          */ 01847000
         TM    AIAPAGDS(AIAPTR),B'00000100'                        0583 01848000
         BNO   @RF00583                                            0583 01849000
*               ASMIORQC=ASMIORQC+1;/* INC COMPLETED COUNT           */ 01850000
         LA    @12,1                                               0584 01851000
         AL    @12,ASMIORQC(,ASMVTPTR)                             0584 01852000
         ST    @12,ASMIORQC(,ASMVTPTR)                             0584 01853000
*             IF AIAFQPA^=0 THEN    /* IS THERE A HORIZONTAL CHAIN   */ 01854000
@RF00583 SLR   @12,@12                                             0585 01855000
         L     @07,AIAFQPA(,AIAPTR)                                0585 01856000
         CR    @07,@12                                             0585 01857000
         BE    @RF00585                                            0585 01858000
*               DO;                 /* YES-                          */ 01859000
*                 AIAPTR=AIAFQPA;   /* GET NEXT ONE                  */ 01860000
         LR    AIAPTR,@07                                          0587 01861000
*                 ASM3PIOP->AIAFQPA=0;/* ZERO CHAIN FIELD            */ 01862000
         L     @07,ASM3PIOP(,ASMVTPTR)                             0588 01863000
         ST    @12,AIAFQPA(,@07)                                   0588 01864000
*               END;                                               0589 01865000
*           END;                    /* END-CYCLE THROUGH AIA'S       */ 01866000
@RF00585 DS    0H                                                  0590 01867000
@DE00580 C     AIAPTR,ASM3PIOP(,ASMVTPTR)                          0590 01868000
         BNE   @DL00580                                            0590 01869000
*         ELSE                      /* NORMAL READ COMPLETION        */ 01870000
*           DO;                     /* DO READ PROCESSING            */ 01871000
         B     @RC00579                                            0591 01872000
@RF00579 DS    0H                                                  0592 01873000
*             IF AIAPAGDS='1'B THEN /* WAS PAGE DATA SET USED        */ 01874000
         TM    AIAPAGDS(AIAPTR),B'00000100'                        0592 01875000
         BNO   @RF00592                                            0592 01876000
*               DO;                 /* PAGE DATA SET PROCESSING      */ 01877000
*                                                                  0593 01878000
*                 /***************************************************/ 01879000
*                 /*                                                 */ 01880000
*                 /* IF A RETRY IS SPECIFIED JUST PUT THE AIA BACK ON*/ 01881000
*                 /* THE STAGE QUEUE FOR REDRIVE.                    */ 01882000
*                 /*                                                 */ 01883000
*                 /***************************************************/ 01884000
*                                                                  0594 01885000
*                 IF AIAIORTY='1'B THEN/* IS THIS A RETRY?           */ 01886000
         TM    AIAIORTY(AIAPTR),B'00100000'                        0594 01887000
         BNO   @RF00594                                            0594 01888000
*                   DO;             /* RETRY PROCESSING              */ 01889000
*                     AIAIORTY='0'B;/* TURN OFF RETRY BIT            */ 01890000
         NI    AIAIORTY(AIAPTR),B'11011111'                        0596 01891000
*                     ATAAIA=0;     /* ZERO THE RECOVERY POINTER     */ 01892000
         SLR   @12,@12                                             0597 01893000
         ST    @12,ATAAIA(,ATAPTR)                                 0597 01894000
*                     CALL STAGEIT; /* PUT AIA ON STAGE QUEUE        */ 01895000
         BAL   @14,STAGEIT                                         0598 01896000
*                   END;            /* END RETRY PROCESSING          */ 01897000
*                                                                  0599 01898000
*                 /***************************************************/ 01899000
*                 /*                                                 */ 01900000
*                 /* THE SLOT THAT WAS JUST READ IN CAN NOW BE MARKED*/ 01901000
*                 /* AS INVALID SO IT CAN BE REUSED. IF THERE WAS NO */ 01902000
*                 /* I/O ERROR ON THE READ THE SLOT CAN ALSO BE      */ 01903000
*                 /* FREED.                                          */ 01904000
*                 /*                                                 */ 01905000
*                 /***************************************************/ 01906000
*                                                                  0600 01907000
*                 ELSE              /* NOT A RETRY AIA               */ 01908000
*                   DO;             /* PAGE DATA SET COMPLETION      */ 01909000
         B     @RC00594                                            0600 01910000
@RF00594 DS    0H                                                  0601 01911000
*                     PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));     0601 01912000
         LR    PCBPTR,AIAPTR                                       0601 01913000
         SL    PCBPTR,@CF01923                                     0601 01914000
*                     SPCTLVAL='0'B;/* INVALIDATE SLOT               */ 01915000
         L     @12,SWAPPTR(,PCBPTR)                                0602 01916000
         NI    SPCTLVAL(@12),B'01111111'                           0602 01917000
*                     IF AIAPRIER='0'B THEN/* CHECK FOR NO ERROR     */ 01918000
         TM    AIAPRIER(AIAPTR),B'00001000'                        0603 01919000
         BNZ   @RF00603                                            0603 01920000
*                       CALL FREESLT1;/* FREE THE.SLOT               */ 01921000
         BAL   @14,FREESLT1                                        0604 01922000
*                     ASCBNVSC=ASCBNVSC-1;/* DECREMENTED COUNT       */ 01923000
@RF00603 L     @12,ASCBPTR(,PCBPTR)                                0605 01924000
         LH    @07,ASCBNVSC(,@12)                                  0605 01925000
         BCTR  @07,0                                               0605 01926000
         STH   @07,ASCBNVSC(,@12)                                  0605 01927000
*                     ASMNVSC=ASMNVSC-1;/* DECREMENT COUNT           */ 01928000
         L     @12,ASMNVSC(,ASMVTPTR)                              0606 01929000
         BCTR  @12,0                                               0606 01930000
         ST    @12,ASMNVSC(,ASMVTPTR)                              0606 01931000
*                     AIANXAIA=ASM3PIOP;/* PICK UP EXISTING QUEUE FOR   01932000
*                                      IEAVPIOP                      */ 01933000
         L     @12,ASM3PIOP(,ASMVTPTR)                             0607 01934000
         ST    @12,AIANXAIA(,AIAPTR)                               0607 01935000
*                     ASM3PIOP=AIAPTR;/* PUT AIA ON TOP OF QUEUE     */ 01936000
         ST    AIAPTR,ASM3PIOP(,ASMVTPTR)                          0608 01937000
*                     ASMIORQC=ASMIORQC+1;/* INC COMP COUNT          */ 01938000
         LA    @12,1                                               0609 01939000
         AL    @12,ASMIORQC(,ASMVTPTR)                             0609 01940000
         ST    @12,ASMIORQC(,ASMVTPTR)                             0609 01941000
*                   END;            /* PAGE DATA SET COMPLETION      */ 01942000
*               END;                /* PAGE DATA SET PROCESSING      */ 01943000
*                                                                  0612 01944000
*             /*******************************************************/ 01945000
*             /*                                                     */ 01946000
*             /* THE SWAP IN IS FROM A SWAP SET SO THE AIA'S ARE     */ 01947000
*             /* CHAINED HORIZONTALLY AND HAVE TO BE RECHAINED BEFORE*/ 01948000
*             /* BEING SENT BACK TO RSM.                             */ 01949000
*             /*                                                     */ 01950000
*             /*******************************************************/ 01951000
*                                                                  0612 01952000
*             ELSE                  /* SWAP DATA SET USED            */ 01953000
*               DO;                 /* SWAP DATA SET PROCESSING      */ 01954000
*                                                                  0612 01955000
         B     @RC00592                                            0612 01956000
@RF00592 DS    0H                                                  0613 01957000
*                 /***************************************************/ 01958000
*                 /*                                                 */ 01959000
*                 /* A CHECK IS MADE TO SEE IF RETRY IS SPECIFIED AND*/ 01960000
*                 /* THE AIA IS CHAINED DIRECTLY TO THE SARWAITQ TO  */ 01961000
*                 /* WAIT FOR RESOURCES.                             */ 01962000
*                 /*                                                 */ 01963000
*                 /***************************************************/ 01964000
*                                                                  0613 01965000
*                 IF AIAIORTY='1'B THEN/* RETRY SPECIFIED?           */ 01966000
         TM    AIAIORTY(AIAPTR),B'00100000'                        0613 01967000
         BNO   @RF00613                                            0613 01968000
*                   DO;             /* RETRY PROCESSING              */ 01969000
*                     AIAIORTY='0'B;/* RESET RETRY FLAG              */ 01970000
         NI    AIAIORTY(AIAPTR),B'11011111'                        0615 01971000
*                     ATAAIA=0;     /* ZERO THE RECOVERY POINTER     */ 01972000
         SLR   @12,@12                                             0616 01973000
         ST    @12,ATAAIA(,ATAPTR)                                 0616 01974000
*                     AIANXAIA=SARWAITF;/* CHAIN QUEUE TO AIA        */ 01975000
         L     @07,SARWAITF(,SARTPTR)                              0617 01976000
         ST    @07,AIANXAIA(,AIAPTR)                               0617 01977000
*                     SARWAITF=AIAPTR;/* PUT AIA AT TOP OF QUEUE     */ 01978000
         ST    AIAPTR,SARWAITF(,SARTPTR)                           0618 01979000
*                     IF SARWAITL=0 THEN/* IS QUEUE EMPTY            */ 01980000
         C     @12,SARWAITL(,SARTPTR)                              0619 01981000
         BNE   @RF00619                                            0619 01982000
*                       SARWAITL=AIAPTR;/* YES- POINT LAST PTR TO  0620 01983000
*                                      THIS AIA                      */ 01984000
         ST    AIAPTR,SARWAITL(,SARTPTR)                           0620 01985000
*                   END;            /* END-RETRY PROCESSING          */ 01986000
*                                                                  0622 01987000
*                 /***************************************************/ 01988000
*                 /*                                                 */ 01989000
*                 /* THESE AIA'S WILL BE SENT BACK TO RSM. THE       */ 01990000
*                 /* SPCTE'S ARE INVALIDATED SO THE SWAP SETS CAN BE */ 01991000
*                 /* REUSED. IF AN I/O ERROR OCCURRED IT IS SHOWN    */ 01992000
*                 /* ONLY IN THE FIRST AIA IN THE GROUP. THE         */ 01993000
*                 /* HORIZONTAL CHAIN IS BEING CHANGED NOW SO ANY I/O*/ 01994000
*                 /* ERROR WILL BE PROPAGATED TO EVERY AIA IN THE    */ 01995000
*                 /* CHAIN.                                          */ 01996000
*                 /*                                                 */ 01997000
*                 /***************************************************/ 01998000
*                                                                  0622 01999000
*                 ELSE              /* NOT A RETRY AIA               */ 02000000
*                   DO;             /* NORMAL COMPLETION             */ 02001000
         B     @RC00613                                            0622 02002000
@RF00613 DS    0H                                                  0623 02003000
*                     DO WHILE(AIAFQPA^=0);/* REQUEUE LOOP           */ 02004000
         B     @DE00623                                            0623 02005000
@DL00623 DS    0H                                                  0624 02006000
*                       PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));   0624 02007000
         LR    PCBPTR,AIAPTR                                       0624 02008000
         SL    PCBPTR,@CF01923                                     0624 02009000
*                       SPCTLVAL='0'B;/* INVALIDATE SPCTE            */ 02010000
         L     @12,SWAPPTR(,PCBPTR)                                0625 02011000
         NI    SPCTLVAL(@12),B'01111111'                           0625 02012000
*                       AIANXAIA=AIAFQPA;/* RECHAIN AIA'S            */ 02013000
         L     @12,AIAFQPA(,AIAPTR)                                0626 02014000
         ST    @12,AIANXAIA(,AIAPTR)                               0626 02015000
*                       AIAFQPA=0;  /* ZERO HORIZONTAL PTR           */ 02016000
         SLR   @07,@07                                             0627 02017000
         ST    @07,AIAFQPA(,AIAPTR)                                0627 02018000
*                       IF AIAPRIER='1'B THEN/* I/O ERROR?           */ 02019000
         TM    AIAPRIER(AIAPTR),B'00001000'                        0628 02020000
         BNO   @RF00628                                            0628 02021000
*                         DO;       /* YES-PROPAGATE ERROR           */ 02022000
*                           AIAPTR=AIANXAIA;/* GET NEXT AIA          */ 02023000
         LR    AIAPTR,@12                                          0630 02024000
*                           AIAPRIER='1'B;/* SET ERROR BIT           */ 02025000
         OI    AIAPRIER(AIAPTR),B'00001000'                        0631 02026000
*                         END;      /* END PROPAGATE ERROR           */ 02027000
*                       ELSE        /* NO I/O ERROR                  */ 02028000
*                         AIAPTR=AIANXAIA;/* GET NEXT AIA            */ 02029000
         B     @RC00628                                            0633 02030000
@RF00628 L     AIAPTR,AIANXAIA(,AIAPTR)                            0633 02031000
*                     END;          /* END-REQUEUE LOOP              */ 02032000
@RC00628 DS    0H                                                  0634 02033000
@DE00623 L     @12,AIAFQPA(,AIAPTR)                                0634 02034000
         LTR   @12,@12                                             0634 02035000
         BNZ   @DL00623                                            0634 02036000
*                     PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));     0635 02037000
         LR    PCBPTR,AIAPTR                                       0635 02038000
         SL    PCBPTR,@CF01923                                     0635 02039000
*                     SPCTLVAL='0'B;                               0636 02040000
         L     @12,SWAPPTR(,PCBPTR)                                0636 02041000
         NI    SPCTLVAL(@12),B'01111111'                           0636 02042000
*                     IF AIAPRIER='0'B THEN/* NO ERROR?              */ 02043000
         TM    AIAPRIER(AIAPTR),B'00001000'                        0637 02044000
         BNZ   @RF00637                                            0637 02045000
*                       CALL FREESWAP;/* FREE UP THE SWAP GROUP      */ 02046000
         BAL   @14,FREESWAP                                        0638 02047000
*                     AIANXAIA=ASM3PIOP;/* PICK UP QUEUE FOR IEAVPIOP*/ 02048000
@RF00637 L     @12,ASM3PIOP(,ASMVTPTR)                             0639 02049000
         ST    @12,AIANXAIA(,AIAPTR)                               0639 02050000
*                     ASM3PIOP=ATAAIA;/* PUT FIRST AIA AT THE TOP OF    02051000
*                                      THE QUEUE                     */ 02052000
         L     @12,ATAAIA(,ATAPTR)                                 0640 02053000
         ST    @12,ASM3PIOP(,ASMVTPTR)                             0640 02054000
*                   END;            /* END-NORMAL COMPLETION         */ 02055000
*               END;                /* SWAP DATA SET PROCESSING      */ 02056000
@RC00613 DS    0H                                                  0643 02057000
*           END;                    /* END READ PROCESSING           */ 02058000
@RC00592 DS    0H                                                  0644 02059000
*       END;                        /* END-ERROR CHECK               */ 02060000
@RC00579 DS    0H                                                  0645 02061000
*CKDONE:                                                           0645 02062000
*     ATAASCB=0;                    /* ZERO RECOVERY ASCB ADDR       */ 02063000
@RC00429 DS    0H                                                  0645 02064000
CKDONE   SLR   @12,@12                                             0645 02065000
         ST    @12,ATAASCB(,ATAPTR)                                0645 02066000
*     ATAAIA=ASM3TMPA;                                             0646 02067000
         L     @12,ASM3TMPA(,ASMVTPTR)                             0646 02068000
         ST    @12,ATAAIA(,ATAPTR)                                 0646 02069000
*   END;                            /* END DO LOOP TO CYCLE THROUGH     02070000
*                                      ALL AIA'S                     */ 02071000
@DE00411 SLR   @12,@12                                             0647 02072000
         C     @12,ATAAIA(,ATAPTR)                                 0647 02073000
         BNE   @DL00411                                            0647 02074000
*   IF SARWAITF^=0 THEN             /* IS SOME WORK QUEUED           */ 02075000
         C     @12,SARWAITF(,SARTPTR)                              0648 02076000
         BE    @RF00648                                            0648 02077000
*     DO;                           /* YES REDRIVE ILRSLSQA          */ 02078000
*       R2=0;                       /* ZERO R2 TO INDICATE WORK    0650 02079000
*                                      ALREADY IN SWAP GROUPS        */ 02080000
         SLR   R2,R2                                               0650 02081000
*       CALL ILRSLSQA;              /* REDRIVE WORK                  */ 02082000
         L     @15,@CV01863                                        0651 02083000
         BALR  @14,@15                                             0651 02084000
*     END;                          /* END-REDRIVE ILRSLSQA          */ 02085000
*   R14=ASM3R14A;                   /* RESTORE RETURN ADDRESS        */ 02086000
@RF00648 L     R14,ASM3R14A(,ASMVTPTR)                             0653 02087000
*   ATASCOMP='0'B;                  /* RESET RECOVERY FOOTPRINT      */ 02088000
         NI    ATASCOMP(ATAPTR),B'11011111'                        0654 02089000
*   RFY                                                            0655 02090000
*     SARTPTR UNRSTD;               /* RELEASE REG FOR USE           */ 02091000
*   END SWAPCOMP;                                                  0656 02092000
@EL00003 DS    0H                                                  0656 02093000
@EF00003 DS    0H                                                  0656 02094000
@ER00003 BR    @14                                                 0656 02095000
*                                                                  0657 02096000
*   /*****************************************************************/ 02097000
*   /*                                                               */ 02098000
*   /* THE NEXT TWO ROUTINES WILL PICK UP AN LSID AND SET UP THE     */ 02099000
*   /* LINKAGE REGISTERS TO FREE THE SPECIFIED SLOTS. THE ONLY       */ 02100000
*   /* DIFFERENCE IN THE ROUTINES IS WHICH LSID IS FREED.            */ 02101000
*   /*                                                               */ 02102000
*   /*****************************************************************/ 02103000
*                                                                  0657 02104000
*FREESLT1:                                                         0657 02105000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0657 02106000
FREESLT1 DS    0H                                                  0658 02107000
*   RFY                                                            0658 02108000
*    (R1,                                                          0658 02109000
*     R13,                                                         0658 02110000
*     R15) RSTD;                    /* RESTRICT USE OF REGISTERS     */ 02111000
*   ASM3R14B=R14;                   /* SAVE RETURN ADDRESS           */ 02112000
         ST    R14,ASM3R14B(,ASMVTPTR)                             0659 02113000
*   R1=AIALSID;                     /* GET LSID TO BE FREED          */ 02114000
         L     R1,AIALSID(,AIAPTR)                                 0660 02115000
*   R13=ADDR(ASMSAVE);              /* GET ADDRESS OF SAVE AREA      */ 02116000
         LA    R13,ASMSAVE(,ASMVTPTR)                              0661 02117000
*   CALL ILRFRSL1;                  /* CALL FREESLOT MODULE          */ 02118000
         L     @15,ASMPFRSL(,ASMVTPTR)                             0662 02119000
         BALR  @14,@15                                             0662 02120000
*   R14=ASM3R14B;                   /* RESTORE RETURN ADDRESS        */ 02121000
         L     R14,ASM3R14B(,ASMVTPTR)                             0663 02122000
*   RFY                                                            0664 02123000
*    (R1,                                                          0664 02124000
*     R13,                                                         0664 02125000
*     R15) UNRSTD;                  /* RESTRICT USE OF REGS          */ 02126000
*   END FREESLT1;                                                  0665 02127000
@EL00004 DS    0H                                                  0665 02128000
@EF00004 DS    0H                                                  0665 02129000
@ER00004 BR    @14                                                 0665 02130000
*FREESLT2:                                                         0666 02131000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0666 02132000
FREESLT2 DS    0H                                                  0667 02133000
*   RFY                                                            0667 02134000
*    (R1,                                                          0667 02135000
*     R13,                                                         0667 02136000
*     R15) RSTD;                    /* RELEASE REGS FOR USE          */ 02137000
*   ASM3R14B=R14;                   /* SAVE RETURN ADDRESS           */ 02138000
         ST    R14,ASM3R14B(,ASMVTPTR)                             0668 02139000
*   R1=AIALSID2;                    /* GET LSID TO BE FREED          */ 02140000
         L     R1,AIALSID2(,AIAPTR)                                0669 02141000
*   R13=ADDR(ASMSAVE);              /* GET ADDRESS OF SAVE AREA      */ 02142000
         LA    R13,ASMSAVE(,ASMVTPTR)                              0670 02143000
*   CALL ILRFRSL1;                  /* CALL TO FREE SLOT             */ 02144000
         L     @15,ASMPFRSL(,ASMVTPTR)                             0671 02145000
         BALR  @14,@15                                             0671 02146000
*   R14=ASM3R14B;                   /* RESTORE RETURN ADDRESS        */ 02147000
         L     R14,ASM3R14B(,ASMVTPTR)                             0672 02148000
*   RFY                                                            0673 02149000
*    (R1,                                                          0673 02150000
*     R13,                                                         0673 02151000
*     R15) UNRSTD;                  /* RELEASE REGS FOR USE          */ 02152000
*   END FREESLT2;                                                  0674 02153000
@EL00005 DS    0H                                                  0674 02154000
@EF00005 DS    0H                                                  0674 02155000
@ER00005 BR    @14                                                 0674 02156000
*                                                                  0675 02157000
*   /*****************************************************************/ 02158000
*   /*                                                               */ 02159000
*   /* THIS ROUTINE WILL PICK UP THE LSID FROM THE AIA AND CALL      */ 02160000
*   /* ILRFRSW1 TO FREE UP THE SWAP GROUP GROUP.                     */ 02161000
*   /*                                                               */ 02162000
*   /*****************************************************************/ 02163000
*                                                                  0675 02164000
*FREESWAP:                                                         0675 02165000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0675 02166000
FREESWAP DS    0H                                                  0676 02167000
*   RFY                                                            0676 02168000
*    (R1,                                                          0676 02169000
*     R13,                                                         0676 02170000
*     R15) RSTD;                    /* RESTRICT USE OF REGISTERS     */ 02171000
*   ASM3R14B=R14;                   /* SAVE RETURN ADDRESS           */ 02172000
         ST    R14,ASM3R14B(,ASMVTPTR)                             0677 02173000
*   R1=AIALSID;                     /* GET LSID TO BE FREED          */ 02174000
         L     R1,AIALSID(,AIAPTR)                                 0678 02175000
*   R13=ADDR(ASMSAVE);              /* GET ADDRESS OF SAVE AREA      */ 02176000
         LA    R13,ASMSAVE(,ASMVTPTR)                              0679 02177000
*   CALL ILRFRSW1;                  /* CALL TO FREE THE SWAP SET     */ 02178000
         L     @15,ASMPFRSW(,ASMVTPTR)                             0680 02179000
         BALR  @14,@15                                             0680 02180000
*   R14=ASM3R14B;                   /* RESTORE RETURN ADDRESS        */ 02181000
         L     R14,ASM3R14B(,ASMVTPTR)                             0681 02182000
*   RFY                                                            0682 02183000
*    (R1,                                                          0682 02184000
*     R13,                                                         0682 02185000
*     R15) UNRSTD;                  /* RELEASE REGS FOR USAGE        */ 02186000
*   END FREESWAP;                   /* END FREEING SWAP SET          */ 02187000
@EL00006 DS    0H                                                  0683 02188000
@EF00006 DS    0H                                                  0683 02189000
@ER00006 BR    @14                                                 0683 02190000
*                                                                  0684 02191000
*   /*****************************************************************/ 02192000
*   /*                                                               */ 02193000
*   /* THIS PROCEDURE WILL TAKE THE AIA THAT IS POINTED TO BY AIAPTR */ 02194000
*   /* AND PUT IT AT THE TOP OF THE ASMSTAGQ. IF THE QUE IS EMPTY    */ 02195000
*   /* BOTH POINTERS MUST POINT TO THIS AIA. THE AIA IS PUT ON THE   */ 02196000
*   /* TOP OF THE QUEUE BECAUSE IT HAS BEEN IN PROCESS THE LONGEST.  */ 02197000
*   /*                                                               */ 02198000
*   /*****************************************************************/ 02199000
*                                                                  0684 02200000
*STAGEIT:                                                          0684 02201000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0684 02202000
STAGEIT  DS    0H                                                  0685 02203000
*   AIANXAIA=ASMSTAGF;              /* PICK UP THE EXISTING QUE      */ 02204000
         L     @12,ASMSTAGF(,ASMVTPTR)                             0685 02205000
         ST    @12,AIANXAIA(,AIAPTR)                               0685 02206000
*   ASMSTAGF=AIAPTR;                /* PUT THIS AIA ON TOP OF QUE    */ 02207000
         ST    AIAPTR,ASMSTAGF(,ASMVTPTR)                          0686 02208000
*   IF ASMSTAGL=0 THEN              /* IS THE QUEUE EMPTY            */ 02209000
         L     @12,ASMSTAGL(,ASMVTPTR)                             0687 02210000
         LTR   @12,@12                                             0687 02211000
         BNZ   @RF00687                                            0687 02212000
*     ASMSTAGL=AIAPTR;              /* ALSO PUT THIS ONE AS LAST     */ 02213000
         ST    AIAPTR,ASMSTAGL(,ASMVTPTR)                          0688 02214000
*   END STAGEIT;                                                   0689 02215000
@EL00007 DS    0H                                                  0689 02216000
@EF00007 DS    0H                                                  0689 02217000
@ER00007 BR    @14                                                 0689 02218000
*   END ILRPAGCM                                                   0690 02219000
*                                                                  0690 02220000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02221000
*/*%INCLUDE SYSLIB  (ILRAIA  )                                       */ 02222000
