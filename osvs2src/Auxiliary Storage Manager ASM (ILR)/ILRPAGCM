         TITLE 'ILRPAGCM-PAGE COMPLETION MODULE OF I/O CONTROL         *00001000
                        '                                               00002000
ILRPAGCM CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRPAGCM  77.028'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @08,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@08                                         0001 00013000
*ILRCBPGM:                                                         0078 00014000
*   R0=R13;                         /* SAVE SAVE AREA ADDRESS        */ 00015000
ILRCBPGM LR    R0,R13                                              0078 00016000
*   ASMVTPTR=CVTASMVT;              /* GET ASMVT ADDRESS             */ 00017000
         L     @12,CVTPTR                                          0079 00018000
         L     ASMVTPTR,CVTASMVT(,@12)                             0079 00019000
*   GENERATE REFS(PSALITA,FLC,PSACSTK,PSA,FRRSCURR,FRRS,R12,FRRSLAST,   00020000
*       PSALSFCC,FRRSELEN,FRRSFRRA,FRRSENTR);                      0080 00021000
GETLOCK SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                       CC00022000
               RELATED=(PAGE/SWAP,ILRPAGCM(FREELOCK))                   00023000
        L     R12,ASMPIOFR(,ASMVTPTR)                                   00024000
        SETFRR A,FRRAD=(R12),WRKREGS=(6,7),PARMAD=(4)                   00025000
*   ASMWKSA3=''B;                   /* CLEAR WORKAREA TO ZEROES      */ 00026000
         XC    ASMWKSA3(80,ASMVTPTR),ASMWKSA3(ASMVTPTR)            0081 00027000
*   ASM3SR13=R0;                    /* SAVE SAVE AREA ADDRESS        */ 00028000
         ST    R0,ASM3SR13(,ASMVTPTR)                              0082 00029000
*   ATAMODID=ATAMPGCM;              /* SET MODID FOR RECOVERY        */ 00030000
         MVI   ATAMODID(ATAPTR),X'02'                              0083 00031000
*   ATAPAGCM='1'B;                  /* SET FOOTPRINT IN ATA          */ 00032000
         OI    ATAPAGCM(ATAPTR),B'00000001'                        0084 00033000
*   AIAPTR=R1;                      /* GET THE ADDRESS OF 1ST AIA    */ 00034000
         LR    AIAPTR,R1                                           0085 00035000
*   DO WHILE(AIAPTR^=0);            /* LOOP TO CHAIN AIA'S           */ 00036000
         B     @DE00086                                            0086 00037000
@DL00086 DS    0H                                                  0087 00038000
*     ATAAIA=AIANXAIA;              /* SAVE ADDRESS OF NEXT AIA      */ 00039000
         L     @12,AIANXAIA(,AIAPTR)                               0087 00040000
         ST    @12,ATAAIA(,ATAPTR)                                 0087 00041000
*     IF AIALSQA='1'B THEN          /* IF AIA IS LSQA                */ 00042000
         TM    AIALSQA(AIAPTR),B'00001000'                         0088 00043000
         BNO   @RF00088                                            0088 00044000
*       DO;                         /* PUT IT ON THE SWAP QUEUE      */ 00045000
*         AIANXAIA=ASM3SWPQ;        /* CHAIN QUEUE TO THIS AIA       */ 00046000
         L     @12,ASM3SWPQ(,ASMVTPTR)                             0090 00047000
         ST    @12,AIANXAIA(,AIAPTR)                               0090 00048000
*         ASM3SWPQ=AIAPTR;          /* PUT THIS AIA ON TOP OF QUE    */ 00049000
         ST    AIAPTR,ASM3SWPQ(,ASMVTPTR)                          0091 00050000
*       END;                        /* END SWAP QUEUEING             */ 00051000
*     ELSE                          /* AIA IS NOT LSQA               */ 00052000
*       DO;                         /* PUT IT ON PAGECOMP'S QUEUE    */ 00053000
         B     @RC00088                                            0093 00054000
@RF00088 DS    0H                                                  0094 00055000
*         AIANXAIA=ASM3GENQ;        /* CHAIN QUE TO THIS AIA         */ 00056000
         L     @12,ASM3GENQ(,ASMVTPTR)                             0094 00057000
         ST    @12,AIANXAIA(,AIAPTR)                               0094 00058000
*         ASM3GENQ=AIAPTR;          /* PUT THIS AIA AT TOP OF QUE    */ 00059000
         ST    AIAPTR,ASM3GENQ(,ASMVTPTR)                          0095 00060000
*       END;                        /* END PAGE QUEUEING             */ 00061000
*     AIAPTR=ATAAIA;                /* GET THE NEXT AIA              */ 00062000
@RC00088 L     AIAPTR,ATAAIA(,ATAPTR)                              0097 00063000
*   END;                                                           0098 00064000
@DE00086 SLR   @12,@12                                             0098 00065000
         CR    AIAPTR,@12                                          0098 00066000
         BNE   @DL00086                                            0098 00067000
*                                                                  0099 00068000
*   /*****************************************************************/ 00069000
*   /*                                                               */ 00070000
*   /* THE INTERNAL QUEUES THAT WERE JUST BUILT ARE NOW CHECKED TO   */ 00071000
*   /* SEE IF THERE ARE ANY IOE'S FOR PAGECOMP OR SWAPCOMP           */ 00072000
*   /*                                                               */ 00073000
*   /*****************************************************************/ 00074000
*                                                                  0099 00075000
*   IF ASM3GENQ^=0 THEN             /* ANY AIA'S FOR PAGECOMP        */ 00076000
         L     @09,ASM3GENQ(,ASMVTPTR)                             0099 00077000
         CR    @09,@12                                             0099 00078000
         BE    @RF00099                                            0099 00079000
*     DO;                           /* YES                           */ 00080000
*       ATAAIA=ASM3GENQ;            /* PICK UP THE AIA CHAIN         */ 00081000
         ST    @09,ATAAIA(,ATAPTR)                                 0101 00082000
*       ASM3GENQ=0;                 /* ZERO THE QUEUE PTR            */ 00083000
         ST    @12,ASM3GENQ(,ASMVTPTR)                             0102 00084000
*       CALL PAGECOMP;              /* GO TO PROCESS AIA'S           */ 00085000
         BAL   @14,PAGECOMP                                        0103 00086000
*     END;                                                         0104 00087000
*   IF ASM3SWPQ^=0 THEN             /* ANY AIA'S FOR SWAPCOMP        */ 00088000
@RF00099 SLR   @12,@12                                             0105 00089000
         L     @09,ASM3SWPQ(,ASMVTPTR)                             0105 00090000
         CR    @09,@12                                             0105 00091000
         BE    @RF00105                                            0105 00092000
*     DO;                           /* YES                           */ 00093000
*       ATAAIA=ASM3SWPQ;            /* PICK UP AIA CHAIN             */ 00094000
         ST    @09,ATAAIA(,ATAPTR)                                 0107 00095000
*       ASM3SWPQ=0;                 /* ZERO THE QUEUE PTR            */ 00096000
         ST    @12,ASM3SWPQ(,ASMVTPTR)                             0108 00097000
*       CALL SWAPCOMP;              /* GO TO PROCESS THE AIA'S       */ 00098000
         BAL   @14,SWAPCOMP                                        0109 00099000
*     END;                                                         0110 00100000
*                                                                  0111 00101000
*   /*****************************************************************/ 00102000
*   /*                                                               */ 00103000
*   /* DURING PROCESSING IN PAGECOMP AND SWAPCOMP AIA'S MAY HAVE BEEN*/ 00104000
*   /* PUT ON THE QUEUE TO BE PASSED TO RSM(IEAVPIOP). THESE AIA'S   */ 00105000
*   /* MAY BE NORMAL COMPLETIONS OR ERRORS WHICH COULD NOT BE        */ 00106000
*   /* REDRIVEN. THIS QUEUE(ASM3PIOP) IS CHECKED NOW TO SEE IF ANY   */ 00107000
*   /* MUST BE PASSED. ON RETURN FROM PAGECOMP AND SWAPCOMP THE      */ 00108000
*   /* ATAAIA FIELD IS ALWAYS ZERO.                                  */ 00109000
*   /*                                                               */ 00110000
*   /*****************************************************************/ 00111000
*                                                                  0111 00112000
*ILRCRPC1:                                                         0111 00113000
*   IF ASM3PIOP^=0 THEN             /* ANY AIA'S FOR IEAVPIOP        */ 00114000
@RF00105 DS    0H                                                  0111 00115000
ILRCRPC1 SLR   @12,@12                                             0111 00116000
         L     @09,ASM3PIOP(,ASMVTPTR)                             0111 00117000
         CR    @09,@12                                             0111 00118000
         BE    @RF00111                                            0111 00119000
*     DO;                           /* YES                           */ 00120000
*       RFY                                                        0113 00121000
*         R1 RSTD;                  /* RESTRICT USAGE OF REG         */ 00122000
*       R1=ASM3PIOP;                /* PUT QUEUE ADDRESS IN REG      */ 00123000
         LR    R1,@09                                              0114 00124000
*       ASM3PIOP=0;                 /* ZERO INTERNAL QUEUE PTR       */ 00125000
         ST    @12,ASM3PIOP(,ASMVTPTR)                             0115 00126000
*       CALL IEAVPIOP;              /* SEND AIA'S TO IEAVPIOP        */ 00127000
         L     @15,@CV01860                                        0116 00128000
         BALR  @14,@15                                             0116 00129000
*       RFY                                                        0117 00130000
*         R1 UNRSTD;                /* RELEASE REG FOR USE           */ 00131000
*     END;                                                         0118 00132000
*                                                                  0118 00133000
*   /*****************************************************************/ 00134000
*   /*                                                               */ 00135000
*   /* IF ANY AIA'S FOR PAGE DATA SETS HAD TO BE REDRIVEN THEY WOULD */ 00136000
*   /* HAVE BEEN PUT ON THE ASMSTAGQ. IF ANY AIA'S ARE ON THE STAGE  */ 00137000
*   /* QUEUE THEY ARE SENT TO ILRQIOE FOR REDRIVE.                   */ 00138000
*   /*                                                               */ 00139000
*   /*****************************************************************/ 00140000
*                                                                  0119 00141000
*   IF ASMSTAGF^=0 THEN             /* ANY AIA'S FOR ILRQIOE         */ 00142000
@RF00111 L     @12,ASMSTAGF(,ASMVTPTR)                             0119 00143000
         LTR   @12,@12                                             0119 00144000
         BZ    @RF00119                                            0119 00145000
*     DO;                           /* YES                           */ 00146000
*       ASMCALLQ='1'B;              /* TURN ON BIT TO INDICATE THAT     00147000
*                                      COMPLETION IS CALLING QIOE    */ 00148000
         OI    ASMCALLQ(ASMVTPTR),B'10000000'                      0121 00149000
*       CALL ILRQIOE;               /* GO TO QIOE TO BUILD IOE'S     */ 00150000
         L     @15,ASMPQIOE(,ASMVTPTR)                             0122 00151000
         BALR  @14,@15                                             0122 00152000
*       ASMCALLQ='0'B;              /* TURN OF INDICATOR BIT         */ 00153000
         NI    ASMCALLQ(ASMVTPTR),B'01111111'                      0123 00154000
*     END;                                                         0124 00155000
*   R15=ASM3SR13;                   /* SAVE THE SAVE AREA ADDRESS    */ 00156000
*                                                                  0125 00157000
@RF00119 L     R15,ASM3SR13(,ASMVTPTR)                             0125 00158000
*   /*****************************************************************/ 00159000
*   /*                                                               */ 00160000
*   /* PROCESSING IS NOW COMPLETE SO THE FRR IS DELETED AND THE      */ 00161000
*   /* SALLOC LOCK IS RELEASED.                                      */ 00162000
*   /*                                                               */ 00163000
*   /*****************************************************************/ 00164000
*                                                                  0126 00165000
*   GENERATE REFS(R6,R7,FRRSELEN,FRRSCURR,FRRSEMP,PSACSTK,FRRS,CVTGSMQ, 00166000
*       SRBFLNK,SRBSECT);                                          0126 00167000
        SETFRR D,WRKREGS=(6,7)                                          00168000
FREELOCK SETLOCK RELEASE,TYPE=SALLOC,                                 CC00169000
               RELATED=(PAGE/SWAP,ILRPAGCM(GETLOCK))                    00170000
*   R13=R15;                        /* GET SAVE AREA ADDRESS         */ 00171000
         LR    R13,R15                                             0127 00172000
*   RETURN;                         /* RETURN TO CALLER              */ 00173000
@EL00001 DS    0H                                                  0128 00174000
@EF00001 DS    0H                                                  0128 00175000
@ER00001 LM    @14,@12,12(@13)                                     0128 00176000
         BR    @14                                                 0128 00177000
*                                                                  0129 00178000
*   /*****************************************************************/ 00179000
*   /*                                                               */ 00180000
*   /* THE PAGE COMPLETIONS THAT WERE PUT ON THE INTGENQ IN ILRPAGCM */ 00181000
*   /* ARE PASSED TO THIS ROUTINE VIA THE ATAAIA.                    */ 00182000
*   /*                                                               */ 00183000
*   /*****************************************************************/ 00184000
*                                                                  0129 00185000
*PAGECOMP:                                                         0129 00186000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0129 00187000
PAGECOMP DS    0H                                                  0130 00188000
*   ATAPCOMP='1'B;                  /* SET FOOTPRINT FOR RECOVERY    */ 00189000
         OI    ATAPCOMP(ATAPTR),B'00001000'                        0130 00190000
*   ASM3R14A=R14;                   /* SAVE RETURN ADDRESS           */ 00191000
         ST    R14,ASM3R14A(,ASMVTPTR)                             0131 00192000
*   DO WHILE(ATAAIA^=0);            /* LOOP THROUGH ALL AIA'S        */ 00193000
         B     @DE00132                                            0132 00194000
@DL00132 DS    0H                                                  0133 00195000
*     AIAPTR=ATAAIA;                /* GET AIA ADDRESS               */ 00196000
*                                                                  0133 00197000
         L     AIAPTR,ATAAIA(,ATAPTR)                              0133 00198000
*     /***************************************************************/ 00199000
*     /*                                                             */ 00200000
*     /* FIRST THE AIATERMR FLAG IS CHECKED TO SEE IF THE AIA IS FROM*/ 00201000
*     /* A MEMORY WHICH HAS BEEN TERMINATED. IF IT IS WE CANNOT      */ 00202000
*     /* ADDRESS ANY CONTROL BLOCKS FOR THIS ADDRESS SPACE BECAUSE   */ 00203000
*     /* THEY MAY HAVE BEEN WIPED OUT. THE OBJECTIVE OF THIS         */ 00204000
*     /* PROCESSING IS TO CLEAN UP WHERE POSSIBLE AND GIVE THE AIA   */ 00205000
*     /* BACK TO RSM.                                                */ 00206000
*     /*                                                             */ 00207000
*     /***************************************************************/ 00208000
*                                                                  0134 00209000
*     IF AIATERMR='1'B THEN         /* IS THIS AIA FROM A MEMORY   0134 00210000
*                                      WHICH HAS BEEN TERMINATED     */ 00211000
         TM    AIATERMR(AIAPTR),B'01000000'                        0134 00212000
         BNO   @RF00134                                            0134 00213000
*       DO;                         /* DO TERMR PROCESSING           */ 00214000
*         IF AIAPRIER='0'B THEN     /* WAS THERE AN ERROR ON SLOT    */ 00215000
         TM    AIAPRIER(AIAPTR),B'00001000'                        0136 00216000
         BNZ   @RF00136                                            0136 00217000
*           CALL FREESLT1;          /* FREE THE SLOT                 */ 00218000
         BAL   @14,FREESLT1                                        0137 00219000
*                                                                  0138 00220000
*         /***********************************************************/ 00221000
*         /*                                                         */ 00222000
*         /* NEXT WE DETERMINE IF WE HAVE TO DECREMENT ANY OF THE    */ 00223000
*         /* ALLOCATED SLOT COUNTS. ASMVSC MUST BE DECREMENTED FOR   */ 00224000
*         /* VIO PAGES AND ASMNVSC MUST BE DECREMENTED FOR NON-VIO   */ 00225000
*         /* PRIVATE AREA PAGES.                                     */ 00226000
*         /*                                                         */ 00227000
*         /***********************************************************/ 00228000
*                                                                  0138 00229000
*         IF AIAVIO='1'B THEN       /* IS THIS A VIO PAGE            */ 00230000
@RF00136 TM    AIAVIO(AIAPTR),B'00100000'                          0138 00231000
         BNO   @RF00138                                            0138 00232000
*           DO;                     /* YES-DECREMENT ASMVSC          */ 00233000
*             RFY                                                  0140 00234000
*              (R5,                                                0140 00235000
*               R6) RSTD;           /* RESTRICT REGS FROM USE        */ 00236000
*             R5=ASMVSC;            /* GET THE COUNT                 */ 00237000
         L     R5,ASMVSC(,ASMVTPTR)                                0141 00238000
*AGAIN1:                                                           0142 00239000
*             R6=R5-1;              /* DECREMENT IT                  */ 00240000
AGAIN1   LR    R6,R5                                               0142 00241000
         BCTR  R6,0                                                0142 00242000
*             CS(R5,R6,ASMVSC);     /* COMPARE AND SWAP              */ 00243000
         CS    R5,@06,ASMVSC(ASMVTPTR)                             0143 00244000
*             BC(7,AGAIN1);         /* NOT SUCCESSFUL-TRY AGAIN      */ 00245000
         BC    7,AGAIN1                                            0144 00246000
*           END;                    /* END-DECREMENT ASMVSC          */ 00247000
*         ELSE                      /* NOT A VIO PAGE                */ 00248000
*           IF AIAPRIV='1'B THEN    /* IS IT A PRIVATE AREA PAGE     */ 00249000
         B     @RC00138                                            0146 00250000
@RF00138 TM    AIAPRIV(AIAPTR),B'01000000'                         0146 00251000
         BNO   @RF00146                                            0146 00252000
*             ASMNVSC=ASMNVSC-1;    /* DECREMENT ASMNVSC             */ 00253000
*                                                                  0147 00254000
         L     @12,ASMNVSC(,ASMVTPTR)                              0147 00255000
         BCTR  @12,0                                               0147 00256000
         ST    @12,ASMNVSC(,ASMVTPTR)                              0147 00257000
*         /***********************************************************/ 00258000
*         /*                                                         */ 00259000
*         /* IF A COUNT HAD TO BE DECREMENTED IT HAS BEEN DONE SO WE */ 00260000
*         /* WILL NOW PICK UP THE NEXT AIA AND PUT THIS ONE ON THE   */ 00261000
*         /* INTERNAL QUEUE FOR IEAVPIOP. THE TOTAL COUNT OF AIA'S   */ 00262000
*         /* COMPLETED IS INCREMENTED                                */ 00263000
*         /*                                                         */ 00264000
*         /***********************************************************/ 00265000
*                                                                  0148 00266000
*         ATAAIA=AIANXAIA;          /* GET THE NEXT AIA              */ 00267000
@RF00146 DS    0H                                                  0148 00268000
@RC00138 L     @12,AIANXAIA(,AIAPTR)                               0148 00269000
         ST    @12,ATAAIA(,ATAPTR)                                 0148 00270000
*         AIANXAIA=ASM3PIOP;        /* CHAIN THE QUEUE TO THIS AIA   */ 00271000
         L     @12,ASM3PIOP(,ASMVTPTR)                             0149 00272000
         ST    @12,AIANXAIA(,AIAPTR)                               0149 00273000
*         ASM3PIOP=AIAPTR;          /* PUT THIS AIA TO TOP OF Q      */ 00274000
         ST    AIAPTR,ASM3PIOP(,ASMVTPTR)                          0150 00275000
*         ASMIORQC=ASMIORQC+1;      /* INCREMENT THE COMPLETED CNT   */ 00276000
*                                                                  0151 00277000
         LA    @12,1                                               0151 00278000
         AL    @12,ASMIORQC(,ASMVTPTR)                             0151 00279000
         ST    @12,ASMIORQC(,ASMVTPTR)                             0151 00280000
*         /***********************************************************/ 00281000
*         /*                                                         */ 00282000
*         /* A BRANCH IS TAKEN TO AVOID THE CHECK TO SEE IF SWAP     */ 00283000
*         /* PROCESSING HAS COMPLETED. THE CONTROL BLOCKS MAY NOT BE */ 00284000
*         /* AVAILABLE AND THE TEST CANNOT BE DONE.                  */ 00285000
*         /*                                                         */ 00286000
*         /***********************************************************/ 00287000
*                                                                  0152 00288000
*         GO TO CHKMORE;            /* GET ANOTHER AIA               */ 00289000
         B     CHKMORE                                             0152 00290000
*       END;                        /* END-TERMR PROCESSING          */ 00291000
*                                                                  0154 00292000
*     /***************************************************************/ 00293000
*     /*                                                             */ 00294000
*     /* SINCE THE AIATERMR FLAG WAS NOT SET WE HAVE ACCESS TO THE   */ 00295000
*     /* CONTROL BLOCKS. WE SET UP THE ADDRESSES OF THE NECESSARY    */ 00296000
*     /* CONTROL BLOCKS.                                             */ 00297000
*     /*                                                             */ 00298000
*     /***************************************************************/ 00299000
*                                                                  0154 00300000
*     PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));                     0154 00301000
@RF00134 LR    PCBPTR,AIAPTR                                       0154 00302000
         SL    PCBPTR,@CF01923                                     0154 00303000
*     RFY                                                          0155 00304000
*       XPTEPTR RSTD;               /* RESTRICT USE OF REGISTER      */ 00305000
*     XPTEPTR=PCBXPT;               /* GET ADDRESS OF THE XPTE       */ 00306000
         L     XPTEPTR,PCBXPT(,PCBPTR)                             0156 00307000
*     RSMHDPTR=ASCBRSM;             /* GET ADDRESS OF THE RSMHD      */ 00308000
         L     @01,ASCBPTR(,PCBPTR)                                0157 00309000
         L     RSMHDPTR,ASCBRSM(,@01)                              0157 00310000
*                                                                  0158 00311000
*     /***************************************************************/ 00312000
*     /*                                                             */ 00313000
*     /* THIS IS THE PROCESSING SECTION FOR VIO COMPLETIONS. IF IT IS*/ 00314000
*     /* A RETRY CASE THE AIA CAN BE REQUEUED TO THE ASMSTAGQ FOR    */ 00315000
*     /* REDRIVE AND ON A WRITE REDRIVE THE ALLOCATED SLOT WILL BE   */ 00316000
*     /* FREED. THE AIA WILL ALSO BE REDRIVEN IF THERE WAS AN ERROR  */ 00317000
*     /* ON A WRITE REQUEST. ANY OTHER CASE WILL BE SENT TO VIO      */ 00318000
*     /* COMPLETION AND SUBSEQUENTLY TO RSM.                         */ 00319000
*     /*                                                             */ 00320000
*     /***************************************************************/ 00321000
*                                                                  0158 00322000
*     IF AIAVIO='1'B THEN                                          0158 00323000
         TM    AIAVIO(AIAPTR),B'00100000'                          0158 00324000
         BNO   @RF00158                                            0158 00325000
*       DO;                         /* DO-VIO COMPLETION             */ 00326000
*         IF AIAIORTY='0'B THEN     /* IS THIS AIA OK FOR RETRY      */ 00327000
         TM    AIAIORTY(AIAPTR),B'00100000'                        0160 00328000
         BNZ   @RF00160                                            0160 00329000
*           DO;                     /* NO-DO-NO RETRY PROCESSING     */ 00330000
*             IF AIAWRITE='1'B&     /* IS IT A WRITE??               */ 00331000
*                 AIAPRIER='1'B THEN/* YES-DID I/O ERROR OCCUR       */ 00332000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0162 00333000
         BNO   @RF00162                                            0162 00334000
         TM    AIAPRIER(AIAPTR),B'00001000'                        0162 00335000
         BNO   @RF00162                                            0162 00336000
*               DO;                 /* YES-WRITE ERROR-SET IT UP FOR    00337000
*                                      RECOVERY                      */ 00338000
*                 AIAPRIER='0'B;    /* TURN OFF ERROR INDICATOR      */ 00339000
         NI    AIAPRIER(AIAPTR),B'11110111'                        0164 00340000
*                 AIALSID=''B;      /* ZERO THE LSID IN THE AIA      */ 00341000
         SLR   @12,@12                                             0165 00342000
         ST    @12,AIALSID(,AIAPTR)                                0165 00343000
*                 GO TO STAGEAIA;   /* GO TO QUE AIA FOR QIOE        */ 00344000
         B     STAGEAIA                                            0166 00345000
*               END;                                               0167 00346000
*                                                                  0167 00347000
*             /*******************************************************/ 00348000
*             /*                                                     */ 00349000
*             /* EITHER THERE ARE NO ERRORS OR THERE IS A READ       */ 00350000
*             /* ERROR(WRITE ERRORS ARE ALWAYS REDRIVEN). THE AIA    */ 00351000
*             /* WILL BE SENT TO VIO COMPLETION.                     */ 00352000
*             /*                                                     */ 00353000
*             /*******************************************************/ 00354000
*                                                                  0168 00355000
*             ELSE                  /* THIS IS NOT A WRITE AIA       */ 00356000
*               CALL ILRVIOCM;      /* CALL VIO COMPLETION FOR TO  0168 00357000
*                                      UPDATE HIS TABLES             */ 00358000
@RF00162 L     @15,@CV01859                                        0168 00359000
         BALR  @14,@15                                             0168 00360000
*           END;                    /* END-NO RETRY PROCESSING       */ 00361000
*                                                                  0170 00362000
*         /***********************************************************/ 00363000
*         /*                                                         */ 00364000
*         /* THIS IS VIO PROCESSING FOR THE REDRIVE CASE. THE I/O CAN*/ 00365000
*         /* BE REDRIVEN SO THE AIA IS PUT ON THE ASMSTAGQ.          */ 00366000
*         /*                                                         */ 00367000
*         /***********************************************************/ 00368000
*                                                                  0170 00369000
*         ELSE                      /* RETRY CASE                    */ 00370000
*           DO;                     /* DO-RETRY PROCESSING           */ 00371000
         B     @RC00160                                            0170 00372000
@RF00160 DS    0H                                                  0171 00373000
*             AIAIORTY='0'B;        /* TURN OFF RETRY INDICATOR      */ 00374000
         NI    AIAIORTY(AIAPTR),B'11011111'                        0171 00375000
*             IF AIAWRITE='1'B THEN /* IS THIS AIA FOR A WRITE       */ 00376000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0172 00377000
         BNO   @RF00172                                            0172 00378000
*               CALL FREESLT1;      /* YES-FREE THE ALLOCATED SLOT   */ 00379000
         BAL   @14,FREESLT1                                        0173 00380000
*             GO TO STAGEAIA;       /* GO TO QUE AIA FOR QIOE        */ 00381000
         B     STAGEAIA                                            0174 00382000
*           END;                    /* END-RETRY PROCESSING          */ 00383000
*         GO TO ENDUP;              /* GO TO QUEUE FOR PIOP          */ 00384000
         B     ENDUP                                               0176 00385000
*       END;                        /* END-VIO COMPLETION            */ 00386000
*                                                                  0178 00387000
*     /***************************************************************/ 00388000
*     /*                                                             */ 00389000
*     /* INITIALLY ALL SPECIAL BITS ARE CHECKED FOR ZERO TO MAKE SURE*/ 00390000
*     /* THE AIA HAS NO ERRORS. THEN THE NO ERROR PROCESSING IS DONE */ 00391000
*     /* AND THE SPECIAL PROCESSING FOLLOWS IT WITH ALL THE TESTS FOR*/ 00392000
*     /* EXCEPTIONAL CONDITIONS. THE REASONING IS TO GET THE NORMAL  */ 00393000
*     /* COMPLETION THROUGH AS FAST AS POSSIBLE.                     */ 00394000
*     /*                                                             */ 00395000
*     /***************************************************************/ 00396000
*                                                                  0178 00397000
*     IF AIAFRAUX='0'B&             /* WAS REQUEST PURGED            */ 00398000
*         AIAIORTY='0'B&            /* MUST IT BE RETRIED            */ 00399000
*         AIAPRIER='0'B&            /* WAS THERE AN I/O ERROR        */ 00400000
*         AIASECER='0'B&            /* WAS THERE AN I/O ERROR        */ 00401000
*         AIAERROR='0'B&            /* MISCELLANEOUS ERROR           */ 00402000
*         AIABADID='0'B THEN        /* WAS THERE A BAD LSID          */ 00403000
@RF00158 TM    AIAFRAUX(AIAPTR),B'10101111'                        0178 00404000
         BNZ   @RF00178                                            0178 00405000
*       DO;                         /* NO ERROR PROCESSING           */ 00406000
*         IF AIAWRITE='1'B THEN     /* IS REQUEST FOR A WRITE        */ 00407000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0180 00408000
         BNO   @RF00180                                            0180 00409000
*           DO;                     /* DO-WRITE PROCESSING           */ 00410000
*                                                                  0181 00411000
*             /*******************************************************/ 00412000
*             /*                                                     */ 00413000
*             /* THE AIA HAS COMPLETED OK. THE LSID MUST BE MOVED    */ 00414000
*             /* INTO THE XPTE. WE NEXT DETERMINE WHAT TYPE OF       */ 00415000
*             /* COMPLETION WE HAVE.                                 */ 00416000
*             /*                                                     */ 00417000
*             /*******************************************************/ 00418000
*                                                                  0182 00419000
*             IF AIAPRIV='1'B THEN  /* PRIVATE?                      */ 00420000
         TM    AIAPRIV(AIAPTR),B'01000000'                         0182 00421000
         BNO   @RF00182                                            0182 00422000
*               DO;                 /* YES-PRIVATE AREA COMPLETION   */ 00423000
*                                                                  0183 00424000
*                 /***************************************************/ 00425000
*                 /*                                                 */ 00426000
*                 /* IF THE PAGE IS INVOLVED IN A SWAP OPERATION PUT */ 00427000
*                 /* THE LSID INTO THE SPCTE FROM THE AIA.           */ 00428000
*                 /*                                                 */ 00429000
*                 /***************************************************/ 00430000
*                                                                  0184 00431000
*                 IF AIASWPFX='1'B THEN                            0184 00432000
         TM    AIASWPFX(AIAPTR),B'00000010'                        0184 00433000
         BNO   @RF00184                                            0184 00434000
*                   DO;             /* UPDATE SPCTE                  */ 00435000
*                     SPCTSSID=AIASSID;/* LSID                       */ 00436000
         L     @12,SWAPPTR(,PCBPTR)                                0186 00437000
         MVC   SPCTSSID(3,@12),AIASSID(AIAPTR)                     0186 00438000
*                     SPCTLVAL='1'B;/* VALIDATE                      */ 00439000
         OI    SPCTLVAL(@12),B'10000000'                           0187 00440000
*                   END;            /* END UPDATE SPCTE              */ 00441000
*                                                                  0189 00442000
*                 /***************************************************/ 00443000
*                 /*                                                 */ 00444000
*                 /* A TRAS IS DONE TO GET THE CORRECT STOR TO       */ 00445000
*                 /* ADDRESS THE XPTE FOR THIS SLOT.                 */ 00446000
*                 /*                                                 */ 00447000
*                 /***************************************************/ 00448000
*                                                                  0189 00449000
*                 RFY                                              0189 00450000
*                   R5 RSTD;                                       0189 00451000
@RF00184 DS    0H                                                  0190 00452000
*                 R5=RSMASCB;       /* ASCB ADDRESS                  */ 00453000
         L     R5,RSMASCB(,RSMHDPTR)                               0190 00454000
*                 ATAASCB=R5;       /* RECOVERY PTR                  */ 00455000
         ST    R5,ATAASCB(,ATAPTR)                                 0191 00456000
*                 DO;               /* ILRTRAS TO(R5)                */ 00457000
*                   DCL                                            0193 00458000
*                     TRASASID PTR(15) DEFINED(ASCBASID);          0193 00459000
*                   PSALCCAV->LCCAASID=R5->TRASASID;               0194 00460000
         LH    @12,TRASASID(,R5)                                   0194 00461000
         L     @07,PSALCCAV                                        0194 00462000
         ST    @12,LCCAASID(,@07)                                  0194 00463000
*                   GEN(LCTL  1,1,ASCBSTOR(R5))                    0195 00464000
*                   REFS(ASCBSTOR,R5);                             0195 00465000
         LCTL  1,1,ASCBSTOR(R5)                                         00466000
*                 END;              /* ILRTRAS TO(R5) GET STOR       */ 00467000
*                 XPTLSID=AIALSID;  /* XPTE LSID                     */ 00468000
         L     @12,AIALSID(,AIAPTR)                                0197 00469000
         ST    @12,XPTLSID(,XPTEPTR)                               0197 00470000
*                 XPTVALID='1'B;    /* VALIDATE                      */ 00471000
*                 XPTPOINP='0'B;                                   0199 00472000
         OI    XPTVALID(XPTEPTR),B'10000000'                       0199 00473000
         NI    XPTPOINP(XPTEPTR),B'11011111'                       0199 00474000
*                 DO;               /* ILRTRAS BACK                  */ 00475000
*                   RFY                                            0201 00476000
*                     R15 RSTD;                                    0201 00477000
*                   R15=PSAAOLD;                                   0202 00478000
         L     R15,PSAAOLD                                         0202 00479000
*                   GEN(LCTL  1,1,ASCBSTOR(15))                    0203 00480000
*                   REFS(ASCBSTOR);                                0203 00481000
         LCTL  1,1,ASCBSTOR(15)                                         00482000
*                   PSALCCAV->LCCAASID=0;                          0204 00483000
         SLR   @14,@14                                             0204 00484000
         L     @12,PSALCCAV                                        0204 00485000
         ST    @14,LCCAASID(,@12)                                  0204 00486000
*                   RFY                                            0205 00487000
*                     R15 UNRSTD;                                  0205 00488000
*                 END;              /* ILRTRAS BACK BACK TO STOR     */ 00489000
*                 RFY                                              0207 00490000
*                   R5 UNRSTD;      /* RELEASE REG                   */ 00491000
*                 ATAASCB=0;        /* ZERO RECOVERY FIELD           */ 00492000
         ST    @14,ATAASCB(,ATAPTR)                                0208 00493000
*               END;                /* END-PRIVATE AREA COMPLETION   */ 00494000
*                                                                  0210 00495000
*             /*******************************************************/ 00496000
*             /*                                                     */ 00497000
*             /* THE NEXT CHECK IS FOR A DUPLEX SLOT. IF IT IS       */ 00498000
*             /* DUPLEXED THE CASE IS TAKEN CARE OF WHERE THE COMMON */ 00499000
*             /* DATA SET WAS NOT AVAILABLE BUT THE DUPLEXED COPY WAS*/ 00500000
*             /* WRITTEN. THE LSID FOR THE DUPLEXED SLOT IS PUT IN   */ 00501000
*             /* PLACE OF THE LSID FOR THE COMMON DATA SET           */ 00502000
*             /*                                                     */ 00503000
*             /*******************************************************/ 00504000
*                                                                  0210 00505000
*             ELSE                  /* NOT PRIVATE AREA              */ 00506000
*               DO;                 /* DO-NOT PRIVATE AREA           */ 00507000
         B     @RC00182                                            0210 00508000
@RF00182 DS    0H                                                  0211 00509000
*                 IF AIADUPLX='1'B THEN/* DPLX'D                     */ 00510000
         TM    AIADUPLX(AIAPTR),B'00010000'                        0211 00511000
         BNO   @RF00211                                            0211 00512000
*                   DO;             /* YES CHECK COMMON LSID         */ 00513000
*                     IF AIALSID=0 THEN                            0213 00514000
         L     @12,AIALSID(,AIAPTR)                                0213 00515000
         LTR   @12,@12                                             0213 00516000
         BNZ   @RF00213                                            0213 00517000
*                       DO;         /* MOVE DPLX TO COMMON           */ 00518000
*                         AIALSID=AIALSID2;                        0215 00519000
         L     @12,AIALSID2(,AIAPTR)                               0215 00520000
         ST    @12,AIALSID(,AIAPTR)                                0215 00521000
*                         AIALSID2=''B;/* ZERO                       */ 00522000
         SLR   @12,@12                                             0216 00523000
         ST    @12,AIALSID2(,AIAPTR)                               0216 00524000
*                       END;        /* END-DPLX TO COMMON            */ 00525000
*                     XPTLPID=AIAID;                               0218 00526000
@RF00213 MVC   XPTLPID(8,XPTEPTR),AIAID(AIAPTR)                    0218 00527000
*                   END;            /* END-CHECK COMMON LSID         */ 00528000
*                 ELSE              /* NOT A DUPLEXED REQUEST        */ 00529000
*                   XPTLSID=AIALSID;/* UPDTE XPTE                    */ 00530000
         B     @RC00211                                            0220 00531000
@RF00211 L     @12,AIALSID(,AIAPTR)                                0220 00532000
         ST    @12,XPTLSID(,XPTEPTR)                               0220 00533000
*                 XPTVALID='1'B;    /* VALIDATE XPTE                 */ 00534000
@RC00211 DS    0H                                                  0222 00535000
*                 XPTPOINP='0'B;    /* TURN OFF IN PROGRESS BIT      */ 00536000
         OI    XPTVALID(XPTEPTR),B'10000000'                       0222 00537000
         NI    XPTPOINP(XPTEPTR),B'11011111'                       0222 00538000
*               END;                /* END-NOT PRIVATE AREA          */ 00539000
*           END;                    /* END-WRITE PROCESSING          */ 00540000
*                                                                  0224 00541000
*         /***********************************************************/ 00542000
*         /*                                                         */ 00543000
*         /* EITHER THE AIA IS FOR A READ COMPLETION OR A WRITE      */ 00544000
*         /* COMPLETION AND THERE ARE NO ERRORS WITH THE AIA. READ   */ 00545000
*         /* AIA'S REQUIRE NO SPECIFIC PROCESSING SO THEY ARE SENT   */ 00546000
*         /* DIRECTLY BACK TO IEAVPIOP.                              */ 00547000
*         /*                                                         */ 00548000
*         /***********************************************************/ 00549000
*                                                                  0225 00550000
*         GO TO ENDUP;              /* GO TO PUT THE AIA ON THE PIOPQ*/ 00551000
         B     ENDUP                                               0225 00552000
*       END;                        /* END-NO ERROR PROCESSING       */ 00553000
*                                                                  0227 00554000
*     /***************************************************************/ 00555000
*     /*                                                             */ 00556000
*     /* IF SOMETHING HAPPENED TO THE AIA WHILE PROCESSING IT        */ 00557000
*     /* (AIABADID='1' OR AIAERROR='1' THE AIA IS QUEUED DIRECTLY TO */ 00558000
*     /* THE INTERNAL Q FOR IEAVPIOP. IF NEITHER OF THESE ERRORS READ*/ 00559000
*     /* OR WRITE PROCESSING IS DONE                                 */ 00560000
*     /*                                                             */ 00561000
*     /***************************************************************/ 00562000
*                                                                  0227 00563000
*     IF(AIABADID='1'B|             /* IS ERROR BIT ON               */ 00564000
*         AIAERROR='1'B) THEN       /* IS ERROR BIT ON               */ 00565000
@RF00178 TM    AIABADID(AIAPTR),B'00000011'                        0227 00566000
         BNZ   @RT00227                                            0227 00567000
*       GO TO ENDUP;                /* GO TO QUEUE AIA FOR PIOP      */ 00568000
*                                                                  0228 00569000
*     /***************************************************************/ 00570000
*     /*                                                             */ 00571000
*     /* BOTH OF THE ERROR BITS ARE OFF SO READ OR WRITE PROCESSING  */ 00572000
*     /* WILL BE DONE.                                               */ 00573000
*     /*                                                             */ 00574000
*     /***************************************************************/ 00575000
*                                                                  0229 00576000
*     IF AIAWRITE='1'B THEN         /* IS THIS A WRITE               */ 00577000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0229 00578000
         BNO   @RF00229                                            0229 00579000
*       DO;                         /* YES-DO WRITE PROCESSING       */ 00580000
*                                                                  0230 00581000
*         /***********************************************************/ 00582000
*         /*                                                         */ 00583000
*         /* THE AIA IS FIRST CHECKED TO SEE IF THE REQUEST WAS      */ 00584000
*         /* PURGED DURING PROCESSING. IF IT WAS PURGED THE SLOTS ARE*/ 00585000
*         /* FREED AND THE PROPER COUNTS ARE DECREMENTED.            */ 00586000
*         /*                                                         */ 00587000
*         /***********************************************************/ 00588000
*                                                                  0231 00589000
*         IF AIAFRAUX='1'B THEN     /* WAS REQUEST PURGED            */ 00590000
         TM    AIAFRAUX(AIAPTR),B'10000000'                        0231 00591000
         BNO   @RF00231                                            0231 00592000
*           DO;                     /* DO-PURGE PROCESSING           */ 00593000
*             IF AIADUPLX='1'B THEN /* IS IT DUPLEXED                */ 00594000
         TM    AIADUPLX(AIAPTR),B'00010000'                        0233 00595000
         BNO   @RF00233                                            0233 00596000
*               DO;                                                0234 00597000
*                                                                  0235 00598000
*                 /***************************************************/ 00599000
*                 /*                                                 */ 00600000
*                 /* IF NO WRITE ERRORS OCCURRED THE SLOT IS FREED   */ 00601000
*                 /* FOR FURTHER USE.                                */ 00602000
*                 /*                                                 */ 00603000
*                 /***************************************************/ 00604000
*                                                                  0235 00605000
*                 IF AIALSID2^=0&   /* IS THERE A SECONDARY SLOT TO     00606000
*                                      BE FREED                      */ 00607000
*                     AIASECER='0'B THEN/* NO ERROR?                 */ 00608000
         L     @12,AIALSID2(,AIAPTR)                               0235 00609000
         LTR   @12,@12                                             0235 00610000
         BZ    @RF00235                                            0235 00611000
         TM    AIASECER(AIAPTR),B'00000100'                        0235 00612000
         BNZ   @RF00235                                            0235 00613000
*                   CALL FREESLT2;  /* GO TO FREE SLOT               */ 00614000
         BAL   @14,FREESLT2                                        0236 00615000
*               END;                /* END-IS IT DUPLEXED            */ 00616000
*                                                                  0237 00617000
@RF00235 DS    0H                                                  0238 00618000
*             /*******************************************************/ 00619000
*             /*                                                     */ 00620000
*             /* SLOTS ARE FREED ONLY IF THERE IS NO ERROR TRYING TO */ 00621000
*             /* PROCESS THEM. ONCE AN ERROR OCCURS THE SLOT IS NOT  */ 00622000
*             /* REUSED.                                             */ 00623000
*             /*                                                     */ 00624000
*             /*******************************************************/ 00625000
*                                                                  0238 00626000
*             IF AIAPRIER='0'B THEN /* NO ERROR?                     */ 00627000
@RF00233 TM    AIAPRIER(AIAPTR),B'00001000'                        0238 00628000
         BNZ   @RF00238                                            0238 00629000
*               CALL FREESLT1;      /* FREE THE SLOT                 */ 00630000
*                                                                  0239 00631000
         BAL   @14,FREESLT1                                        0239 00632000
*             /*******************************************************/ 00633000
*             /*                                                     */ 00634000
*             /* IF THE AIA REPRESENTS A PRIVATE AREA PAGE THE       */ 00635000
*             /* NON-VIO ALLOCATED SLOT COUNTS IN THE ASMVT AND ASCB */ 00636000
*             /* MUST BE DECREMENTED                                 */ 00637000
*             /*                                                     */ 00638000
*             /*******************************************************/ 00639000
*                                                                  0240 00640000
*             IF AIAPRIV='1'B THEN  /* PRIVATE AREA PG?              */ 00641000
@RF00238 TM    AIAPRIV(AIAPTR),B'01000000'                         0240 00642000
         BNO   @RF00240                                            0240 00643000
*               DO;                 /* YES-DECREMENT COUNTS          */ 00644000
*                 ASCBNVSC=ASCBNVSC-1;/* ASCB COUNT                  */ 00645000
         L     @12,ASCBPTR(,PCBPTR)                                0242 00646000
         LH    @07,ASCBNVSC(,@12)                                  0242 00647000
         BCTR  @07,0                                               0242 00648000
         STH   @07,ASCBNVSC(,@12)                                  0242 00649000
*                 ASMNVSC=ASMNVSC-1;/* DECREMENT COUNT               */ 00650000
         L     @12,ASMNVSC(,ASMVTPTR)                              0243 00651000
         BCTR  @12,0                                               0243 00652000
         ST    @12,ASMNVSC(,ASMVTPTR)                              0243 00653000
*               END;                /* END- DECREMENT COUNTS         */ 00654000
*             GO TO ENDUP;          /* GO TO QUEUE FOR PIOP          */ 00655000
         B     ENDUP                                               0245 00656000
*           END;                    /* END-WAS REQUEST PURGED        */ 00657000
*                                                                  0247 00658000
*         /***********************************************************/ 00659000
*         /*                                                         */ 00660000
*         /* IF ANY ERRORS OCCURRED THE AIA WILL BE REQUEUED TO THE  */ 00661000
*         /* ASMSTAGQ FOR REDRIVE. IF ANYTHING CAN BE CLEANED UP     */ 00662000
*         /* PRIOR TO REDRIVE IT IS DONE HERE.                       */ 00663000
*         /*                                                         */ 00664000
*         /***********************************************************/ 00665000
*                                                                  0247 00666000
*         IF AIAPRIER='1'B|         /* ERROR ON PRIMARY?             */ 00667000
*             AIASECER='1'B THEN    /* ERR ON SECONDRY               */ 00668000
@RF00231 TM    AIAPRIER(AIAPTR),B'00001100'                        0247 00669000
         BZ    @RF00247                                            0247 00670000
*           DO;                     /* YES-AIA MUST BE REDRIVEN      */ 00671000
*             IF AIADUPLX='1'B THEN /* DUPLEXED?                     */ 00672000
         TM    AIADUPLX(AIAPTR),B'00010000'                        0249 00673000
         BNO   @RF00249                                            0249 00674000
*               DO;                 /* YES-DUPLEX ERROR PROCESSING   */ 00675000
*                                                                  0250 00676000
*                 /***************************************************/ 00677000
*                 /*                                                 */ 00678000
*                 /* WE NEXT CHECK IF THE AIA IS FOR REDRIVE. IF IT  */ 00679000
*                 /* IS WE FREE UP THE ALLOCATED SLOTS AND PUT THE   */ 00680000
*                 /* AIA ON THE ASMSTAGQ                             */ 00681000
*                 /*                                                 */ 00682000
*                 /***************************************************/ 00683000
*                                                                  0251 00684000
*                 IF AIAIORTY='1'B THEN/* REDRIVE                    */ 00685000
         TM    AIAIORTY(AIAPTR),B'00100000'                        0251 00686000
         BNO   @RF00251                                            0251 00687000
*                   DO;             /* DO-RETRY PROCESSING           */ 00688000
*                     AIAIORTY='0'B;/* BIT OFF                       */ 00689000
         NI    AIAIORTY(AIAPTR),B'11011111'                        0253 00690000
*                     IF AIAPRIER='0'B THEN/* ERR                    */ 00691000
         TM    AIAPRIER(AIAPTR),B'00001000'                        0254 00692000
         BNZ   @RF00254                                            0254 00693000
*                       CALL FREESLT1;/* FREE SLOT                   */ 00694000
         BAL   @14,FREESLT1                                        0255 00695000
*                     IF AIASECER='0'B THEN/* ERR                    */ 00696000
@RF00254 TM    AIASECER(AIAPTR),B'00000100'                        0256 00697000
         BNZ   @RF00256                                            0256 00698000
*                       CALL FREESLT2;/* FREE SLOT                   */ 00699000
         BAL   @14,FREESLT2                                        0257 00700000
*                     AIAPRIER='0'B;/* ERR OFF                       */ 00701000
@RF00256 DS    0H                                                  0259 00702000
*                     AIASECER='0'B;/* ERR OFF                       */ 00703000
         NI    AIAPRIER(AIAPTR),B'11110011'                        0259 00704000
*                     AIAID=''B;    /* ZERO LSID AREA                */ 00705000
         XC    AIAID(8,AIAPTR),AIAID(AIAPTR)                       0260 00706000
*                     GO TO STAGEAIA;/* STAGEQ                       */ 00707000
         B     STAGEAIA                                            0261 00708000
*                   END;            /* END-RETRY PROCESSING          */ 00709000
*                                                                  0263 00710000
*                 /***************************************************/ 00711000
*                 /*                                                 */ 00712000
*                 /* THERE IS AN ERROR IN THE AIA (AIAPRIER OR       */ 00713000
*                 /* AIASECER) BUT THE RETRY (AIAIORTY) FLAG WAS NOT */ 00714000
*                 /* ON.                                             */ 00715000
*                 /*                                                 */ 00716000
*                 /***************************************************/ 00717000
*                                                                  0263 00718000
*                 ELSE              /* NOT A RETRY                   */ 00719000
*                   DO;             /* DO-NOT A DUPLEX RETRY         */ 00720000
*                                                                  0263 00721000
@RF00251 DS    0H                                                  0264 00722000
*                     /***********************************************/ 00723000
*                     /*                                             */ 00724000
*                     /* WE KNOW THERE IS SOME TYPE OF ERROR AND IF  */ 00725000
*                     /* IT IS ONLY A PRIMARY(AIAPRIER) AND THERE IS */ 00726000
*                     /* NO COMMON DATA SET AVAILABLE THE LSID FOR   */ 00727000
*                     /* THE DUPLEXED DATA SET IS USED AS THE ONE AND*/ 00728000
*                     /* ONLY COPY AND THE AIA WILL NOT BE REDRIVEN. */ 00729000
*                     /*                                             */ 00730000
*                     /***********************************************/ 00731000
*                                                                  0264 00732000
*                     IF AIASECER='0'B THEN/* SEC ERR                */ 00733000
         TM    AIASECER(AIAPTR),B'00000100'                        0264 00734000
         BNZ   @RF00264                                            0264 00735000
*                       DO;         /* NO DO-CHK CW QUEUE            */ 00736000
*                         IF ASMNOCWQ='1'B THEN/* COMMON             */ 00737000
         TM    ASMNOCWQ(ASMVTPTR),B'01000000'                      0266 00738000
         BNO   @RF00266                                            0266 00739000
*                           DO;     /* USE ONLY ONE SLOT             */ 00740000
*                             AIALSID=AIALSID2;/* LSID               */ 00741000
         L     @12,AIALSID2(,AIAPTR)                               0268 00742000
         ST    @12,AIALSID(,AIAPTR)                                0268 00743000
*                             AIALSID2=''B;/* ZERO LSID2             */ 00744000
         SLR   @12,@12                                             0269 00745000
         ST    @12,AIALSID2(,AIAPTR)                               0269 00746000
*                             AIAPRIER='0'B;/* ERR OFF               */ 00747000
         NI    AIAPRIER(AIAPTR),B'11110111'                        0270 00748000
*                             XPTLPID=AIAID;/* UPDTE XPTE            */ 00749000
         MVC   XPTLPID(8,XPTEPTR),AIAID(AIAPTR)                    0271 00750000
*                             XPTVALID='1'B;/* VALIDATE              */ 00751000
*                             XPTPOINP='0'B;/* ZERO IN PROG          */ 00752000
         OI    XPTVALID(XPTEPTR),B'10000000'                       0273 00753000
         NI    XPTPOINP(XPTEPTR),B'11011111'                       0273 00754000
*                           END;    /* USE ONLY ONE SLOT             */ 00755000
*                         ELSE      /* COMMON DS AVAILABLE           */ 00756000
*                           GO TO STAGEAIA;/* QUE AIA TO STAGEQ      */ 00757000
*                       END;                                       0276 00758000
*                     ELSE          /* MUST BE SECONDARY ERROR       */ 00759000
*                                                                  0277 00760000
*                       /*********************************************/ 00761000
*                       /*                                           */ 00762000
*                       /* IF BOTH ERROR BITS ARE ON AND THERE IS NO */ 00763000
*                       /* COMMON DATA SET ONLY THE SECONDARY MUST BE*/ 00764000
*                       /* RETRIED SO THE AIAPRIER FLAG IS TURNED    */ 00765000
*                       /* OFF.                                      */ 00766000
*                       /*                                           */ 00767000
*                       /*********************************************/ 00768000
*                                                                  0277 00769000
*                       IF ASMDUPLX='1'B THEN/* DUPLX ACTIVE         */ 00770000
         B     @RC00264                                            0277 00771000
@RF00264 TM    ASMDUPLX(ASMVTPTR),B'10000000'                      0277 00772000
         BNO   @RF00277                                            0277 00773000
*                         DO;       /* CHK FOR PRIMARY ALSO          */ 00774000
*                           IF AIAPRIER='1'B&/* PRIMARY ERROR??      */ 00775000
*                               ASMNOCWQ='1'B THEN/* COMM DS??       */ 00776000
         TM    AIAPRIER(AIAPTR),B'00001000'                        0279 00777000
         BNO   @RF00279                                            0279 00778000
         TM    ASMNOCWQ(ASMVTPTR),B'01000000'                      0279 00779000
         BNO   @RF00279                                            0279 00780000
*                             DO;   /* SET UP FOR NO PRIMARY RETRY   */ 00781000
*                               AIALSID=''B;/* ZERO THE LSID         */ 00782000
         SLR   @12,@12                                             0281 00783000
         ST    @12,AIALSID(,AIAPTR)                                0281 00784000
*                               AIAPRIER='0'B;/* TURN OFF ERROR FLAG */ 00785000
         NI    AIAPRIER(AIAPTR),B'11110111'                        0282 00786000
*                             END;  /* SET UP FOR NO PRIMARY RETRY   */ 00787000
*                           GO TO STAGEAIA;/* GO TO PUT THE AIA ON 0284 00788000
*                                      STAGE Q                       */ 00789000
         B     STAGEAIA                                            0284 00790000
*                         END;                                     0285 00791000
*                       ELSE        /* DUPLEX NOT ACTIVE             */ 00792000
*                                                                  0286 00793000
*                         /*******************************************/ 00794000
*                         /*                                         */ 00795000
*                         /* THIS AN AIA WITH A SECONDARY ERROR BUT  */ 00796000
*                         /* DUPLEXING IS NOT ACTIVE HENCE THE       */ 00797000
*                         /* REQUEST WILL NOT BE RETRIED FOR THE     */ 00798000
*                         /* DUPLEX COPY.                            */ 00799000
*                         /*                                         */ 00800000
*                         /*******************************************/ 00801000
*                                                                  0286 00802000
*                         DO;       /* SECONDARY ERROR               */ 00803000
@RF00277 DS    0H                                                  0287 00804000
*                           AIASECER='0'B;/* ERR OFF                 */ 00805000
         NI    AIASECER(AIAPTR),B'11111011'                        0287 00806000
*                           AIALSID2=''B;/* ZERO LSID                */ 00807000
         SLR   @12,@12                                             0288 00808000
         ST    @12,AIALSID2(,AIAPTR)                               0288 00809000
*                           AIADUPLX='0'B;/* DPX OFF                 */ 00810000
*                                                                  0289 00811000
         NI    AIADUPLX(AIAPTR),B'11101111'                        0289 00812000
*                           /*****************************************/ 00813000
*                           /*                                       */ 00814000
*                           /* IF A PRIMARY ERROR ALSO OCCURRED THE  */ 00815000
*                           /* AIA MUST BE REDRIVEN. IF THE PRIMARY  */ 00816000
*                           /* COPY IS OK MOVE THE LSID INTO THE XPTE*/ 00817000
*                           /* AND VALIDATE IT. THERE WILL BE NO     */ 00818000
*                           /* DUPLEX COPY FOR THIS PAGE.            */ 00819000
*                           /*                                       */ 00820000
*                           /*****************************************/ 00821000
*                                                                  0290 00822000
*                           IF AIAPRIER='1'B THEN                  0290 00823000
         TM    AIAPRIER(AIAPTR),B'00001000'                        0290 00824000
         BO    @RT00290                                            0290 00825000
*                             GO TO STAGEAIA;                      0291 00826000
*                           ELSE    /* NO PRIMARY ERROR              */ 00827000
*                             DO;   /* VALIDATE XPTE                 */ 00828000
*                               XPTLPID=AIAID;/* LSID                */ 00829000
         MVC   XPTLPID(8,XPTEPTR),AIAID(AIAPTR)                    0293 00830000
*                               XPTVALID='1'B;/* VALID               */ 00831000
*                               XPTPOINP='0'B;                     0295 00832000
         OI    XPTVALID(XPTEPTR),B'10000000'                       0295 00833000
         NI    XPTPOINP(XPTEPTR),B'11011111'                       0295 00834000
*                             END;  /* END-VALIDATE XPTE             */ 00835000
*                         END;      /* END SECONDARY ERROR           */ 00836000
*                   END;            /* END-NOT A DUPLEX RETRY        */ 00837000
@RC00264 DS    0H                                                  0299 00838000
*               END;                /* END-DUPLEX ERROR PROCESSING   */ 00839000
*                                                                  0299 00840000
*             /*******************************************************/ 00841000
*             /*                                                     */ 00842000
*             /* SINCE IT IS NOT DUPLEXED THE AIA CAN BE PUT ON THE  */ 00843000
*             /* STAGE Q FOR REDRIVE WITHOUT ANY FURTHER CHECKING    */ 00844000
*             /*                                                     */ 00845000
*             /*******************************************************/ 00846000
*                                                                  0300 00847000
*             ELSE                  /* NOT A DUPLEXED SLOT           */ 00848000
*               DO;                                                0300 00849000
         B     @RC00249                                            0300 00850000
@RF00249 DS    0H                                                  0301 00851000
*                 AIAPRIER='0'B;    /* ERR BIT OFF                   */ 00852000
         NI    AIAPRIER(AIAPTR),B'11110111'                        0301 00853000
*                 AIALSID=''B;      /* ZERO LSID                     */ 00854000
         SLR   @12,@12                                             0302 00855000
         ST    @12,AIALSID(,AIAPTR)                                0302 00856000
*                 GO TO STAGEAIA;   /* AIA TO STAGEQ                 */ 00857000
         B     STAGEAIA                                            0303 00858000
*               END;                                               0304 00859000
*           END;                    /* END-AIA MUST BE REDRIVEN      */ 00860000
*                                                                  0306 00861000
*         /***********************************************************/ 00862000
*         /*                                                         */ 00863000
*         /* THIS CODE IS FOR THE AIA THAT HAS COMPLETED WITH NO     */ 00864000
*         /* ERRORS. IT MIGHT HAVE TO BE RETRIED SO THE RETRY BIT    */ 00865000
*         /* (AIAIORTY) IS TESTED. THE FIRST CHECK MADE IS TO SEE IF */ 00866000
*         /* A RETRY IS SPECIFIED. IF SO THE ALLOCATED SLOTS ARE     */ 00867000
*         /* FREED AND THE AIA IS QUEUED TO THE ASMSTGQ.             */ 00868000
*         /*                                                         */ 00869000
*         /***********************************************************/ 00870000
*                                                                  0306 00871000
*         ELSE                      /* NO ERRORS ON THE REQUEST      */ 00872000
*           DO;                     /* NO ERROR COMPLETION           */ 00873000
         B     @RC00247                                            0306 00874000
@RF00247 DS    0H                                                  0307 00875000
*             IF AIAIORTY='1'B THEN /* RETRY?                        */ 00876000
         TM    AIAIORTY(AIAPTR),B'00100000'                        0307 00877000
         BNO   @RF00307                                            0307 00878000
*               DO;                 /* CLEAN UP FOR RETRY            */ 00879000
*                 AIAIORTY='0'B;    /* TURN BIT OFF                  */ 00880000
         NI    AIAIORTY(AIAPTR),B'11011111'                        0309 00881000
*                 CALL FREESLT1;    /* FREE PRIMARY                  */ 00882000
         BAL   @14,FREESLT1                                        0310 00883000
*                 IF AIALSID2^=0 THEN/* SECONDARY                    */ 00884000
         L     @12,AIALSID2(,AIAPTR)                               0311 00885000
         LTR   @12,@12                                             0311 00886000
         BZ    @RF00311                                            0311 00887000
*                   CALL FREESLT2;  /* YES-FREE IT                   */ 00888000
         BAL   @14,FREESLT2                                        0312 00889000
*                 AIAID=''B;        /* ZERO LSID FIELDS              */ 00890000
@RF00311 XC    AIAID(8,AIAPTR),AIAID(AIAPTR)                       0313 00891000
*                 GO TO STAGEAIA;   /* AIA TO STAGEQ                 */ 00892000
         B     STAGEAIA                                            0314 00893000
*               END;                /* END-CLEAN UP FOR RETRY        */ 00894000
*           END;                    /* END-NO ERROR COMPLETION       */ 00895000
*         GO TO ENDUP;              /* GO TO QUEUE AIA FOR PIOP      */ 00896000
         B     ENDUP                                               0317 00897000
*       END;                        /* END-DO WRITE PROCESSING       */ 00898000
*                                                                  0319 00899000
*     /***************************************************************/ 00900000
*     /*                                                             */ 00901000
*     /* THIS CODE IS THE COMPLETION PROCESSING FOR READ REQUESTS.   */ 00902000
*     /* THE ONLY TIME READ REQUESTS ARE REDRIVEN IS WHEN A DUPLEXED */ 00903000
*     /* COPY IS AVAILABLE. FOR NON-RETRIABLE READ ERRORS THE        */ 00904000
*     /* XPTIOERR FLAG IS TURNED ON. IF THE REQUEST WAS PURGED THE   */ 00905000
*     /* ALLOCATED SLOTS ARE FREED AND COUNTS ARE UPDATED.           */ 00906000
*     /*                                                             */ 00907000
*     /***************************************************************/ 00908000
*                                                                  0319 00909000
*     IF AIAFRAUX='1'B THEN         /* WAS REQUEST PURGED            */ 00910000
@RF00229 TM    AIAFRAUX(AIAPTR),B'10000000'                        0319 00911000
         BNO   @RF00319                                            0319 00912000
*       DO;                         /* DO-READ PURGE PROCESSING      */ 00913000
*         IF AIAPRIER='0'B THEN     /* I/O ERROR?                    */ 00914000
         TM    AIAPRIER(AIAPTR),B'00001000'                        0321 00915000
