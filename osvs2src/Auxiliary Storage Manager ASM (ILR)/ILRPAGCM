*               CALL FREESLT2;      /* YES-FREE IT                   */ 00942000
         BAL   @14,FREESLT2                                        0330 00943000
*           END;                                                   0331 00944000
*       END;                        /* END-READ PURGE PROCESSING     */ 00945000
*                                                                  0332 00946000
*     /***************************************************************/ 00947000
*     /*                                                             */ 00948000
*     /* A CHECK IS MADE TO SEE IF THE REQUEST SHOULD BE RETRIED AND */ 00949000
*     /* IF SO IT IS PUT BACK ON THE ASMSTAGQ.                       */ 00950000
*     /*                                                             */ 00951000
*     /***************************************************************/ 00952000
*                                                                  0333 00953000
*     ELSE                          /* NOT A PURGE REQUEST           */ 00954000
*       DO;                         /* NON PURGE PROCESSING          */ 00955000
