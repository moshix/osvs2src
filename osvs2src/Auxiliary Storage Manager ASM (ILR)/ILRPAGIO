         TITLE 'ILRPAGIO-PAGING I/O CONTROL MODULE                     *00001000
                        '                                               00002000
ILRPAGIO CSECT                                                          00003000
        USING *,R15                                                     00004000
        MODID                                                           00005000
        L     R15,FLCCVT              GET THE ADDRESS OF THE CVT        00006000
        USING CVTMAP,R15              USE R15 TO ADDRESS THE CVT        00007000
        L     R15,CVTASMVT            PICK UP ADDRESS OF THE ASMVT      00008000
        USING ASMVT,R15               USE R15 TO ADDRESS THE ASMVT      00009000
        STM   R2,R14,ASMWKSA1+4       STORE REGISTERS IN WORK/SAVE      00010000
        BALR  R10,0                   GET BASE ADDRESS                  00011000
        USING *,R10                   SET UP CODE ADDRESSABILITY        00012000
        DROP 15                       DROP 15 AS BASE                   00013000
*ILRCBPGI:                                                         0077 00014000
*   ASMVTPTR=R15;                   /* PUT ASMVT ADDRESS IN STANDARD    00015000
*                                      POINTER REGISTER              */ 00016000
ILRCBPGI LR    ASMVTPTR,R15                                        0077 00017000
*   RFY                                                            0078 00018000
*     R15 UNRSTD;                   /* RELEASE REG FOR USE           */ 00019000
*   ASM1RG1=0;                      /* ZERO PARAMETER REG SAVE       */ 00020000
         SLR   @12,@12                                             0079 00021000
         ST    @12,ASM1RG1(,ASMVTPTR)                              0079 00022000
*   ASM1RG15=0;                     /* ZERO RETURN CODE REG SAVE     */ 00023000
         ST    @12,ASM1RG15(,ASMVTPTR)                             0080 00024000
*   RFY                                                            0081 00025000
*     R8 RSTD;                      /* RESTRICT REG FROM USE         */ 00026000
*   R8=ASMPIOFR;                    /* GET ADDRESS OF THE FRR        */ 00027000
         L     R8,ASMPIOFR(,ASMVTPTR)                              0082 00028000
*                                                                  0083 00029000
*   /*****************************************************************/ 00030000
*   /*                                                               */ 00031000
*   /* WE WILL NOW ESTABLISH THE FUNCTIONAL RECOVERY ROUTINE AND ON  */ 00032000
*   /* RETURN REGISTER 4 WILL HAVE THE ADDRESS OF THE 24 BYTE ASM    */ 00033000
*   /* TRACKING AREA(ATA).                                           */ 00034000
*   /*                                                               */ 00035000
*   /*****************************************************************/ 00036000
*                                                                  0083 00037000
*   GENERATE REFS(PSACSTK,PSA,FRRSCURR,FRRS,FRRSLAST,PSALSFCC,FRRSELEN, 00038000
*       FRRSFRRA,FRRSENTR,FRRSPARM);                               0083 00039000
        SETFRR A,FRRAD=(8),WRKREGS=(9,12),PARMAD=(4)                    00040000
*   RFY                                                            0084 00041000
*     R8 UNRSTD;                    /* RELEASE REG FOR USE           */ 00042000
*   ATAMODID=ATAMPGIO;              /* PUT MODID IN ATA              */ 00043000
         MVI   ATAMODID(ATAPTR),X'01'                              0085 00044000
*   ATAPAGIO='1'B;                  /* TURN ON FLAG IN ATA           */ 00045000
         OI    ATAPAGIO(ATAPTR),B'00000010'                        0086 00046000
*   ATAAIA=R1;                      /* GET THE ADDRESS OF THE AIA    */ 00047000
         ST    R1,ATAAIA(,ATAPTR)                                  0087 00048000
*   AIAPTR=R1;                      /* SET UP AIA POINTER            */ 00049000
         LR    AIAPTR,R1                                           0088 00050000
*   RFY                                                            0089 00051000
*     R1 UNRSTD;                    /* RELEASE REG FOR USE           */ 00052000
*                                                                  0089 00053000
*   /*****************************************************************/ 00054000
*   /*                                                               */ 00055000
*   /* NEXT WE COMPUTE THE ADDRESS OF THE PCB WHICH PRECEDES THE AIA.*/ 00056000
*   /* WE NEED THIS ADDRESS TO GET THE XPTE                          */ 00057000
*   /*                                                               */ 00058000
*   /*****************************************************************/ 00059000
*                                                                  0090 00060000
*   RFY                                                            0090 00061000
*     PCBPTR RSTD;                  /* RESTRICT USE OF XPTEPTR       */ 00062000
*   RFY                                                            0091 00063000
*     ASCB BASED(ASM1ASCB);         /* ADDRESSABILITY FOR ASCB       */ 00064000
*   PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));                       0092 00065000
         LR    PCBPTR,AIAPTR                                       0092 00066000
         SL    PCBPTR,@CF01755                                     0092 00067000
*   ASM1ASCB=PCBASCB;               /* GET THE ADDRESS OF THE ASCB   */ 00068000
         L     @12,PCBASCB(,PCBPTR)                                0093 00069000
         ST    @12,ASM1ASCB(,ASMVTPTR)                             0093 00070000
*                                                                  0094 00071000
*   /*****************************************************************/ 00072000
*   /*                                                               */ 00073000
*   /* THIS DO LOOP WILL PROCESS THE CHAIN OF AIA'S JUST PASSED BY   */ 00074000
*   /* THE CALLER.                                                   */ 00075000
*   /*                                                               */ 00076000
*   /*****************************************************************/ 00077000
*                                                                  0094 00078000
*   DO WHILE(AIAPTR^=0);            /* CYCLE THROUGH AIA CHAIN       */ 00079000
         B     @DE00094                                            0094 00080000
@DL00094 DS    0H                                                  0095 00081000
*     RFY                                                          0095 00082000
*       XPTEPTR RSTD;               /* RESTRICT USE OF XPTEPTR       */ 00083000
*     XPTEPTR=PCBXPT;               /* GET ADDRESS OF THE XPTE       */ 00084000
         L     XPTEPTR,PCBXPT(,PCBPTR)                             0096 00085000
*     RFY                                                          0097 00086000
*       PCBPTR UNRSTD;              /* RELEASE PCBPTR FOR USE        */ 00087000
*     IF AIAWRITE='0'B THEN         /* IS THIS A READ AIA            */ 00088000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0098 00089000
         BNZ   @RF00098                                            0098 00090000
*       DO;                         /* YES-DO THE READ PROCESSING    */ 00091000
*/********************************************************************/ 00092000
*/*                                                                  */ 00093000
*/* FUNCTION = THIS CODE PROCESSES READ REQUESTS(AIAWRITE='0').      */ 00094000
*/*            IF THIS IS A PRIVATE AREA PAGE IN A SWAP, UPDATE THE  */ 00095000
*/*            ASHIOCNT AND QUEUE THE AIA TO THE ASMSTAGQ.           */ 00096000
*/*            IF THE XPTE SPECIFIES A VIO SLOT THE AIA IS SENT TO   */ 00097000
*/*            ILRPOS TO CHECK OUT THE REQUEST. THE AIA IS DEQUEUED  */ 00098000
*/*            AND A SINGLE AIA IS SENT. IF POS RETURNS AN AIA IN THE*/ 00099000
*/*            ATA IT MEANS THE AIA IS AN ERROR SO THIS AIA AND THE  */ 00100000
*/*            REST OF THE CHAIN WILL BE RETURNED TO RSM.            */ 00101000
*/*            IF AN ERROR BIT IS ON IN THE XPTE PUT AN ERROR BIT ON */ 00102000
*/*            IN THE AIA AND RETURN THE AIA CHAIN TO RSM.           */ 00103000
*/*            IF NO ERRORS ARE FOUND AND IT IS NOT A VIO REQUEST    */ 00104000
*/*            MOVE THE LSID FROM THE XPTE TO THE AIA.               */ 00105000
*/*                                                                  */ 00106000
*/* INPUT = RSMHDPTR = ADDRESS OF THE RSMHD                          */ 00107000
*/*         ASMVTPTR = ADDRESS OF THE ASMVT                          */ 00108000
*/*         ATAPTR = ADDRESS OF THE ATA                              */ 00109000
*/*         AIAPTR = ADDRESS OF THE CURRENT AIA                      */ 00110000
*/*         XPTEPTR = ADDRESS OF THE XPTE                            */ 00111000
*/*                                                                  */ 00112000
*/* EXTERNAL ROUTINES AND REFERENCES = ILRPOS TO CHECK VIO REQUESTS  */ 00113000
*/*                                                                  */ 00114000
*/********************************************************************/ 00115000
*                                                                  0100 00116000
*         IF AIASWPFX='1'B THEN     /* IS THIS A P.A. PAGE IN SWAP   */ 00117000
         TM    AIASWPFX(AIAPTR),B'00000010'                        0100 00118000
         BNO   @RF00100                                            0100 00119000
*           DO;                     /* YES-NO SETUP REQUIRED         */ 00120000
*             ASHIOCNT=ASHIOCNT+1;  /* INCREMENT COUNT OF P.A. I/O 0102 00121000
*                                      REQUESTS                      */ 00122000
         LA    @12,1                                               0102 00123000
         AL    @12,ASHIOCNT(,RSMHDPTR)                             0102 00124000
         ST    @12,ASHIOCNT(,RSMHDPTR)                             0102 00125000
*             GO TO QUEIT;          /* TO TO QUEUE THE AIA           */ 00126000
         B     QUEIT                                               0103 00127000
*           END;                    /* END SWAP PROCESSING           */ 00128000
*                                                                  0105 00129000
*         /***********************************************************/ 00130000
*         /*                                                         */ 00131000
*         /* VIO AIA'S MUST BE SENT TO POS TO BE CHECKED. POS WILL   */ 00132000
*         /* PUT IT ON THE ASMSTAGQ IF IT CAN BE STARTED. A NON-ZERO */ 00133000
*         /* ATAAIA FROM POS MEANS THE AIA IS AN ERROR.              */ 00134000
*         /*                                                         */ 00135000
*         /***********************************************************/ 00136000
*                                                                  0105 00137000
*         IF XPTVIOLP='1'B THEN     /* IS THIS A VIO XPTE            */ 00138000
@RF00100 TM    XPTVIOLP(XPTEPTR),B'10000000'                       0105 00139000
         BNO   @RF00105                                            0105 00140000
*           DO;                     /* PREPARE FOR ILRPOS00          */ 00141000
*             AIALPID=XPTLPID;      /* MOVE LPID INTO AIA            */ 00142000
         MVC   AIALPID(8,AIAPTR),XPTLPID(XPTEPTR)                  0107 00143000
*             AIAVIO='1'B;          /* MARK AIA AS VIO AIA           */ 00144000
         OI    AIAVIO(AIAPTR),B'00100000'                          0108 00145000
*             ASM1RG1=AIANXAIA;     /* SAVE NEXT AIA ADDRESS         */ 00146000
         L     @12,AIANXAIA(,AIAPTR)                               0109 00147000
         ST    @12,ASM1RG1(,ASMVTPTR)                              0109 00148000
*             AIANXAIA=0;           /* DEQUEUE THE REST OF CHAIN     */ 00149000
         SLR   @12,@12                                             0110 00150000
         ST    @12,AIANXAIA(,AIAPTR)                               0110 00151000
*             CALL ILRPOS;          /* CALL POS FOR CHECKING AIA'S   */ 00152000
         L     @15,@CV01693                                        0111 00153000
         BALR  @14,@15                                             0111 00154000
*             IF ATAAIA=0 THEN      /* IS THIS AIA OK                */ 00155000
         L     @12,ATAAIA(,ATAPTR)                                 0112 00156000
         LTR   @12,@12                                             0112 00157000
         BNZ   @RF00112                                            0112 00158000
*               DO;                 /* YES-THERE IS NO ERROR         */ 00159000
*                 ATAAIA=ASM1RG1;   /* GET NEXT AIA FROM SAVE AREA   */ 00160000
         L     @12,ASM1RG1(,ASMVTPTR)                              0114 00161000
         ST    @12,ATAAIA(,ATAPTR)                                 0114 00162000
*                 GO TO CHKNEXT;    /* GO CHECK FOR ANOTHER AIA      */ 00163000
         B     CHKNEXT                                             0115 00164000
*               END;                /* END OF NORMAL PROCESSING      */ 00165000
*             ELSE                  /* AIA FROM POS IS ERROR         */ 00166000
*               DO;                 /* SET UP ERROR RETURN           */ 00167000
@RF00112 DS    0H                                                  0118 00168000
*                 AIANXAIA=ASM1RG1; /* REESTABLISH AIA CHAIN         */ 00169000
         L     @12,ASM1RG1(,ASMVTPTR)                              0118 00170000
         ST    @12,AIANXAIA(,AIAPTR)                               0118 00171000
*                 ASM1RG15=4;       /* PUT CODE IN SAVE AREA(R15)    */ 00172000
         MVC   ASM1RG15(4,ASMVTPTR),@CF00094                       0119 00173000
*                 ASM1RG1=AIAPTR;   /* PUT ERROR AIA IN SAVE(R1)     */ 00174000
         ST    AIAPTR,ASM1RG1(,ASMVTPTR)                           0120 00175000
*                 GO TO FINISH;     /* GET OUT OF LOOP               */ 00176000
         B     FINISH                                              0121 00177000
*               END;                                               0122 00178000
*           END;                    /* END VIO AIA PROCESSSING       */ 00179000
*                                                                  0123 00180000
*         /***********************************************************/ 00181000
*         /*                                                         */ 00182000
*         /* THIS IS NOT A VIO READ SO THE XPTE MUST BE CHECKED TO   */ 00183000
*         /* SEE IF THE SLOT CAN BE FREED                            */ 00184000
*         /*                                                         */ 00185000
*         /***********************************************************/ 00186000
*                                                                  0124 00187000
*         ELSE                      /* NOT A VIO PAGE-MUST BE A READ */ 00188000
*           DO;                                                    0124 00189000
@RF00105 DS    0H                                                  0125 00190000
*             IF XPTVALID='1'B&     /* IS XPTE VALID                 */ 00191000
*                 XPTIOERR='0'B THEN/* NO PREVIOUS ERROR             */ 00192000
         TM    XPTVALID(XPTEPTR),B'10000000'                       0125 00193000
         BNO   @RF00125                                            0125 00194000
         TM    XPTIOERR(XPTEPTR),B'00010000'                       0125 00195000
         BNZ   @RF00125                                            0125 00196000
*               AIALSID=XPTLSID;    /* NO ERRORS-MOVE THE LSID INTO     00197000
*                                      THE AIA FOR THE READ          */ 00198000
         L     @12,XPTLSID(,XPTEPTR)                               0126 00199000
         ST    @12,AIALSID(,AIAPTR)                                0126 00200000
*             ELSE                  /* THERE WAS AN ERROR IN XPTE    */ 00201000
*               DO;                 /* DETERMINE WHICH ERROR         */ 00202000
         B     @RC00125                                            0127 00203000
@RF00125 DS    0H                                                  0128 00204000
*                 IF XPTVALID='0'B THEN/* XPT NOT VALID?             */ 00205000
         TM    XPTVALID(XPTEPTR),B'10000000'                       0128 00206000
         BNZ   @RF00128                                            0128 00207000
*                   AIABADID='1'B;  /* SET ERROR BIT IN AIA          */ 00208000
         OI    AIABADID(AIAPTR),B'00000001'                        0129 00209000
*                 ELSE              /* MUST BE XPTIOERR              */ 00210000
*                   AIAPRIER='1'B;  /* SET I/O ERROR BIT             */ 00211000
         B     @RC00128                                            0130 00212000
@RF00128 OI    AIAPRIER(AIAPTR),B'00001000'                        0130 00213000
*                 ASM1RG15=4;       /* SET BAD RETURN CODE           */ 00214000
@RC00128 MVC   ASM1RG15(4,ASMVTPTR),@CF00094                       0131 00215000
*                 ASM1RG1=AIAPTR;   /* GET BAD AIA ADDRESS           */ 00216000
         ST    AIAPTR,ASM1RG1(,ASMVTPTR)                           0132 00217000
*                 GO TO FINISH;     /* GET OUT OF LOOP               */ 00218000
         B     FINISH                                              0133 00219000
*               END;                /* END-DETERMINE WHICH ERROR     */ 00220000
*           END;                                                   0135 00221000
@RC00125 DS    0H                                                  0136 00222000
*       END;                        /* END READ PROCESSING           */ 00223000
*                                                                  0137 00224000
*/********************************************************************/ 00225000
*/*                                                                  */ 00226000
*/* FUNCTION = THIS CODE WILL BE EXECUTED FOR VAMSI REQUESTS         */ 00227000
*/*            (AIAVIO='1'). THIS MEANS THAT RSM HAS SCHEDULED A     */ 00228000
*/*            MOVEOUT OF VIO BUFFERS AND THE ENTIRE CHAIN HAS TO BE */ 00229000
*/*            SENT TO ILRPOS TO BE CHECKED. IF AN ADDRESS IS        */ 00230000
*/*            RETURNED IN THE ATA IT MEANS THERE IS AN ERROR AND IT */ 00231000
*/*            MUST BE SENT BACK TO RSM ALONG WITH THE REST OF THE   */ 00232000
*/*            CHAIN.                                                */ 00233000
*/*                                                                  */ 00234000
*/* INPUT = RSMHDPTR = ADDRESS OF THE RSMHD                          */ 00235000
*/*         ASMVTPTR = ADDRESS OF THE ASMVT                          */ 00236000
*/*         ATAPTR = ADDRESS OF THE ATA                              */ 00237000
*/*         XPTEPTR = ADDRESS OF THE XPTE                            */ 00238000
*/*                                                                  */ 00239000
*/* EXTERNAL REOUTINES AND REFERENCES = ILRPOS                       */ 00240000
*/*                                                                  */ 00241000
*/********************************************************************/ 00242000
*                                                                  0137 00243000
*     ELSE                          /* IF NOT A READ                 */ 00244000
*       DO;                         /* NON-READ PROCESSING           */ 00245000
         B     @RC00098                                            0137 00246000
@RF00098 DS    0H                                                  0138 00247000
*         IF AIAVIO='1'B THEN       /* IS THIS A VAMSI REQUEST       */ 00248000
         TM    AIAVIO(AIAPTR),B'00100000'                          0138 00249000
         BNO   @RF00138                                            0138 00250000
*           DO;                     /* YES-DO VAMSI PROCESSING       */ 00251000
*             CALL ILRPOS;          /* GO TO POS FOR CHECKING        */ 00252000
         L     @15,@CV01693                                        0140 00253000
         BALR  @14,@15                                             0140 00254000
*             IF ATAAIA^=0 THEN     /* ANY ERRORS RETURNED           */ 00255000
         L     @12,ATAAIA(,ATAPTR)                                 0141 00256000
         LTR   @12,@12                                             0141 00257000
         BZ    @RF00141                                            0141 00258000
*               DO;                 /* YES-RETURN ERRORS             */ 00259000
*                 ASM1RG15=4;       /* SET A BAD RETURN CODE         */ 00260000
         MVC   ASM1RG15(4,ASMVTPTR),@CF00094                       0143 00261000
*                 ASM1RG1=ATAAIA;   /* ERROR AIA ADDR IN REG 1 IN THE   00262000
*                                      SAVE AREA                     */ 00263000
         ST    @12,ASM1RG1(,ASMVTPTR)                              0144 00264000
*               END;                /* END ERROR SETUP               */ 00265000
*             GO TO FINISH;         /* GET OUT OF THE LOOP           */ 00266000
         B     FINISH                                              0146 00267000
*           END;                    /* END VAMSI PROCESSING          */ 00268000
*         ELSE                      /* NO READ AND NO VAMSI          */ 00269000
*                                                                  0148 00270000
*           /*********************************************************/ 00271000
*           /*                                                       */ 00272000
*           /* HERE WE KNOW WE HAVE A NON-VAMSI WRITE REQUEST WHICH  */ 00273000
*           /* MEANS THAT THE SLOT/SLOTS REPRESENTED BY THE XPTE CAN */ 00274000
*           /* BE FREED IF THE XPTE IS VALID AND NOT A VIO SLOT      */ 00275000
*           /*                                                       */ 00276000
*           /*********************************************************/ 00277000
*                                                                  0148 00278000
*           DO;                     /* WRITE PROCESSING              */ 00279000
@RF00138 DS    0H                                                  0149 00280000
*             IF XPTVALID='1'B THEN /* IS THE XPTE VALID             */ 00281000
         TM    XPTVALID(XPTEPTR),B'10000000'                       0149 00282000
         BNO   @RF00149                                            0149 00283000
*               DO;                 /* YES-DO VALID PROCESSING       */ 00284000
*                 RFY                                              0151 00285000
*                  (R1,                                            0151 00286000
*                   R13) RSTD;      /* RESTRICT USE OF REGS          */ 00287000
*                 R1=XPTLSID;       /* GET LSID TO BE FREED          */ 00288000
         L     R1,XPTLSID(,XPTEPTR)                                0152 00289000
*                 R13=ADDR(ASMSAVE);/* GET ADDRESS OF SAVE AREA      */ 00290000
         LA    R13,ASMSAVE(,ASMVTPTR)                              0153 00291000
*                 CALL FREESLOT;    /* GO TO FREE THE SLOT           */ 00292000
         L     @15,ASMPFRSL(,ASMVTPTR)                             0154 00293000
         BALR  @14,@15                                             0154 00294000
*                 IF AIAPRIV='0'B THEN/* PRIVATE AREA PAGE??         */ 00295000
         TM    AIAPRIV(AIAPTR),B'01000000'                         0155 00296000
         BNZ   @RF00155                                            0155 00297000
*                   DO;             /* FREE SECONDARY SLOT           */ 00298000
*                     IF XPTLSID2^=0 THEN/* IS THERE A SECONDARY ??  */ 00299000
         L     @12,XPTLSID2(,XPTEPTR)                              0157 00300000
         LTR   @12,@12                                             0157 00301000
         BZ    @RF00157                                            0157 00302000
*                       DO;         /* FREE SECONDARY                */ 00303000
*                         R1=XPTLSID2;/* GET LSID TO BE FREED        */ 00304000
         LR    R1,@12                                              0159 00305000
*                         CALL FREESLOT;/* GO TO FREE THE SLOT       */ 00306000
         L     @15,ASMPFRSL(,ASMVTPTR)                             0160 00307000
         BALR  @14,@15                                             0160 00308000
*                         RFY                                      0161 00309000
*                          (R1,                                    0161 00310000
*                           R13) UNRSTD;/* RELEASE REGS FOR USE      */ 00311000
*                       END;        /* END FREE SECONDARY            */ 00312000
*                     GO TO CHKDPX; /* GO CHK DUPLEX ACTIVE          */ 00313000
         B     CHKDPX                                              0163 00314000
*                   END;            /* END-FREE SECONDARY SLOT       */ 00315000
*               END;                /* END VALID PROCESSING          */ 00316000
*                                                                  0166 00317000
*             /*******************************************************/ 00318000
*             /*                                                     */ 00319000
*             /* IF THIS IS A PRIVATE AREA PAGE THE PRIVATE AREA     */ 00320000
*             /* ALLOCATED COUNTS IN THE ASMVT AND ASCB MUST BE      */ 00321000
*             /* INCREMENTED. THE COUNTS ARE INCREMENTED ONLY FOR    */ 00322000
*             /* SLOTS THAT HAVE TO BE ALLOCATED. IF A SLOT WAS FREED*/ 00323000
*             /* AND REUSED THE COUNT IS NOT INCREMENTED SINCE THERE */ 00324000
*             /* WAS AN ALLOCATED SLOT.                              */ 00325000
*             /*                                                     */ 00326000
*             /*******************************************************/ 00327000
*                                                                  0166 00328000
*             ELSE                  /* THE XPTE IS NOT VALID SO NO 0166 00329000
*                                      SLOTS CAN BE FREED            */ 00330000
*               DO;                 /* UPDATE CNTS AND CHK DPX       */ 00331000
         B     @RC00149                                            0166 00332000
@RF00149 DS    0H                                                  0167 00333000
*                 IF AIAPRIV='1'B THEN/* IF THIS IS A PRIVATE AREA 0167 00334000
*                                      PAGE AND THERE WAS NO       0167 00335000
*                                      PREVIOUSLY WRITTEN SLOT THE 0167 00336000
*                                      ALLOCATED COUNTS IN THE ASCB     00337000
*                                      AND ASMVT ARE INCREMENTED     */ 00338000
         TM    AIAPRIV(AIAPTR),B'01000000'                         0167 00339000
         BNO   @RF00167                                            0167 00340000
*                   DO;             /* UPDATE COUNTS                 */ 00341000
*                     XPTVIOLP='0'B;/* MAKE SURE VIOLP BIT IS OFF    */ 00342000
         NI    XPTVIOLP(XPTEPTR),B'01111111'                       0169 00343000
*                     ASCBNVSC=ASCBNVSC+1;/* INCREMENT THE ASCB    0170 00344000
*                                      NON-VIO ALLOCATED SLOT COUNT  */ 00345000
         L     @12,ASM1ASCB(,ASMVTPTR)                             0170 00346000
         LA    @07,1                                               0170 00347000
         LH    @15,ASCBNVSC(,@12)                                  0170 00348000
         ALR   @15,@07                                             0170 00349000
         STH   @15,ASCBNVSC(,@12)                                  0170 00350000
*                     ASMNVSC=ASMNVSC+1;/* INCREMENT ASMVT COUNT     */ 00351000
         AL    @07,ASMNVSC(,ASMVTPTR)                              0171 00352000
         ST    @07,ASMNVSC(,ASMVTPTR)                              0171 00353000
*                   END;                                           0172 00354000
*                 ELSE              /* NOT PRIVATE PAGE-CHECK IF DPX    00355000
*                                      ACTIVE                        */ 00356000
*                                                                  0173 00357000
*                   /*************************************************/ 00358000
*                   /*                                               */ 00359000
*                   /* THIS IS NOT A PRIVATE AREA PAGE SO WE CHECK IF*/ 00360000
*                   /* DUPLEXING IS ACTIVE AND IF A COMMON DATA SET  */ 00361000
*                   /* IS AVAILABLE. IF DUPLEXING IS ACTIVE THE      */ 00362000
*                   /* DUPLEX FLAG IN THE AIA IS TURNED ON. IF THE   */ 00363000
*                   /* COMMON DATA SET IS NOT AVAILABLE THE SECONDARY*/ 00364000
*                   /* ERROR FLAG IS TURNED ON.                      */ 00365000
*                   /*                                               */ 00366000
*                   /*************************************************/ 00367000
*                                                                  0173 00368000
*CHKDPX:                                                           0173 00369000
*                   DO;             /* CHECK COMMON DS AVAILABILITY  */ 00370000
         B     @RC00167                                            0173 00371000
@RF00167 DS    0H                                                  0173 00372000
CHKDPX   DS    0H                                                  0174 00373000
*                     IF ASMDUPLX='1'B THEN/* IS DUPLEXING ACTIVE    */ 00374000
         TM    ASMDUPLX(ASMVTPTR),B'10000000'                      0174 00375000
         BNO   @RF00174                                            0174 00376000
*                       DO;         /* YES-CHECK FOR A COMMON DS     */ 00377000
*                         AIADUPLX='1'B;/* TURN ON DUPLEX FLAG       */ 00378000
         OI    AIADUPLX(AIAPTR),B'00010000'                        0176 00379000
*                         IF ASMNOCWQ='1'B THEN/* COMMON DS AVAIL    */ 00380000
         TM    ASMNOCWQ(ASMVTPTR),B'01000000'                      0177 00381000
         BNO   @RF00177                                            0177 00382000
*                           AIASECER='1'B;/* NO-TURN ON THE ERROR  0178 00383000
*                                      FLAG TO MAKE ILRQIOE THINK  0178 00384000
*                                      THIS IS A REDRIVE AND ONLY THE   00385000
*                                      SECONDARY COPY WILL BE WRITTEN   00386000
*                                      THUS BYPASSING THE COMMON DATA   00387000
*                                      SET.                          */ 00388000
         OI    AIASECER(AIAPTR),B'00000100'                        0178 00389000
*                       END;        /* END-CHECK FOR A COMMON DS     */ 00390000
@RF00177 DS    0H                                                  0180 00391000
*                   END;            /* END COMMON DS AVAILABILITY    */ 00392000
@RF00174 DS    0H                                                  0181 00393000
*               END;                /* UPDATE COUNTS AND DPX CHK     */ 00394000
*                                                                  0181 00395000
@RC00167 DS    0H                                                  0182 00396000
*             /*******************************************************/ 00397000
*             /*                                                     */ 00398000
*             /* THE XPTE IS NOW ZEROED AND THE PAGE OUT IN PROGRESS */ 00399000
*             /* FLAG IS TURNED ON. FOR PRIVATE AREA PAGES THE       */ 00400000
*             /* ASHIOCNT IS INCREMENTED. THE ASHIOCNT ACTS AS A     */ 00401000
*             /* TRIGGER FOR SWAP OUT OPERATIONS. WHEN ALL PRIVATE   */ 00402000
*             /* AREA PAGES ARE FINISHED BEING SWAPPED OUT THE LSQA  */ 00403000
*             /* CAN BE STARTED. THIS COUNT(DECREMENTED ON           */ 00404000
*             /* COMPLETION) TELLS WHEN THE PRIVATE AREA IS FINISHED */ 00405000
*             /*                                                     */ 00406000
*             /*******************************************************/ 00407000
*                                                                  0182 00408000
*             XPTVALID='0'B;        /* INVALIDATE THE XPTE           */ 00409000
@RC00149 NI    XPTVALID(XPTEPTR),B'01111111'                       0182 00410000
*             XPTLPID=XPTLPID&&XPTLPID;/* CLEAR THE XPTE FIELD       */ 00411000
         XC    XPTLPID(8,XPTEPTR),XPTLPID(XPTEPTR)                 0183 00412000
*             XPTPOINP='1'B;        /* TURN ON PAGE OUT IN PROGRESS     00413000
*                                      FLAG                          */ 00414000
         OI    XPTPOINP(XPTEPTR),B'00100000'                       0184 00415000
*             RFY                                                  0185 00416000
*               XPTEPTR UNRSTD;     /* RELEASE REG FOR USE           */ 00417000
*           END;                    /* END WRITE PROCESSING          */ 00418000
*       END;                        /* END NON-READ PROCESSING       */ 00419000
*                                                                  0188 00420000
*     /***************************************************************/ 00421000
*     /*                                                             */ 00422000
*     /* THIS SECTION OF CODE IS COMMON FOR READ, VIO, AND WRITE     */ 00423000
*     /* PROCESSING. THE NORMAL FLOW WILL COME THROUGH HERE.         */ 00424000
*     /*                                                             */ 00425000
*     /***************************************************************/ 00426000
*                                                                  0188 00427000
*     IF AIAPRIV='1'B THEN          /* PRIVATE AREA PAGE?            */ 00428000
@RC00098 TM    AIAPRIV(AIAPTR),B'01000000'                         0188 00429000
         BNO   @RF00188                                            0188 00430000
*       ASHIOCNT=ASHIOCNT+1;        /* YES-INCREMENT ASMHD COUNT     */ 00431000
*                                                                  0189 00432000
         LA    @12,1                                               0189 00433000
         AL    @12,ASHIOCNT(,RSMHDPTR)                             0189 00434000
         ST    @12,ASHIOCNT(,RSMHDPTR)                             0189 00435000
*     /***************************************************************/ 00436000
*     /*                                                             */ 00437000
*     /* THE AIA IS NOW PUT ON THE ASMSTAGQ FOR ILRQIOE. THIS QUEUE  */ 00438000
*     /* ALSO SERVES TO TELL PAGIO IF ILRQIOE SHOULD BE INVOKED.     */ 00439000
*     /*                                                             */ 00440000
*     /***************************************************************/ 00441000
*                                                                  0190 00442000
*QUEIT:                                                            0190 00443000
*     ASM1NXTA=AIANXAIA;            /* SAVE NEXT AIA ADDRESS         */ 00444000
@RF00188 DS    0H                                                  0190 00445000
QUEIT    L     @12,AIANXAIA(,AIAPTR)                               0190 00446000
         ST    @12,ASM1NXTA(,ASMVTPTR)                             0190 00447000
*     AIANXAIA=0;                   /* ZERO THE NEXT AIA ADDRESS     */ 00448000
         SLR   @12,@12                                             0191 00449000
         ST    @12,AIANXAIA(,AIAPTR)                               0191 00450000
*     ASMIORQR=ASMIORQR+1;          /* INCREMENT AIA'S RECIEVED COUNT*/ 00451000
         LA    @15,1                                               0192 00452000
         AL    @15,ASMIORQR(,ASMVTPTR)                             0192 00453000
         ST    @15,ASMIORQR(,ASMVTPTR)                             0192 00454000
*     IF ASMSTAGF=0 THEN            /* IS STAGEQ EMPTY               */ 00455000
         C     @12,ASMSTAGF(,ASMVTPTR)                             0193 00456000
         BNE   @RF00193                                            0193 00457000
*       ASMSTAGF=AIAPTR;            /* SET FIRST POINTER             */ 00458000
         ST    AIAPTR,ASMSTAGF(,ASMVTPTR)                          0194 00459000
*     ELSE                          /* ASMSTAGQ NOT EMPTY            */ 00460000
*                                                                  0195 00461000
*       /*************************************************************/ 00462000
*       /*                                                           */ 00463000
*       /* THE ASMSTAGQ IS NOT EMPTY SO WE WILL QUEUE THE CURRENT AIA*/ 00464000
*       /* TO THE END OF THE QUEUE.                                  */ 00465000
*       /*                                                           */ 00466000
*       /*************************************************************/ 00467000
*                                                                  0195 00468000
*       ASMSTAGL->AIANXAIA=AIAPTR;  /* POINT LAST ONE TO THIS ONE    */ 00469000
         B     @RC00193                                            0195 00470000
@RF00193 L     @12,ASMSTAGL(,ASMVTPTR)                             0195 00471000
         ST    AIAPTR,AIANXAIA(,@12)                               0195 00472000
*     ASMSTAGL=AIAPTR;              /* SET THE LAST POINTER          */ 00473000
@RC00193 ST    AIAPTR,ASMSTAGL(,ASMVTPTR)                          0196 00474000
*     ATAAIA=ASM1NXTA;              /* GET NEXT AIA ADDRESS          */ 00475000
*                                                                  0197 00476000
         L     @12,ASM1NXTA(,ASMVTPTR)                             0197 00477000
         ST    @12,ATAAIA(,ATAPTR)                                 0197 00478000
*     /***************************************************************/ 00479000
*     /*                                                             */ 00480000
*     /* THE ADDRESS OF THE NEXT AIA WAS SAVED IN ATAAIA WHEN THE AIA*/ 00481000
*     /* WAS QUEUED TO ASMSTAGQ. IT IS NOW PICKED UP TO SEE IF THERE */ 00482000
*     /* IS ANOTHER ONE TO PROCESS.                                  */ 00483000
*     /*                                                             */ 00484000
*     /***************************************************************/ 00485000
*                                                                  0198 00486000
*CHKNEXT:                                                          0198 00487000
*     AIAPTR=ATAAIA;                /* SET POINTER TO NEW AIA        */ 00488000
CHKNEXT  L     AIAPTR,ATAAIA(,ATAPTR)                              0198 00489000
*     RFY                                                          0199 00490000
*       PCBPTR RSTD;                /* RESTRICT USE OF REG           */ 00491000
*     PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));                     0200 00492000
         LR    PCBPTR,AIAPTR                                       0200 00493000
         SL    PCBPTR,@CF01755                                     0200 00494000
*     RFY                                                          0201 00495000
*       PCBPTR UNRSTD;              /* RELEASE REG FOR USE           */ 00496000
*   END;                            /* END OF THE LOOP TO CYCLE    0202 00497000
*                                      THROUGH ALL THE AIA'S         */ 00498000
@DE00094 LTR   AIAPTR,AIAPTR                                       0202 00499000
         BNZ   @DL00094                                            0202 00500000
*                                                                  0203 00501000
*   /*****************************************************************/ 00502000
*   /*                                                               */ 00503000
*   /* WHEN CONTROL FALLS TO THIS POINT ALL OF THE INPUT AIA'S HAVE  */ 00504000
*   /* BEEN PROCESSED AND THE ASMSTAGQ WILL BE CHECKED TO SEE IF     */ 00505000
*   /* ILRQIOE SHOULD BE CALLED. ON RETURN FROM ILRQIOE THE FOOTPRINT*/ 00506000
*   /* IN THE ATA IS TURNED OFF, THE FRR IS DELETED, REGISTERS       */ 00507000
*   /* RESTORED AND CONTROL IS RETURNED TO THE CALLER.               */ 00508000
*   /*                                                               */ 00509000
*   /*****************************************************************/ 00510000
*                                                                  0203 00511000
*ILRCRPG1:                                                         0203 00512000
*FINISH:                                                           0203 00513000
*   ATAAIA=0;                       /* ZERO THE ATA POINTER          */ 00514000
ILRCRPG1 DS    0H                                                  0203 00515000
FINISH   SLR   @12,@12                                             0203 00516000
         ST    @12,ATAAIA(,ATAPTR)                                 0203 00517000
*   IF ASMSTAGF^=0 THEN             /* IS THERE AN AIA ON QUEUE      */ 00518000
         C     @12,ASMSTAGF(,ASMVTPTR)                             0204 00519000
         BE    @RF00204                                            0204 00520000
*     CALL ILRQIOE;                 /* CALL MODULE TO BUILD AND QUEUE   00521000
*                                      IOE'S                         */ 00522000
         BAL   @14,ILRQIOE                                         0205 00523000
*   GENERATE REFS(R9,R12,PSACSTK,FRRSCURR,FRRSEMP,FRRSELEN,FRRS,   0206 00524000
*       ASMVTPTR,ASMWKSA1);                                        0206 00525000
@RF00204 DS    0H                                                  0206 00526000
            SETFRR D,WRKREGS=(R9,R12)                                   00527000
            LM    R1,R15,ASMWKSA1(ASMVTPTR)                             00528000
*   RETURN;                                                        0207 00529000
@EL00001 DS    0H                                                  0207 00530000
@EF00001 DS    0H                                                  0207 00531000
@ER00001 BR    @14                                                 0207 00532000
*   RFY                                                            0208 00533000
*     RSMHDPTR UNRSTD;                                             0208 00534000
*ILRQIOE:                                                          0209 00535000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0209 00536000
*                                                                  0209 00537000
ILRQIOE  DS    0H                                                  0210 00538000
*   /*****************************************************************/ 00539000
*   /*                                                               */ 00540000
*   /* THE PURPOSE OF ILRQIOE IS TO BUILD IOE'S FOR THE AIA FOUND ON */ 00541000
*   /* THE ASMSTAGQ AND QUEUE THEM TO THE APPROPRIATE QUEUES IN THE  */ 00542000
*   /* PART(WRITES) OR PART ENTRIES(READS). ERRORS ARE PUT ON THE    */ 00543000
*   /* ERROR QUEUE(PARTAIAE).                                        */ 00544000
*   /*                                                               */ 00545000
*   /*****************************************************************/ 00546000
*                                                                  0210 00547000
*   ATAQIOE='1'B;                   /* TURN ILRQIOE FLAG ON IN ATA   */ 00548000
         OI    ATAQIOE(ATAPTR),B'10000000'                         0210 00549000
*   GENERATE REFS(ASMWKSA2,ASMVTPTR,R2,R14,R12);                   0211 00550000
        ENTRY ILRQIOE                                                   00551000
        STM   R2,R14,ASMWKSA2(ASMVTPTR) SAVE REGS IN ASMVT              00552000
        BALR  R10,0                    SET BASE ADDRESS                 00553000
        USING *,R10                    ESTABLISH ADDRESSABILITY         00554000
*   ASM2WRKA=''B;                   /* ZERO THE WORKAREA             */ 00555000
         XC    ASM2WRKA(28,ASMVTPTR),ASM2WRKA(ASMVTPTR)            0212 00556000
*   AIAPTR=ASMSTAGF;                /* PICK UP FIRST AIA ADDRESS     */ 00557000
         L     AIAPTR,ASMSTAGF(,ASMVTPTR)                          0213 00558000
*   IF ASMCALLQ='1'B THEN           /* IS COMPLETION THE CALLER      */ 00559000
         TM    ASMCALLQ(ASMVTPTR),B'10000000'                      0214 00560000
         BNO   @RF00214                                            0214 00561000
*     ASM2RFLG='1'B;                /* YES-AN SRB WILL NOT BE      0215 00562000
*                                      SCHEDULED IN THIS CASE SO THIS   00563000
*                                      FLAG WILL PREVENT THE IORB  0215 00564000
*                                      QUEUES FROM BEING SEARCHED    */ 00565000
         OI    ASM2RFLG(ASMVTPTR),B'10000000'                      0215 00566000
*   IF AIAPTR=0 THEN                /* IS THERE AN AIA TO PROCESS    */ 00567000
@RF00214 LTR   AIAPTR,AIAPTR                                       0216 00568000
         BZ    @RT00216                                            0216 00569000
*     GO TO SCHEDULE;               /* NO-GO TO SEE IF PTM00 SHOULD     00570000
*                                      BE SCHEDULED                  */ 00571000
*   DO UNTIL(AIAPTR=0);             /* CYCLE THROUGH AIA'S           */ 00572000
*                                                                  0218 00573000
@DL00218 DS    0H                                                  0219 00574000
*     /***************************************************************/ 00575000
*     /*                                                             */ 00576000
*     /* ONE IOE IS ACQUIRED FROM THE POOL. IF NONE IS AVAILABLE     */ 00577000
*     /* REGISTER 1 WILL BE RETURNED WITH A ZERO IN IT.              */ 00578000
*     /*                                                             */ 00579000
*     /***************************************************************/ 00580000
*                                                                  0219 00581000
*     CALL GETIOE;                  /* GET AN IOE FROM THE POOL      */ 00582000
         BAL   @14,GETIOE                                          0219 00583000
*     RFY                                                          0220 00584000
*       R1 RSTD;                    /* RESTRICT REG FROM USE         */ 00585000
*     IF R1=0 THEN                  /* WAS AN IOE AVAILABLE          */ 00586000
         SLR   @12,@12                                             0221 00587000
         CR    R1,@12                                              0221 00588000
         BE    @RT00221                                            0221 00589000
*       GO TO CHKQUE;               /* NO-SEE IF ANYTHING IS TO BE 0222 00590000
*                                      QUEUED                        */ 00591000
*     RFY                                                          0223 00592000
*       IOEPTR RSTD;                /* RESTRICT USE OF IOEPTR        */ 00593000
*     IOEPTR=R1;                    /* GET ADDRESS OF THE IOE        */ 00594000
         LR    IOEPTR,R1                                           0224 00595000
*     IOE=''B;                      /* ZERO OUT THE IOE              */ 00596000
         XC    IOE(16,IOEPTR),IOE(IOEPTR)                          0225 00597000
*     RFY                                                          0226 00598000
*       R1 UNRSTD;                  /* RELEASE REG FOR USE           */ 00599000
*     ATAAIA=AIAPTR;                /* SET AIA RECOVERY ADDRESS      */ 00600000
         ST    AIAPTR,ATAAIA(,ATAPTR)                              0227 00601000
*     ASMSTAGF=AIANXAIA;            /* TAKE TOP AIA OFF QUEUE        */ 00602000
         L     @07,AIANXAIA(,AIAPTR)                               0228 00603000
         ST    @07,ASMSTAGF(,ASMVTPTR)                             0228 00604000
*     AIANXAIA=0;                   /* ZERO THE NEXT POINTER         */ 00605000
         ST    @12,AIANXAIA(,AIAPTR)                               0229 00606000
*                                                                  0230 00607000
*/********************************************************************/ 00608000
*/*                                                                  */ 00609000
*/* FUNCTION = THIS CODE WILL GET AND BUILD THE IOE'S NECESSARY FOR  */ 00610000
*/*            THE REQUESTED I/O OPERATIONS. DEPENDING ON WHAT TYPE  */ 00611000
*/*            OF OPERATION IS REQUESTED EITHER ONE OR TWO IOE'S     */ 00612000
*/*            MUST BE BUILT. DUPLEXING REQUESTS NEED TWO IOE'S      */ 00613000
*/*            EXCEPT IN THE REDRIVE CASE WHEN ONE FRAME HAS ALREADY */ 00614000
*/*            BEEN WRITTEN TO A SLOT AND ONLY ONE HAS TO BE         */ 00615000
*/*            REWRITTEN.                                            */ 00616000
*/*            FIRST WE DECIDE WHICH IOE MUST BE BUILT AND INITIALIZE*/ 00617000
*/*            IT.                                                   */ 00618000
*/*            IF THIS IS AN INITIAL DUPLEXING REQUEST OR A REDRIVE  */ 00619000
*/*            WITH BOTH ERROR BITS ON TWO IOE'S MUST BE BUILT SO    */ 00620000
*/*            ANOTHER ONE IS ACQUIRED AND BOTH ARE INITIALIZED.     */ 00621000
*/*            IF A SECOND IOE ISN'T AVAILABLE WE FREE THE FIRST IOE */ 00622000
*/*            AND GO TO FINISH PROCESSING.                          */ 00623000
*/*            ALL IOE'S ARE PUT ON THE TOP OF AN INTERNAL PUSH DOWN */ 00624000
*/*            STACK OF IOE'S. THE INTERNAL FLAG(ASM2WFLG) IS SET TO */ 00625000
*/*            TELL US LATER THAT IOE'S HAVE BEEN BUILT TO PUT ON    */ 00626000
*/*            THE PART WRITE QUEUE.                                 */ 00627000
*/*                                                                  */ 00628000
*/* INPUT = ASMVTPTR = ADDRESS TO THE ASMVT                          */ 00629000
*/*         ATAPTR = ADDRESS OF THE ATA                              */ 00630000
*/*         AIAPTR = CURRENT AIA ADDRESS                             */ 00631000
*/*         IOEPTR = POINTER TO THE IOE                              */ 00632000
*/*                                                                  */ 00633000
*/* EXTERNAL ROUTINES AND REFERENCES = NONE                          */ 00634000
*/*                                                                  */ 00635000
*/********************************************************************/ 00636000
*                                                                  0230 00637000
*     IF AIADUPLX='1'B THEN         /* IS THIS A DUPLEX REQUEST      */ 00638000
         TM    AIADUPLX(AIAPTR),B'00010000'                        0230 00639000
         BNO   @RF00230                                            0230 00640000
*       DO;                         /* YES-PROCESS DUPLEX REQUEST    */ 00641000
*         IF(AIAPRIER='1'B&         /* BOTH ERROR BITS ON            */ 00642000
*             AIASECER='1'B)|(AIAPRIER='0'B&/* NEITHER ERROR BIT ON  */ 00643000
*             AIASECER='0'B) THEN                                  0232 00644000
         TM    AIAPRIER(AIAPTR),B'00001100'                        0232 00645000
         BO    @RT00232                                            0232 00646000
         TM    AIAPRIER(AIAPTR),B'00001100'                        0232 00647000
         BNZ   @RF00232                                            0232 00648000
@RT00232 DS    0H                                                  0233 00649000
*           DO;                     /* BUILD TWO IOE'S               */ 00650000
*                                                                  0233 00651000
*             /*******************************************************/ 00652000
*             /*                                                     */ 00653000
*             /* TWO IOE'S ARE NEEDED SO WE GET ANOTHER ONE. IF IT IS*/ 00654000
*             /* NOT AVAILABLE THE FIRST ONE MUST BE FREED AND THE   */ 00655000
*             /* AIA PUT BACK ON THE ASMSTAGQ.                       */ 00656000
*             /*                                                     */ 00657000
*             /*******************************************************/ 00658000
*                                                                  0234 00659000
*             CALL GETIOE;          /* GO TO GET AN IOE              */ 00660000
         BAL   @14,GETIOE                                          0234 00661000
*             RFY                                                  0235 00662000
*               R1 RSTD;            /* RESTRICT USE OF REG 1         */ 00663000
*             IF R1=0 THEN          /* IS IOE AVAILABLE              */ 00664000
         LTR   R1,R1                                               0236 00665000
         BNZ   @RF00236                                            0236 00666000
*               DO;                 /* NO-STOP PROCESSING            */ 00667000
*                 R1=IOEPTR;        /* MUST FREE UP FIRST IOE        */ 00668000
         LR    R1,IOEPTR                                           0238 00669000
*                 CALL FREEIOE;     /* FREE THE PRIMARY IOE          */ 00670000
         BAL   @14,FREEIOE                                         0239 00671000
*                 ATAAIA=0;         /* ZERO THE RECOVERY POINTER     */ 00672000
         SLR   @12,@12                                             0240 00673000
         ST    @12,ATAAIA(,ATAPTR)                                 0240 00674000
*                 AIANXAIA=ASMSTAGF;/* CHAIN Q TO THIS AIA           */ 00675000
         L     @12,ASMSTAGF(,ASMVTPTR)                             0241 00676000
         ST    @12,AIANXAIA(,AIAPTR)                               0241 00677000
*                 ASMSTAGF=AIAPTR;  /* PUT AIA BACK ON QUEUE         */ 00678000
         ST    AIAPTR,ASMSTAGF(,ASMVTPTR)                          0242 00679000
*                 GO TO CHKQUE;     /* CHECK IF ANYTHING FOR PART    */ 00680000
         B     CHKQUE                                              0243 00681000
*               END;                /* END NO IOE PROCESSING         */ 00682000
*             ELSE                  /* THERE IS AN IOE AVAILABLE     */ 00683000
*               DO;                 /* START NORMAL IOE BUILD        */ 00684000
*                                                                  0245 00685000
@RF00236 DS    0H                                                  0246 00686000
*                 /***************************************************/ 00687000
*                 /*                                                 */ 00688000
*                 /* TWO IOE'S ARE AVAILABLE AND THEY ARE NOW        */ 00689000
*                 /* INITIALIZED AND QUEUED TO THE WRITE QUEUES IN   */ 00690000
*                 /* THE PART HEADER.                                */ 00691000
*                 /*                                                 */ 00692000
*                 /***************************************************/ 00693000
*                                                                  0246 00694000
*                 RFY                                              0246 00695000
*                   IOE2PTR RSTD;   /* RESTRICT USE OF REG 1 WHICH IS   00696000
*                                      USED TO ADDRESS IOE 2         */ 00697000
*                 IOE2PTR->IOE=''B; /* ZERO SECOND IOE               */ 00698000
         XC    IOE(16,IOE2PTR),IOE(IOE2PTR)                        0247 00699000
*                 RFY                                              0248 00700000
*                   R1 UNRSTD;      /* RELEASE REG FOR USE           */ 00701000
*                 AIADPXCT=2;       /* SET DUPLEX CT FOR TWO IOE'S   */ 00702000
         MVC   AIADPXCT(4,AIAPTR),@CF00080                         0249 00703000
*                 AIAID=AIAID&&AIAID;/* ZERO THE AIA                 */ 00704000
*                                                                  0250 00705000
         XC    AIAID(8,AIAPTR),AIAID(AIAPTR)                       0250 00706000
*                 /***************************************************/ 00707000
*                 /*                                                 */ 00708000
*                 /* BUILD THE IOE FOR DUPLEX WRITE                  */ 00709000
*                 /*                                                 */ 00710000
*                 /***************************************************/ 00711000
*                                                                  0251 00712000
*                 IOE2PTR->IOEAIA=AIAPTR;/* GET AIA ADDRESS          */ 00713000
         ST    AIAPTR,IOEAIA(,IOE2PTR)                             0251 00714000
*                 IOE2PTR->IOELSIDA=ADDR(AIALSID2);/* LSID@          */ 00715000
*                                                                  0252 00716000
         LA    @12,AIALSID2(,AIAPTR)                               0252 00717000
         ST    @12,IOELSIDA(,IOE2PTR)                              0252 00718000
*                 /***************************************************/ 00719000
*                 /*                                                 */ 00720000
*                 /* BUILD THE IOE FOR THE COMMON WRITE              */ 00721000
*                 /*                                                 */ 00722000
*                 /***************************************************/ 00723000
*                                                                  0253 00724000
*                 IOEAIA=AIAPTR;    /* GET THE AIA ADDRESS           */ 00725000
         ST    AIAPTR,IOEAIA(,IOEPTR)                              0253 00726000
*                 IOELSIDA=ADDR(AIALSID);/* ADDRESS OF PLACE FOR THE    00727000
*                                      LSID                          */ 00728000
         LA    @12,AIALSID(,AIAPTR)                                0254 00729000
         ST    @12,IOELSIDA(,IOEPTR)                               0254 00730000
*                 AIAPRIER='0'B;    /* TURN OF PRIMARY ERROR FLAG    */ 00731000
*                 AIASECER='0'B;    /* TURN OFF SECONDARY ERROR      */ 00732000
         NI    AIAPRIER(AIAPTR),B'11110011'                        0256 00733000
*                 ATAAIA=0;         /* ZERO THE RECOVERY POINTER     */ 00734000
         SLR   @12,@12                                             0257 00735000
         ST    @12,ATAAIA(,ATAPTR)                                 0257 00736000
*                 IOE2PTR->IOENXT=ASM2DUPQ;/* Q CHAIN TO IOE         */ 00737000
         L     @12,ASM2DUPQ(,ASMVTPTR)                             0258 00738000
         ST    @12,IOENXT(,IOE2PTR)                                0258 00739000
*                 ASM2DUPQ=IOE2PTR; /* PUT IOE ON TOP OF QUEUE       */ 00740000
         ST    IOE2PTR,ASM2DUPQ(,ASMVTPTR)                         0259 00741000
*                 IOENXT=ASM2CAQ;   /* QUE CHAIN TO THIS IOE         */ 00742000
         L     @12,ASM2CAQ(,ASMVTPTR)                              0260 00743000
         ST    @12,IOENXT(,IOEPTR)                                 0260 00744000
*                 ASM2CAQ=IOEPTR;   /* PUT IOE ON TOP OF QUE         */ 00745000
         ST    IOEPTR,ASM2CAQ(,ASMVTPTR)                           0261 00746000
*                 RFY                                              0262 00747000
*                   IOE2PTR UNRSTD; /* RELEASE REG FOR USE           */ 00748000
*               END;                /* END IOE BUILD                 */ 00749000
*           END;                    /* END BUILDING TWO IOE'S        */ 00750000
*                                                                  0265 00751000
*         /***********************************************************/ 00752000
*         /*                                                         */ 00753000
*         /* THIS CODE IS FOR THE REDRIVE CASE WHEN ONLY ONE OF THE  */ 00754000
*         /* ERROR BITS IS ON. THE IOE WILL BE INITIALIZED AND QUEUED*/ 00755000
*         /* DEPENDING ON WHICH BIT IS ON.                           */ 00756000
*         /*                                                         */ 00757000
*         /***********************************************************/ 00758000
*                                                                  0265 00759000
*         ELSE                                                     0265 00760000
*           DO;                     /* ONE OF THE ERROR BITS IS ON 0265 00761000
*                                      THEREFORE ONLY ONE IOE HAS TO    00762000
*                                      BE BUILT                      */ 00763000
         B     @RC00232                                            0265 00764000
@RF00232 DS    0H                                                  0266 00765000
*             AIADPXCT=1;           /* SET THE DUPLEX TO 1 SINCE   0266 00766000
*                                      THERE IS 1 IOE FOR THIS AIA   */ 00767000
         MVC   AIADPXCT(4,AIAPTR),@CF00041                         0266 00768000
*             IOEAIA=AIAPTR;        /* POINT IOE TO THE AIA          */ 00769000
         ST    AIAPTR,IOEAIA(,IOEPTR)                              0267 00770000
*             IF AIASECER='1'B THEN /* SECONDARY ERROR               */ 00771000
         TM    AIASECER(AIAPTR),B'00000100'                        0268 00772000
         BNO   @RF00268                                            0268 00773000
*               DO;                 /* YES-BUILE IOE FOR DUPE QUE    */ 00774000
*                 IOELSIDA=ADDR(AIALSID2);/* LSID ADDRESS            */ 00775000
         LA    @12,AIALSID2(,AIAPTR)                               0270 00776000
         ST    @12,IOELSIDA(,IOEPTR)                               0270 00777000
*                 AIASECER='0'B;    /* ZERO THE ERROR FLAG           */ 00778000
         NI    AIASECER(AIAPTR),B'11111011'                        0271 00779000
*                 AIALSID2=0;       /* ZERO LSID2 FIELD IN AIA       */ 00780000
         SLR   @12,@12                                             0272 00781000
         ST    @12,AIALSID2(,AIAPTR)                               0272 00782000
*                 ATAAIA=0;         /* ZERO RECOVERY POINTER         */ 00783000
         ST    @12,ATAAIA(,ATAPTR)                                 0273 00784000
*                 IOENXT=ASM2DUPQ;  /* QUE CHAIN TO THIS IOE         */ 00785000
         L     @12,ASM2DUPQ(,ASMVTPTR)                             0274 00786000
         ST    @12,IOENXT(,IOEPTR)                                 0274 00787000
*                 ASM2DUPQ=IOEPTR;  /* PUT IOE ON TOP OF QUEUE       */ 00788000
         ST    IOEPTR,ASM2DUPQ(,ASMVTPTR)                          0275 00789000
*               END;                /* END SECONDARY ERROR PROCESS   */ 00790000
*             ELSE                  /* MUST BE PRIMARY ERROR         */ 00791000
*               DO;                 /* PRIMARY ERROR PROCESSING      */ 00792000
         B     @RC00268                                            0277 00793000
@RF00268 DS    0H                                                  0278 00794000
*                 IOELSIDA=ADDR(AIALSID);/* LSID ADDRESS             */ 00795000
         LA    @12,AIALSID(,AIAPTR)                                0278 00796000
         ST    @12,IOELSIDA(,IOEPTR)                               0278 00797000
*                 AIAPRIER='0'B;    /* ZERO THE ERROR FLAG           */ 00798000
         NI    AIAPRIER(AIAPTR),B'11110111'                        0279 00799000
*                 AIALSID=0;        /* ZERO THE LSID IN THE AIA      */ 00800000
         SLR   @12,@12                                             0280 00801000
         ST    @12,AIALSID(,AIAPTR)                                0280 00802000
*                 ATAAIA=0;         /* ZERO THE RECOVERY POINTER     */ 00803000
         ST    @12,ATAAIA(,ATAPTR)                                 0281 00804000
*                 IOENXT=ASM2CAQ;   /* QUE CHAIN TO THIS IOE         */ 00805000
         L     @12,ASM2CAQ(,ASMVTPTR)                              0282 00806000
         ST    @12,IOENXT(,IOEPTR)                                 0282 00807000
*                 ASM2CAQ=IOEPTR;   /* PUT THIS IOE ON TOP OF Q      */ 00808000
         ST    IOEPTR,ASM2CAQ(,ASMVTPTR)                           0283 00809000
*               END;                /* END PRIMARY ERROR PROCESS     */ 00810000
*           END;                    /* END ONE ERROR PROCESSING      */ 00811000
@RC00268 DS    0H                                                  0286 00812000
*         ASM2WFLG='1'B;            /* SET INTERNAL WRITE IOE FLAG   */ 00813000
@RC00232 OI    ASM2WFLG(ASMVTPTR),B'01000000'                      0286 00814000
*       END;                                                       0287 00815000
*                                                                  0288 00816000
*/********************************************************************/ 00817000
*/*                                                                  */ 00818000
*/* FUNCTION = THIS CODE INITIALIZES THE IOE FOR SIMPLEX WRITE       */ 00819000
*/*            OPERATIONS. THE ONLY DECISION MADE IS WHAT QUEUE THE  */ 00820000
*/*            IOE SHOULD BE PUT ON. PRIVATE AREA PAGES(AIAPRIV='1') */ 00821000
*/*            AND LSQA SWAP PAGES(AIALSQA='1' MEANS THAT NO SWAP    */ 00822000
*/*            SETS ARE AVAILABLE OR THAT THERE IS NO SPACE IN THE   */ 00823000
*/*            THE SWAP DATA SETS) GO TO THE INTERNAL PRIVATE AREA   */ 00824000
*/*            QUEUE AND ALL OTHERS GO TO THE INTERNAL COMMON AREA   */ 00825000
*/*            QUEUE.                                                */ 00826000
*/*            THE INTERNAL FLAG(ASM2WFLG) IS TURNED ON FOR LATER    */ 00827000
*/*            QUEUEING TO THE PART WRITE QUEUE.                     */ 00828000
*/*                                                                  */ 00829000
*/* INPUT = ASMVTPTR = ADDRESS OF THE ASMVT                          */ 00830000
*/*         ATAPTR = ADDRESS OF THE ATA                              */ 00831000
*/*         AIAPTR = ADDRESS OF THE CURRENT AIA                      */ 00832000
*/*         IOEPTR = ADDRESS OF THE IOE                              */ 00833000
*/*                                                                  */ 00834000
*/* EXTERNAL ROUTINES AND REFERENCES = NONE                          */ 00835000
*/*                                                                  */ 00836000
*/********************************************************************/ 00837000
*                                                                  0288 00838000
*     ELSE                          /* NOT A DUPLEXING REQUEST       */ 00839000
*       DO;                         /* DO-SIMPLEX PROCESSING         */ 00840000
         B     @RC00230                                            0288 00841000
@RF00230 DS    0H                                                  0289 00842000
*         IOEAIA=AIAPTR;            /* PUT THE ADDRESS OF THE AIA  0289 00843000
*                                      INTO THE IOE-THIS IS THE ONLY    00844000
*                                      SETUP NECESSARY FOR A SIMPLEX    00845000
*                                      REQUEST.                      */ 00846000
         ST    AIAPTR,IOEAIA(,IOEPTR)                              0289 00847000
*         IF AIAWRITE='1'B THEN     /* IS IT A WRITE REQUEST         */ 00848000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0290 00849000
         BNO   @RF00290                                            0290 00850000
*           DO;                     /* YES                           */ 00851000
*             ATAAIA=0;             /* ZERO THE ATA POINTER          */ 00852000
         SLR   @12,@12                                             0292 00853000
         ST    @12,ATAAIA(,ATAPTR)                                 0292 00854000
*             IF AIAPRIV='1'B|      /* IS THIS A P.A. PAGE           */ 00855000
*                 AIALSQA='1'B THEN /* NO-IS IT LSQA SWAP PAGE TO GO    00856000
*                                      TO PRIVATE AREA DATA SET      */ 00857000
         TM    AIAPRIV(AIAPTR),B'01001000'                         0293 00858000
         BZ    @RF00293                                            0293 00859000
*               DO;                                                0294 00860000
*                 IOENXT=ASM2PAQ;   /* POINT THIS IOE TO QUEUE       */ 00861000
         L     @12,ASM2PAQ(,ASMVTPTR)                              0295 00862000
         ST    @12,IOENXT(,IOEPTR)                                 0295 00863000
*                 ASM2PAQ=IOEPTR;   /* PUT THIS IOE ON TOP OF Q      */ 00864000
         ST    IOEPTR,ASM2PAQ(,ASMVTPTR)                           0296 00865000
*               END;                                               0297 00866000
*             ELSE                  /* PUT IT ON C.A. QUEUE          */ 00867000
*               DO;                                                0298 00868000
         B     @RC00293                                            0298 00869000
@RF00293 DS    0H                                                  0299 00870000
*                 IOENXT=ASM2CAQ;   /* POINT THIS IOE TO QUE         */ 00871000
         L     @12,ASM2CAQ(,ASMVTPTR)                              0299 00872000
         ST    @12,IOENXT(,IOEPTR)                                 0299 00873000
*                 ASM2CAQ=IOEPTR;   /* PUT THIS IOE ON TOP OF QUE    */ 00874000
         ST    IOEPTR,ASM2CAQ(,ASMVTPTR)                           0300 00875000
*               END;                                               0301 00876000
*             ASM2WFLG='1'B;        /* SET WRITE IOE BUILT FLAG      */ 00877000
@RC00293 OI    ASM2WFLG(ASMVTPTR),B'01000000'                      0302 00878000
*           END;                                                   0303 00879000
*                                                                  0304 00880000
*/********************************************************************/ 00881000
*/*                                                                  */ 00882000
*/* FUNCTION = READ IOE'S ARE CHAINED FROM A PART ENTRY SO THE PART  */ 00883000
*/*            HAS TO BE INDEXED TO GET TO THE RIGHT ENTRY. IF THERE */ 00884000
*/*            IS NO ENTRY FOR THE NN VALUE SPECIFIED IN THE LSID THE*/ 00885000
*/*            AIA IS PUT ON THE PARTAIAE QUEUE AND THE IOE IS FREED.*/ 00886000
*/*            ALSO IF THE DATA SET IS BAD(PAREDSBD='1'B) THE AIA IS */ 00887000
*/*            PUT ON THE PARTAIAE QUEUE AND THE IOE IS FREED. ONCE  */ 00888000
*/*            THE IOE IS CHAINED TO THE PART ENTRY THE IORB'S       */ 00889000
*/*            CHAINED FROM THAT ENTRY ARE SEARCHED FOR ONE THAT IS  */ 00890000
*/*            NOT BUSY. IF ONE IS FOUND, THE INTERNAL FLAG(ASM2RFLG)*/ 00891000
*/*            IS SET TO INDICATE THAT PART MONITOR CAN BE SCHEDULED */ 00892000
*/*            SINCE USEFUL WORK CAN BE DONE.                        */ 00893000
*/*                                                                  */ 00894000
*/* INPUT = ASMVTPTR = ADDRESS OF THE ASMVT                          */ 00895000
*/*         ATAPTR = ADDRESS OF THE ATA                              */ 00896000
*/*         AIAPTR = ADDRESS OF THE CURRENT AIA                      */ 00897000
*/*                                                                  */ 00898000
*/* EXTERNAL ROUTINES AND REFERENCES = NONE                          */ 00899000
*/*                                                                  */ 00900000
*/********************************************************************/ 00901000
*                                                                  0304 00902000
*         ELSE                      /* NOW WRITE-MUST BE READ        */ 00903000
*           DO;                     /* DO-READ PROCESSING            */ 00904000
         B     @RC00290                                            0304 00905000
@RF00290 DS    0H                                                  0305 00906000
*             RFY                                                  0305 00907000
*               PARTPTR RSTD;       /* RESTRICT USE OF PARTPTR       */ 00908000
*             RFY                                                  0306 00909000
*               PARTENDX RSTD;      /* RESTRICT USE OF PARTENDX      */ 00910000
*             PARTENDX=AIALSID;     /* GET THE LSID FROM THE AIA     */ 00911000
         L     PARTENDX,AIALSID(,AIAPTR)                           0307 00912000
*             SLL(PARTENDX,8);      /* SHIFT OUT UNUSED BITS         */ 00913000
         SLL   PARTENDX,8                                          0308 00914000
*             SRL(PARTENDX,24);     /* ISOLATE NN PORTION OF LSID    */ 00915000
         SRL   PARTENDX,24                                         0309 00916000
*             PARTPTR=ASMPART;      /* GET THE ADDRESS OF THE PART   */ 00917000
         L     PARTPTR,ASMPART(,ASMVTPTR)                          0310 00918000
*                                                                  0311 00919000
*             /*******************************************************/ 00920000
*             /*                                                     */ 00921000
*             /* CHECK IF THE NN VALUE SPECIFIED IN THE LSID IS      */ 00922000
*             /* GREATER THAN THE NUMBER OF PARTE'S IN USE. IF IT IS */ 00923000
*             /* THE REQUEST CANNOT BE SATISFIED AND THE AIA IS PUT  */ 00924000
*             /* ON THE PART ERROR QUEUE.                            */ 00925000
*             /*                                                     */ 00926000
*             /*******************************************************/ 00927000
*                                                                  0311 00928000
*             RFY                                                  0311 00929000
*               PAREPTR RSTD;       /* RESTRICT USE OF REGISTER      */ 00930000
*             PAREPTR=(PARTENDX*LENGTH(PARTENT))+ADDR(PARTENTS);   0312 00931000
         LR    PAREPTR,PARTENDX                                    0312 00932000
         SLA   PAREPTR,6                                           0312 00933000
         LA    @12,PARTENTS(,PARTPTR)                              0312 00934000
         ALR   PAREPTR,@12                                         0312 00935000
*             IF PARTENDX>=PARTEUSE|/* GT NUMBER PARTE'S USED        */ 00936000
*                 PARENUSE='1'B THEN/* PARTE NOT IN USE??            */ 00937000
         C     PARTENDX,PARTEUSE(,PARTPTR)                         0313 00938000
         BNL   @RT00313                                            0313 00939000
         TM    PARENUSE(PAREPTR),B'10000000'                       0313 00940000
         BNO   @RF00313                                            0313 00941000
@RT00313 DS    0H                                                  0314 00942000
*               DO;                                                0314 00943000
*                 RFY                                              0315 00944000
*                   PARTENDX UNRSTD;/* RELEASE REG FOR USE           */ 00945000
*                 AIABADID='1'B;    /* SET BAD AIA INDICATOR         */ 00946000
         OI    AIABADID(AIAPTR),B'00000001'                        0316 00947000
*                 ATAAIA=0;         /* ZERO RECOVERY POINTER         */ 00948000
         SLR   @12,@12                                             0317 00949000
         ST    @12,ATAAIA(,ATAPTR)                                 0317 00950000
*                 RFY                                              0318 00951000
*                   R8 RSTD;        /* RESTRICT USE OF REG           */ 00952000
*                 R8=PARTAIAE;      /* GET ADDRESS OF FIRST ON QUE   */ 00953000
         L     R8,PARTAIAE(,PARTPTR)                               0319 00954000
*AGAIN:                                                            0320 00955000
*                 AIANXAIA=R8;      /* PUT ADDRESS IN AIA            */ 00956000
AGAIN    ST    R8,AIANXAIA(,AIAPTR)                                0320 00957000
*                 CS(R8,AIAPTR,PARTAIAE);/* C&S NEW ADDRESS          */ 00958000
         CS    R8,@05,PARTAIAE(PARTPTR)                            0321 00959000
*                 BC(7,AGAIN);      /* NOT SUCCESSFUL-TRY AGAIN      */ 00960000
         BC    7,AGAIN                                             0322 00961000
*                 RFY                                              0323 00962000
*                   R8 UNRSTD;      /* RELEASE REG FOR USE           */ 00963000
*                 RFY                                              0324 00964000
*                   R1 RSTD;                                       0324 00965000
*                 R1=IOEPTR;                                       0325 00966000
         LR    R1,IOEPTR                                           0325 00967000
*                 CALL FREEIOE;     /* FREE IOE-NOT NEEDED           */ 00968000
         BAL   @14,FREEIOE                                         0326 00969000
*                 RFY                                              0327 00970000
*                   R1 UNRSTD;                                     0327 00971000
*               END;                                               0328 00972000
*                                                                  0329 00973000
*             /*******************************************************/ 00974000
*             /*                                                     */ 00975000
*             /* IF THE DATA SET IS OK THE IOE IS PUT ON THE READ    */ 00976000
*             /* QUEUE OF THE PART ENTRY.                            */ 00977000
*             /*                                                     */ 00978000
*             /*******************************************************/ 00979000
*                                                                  0329 00980000
*             ELSE                  /* PARTE IS OK-PUT IOE ON THE  0329 00981000
*                                      READ QUEUE                    */ 00982000
*               DO;                 /* DO-READ QUEUE PROCESSING      */ 00983000
         B     @RC00313                                            0329 00984000
@RF00313 DS    0H                                                  0330 00985000
*                 ATAAIA=0;         /* ZERO RECOVERY POINTER         */ 00986000
         SLR   @12,@12                                             0330 00987000
         ST    @12,ATAAIA(,ATAPTR)                                 0330 00988000
*                 RFY                                              0331 00989000
*                   R8 RSTD;        /* RESTRICT USE OF REGISTER      */ 00990000
*                 R8=PAREIOEQ;      /* GET FIRST IOE ON QUEUE        */ 00991000
         L     R8,PAREIOEQ(,PAREPTR)                               0332 00992000
*AGAIN1:                                                           0333 00993000
*                 IOENXT=R8;        /* PUT QUEUE ADDR IN IOE         */ 00994000
AGAIN1   ST    R8,IOENXT(,IOEPTR)                                  0333 00995000
*                 CS(R8,IOEPTR,PAREIOEQ);/* C&S NEW ADDRESS          */ 00996000
         CS    R8,@06,PAREIOEQ(PAREPTR)                            0334 00997000
*                 BC(7,AGAIN1);     /* NOT SUCCESSFUL-TRY AGAIN      */ 00998000
         BC    7,AGAIN1                                            0335 00999000
*                 RFY                                              0336 01000000
*                   R8 UNRSTD;      /* RELEASE REG FOR USE           */ 01001000
*                 RFY                                              0337 01002000
*                   IORBPTR RSTD;   /* RESTRICT USE OF REGISTER      */ 01003000
*                                                                  0337 01004000
*                 /***************************************************/ 01005000
*                 /*                                                 */ 01006000
*                 /* WE HAVE JUST PUT AN IOE ON THE PART ENTRY READ  */ 01007000
*                 /* QUEUE AND NOW MUST DETERMINE IF WORK COULD BE   */ 01008000
*                 /* DONE IF ILRPTM00 IS DISPATCHED. TO DETERMINE    */ 01009000
*                 /* THIS THE IORB'S ARE SEARCHED FOR ONE THAT IS NOT*/ 01010000
*                 /* BUSY. IF ONE IS FOUND A FLAG IS TURNED ON WHICH */ 01011000
*                 /* WILL LATER BE USED FOR SCHEDULING ILRPTM00.     */ 01012000
*                 /*                                                 */ 01013000
*                 /***************************************************/ 01014000
*                                                                  0338 01015000
*                 IORBPTR=PAREIORB; /* GET FIRST IORB ADDRESS        */ 01016000
         L     IORBPTR,PAREIORB(,PAREPTR)                          0338 01017000
*                 RFY                                              0339 01018000
*                  (PARTPTR,                                       0339 01019000
*                   PAREPTR) UNRSTD;/* RELEASE REGS                  */ 01020000
*                 DO WHILE(ASM2RFLG='0'B&/* GO THROUGH CHAIN OF    0340 01021000
*                                      IORB'S                        */ 01022000
*                       IORBPTR^=0);                               0340 01023000
         B     @DE00340                                            0340 01024000
@DL00340 DS    0H                                                  0341 01025000
*                   IF IORFUSE='1'B THEN/* IS IORB IN USE            */ 01026000
         TM    IORFUSE(IORBPTR),B'10000000'                        0341 01027000
         BNO   @RF00341                                            0341 01028000
*                     IORBPTR=IORIORB;/* GET NEXT IORB               */ 01029000
         L     IORBPTR,IORIORB(,IORBPTR)                           0342 01030000
*                   ELSE            /* IORB NOT IN USE               */ 01031000
*                     ASM2RFLG='1'B;/* SET INTERNAL FLAG             */ 01032000
         B     @RC00341                                            0343 01033000
@RF00341 OI    ASM2RFLG(ASMVTPTR),B'10000000'                      0343 01034000
*                 END;              /* END IORB SEARCH               */ 01035000
@RC00341 DS    0H                                                  0344 01036000
@DE00340 TM    ASM2RFLG(ASMVTPTR),B'10000000'                      0344 01037000
         BNZ   @DC00340                                            0344 01038000
         LTR   IORBPTR,IORBPTR                                     0344 01039000
         BNZ   @DL00340                                            0344 01040000
@DC00340 DS    0H                                                  0345 01041000
*                 RFY                                              0345 01042000
*                   IORBPTR UNRSTD; /* RELEASE REG FOR USE           */ 01043000
*               END;                /* END READ QUEUE PROCESSING     */ 01044000
*           END;                    /* END-READ PROCESSING           */ 01045000
@RC00313 DS    0H                                                  0348 01046000
*       END;                        /* END-SIMPLEX PROCESSING        */ 01047000
@RC00290 DS    0H                                                  0349 01048000
*     RFY                                                          0349 01049000
*       IOEPTR UNRSTD;              /* RELEASE IOEPTR FOR USE        */ 01050000
@RC00230 DS    0H                                                  0350 01051000
*                                                                  0350 01052000
*     /***************************************************************/ 01053000
*     /*                                                             */ 01054000
*     /* THIS ENDS THE DO WHILE LOOP TO GO THROUGH ALL OF THE AIA'S. */ 01055000
*     /* IF THERE ARE NO MORE THE LAST POINTER IS SET TO ZERO.       */ 01056000
*     /*                                                             */ 01057000
*     /***************************************************************/ 01058000
*                                                                  0350 01059000
*     AIAPTR=ASMSTAGF;              /* GET ADDRESS OF NEXT AIA       */ 01060000
         L     AIAPTR,ASMSTAGF(,ASMVTPTR)                          0350 01061000
*   END;                            /* THIS ENDS THE LOOP OF CYCLING    01062000
*                                      THROUGH THE AIA'S             */ 01063000
@DE00218 SLR   @12,@12                                             0351 01064000
         CR    AIAPTR,@12                                          0351 01065000
         BNE   @DL00218                                            0351 01066000
*   ASMSTAGL=0;                     /* NO MORE TO PROCESS-SET LAST 0352 01067000
*                                      POINTER TO ZERO               */ 01068000
         ST    @12,ASMSTAGL(,ASMVTPTR)                             0352 01069000
*                                                                  0353 01070000
*   /*****************************************************************/ 01071000
*   /*                                                               */ 01072000
*   /* IF ANY WRITE IOE'S HAVE BEEN BUILT THE FLAG IS TESTED TO SEE  */ 01073000
*   /* IF THE IOE'S MUST BE PUT ON THE PART WRITE QUEUE              */ 01074000
*   /*                                                               */ 01075000
*   /*****************************************************************/ 01076000
*                                                                  0353 01077000
*CHKQUE:                                                           0353 01078000
*   RFY                                                            0353 01079000
*     PARTPTR RSTD;                 /* RESTRICT USE OF REG           */ 01080000
CHKQUE   DS    0H                                                  0354 01081000
*   PARTPTR=ASMPART;                /* GET ADDRESS OF THE PART       */ 01082000
         L     PARTPTR,ASMPART(,ASMVTPTR)                          0354 01083000
*   IF ASM2WFLG='1'B THEN           /* ANY WRITE IOE'S TO BE QUED    */ 01084000
         TM    ASM2WFLG(ASMVTPTR),B'01000000'                      0355 01085000
         BNO   @RF00355                                            0355 01086000
*     DO;                           /* YES-QUEUE TO PART QUEUES      */ 01087000
*       RFY                                                        0357 01088000
*         R11 RSTD;                 /* RESTRICT USE OF REG           */ 01089000
*       R11=ADDR(PARTLOCK);         /* GET ADDRESS OF LOCKWORD       */ 01090000
         LA    R11,PARTLOCK(,PARTPTR)                              0358 01091000
*       GENERATE REFS(PSALITA,FLC);                                0359 01092000
GETLOCK SETLOCK OBTAIN,TYPE=ASM,ADDR=(11),MODE=UNCOND,RELATED=(PART.ILRR01093000
               QIOE(FREELOCK))                                          01094000
*       RFY                                                        0360 01095000
*         R11 UNRSTD;               /* RELEASE REG FOR USE           */ 01096000
*       RFY                                                        0361 01097000
*        (R6,                                                      0361 01098000
*         R7) RSTD;                 /* RESTRICT USE OF REG           */ 01099000
*       IF ASM2PAQ^=0 THEN          /* IS THERE A LOCAL P.A. QUEUE   */ 01100000
         L     @12,ASM2PAQ(,ASMVTPTR)                              0362 01101000
         LTR   @12,@12                                             0362 01102000
         BZ    @RF00362                                            0362 01103000
*         DO;                       /* YES-CHECK WHERE TO PUT IT     */ 01104000
*           IF ASMNOLCL='1'B THEN   /* IS A LOCAL DATA SET AVAIL     */ 01105000
         TM    ASMNOLCL(ASMVTPTR),B'00100000'                      0364 01106000
         BNO   @RF00364                                            0364 01107000
*             R6=ADDR(PARTCOMQ);    /* GET ADDRESS OF COMMON QUEUE   */ 01108000
         LA    R6,PARTCOMQ(,PARTPTR)                               0365 01109000
