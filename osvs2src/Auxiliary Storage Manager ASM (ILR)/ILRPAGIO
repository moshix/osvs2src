*       RFY                                                        0383 01147000
*         R11 RSTD;                 /* RESTRICT REG FROM USE         */ 01148000
*       R11=ADDR(PARTLOCK);         /* GET ADDRESS OF LOCKWORD       */ 01149000
         LA    R11,PARTLOCK(,PARTPTR)                              0384 01150000
*       GENERATE;                                                  0385 01151000
FREELOCK SETLOCK RELEASE,TYPE=ASM,ADDR=(11),RELATED=(PART,ILRQIOE(GETLOO01152000
               CK))                                                     01153000
*     END;                          /* END-QUEUE TO PART QUEUES      */ 01154000
*   RFY                                                            0387 01155000
*     R11 UNRSTD;                   /* RELEASE REG FOR USE           */ 01156000
@RF00355 DS    0H                                                  0388 01157000
*                                                                  0388 01158000
*   /*****************************************************************/ 01159000
*   /*                                                               */ 01160000
*   /* WE NOW DETERMINE IF PART MONITOR SHOULD BE SCHEDULED. IT WILL */ 01161000
*   /* NOT BE SCHEDULED IF IT ALREADY HAS BEEN SCHEDULED.            */ 01162000
*   /*                                                               */ 01163000
*   /*****************************************************************/ 01164000
*                                                                  0388 01165000
*SCHEDULE:                                                         0388 01166000
*   IF ASMCALLQ='0'B THEN           /* WAS COMPLETION THE CALLER     */ 01167000
SCHEDULE TM    ASMCALLQ(ASMVTPTR),B'10000000'                      0388 01168000
         BNZ   @RF00388                                            0388 01169000
*     IF ASMIOCNT=0|                /* HAS ALL I/O COMPLETED         */ 01170000
*         ASM2RFLG='1'B|            /* NO-WAS IOE PUT ON A READ Q    */ 01171000
*         ASM2WFLG='1'B|            /* WAS IOE PUT ON A WRITE Q      */ 01172000
*         PARTAIAE^=0 THEN          /* WAS ONE PUT ON ERROR QUEUE    */ 01173000
         SLR   @12,@12                                             0389 01174000
         C     @12,ASMIOCNT(,ASMVTPTR)                             0389 01175000
         BE    @RT00389                                            0389 01176000
         TM    ASM2RFLG(ASMVTPTR),B'11000000'                      0389 01177000
         BNZ   @RT00389                                            0389 01178000
         C     @12,PARTAIAE(,PARTPTR)                              0389 01179000
         BE    @RF00389                                            0389 01180000
@RT00389 DS    0H                                                  0390 01181000
*       IF ASMSRBCT=0 THEN          /* IS AN SRB ALREADY SCHED       */ 01182000
         L     @12,ASMSRBCT(,ASMVTPTR)                             0390 01183000
         LTR   @12,@12                                             0390 01184000
         BNZ   @RF00390                                            0390 01185000
*         DO;                       /* NO-INITIALIZE AND SCHEDULE    */ 01186000
*           RFY                                                    0392 01187000
*             R1 RSTD;              /* RESTRICT USE OF REG           */ 01188000
*           RFY                                                    0393 01189000
*             R8 RSTD;              /* RESTRICT USE OF REG           */ 01190000
*           R1=0;                   /* SRB COUNT SHOULD BE 0         */ 01191000
         SLR   R1,R1                                               0394 01192000
*           R8=1;                   /* NEW COUNT WILL BE 1           */ 01193000
         LA    R8,1                                                0395 01194000
*           CS(R1,R8,ASMSRBCT);     /* C&S THE NEW COUNT             */ 01195000
         CS    R1,@08,ASMSRBCT(ASMVTPTR)                           0396 01196000
*           BC(7,ILRCRQI1);         /* NE-DO NOT SCHEDULE            */ 01197000
         BC    7,ILRCRQI1                                          0397 01198000
*           R1=ASMPSRB;             /* GET ADDRESS OF THE SRB        */ 01199000
         L     R1,ASMPSRB(,ASMVTPTR)                               0398 01200000
*           GENERATE REFS(CVTGSMQ,SRBFLNK,SRBSECT);                0399 01201000
                SCHEDULE SRB=(1),SCOPE=GLOBAL                           01202000
*         END;                                                     0400 01203000
*   RFY                                                            0401 01204000
*    (R1,                                                          0401 01205000
*     R8,                                                          0401 01206000
*     PARTPTR) UNRSTD;              /* RELEASE REGS FOR USE          */ 01207000
@RF00390 DS    0H                                                  0401 01208000
@RF00389 DS    0H                                                  0401 01209000
@RF00388 DS    0H                                                  0402 01210000
*ILRCRQI1:                                                         0402 01211000
*   LM(R2,R14,ASMWKSA2);            /* RELOAD REGS OF CALLER         */ 01212000
ILRCRQI1 LM    R2,R14,ASMWKSA2(ASMVTPTR)                           0402 01213000
*   ATAQIOE='0'B;                   /* TURN OFF QIOE FLAG IN ATA     */ 01214000
         NI    ATAQIOE(ATAPTR),B'01111111'                         0403 01215000
*   RETURN;                         /* RETURN TO THE CALLER          */ 01216000
@EL00002 DS    0H                                                  0404 01217000
@EF00002 DS    0H                                                  0404 01218000
@ER00002 BR    @14                                                 0404 01219000
*PARTQUE:                                                          0405 01220000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0405 01221000
*                                                                  0405 01222000
PARTQUE  DS    0H                                                  0406 01223000
*   /*****************************************************************/ 01224000
*   /*                                                               */ 01225000
*   /* THIS ROUTINE WILL QUEUE THE IOE'S TO THE PART WRITE QUEUE     */ 01226000
*   /* REGISTER 6 POINTS TO THE QUEUE POINTERS THAT THE IOE'S ARE TO */ 01227000
*   /* BE QUEUED TO. REGISTER 7 POINTS TO THE IOE'S TO BE QUEUED TO  */ 01228000
*   /* THE PART QUEUES.                                              */ 01229000
*   /*                                                               */ 01230000
*   /*****************************************************************/ 01231000
*                                                                  0406 01232000
*   RFY                                                            0406 01233000
*    (R6,                                                          0406 01234000
*     R7,                                                          0406 01235000
*     R8,                                                          0406 01236000
*     R9) RSTD;                     /* RESTRICT REGS FROM USE        */ 01237000
*   GEN(LM    R8,R9,0(R6));         /* GET QUEUE POINTERS            */ 01238000
         LM    R8,R9,0(R6)                                              01239000
*   IF R8=0 THEN                    /* IS ANYTHING ON QUEUE          */ 01240000
         LTR   R8,R8                                               0408 01241000
         BNZ   @RF00408                                            0408 01242000
*     R8=R7;                        /* SET FIRST POINTER TO IOE      */ 01243000
         LR    R8,R7                                               0409 01244000
*   ELSE                            /* IOE'S ALREADY ON QUEUE        */ 01245000
*     R9->IOENXT=R7;                /* CHAIN NEW IOE'S TO LAST IOE ON   01246000
*                                      QUEUE                         */ 01247000
         B     @RC00408                                            0410 01248000
@RF00408 ST    R7,IOENXT(,R9)                                      0410 01249000
*   DO WHILE(R7->IOENXT^=0);        /* GET ADDR OF LAST IOE          */ 01250000
@RC00408 B     @DE00411                                            0411 01251000
@DL00411 DS    0H                                                  0412 01252000
*     R7=R7->IOENXT;                /* GET NEXT POINTER              */ 01253000
         L     R7,IOENXT(,R7)                                      0412 01254000
*   END;                            /* LOOP UNTIL LAST IS FOUND      */ 01255000
@DE00411 L     @12,IOENXT(,R7)                                     0413 01256000
         LTR   @12,@12                                             0413 01257000
         BNZ   @DL00411                                            0413 01258000
*   R9=R7;                          /* SET NEW LAST POINTER          */ 01259000
         LR    R9,R7                                               0414 01260000
*   GEN(STM   R8,R9,0(R6));         /* RESET NEW QUEUE POINTERS      */ 01261000
         STM   R8,R9,0(R6)                                              01262000
*   RFY                                                            0416 01263000
*    (R6,                                                          0416 01264000
*     R7,                                                          0416 01265000
*     R8,                                                          0416 01266000
*     R9) UNRSTD;                   /* RELEASE REGS FROM USE         */ 01267000
*   END PARTQUE;                                                   0417 01268000
@EL00003 DS    0H                                                  0417 01269000
@EF00003 DS    0H                                                  0417 01270000
@ER00003 BR    @14                                                 0417 01271000
*GETIOE:                                                           0418 01272000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0418 01273000
*                                                                  0418 01274000
GETIOE   DS    0H                                                  0419 01275000
*   /*****************************************************************/ 01276000
*   /*                                                               */ 01277000
*   /* THIS ROUTINE GETS AN IOE ONE AT A TIME. IF NONE ARE AVAILABLE */ 01278000
*   /* REGISTER 1 IS SET TO ZERO.                                    */ 01279000
*   /*                                                               */ 01280000
*   /*****************************************************************/ 01281000
*                                                                  0419 01282000
*   ASM2SR14=R14;                   /* SAVE REGISTER 14              */ 01283000
*                                                                  0419 01284000
         ST    R14,ASM2SR14(,ASMVTPTR)                             0419 01285000
*   /*****************************************************************/ 01286000
*   /*                                                               */ 01287000
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */ 01288000
*   /*                                                               */ 01289000
*   /*****************************************************************/ 01290000
*                                                                  0420 01291000
*   DO;                             /* ILRGMA GET                  0420 01292000
*                                      CELL(IOE)WORKREGS(R12,R11)    */ 01293000
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */ 01294000
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0421 01295000
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */ 01296000
*       1 GMACELL BASED BDY(WORD),                                 0422 01297000
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */ 01298000
*     RESPECIFY                                                    0423 01299000
*      (R0,                                                        0423 01300000
*       R1,                                                        0423 01301000
*       R12,                                                       0423 01302000
*       R11,                                                       0423 01303000
*       R14,                                                       0423 01304000
*       R15) RESTRICTED;                                           0423 01305000
*     RESPECIFY                                                    0424 01306000
*       ASMPOOLS BASED(R12);                                       0424 01307000
*     RESPECIFY                                                    0425 01308000
*       GMACELL BASED(R14);                                        0425 01309000
*     R12=ADDR(ASMIOEPC);                                          0426 01310000
         LA    R12,ASMIOEPC(,ASMVTPTR)                             0426 01311000
*     LM(R14,R15,ASMCPAVQ);         /* GET ADDR OF 1ST CELL          */ 01312000
         LM    R14,R15,ASMCPAVQ(R12)                               0427 01313000
*GMA00010:                                                         0428 01314000
*     IF R14^=0 THEN                /* TEST CELL POOL                */ 01315000
GMA00010 LTR   R14,R14                                             0428 01316000
         BZ    @RF00428                                            0428 01317000
*       DO;                         /* CELL AVAILABLE, REMOVE CELL   */ 01318000
*         R0=GMANEXT;                                              0430 01319000
         L     R0,GMANEXT(,R14)                                    0430 01320000
*         R1=R15-1;                                                0431 01321000
         LR    R1,R15                                              0431 01322000
         BCTR  R1,0                                                0431 01323000
*         CDS(R14,R0,ASMCPAVQ);                                    0432 01324000
         CDS   R14,R0,ASMCPAVQ(R12)                                0432 01325000
*         BC(7,GMA00010);                                          0433 01326000
         BC    7,GMA00010                                          0433 01327000
*       END;                                                       0434 01328000
*     R1=R14;                                                      0435 01329000
@RF00428 LR    R1,R14                                              0435 01330000
*     RESPECIFY                                                    0436 01331000
*      (R0,                                                        0436 01332000
*       R1,                                                        0436 01333000
*       R12,                                                       0436 01334000
*       R11,                                                       0436 01335000
*       R14,                                                       0436 01336000
