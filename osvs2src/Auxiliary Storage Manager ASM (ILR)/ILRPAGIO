         LA    R6,PARTCOMQ(,PARTPTR)                               0373 01127000
*           CALL PARTQUE;           /* PUT IOE'S ON PART QUEUE       */ 01128000
         BAL   @14,PARTQUE                                         0374 01129000
*         END;                      /* END COMMON QUEUE PROCESSING   */ 01130000
*       IF ASM2DUPQ^=0 THEN         /* ANY IOE'S ON DUPE QUEUE       */ 01131000
@RF00370 L     @12,ASM2DUPQ(,ASMVTPTR)                             0376 01132000
         LTR   @12,@12                                             0376 01133000
         BZ    @RF00376                                            0376 01134000
*         DO;                       /* YES-DUPE QUEUE PROCESSING     */ 01135000
*           R7=ASM2DUPQ;            /* GET FIRST IOE ON DUPE QUEUE   */ 01136000
         LR    R7,@12                                              0378 01137000
*           R6=ADDR(PARTDUPQ);      /* GET ADDRESS OF QUEUE          */ 01138000
