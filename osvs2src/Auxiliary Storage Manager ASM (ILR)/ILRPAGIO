*             R6=ADDR(PARTLOCQ);    /* PUT THEM ON LOCAL QUEUE       */ 01111000
         B     @RC00364                                            0366 01112000
@RF00364 LA    R6,PARTLOCQ(,PARTPTR)                               0366 01113000
*           R7=ASM2PAQ;             /* GET FIRST IOE ON P.A. QUE     */ 01114000
@RC00364 L     R7,ASM2PAQ(,ASMVTPTR)                               0367 01115000
*           CALL PARTQUE;           /* GO TO PUT THEM ON QUEUE       */ 01116000
         BAL   @14,PARTQUE                                         0368 01117000
*         END;                      /* END ASM2PAQ PROCESSING        */ 01118000
*       IF ASM2CAQ^=0 THEN          /* IS THERE A COMMON QUEUE       */ 01119000
@RF00362 L     @12,ASM2CAQ(,ASMVTPTR)                              0370 01120000
         LTR   @12,@12                                             0370 01121000
         BZ    @RF00370                                            0370 01122000
*         DO;                       /* COMMON QUEUE PROCESSING       */ 01123000
*           R7=ASM2CAQ;             /* GET FIRST IOE ON COMMON QUE   */ 01124000
         LR    R7,@12                                              0372 01125000
