         TITLE 'ILRPEX, ASM POOL EXTENDER                              *00001000
                        '                                               00002000
ILRPEX   CSECT                                                          00003000
         USING *,R15              ADDRESSABILITY FOR MODID MACRO        00004000
         MODID                                                          00005000
         DROP  R15                DROP ADDRESSABILITY                   00006000
         LR    R15,R13            SAVE REG 13                           00007000
         LR    R0,R14             SAVE REG 14                           00008000
GETLOCK  SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                      **00009000
               RELATED=(POOLEXPAND,ILRPEX(FREELOCK))                    00010000
         L     R14,FLCCVT         GET CVT ADDRESS                       00011000
         L     R14,CVTASMVT(,R14) GET ASMVT ADDRESS                     00012000
         STM   R15,R10,ASMWKSA7(R14) SAVE CALLERS REGS                  00013000
         BALR  R12,0              ESTABLISH MODULE                      00014000
         USING *,R12                  ADDRESSABILITY                    00015000
*   ASMVTPTR=R14;                   /* SET PERMANENT ASMVT BASE REG  */ 00016000
         LR    ASMVTPTR,R14                                        0042 00017000
*   POOLADDR=R1;                    /* SET PERMANENT POOL CONTROLLER    00018000
*                                      BASE REG                      */ 00019000
         LR    POOLADDR,R1                                         0043 00020000
*   R1=0;                           /* INIT RETURN PARM REG TO ZERO  */ 00021000
         SLR   R1,R1                                               0044 00022000
*                                                                  0045 00023000
*   /*****************************************************************/ 00024000
*   /*                                                               */ 00025000
*   /* THE SETLOCK RETURN CODE IS CHECKED. IT INDICATES WHETHER OR   */ 00026000
*   /* NOT THE SALLOC LOCK WAS HELD ON ENTRY. IF THE SALLOC LOCK WAS */ 00027000
*   /* NOT HELD ON ENTRY (RETURN CODE ^= 0), A CELL IS RETURNED TO   */ 00028000
*   /* THE CALLER ONLY IF ONE EXISTS ON THE ACE RESERVE Q. OTHERWISE,*/ 00029000
*   /* A ZERO IS RETURNED IN REG 1.                                  */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                                  0045 00033000
*   IF R13^=0 THEN                  /* TEST SETLOCK RETURN CODE      */ 00034000
         LTR   R13,R13                                             0045 00035000
         BZ    @RF00045                                            0045 00036000
*     DO;                           /* SALLOC WAS HELD ON ENTRY TO 0046 00037000
*                                      PEX                           */ 00038000
*       IF ASMCPID=ACEPID THEN      /* CHECK FOR ACE POOL            */ 00039000
         CLC   ASMCPID(4,POOLADDR),ACEPID                          0047 00040000
         BNE   @RF00047                                            0047 00041000
*         DO;                       /* WAS ACE POOL, TAKE A CELL     */ 00042000
*           RFY                                                    0049 00043000
*            (EVEN1,                                               0049 00044000
*             ODD1,                                                0049 00045000
*             EVEN2,                                               0049 00046000
*             ODD2) RSTD;                                          0049 00047000
*           LM(EVEN1,ODD1,ASMCPRSQ);/* GET CONTENTS OF POOL        0050 00048000
*                                      CONTROLLER                    */ 00049000
         LM    EVEN1,ODD1,ASMCPRSQ(POOLADDR)                       0050 00050000
*TAKAGAIN:                                                         0051 00051000
*           IF ODD1^=0 THEN         /* TEST FOR RESERVE POOL EMPTY   */ 00052000
TAKAGAIN LTR   ODD1,ODD1                                           0051 00053000
         BZ    @RF00051                                            0051 00054000
*             DO;                   /* POOL NOT EMPTY                */ 00055000
*               EVEN2=EVEN1+1;      /* INCR TAKEN COUNT FOR CDS      */ 00056000
         LA    EVEN2,1                                             0053 00057000
         ALR   EVEN2,EVEN1                                         0053 00058000
*               ODD2=ODD1->CELLNEXT;/* ADDR OF NEXT CELL IN POOL     */ 00059000
         L     ODD2,CELLNEXT(,ODD1)                                0054 00060000
*               CDS(EVEN1,EVEN2,ASMCPRSQ);/* REMOVE CELL             */ 00061000
         CDS   EVEN1,EVEN2,ASMCPRSQ(POOLADDR)                      0055 00062000
*               BC(7,TAKAGAIN);     /* IF FAIL, TRY TO TAKE AGAIN    */ 00063000
         BC    7,TAKAGAIN                                          0056 00064000
*               R1=ODD1;            /* IS SUCCEED, PUT CELL ADDR IN     00065000
*                                      REG 1                         */ 00066000
         LR    R1,ODD1                                             0057 00067000
*               RFY                                                0058 00068000
*                (EVEN1,                                           0058 00069000
*                 ODD1,                                            0058 00070000
*                 EVEN2,                                           0058 00071000
*                 ODD2) UNRSTD;                                    0058 00072000
*             END;                  /* END POOL NOT EMPTY            */ 00073000
*         END;                      /* END ACE POOL PROCESSING       */ 00074000
*     END;                          /* END SALLOC HELD ON ENTRY    0061 00075000
*                                      PROCESSING                    */ 00076000
*                                                                  0062 00077000
*   /*****************************************************************/ 00078000
*   /*                                                               */ 00079000
*   /* THE SALLOC LOCK WAS NOT HELD ON ENTRY TO PEX. IT IS HELD NOW. */ 00080000
*   /* THE INPUT POOL IS TESTED TO DETERMINE IF IT HAS ALREADY BEEN  */ 00081000
*   /* EXPANDED. IF SO, RETURN IS MADE WITH THE ADDRESS OF A CELL IN */ 00082000
*   /* REGISTER 1.                                                   */ 00083000
*   /*                                                               */ 00084000
*   /*****************************************************************/ 00085000
*                                                                  0062 00086000
*   ELSE                            /* SETLOCK RETURN CODE WAS ZERO  */ 00087000
*     DO;                                                          0062 00088000
         B     @RC00045                                            0062 00089000
@RF00045 DS    0H                                                  0063 00090000
*       RFY                                                        0063 00091000
*        (EVEN1,                                                   0063 00092000
*         ODD1,                                                    0063 00093000
*         EVEN2,                                                   0063 00094000
*         ODD2) RSTD;                                              0063 00095000
*       LM(EVEN1,ODD1,ASMCPAVQ);    /* GET CONTENTS OF POOL        0064 00096000
*                                      CONTROLLER                    */ 00097000
         LM    EVEN1,ODD1,ASMCPAVQ(POOLADDR)                       0064 00098000
*TESTAGIN:                                                         0065 00099000
*       IF EVEN1^=0 THEN            /* TEST FOR POOL BEING EMPTY     */ 00100000
TESTAGIN LTR   EVEN1,EVEN1                                         0065 00101000
         BZ    @RF00065                                            0065 00102000
*         DO;                       /* POOL NOT EMPTY, TAKE A CELL   */ 00103000
*           EVEN2=EVEN1->CELLNEXT;  /* GET ADDR OF NEXT CELL         */ 00104000
         L     EVEN2,CELLNEXT(,EVEN1)                              0067 00105000
*           ODD2=ODD1-1;            /* DECREMENT COUNT               */ 00106000
         LR    ODD2,ODD1                                           0068 00107000
         BCTR  ODD2,0                                              0068 00108000
*           CDS(EVEN1,EVEN2,ASMCPAVQ);/* REMOVE CELL                 */ 00109000
         CDS   EVEN1,EVEN2,ASMCPAVQ(POOLADDR)                      0069 00110000
*           BC(7,TESTAGIN);         /* IF FAIL, TEST POOL AGAIN      */ 00111000
         BC    7,TESTAGIN                                          0070 00112000
*           R1=EVEN1;               /* IF SUCCEED, PUT CELL ADDR IN     00113000
*                                      REG 1                         */ 00114000
         LR    R1,EVEN1                                            0071 00115000
*           RFY                                                    0072 00116000
*            (EVEN1,                                               0072 00117000
*             ODD1,                                                0072 00118000
*             EVEN2,                                               0072 00119000
*             ODD2) UNRSTD;                                        0072 00120000
*         END;                      /* END POOL NOT EMPTY PROCESSING */ 00121000
*                                                                  0074 00122000
*       /*************************************************************/ 00123000
*       /*                                                           */ 00124000
*       /* THE INPUT POOL WAS STILL EMPTY AFTER OBTAINING THE SALLOC */ 00125000
*       /* LOCK. THE SIZE OF SQA TO GETMAIN IS CALCULATED USING THE  */ 00126000
*       /* POOL CONTROLLER FIELDS. IF THE GETMAIN FAILS, REG 1 IS SET*/ 00127000
*       /* TO ZERO AND EXIT IS MADE. IF THE GETMAIN IS SUCCESSFUL,   */ 00128000
*       /* THE AREA OBTAINED IS FORMATTED INTO CELLS AND QUEUED TO   */ 00129000
*       /* THE POOL CONTROLLER. THE FIRST CELL IN THE AREA IS PASSED */ 00130000
*       /* BACK TO THE CALLER IN REGISTER 1.                         */ 00131000
*       /*                                                           */ 00132000
*       /*************************************************************/ 00133000
*                                                                  0074 00134000
*       ELSE                        /* INPUT POOL CONTROLLER WAS   0074 00135000
*                                      EMPTY                         */ 00136000
*         DO;                                                      0074 00137000
         B     @RC00065                                            0074 00138000
@RF00065 DS    0H                                                  0075 00139000
*           R0=LENGTH(POOLHDR)+(ASMCPEXT*ASMCPSIZ);/* CALCULATE THE     00140000
*                                      LENGTH FOR GETMAIN            */ 00141000
         LH    R0,ASMCPEXT(,POOLADDR)                              0075 00142000
         MH    R0,ASMCPSIZ(,POOLADDR)                              0075 00143000
         AL    R0,@CF00036                                         0075 00144000
*           GEN(GETMAIN RC,LV=(0),SP=245,BRANCH=(YES,GLOBAL));/*   0076 00145000
*                                      ISSUE GETMAIN                 */ 00146000
         GETMAIN RC,LV=(0),SP=245,BRANCH=(YES,GLOBAL)                   00147000
*           ASMVTPTR=CVTASMVT;      /* RESTORE ASMVT BASE REG        */ 00148000
         L     @11,CVTPTR                                          0077 00149000
         L     ASMVTPTR,CVTASMVT(,@11)                             0077 00150000
*           IF R15=0 THEN           /* TEST GETMAIN RETURN CODE      */ 00151000
         LTR   R15,R15                                             0078 00152000
         BNZ   @RF00078                                            0078 00153000
*             DO;                   /* STORAGE WAS ABTAINED          */ 00154000
*               RFY                                                0080 00155000
*                 AREA@ RSTD;                                      0080 00156000
*               AREA@=R1;           /* SAVE ADDR OF OBTAINED AREA    */ 00157000
         LR    AREA@,R1                                            0081 00158000
*               R1=R0;              /* SET LENGTH FOR MVCL         0082 00159000
*                                      INSTRUCTION                   */ 00160000
         LR    R1,R0                                               0082 00161000
*               R0=AREA@;           /* SET ADDR TO CLEAR FOR MVCL  0083 00162000
*                                      INSTRUCTION                   */ 00163000
         LR    R0,AREA@                                            0083 00164000
*               MVCL(R0,R14);       /* CLEAR OBTAINED AREA. R14 NOT     00165000
*                                      REQUIRED TO BE SET UP. R15 IS    00166000
*                                      ZERO FROM ABOVE TEST          */ 00167000
         MVCL  R0,R14                                              0084 00168000
*               AREA@->POOLHDR(1:3)=ASMCPID(1:3);/* SET POOL TYPE IN    00169000
*                                      HEADER                        */ 00170000
         MVC   POOLHDR(3,AREA@),ASMCPID(POOLADDR)                  0085 00171000
*               AREA@->POOLHDR(4:8)=EXTDID;/* SET EXTENSION IN HEADER*/ 00172000
         MVC   POOLHDR+3(5,AREA@),EXTDID                           0086 00173000
*               RFY                                                0087 00174000
*                (CELLSAVE,                                        0087 00175000
*                 CELLPTR,                                         0087 00176000
*                 CELLCNT) RSTD;                                   0087 00177000
*               CELLSAVE=AREA@+LENGTH(POOLHDR);/* GET ADDR OF CELL TO   00178000
*                                      BE RETURNED TO CALLER         */ 00179000
         LA    CELLSAVE,8                                          0088 00180000
         ALR   CELLSAVE,AREA@                                      0088 00181000
*               CELLPTR=CELLSAVE+ASMCPSIZ;/* GET ADDR OF FIRST CELL     00182000
*                                      TO FORMAT                     */ 00183000
         LR    CELLPTR,CELLSAVE                                    0089 00184000
         AH    CELLPTR,ASMCPSIZ(,POOLADDR)                         0089 00185000
*               DO CELLCNT=ASMCPEXT-2 TO 1 BY-1;/* INIT. ALL CELLS 0090 00186000
*                                      BUT LAST                      */ 00187000
         LH    CELLCNT,ASMCPEXT(,POOLADDR)                         0090 00188000
         BCTR  CELLCNT,0                                           0090 00189000
         BCTR  CELLCNT,0                                           0090 00190000
         B     @DE00090                                            0090 00191000
@DL00090 DS    0H                                                  0091 00192000
*                 CELLNEXT=CELLPTR+ASMCPSIZ;/* SET PTR TO NEXT CELL  */ 00193000
         LR    @11,CELLPTR                                         0091 00194000
         AH    @11,ASMCPSIZ(,POOLADDR)                             0091 00195000
         ST    @11,CELLNEXT(,CELLPTR)                              0091 00196000
*                 CELLPTR=CELLNEXT; /* GET ADDR OF NEXT CELL         */ 00197000
         LR    CELLPTR,@11                                         0092 00198000
*               END;                /* END DO LOOP                   */ 00199000
         BCTR  CELLCNT,0                                           0093 00200000
@DE00090 LTR   CELLCNT,CELLCNT                                     0093 00201000
         BP    @DL00090                                            0093 00202000
*               RFY                                                0094 00203000
*                (WORKREG1,                                        0094 00204000
*                 WORKREG2) RSTD;                                  0094 00205000
*                                                                  0094 00206000
*               /*****************************************************/ 00207000
*               /*                                                   */ 00208000
*               /* DO LOOP IS ENDED WITH CELLPTR POINTING TO LAST    */ 00209000
*               /* CELL TO FORMAT. ALL BUT FIRST CELL ARE NOW ADDED  */ 00210000
*               /* TO POOL CONTROLLER                                */ 00211000
*               /*                                                   */ 00212000
*               /*****************************************************/ 00213000
*                                                                  0095 00214000
*               WORKREG2=CELLSAVE+ASMCPSIZ;/* GET ADDR OF FIRST CELL    00215000
*                                      FORMATTEDD                    */ 00216000
         LR    WORKREG2,CELLSAVE                                   0095 00217000
         AH    WORKREG2,ASMCPSIZ(,POOLADDR)                        0095 00218000
*               WORKREG1=ASMCPAVL;  /* GET ADDR OF FIRST CELL IN POOL*/ 00219000
         L     WORKREG1,ASMCPAVL(,POOLADDR)                        0096 00220000
*TRYAGAIN:                                                         0097 00221000
*               CELLNEXT=WORKREG1;  /* POINT LAST CELL BUILT TO FIRST   00222000
*                                      CURRENTLY IN POOL             */ 00223000
TRYAGAIN ST    WORKREG1,CELLNEXT(,CELLPTR)                         0097 00224000
*               CS(WORKREG1,WORKREG2,ASMCPAVL);/* ADD CELLS TO POOL  */ 00225000
         CS    WORKREG1,@11,ASMCPAVL(POOLADDR)                     0098 00226000
*               BC(7,TRYAGAIN);     /* IF FAIL, TRY AGAIN            */ 00227000
         BC    7,TRYAGAIN                                          0099 00228000
*               R1=CELLSAVE;        /* SET CELL ADDRESS TO BE      0100 00229000
*                                      RETURNED WHEN DONE            */ 00230000
         LR    R1,CELLSAVE                                         0100 00231000
*               RFY                                                0101 00232000
*                (CELLSAVE,                                        0101 00233000
*                 CELLPTR,                                         0101 00234000
*                 CELLCNT,                                         0101 00235000
*                 AREA@,                                           0101 00236000
*                 WORKREG1,                                        0101 00237000
*                 WORKREG2) UNRSTD;                                0101 00238000
*             END;                  /* END STORAGE OBTAINED        0102 00239000
*                                      PROCESSING                    */ 00240000
*           ELSE                                                   0103 00241000
*             R1=0;                 /* GETMAIN FAILED, SET REG 1 TO     00242000
*                                      ZERO FOR RETURN               */ 00243000
         B     @RC00078                                            0103 00244000
@RF00078 SLR   R1,R1                                               0103 00245000
*         END;                      /* END INPUT POOL CONTROLLER   0104 00246000
*                                      EMPTY PROCESSING              */ 00247000
@RC00078 DS    0H                                                  0105 00248000
*     END;                          /* END SETLOCK RETURN CODE ZERO     00249000
*                                      PROCESSING                    */ 00250000
@RC00065 DS    0H                                                  0106 00251000
*                                                                  0106 00252000
*   /*****************************************************************/ 00253000
*   /*                                                               */ 00254000
*   /* PROCESSING IS COMPLETE. THE SALLOC LOCK IS RELEASED AFTER     */ 00255000
*   /* REGISTERS HAVE BEEN RESTORED AND THEN REGISTERS 13 AND 14 ARE */ 00256000
*   /* RESTORED AS SAVED AT ENTRY. RETURN IS MADE TO THE CALLER      */ 00257000
*   /*                                                               */ 00258000
*   /*****************************************************************/ 00259000
*                                                                  0106 00260000
*   GENERATE;                                                      0106 00261000
@RC00045 DS    0H                                                  0106 00262000
         LM    R15,R0,ASMWKSA7(ASMVTPTR)    RESTORE REGS 15 AND 0       00263000
         SPACE                                                          00264000
*   REGISTER 1 HAS BEEN SET WITH RETURN INFO NOT TO BE DESTROYED        00265000
         SPACE                                                          00266000
         LM    R2,R10,ASMWKSA7+12(ASMVTPTR) RESTORE REST OF REGS        00267000
         LTR   R13,R13            TEST SETLOCK RETURN CODE IN REG 13    00268000
         BNZ   EXIT               RETURN CODE NONZERO, EXIT             00269000
         SPACE                                                          00270000
*   A ZERO RETURN CODE INDICATES SALLOC LOCK WAS OBTAINED BY PEX,       00271000
*   IT MUST BE RELEASED BEFORE EXIT                                     00272000
         SPACE                                                          00273000
FREELOCK SETLOCK RELEASE,TYPE=SALLOC,                                 **00274000
               RELATED=(POOLEXPAND,ILRPEX(GETLOCK))                     00275000
EXIT     LR    R13,R15            RESTORE REG 13                        00276000
         LR    R14,R0             RESTORE REG 14                        00277000
         BR    R14                RETURN TO CALLER                      00278000
*   END ILRPEX                                                     0107 00279000
*                                                                  0107 00280000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00281000
