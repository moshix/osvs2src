         TITLE 'ILRPGEXP-PAGE/SWAP DATASET EXPANSION ROUTINE           *00001000
                        '                                               00002000
ILRPGEXP CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRPGEXP  76.105'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @10,0                                               0001 00010000
@PSTART  LA    @11,4095(,@10)                                      0001 00011000
         USING @PSTART,@10                                         0001 00012000
         USING @PSTART+4095,@11                                    0001 00013000
         LR    @03,@01                                             0001 00014000
         L     @00,@SIZDATD                                        0001 00015000
         GETMAIN  R,LV=(0)                                              00016000
         LR    @12,@01                                             0001 00017000
         USING @DATD,@12                                           0001 00018000
         LR    @01,@03                                             0001 00019000
         ST    @13,@SA00001+4                                      0001 00020000
         LR    @13,@12                                             0001 00021000
*   SAVERTRN=R14;                   /* SAVE RETURN ADDRESS           */ 00022000
         ST    R14,SAVERTRN                                        0175 00023000
*   RFY                                                            0176 00024000
*     R14 UNRSTD;                                                  0176 00025000
*   PTRS=''B;                       /* ZERO PART OF WORKAREA         */ 00026000
         XC    PTRS(34),PTRS                                       0177 00027000
*   CSCBPTR=R1;                     /* OBTAIN INPUT CSCB ADDRESS     */ 00028000
         ST    R1,CSCBPTR                                          0178 00029000
*   RFY                                                            0179 00030000
*    (R0,                                                          0179 00031000
*     R15) RSTD;                                                   0179 00032000
*   GEN(LOAD EP=IEECB860);          /* LOAD MASTER SCHED ESTAE RTN   */ 00033000
         LOAD EP=IEECB860                                               00034000
*   CB860=R0;                       /* SAVE IEECB860'S ADDRESS       */ 00035000
         LR    CB860,R0                                            0181 00036000
*   RFY                                                            0182 00037000
*     R0 UNRSTD;                                                   0182 00038000
*   R1=ADDR(CB860PL);               /* SET UP PARMS FOR IEECB860     */ 00039000
         LA    R1,CB860PL                                          0183 00040000
*   CB86CMDP=ADDR(CB86CMD);         /* SET UP ADDR OF CMD NAME       */ 00041000
         LA    @14,CB86CMD                                         0184 00042000
         ST    @14,CB86CMDP                                        0184 00043000
*   CB86CSCB=CSCBPTR;               /* PUT CSCB ADDRESS IN PARM LIST */ 00044000
         MVC   CB86CSCB(4),CSCBPTR                                 0185 00045000
*   RFY                                                            0186 00046000
*    (R12,                                                         0186 00047000
*     R13,                                                         0186 00048000
*     R14) RSTD;                                                   0186 00049000
*   STM(R14,R12,SAVER);             /* CB860 DESTROYS REGS SO SAVE   */ 00050000
         STM   R14,R12,SAVER(R13)                                  0187 00051000
*   CALL IEECB860;                  /* SET UP MASTER ESTAE           */ 00052000
         LR    @15,CB860                                           0188 00053000
         BALR  @14,@15                                             0188 00054000
*   LM(R14,R12,SAVER);              /* RESTORE REGISTERS             */ 00055000
         LM    R14,R12,SAVER(R13)                                  0189 00056000
*   RFY                                                            0190 00057000
*    (R13,                                                         0190 00058000
*     R14) UNRSTD;                                                 0190 00059000
*                                                                  0190 00060000
*   /*****************************************************************/ 00061000
*   /*                                                               */ 00062000
*   /* ENQ ON PAGEADD- LOAD ILRPREAD & ILROPS00 - ESTABLISH ESTAE    */ 00063000
*   /*                                                               */ 00064000
*   /*****************************************************************/ 00065000
*                                                                  0191 00066000
*   RFY                                                            0191 00067000
*     R0 RSTD;                                                     0191 00068000
*   GEN REFS(QNAME,RNAME)(ENQ (QNAME,RNAME,E,7,SYSTEM),RET=NONE);  0192 00069000
         ENQ (QNAME,RNAME,E,7,SYSTEM),RET=NONE                          00070000
*   RFY                                                            0193 00071000
*     R15 UNRSTD;                                                  0193 00072000
*   GEN(LOAD EP=ILRPREAD);          /* LOAD ILRPREAD                 */ 00073000
         LOAD EP=ILRPREAD                                               00074000
*   PREAD=R0;                       /* SAVE ILRPREAD'S ADDRESS       */ 00075000
         ST    R0,PREAD                                            0195 00076000
*   GEN(LOAD EP=ILROPS00);          /* LOAD ILROPS00                 */ 00077000
         LOAD EP=ILROPS00                                               00078000
*   RFY                                                            0197 00079000
