*               R1=ADDR(PGRWPLST);  /* GET ADDR OF PREAD PARM LIST   */ 00802000
         LA    R1,PGRWPLST                                         0442 00803000
*               READWRTE=OFF;       /* INDICATE READ                 */ 00804000
*               PGTPARB=OFF;        /* INDICATE PARTE ADDRESS        */ 00805000
         NI    READWRTE,B'00111111'                                0444 00806000
