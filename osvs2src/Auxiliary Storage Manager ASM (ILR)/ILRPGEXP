*       SDWAPTR=R1;                 /* OBTAIN SDWA ADDRESS           */ 00556000
         LR    SDWAPTR,R1                                          0357 00557000
*       RFY                                                        0358 00558000
*         R1 UNRSTD;                                               0358 00559000
*       PGSDWA=ON;                  /* INDICATE HAVE AN SDWA         */ 00560000
         OI    PGSDWA,B'00000100'                                  0359 00561000
*       SDWARECP=PGNAMES;           /* FILL IN RECORD FIELD OF SDWA  */ 00562000
         MVC   SDWARECP(24,SDWAPTR),@CC02447                       0360 00563000
*     END;                          /* END SDWA EXISTS               */ 00564000
*   ELSE                                                           0362 00565000
*     DO;                           /* IF NO SDWA R2 POINTS TO PARMS */ 00566000
