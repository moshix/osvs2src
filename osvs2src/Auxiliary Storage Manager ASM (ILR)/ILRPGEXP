*   OPS00=R0;                       /* SAVE ILROPS00'S ADDRESS       */ 00081000
         ST    R0,OPS00                                            0198 00082000
*   RFY                                                            0199 00083000
*    (R0,                                                          0199 00084000
*     R15) UNRSTD;                                                 0199 00085000
*   WORKAPTR=R12;                   /* PUT DATA REG IN ESTAE PLIST   */ 00086000
         ST    R12,WORKAPTR                                        0200 00087000
*   RFY                                                            0201 00088000
*    (R0,                                                          0201 00089000
*     R2,                                                          0201 00090000
*     R3) RSTD;                                                    0201 00091000
*   ESTAELST=''B;                   /* ZERO ESTAE MACRO LIST FORM    */ 00092000
         XC    ESTAELST(16),ESTAELST                               0202 00093000
*   R2=ADDR(ESTAER);                /* OBTAIN ADDRESS OF ESTAE RTN   */ 00094000
         LA    R2,ESTAER                                           0203 00095000
*   R3=ADDR(ESTAEPL);               /* OBTAIN RETURN PARM LIST ADDR  */ 00096000
         LA    R3,ESTAEPL                                          0204 00097000
*   GEN REFS(ESTAELST,R2,R3);                                      0205 00098000
        ESTAE (R2),CT,PARAM=(R3),TERM=YES,PURGE=NONE,RECORD=YES,       X00099000
               MF=(E,ESTAELST)                                          00100000
*   RFY                                                            0206 00101000
*    (R0,                                                          0206 00102000
*     R1,                                                          0206 00103000
*     R2,                                                          0206 00104000
*     R3) UNRSTD;                                                  0206 00105000
*                                                                  0206 00106000
*   /*****************************************************************/ 00107000
*   /*                                                               */ 00108000
*   /* PAGE FIX MODULE - NEEDED TO OBTAIN SALLOC FOR SERIALIZATION OF*/ 00109000
*   /* CONTROL BLOCKS                                                */ 00110000
*   /*                                                               */ 00111000
*   /*****************************************************************/ 00112000
*                                                                  0207 00113000
*   RFY                                                            0207 00114000
*    (R0,                                                          0207 00115000
*     R1,                                                          0207 00116000
*     R2,                                                          0207 00117000
*     R15) RSTD;                                                   0207 00118000
*   R1=ADDR(ILRPGEXP);              /* BEGINNING ADDR OF MODULE      */ 00119000
         L     R1,@CA00028                                         0208 00120000
*   R15=ADDR(MODEND);               /* END ADDR OF MODULE            */ 00121000
         LA    R15,MODEND                                          0209 00122000
*   PGECB=''B;                      /* CLEAR ECB ADDRESS             */ 00123000
         SLR   @14,@14                                             0210 00124000
         ST    @14,PGECB                                           0210 00125000
*   R2=ADDR(PGECB);                 /* GET ADDR OF ECB               */ 00126000
         LA    R2,PGECB                                            0211 00127000
*   GEN;                                                           0212 00128000
        PGFIX R,A=(1),ECB=(2),EA=(15) PAGE FIX MODULE TO GET LOCK       00129000
        WAIT ECB=PGECB               WAIT FOR PAGE FIX TO COMPLETE      00130000
*   RFY                                                            0213 00131000
*    (R0,                                                          0213 00132000
*     R1,                                                          0213 00133000
*     R2,                                                          0213 00134000
*     R15) UNRSTD;                                                 0213 00135000
*                                                                  0213 00136000
*   /*****************************************************************/ 00137000
*   /*                                                               */ 00138000
*   /* DETERMINE IF PAGE OR SWAP- CHECK FOR ZERO DSN LIST ADDRESS    */ 00139000
*   /*                                                               */ 00140000
*   /*****************************************************************/ 00141000
*                                                                  0214 00142000
*   MSGLIST(1:LENGTH(MSGLIST))=WTOLMAP;/* INIT MLWTO BUFFER          */ 00143000
         MVC   MSGLIST(194),WTOLMAP                                0214 00144000
*   PGCMDPTR=ADDR(CHBUF);           /* OBTAIN ADDR OF COMMAND INPUT  */ 00145000
         L     @01,CSCBPTR                                         0215 00146000
         LA    @15,CHBUF(,@01)                                     0215 00147000
         ST    @15,PGCMDPTR                                        0215 00148000
*   IF PGCMD(1:LENGTH(SWAPEQ))=SWAPEQ THEN                         0216 00149000
         CLC   PGCMD(5,@15),@CC02440                               0216 00150000
         BNE   @RF00216                                            0216 00151000
*     DO;                           /* IF SWAP COMMAND               */ 00152000
*       PGCMDPTR=PGCMDPTR+LENGTH(SWAPEQ);/* POINT TO DATA SET NAMES  */ 00153000
         AH    @15,@CH01000                                        0218 00154000
         ST    @15,PGCMDPTR                                        0218 00155000
*       TYPE=' SWAP ';              /* INIT TYPE TO SWAP             */ 00156000
         MVC   TYPE(6),@CC02526                                    0219 00157000
*       SARTPTR=ASMSART;            /* INITIALIZE SART ADDRESS       */ 00158000
         L     @01,CVTPTR                                          0220 00159000
         L     @01,ASMVTPTR(,@01)                                  0220 00160000
         L     @15,ASMSART(,@01)                                   0220 00161000
         ST    @15,SARTPTR                                         0220 00162000
*       PGDSNNUM=SARUSE;            /* OBTAIN NUMBER OF SARTES IN USE*/ 00163000
         L     PGDSNNUM,SARUSE(,@15)                               0221 00164000
*       DSNLPTR=SARDSNL;            /* GET DSNAME LIST ADDRESS       */ 00165000
         L     DSNLPTR,SARDSNL(,@15)                               0222 00166000
*     END;                          /* END SWAP DATA SET             */ 00167000
*   ELSE                                                           0224 00168000
*     DO;                           /* FOR PAGE DATA SET             */ 00169000
         B     @RC00216                                            0224 00170000
@RF00216 DS    0H                                                  0225 00171000
*       PGPAGE=ON;                  /* INDICATE PAGE DATA SET        */ 00172000
         OI    PGPAGE,B'00001000'                                  0225 00173000
*       PARTPTR=ASMPART;            /* INITIALIZE PART ADDRESS       */ 00174000
         L     @01,CVTPTR                                          0226 00175000
         L     @01,ASMVTPTR(,@01)                                  0226 00176000
         L     @15,ASMPART(,@01)                                   0226 00177000
         ST    @15,PARTPTR                                         0226 00178000
*       TYPE=' PAGE ';              /* SET TYPE TO PAGE              */ 00179000
         MVC   TYPE(6),@CC02527                                    0227 00180000
*       PGDSNNUM=PARTEUSE;          /* GET NUMBER OF PARTES IN USE   */ 00181000
         L     PGDSNNUM,PARTEUSE(,@15)                             0228 00182000
*       DSNLPTR=PARTDSNL;           /* GET ADDR OF DSNAME LIST       */ 00183000
         L     DSNLPTR,PARTDSNL(,@15)                              0229 00184000
*     END;                          /* END PAGE DATA SET             */ 00185000
*   IF DSNLPTR=0 THEN                                              0231 00186000
@RC00216 LTR   DSNLPTR,DSNLPTR                                     0231 00187000
         BNZ   @RF00231                                            0231 00188000
*     DO;                           /* IF DSNLIST NOT BUILT FAIL CMD */ 00189000
*       PGFC=ON;                    /* INDICATE FAIL COMMAND         */ 00190000
*       PGNVAR=ON;                  /* INDICATE NON-VARIABLE MSG     */ 00191000
         OI    PGFC,B'11000000'                                    0234 00192000
*       CALL MSG;                   /* PUT OUT MSG IEE781I           */ 00193000
         BAL   @14,MSG                                             0235 00194000
*     END;                          /* END NO DSNAME LIST            */ 00195000
*   ELSE                                                           0237 00196000
*     DO;                           /* IF DSNLIST EXISTS             */ 00197000
         B     @RC00231                                            0237 00198000
@RF00231 DS    0H                                                  0238 00199000
*       TABLE=''B;                  /* ZERO TRANSLATE TABLE          */ 00200000
         XC    TABLE(256),TABLE                                    0238 00201000
*       TABLE2(' '+1)=BLANK;        /* FILL IN BLANK SPACE IN TABLE  */ 00202000
         MVI   TABLE2+64,B'00000001'                               0239 00203000
*       TABLE2(','+1)=COMMA;        /* FILL IN COMMA SPACE IN TABLE  */ 00204000
         MVI   TABLE2+107,B'00000010'                              0240 00205000
*                                                                  0241 00206000
*       /*************************************************************/ 00207000
*       /*                                                           */ 00208000
*       /* CHECK FOR CORRECT LENGTH OF DATA SET NAMES                */ 00209000
*       /*                                                           */ 00210000
*       /*************************************************************/ 00211000
*                                                                  0241 00212000
*       PGDSNPTR=PGCMDPTR;          /* SAVE COMMAND ADDRESS          */ 00213000
         L     PGDSNPTR,PGCMDPTR                                   0241 00214000
*       DO WHILE PGFC=OFF&PGEND=OFF;/* SYNTAX CHECK COMMAND INPUT    */ 00215000
         B     @DE00242                                            0242 00216000
@DL00242 DS    0H                                                  0243 00217000
*         RFY                                                      0243 00218000
*          (R1,                                                    0243 00219000
*           R2) RSTD;                                              0243 00220000
*         R2=BLANK;                 /* INIT ARGUMENT REGISTER        */ 00221000
         LA    R2,1                                                0244 00222000
*         TRT(PGCMD,TABLE);         /* SCAN DSNAME                   */ 00223000
         L     @03,PGCMDPTR                                        0245 00224000
         TRT   PGCMD(124,@03),TABLE                                0245 00225000
*         BC(5,NOEND);              /* IF NOT END OF INPUT CONTINUE  */ 00226000
         BC    5,NOEND                                             0246 00227000
*         PGEND=ON;                 /* END OF INPUT                  */ 00228000
         OI    PGEND,B'00001000'                                   0247 00229000
*NOEND:                                                            0248 00230000
*         IF R2=BLANK THEN                                         0248 00231000
NOEND    CH    R2,@CH02453                                         0248 00232000
         BNE   @RF00248                                            0248 00233000
*           PGEND=ON;               /* LAST DSN INDICATE END         */ 00234000
         OI    PGEND,B'00001000'                                   0249 00235000
*         RFY                                                      0250 00236000
*           R2 UNRSTD;                                             0250 00237000
@RF00248 DS    0H                                                  0251 00238000
*         PGCMDLEN=R1-PGCMDPTR;     /* GET LENGTH OF DSNAME          */ 00239000
         LR    PGCMDLEN,R1                                         0251 00240000
         SL    PGCMDLEN,PGCMDPTR                                   0251 00241000
*         RFY                                                      0252 00242000
*           R1 UNRSTD;                                             0252 00243000
*         IF PGCMDLEN=0|            /* COMMAND LENGTH (DSNAME) IS    */ 00244000
*             PGCMDLEN>44 THEN                                     0253 00245000
         LTR   PGCMDLEN,PGCMDLEN                                   0253 00246000
         BZ    @RT00253                                            0253 00247000
         CH    PGCMDLEN,@CH01442                                   0253 00248000
         BNH   @RF00253                                            0253 00249000
@RT00253 DS    0H                                                  0254 00250000
*           DO;                     /* GREATER THAN 44               */ 00251000
*             IF PGCMDLEN=0 THEN                                   0255 00252000
         LTR   PGCMDLEN,PGCMDLEN                                   0255 00253000
         BNZ   @RF00255                                            0255 00254000
*               DO;                 /* SET UP MSG WITH ZERO LENGTH   */ 00255000
*                 MSG2(1:LENGTH(TYPE)-1)=TYPE(2:6);                0257 00256000
         MVC   MSG2(5),TYPE+1                                      0257 00257000
*                 MSG2(LENGTH(TYPE):LENGTH(TYPE)+LENGTH(DATASET)-1)=    00258000
*                     DATASET;                                     0258 00259000
         MVC   MSG2+5(8),@CC02433                                  0258 00260000
*               END;                /* END ZERO LENGTH DSNAME        */ 00261000
*             ELSE                                                 0260 00262000
*               DO;                 /* SET UP MSG WITH LENGTH 44     */ 00263000
         B     @RC00255                                            0260 00264000
@RF00255 DS    0H                                                  0261 00265000
*                 MSG2(1:DSNL)=PGCMD;                              0261 00266000
         L     @09,PGCMDPTR                                        0261 00267000
         MVC   MSG2(44),PGCMD(@09)                                 0261 00268000
*                 MSG2(DSNL+1:DSNL+LENGTH(TYPE))=TYPE;             0262 00269000
         MVC   MSG2+44(6),TYPE                                     0262 00270000
*                 MSG2(DSNL+1+LENGTH(TYPE):DSNL+LENGTH(TYPE)+LENGTH(    00271000
*                     DATASET))=DATASET;                           0263 00272000
         MVC   MSG2+50(8),@CC02433                                 0263 00273000
*               END;                /* END GREATER THAN 44 DSNAME    */ 00274000
*             MSG1(4:LENGTH(M784A)+3)=M784A;                       0265 00275000
@RC00255 MVC   MSG1+3(3),@CC02375                                  0265 00276000
*             MSG3(1:LENGTH(M784B))=M784B;                         0266 00277000
         MVC   MSG3(47),@CC02378                                   0266 00278000
*             PGFC=ON;              /* FAIL COMMAND                  */ 00279000
         OI    PGFC,B'01000000'                                    0267 00280000
*             CALL MSG;             /* ISSUE MSG IEE784I             */ 00281000
         BAL   @14,MSG                                             0268 00282000
*           END;                    /* END WRONG LENGTH DSNAME       */ 00283000
*         ELSE                                                     0270 00284000
*           DO;                     /* DSNAME LENGTH IS OK           */ 00285000
         B     @RC00253                                            0270 00286000
@RF00253 DS    0H                                                  0271 00287000
*             PGCMDPTR=PGCMDPTR+PGCMDLEN+1;/* POINT TO NEXT DSNAME   */ 00288000
         LR    @09,PGCMDLEN                                        0271 00289000
         AL    @09,PGCMDPTR                                        0271 00290000
         AH    @09,@CH00040                                        0271 00291000
         ST    @09,PGCMDPTR                                        0271 00292000
*           END;                    /* END LENGTH OK                 */ 00293000
*       END;                        /* END DO WHILE                  */ 00294000
@RC00253 DS    0H                                                  0273 00295000
@DE00242 TM    PGFC,B'01001000'                                    0273 00296000
         BZ    @DL00242                                            0273 00297000
*       PGCMDPTR=PGDSNPTR;          /* REINIT CMDPTR                 */ 00298000
         ST    PGDSNPTR,PGCMDPTR                                   0274 00299000
*       PGEND=OFF;                  /* TURN OFF END INDICATOR        */ 00300000
         NI    PGEND,B'11110111'                                   0275 00301000
*                                                                  0276 00302000
*       /*************************************************************/ 00303000
*       /*                                                           */ 00304000
*       /* MAIN LOOP TO PROCESS LIST OF DATA SETS- EACH ONE IS       */ 00305000
*       /* PROCESSED INDIVIDUALLY WHETHER OR NOT THE PREVIOUS ONES   */ 00306000
*       /* WERE SUCCESSFUL                                           */ 00307000
*       /*                                                           */ 00308000
*       /*************************************************************/ 00309000
*                                                                  0276 00310000
*       DO WHILE PGFC=OFF&PGEND=OFF;/* PROCESS EACH DATA SET         */ 00311000
         B     @DE00276                                            0276 00312000
@DL00276 DS    0H                                                  0277 00313000
*         RFY                                                      0277 00314000
*          (R1,                                                    0277 00315000
*           R2) RSTD;                                              0277 00316000
*         R2=BLANK;                 /* INIT ARGUMENT REGISTER        */ 00317000
         LA    R2,1                                                0278 00318000
*         TRT(PGCMD,TABLE);         /* OBTAIN DATA SET NAME          */ 00319000
         L     @03,PGCMDPTR                                        0279 00320000
         TRT   PGCMD(124,@03),TABLE                                0279 00321000
*         BC(5,NOTEND);             /* IF CC^= 0 OR 2 CONTINUE       */ 00322000
         BC    5,NOTEND                                            0280 00323000
*         PGEND=ON;                 /* CC=0 OR 2 INDICATES LAST    0281 00324000
*                                      DSNAME                        */ 00325000
         OI    PGEND,B'00001000'                                   0281 00326000
*NOTEND:                                                           0282 00327000
*         IF R2=BLANK THEN                                         0282 00328000
NOTEND   CH    R2,@CH02453                                         0282 00329000
         BNE   @RF00282                                            0282 00330000
*           PGEND=ON;               /* BLANK DELIMITS LAST DATASET 0283 00331000
*                                      NAME                          */ 00332000
         OI    PGEND,B'00001000'                                   0283 00333000
*         RFY                                                      0284 00334000
*           R2 UNRSTD;                                             0284 00335000
@RF00282 DS    0H                                                  0285 00336000
*         PGDSNL=R1-PGCMDPTR;       /* OBTAIN LENGTH OF DSNAME       */ 00337000
         L     @02,PGCMDPTR                                        0285 00338000
         LR    @00,R1                                              0285 00339000
         SLR   @00,@02                                             0285 00340000
         STH   @00,PGDSNL                                          0285 00341000
*         RFY                                                      0286 00342000
*           R1 UNRSTD;                                             0286 00343000
*                                                                  0286 00344000
*         /***********************************************************/ 00345000
*         /*                                                         */ 00346000
*         /* CHECK FOR DUPLICATE DATA SET NAMES                      */ 00347000
*         /*                                                         */ 00348000
*         /***********************************************************/ 00349000
*                                                                  0287 00350000
*         PGDSN=PGCMD(1:PGDSNL);    /* COPY DATA SET NAME            */ 00351000
         MVI   PGDSN+1,C' '                                        0287 00352000
         MVC   PGDSN+2(42),PGDSN+1                                 0287 00353000
         BCTR  @00,0                                               0287 00354000
         LR    @01,@00                                             0287 00355000
         EX    @01,@SM02602                                        0287 00356000
*         DSNLEND=DSNLPTR+DSNL*(PGDSNNUM-1);/* COMPUTE LAST ENTRY  0288 00357000
*                                      ADDR                          */ 00358000
         LR    DSNLEND,PGDSNNUM                                    0288 00359000
         BCTR  DSNLEND,0                                           0288 00360000
         MH    DSNLEND,@CH01442                                    0288 00361000
         ALR   DSNLEND,DSNLPTR                                     0288 00362000
*         DO DSNENTAD=DSNLPTR TO DSNLEND BY DSNL WHILE PGDSN^=DSNLE;    00363000
         LR    @02,DSNLPTR                                         0289 00364000
         ST    @02,DSNENTAD                                        0289 00365000
         B     @DE00289                                            0289 00366000
@DL00289 CLC   PGDSN(44),DSNLE(@02)                                0289 00367000
         BE    @DC00289                                            0289 00368000
*                                   /* LOOP THRU DSNAMES FOR       0290 00369000
*                                      DUPLICATE                     */ 00370000
*         END;                      /* END SEARCH FOR DUPLICATE      */ 00371000
         AH    @02,@CH01442                                        0290 00372000
         ST    @02,DSNENTAD                                        0290 00373000
@DE00289 CR    @02,DSNLEND                                         0290 00374000
         BNH   @DL00289                                            0290 00375000
@DC00289 DS    0H                                                  0291 00376000
*         IF PGDSN=DSNLE THEN                                      0291 00377000
         L     @01,DSNENTAD                                        0291 00378000
         CLC   PGDSN(44),DSNLE(@01)                                0291 00379000
         BNE   @RF00291                                            0291 00380000
*           DO;                     /* IF ALREADY IN USE ISSUE MSG   */ 00381000
*             PGSAME=ON;            /* FILL IN MIDDLE OF MSG         */ 00382000
*             PG782=ON;             /* MSG IEE782I                   */ 00383000
         OI    PGSAME,B'00010010'                                  0294 00384000
*             MSG3(1:LENGTH(M782B))=M782B;/* FILL IN LAST LINE OF MSG*/ 00385000
         MVC   MSG3(24),@CC02347                                   0295 00386000
*             CALL MSG;             /* ISSUE MSG                     */ 00387000
         BAL   @14,MSG                                             0296 00388000
*           END;                    /* END DUPLICATE DSNAME          */ 00389000
*         ELSE                                                     0298 00390000
*           DO;                     /* OK SO FAR CONTINUE            */ 00391000
         B     @RC00291                                            0298 00392000
@RF00291 DS    0H                                                  0299 00393000
*             IF PGPAGE=ON THEN                                    0299 00394000
         TM    PGPAGE,B'00001000'                                  0299 00395000
         BNO   @RF00299                                            0299 00396000
*               DO;                 /* FOR PAGE DATA SET             */ 00397000
*                 CALL PAGERTN;     /* DO PAGE PROCESSING            */ 00398000
         BAL   @14,PAGERTN                                         0301 00399000
*               END;                /* END PAGE PROCESS              */ 00400000
*             ELSE                                                 0303 00401000
*               DO;                 /* FOR SWAP DATA SET             */ 00402000
         B     @RC00299                                            0303 00403000
@RF00299 DS    0H                                                  0304 00404000
*                 CALL SWAPRTN;     /* DO SWAP PROCESSING            */ 00405000
         BAL   @14,SWAPRTN                                         0304 00406000
*               END;                /* END SWAP PROCESS              */ 00407000
*             IF PGFDS=ON|PGFC=ON THEN                             0306 00408000
@RC00299 TM    PGFDS,B'01100000'                                   0306 00409000
         BZ    @RF00306                                            0306 00410000
*               DO;                 /* IF FAILURE OCCURRED           */ 00411000
*                 CALL ERROR;       /* CLEAN UP                      */ 00412000
         BAL   @14,ERROR                                           0308 00413000
*                 PGFDS=OFF;        /* CLEAR BIT INDICATING LAST DATA   00414000
*                                      SET FAILED                    */ 00415000
         NI    PGFDS,B'11011111'                                   0309 00416000
*               END;                /* END FAILURE OCCURRED          */ 00417000
*             ELSE                                                 0311 00418000
*               DO;                 /* DATA SET SUCCESSFUL           */ 00419000
         B     @RC00306                                            0311 00420000
@RF00306 DS    0H                                                  0312 00421000
*                 MSG1(4:LENGTH(M783A)+3)=M783A;/* FILL IN MSG ID    */ 00422000
         MVC   MSG1+3(3),@CC02369                                  0312 00423000
*                 PGSAME=ON;        /* FILL IN MSG MIDDLE            */ 00424000
         OI    PGSAME,B'00010000'                                  0313 00425000
*                 MSG3(1:LENGTH(M783B))=M783B;/* FILL IN MSG END     */ 00426000
         MVC   MSG3(28),@CC02371                                   0314 00427000
*                 CALL MSG;         /* ISSUE MSG IEE783I             */ 00428000
         BAL   @14,MSG                                             0315 00429000
*               END;                /* END SUCCESSFUL DATA SET       */ 00430000
*           END;                    /* END NOT DUPLICATE DATA SET    */ 00431000
@RC00306 DS    0H                                                  0318 00432000
*         PGCMDPTR=PGCMDPTR+PGDSNL+1;/* GET ADDR OF NEXT DATA SET    */ 00433000
@RC00291 LH    @15,PGDSNL                                          0318 00434000
         AL    @15,PGCMDPTR                                        0318 00435000
         AH    @15,@CH00040                                        0318 00436000
         ST    @15,PGCMDPTR                                        0318 00437000
*       END;                        /* END DO WHILE                  */ 00438000
@DE00276 TM    PGFC,B'01001000'                                    0319 00439000
         BZ    @DL00276                                            0319 00440000
*     END;                          /* END NO DSNLIST                */ 00441000
*                                                                  0320 00442000
*   /*****************************************************************/ 00443000
*   /*                                                               */ 00444000
*   /* FINISHED WITH LIST OF NAMES- CLEAN UP AND EXIT                */ 00445000
*   /*                                                               */ 00446000
*   /*****************************************************************/ 00447000
*                                                                  0321 00448000
*   IF PGGTPART=ON THEN                                            0321 00449000
@RC00231 TM    PGGTPART,B'00000001'                                0321 00450000
         BNO   @RF00321                                            0321 00451000
*     DO;                           /* TPARTBLE WORKAREA GOTTEN      */ 00452000
*       RFY                                                        0323 00453000
*        (R0,                                                      0323 00454000
*         R1,                                                      0323 00455000
*         R15) RSTD;                                               0323 00456000
