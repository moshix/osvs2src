         LA    @08,PARTENTS(,@08)                                  0447 00816000
         ST    @08,PGPARTAD                                        0447 00817000
*               PGRIP=ON;           /* INDICATE READ IN PROGRESS     */ 00818000
         OI    PGRIP,B'10000000'                                   0448 00819000
*               CALL ILRPREAD;      /* CALL ILRPREAD TO READ TPARTBLE*/ 00820000
         L     @15,PREAD                                           0449 00821000
         BALR  @14,@15                                             0449 00822000
*               RFY                                                0450 00823000
*                 R1 UNRSTD;                                       0450 00824000
*               RFY                                                0451 00825000
*                 R15 RSTD;                                        0451 00826000
