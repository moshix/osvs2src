*     END;                          /* END READ OR WRITE IN PROGRESS */ 00615000
*                                                                  0382 00616000
*   /*****************************************************************/ 00617000
*   /*                                                               */ 00618000
*   /* VALIDITY CHECK THE ADDRESSES OF ALL CONTROL BLOCKS TO BE FREED*/ 00619000
*   /* AND CLEARED BY THE ERROR SUBROUTINE                           */ 00620000
*   /*                                                               */ 00621000
*   /*****************************************************************/ 00622000
*                                                                  0383 00623000
*   DO PGINCR3=1 TO DIM(ESTAEPTR);  /* LOOP THRU PTRS TO VERIFY ADDRS*/ 00624000
@RF00367 LA    PGINCR3,1                                           0383 00625000
@DL00383 DS    0H                                                  0384 00626000
*     IF ESTAEPTR(PGINCR3)^=0 THEN                                 0384 00627000
         LR    @05,PGINCR3                                         0384 00628000
         SLA   @05,2                                               0384 00629000
         L     @05,ESTAEPTR-4(@05)                                 0384 00630000
         LTR   @05,@05                                             0384 00631000
