@EL00002 DS    0H                                                  0404 00686000
@EF00002 DS    0H                                                  0404 00687000
@ER00002 BR    @14                                                 0404 00688000
*   END ESTAER;                                                    0405 00689000
*PAGERTN:                                                          0406 00690000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0406 00691000
*                                                                  0406 00692000
PAGERTN  DS    0H                                                  0407 00693000
*/********************************************************************/ 00694000
*/*                                                                  */ 00695000
*/* PAGE: BUILD THE CONTROL BLOCKS NECESSARY FOR A NEW PAGE DATA SET */ 00696000
*/*       AND UPDATE EXISTING CONTROL BLOCKS                         */ 00697000
*/*                                                                  */ 00698000
*/********************************************************************/ 00699000
*                                                                  0407 00700000
*   RFY                                                            0407 00701000
*     R14 RSTD;                                                    0407 00702000
*   PAGERTRN=R14;                   /* SAVE RETURN ADDRESS           */ 00703000
         LR    PAGERTRN,R14                                        0408 00704000
*   RFY                                                            0409 00705000
*     R14 UNRSTD;                                                  0409 00706000
*   IF PARTSIZE<=PARTEUSE THEN                                     0410 00707000
         L     @08,PARTPTR                                         0410 00708000
         L     @15,PARTSIZE(,@08)                                  0410 00709000
         C     @15,PARTEUSE(,@08)                                  0410 00710000
         BH    @RF00410                                            0410 00711000
*     DO;                           /* IF NO MORE ROOM IN THE PART   */ 00712000
*       PGFC=ON;                    /* INDICATE FAIL COMMAND         */ 00713000
*       PGNOMORE=ON;                /* PUT OUT MSG IEE785I           */ 00714000
         OI    PGFC,B'01000100'                                    0413 00715000
*       CALL MSG;                   /* CALL MESSAGE RTN              */ 00716000
         BAL   @14,MSG                                             0414 00717000
*     END;                          /* END NO ROOM IN PART           */ 00718000
*   ELSE                                                           0416 00719000
*     DO;                           /* ROOM IN THE PART              */ 00720000
