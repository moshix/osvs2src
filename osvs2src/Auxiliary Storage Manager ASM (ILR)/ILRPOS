         NI    WORKPEND(ASMVTPTR),B'01111111'                      0206 00572000
*       END;                                                       0207 00573000
*   END;                            /* ENDS LOOP THRU CHAIN OF AIAS  */ 00574000
@RF00203 DS    0H                                                  0208 00575000
@DE00063 SLR   @12,@12                                             0208 00576000
         CR    AIAPTR,@12                                          0208 00577000
         BNE   @DL00063                                            0208 00578000
*   RFY                                                            0209 00579000
*     LGVTPTR UNRSTD;                                              0209 00580000
*   RTNCODE=CODE00;                 /* OPERATIONS COMPLETED        0210 00581000
*                                      SUCCESSFULLY                  */ 00582000
