         EX    @15,@SX01851                                        0564 01988000
*           /*********************************************************/ 01989000
*           /*                                                       */ 01990000
*           /* THE ASST EXTENSION IS NOW INTIALIZED                  */ 01991000
*           /*                                                       */ 01992000
*           /*********************************************************/ 01993000
*                                                                  0565 01994000
*           RFY                                                    0565 01995000
*            (R1AREA,                                              0565 01996000
*             R0) UNRSTD;                                          0565 01997000
@RF00563 DS    0H                                                  0566 01998000
*           ASPEIDNT=ASST;          /* SET THE EXT ID                */ 01999000
         MVC   ASPEIDNT(4,ASPEXPTR),@CC01730                       0566 02000000
*           ASPESSYM=ASPSSYM;       /* PROPAGATE THE 'S' SYMBOL      */ 02001000
         MVC   ASPESSYM(8,ASPEXPTR),ASPSSYM(ASPCTPTR)              0567 02002000
*           ASPASSTN=ASPAEXCT+ONE;  /* SET THE ASST EXT. INDEX       */ 02003000
         LA    @15,1                                               0568 02004000
         LH    @14,ASPAEXCT(,ASPCTPTR)                             0568 02005000
         LR    @01,@14                                             0568 02006000
         ALR   @01,@15                                             0568 02007000
         STC   @01,ASPASSTN(,ASPEXPTR)                             0568 02008000
*           IF ASPAEXCT=HLFZERO THEN                               0569 02009000
         LTR   @14,@14                                             0569 02010000
         BNZ   @RF00569                                            0569 02011000
*             ASPEXTNM=ONE;         /* IF NO PREVIOUS EXT            */ 02012000
         ST    @15,ASPEXTNM(,ASPEXPTR)                             0570 02013000
*           ELSE                                                   0571 02014000
*             ASPEXTNM=LASTLPME->ASPEXTNM+ONE;/* PREVIOUS EXT, INCR     02015000
*                                      LAST EXT NUMBER               */ 02016000
         B     @RC00569                                            0571 02017000
@RF00569 LA    @15,1                                               0571 02018000
         AL    @15,ASPEXTNM(,LASTLPME)                             0571 02019000
         ST    @15,ASPEXTNM(,ASPEXPTR)                             0571 02020000
*           ASPAEXCT=ASPAEXCT+ONE;  /* SET THE ASST EXT. COUNTER IN     02021000
*                                      BASE ASPCT                    */ 02022000
@RC00569 LA    @15,1                                               0572 02023000
         AH    @15,ASPAEXCT(,ASPCTPTR)                             0572 02024000
         STH   @15,ASPAEXCT(,ASPCTPTR)                             0572 02025000
*           ASPASSTP(ASPAEXCT)=ASPEXPTR;/* SET EXT. POINTER IN BASE     02026000
*                                      ASPCT                         */ 02027000
         SLA   @15,2                                               0573 02028000
         ST    ASPEXPTR,ASPASSTP-4(@15,ASPCTPTR)                   0573 02029000
*           LASTASST=ASPEXPTR;      /* LAST ASST EXT IS NEW ONE JUST    02030000
*                                      BUILT                         */ 02031000
         LR    LASTASST,ASPEXPTR                                   0574 02032000
*           ASPEXPTR=ASPEXPTR+LENGTH(ASPEXTSN);/* INCR ASST EXT ADDR    02033000
*                                      BY LENGTH SINGLE EXT, OBTAINS    02034000
*                                      LPME EXT ADDR.                */ 02035000
         AL    ASPEXPTR,@CF01834                                   0575 02036000
*         END;                      /* END PROCESSING WHERE BOTH EXT.   02037000
*                                      SUCCESSFULLY OBTAINED         */ 02038000
*     END;                          /* END PROCESSING FOR BOTH EXT.     02039000
*                                      NEEDED                        */ 02040000
@RF00552 DS    0H                                                  0578 02041000
*                                                                  0578 02042000
*   /*****************************************************************/ 02043000
*   /*                                                               */ 02044000
*   /* THE FOLLOWING INTITALIZES THE LPME EXT. IF IT WAS SUCCESSFULLY*/ 02045000
*   /* OBTAINED AND RETURNS TO THE CALLER.                           */ 02046000
*   /*                                                               */ 02047000
*   /*****************************************************************/ 02048000
*                                                                  0578 02049000
*   IF RTNCODE=CODE00 THEN                                         0578 02050000
@RF00537 L     @15,RTNCODE(,ASMVTPTR)                              0578 02051000
         LTR   @15,@15                                             0578 02052000
         BNZ   @RF00578                                            0578 02053000
*     DO;                                                          0579 02054000
*       ASPEIDNT=LPME;              /* SET THE EXT ID                */ 02055000
         MVC   ASPEIDNT(4,ASPEXPTR),@CC01732                       0580 02056000
*       ASPESSYM=ASPSSYM;           /* PROPAGATE THE 'S' SYMBOL      */ 02057000
         MVC   ASPESSYM(8,ASPEXPTR),ASPSSYM(ASPCTPTR)              0581 02058000
*       ASPASSTN=ASPAEXCT;          /* SET THE ASST EXT. INDEX       */ 02059000
         LH    @15,ASPAEXCT(,ASPCTPTR)                             0582 02060000
         STC   @15,ASPASSTN(,ASPEXPTR)                             0582 02061000
*       ASPASSTI=((ASPLEXCT+ONE)//ASPNLPME);/* SET THE LPME EXT.   0583 02062000
*                                      INDEX                         */ 02063000
         LA    @15,1                                               0583 02064000
         LH    @00,ASPLEXCT(,ASPCTPTR)                             0583 02065000
         ALR   @00,@15                                             0583 02066000
         SRDA  @00,32                                              0583 02067000
         D     @00,@CF00518                                        0583 02068000
         STH   @00,ASPASSTI(,ASPEXPTR)                             0583 02069000
*       IF EXPBIT=ON THEN                                          0584 02070000
         TM    EXPBIT(ASMVTPTR),B'01000000'                        0584 02071000
         BNO   @RF00584                                            0584 02072000
*         ASPEXTNM=LASTASST->ASPEXTNM+ONE;/* EXT NUM IS INCR BY EXT     02073000
*                                      NUM OF ASST EXT JUST BUILT IF    02074000
*                                      BOTH EXTS WERE NEEDED         */ 02075000
         AL    @15,ASPEXTNM(,LASTASST)                             0585 02076000
         ST    @15,ASPEXTNM(,ASPEXPTR)                             0585 02077000
*       ELSE                                                       0586 02078000
*         ASPEXTNM=LASTLPME->ASPEXTNM+ONE;/* ONLY LPME EXT NEEDED, 0586 02079000
*                                      INCR LAST LPME EXT NUM        */ 02080000
         B     @RC00584                                            0586 02081000
@RF00584 LA    @15,1                                               0586 02082000
         AL    @15,ASPEXTNM(,LASTLPME)                             0586 02083000
         ST    @15,ASPEXTNM(,ASPEXPTR)                             0586 02084000
*       ASPLEXCT=ASPLEXCT+ONE;      /* INCR LPME EXT CNT             */ 02085000
@RC00584 LA    @15,1                                               0587 02086000
         AH    @15,ASPLEXCT(,ASPCTPTR)                             0587 02087000
         STH   @15,ASPLEXCT(,ASPCTPTR)                             0587 02088000
*       LASTASST->ASPASSTE(ASPASSTI+ONE)=ASPEXPTR;/* STORE THE LPME     02089000
*                                      EXT PTR IN THE ASST EXT       */ 02090000
         LH    @15,ASPASSTI(,ASPEXPTR)                             0588 02091000
         SLA   @15,2                                               0588 02092000
         ST    ASPEXPTR,ASPASSTE(@15,LASTASST)                     0588 02093000
*       RFY                                                        0589 02094000
*        (ASPEXPTR,                                                0589 02095000
*         LASTASST,                                                0589 02096000
*         LASTLPME) UNRSTD;                                        0589 02097000
*       RTNCODE=CODE00;             /* OPERATIONS SUCCESSFUL         */ 02098000
         SLR   @12,@12                                             0590 02099000
         ST    @12,RTNCODE(,ASMVTPTR)                              0590 02100000
*       RFY                                                        0591 02101000
*         R14 RSTD;                                                0591 02102000
*     END;                          /* END INITIALIZATION OF LPME  0592 02103000
*                                      EXT.                          */ 02104000
*   R14=R8;                         /* RESTORE RETURN ADDR           */ 02105000
@RF00578 LR    R14,R8                                              0593 02106000
*   RFY                                                            0594 02107000
*     R8 UNRSTD;                                                   0594 02108000
*   END EXPAND;                                                    0595 02109000
@EL00004 DS    0H                                                  0595 02110000
@EF00004 DS    0H                                                  0595 02111000
@ER00004 BR    @14                                                 0595 02112000
@PB00004 DS    0H                                                  0595 02113000
