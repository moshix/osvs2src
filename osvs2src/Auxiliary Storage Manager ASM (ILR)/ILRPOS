         AH    @15,ASCBVSC(,ASCBPTR)                               0345 01200000
         STH   @15,ASCBVSC(,ASCBPTR)                               0345 01201000
*           RFY                                                    0346 01202000
*             ASCBPTR UNRSTD;                                      0346 01203000
*           R0=ASMVSC;              /* GET ASMVT VIO COUNT           */ 01204000
         L     R0,ASMVSC(,ASMVTPTR)                                0347 01205000
*RVIOINCR:                                                         0348 01206000
*           R1=R0+ONE;              /* INCR COUNT                    */ 01207000
RVIOINCR LA    R1,1                                                0348 01208000
         ALR   R1,R0                                               0348 01209000
*           CS(R0,R1,ASMVSC);       /* INCR VIO COUNT FIELD IN ASMVT */ 01210000
         CS    R0,@01,ASMVSC(ASMVTPTR)                             0349 01211000
*           BC(NE,RVIOINCR);        /* NOT SUCCESSFUL, TRY AGAIN     */ 01212000
         BC    7,RVIOINCR                                          0350 01213000
*           RFY                                                    0351 01214000
*            (R0,                                                  0351 01215000
*             R1) UNRSTD;                                          0351 01216000
*         END;                      /* END INVALID LSID PROCESSING   */ 01217000
*       ELSE                                                       0353 01218000
*         DO;                       /* VALID LSID                    */ 01219000
         B     @RC00341                                            0353 01220000
@RF00341 DS    0H                                                  0354 01221000
*           IF ASPLSAVE=ON THEN                                    0354 01222000
         TM    ASPLSAVE(LPMEPTR),B'01000000'                       0354 01223000
         BNO   @RF00354                                            0354 01224000
*             DO;                   /* LSID HAS ALREADY BEEN SAVED   */ 01225000
*               ASPLSVRP=ON;        /* SET RELEASE AFTER SAVE FLAG IN   01226000
*                                      LPME                          */ 01227000
         OI    ASPLSVRP(LPMEPTR),B'00001000'                       0356 01228000
*               ASPSAVRP=ON;        /* SET RELEASE AFTER SAVE FLAG IN   01229000
*                                      ASPCT                         */ 01230000
         OI    ASPSAVRP(ASPCTPTR),B'01000000'                      0357 01231000
*               ASPLSAVE=OFF;       /* RESET LSID ALREADY SAVED FLAG */ 01232000
         NI    ASPLSAVE(LPMEPTR),B'10111111'                       0358 01233000
*               RFY                                                0359 01234000
*                (R0,                                              0359 01235000
*                 R1,                                              0359 01236000
*                 ASCBPTR) RSTD;                                   0359 01237000
*               ASCBPTR=ASPASCB;    /* OBTAIN ASCB OWNING THIS LG    */ 01238000
         L     ASCBPTR,ASPASCB(,ASPCTPTR)                          0360 01239000
*               ASCBVSC=ASCBVSC+ONE;/* INCR ASCB VIO COUNT           */ 01240000
         LA    @15,1                                               0361 01241000
         AH    @15,ASCBVSC(,ASCBPTR)                               0361 01242000
         STH   @15,ASCBVSC(,ASCBPTR)                               0361 01243000
*               RFY                                                0362 01244000
*                 ASCBPTR UNRSTD;                                  0362 01245000
*               R0=ASMVSC;          /* GET ASMVT VIO COUNT           */ 01246000
         L     R0,ASMVSC(,ASMVTPTR)                                0363 01247000
*WVIOINCR:                                                         0364 01248000
*               R1=R0+ONE;          /* INCR COUNT                    */ 01249000
WVIOINCR LA    R1,1                                                0364 01250000
         ALR   R1,R0                                               0364 01251000
*               CS(R0,R1,ASMVSC);   /* INCR VIO COUNT FIELD IN ASMVT */ 01252000
         CS    R0,@01,ASMVSC(ASMVTPTR)                             0365 01253000
*               BC(NE,WVIOINCR);    /* NOT SUCCESSFUL, TRY AGAIN     */ 01254000
         BC    7,WVIOINCR                                          0366 01255000
*               RFY                                                0367 01256000
*                (R0,                                              0367 01257000
*                 R1) UNRSTD;                                      0367 01258000
*             END;                  /* END SAVED LSID PROCESSING     */ 01259000
*           ELSE                                                   0369 01260000
*             IF ASPLIOER=OFF THEN                                 0369 01261000
         B     @RC00354                                            0369 01262000
@RF00354 TM    ASPLIOER(LPMEPTR),B'00010000'                       0369 01263000
         BNZ   @RF00369                                            0369 01264000
*               DO;                 /* NO READ I/O ERROR             */ 01265000
*                 RFY                                              0371 01266000
*                  (R11,                                           0371 01267000
*                   R1,                                            0371 01268000
*                   R12) RSTD;                                     0371 01269000
*                 R11=R14;          /* SAVE RETURN ADDR              */ 01270000
         LR    R11,R14                                             0372 01271000
*                 R12=R13;          /* MAINTAIN R13, REPRESENTS    0373 01272000
*                                      LGVTPTR, R13 NEEDED FOR S/A   */ 01273000
         LR    R12,R13                                             0373 01274000
*                 R1=ASPVLSID;      /* OBTAIN LSID FOR FREE SLOT     */ 01275000
         L     R1,ASPVLSID-1(,LPMEPTR)                             0374 01276000
         LA    R1,0(,R1)                                           0374 01277000
*                 R13=ADDR(ASMSAVE);/* ADDR SAVE AREA FOR FREE SLOT  */ 01278000
         LA    R13,ASMSAVE(,ASMVTPTR)                              0375 01279000
*                 CALL ILRFRSL1;    /* FREES SLOT IDENTIFIED IN R1   */ 01280000
         L     @15,ASMPFRSL(,ASMVTPTR)                             0376 01281000
         BALR  @14,@15                                             0376 01282000
*                 R13=R12;          /* RESTORE LGVTPTR               */ 01283000
         LR    R13,R12                                             0377 01284000
*                 R14=R11;          /* RESTORE RETURN ADDR           */ 01285000
         LR    R14,R11                                             0378 01286000
*                 RFY                                              0379 01287000
*                  (R11,                                           0379 01288000
*                   R12,                                           0379 01289000
*                   R1) UNRSTD;                                    0379 01290000
*               END;                /* END NO I/O ERROR DO           */ 01291000
*           ASPVLSID=ASPVLSID&&ASPVLSID;/* CLEAR THE LSID            */ 01292000
@RF00369 DS    0H                                                  0381 01293000
@RC00354 XC    ASPVLSID(3,LPMEPTR),ASPVLSID(LPMEPTR)               0381 01294000
*           ASPLVALD=OFF;           /* RESET VALID FLAG              */ 01295000
         NI    ASPLVALD(LPMEPTR),B'01111111'                       0382 01296000
*         END;                      /* END VALID LSID PROCESSING     */ 01297000
*       AIALSID=ZERO;               /* CLEAR LSID IN THE AIA         */ 01298000
@RC00341 SLR   @15,@15                                             0384 01299000
         ST    @15,AIALSID(,AIAPTR)                                0384 01300000
*       ASPLOPIN=ON;                /* SET PROCESS IN OPERATION FLAG */ 01301000
         OI    ASPLOPIN(LPMEPTR),B'00100000'                       0385 01302000
*       RTNCODE=CODE00;             /* PROCESSING SUCCESSFUL         */ 01303000
         ST    @15,RTNCODE(,ASMVTPTR)                              0386 01304000
*     END;                          /* END WRITE REQUEST PROCESSING  */ 01305000
*   END LPMEPROC;                                                  0388 01306000
@EL00003 DS    0H                                                  0388 01307000
@EF00003 DS    0H                                                  0388 01308000
@ER00003 BR    @14                                                 0388 01309000
*ILRTRPAG:                                                         0389 01310000
*   ENTRY;                                                         0389 01311000
*                                                                  0389 01312000
@EC00389 DS    0H                                                  0390 01313000
*/********************************************************************/ 01314000
*/*                                                                  */ 01315000
*/*  ILRTRPAG IS CALLED BY RSM TO PROCESS A TRANSFER PAGE REQUEST.   */ 01316000
*/*  AN ACE IS OBTAINED AND INFORMATION IS MOVED FROM THE ACA TO THE */ 01317000
*/*  ACE. THE MAINLINE OF POS IS THEN CALLED TO PROCESS THE TRANSFER */ 01318000
*/*  PAGE ACE.                                                       */ 01319000
*/*                                                                  */ 01320000
*/*  INPUT: R1 - ACAPTR                                              */ 01321000
*/*         R13 - CALLER'S SAVE AREA ADDR                            */ 01322000
*/*         R14 - RETURN ADDR                                        */ 01323000
*/*                                                                  */ 01324000
*/*  OUTPUT: A TRANSFER PAGE ACE IS PROCESSED TO COMPLETION.         */ 01325000
*/*                                                                  */ 01326000
*/********************************************************************/ 01327000
*/********************************************************************/ 01328000
*/*TRPGACE: CHART (AREF)                                             */ 01329000
*/*        HEADER                                                  0390 01330000
*/*        ILRPOS                                                  0390 01331000
*/* ILRTRPAG - OBTAINS AND INITIALIZES A TRANSFER PAGE ACE           */ 01332000
*/*ILRTRPAG: E ENTRY                                                 */ 01333000
*/*        P SAVE CALLER'S REGS                                      */ 01334000
*/*        P ESTABLISH ADDRESSIBILITY                                */ 01335000
*/*        P MAINTAIN THE ACA ADDR (ACAPTR=R1)                       */ 01336000
*/*        L FRR: SET FRR, COVERS TRANSFER PAGE PATH IN POS          */ 01337000
*/*        L ILRGMA: OBTAIN AN ACE                                   */ 01338000
*/*        D (N,,Y,%POS70) RETURN CODE = 0                           */ 01339000
*/*        P (,%POS74) RTN CODE = '28', UNABLE TO OBTAIN STORAGE     */ 01340000
*/*%POS70: P SAVE CELL@ IN ACEPTR AND ZERO THE ACE                   */ 01341000
*/*        P COPY CORRESPONDING FIELDS FROM ACA TO ACE               */ 01342000
*/*        P STORE THE ACE PTR IN THE ATA                            */ 01343000
*/*        D (N,,Y,%POS72) ACE AIA PTR (ACEAIAPT) = 0                */ 01344000
*/*        P SET VIO FLAG IN AIA (AIAVIO)                            */ 01345000
*/*        P SET SPECIAL TRANSFER PAGE FLAG IN AIA (AIATRPSP)        */ 01346000
*/*        P TRANSFER PAGE ACE ADDR IN AIA = ACE ADDR                */ 01347000
*/*        P SET TRANSFER PAGE WAITING FLG IN ACE (ACETRPWT)         */ 01348000
*/*%POS72: S ILRPOS: CALL POS TO PROCESS THE ACE                     */ 01349000
*/*        D (N,,Y,%POS74) RTN CODE = 0                              */ 01350000
*/*        D (Y,,N,%POS73) ACE HAS AN AIA PTR                        */ 01351000
*/*        P RESET AIAVIO AND AIATRPSP FLAGS                         */ 01352000
*/*        P CLEAR THE TRANSFER PAGE ACE ADDR IN THE AIA             */ 01353000
*/*%POS73: P SET THE AIA/ACE ADDR IN ATA = 0                         */ 01354000
*/*        P INITIALIZE CELL@ FOR THE GMA MACRO                      */ 01355000
*/*        L ILRGMA: RETURN ERROR ACE TO ITS POOL                    */ 01356000
*/*%POS74: L FRR: CANCEL RECOVERY FOR POS                            */ 01357000
*/*        P MAINTAIN RETURN CODE                                    */ 01358000
*/*        P RESTORE CALLER'S REGS                                   */ 01359000
*/*        R RETURN                                                  */ 01360000
*/*TRPGACE: END                                                      */ 01361000
*/********************************************************************/ 01362000
*                                                                  0390 01363000
*   RFY                                                            0390 01364000
*    (R1,                                                          0390 01365000
*     R13) RSTD;                                                   0390 01366000
*   GEN REFS(R14,R12,R13,R6BASE);                                  0391 01367000
ILRTRPAG    STM  R14,R12,12(R13)       STORE CALLERS REGS               01368000
TRPSETUP    BALR R6BASE,0              ESTABLISH ADDRESSIBILITY         01369000
            LA   R12,TRPSETUP-POSSETUP SETS CONTENTS OF BASE REG TO     01370000
            SR   R6BASE,R12            THAT OF MAINLINE POS             01371000
*   RFY                                                            0392 01372000
*     ACAPTR RSTD;                                                 0392 01373000
*   ACAPTR=R1;                      /* SAVE THE ACA ADDR (R1) WILL BE   01374000
*                                      DESTROYED BY GMA              */ 01375000
         LR    ACAPTR,R1                                           0393 01376000
*   ASMVTPTR=CVTASMVT;              /* OBTAIN ADDR OF THE ASMVT      */ 01377000
         L     @15,CVTPTR                                          0394 01378000
         L     ASMVTPTR,CVTASMVT(,@15)                             0394 01379000
*   RSMHDPTR=PSAAOLD->ASCBRSM;      /* OBTAIN ADDR RSM HEADER        */ 01380000
         L     @15,PSAAOLD                                         0395 01381000
         L     RSMHDPTR,ASCBRSM(,@15)                              0395 01382000
*   R1=ASMPIOFR;                    /* OBTAIN RECOVERY ROUTINE ADDR  */ 01383000
*                                                                  0396 01384000
         L     R1,ASMPIOFR(,ASMVTPTR)                              0396 01385000
*   /*****************************************************************/ 01386000
*   /*                                                               */ 01387000
*   /* RECOVERY ADDRESS IS ADDED TO COVER THIS ROUTINE AND THE       */ 01388000
*   /* TRANSFER PAGE PATH OF ILRPOS. THE ACE IS THEN OBTAINED.       */ 01389000
*   /*                                                               */ 01390000
*   /*****************************************************************/ 01391000
*                                                                  0397 01392000
*   GEN REFS(R1,R11,R12) SETS(ATAPTR);                             0397 01393000
             SETFRR A,FRRAD=(R1),PARMAD=(ATAPTR),WRKREGS=(R11,R12),   **01394000
               RELATED=(SETFRR,ILRTRPAG(ILRTRPAG))                      01395000
*   ATAMODID=ATAMTRPG;              /* SET MODID IN THE ATA          */ 01396000
         MVI   ATAMODID(ATAPTR),X'04'                              0398 01397000
*   ATATRPAG=ON;                    /* SET TRANSFER PAGE ENTRY FLAG  */ 01398000
*                                                                  0399 01399000
         OI    ATATRPAG(ATAPTR),B'01000000'                        0399 01400000
*   /*****************************************************************/ 01401000
*   /*                                                               */ 01402000
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */ 01403000
*   /*                                                               */ 01404000
*   /*****************************************************************/ 01405000
*                                                                  0400 01406000
*   DO;                             /* ILRGMA GET                  0400 01407000
*                                      CELL(ACE)WORKREGS(R11,R12)    */ 01408000
*     RESPECIFY                                                    0401 01409000
*      (R0,                                                        0401 01410000
*       R1,                                                        0401 01411000
*       R11,                                                       0401 01412000
*       R12,                                                       0401 01413000
*       R14,                                                       0401 01414000
*       R15) RESTRICTED;                                           0401 01415000
*     RESPECIFY                                                    0402 01416000
*       ASMPOOLS BASED(R11);                                       0402 01417000
*     RESPECIFY                                                    0403 01418000
*       GMACELL BASED(R14);                                        0403 01419000
*     R11=ADDR(ASMACEPC);                                          0404 01420000
         LA    R11,ASMACEPC(,ASMVTPTR)                             0404 01421000
*     LM(R14,R15,ASMCPAVQ);         /* GET ADDR OF 1ST CELL          */ 01422000
         LM    R14,R15,ASMCPAVQ(R11)                               0405 01423000
*GMA00020:                                                         0406 01424000
*     IF R14^=0 THEN                /* TEST CELL POOL                */ 01425000
GMA00020 LTR   R14,R14                                             0406 01426000
         BZ    @RF00406                                            0406 01427000
*       DO;                         /* CELL AVAILABLE, REMOVE CELL   */ 01428000
*         R0=GMANEXT;                                              0408 01429000
         L     R0,GMANEXT(,R14)                                    0408 01430000
*         R1=R15-1;                                                0409 01431000
         LR    R1,R15                                              0409 01432000
         BCTR  R1,0                                                0409 01433000
*         CDS(R14,R0,ASMCPAVQ);                                    0410 01434000
         CDS   R14,R0,ASMCPAVQ(R11)                                0410 01435000
*         BC(7,GMA00020);                                          0411 01436000
         BC    7,GMA00020                                          0411 01437000
*         R1=R14;                                                  0412 01438000
         LR    R1,R14                                              0412 01439000
*       END;                                                       0413 01440000
*     ELSE                                                         0414 01441000
*       DO;                         /* CELL POOL EMPTY               */ 01442000
         B     @RC00406                                            0414 01443000
@RF00406 DS    0H                                                  0415 01444000
*         R1=R11;                   /* PREPARE TO CALL PEX           */ 01445000
         LR    R1,R11                                              0415 01446000
*         CALL ILRPEX;              /* EXTEND THE POOL               */ 01447000
         L     @15,ASMPEX(,ASMVTPTR)                               0416 01448000
         BALR  @14,@15                                             0416 01449000
*       END;                                                       0417 01450000
*     RESPECIFY                                                    0418 01451000
*      (R0,                                                        0418 01452000
*       R1,                                                        0418 01453000
*       R11,                                                       0418 01454000
*       R12,                                                       0418 01455000
*       R14,                                                       0418 01456000
*       R15) UNRESTRICTED;                                         0418 01457000
@RC00406 DS    0H                                                  0419 01458000
*   END;                            /* ILRGMA GET                  0419 01459000
*                                      CELL(ACE)WORKREGS(R11,R12)    */ 01460000
*                                                                  0419 01461000
*   /*****************************************************************/ 01462000
*   /*                                                               */ 01463000
*   /* END ILRGMA MACRO EXPANSION                                    */ 01464000
*   /*                                                               */ 01465000
*   /*****************************************************************/ 01466000
*                                                                  0420 01467000
*                                                                  0420 01468000
*   /*****************************************************************/ 01469000
*   /*                                                               */ 01470000
*   /* THE ACE IS CLEARED, INITIALIZED AND ILRPOS IS CALLED TO       */ 01471000
*   /* PROCESS THE REQUEST.                                          */ 01472000
*   /*                                                               */ 01473000
*   /*****************************************************************/ 01474000
*                                                                  0420 01475000
*   RFY                                                            0420 01476000
*     CELL@ RSTD;                                                  0420 01477000
*   IF CELL@=ZERO THEN                                             0421 01478000
         LTR   CELL@,CELL@                                         0421 01479000
         BNZ   @RF00421                                            0421 01480000
*     RTNCODE=CODE28;               /* NO STORAGE AVAILABLE          */ 01481000
         MVC   RTNCODE(4,ASMVTPTR),@CF00486                        0422 01482000
*   ELSE                                                           0423 01483000
*     DO;                           /* ACE SUCCESSFULLY OBTAINED     */ 01484000
         B     @RC00421                                            0423 01485000
@RF00421 DS    0H                                                  0424 01486000
*       ACEPTR=CELL@;               /* SAVE ADDR OF CELL OBTAINED    */ 01487000
         LR    ACEPTR,CELL@                                        0424 01488000
*       RFY                                                        0425 01489000
*         CELL@ UNRSTD;                                            0425 01490000
*       ACE=ACE&&ACE;               /* CLEAR THE ACE                 */ 01491000
         XC    ACE(40,ACEPTR),ACE(ACEPTR)                          0426 01492000
*       ACEOP=ACAOP;                /* PROPAGATE THE OP CODE         */ 01493000
         IC    @12,ACAOP(,ACAPTR)                                  0427 01494000
         STC   @12,ACEOP(,ACEPTR)                                  0427 01495000
*       ACELGN=ACATOLP;             /* PROPAGATE THE LPID            */ 01496000
         MVC   ACELGN(8,ACEPTR),ACATOLP(ACAPTR)                    0428 01497000
*       ACEAIAPT=ACAAIAP;           /* PROPAGATE THE AIA POINTER     */ 01498000
         L     @12,ACAAIAP(,ACAPTR)                                0429 01499000
         ST    @12,ACEAIAPT(,ACEPTR)                               0429 01500000
*       ACESRCID=ACAFLSID;          /* PROPAGATE THE SOURCE LSID     */ 01501000
         L     @11,ACAFLSID(,ACAPTR)                               0430 01502000
         ST    @11,ACESRCID(,ACEPTR)                               0430 01503000
*       RFY                                                        0431 01504000
*         ACAPTR UNRSTD;                                           0431 01505000
*       ATAACE=ACEPTR;              /* RRR-TRACK THE ACE IN THE    0432 01506000
*                                      ATA-RRR                       */ 01507000
         ST    ACEPTR,ATAACE(,ATAPTR)                              0432 01508000
*       IF ACEAIAPT^=ZERO THEN                                     0433 01509000
         LTR   @12,@12                                             0433 01510000
         BZ    @RF00433                                            0433 01511000
*         DO;                       /* SPECIAL TRANSFER PG, I/O NO 0434 01512000
*                                      COMPLETE FOR AIA WITH SOURCE     01513000
*                                      LSID                          */ 01514000
*           RFY                                                    0435 01515000
*             SRCAIAPT RSTD;                                       0435 01516000
*           SRCAIAPT=ACEAIAPT;      /* ADDRESSIBILTY TO SOURCE AIA   */ 01517000
         LR    SRCAIAPT,@12                                        0436 01518000
*           RFY                                                    0437 01519000
*             AIA BASED(SRCAIAPT);                                 0437 01520000
*           AIAVIO=ON;              /* SET VIO FLAG IN AIA           */ 01521000
         OI    AIAVIO(SRCAIAPT),B'00100000'                        0438 01522000
*           AIATRPSP=ON;            /* SET TRANSFER PG SPECIAL FLAG  */ 01523000
         OI    AIATRPSP(SRCAIAPT),B'00100000'                      0439 01524000
*           AIACEPTR=ACEPTR;        /* STORE THE ACE PTR IN THE AIA  */ 01525000
         ST    ACEPTR,AIACEPTR(,SRCAIAPT)                          0440 01526000
*           ACETRPWT=ON;            /* INDICATE OPERATION WAITING FOR   01527000
*                                      I/O TO COMPLETE               */ 01528000
         OI    ACETRPWT(ACEPTR),B'01000000'                        0441 01529000
*           RFY                                                    0442 01530000
*             AIA BASED(AIAPTR);                                   0442 01531000
*         END;                                                     0443 01532000
*       CALL ILRPOS;                /* CALL PAGE OPERATIONS STARTER     01533000
*                                      TO PROCESS THE ACE            */ 01534000
@RF00433 L     @15,@CA00028                                        0444 01535000
         BALR  @14,@15                                             0444 01536000
*       IF ATAACE^=ZERO THEN                                       0445 01537000
         SLR   @12,@12                                             0445 01538000
         L     @11,ATAACE(,ATAPTR)                                 0445 01539000
         CR    @11,@12                                             0445 01540000
         BE    @RF00445                                            0445 01541000
*         DO;                       /* AN ERROR WAS FOUND            */ 01542000
*                                                                  0447 01543000
*           /*********************************************************/ 01544000
*           /*                                                       */ 01545000
*           /* THE FOLLOWING FREES THE ACE IF AN ERROR WAS FOUND BY  */ 01546000
*           /* MAINLINE POS AND RESETS AIA FLAGS FOR THE SPECIAL     */ 01547000
*           /* TRANSFER PAGE CASE.                                   */ 01548000
*           /*                                                       */ 01549000
*           /*********************************************************/ 01550000
*                                                                  0447 01551000
*           ACEPTR=ATAACE;          /* ACE CONTAINING ERROR IS IN ATA*/ 01552000
         LR    ACEPTR,@11                                          0447 01553000
*           IF ACEAIAPT^=ZERO THEN                                 0448 01554000
         L     @11,ACEAIAPT(,ACEPTR)                               0448 01555000
         CR    @11,@12                                             0448 01556000
         BE    @RF00448                                            0448 01557000
*             DO;                   /* IT WAS A SPECIAL TRANSFER PG     01558000
*                                      ACE                           */ 01559000
*               SRCAIAPT=ACEAIAPT;  /* OBTAIN ADDRESSIBILITY TO    0450 01560000
*                                      SOURCE AIA                    */ 01561000
         LR    SRCAIAPT,@11                                        0450 01562000
*               RFY                                                0451 01563000
*                 AIA BASED(SRCAIAPT);                             0451 01564000
*               AIAVIO=OFF;         /* RESET VIO FLAG                */ 01565000
         NI    AIAVIO(SRCAIAPT),B'11011111'                        0452 01566000
*               AIATRPSP=OFF;       /* RESET SPECIAL TRANSFER PG AIA    01567000
*                                      FLAG                          */ 01568000
         NI    AIATRPSP(SRCAIAPT),B'11011111'                      0453 01569000
*               AIACEPTR=ZERO;      /* CLEAR THE ACE ADDR IN THE AIA */ 01570000
         ST    @12,AIACEPTR(,SRCAIAPT)                             0454 01571000
*               RFY                                                0455 01572000
*                 SRCAIAPT UNRSTD;                                 0455 01573000
*               RFY                                                0456 01574000
*                 AIA BASED(AIAPTR);                               0456 01575000
*             END;                                                 0457 01576000
*           ATAACE=ZERO;            /* RRR-STOP TRACKING THE ACE-RRR */ 01577000
@RF00448 SLR   @12,@12                                             0458 01578000
         ST    @12,ATAACE(,ATAPTR)                                 0458 01579000
*           RFY                                                    0459 01580000
*             CELL@ RSTD;                                          0459 01581000
*           CELL@=ACEPTR;                                          0460 01582000
*                                                                  0460 01583000
         LR    CELL@,ACEPTR                                        0460 01584000
*           /*********************************************************/ 01585000
*           /*                                                       */ 01586000
*           /* BEGIN ILRGMA MACRO EXPANSION                          */ 01587000
*           /*                                                       */ 01588000
*           /*********************************************************/ 01589000
*                                                                  0461 01590000
*           DO;                     /* ILRGMA FREE                 0461 01591000
*                                      CELL(ACE)WORKREGS(R11,R12)    */ 01592000
*             RESPECIFY                                            0462 01593000
