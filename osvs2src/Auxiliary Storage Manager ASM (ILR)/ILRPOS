*   GOTO POSEND;                    /* OPERATIONS SUCCESSFUL GET OUT    00584000
*                                      IMMEDIATELY                   */ 00585000
         B     POSEND                                              0211 00586000
*                                                                  0212 00587000
*   /*****************************************************************/ 00588000
*   /*                                                               */ 00589000
*   /* ALL ERRORS WHICH RESULT IN BAD ID'S COME HERE. MY CALLER      */ 00590000
*   /* RETURNS TO RSM WHICH HANDLES THIS RETURN CODE.                */ 00591000
*   /*                                                               */ 00592000
*   /*****************************************************************/ 00593000
*                                                                  0212 00594000
*IDERROR:                                                          0212 00595000
*   AIABADID=ON;                    /* AIA BAD LGID FLAG             */ 00596000
IDERROR  OI    AIABADID(AIAPTR),B'00000001'                        0212 00597000
*   RTNCODE=CODE04;                 /* INDICATE BAD LGN              */ 00598000
*                                                                  0213 00599000
         MVC   RTNCODE(4,ASMVTPTR),@CF00049                        0213 00600000
*   /*****************************************************************/ 00601000
*   /*                                                               */ 00602000
*   /* ATA REFLECTS THE ERROR AIA OR ACE IF ONE EXISTS, ELSE THAT    */ 00603000
*   /* THAT FIELD IS CLEAR. IN ANY CASE ALL PROCESSING FALLS THRU THE*/ 00604000
*   /* REMAINING CODE IN ORDER TO RETURN TO THE CALLER.              */ 00605000
*   /*                                                               */ 00606000
*   /*****************************************************************/ 00607000
*                                                                  0214 00608000
*POSEND:                                                           0214 00609000
*   ATALGE=ZERO;                    /* STOP TRACKING THE LGE         */ 00610000
