         TITLE 'ILRPOS - PAGE OPERATIONS STARTER                       *00001000
                        '                                               00002000
ILRPOS   CSECT                                                          00003000
         SPACE                                                          00004000
         USING *,R15                      SET MODULE ID                 00005000
         MODID BR=YES                                                   00006000
         DROP  R15                                                      00007000
*                                         EXTERNAL ENTRIES              00008000
         ENTRY ILRTRANS                   TRANSFER PAGE PROCESSING      00009000
         ENTRY ILRESTRT                   RESTART AIA PROCESSING        00010000
         ENTRY ILRTRPAG                   TRANSFER PG INITIALIZATION    00011000
         SPACE                                                          00012000
         STM   R0,R14,ASMWKSA6(ASMVTPTR)  STORE CALLERS REGS            00013000
POSSETUP BALR  R6BASE,0                   ESTABLISH ADDRESSIBILITY      00014000
         USING *,R6BASE                                                 00015000
*   ATAPOS=ON;                      /* SET POS FLAG IN ATA           */ 00016000
*                                                                  0054 00017000
         OI    ATAPOS(ATAPTR),B'00000100'                          0054 00018000
*   /*****************************************************************/ 00019000
*   /*                                                               */ 00020000
*   /* THE FOLLOWING OBTAINS THE INPUT ADDR, THE ASM CLASS LOCK, AND */ 00021000
*   /* THE LGVT ADDR.                                                */ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0055 00025000
*   AIAPTR=ATAAIA;                  /* OBTAIN THE ADDR OF THE INPUT     00026000
*                                      AIA CHAIN                     */ 00027000
         L     AIAPTR,ATAAIA(,ATAPTR)                              0055 00028000
*   RFY                                                            0056 00029000
*     R11 RSTD;                                                    0056 00030000
*   R11=ADDR(ASHLOCK);              /* OBTAIN ADDR ASM LOCK WORD FOR    00031000
*                                      SETLOCK                       */ 00032000
         LA    R11,ASHLOCK(,RSMHDPTR)                              0057 00033000
*   GEN REFS(R11);                                                 0058 00034000
*              /* OBTAIN ASM CLASS LOCK */                              00035000
GETASM1  SETLOCK OBTAIN,TYPE=ASM,ADDR=(11),MODE=UNCOND,               **00036000
               RELATED=(VIO,ILRPOS(FREASM1))                            00037000
*   RFY                                                            0059 00038000
*     R11 UNRSTD;                                                  0059 00039000
*   RFY                                                            0060 00040000
*     LGVTPTR RSTD;                                                0060 00041000
*   LGVTPTR=ASMLGVT;                /* OBTAIN ADDR LOGICAL GROUP   0061 00042000
*                                      VECTOR TABLE                  */ 00043000
         L     LGVTPTR,ASMLGVT(,ASMVTPTR)                          0061 00044000
*   WORKPEND=OFF;                   /* CLEAR THE INTERNAL WORK     0062 00045000
*                                      PENDING FLAG                  */ 00046000
         NI    WORKPEND(ASMVTPTR),B'01111111'                      0062 00047000
*                                                                  0063 00048000
*   /*****************************************************************/ 00049000
*   /*                                                               */ 00050000
*   /* THIS DO LOOP GOES THRU EACH ELEMENT ON THE INPUT CHAIN,       */ 00051000
*   /* VALIDITY CHECKS THE REQUEST, STARTS THE OPERATION WHERE       */ 00052000
*   /* POSSIBLE, AND QUEUES THE ELEMENT WHEN NEEDED.                 */ 00053000
*   /*                                                               */ 00054000
*   /*****************************************************************/ 00055000
*                                                                  0063 00056000
*   DO UNTIL AIAPTR=ZERO;           /* LOOP THROUGH CHAIN OF AIAS    */ 00057000
*                                                                  0063 00058000
@DL00063 DS    0H                                                  0064 00059000
*     /***************************************************************/ 00060000
*     /*                                                             */ 00061000
*     /* THE FOLLOWING THREE TESTS DETERMINE IF THE REQUESTED        */ 00062000
*     /* OPERATION IS VALID FOR THE GIVEN LOGICAL GROUP AT THIS TIME.*/ 00063000
*     /*                                                             */ 00064000
*     /***************************************************************/ 00065000
*                                                                  0064 00066000
*     IF AIALGID>LGVMAXLG THEN                                     0064 00067000
         L     @12,AIALGID(,AIAPTR)                                0064 00068000
         C     @12,LGVMAXLG(,LGVTPTR)                              0064 00069000
         BH    @RT00064                                            0064 00070000
*       GOTO IDERROR;               /* LGN VALUE NOT IN LGVT GET OUT    00071000
*                                      IMMEDIATELY                   */ 00072000
*     RFY                                                          0066 00073000
*       LGVTEPTR RSTD;                                             0066 00074000
*     LGVTEPTR=ADDR(LGVENTRS(AIALGID+ONE));/* LGID GIVES OFFSET INTO    00075000
*                                      LGVT ENTRIES FOR PROPER LGVTE */ 00076000
         LR    @09,@12                                             0067 00077000
         SLA   @09,3                                               0067 00078000
         LA    LGVTEPTR,LGVENTRS(@09,LGVTPTR)                      0067 00079000
*     IF LGVELGID=AIALGID|LGVEASCB^=PSAAOLD THEN                   0068 00080000
         C     @12,LGVELGID(,LGVTEPTR)                             0068 00081000
         BE    @RT00068                                            0068 00082000
         CLC   LGVEASCB(4,LGVTEPTR),PSAAOLD                        0068 00083000
         BNE   @RT00068                                            0068 00084000
*       GOTO IDERROR;               /* LGID NOT ASSIGNED OR ASCB NOT    00085000
*                                      FOR THIS ADDRESS SPACE, GET 0069 00086000
*                                      OUT IMMEDIATELY               */ 00087000
*     LGEPTR=LGVELGEP;              /* OBTAIN LGE ADDR               */ 00088000
         L     LGEPTR,LGVELGEP(,LGVTEPTR)                          0070 00089000
*     RFY                                                          0071 00090000
*       LGVTEPTR UNRSTD;                                           0071 00091000
*     ATALGE=LGEPTR;                /* RRR-TRACK THE LGE-RRR         */ 00092000
         ST    LGEPTR,ATALGE(,ATAPTR)                              0072 00093000
*     IF LGERELLG=ON|LGEGRINP=ON|LGESAVRQ=ON THEN                  0073 00094000
         TM    LGERELLG(LGEPTR),B'01110000'                        0073 00095000
         BNZ   @RT00073                                            0073 00096000
*       GOTO IDERROR;               /* GROUP-OP IN PROGRESS OR WORK     00097000
*                                      PENDING GET OUT IMMEDIATELY   */ 00098000
*     SAVDLGID=AIALGID;             /* SAVE THE LGID OF THIS AIA     */ 00099000
*                                                                  0075 00100000
         ST    @12,SAVDLGID(,ASMVTPTR)                             0075 00101000
*     /***************************************************************/ 00102000
*     /*                                                             */ 00103000
*     /* THIS DO STATEMENT LOOPS THRU ALL SEQUENTIAL AIAS FOR THE    */ 00104000
*     /* SAME LOGICAL GROUP                                          */ 00105000
*     /*                                                             */ 00106000
*     /***************************************************************/ 00107000
*                                                                  0076 00108000
*     DO UNTIL AIAPTR=ZERO|AIALGID^=SAVDLGID;/* LOOP THROUGH AIA'S 0076 00109000
*                                      WITH SAME LGID                */ 00110000
@DL00076 DS    0H                                                  0077 00111000
*       RFY                                                        0077 00112000
*         ASPCTPTR RSTD;                                           0077 00113000
*       ASPCTPTR=LGEASPCT;          /* OBTAIN THE ASPCT POINTER      */ 00114000
         L     ASPCTPTR,LGEASPCT(,LGEPTR)                          0078 00115000
*       IF AIARPN<MAXALEXT THEN                                    0079 00116000
         L     @12,AIARPN(,AIAPTR)                                 0079 00117000
         CL    @12,@CF00518                                        0079 00118000
         BNL   @RF00079                                            0079 00119000
*         AIALPMEP=ADDR(ASPLPMES(AIARPN+ONE));/* THE RPN DOES NOT  0080 00120000
*                                      INDEX THE EXTENSIONS. THE BASE   00121000
*                                      LPME ADDR IS STORED IN THE AIA*/ 00122000
         SLL   @12,2                                               0080 00123000
         LA    @12,ASPLPMES(@12,ASPCTPTR)                          0080 00124000
         ST    @12,AIALPMEP(,AIAPTR)                               0080 00125000
*       ELSE                                                       0081 00126000
*         DO;                       /* THIS DO HANDLES THE INDEXING     00127000
*                                      OF AN ASPCT EXT.              */ 00128000
         B     @RC00079                                            0081 00129000
@RF00079 DS    0H                                                  0082 00130000
*           IF AIARPN>ASPMAXPN THEN                                0082 00131000
         CLC   AIARPN(4,AIAPTR),ASPMAXPN(ASPCTPTR)                 0082 00132000
         BH    @RT00082                                            0082 00133000
*             GOTO IDERROR;         /* ASST EXT PTR INDEX INVALID GET   00134000
*                                      OUT IMMEDIATELY               */ 00135000
*                                                                  0083 00136000
*           /*********************************************************/ 00137000
*           /*                                                       */ 00138000
*           /* THIS DO STATEMENT LOOPS UNTIL BOTH AN ASST AND LPME   */ 00139000
*           /* EXTENSION EXISTS FOR THE RELATIVE PAGE NUMBER.        */ 00140000
*           /*                                                       */ 00141000
*           /*********************************************************/ 00142000
*                                                                  0084 00143000
*           DO WHILE ASPASSTP(RPNA+ONE)=ZERO|ASPASSTP(RPNA+ONE)->  0084 00144000
*                 ASPASSTE(RPNAA+ONE)=ZERO;/* NO EXT., LOOP UNTIL  0084 00145000
*                                      CORRECT ONE IS CREATED        */ 00146000
         B     @DE00084                                            0084 00147000
@DL00084 DS    0H                                                  0085 00148000
*             IF AIAOP=ACEIO&AIAWRITE=OFF THEN                     0085 00149000
         CLI   AIAOP(AIAPTR),0                                     0085 00150000
         BNE   @RF00085                                            0085 00151000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0085 00152000
         BZ    @RT00085                                            0085 00153000
*               GOTO IDERROR;       /* READ REQUEST AND PAGE DOES NOT   00154000
*                                      EXIST GET OUT IMMEDIATELY     */ 00155000
*                                                                  0086 00156000
*             /*******************************************************/ 00157000
*             /*                                                     */ 00158000
*             /* THE EXPAND ROUTINE WILL DETERMINE WHETHER AN ASST   */ 00159000
*             /* AND A LPME EXTENSION ARE NEEDED, OR JUST A LPME     */ 00160000
*             /* EXTENSION IS NEEDED. IT WILL THEN BUILD WHATEVER IS */ 00161000
*             /* NECESSARY.                                          */ 00162000
*             /*                                                     */ 00163000
*             /*******************************************************/ 00164000
*                                                                  0087 00165000
*             CALL EXPAND;          /* CREATE THE EXTENSION          */ 00166000
@RF00085 BAL   @14,EXPAND                                          0087 00167000
*             IF RTNCODE^=ZERO THEN                                0088 00168000
         L     @12,RTNCODE(,ASMVTPTR)                              0088 00169000
         LTR   @12,@12                                             0088 00170000
         BZ    @RF00088                                            0088 00171000
*               DO;                 /* EXPAND COULD NOT BE DONE      */ 00172000
*                 AIAERROR=ON;      /* LOGICAL AIA ERROR             */ 00173000
         OI    AIAERROR(AIAPTR),B'00000010'                        0090 00174000
*                 RTNCODE=CODE28;   /* UNABLE TO OBTAIN STORAGE      */ 00175000
         MVC   RTNCODE(4,ASMVTPTR),@CF00486                        0091 00176000
*                 GOTO POSEND;      /* NO STORAGE GET OUT IMMEDIATELY*/ 00177000
         B     POSEND                                              0092 00178000
*               END;                                               0093 00179000
*           END;                    /* END LOOP FOR CALLING EXPAND   */ 00180000
@RF00088 DS    0H                                                  0094 00181000
@DE00084 SLR   @12,@12                                             0094 00182000
         LH    @15,RPNA(,AIAPTR)                                   0094 00183000
         SLA   @15,2                                               0094 00184000
         L     @15,ASPASSTP(@15,ASPCTPTR)                          0094 00185000
         CR    @15,@12                                             0094 00186000
         BE    @DL00084                                            0094 00187000
         SLR   @14,@14                                             0094 00188000
         IC    @14,RPNAA(,AIAPTR)                                  0094 00189000
         SLA   @14,2                                               0094 00190000
         L     @15,ASPASSTE(@14,@15)                               0094 00191000
         CR    @15,@12                                             0094 00192000
         BE    @DL00084                                            0094 00193000
*           AIALPMEP=ADDR(ASPASSTP(RPNA+ONE)->ASPASSTE(RPNAA+ONE)->     00194000
*               ASPLPMEE(RPNLL+ONE));/* THE EXTENDED LPME ADDR IS  0095 00195000
*                                      STORED IN THE AIA             */ 00196000
         SLR   @12,@12                                             0095 00197000
         IC    @12,RPNLL(,AIAPTR)                                  0095 00198000
         SLA   @12,2                                               0095 00199000
         LA    @12,ASPLPMEE(@12,@15)                               0095 00200000
         ST    @12,AIALPMEP(,AIAPTR)                               0095 00201000
*         END;                      /* END DO FOR INDEXING ASPCT EXT.*/ 00202000
*       AIALPMEC=ON;                /* INDICATE LPME ADDR IS IN AIA  */ 00203000
@RC00079 OI    AIALPMEC(AIAPTR),B'00001000'                        0097 00204000
*       NXTAIA=AIANXAIA;            /* OBTAIN NEXT AIA FOR USE LATER */ 00205000
         L     NXTAIA,AIANXAIA(,AIAPTR)                            0098 00206000
*       IF LGEWRKPD=OFF THEN                                       0099 00207000
         TM    LGEWRKPD(LGEPTR),B'10000000'                        0099 00208000
         BNZ   @RF00099                                            0099 00209000
*         DO;                       /* WORK NOT PENDING FOR THE LGE  */ 00210000
*           IF AIALPMEP->ASPLOPIN=ON THEN                          0101 00211000
         L     @12,AIALPMEP(,AIAPTR)                               0101 00212000
         TM    ASPLOPIN(@12),B'00100000'                           0101 00213000
         BNO   @RF00101                                            0101 00214000
*             WORKPEND=ON;          /* PROCESS IN OPERATION FOR PAGE,   00215000
*                                      SET INTERNAL WORK PENDING FLAG*/ 00216000
         OI    WORKPEND(ASMVTPTR),B'10000000'                      0102 00217000
*           ELSE                                                   0103 00218000
*             DO;                   /* PROCESS NOT IN OPERATION FOR     00219000
*                                      PAGE                          */ 00220000
         B     @RC00101                                            0103 00221000
@RF00101 DS    0H                                                  0104 00222000
*               IF ACEOP=ACETRPAG THEN                             0104 00223000
         CLI   ACEOP(ACEPTR),4                                     0104 00224000
         BNE   @RF00104                                            0104 00225000
*                 DO;               /* INPUT IS A TRANSFER PAGE ACE  */ 00226000
*                                                                  0106 00227000
*                   /*************************************************/ 00228000
*                   /*                                               */ 00229000
*                   /* INPUT WAS DETERMINED TO BE A TRANSFER PAGE    */ 00230000
*                   /* ACE. IF STARTABLE, IT IS PROCESSED TO         */ 00231000
*                   /* COMPLETION AND THEN FREED, ELSE IT IS QUEUED  */ 00232000
*                   /* FOR LATER PROCESSING.                         */ 00233000
*                   /*                                               */ 00234000
*                   /*************************************************/ 00235000
*                                                                  0106 00236000
*                   RFY                                            0106 00237000
*                     ASCBPTR RSTD;                                0106 00238000
*                   ASCBPTR=ASPASCB;/* OBTAIN ADDR OF ASCB OWNING  0107 00239000
*                                      THIS LG                       */ 00240000
         L     ASCBPTR,ASPASCB(,ASPCTPTR)                          0107 00241000
*                   ASCBNVSC=ASCBNVSC-ONE;/* DECR ASCB NON-VIO COUNT */ 00242000
         LH    @15,ASCBNVSC(,ASCBPTR)                              0108 00243000
         BCTR  @15,0                                               0108 00244000
         STH   @15,ASCBNVSC(,ASCBPTR)                              0108 00245000
*                   ASCBVSC=ASCBVSC+ONE;/* INCR ASCB VIO COUNT       */ 00246000
         LA    @15,1                                               0109 00247000
         AH    @15,ASCBVSC(,ASCBPTR)                               0109 00248000
         STH   @15,ASCBVSC(,ASCBPTR)                               0109 00249000
*                   RFY                                            0110 00250000
*                     ASCBPTR UNRSTD;                              0110 00251000
*                   ASMNVSC=ASMNVSC-ONE;/* DECR ASMVT NON-VIO COUNT  */ 00252000
         L     @12,ASMNVSC(,ASMVTPTR)                              0111 00253000
         BCTR  @12,0                                               0111 00254000
         ST    @12,ASMNVSC(,ASMVTPTR)                              0111 00255000
*                   RFY                                            0112 00256000
*                    (R0,                                          0112 00257000
*                     R1) RSTD;                                    0112 00258000
*                   R0=ASMVSC;      /* GET ASMVT VIO COUNT           */ 00259000
         L     R0,ASMVSC(,ASMVTPTR)                                0113 00260000
*VIOUPDT:                                                          0114 00261000
*                   R1=R0+ONE;      /* INCR COUNT                    */ 00262000
VIOUPDT  LA    R1,1                                                0114 00263000
         ALR   R1,R0                                               0114 00264000
*                   CS(R0,R1,ASMVSC);/* UPDATE VIO COUNT FIELD IN  0115 00265000
*                                      ASMVT                         */ 00266000
         CS    R0,@01,ASMVSC(ASMVTPTR)                             0115 00267000
*                   BC(NE,VIOUPDT); /* NOT SECCESSFUL, TRY AGAIN     */ 00268000
         BC    7,VIOUPDT                                           0116 00269000
*                   RFY                                            0117 00270000
*                    (R0,                                          0117 00271000
*                     R1) UNRSTD;                                  0117 00272000
*                   RFY                                            0118 00273000
*                     LPMEPTR RSTD;                                0118 00274000
*                   LPMEPTR=ACETLPME;/* OBTAIN THE LPME ADDR         */ 00275000
         L     LPMEPTR,ACETLPME(,ACEPTR)                           0119 00276000
*                   IF ACEAIAPT=ZERO THEN                          0120 00277000
         L     @12,ACEAIAPT(,ACEPTR)                               0120 00278000
         LTR   @12,@12                                             0120 00279000
         BNZ   @RF00120                                            0120 00280000
*                     DO;           /* NORMAL TRANSFER PAGE, SOURCE     00281000
*                                      LSID IN AIA                   */ 00282000
*                       R13=ADDR(ASMSAVE);/* SAVE AREA FOR FREE SLOT */ 00283000
*                                                                  0122 00284000
         LA    R13,ASMSAVE(,ASMVTPTR)                              0122 00285000
*                       /*********************************************/ 00286000
*                       /*                                           */ 00287000
*                       /* TRPPROC PROCESSES THE TRANSFER PAGE ACE TO*/ 00288000
*                       /* COMPLETION.                               */ 00289000
*                       /*                                           */ 00290000
*                       /*********************************************/ 00291000
*                                                                  0123 00292000
*                       CALL TRPPROC;/* CALL THE TRANSFER PAGE     0123 00293000
*                                      ROUTINE                       */ 00294000
         BAL   @14,TRPPROC                                         0123 00295000
*                       RFY                                        0124 00296000
*                        (LPMEPTR,                                 0124 00297000
*                         ASPCTPTR) UNRSTD;                        0124 00298000
*                       RFY                                        0125 00299000
*                         CELL@ RSTD;                              0125 00300000
*                       CELL@=ACEPTR;/* ADDR AREA TO BE RETURNED TO     00301000
*                                      POOL                          */ 00302000
         LR    CELL@,ACEPTR                                        0126 00303000
*                                                                  0127 00304000
*                       /*********************************************/ 00305000
*                       /*                                           */ 00306000
*                       /* FREE THE ACE JUST PROCESSED AND HANDLE THE*/ 00307000
*                       /* SPECIAL TRANSFER PAGE CASE WHERE I/O HAS  */ 00308000
*                       /* NOT COMPLETED FOR THE AIA CONTAINING THE  */ 00309000
*                       /* SOURCE LSID. BEGIN ILRGMA MACRO EXPANSION */ 00310000
*                       /*                                           */ 00311000
*                       /*********************************************/ 00312000
*                                                                  0127 00313000
*                       DO;         /* ILRGMA FREE                 0127 00314000
*                                      CELL(ACE)WORKREGS(R11,R12)    */ 00315000
*                         DECLARE   /* ASMVT VECTOR TO ILRPEX        */ 00316000
*                           ILRPEX ENTRY BASED(ASMPEX) VALRG(0);   0128 00317000
*                         DECLARE   /* MAP FOR CELL CONTROL WORDS    */ 00318000
*                           1 GMACELL BASED BDY(WORD),             0129 00319000
*                            2 GMANEXT PTR(31);/* PTR NEXT FREE CELL */ 00320000
*                         RESPECIFY                                0130 00321000
*                          (R0,                                    0130 00322000
*                           R1,                                    0130 00323000
*                           R11,                                   0130 00324000
*                           R12,                                   0130 00325000
*                           R14,                                   0130 00326000
*                           R15) RESTRICTED;                       0130 00327000
*                         RESPECIFY                                0131 00328000
*                           ASMPOOLS BASED(R11);                   0131 00329000
*                         RESPECIFY                                0132 00330000
*                           GMACELL BASED(R1);                     0132 00331000
*                         R11=ADDR(ASMACEPC);                      0133 00332000
         LA    R11,ASMACEPC(,ASMVTPTR)                             0133 00333000
*                         LM(R14,R15,ASMCPRSQ);/* GET CONTENTS OF Q  */ 00334000
         LM    R14,R15,ASMCPRSQ(R11)                               0134 00335000
*GMA00011:                                                         0135 00336000
*                         IF R14^=0 THEN/* TEST RESERVE Q            */ 00337000
GMA00011 LTR   R14,R14                                             0135 00338000
         BZ    @RF00135                                            0135 00339000
*                           DO;     /* REPLENISH ACE RESERVE Q       */ 00340000
*                             R0=R14-1;/* SET UP FOR COMPARE AND SWAP*/ 00341000
         LR    R0,R14                                              0137 00342000
         BCTR  R0,0                                                0137 00343000
*                             GMANEXT=R15;/* CELL TO FREE IN R1      */ 00344000
         ST    R15,GMANEXT(,R1)                                    0138 00345000
*                             CDS(R14,R0,ASMCPRSQ);/* ADD CELL TO  0139 00346000
*                                      RESERVE Q                     */ 00347000
         CDS   R14,R0,ASMCPRSQ(R11)                                0139 00348000
*                             BC(7,GMA00011);                      0140 00349000
         BC    7,GMA00011                                          0140 00350000
*                           END;                                   0141 00351000
*                         ELSE                                     0142 00352000
*                           DO;     /* RETURN ACE TO NORMAL Q        */ 00353000
         B     @RC00135                                            0142 00354000
@RF00135 DS    0H                                                  0143 00355000
*                             R14=ASMCPAVL;                        0143 00356000
         L     R14,ASMCPAVL(,R11)                                  0143 00357000
*GMA00012:                                                         0144 00358000
*                             GMANEXT=R14;/* PUT ADDR CURRENT FIRST     00359000
*                                      IN INPUT CELL                 */ 00360000
GMA00012 ST    R14,GMANEXT(,R1)                                    0144 00361000
*                             CS(R14,R1,ASMCPAVL);/* ADD INPUT CELL     00362000
*                                      TO POOL                       */ 00363000
         CS    R14,@01,ASMCPAVL(R11)                               0145 00364000
*                             BC(7,GMA00012);                      0146 00365000
         BC    7,GMA00012                                          0146 00366000
*                           END;    /* ACE RETURNED TO NORMAL Q      */ 00367000
*                         RESPECIFY                                0148 00368000
*                          (R0,                                    0148 00369000
*                           R1,                                    0148 00370000
*                           R11,                                   0148 00371000
*                           R12,                                   0148 00372000
*                           R14,                                   0148 00373000
*                           R15) UNRESTRICTED;                     0148 00374000
@RC00135 DS    0H                                                  0149 00375000
*                       END;        /* ILRGMA FREE                 0149 00376000
*                                      CELL(ACE)WORKREGS(R11,R12)    */ 00377000
*                                                                  0149 00378000
*                       /*********************************************/ 00379000
*                       /*                                           */ 00380000
*                       /* END ILRGMA MACRO EXPANSION                */ 00381000
*                       /*                                           */ 00382000
*                       /*********************************************/ 00383000
*                                                                  0150 00384000
*                       RFY                                        0150 00385000
*                         CELL@ UNRSTD;                            0150 00386000
*                       ATAACE=ZERO;/* RRR-CLEAR THE ACE PTR IN THE     00387000
*                                      ATA-RRR                       */ 00388000
         SLR   @12,@12                                             0151 00389000
         ST    @12,ATAACE(,ATAPTR)                                 0151 00390000
*                       RTNCODE=ZERO;/* TRANSFER PAGE SUCCESSFUL     */ 00391000
         ST    @12,RTNCODE(,ASMVTPTR)                              0152 00392000
*                       GOTO POSEND;/* TRANSFER PAGE FINISHED, GET 0153 00393000
*                                      OUT IMMEDIATELY               */ 00394000
         B     POSEND                                              0153 00395000
*                       RFY                                        0154 00396000
*                        (LPMEPTR,                                 0154 00397000
*                         ASPCTPTR) RSTD;                          0154 00398000
*                     END;          /* END DO FOR NORMAL TRANSFER  0155 00399000
*                                      PAGE ACE (NO AIAPTR)          */ 00400000
*                   ELSE                                           0156 00401000
*                     ASPLOPIN=ON;  /* SPECIAL TRANSFER PAGE,      0156 00402000
*                                      PAGE-OUT MUST COMPLETE FOR AIA*/ 00403000
@RF00120 OI    ASPLOPIN(LPMEPTR),B'00100000'                       0156 00404000
*                 END;              /* END DO FOR TRANSFER PAGE ACE  */ 00405000
*                                                                  0158 00406000
*               /*****************************************************/ 00407000
*               /*                                                   */ 00408000
*               /* INPUT IS DETERMINED TO BE A STARTABLE PAGING AIA. */ 00409000
*               /* LPMEPROC WILL CLEAR THE LSID ON A PAGE-OUT REUEST */ 00410000
*               /* AND MOVE THE LSID TO THE AIA ON A PAGE-IN REQUEST.*/ 00411000
*               /*                                                   */ 00412000
*               /*****************************************************/ 00413000
*                                                                  0158 00414000
*               ELSE                                               0158 00415000
*                 DO;               /* INPUT A PAGING AIA            */ 00416000
         B     @RC00104                                            0158 00417000
@RF00104 DS    0H                                                  0159 00418000
*                   AIAPRINO=ON;    /* SET PROCESS IN OPERATION FLAG */ 00419000
         OI    AIAPRINO(AIAPTR),B'01000000'                        0159 00420000
*                   CALL LPMEPROC;  /* PROCESS THE PAGE I/O REQUEST  */ 00421000
