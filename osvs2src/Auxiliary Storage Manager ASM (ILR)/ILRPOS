*   GEN REFS(R11);                                                 0217 00618000
*              /* RELEASE ASM LOCK */                                   00619000
FREASM1  SETLOCK RELEASE,TYPE=ASM,ADDR=(11),                          **00620000
               RELATED=(VIO,ILRPOS(GETASM1))                            00621000
*ILRCRPS1:                                                         0218 00622000
*   ATAPOS=OFF;                     /* CLEAR ENTRY FOOTPRINT IN ATA  */ 00623000
ILRCRPS1 NI    ATAPOS(ATAPTR),B'11111011'                          0218 00624000
*   GEN REFS(R15,R0,R14,ASMWKSA6,ASMVTPTR);                        0219 00625000
*                                        /* RESTORE CALLER'S REGS */    00626000
         LM R0,R15,ASMWKSA6(ASMVTPTR)     RESTORE REGS                  00627000
         BR R14                           RETURN                        00628000
*ILRTRANS:                                                         0220 00629000
*   ENTRY;                          /* EXTERNAL ENTRY                */ 00630000
*/********************************************************************/ 00631000
*/*                                                                  */ 00632000
*/*  THE TRANSFER PAGE ROUTINE IS CALLED EXTERNALLY BY THE SRB       */ 00633000
*/*  CONTROLLER OR INTERNALLY BY POS ITSELF TO PROCESS A TRANSFER    */ 00634000
*/*  PAGE ACE. THE ASM CLASS LOCK MUST BE HELD ON ENTRY.             */ 00635000
*/*                                                                  */ 00636000
*/*  INPUT: R13 - CALLER'S SAVE AREA ADDR                            */ 00637000
*/*         R14 - RETURN ADDR                                        */ 00638000
*/*         R0  - SAVE AREA ADDR FOR FREE SLOT ROUTINE               */ 00639000
*/*         R1  - ACEPTR                                             */ 00640000
*/*         R2 THRU R4 IS STANDARD AS IN MAPPING MACRO POINTERS      */ 00641000
*/*                                                                  */ 00642000
*/*  OUTPUT: UPDATED VIO SLOT COUNTS, MODIFIED FLAGS IN THE ASPCT,   */ 00643000
*/*          LPME, AND ACE, THE SOURCE LSID IS MOVED TO THE LPME, A  */ 00644000
*/*          SLOT MAY BE FREED.                                      */ 00645000
*/*                                                                  */ 00646000
*/********************************************************************/ 00647000
*/********************************************************************/ 00648000
*/*TRANSPG: CHART (AREF)                                             */ 00649000
*/*        HEADER                                                  0221 00650000
*/*        ILRPOS                                                  0221 00651000
*/* ILRTRANS (TRPPROC) - TRANSFER PAGE ROUTINE                       */ 00652000
*/*ILRTRANS: E EXTERNAL ENTRY                                        */ 00653000
*/*        P SAVE CALLER'S REGS                                      */ 00654000
*/*        P ESTABLISH ADDRESSIBILITY                                */ 00655000
*/*        P RECORD ENTRY INFORMATION IN ATA                         */ 00656000
*/*        N ACEPTR = R1                                             */ 00657000
*/*        P STORE THE ACEPTR IN THE ATA                             */ 00658000
*/*        P (,%POS28) LGEPTR EQUALS ACELGE                          */ 00659000
*/*TRPPROC: E INTERNAL ENTRY                                         */ 00660000
*/*        P SAVE RETURN ADDRESS                                     */ 00661000
*/*%POS28: P ASPCT PTR (ASPCTPTR) = LGEASPCT                         */ 00662000
*/*        P LPME PTR (LPMEPTR) = ACE TARGET LPME (ACETLPME)         */ 00663000
*/*        D (Y,,N,%POS30) LPME LSID VALID (ASPLVALD)                */ 00664000
*/*        D (Y,,N,%POS29) LSID SAVED (ASPLSAVE)                     */ 00665000
*/*        P SET RELEASE AFTER SAVE FLAG IN LPME (ASPLSVRP)          */ 00666000
*/*        P SET RELEASE AFTER SAVE FLAG IN ASPCT HDR (ASPSAVRP)     */ 00667000
*/*        P (,%POS30) RESET LSID SAVED FLAG (ASPLSAVE)              */ 00668000
*/*%POS29: D (N,,Y,%POS30) READ I/O ERROR (ASPLIOER)                 */ 00669000
*/*        P SET REGS FOR FREE SLOT ROUTINE OF I/O CONTROL           */ 00670000
*/*        S ILRFRSLT: FREE SLOT IDENTIFIED BY LSID IN LPME          */ 00671000
*/*        P DECR VIO SLOT CNTS, ASCBVSC, AND ASMVSC WITH CS         */ 00672000
*/*%POS30: P RESET I/O ERROR FLAG (ASPLIOER)                         */ 00673000
*/*        P MOVE THE SOURCE LSID (ACEVLSID) TO THE LPME (ASPVLSID)  */ 00674000
*/*        P SET THE LSID VALID FLAG (ASPLVALD)                      */ 00675000
*/*        P RESET THE PROCESS IN OPERATION FOR THE PG FLG (ASPLOPIN)*/ 00676000
*/*%POS31: D (Y,,N,%POS32) EXTERNAL ENTRY                            */ 00677000
*/*        P CLEAR THE ACEPTR IN THE ATA                             */ 00678000
*/*        P RESET ENTRY INFORMATION IN ATA                          */ 00679000
*/*        P RESTORE CALLER'S REGS                                   */ 00680000
*/*%POS32: R RETURN TO CALLER                                        */ 00681000
*/*TRANSPG: END                                                      */ 00682000
*/********************************************************************/ 00683000
*                                                                  0221 00684000
*                                                                  0221 00685000
*   /*****************************************************************/ 00686000
*   /*                                                               */ 00687000
*   /* EXTERNAL ENTRY: SAVES REGS, INITIALIZES POINTERS, CALLS       */ 00688000
*   /* INTERNAL ENTRY, RESTORES REGS, AND RETURNS.                   */ 00689000
*   /*                                                               */ 00690000
*   /*****************************************************************/ 00691000
*                                                                  0221 00692000
*   RFY                                                            0221 00693000
*    (R1,                                                          0221 00694000
*     R13,                                                         0221 00695000
*     R0) RSTD;                                                    0221 00696000
*   GEN REFS(R14,R12,R13,R6BASE);                                  0222 00697000
ILRTRANS STM  R14,R12,12(R13)           STORE CALLERS REGS              00698000
TRNSETUP BALR R6BASE,0                  ESTABLISH ADDRESSIBILITY        00699000
         LA   R12,TRNSETUP-POSSETUP     SETS CONTENTS OF BASE REG TO    00700000
         SR   R6BASE,R12                THAT OF MAINLINE POS            00701000
*   ACEPTR=R1;                      /* OBTAIN ADDR OF THE INPUT ACE  */ 00702000
         LR    ACEPTR,R1                                           0223 00703000
*   RFY                                                            0224 00704000
*     R1 UNRSTD;                                                   0224 00705000
