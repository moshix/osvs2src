*   ATAACE=ZERO;                    /* STOP TRACKING THE ACE         */ 00732000
         SLR   @12,@12                                             0236 00733000
         ST    @12,ATAACE(,ATAPTR)                                 0236 00734000
*TRANSEND:                                                         0237 00735000
*   GEN REFS(R14,R12,R13);                                         0237 00736000
TRANSEND DS    0H                                                  0237 00737000
        LM   R14,R12,12(R13)      RESTORE CALLERS REGS                  00738000
        BR   R14                  RETURN                                00739000
*                                                                  0238 00740000
*   /*****************************************************************/ 00741000
*   /*                                                               */ 00742000
*   /* INTERNAL ENTRY: SETS ASPCT AND LPME FLAGS, MOVES SOURCE LSID  */ 00743000
*   /* TO THE LPME, AND FREES THE SLOT IF NECESSARY. R13 = SAVE AREA */ 00744000
*   /* ADDR TO BE USED BY FREE SLOT.                                 */ 00745000
*   /*                                                               */ 00746000
*   /*****************************************************************/ 00747000
*                                                                  0238 00748000
*TRPPROC:                                                          0238 00749000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/* INTERNAL ENTRY           */ 00750000
         B     @PB00002                                            0238 00751000
TRPPROC  DS    0H                                                  0239 00752000
*   RFY                                                            0239 00753000
*    (R8,                                                          0239 00754000
*     R14,                                                         0239 00755000
*     R13,                                                         0239 00756000
*     R11,                                                         0239 00757000
*     ASPCTPTR,                                                    0239 00758000
*     LPMEPTR) RSTD;                                               0239 00759000
*   R8=R14;                         /* MAINTAIN RETURN ADDR,       0240 00760000
*                                      DESTROYED IN CALL TO FREE SLOT*/ 00761000
         LR    R8,R14                                              0240 00762000
*   RFY                                                            0241 00763000
*     R14 UNRSTD;                                                  0241 00764000
*   IF ASPLVALD=ON THEN                                            0242 00765000
         TM    ASPLVALD(LPMEPTR),B'10000000'                       0242 00766000
         BNO   @RF00242                                            0242 00767000
*     IF ASPLSAVE=ON THEN                                          0243 00768000
         TM    ASPLSAVE(LPMEPTR),B'01000000'                       0243 00769000
         BNO   @RF00243                                            0243 00770000
*       DO;                         /* VALID LPME LSID AND LSID    0244 00771000
*                                      ALREADY SAVED                 */ 00772000
*         ASPLSVRP=ON;              /* SET LPME RELEASE AFTER SAVE 0245 00773000
*                                      FLAG                          */ 00774000
         OI    ASPLSVRP(LPMEPTR),B'00001000'                       0245 00775000
*         ASPSAVRP=ON;              /* SET ASPCT RELEASE AFTER SAVE     00776000
*                                      FLAG                          */ 00777000
         OI    ASPSAVRP(ASPCTPTR),B'01000000'                      0246 00778000
*         ASPLSAVE=OFF;             /* RESET LSID SAVED FLAG         */ 00779000
         NI    ASPLSAVE(LPMEPTR),B'10111111'                       0247 00780000
*       END;                        /* END DO FOR SAVED LSID         */ 00781000
*     ELSE                                                         0249 00782000
*       DO;                         /* LSID NOT ALREADY SAVED        */ 00783000
         B     @RC00243                                            0249 00784000
@RF00243 DS    0H                                                  0250 00785000
*         IF ASPLIOER=OFF THEN                                     0250 00786000
         TM    ASPLIOER(LPMEPTR),B'00010000'                       0250 00787000
         BNZ   @RF00250                                            0250 00788000
*           DO;                     /* NO READ I/O ERROR             */ 00789000
*             RFY                                                  0252 00790000
