         TITLE ' ILRPREAD-ASM READ/WRITE ROUTINE                       *00001000
                        '                                               00002000
ILRPREAD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRPREAD  76.114'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @09,@01                                             0001 00016000
         USING @DATD,@09                                           0001 00017000
         LM    @00,@01,20(@13)                                     0001 00018000
*                                                                  0181 00019000
*/********************************************************************/ 00020000
*/*                                                                  */ 00021000
*/* INITIALIZATION: ILRPREAD INITIALIZATION ROUTINE                  */ 00022000
*/*                                                                  */ 00023000
*/* INPUT: INPUT PARAMETER LIST IS POINTED BY REG1                   */ 00024000
*/*             FLAG     -FLAGS FOR ILRPREAD                         */ 00025000
*/*             TOTSLOTS -NUMBER OF SLOTS TO BE READ/WRITE           */ 00026000
*/*             FRTSLOT  -FIRST SLOT NUMBER                          */ 00027000
*/*             TABENTRY -PTR TO PART OR TPABTBLE ENTRY              */ 00028000
*/*             INBUF    -PTR TO BUFFER                              */ 00029000
*/*                                                                  */ 00030000
*/* FUNCTION:  1.OBTAIN SQA FOR BUILDING IOSB,SRB AND CHANNEL PROGRAM*/ 00031000
*/*            2.ISSUE ESTAE FOR ABNORMAL TERMINATION                */ 00032000
*/*            3.GET UCB AND EDB HEADER PTR                          */ 00033000
*/*                                                                  */ 00034000
*/* OUTPUT: WORK AREA FOR CHANNEL PROGRAM,IOSB AND SRB OBTAINED.     */ 00035000
*/*         ESTABLISH ESTAE EXIT ROUTINE TO HANDLE ABNORMAL          */ 00036000
*/*         TERMINATION                                              */ 00037000
*/*                                                                  */ 00038000
*/********************************************************************/ 00039000
*                                                                  0181 00040000
*   INPTR=R1;                       /* SAVE INPUT PARAMETER REG      */ 00041000
         ST    R1,INPTR                                            0181 00042000
*   R13SAVE=RSAVE;                  /* SAVE REGISTER 13              */ 00043000
         ST    RSAVE,R13SAVE                                       0182 00044000
*   RSAVE=ADDR(SAVAREA);            /* R13 PTR TO 18 WORDS SAVEAREA  */ 00045000
         LA    RSAVE,SAVAREA                                       0183 00046000
*   RTCODE=ZERO;                    /* INIT RETURN CODE              */ 00047000
         SLR   RTCODE,RTCODE                                       0184 00048000
*   INBUF=BUFPTR;                   /* SET INPUT BUFFER PTR          */ 00049000
         L     @15,INPTR                                           0185 00050000
         L     @15,BUFPTR(,@15)                                    0185 00051000
         ST    @15,INBUF                                           0185 00052000
*   IOSBLEN=(LENGTH(IOSB)+SEVEN)/EIGHT*EIGHT;/* ROUND IOSB TO DOUBLE    00053000
*                                      WORD                          */ 00054000
         LA    IOSBLEN,112                                         0186 00055000
*   SRBLEN=(LENGTH(SRB)+SEVEN)/EIGHT*EIGHT;/* ROUND SRB TO DOUBLE  0187 00056000
*                                      WORD                          */ 00057000
         LA    SRBLEN,48                                           0187 00058000
*   CCWLEN=LENGTH(SEEKADDR)+MAXSLOTS*(LENGTH(SRHADDR)+LENGTH(CCWS));    00059000
         LA    CCWLEN,408                                          0188 00060000
*                                   /* CALCULATE SIZE REQUIRED TO  0189 00061000
*                                      BUILD CHANNEL PROGRAM(S)      */ 00062000
*   SQASIZE=IOSBLEN+SRBLEN+CCWLEN;  /* CALC TOTAL CORE REQUIRED      */ 00063000
         LR    @15,IOSBLEN                                         0189 00064000
         ALR   @15,SRBLEN                                          0189 00065000
         ALR   @15,CCWLEN                                          0189 00066000
         ST    @15,SQASIZE                                         0189 00067000
*   DO;                             /* GETMAIN (RC) LV(SQASIZE)    0190 00068000
*                                      SP(245)                       */ 00069000
*     RESPECIFY                                                    0191 00070000
*      (GPR01F,                                                    0191 00071000
*       GPR15F,                                                    0191 00072000
*       GPR00F) RESTRICTED;                                        0191 00073000
*     GPR01F=0;                     /* REG 1 MUST BE ZERO            */ 00074000
         SLR   GPR01F,GPR01F                                       0192 00075000
*     GPR15F=0;                     /* RC-TYPE GETMAIN               */ 00076000
         SLR   GPR15F,GPR15F                                       0193 00077000
*     GPR00F=SQASIZE;               /* LENGTH REQUESTED              */ 00078000
         L     @11,SQASIZE                                         0194 00079000
         LR    GPR00F,@11                                          0194 00080000
*     GPR15F=GPR15F|((245)*256);    /* SP IN BYTE 2                  */ 00081000
         O     GPR15F,@CF02517                                     0195 00082000
*     SVC(120);                     /* RC/RU-FORM OF GETMAIN         */ 00083000
         SVC   120                                                 0196 00084000
*     RESPECIFY                                                    0197 00085000
*      (GPR01F,                                                    0197 00086000
*       GPR15F,                                                    0197 00087000
*       GPR00F) UNRESTRICTED;                                      0197 00088000
*   END;                            /* GETMAIN (RC) LV(SQASIZE)    0198 00089000
*                                      SP(245)                       */ 00090000
*   RFY                                                            0199 00091000
*    (R0,                                                          0199 00092000
*     R15) RSTD;                    /* RESTRICT REG 0AND 15          */ 00093000
*   IF R15=SUCCESS THEN             /* GETMAIN SUCCESSFUL?           */ 00094000
         LTR   R15,R15                                             0200 00095000
         BNZ   @RF00200                                            0200 00096000
*     DO;                           /* YES                           */ 00097000
*       SQAPTR=R1;                  /* SAVE GETMAIN SQA PTR          */ 00098000
         ST    R1,SQAPTR                                           0202 00099000
*       R0=SQAPTR;                  /* SET GETMAIN ADDRESS           */ 00100000
         L     @11,SQAPTR                                          0203 00101000
         LR    R0,@11                                              0203 00102000
*       R1=SQASIZE;                 /* SET GETMAIN LENGTH            */ 00103000
         L     R1,SQASIZE                                          0204 00104000
*       MVCL(R0,R14);               /* CLEAR GETMAIN WORKAREA WITH 0205 00105000
*                                      ZERO                          */ 00106000
         MVCL  R0,R14                                              0205 00107000
*     END;                                                         0206 00108000
*   ELSE                            /* GETMAIN FAIL                  */ 00109000
*     DO;                                                          0207 00110000
         B     @RC00200                                            0207 00111000
@RF00200 DS    0H                                                  0208 00112000
*       RTCODE=NOCORE;              /* SET RETURN CODE-NO WORKAREA   */ 00113000
         LA    RTCODE,8                                            0208 00114000
*       GOTO EXIT;                  /* RETURN TO CALLER              */ 00115000
         B     EXIT                                                0209 00116000
*     END;                                                         0210 00117000
*   RFY                                                            0211 00118000
*    (R0,                                                          0211 00119000
*     R15) UNRSTD;                  /* RELEASE REG 0 AND REG15       */ 00120000
@RC00200 DS    0H                                                  0212 00121000
*   ESTAEPL(1:ESTPLLEN)=ESTPLFMT(1:ESTPLLEN);/* INIT PARM LIST       */ 00122000
         L     @15,ESTPLLEN                                        0212 00123000
         BCTR  @15,0                                               0212 00124000
         EX    @15,@SM02520                                        0212 00125000
*   RFY                                                            0213 00126000
*     R15 RSTD;                     /* RESTRICT REG15                */ 00127000
*   ESTPTR=ADDR(SQAINFO);           /* PASS SQA GETMAIN INFO TO ESTAE*/ 00128000
         LA    ESTPTR,SQAINFO                                      0214 00129000
*/*          THE REASON FOR ISSUING PURGE=HALT                       */ 00130000
*/*      WHEN THE ESTAE EXIT IS TAKEN, ALL OUTSTANDING REQUESTS FOR  */ 00131000
*/*      I/O OPERATION WILL NOT BE SAVED.  AND I/O OPERATION WILL    */ 00132000
*/*       BE IMMEDIATELY STOP. THIS MAY CAUSE THE PAGE BEEN READ OR  */ 00133000
*/*      WRITTEN WITH GARBAGE IN IT.  A WARNING MESSAGE WILL BE      */ 00134000
*/*      ISSUED TO THE USER BY THE CALLER OF ILRPREAD TO INDICATE    */ 00135000
*/*      ABNORMAL TERMINATION HAS OCCURED AND SYSTEM CAN NOT BE      */ 00136000
*/*      WARM STARTED.                                               */ 00137000
*                                                                  0215 00138000
*   GEN REFS(ESTAEXIT);                                            0215 00139000
*                                     /*                                00140000
 ESTAE  ESTAEXIT,PARAM=(ESTPTR),TERM=YES,PURGE=HALT,                  **00141000
               RELATED=(ILRPREAD,'CANCEL ESTAE'),MF=(E,ESTAEPL)         00142000
*   IF R15^=SUCCESS THEN            /* ESTAE REQUEST FAIL?           */ 00143000
         LTR   R15,R15                                             0216 00144000
         BZ    @RF00216                                            0216 00145000
*     DO;                           /* YES                           */ 00146000
*       RTCODE=REQFAIL;             /* CAN NOT ESTABLISH ESTAE       */ 00147000
         LA    RTCODE,4                                            0218 00148000
*       GOTO FREE;                  /* RETURN TO CALLER              */ 00149000
         B     FREE                                                0219 00150000
*     END;                          /* END OF ESTAE FAIL             */ 00151000
*   ELSE                                                           0221 00152000
*     ;                             /* ESTAE REQUEST ESTABLISHED     */ 00153000
@RF00216 DS    0H                                                  0222 00154000
*   RFY                                                            0222 00155000
*     R15 UNRSTD;                   /* RELEASE REG 15                */ 00156000
*   IOSBPTR=SQAPTR;                 /* SET IOSB ADDRESS              */ 00157000
         L     @15,SQAPTR                                          0223 00158000
         LR    IOSBPTR,@15                                         0223 00159000
*   SRBPTR=SQAPTR+IOSBLEN;          /* SET SRB ADDRESS               */ 00160000
         ALR   @15,IOSBLEN                                         0224 00161000
         LR    SRBPTR,@15                                          0224 00162000
*   SEEKPTR=SRBPTR+SRBLEN;          /* PTR TO THE CCW SEEK ADDRESS   */ 00163000
         LR    SEEKPTR,SRBPTR                                      0225 00164000
         ALR   SEEKPTR,SRBLEN                                      0225 00165000
*   RFY                                                            0226 00166000
*     IDAEDB BASED(EDBPTR);         /* SET ADDRESS ABILITY TO EDB    */ 00167000
*   RFY                                                            0227 00168000
*     IDAEDBHD BASED(EDBHDPTR);     /* SET ADDR. ABILITY TO EDB HDR  */ 00169000
*   IF TPARB=ON THEN                /* TPARB IN USE?                 */ 00170000
         L     @15,INPTR                                           0228 00171000
         TM    TPARB(@15),B'01000000'                              0228 00172000
         BNO   @RF00228                                            0228 00173000
*     DO;                           /* YES                           */ 00174000
*       TPAEPTR=TABENTRY;           /* GET TPARTBLE ENTRY PTR        */ 00175000
         L     TPAEPTR,TABENTRY(,@15)                              0230 00176000
*       UCBPTR=TPAEUCB;             /* GET UCB PTR                   */ 00177000
         L     UCBPTR,TPAEUCB(,TPAEPTR)                            0231 00178000
*       EDBHDPTR=TPAEEDB;           /* GET EDB HEADER PTR            */ 00179000
         L     @15,TPAEEDB(,TPAEPTR)                               0232 00180000
         ST    @15,EDBHDPTR                                        0232 00181000
*     END;                                                         0233 00182000
*   ELSE                            /* PART IN USE                   */ 00183000
*     DO;                                                          0234 00184000
         B     @RC00228                                            0234 00185000
@RF00228 DS    0H                                                  0235 00186000
*       PAREPTR=TABENTRY;           /* GET PART ENTRY PTR            */ 00187000
         L     @15,INPTR                                           0235 00188000
         L     PAREPTR,TABENTRY(,@15)                              0235 00189000
*       UCBPTR=PAREUCBP;            /* GET PTR TO UCB                */ 00190000
         L     UCBPTR,PAREUCBP(,PAREPTR)                           0236 00191000
*       EDBHDPTR=PAREEDBP;          /* GET EDB HDR PTR               */ 00192000
         L     @15,PAREEDBP(,PAREPTR)                              0237 00193000
         ST    @15,EDBHDPTR                                        0237 00194000
*     END;                                                         0238 00195000
*                                                                  0239 00196000
*/********************************************************************/ 00197000
*/*                                                                  */ 00198000
*/* BLDIOSB: SEGMENT TO BUILD IOSB TO START I/O                      */ 00199000
*/*                                                                  */ 00200000
*/* INPUT:   IOSB FROM GETMAIN IN SQA                                */ 00201000
*/*                                                                  */ 00202000
*/* PROCESS: TO INITIALIZE IOSB FOR READ/WRITE THROUGH MISCELLANEOUS */ 00203000
*/*          DRIVER ID. A LOCAL LOCK IS REQUESTED FOR THIS DRIVER.   */ 00204000
*/*          THE NORMAL AND ABNORMAL END APPENDAGES,AND I/O          */ 00205000
*/*          TERMINATION ROUTINE WILL BE LOCATED BY IOSB.            */ 00206000
*/*                                                                  */ 00207000
*/* OUTPUT:  THE FOLLOWING FIELDS ARE INITIALIZED:                   */ 00208000
*/*          IOSASID          IOSDCHN           IOSRST               */ 00209000
*/*          IOSDVRID         IOSCCHN                                */ 00210000
*/*          IOSPRLVL         IOSUCB                                 */ 00211000
*/*          IOSPGAD          IOSVST                                 */ 00212000
*/*          IOSNRM           IOSUSE(ECB)                            */ 00213000
*/*          IOSABN           IOSFMSK                                */ 00214000
*/*                                                                  */ 00215000
*/********************************************************************/ 00216000
*/*BLDIOSB:    CHART (AREF)                                          */ 00217000
*/*HEADER                                                          0239 00218000
*/*  BUILD IOSB                                                      */ 00219000
*/*BLDIOSB:    E  BUILD IOSB                                         */ 00220000
*/*            P  FILL IN ASID                                       */ 00221000
*/*            P  SET DRIVER ID TO MISCELLANEOUS ID                  */ 00222000
*/*            P  SET PRIORITY LEVEL TO 4                            */ 00223000
*/*            P  SET TERMINATION ADDRESS                            */ 00224000
*/*            P  SET NORMAL APPENDAGE ADDRESS                       */ 00225000
*/*            P  SET ABNORMAL APPENDAGE ADDRESS                     */ 00226000
*/*            P  USE IOSUSE AS ECB                                  */ 00227000
*/*            P  LOAL LOCK IS REQUIRED                              */ 00228000
*/*            P  SET I/O FILE MASK                                  */ 00229000
*/*            P  COMMAND AND DATA CHAINING REQUIRED                 */ 00230000
*/*            P  SET UCB ADDRESS                                    */ 00231000
*/*            P  SET SRB ADDRESS                                    */ 00232000
*/*            R  END OF BUILDING IOSB                               */ 00233000
*/*BLDIOSB:    END                                                   */ 00234000
*                                                                  0239 00235000
*   ASCBPTR=PSAAOLD;                /* SET ADDRESSABILITY TO ASCB    */ 00236000
@RC00228 L     @15,PSAAOLD                                         0239 00237000
         ST    @15,ASCBPTR                                         0239 00238000
*   IOSASID=ASCBASID;               /* ASM'S ASID                    */ 00239000
         MVC   IOSASID(2,IOSBPTR),ASCBASID(@15)                    0240 00240000
*   IOSDVRID=IOSMISID;              /* MISCELLANEOUS DRIVER ID-'01'  */ 00241000
         MVI   IOSDVRID(IOSBPTR),X'01'                             0241 00242000
*   IOSPRLVL=NQUISCFG;              /* SET PRIORITY LEVEL TO 4       */ 00243000
         MVI   IOSPRLVL(IOSBPTR),X'04'                             0242 00244000
*   IOSPGAD=ADDR(PREADTRM);         /* SET TERMINATION ADDRESS       */ 00245000
         LA    @11,PREADTRM                                        0243 00246000
         ST    @11,IOSPGAD(,IOSBPTR)                               0243 00247000
*   IOSNRM=ADDR(PREADNRM);          /* SET NORMAL END APPENDAGE      */ 00248000
         LA    @11,PREADNRM                                        0244 00249000
         ST    @11,IOSNRM(,IOSBPTR)                                0244 00250000
*   IOSABN=ADDR(PREADABN);          /* SET ABNORMAL END APPENDAGE    */ 00251000
         LA    @11,PREADABN                                        0245 00252000
         ST    @11,IOSABN(,IOSBPTR)                                0245 00253000
*   IOSFMSK=FILEMASK;               /* PAGE SPACE FILE MASK          */ 00254000
         MVI   IOSFMSK(IOSBPTR),X'88'                              0246 00255000
*   IOSDCHN=ON;                     /* SET DATA CHAINING             */ 00256000
*   IOSCCHN=ON;                     /* SET COMMAND CHAINING          */ 00257000
         OI    IOSDCHN(IOSBPTR),B'11000000'                        0248 00258000
*   IOSUCB=UCBPTR;                  /* SET UCB ADDRESS               */ 00259000
         ST    UCBPTR,IOSUCB(,IOSBPTR)                             0249 00260000
*   IOSSRB=SRBPTR;                  /* SET SRB ADDRESS               */ 00261000
         ST    SRBPTR,IOSSRB(,IOSBPTR)                             0250 00262000
*                                                                  0251 00263000
*/********************************************************************/ 00264000
*/*                                                                  */ 00265000
*/* BLDSRB: SEGMENT TO BUILD SRB TO START I/O                        */ 00266000
*/*                                                                  */ 00267000
*/* INPUT:  SRB FROM GETMAIN IN SQA                                  */ 00268000
*/*                                                                  */ 00269000
*/* PROCESS: TO INITIALIZE SRB FOR SCHEDULE A READ/WRITE OPERATION.  */ 00270000
*/*          SRB WILL CONTAIN THE CURRENT TCB ADDRESS AND ASID FOR   */ 00271000
*/*          PURGE I/O WHENEVER AN ABNORMAL CONDITION OCCURRED.      */ 00272000
*/*                                                                  */ 00273000
*/* OUTPUT:  THE FOLLOWING FIELDS ARE INITIALIZED:                   */ 00274000
*/*                                                                  */ 00275000
*/*              SRBID        SRBPARM                                */ 00276000
*/*              SRBPRIOR     SRBPASID                               */ 00277000
*/*              SRBPTCB                                             */ 00278000
*/*                                                                  */ 00279000
*/*                                                                  */ 00280000
*/********************************************************************/ 00281000
*/*BLDSRB:     CHART (AREF)                                          */ 00282000
*/*HEADER                                                          0251 00283000
*/*  BUILD SRB                                                       */ 00284000
*/*BLDSRB:     E  BUILD SRB                                          */ 00285000
*/*            P  SET SRB ID                                         */ 00286000
*/*            P  SET ASID FOR PURGE                                 */ 00287000
*/*            P  SET TCB ADDRESS FOR PURGE                          */ 00288000
*/*            P  SET IOSB ADDRESS IN SRB PARAMETER                  */ 00289000
*/*            P  SET PRIORITY LEVEL TO FOUR                         */ 00290000
*/*            R  END OF BUILDING SRB                                */ 00291000
*/*BLDSRB:     END                                                   */ 00292000
*                                                                  0251 00293000
*   SRBID=SRBIDCHR;                 /* SET SRB ID FIELD              */ 00294000
         MVC   SRBID(4,SRBPTR),SRBIDCHR                            0251 00295000
*   SRBPASID=ASCBASID;              /* SET ASM'S ASID                */ 00296000
         LH    @15,ASCBASID(,@15)                                  0252 00297000
         N     @15,@CF02522                                        0252 00298000
         STH   @15,SRBPASID(,SRBPTR)                               0252 00299000
*   SRBPTCB=PSATOLD;                /* SET TCB ADDRESS               */ 00300000
         L     @15,PSATOLD                                         0253 00301000
         ST    @15,SRBPTCB(,SRBPTR)                                0253 00302000
*   SRBPARM=IOSBPTR;                /* ASSOCIATE IOSB TO SRB         */ 00303000
         ST    IOSBPTR,SRBPARM(,SRBPTR)                            0254 00304000
*   SRBPRIOR=NQUISCFG;              /* NON-QUIESCEABLE PRIORITY      */ 00305000
         MVI   SRBPRIOR(SRBPTR),X'04'                              0255 00306000
*                                                                  0256 00307000
*/********************************************************************/ 00308000
*/*                                                                  */ 00309000
*/*  BLDCP: BUILD CHANNEL PROGRAM TO READ/WRITE REQUESTED SLOTS      */ 00310000
*/*                                                                  */ 00311000
*/*  INPUT: FRTSLOT-FIRST SLOT NUMBER                                */ 00312000
*/*         TOTSLOTS-TOTAL NUMBER OF SLOTS TO BE READ/WRITTEN        */ 00313000
*/*                                                                  */ 00314000
*/*  PROCESS: TO BUILD CHANNEL PROGRAM TO READ AND WRITE RESQUESTD   */ 00315000
*/*           SLOTS. THE MAXIMUM NUMBER OF CCWS CAN  NOT EXCEED TO   */ 00316000
*/*           READ OR WRITE MORE THAN 10 SLOTS OR ACCROSS            */ 00317000
*/*           CYLINDER BOUNDARY                                      */ 00318000
*/*                                                                  */ 00319000
*/*  OUTPUT: A CHANNEL PROGRAM WILL BE BUILT TO READ OR WRITE        */ 00320000
*/*          REQUESTED SLOTS                                         */ 00321000
*/*                                                                  */ 00322000
*/*                                                                  */ 00323000
*/********************************************************************/ 00324000
*/*BUILDCP:        CHART (AREF)                                      */ 00325000
*/*HEADER                                                          0256 00326000
*/*  BUILD CHANNEL PROGRAM                                           */ 00327000
*/*BUILDCP:        E BUILDCP                                         */ 00328000
*/*                P CALCULATE RBA FOR  REQUESTED SLOT               */ 00329000
*/*                S CONVERT: CONVERT RBA TO MBBCCHHR                */ 00330000
*/*                D (YES,,NO,%CONERR) CONVERT OK?                   */ 00331000
*/*                P SET SEEK ADDRESS IN IOSB                        */ 00332000
*/*%BLDCP:         P SET UP SEEK HEAD IN CCW                         */ 00333000
*/*                P SET UP SEARCH ID EQUAL                          */ 00334000
*/*                P SET UP TIC CCW                                  */ 00335000
*/*                D (READ,,WRIT,%WRCCW) READ OR WRITE REQUESTED?    */ 00336000
*/*                P (,%SETBUF) SET UP READ OP CODE IN CCW           */ 00337000
*/*%WRCCW:         P SET UP WRITE OP CODE IN CCW                     */ 00338000
*/*%SETBUF:        P GET BUFFER REAL ADDRESS                         */ 00339000
*/*                P SET UP READ/WRITE CCW                           */ 00340000
*/*                P DECREMENT COUNT OF REQUESTS                     */ 00341000
*/*                D (YES,,NO,%ENDCP) MORE REQUESTS TO PROCESS?      */ 00342000
*/*                P CALCULATE RBA FOR NEXT REQUESTED SLOT           */ 00343000
*/*                S CONVERT: CONVERT RBA TO MBBCCHHR                */ 00344000
*/*                D (YES,,NO,%CONERR) CONVERT OK?                   */ 00345000
*/*                D (YES,%ENDCP,NO,) PROCESS MORE THAN 10 SLOTS?    */ 00346000
*/*                D (YES,%BLDCP,NO,) REQUESTED SLOT ON SAME CYL?    */ 00347000
*/*%ENDCP:         P TURN OFF CHAIN BIT IN THE LAST CCW              */ 00348000
*/*                R END OF BUILD CHANNEL PROGRAM                    */ 00349000
*/*%CONERR:        P SET CONVERT ERROR                               */ 00350000
*/*                R END OF BUILD CHANNEL PROGRAM                    */ 00351000
*/*BUILDCP:        END                                               */ 00352000
*                                                                  0256 00353000
*   REQUESTS=TOTSLOTS;              /* TOTAL NUMBER OF REQUESTED I/O */ 00354000
         L     @15,INPTR                                           0256 00355000
         SLR   @11,@11                                             0256 00356000
         IC    @11,TOTSLOTS(,@15)                                  0256 00357000
         ST    @11,REQUESTS                                        0256 00358000
*   SLOT=FRTSLOT;                   /* GET FIRST REQUESTED SLOT      */ 00359000
         LH    @15,FRTSLOT(,@15)                                   0257 00360000
         ST    @15,SLOT                                            0257 00361000
*   CYLBREAK=OFF;                   /* INIT CHANNEL PGM NOT BUILT    */ 00362000
         NI    CYLBREAK,B'01111111'                                0258 00363000
*   DO WHILE(REQUESTS^=ZERO&RTCODE=ZERO);/* PROCESS ALL I/O REQS     */ 00364000
         B     @DE00259                                            0259 00365000
@DL00259 DS    0H                                                  0260 00366000
*     SRHPTR=SEEKPTR+LENGTH(SEEKADDR);/* SET SEARCH ADDR PTR         */ 00367000
         LA    SRHPTR,8                                            0260 00368000
         ALR   SRHPTR,SEEKPTR                                      0260 00369000
*     CCWPTR=SRHPTR+MAXSLOTS*LENGTH(SRHADDR);/* SET CCWS PTR         */ 00370000
         LA    CCWPTR,80                                           0261 00371000
         ALR   CCWPTR,SRHPTR                                       0261 00372000
*     SRHAREA=''B;                  /* INIT SEARCH ADDRESSES AREA    */ 00373000
         XC    SRHAREA(80,SRHPTR),SRHAREA(SRHPTR)                  0262 00374000
*     IOSVST=CCWPTR;                /* SET CHANNEL PGM VIRTUAL ADDR  */ 00375000
         ST    CCWPTR,IOSVST(,IOSBPTR)                             0263 00376000
*     R1=CCWPTR;                    /* SET INDEX REG FOR CCWS        */ 00377000
         LR    R1,CCWPTR                                           0264 00378000
*     RFY                                                          0265 00379000
*       R5 RSTD;                    /* RESTRICT REG 5                */ 00380000
*     GEN REFS(CCWS,R1,R5,ADDRERR) SETS(R5);                       0266 00381000
*                                            /*                         00382000
    LRA  R5,CCWS(R1)                                                    00383000
    BC   7,ADDRERR                                                      00384000
*     IOSRST=R5;                    /* SET CHANNEL PGM REAL ADDR     */ 00385000
         ST    R5,IOSRST(,IOSBPTR)                                 0267 00386000
*     RFY                                                          0268 00387000
*       R5 UNRSTD;                  /* RELEASE REG 5                 */ 00388000
*     IF CYLBREAK=OFF THEN          /* CHANNEL PGM BUILT?            */ 00389000
         TM    CYLBREAK,B'10000000'                                0269 00390000
         BNZ   @RF00269                                            0269 00391000
*       DO;                         /* NO                            */ 00392000
*         SEEKADDR=''B;             /* INIT SEEK ADDR-MBBCCHHR       */ 00393000
         XC    SEEKADDR(8,SEEKPTR),SEEKADDR(SEEKPTR)               0271 00394000
*         SEEKRBA=SLOT*PAGESIZE;    /* CALCULATE RBA                 */ 00395000
         L     @15,SLOT                                            0272 00396000
         SLA   @15,12                                              0272 00397000
