         TITLE 'ILRQSRIT-QSR INITIALIZATION ROUTINE                    *00001000
                        '                                               00002000
*                                                                       00003000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00004000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00005000
* WILL ASSEMBLE PROPERLY.                                               00006000
*                                                      /*               00007000
ILRQSRIT  CSECT                                                         00008000
          USING *,R15                                                   00009000
          B PROLOG                                                      00010000
          MODID  BR=NO                                                  00011000
PROLOG    DS  0H                                                        00012000
          STM R0,R15,NIP05SAV                                           00013000
          DROP R15                                                      00014000
          BALR  R12,0                                                   00015000
          USING *,R12                                                   00016000
*   R13=ADDR(QSRSAVE);              /* SET R13 POINTS TO QSRIT SAVE  */ 00017000
         LA    R13,QSRSAVE                                         0115 00018000
*/*SET VIRTUAL=REAL                                                  */ 00019000
*                                                                  0116 00020000
*   RFY                                                            0116 00021000
*    (RENTRY,                                                      0116 00022000
*     REXIT) RSTD;                  /* RESTRICT REG 14 AND REG 15    */ 00023000
*   GEN REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)(IEAPMNIP  TYPE=REAL);     00024000
         IEAPMNIP  TYPE=REAL                                            00025000
*   RFY                                                            0118 00026000
*    (RENTRY,                                                      0118 00027000
*     REXIT) UNRSTD;                /* RELEASE REG14 AND REG 15      */ 00028000
*   QSRSAVE=''B;                    /* INIT QSRIT SAVE AREA          */ 00029000
         XC    QSRSAVE(72),QSRSAVE                                 0119 00030000
*   MSGLIST=''B;                    /* INIT MSG PARAMETER LIST       */ 00031000
         XC    MSGLIST(24),MSGLIST                                 0120 00032000
*   NVTPR=R2;                       /* SAVE NVT PTR                  */ 00033000
         ST    R2,NVTPR                                            0121 00034000
*   CVTPR=R3;                       /* SAVE CVT PTR IN MESSAGE PARAM    00035000
*                                      LIST                          */ 00036000
         ST    R3,CVTPR                                            0122 00037000
*   CVTPTR=R3;                      /* SET PTR TO CVT TABLE          */ 00038000
         ST    R3,CVTPTR                                           0123 00039000
*   PREADLST=''B;                   /* INIT PREAD PARMETER LIST      */ 00040000
         XC    PREADLST(12),PREADLST                               0124 00041000
*   ASMVTPTR=CVTASMVT;              /* SET ASMVT PTR                 */ 00042000
         L     @04,CVTPTR                                          0125 00043000
         L     ASMVTPTR,CVTASMVT(,@04)                             0125 00044000
*   PARTPTR=ASMPART;                /* SET PART PTR                  */ 00045000
         L     PARTPTR,ASMPART(,ASMVTPTR)                          0126 00046000
*   TPARPTR=PARTTPAR;               /* SET TPARB PTR                 */ 00047000
         L     @04,PARTTPAR(,PARTPTR)                              0127 00048000
         ST    @04,TPARPTR                                         0127 00049000
*   QSRPTR=NVTQSBUF;                /* SET QSR PTR                   */ 00050000
         L     QSRPTR,NVTQSBUF(,RNVT)                              0128 00051000
*   QSRLPAHA=NVTLPAHA;              /* SET PLPA HASH IN QSR          */ 00052000
         L     @04,NVTLPAHA(,RNVT)                                 0129 00053000
         ST    @04,QSRLPAHA(,QSRPTR)                               0129 00054000
*   QSRLPALO=NVTLPALO;              /* SET PLPA LOW ADDR IN QSR      */ 00055000
         L     @04,NVTLPALO(,RNVT)                                 0130 00056000
         ST    @04,QSRLPALO(,QSRPTR)                               0130 00057000
*   QSRLPAHI=NVTLPAND;              /* SET PLPA HIGH ADDR IN QSR     */ 00058000
         L     @04,NVTLPAND(,RNVT)                                 0131 00059000
         ST    @04,QSRLPAHI(,QSRPTR)                               0131 00060000
*   IF ASMPLPAF=ON THEN             /* CHECK FOR PLPA FULL           */ 00061000
         TM    ASMPLPAF(ASMVTPTR),B'00000100'                      0132 00062000
         BNO   @RF00132                                            0132 00063000
*     DO;                                                          0133 00064000
*       QSRPLPAF=ON;                /* YES,SET SW IN QSR             */ 00065000
         OI    QSRPLPAF(QSRPTR),B'10000000'                        0134 00066000
*       ASMPLPAS=ON;                /* MARK PLPA SPILL IN ASMVT      */ 00067000
         OI    ASMPLPAS(ASMVTPTR),B'00000001'                      0135 00068000
*     END;                                                         0136 00069000
*   IF ASMCOMMF=ON THEN             /* CHECK FOR COMMON DATA SET FULL*/ 00070000
@RF00132 TM    ASMCOMMF(ASMVTPTR),B'00000010'                      0137 00071000
         BNO   @RF00137                                            0137 00072000
*     QSRCOMMF=ON;                  /* YES,SET SW IN QSR             */ 00073000
         OI    QSRCOMMF(QSRPTR),B'01000000'                        0138 00074000
*   RFY                                                            0139 00075000
*    (R0,                                                          0139 00076000
*     R14,                                                         0139 00077000
*     R15) RSTD;                    /* RESTRICT REG 0,14,15          */ 00078000
@RF00137 DS    0H                                                  0140 00079000
*   R0=ADDR(QSRMAP);                /* SET ADDR OF QSR LSID MAP      */ 00080000
         LA    R0,QSRMAP(,QSRPTR)                                  0140 00081000
*   R1=LENGTH(QSRMAP);              /* SET LENGTH OF QSR LSID MAP    */ 00082000
         L     R1,@CF00301                                         0141 00083000
*   R15=ZERO;                       /* SET PADDING CHAR ZERO         */ 00084000
         SLR   R15,R15                                             0142 00085000
*   MVCL(R0,R14);                   /* ZERO QSR LSID MAP AREA        */ 00086000
         MVCL  R0,R14                                              0143 00087000
*   RFY                                                            0144 00088000
*    (R0,                                                          0144 00089000
*     R14,                                                         0144 00090000
*     R15) UNRSTD;                  /* RELEASE REG 0,14,15           */ 00091000
*                                                                  0145 00092000
*/********************************************************************/ 00093000
*/*                                                                  */ 00094000
*/* MAIN ROUTINE                                                     */ 00095000
*/*                                                                  */ 00096000
*/* FUNCTION - IT COPIES LSIDS FROM XPTE TO QSRCD FOR ALL THE PAGES  */ 00097000
*/*            IN PLPA.                                              */ 00098000
*/*                                                                  */ 00099000
*/********************************************************************/ 00100000
*                                                                  0145 00101000
*   LOWPAGE=QSRLPALO;               /* GET PLPA LOW ADDR             */ 00102000
         L     @04,QSRLPALO(,QSRPTR)                               0145 00103000
         LR    LOWPAGE,@04                                         0145 00104000
*   SLL(LOWPAGE,SIXTEEN);           /* SHIFT OUT SEG INDEX           */ 00105000
         SLL   LOWPAGE,16                                          0146 00106000
*   SRL(LOWPAGE,TWENEIGT);          /* SHIFT OUT BYTE INDEX          */ 00107000
         SRL   LOWPAGE,28                                          0147 00108000
*   PAGENO=SIXTEEN-LOWPAGE;         /* OBTAIN NO OF PAGES FOR PLPA 0148 00109000
*                                      LOW                           */ 00110000
         LA    PAGENO,16                                           0148 00111000
         SLR   PAGENO,LOWPAGE                                      0148 00112000
*   SEGADDR=QSRLPALO;               /* GET PLPA LOW ADDR             */ 00113000
         ST    @04,SEGADDR                                         0149 00114000
*   GEN REFS(SEGADDR,SEGMASK) SETS(SEGADDR);                       0150 00115000
*                                         /*                            00116000
      NC   SEGADDR,SEGMASK                                              00117000
*   LOWPLPA=ON;                     /* SET PROCESS LOW PLPA SEG SW   */ 00118000
         OI    LOWPLPA,B'10000000'                                 0151 00119000
*   QSREPTR=ADDR(QSRMAP);           /* SET PTR TO FIRST ENTRY IN QSR */ 00120000
         LA    QSREPTR,QSRMAP(,QSRPTR)                             0152 00121000
*   DO WHILE(SEGADDR<QSRLPAHI);     /* PROCESS ALL PAGES IN PIPA     */ 00122000
         B     @DE00153                                            0153 00123000
@DL00153 DS    0H                                                  0154 00124000
*     R1=SEGADDR;                   /* PASS VIRTUAL ADDR TO FIND PAGE*/ 00125000
         L     R1,SEGADDR                                          0154 00126000
*     RFY                                                          0155 00127000
*       R15 RSTD;                   /* RESTRICT REG 15               */ 00128000
*     CALL IEAVFP1;                 /* GET EXTERNAL PAGE TABLE       */ 00129000
         L     @04,CVTPTR                                          0156 00130000
         L     @15,CVTVFP(,@04)                                    0156 00131000
         BALR  @14,@15                                             0156 00132000
*     RETCODE=R15;                  /* SAVE RETURN CODE              */ 00133000
         LR    RETCODE,R15                                         0157 00134000
*     RFY                                                          0158 00135000
*       R15 UNRSTD;                 /* RELEASE R15                   */ 00136000
*     IF RETCODE^=ZERO THEN         /* XPTE FOUND:                   */ 00137000
         LTR   RETCODE,RETCODE                                     0159 00138000
         BZ    @RF00159                                            0159 00139000
*       DO;                         /* NO                            */ 00140000
*         MSGID=MSG943W;            /* SET MSG ID-IEA943W            */ 00141000
         MVC   MSGID(4),@CC01323                                   0161 00142000
*         R1=ADDR(MSGLIST);         /* PASS PARAMETER LIST           */ 00143000
         LA    R1,MSGLIST                                          0162 00144000
*         CALL ILRIMMSG;            /* ISSUE MSG                     */ 00145000
         L     @15,@CV01273                                        0163 00146000
         BALR  @14,@15                                             0163 00147000
*       END;                        /* END OF ISSUE MSG              */ 00148000
*     ELSE                          /* XPTE FOUND                    */ 00149000
*       DO;                                                        0165 00150000
         B     @RC00159                                            0165 00151000
@RF00159 DS    0H                                                  0166 00152000
*         IF LOWPLPA=ON THEN        /* PROCESS LOWEST PAGE ADDR?     */ 00153000
         TM    LOWPLPA,B'10000000'                                 0166 00154000
         BNO   @RF00166                                            0166 00155000
*           DO;                                                    0167 00156000
*             LOWPLPA=OFF;          /* MARK LOWEST PAGES PROCESSED   */ 00157000
         NI    LOWPLPA,B'01111111'                                 0168 00158000
*             XPTEPTR=R1+LOWPAGE*LENGTH(XPTE);/* SKIP PAGES BELOW  0169 00159000
*                                      PLPALOW                       */ 00160000
         LR    XPTEPTR,LOWPAGE                                     0169 00161000
         MH    XPTEPTR,@CH01351                                    0169 00162000
         ALR   XPTEPTR,R1                                          0169 00163000
*           END;                    /* END OF SETING NEXT SEG        */ 00164000
*         ELSE                                                     0171 00165000
*           XPTEPTR=R1;             /* NO-SET PTR TO FIRST XPTE      */ 00166000
         B     @RC00166                                            0171 00167000
@RF00166 LR    XPTEPTR,R1                                          0171 00168000
*         DO I=ONE TO PAGENO;       /* PROCESS XPTE ENTRY IN A SEG   */ 00169000
@RC00166 LA    I,1                                                 0172 00170000
         B     @DE00172                                            0172 00171000
@DL00172 DS    0H                                                  0173 00172000
*           QSRENTRY=XPTLPID;       /* COPY LSID OF XPTE TO QSR      */ 00173000
         MVC   QSRENTRY(8,QSREPTR),XPTLPID(XPTEPTR)                0173 00174000
*           XPTEPTR=XPTEPTR+LENGTH(XPTE);/* GET NEXT XPTE ENTRY      */ 00175000
         AL    XPTEPTR,@CF01351                                    0174 00176000
*           QSREPTR=QSREPTR+LENGTH(QSRENTRY);/* GET NEXT QSR ENTRY   */ 00177000
         AL    QSREPTR,@CF00038                                    0175 00178000
*         END;                      /* END OF PROCESS TOTAL PAGES    */ 00179000
         AL    I,@CF00122                                          0176 00180000
@DE00172 CR    I,PAGENO                                            0176 00181000
         BNH   @DL00172                                            0176 00182000
*         SEGADDR=SEGADDR+SEGINCMT; /* GET NEXT SEG BOUNDARY         */ 00183000
         L     @09,@CF01319                                        0177 00184000
         AL    @09,SEGADDR                                         0177 00185000
         ST    @09,SEGADDR                                         0177 00186000
*         PAGENO=SIXTEEN;           /* SET PAGE NUMBER IN A SEG      */ 00187000
         LA    PAGENO,16                                           0178 00188000
*       END;                        /* END OF PROCESS FOUND XPTE     */ 00189000
*   END;                            /* END OF PROCESS EACH SEG PAGES    00190000
*                                      IN PLPA                       */ 00191000
@RC00159 DS    0H                                                  0180 00192000
@DE00153 CLC   SEGADDR(4),QSRLPAHI(QSRPTR)                         0180 00193000
         BL    @DL00153                                            0180 00194000
*                                                                  0181 00195000
*/********************************************************************/ 00196000
*/* SEGMENT NAME-QSRTERM                                             */ 00197000
*/*                                                                  */ 00198000
*/* DESCRIPTIVE NAME-ILRQSRIT TERMINATION ROUTINE                    */ 00199000
*/*                                                                  */ 00200000
*/* FUNCTION-WRITE TPARB AND QSRCD TO PLPA DATA SET                  */ 00201000
*/*          AND MARKS THE PLPA DATASET READ-ONLY AND COMMON DATASET */ 00202000
*/*          ACTIVE                                                  */ 00203000
*/*                                                                  */ 00204000
*/********************************************************************/ 00205000
*/* PARENUSE FLAG NEED NOT BE CONSIDERED FOR DATASET BAD CONDITIONS  */ 00206000
*/* BECAUSE RECOVERY SETS THAT FLAG AND RECOVERY IS NOT BEEN INVOKED */ 00207000
*/* THIS EARLY TIME IN NIP                                           */ 00208000
*                                                                  0181 00209000
*   IF ADDR(PARTENTS(PARTPLPN+ONE))->PAREDSBD=OFF THEN/* PLAP NOT BAD*/ 00210000
         LA    @04,PARTENTS(,PARTPTR)                              0181 00211000
         TM    PAREDSBD(@04),B'01000000'                           0181 00212000
         BNZ   @RF00181                                            0181 00213000
*     IF ADDR(PARTENTS(PARTCOMN+ONE))->PAREDSBD=ON THEN/* COMN BAD   */ 00214000
         LA    @15,PARTENTS+64(,PARTPTR)                           0182 00215000
         TM    PAREDSBD(@15),B'01000000'                           0182 00216000
         BNO   @RF00182                                            0182 00217000
*       ADDR(PARTENTS(PARTPLPN+ONE))->PAREWTQE=ADDR(PARTCOMQ);/*   0183 00218000
*                                      COMMON REQUESTS ARE WRITTEN TO   00219000
*                                      PLPA DATASET                  */ 00220000
         LA    @00,PARTCOMQ(,PARTPTR)                              0183 00221000
         ST    @00,PAREWTQE(,@04)                                  0183 00222000
*     ELSE                          /* COMMON DATASET GOOD           */ 00223000
*       ADDR(PARTENTS(PARTPLPN+ONE))->PAREWTQE=ADDR(PARTSPLQ);/* SET    00224000
*                                      PTR TO SPILL QUEUE FOR COMN 0184 00225000
*                                      FULL                          */ 00226000
         B     @RC00182                                            0184 00227000
@RF00182 LA    @04,PARTENTS(,PARTPTR)                              0184 00228000
         LA    @00,PARTSPLQ(,PARTPTR)                              0184 00229000
         ST    @00,PAREWTQE(,@04)                                  0184 00230000
*   ELSE                                                           0185 00231000
*     ;                             /* PLPA BAD                      */ 00232000
@RF00181 DS    0H                                                  0186 00233000
*   IF ADDR(PARTENTS(PARTCOMN+ONE))->PAREDSBD=OFF THEN/* COMMON GOOD */ 00234000
@RC00181 LA    @04,PARTENTS+64(,PARTPTR)                           0186 00235000
         TM    PAREDSBD(@04),B'01000000'                           0186 00236000
         BNZ   @RF00186                                            0186 00237000
*     ADDR(PARTENTS(PARTCOMN+ONE))->PAREWTQE=ADDR(PARTCOMQ);/* COMN     00238000
*                                      REQUESTS ARE WRITTEN TO COMNON   00239000
*                                      DATASET                       */ 00240000
         LA    @00,PARTCOMQ(,PARTPTR)                              0187 00241000
         ST    @00,PAREWTQE(,@04)                                  0187 00242000
*   IF ADDR(PARTENTS(PARTPLPN+ONE))->PAREDSBD=OFF|/* PLPA GOOD       */ 00243000
*       ADDR(PARTENTS(PARTCOMN+ONE))->PAREDSBD=OFF THEN/* COMN GOOD  */ 00244000
@RF00186 LA    @04,PARTENTS(,PARTPTR)                              0188 00245000
         TM    PAREDSBD(@04),B'01000000'                           0188 00246000
         BZ    @RT00188                                            0188 00247000
         LA    @04,PARTENTS+64(,PARTPTR)                           0188 00248000
         TM    PAREDSBD(@04),B'01000000'                           0188 00249000
         BNZ   @RF00188                                            0188 00250000
@RT00188 DS    0H                                                  0189 00251000
*     ASMNOCWQ=OFF;                 /* ZERO NO WRITE QUEUE FOR COMN     00252000
*                                      DATASET                       */ 00253000
         NI    ASMNOCWQ(ASMVTPTR),B'10111111'                      0189 00254000
*   ELSE                                                           0190 00255000
*     ;                             /* PLPA AND COMN BOTH BAD        */ 00256000
@RF00188 DS    0H                                                  0191 00257000
*   ACTION=ON;                      /* WRITE TPARB AND QSRCD         */ 00258000
*   TABLE=ON;                       /* TPARB IN USE                  */ 00259000
         OI    ACTION,B'11000000'                                  0192 00260000
*   SLOTNO=SIX;                     /* WRITE 6 SLOTS                 */ 00261000
         MVI   SLOTNO,X'06'                                        0193 00262000
*   FIRSTSLT=ZERO;                  /* SET FIRST SLOT NUMBER TO WRITE*/ 00263000
         SLR   @00,@00                                             0194 00264000
         STH   @00,FIRSTSLT                                        0194 00265000
*   TABLEPTR=ADDR(TPARPENT);        /* SET PTR TO PLPA IN TPARTBLE   */ 00266000
         L     @15,TPARPTR                                         0195 00267000
         LA    @00,TPARPENT(,@15)                                  0195 00268000
         ST    @00,TABLEPTR                                        0195 00269000
*   BUFFRPTR=TPARPTR;               /* SET BUFFER PTR TO TPARTBLE AND   00270000
*                                      QSRCD                         */ 00271000
         ST    @15,BUFFRPTR                                        0196 00272000
*   DO;                             /* LOAD EP('ILRPREAD')           */ 00273000
*     RESPECIFY                                                    0198 00274000
*      (GPR00P,                                                    0198 00275000
*       GPR01P) RESTRICTED;                                        0198 00276000
*     GPR00P=ADDR('ILRPREAD');      /* LOAD ENTRY POINT NAME ADDRESS */ 00277000
         LA    GPR00P,@CC01335                                     0199 00278000
*     GPR01P=0;                     /* JOBLIB OR LINKLIB DCB         */ 00279000
         SLR   GPR01P,GPR01P                                       0200 00280000
*     SVC(8);                       /* ISSUE LOAD SVC                */ 00281000
         SVC   8                                                   0201 00282000
*     RESPECIFY                                                    0202 00283000
*      (GPR00P,                                                    0202 00284000
*       GPR01P) UNRESTRICTED;                                      0202 00285000
*   END;                            /* LOAD EP('ILRPREAD') LOAD    0203 00286000
*                                      ILRPREAD                      */ 00287000
*   RFY                                                            0204 00288000
*     R0 RSTD;                      /* RESTRICT REG 0                */ 00289000
*   PREADPTR=R0;                    /* SET ILRPREAD ENTRY POINT      */ 00290000
         LR    PREADPTR,R0                                         0205 00291000
*   RFY                                                            0206 00292000
*     R0 UNRSTD;                    /* RELEASE REG 0                 */ 00293000
*   R1=ADDR(PREADLST);              /* GET ADDR OF PARAMETER LIST    */ 00294000
         LA    R1,PREADLST                                         0207 00295000
*   RFY                                                            0208 00296000
*     R15 RSTD;                     /* RESTRICTED R15                */ 00297000
*   CALL ILRPREAD;                  /* CALL READ/WRITE ROUTINE       */ 00298000
         LR    @15,PREADPTR                                        0209 00299000
         BALR  @14,@15                                             0209 00300000
*   RETCODE=R15;                    /* SAVE RETURN CODE              */ 00301000
         LR    RETCODE,R15                                         0210 00302000
*   RFY                                                            0211 00303000
*     R15 UNRSTD;                   /* RELEASE R15                   */ 00304000
*   IF RETCODE^=ZERO THEN           /* WRITE SUCCESSFUL?             */ 00305000
         LTR   RETCODE,RETCODE                                     0212 00306000
         BZ    @RF00212                                            0212 00307000
*     DO;                           /* NO                            */ 00308000
*       ASMNOTPT=ON;                /* MARK BAD TPART                */ 00309000
         OI    ASMNOTPT(ASMVTPTR),B'00010000'                      0214 00310000
*       MSGID=MSG936I;              /* SET MSG ID                    */ 00311000
         MVC   MSGID(4),@CC01325                                   0215 00312000
*       R1=ADDR(MSGLIST);           /* PASS PARAMETER LIST           */ 00313000
         LA    R1,MSGLIST                                          0216 00314000
*       CALL ILRIMMSG;              /* ISSUE ERR MSG                 */ 00315000
         L     @15,@CV01273                                        0217 00316000
         BALR  @14,@15                                             0217 00317000
*     END;                          /* END OF ISSUE ERR MSG          */ 00318000
*   ELSE                                                           0219 00319000
*     ;                             /* RETURN FROM WRITE SUCCESSFUL  */ 00320000
@RF00212 DS    0H                                                  0220 00321000
*   DO;                             /* DELETE EP('ILRPREAD')         */ 00322000
*     RESPECIFY                                                    0221 00323000
*      (GPR00P) RESTRICTED;                                        0221 00324000
*     GPR00P=ADDR('ILRPREAD');      /* ->ENTRY NAME                  */ 00325000
         LA    GPR00P,@CC01335                                     0222 00326000
*     SVC(9);                       /* DELETE SVC                    */ 00327000
         SVC   9                                                   0223 00328000
*     RESPECIFY                                                    0224 00329000
*      (GPR00P) UNRESTRICTED;                                      0224 00330000
*   END;                            /* DELETE EP('ILRPREAD')         */ 00331000
*   DO;                             /* FREEMAIN(RU)                0226 00332000
*                                      LV(LENGTH(QSR))A(QSR) SP(245) */ 00333000
*     RESPECIFY                                                    0227 00334000
*      (GPR01F,                                                    0227 00335000
*       GPR15F,                                                    0227 00336000
*       GPR00F) RESTRICTED;                                        0227 00337000
*     GPR01F=0;                     /* REG 1 MUST BE ZERO            */ 00338000
         SLR   GPR01F,GPR01F                                       0228 00339000
*     GPR15F=3;                     /* SET TYPE                      */ 00340000
         LA    GPR15F,3                                            0229 00341000
*     GPR00F=LENGTH(QSR);           /* LENGTH REQUESTED              */ 00342000
         L     GPR00F,@CF01352                                     0230 00343000
*     GPR01F=ADDR(QSR);             /* SET REGISTER ONE              */ 00344000
         LR    GPR01F,QSRPTR                                       0231 00345000
*     GPR15F=GPR15F+((245)*256);    /* SUBPOOL VALUE                 */ 00346000
         AL    GPR15F,@CF01353                                     0232 00347000
*     SVC(120);                     /* RC/RU-FORM OF GETMAIN         */ 00348000
         SVC   120                                                 0233 00349000
*     RESPECIFY                                                    0234 00350000
*      (GPR01F,                                                    0234 00351000
*       GPR15F,                                                    0234 00352000
*       GPR00F) UNRESTRICTED;                                      0234 00353000
*   END;                            /* FREEMAIN(RU)                0235 00354000
*                                      LV(LENGTH(QSR))A(QSR) SP(245) */ 00355000
*   NVTQSBUF=ZERO;                  /* ZERO QSR PTR IN NVT           */ 00356000
         SLR   @00,@00                                             0236 00357000
         ST    @00,NVTQSBUF(,RNVT)                                 0236 00358000
*/*SET VIRTUAL=VIRTUAL                                               */ 00359000
*                                                                  0237 00360000
*   RFY                                                            0237 00361000
*    (RENTRY,                                                      0237 00362000
*     REXIT) RSTD;                  /* RESTRICT REG 14 AND REG 15    */ 00363000
*   GEN REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)(IEAPMNIP  TYPE=VIRT);     00364000
         IEAPMNIP  TYPE=VIRT                                            00365000
*   RFY                                                            0239 00366000
*    (RENTRY,                                                      0239 00367000
*     REXIT) UNRSTD;                /* RELEASE REG14 AND REG15       */ 00368000
*   LM(R0,R15,NIP05SAV);            /* RESTORE REG0-REG15 FOR NP05   */ 00369000
         LM    R0,R15,NIP05SAV                                     0240 00370000
*   RETURN;                         /* RETURN TO CALLER              */ 00371000
@EL00001 DS    0H                                                  0241 00372000
@EF00001 DS    0H                                                  0241 00373000
@ER00001 BR    @14                                                 0241 00374000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00375000
*     GPR00F FIXED(31) REG(0),                                     0242 00376000
*     GPR01F FIXED(31) REG(1),                                     0242 00377000
*     GPR15F FIXED(31) REG(15),                                    0242 00378000
*     GPR00P PTR(31) REG(0),                                       0242 00379000
*     GPR01P PTR(31) REG(1);                                       0242 00380000
*   DECLARE                         /* COMMON VARIABLES              */ 00381000
*     I256C CHAR(256) BASED,                                       0243 00382000
*     I031F FIXED(31) BASED,                                       0243 00383000
*     I031P PTR(31) BASED,                                         0243 00384000
*     I015F FIXED(15) BASED,                                       0243 00385000
*     I015P PTR(15) BASED,                                         0243 00386000
*     I008P PTR(8) BASED,                                          0243 00387000
*     I001C CHAR(1) BASED;                                         0243 00388000
*   END ILRQSRIT                                                   0244 00389000
