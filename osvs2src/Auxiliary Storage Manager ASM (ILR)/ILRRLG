         TITLE 'ILRRLG - RELEASE LOGICAL GROUP EXECUTIVE               *00001000
                        '                                               00002000
ILRRLG   CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRRLG    76.315'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@10                                         0001 00013000
*                                                                  0059 00014000
*/********************************************************************/ 00015000
*/*                                                                  */ 00016000
*/*                           INITIALIZATION                         */ 00017000
*/*                                                                  */ 00018000
*/*  THE FOLLOWING POINTERS ARE INPUT TO THIS MODULE FROM            */ 00019000
*/*  THE CALLER.                                                     */ 00020000
*/*       REGISTER 1 = @ACE                                          */ 00021000
*/*       REGISTER 2 = @RSMHD                                        */ 00022000
*/*       REGISTER 3 = @ASMVT                                        */ 00023000
*/*       REGISTER 4 = @EPATH                                        */ 00024000
*/*       REGISTER 13= @SAVEAREA                                     */ 00025000
*/*  REGISTERS ARE SAVED IN THE SAVEAREA POINTED TO BY REGISTER      */ 00026000
*/*  13.  A WORKAREA IS OBTAINED, OR ELSE SET ERROR CODE AND RETURN. */ 00027000
*/*                                                                  */ 00028000
*/********************************************************************/ 00029000
*                                                                  0059 00030000
*   EPARLG=ON;                      /* RECORD ENTRY INFO IN EPATH    */ 00031000
         OI    EPARLG(EPAPTR),B'01000000'                          0059 00032000
*   RFY                                                            0060 00033000
*    (R1,                                                          0060 00034000
*     R2) RSTD;                     /* RESTRICT USE OF REG 2         */ 00035000
*   R2=R1;                          /* GET POINTER TO ACE FROM REG1  */ 00036000
*                                                                  0061 00037000
         LR    R2,R1                                               0061 00038000
*   /*****************************************************************/ 00039000
*   /*                                                               */ 00040000
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */ 00041000
*   /*                                                               */ 00042000
*   /*****************************************************************/ 00043000
*                                                                  0062 00044000
*   DO;                             /* ILRGMA GET                  0062 00045000
*                                      CELL(BWK)WORKREGS(R11,R12)    */ 00046000
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */ 00047000
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0063 00048000
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */ 00049000
*       1 GMACELL BASED BDY(WORD),                                 0064 00050000
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */ 00051000
*     RESPECIFY                                                    0065 00052000
*      (R0,                                                        0065 00053000
*       R1,                                                        0065 00054000
*       R11,                                                       0065 00055000
*       R12,                                                       0065 00056000
*       R14,                                                       0065 00057000
*       R15) RESTRICTED;                                           0065 00058000
*     RESPECIFY                                                    0066 00059000
*       ASMPOOLS BASED(R11);                                       0066 00060000
*     RESPECIFY                                                    0067 00061000
*       GMACELL BASED(R14);                                        0067 00062000
*     R11=ADDR(ASMBWKPC);                                          0068 00063000
         LA    R11,ASMBWKPC(,ASMVTPTR)                             0068 00064000
*     LM(R14,R15,ASMCPAVQ);         /* GET ADDR OF 1ST CELL          */ 00065000
         LM    R14,R15,ASMCPAVQ(R11)                               0069 00066000
*GMA00010:                                                         0070 00067000
*     IF R14^=0 THEN                /* TEST CELL POOL                */ 00068000
GMA00010 LTR   R14,R14                                             0070 00069000
         BZ    @RF00070                                            0070 00070000
*       DO;                         /* CELL AVAILABLE, REMOVE CELL   */ 00071000
*         R0=GMANEXT;                                              0072 00072000
         L     R0,GMANEXT(,R14)                                    0072 00073000
*         R1=R15-1;                                                0073 00074000
         LR    R1,R15                                              0073 00075000
         BCTR  R1,0                                                0073 00076000
*         CDS(R14,R0,ASMCPAVQ);                                    0074 00077000
         CDS   R14,R0,ASMCPAVQ(R11)                                0074 00078000
*         BC(7,GMA00010);                                          0075 00079000
         BC    7,GMA00010                                          0075 00080000
*         R1=R14;                                                  0076 00081000
         LR    R1,R14                                              0076 00082000
*       END;                                                       0077 00083000
*     ELSE                                                         0078 00084000
*       DO;                         /* CELL POOL EMPTY               */ 00085000
         B     @RC00070                                            0078 00086000
@RF00070 DS    0H                                                  0079 00087000
*         R1=R11;                   /* PREPARE TO CALL PEX           */ 00088000
         LR    R1,R11                                              0079 00089000
*         CALL ILRPEX;              /* EXTEND THE POOL               */ 00090000
         L     @15,ASMPEX(,ASMVTPTR)                               0080 00091000
         BALR  @14,@15                                             0080 00092000
*       END;                                                       0081 00093000
*     RESPECIFY                                                    0082 00094000
*      (R0,                                                        0082 00095000
*       R1,                                                        0082 00096000
*       R11,                                                       0082 00097000
*       R12,                                                       0082 00098000
*       R14,                                                       0082 00099000
*       R15) UNRESTRICTED;                                         0082 00100000
@RC00070 DS    0H                                                  0083 00101000
*   END;                            /* ILRGMA GET                  0083 00102000
*                                      CELL(BWK)WORKREGS(R11,R12)    */ 00103000
*                                                                  0083 00104000
*   /*****************************************************************/ 00105000
*   /*                                                               */ 00106000
*   /* END ILRGMA MACRO EXPANSION                                    */ 00107000
*   /*                                                               */ 00108000
*   /*****************************************************************/ 00109000
*                                                                  0084 00110000
*                                   /* GET WORKAREA                  */ 00111000
*   RFY                                                            0084 00112000
*     R1 RSTD;                      /* RESTRICT REG 1                */ 00113000
*   IF R1=ZERO                      /* IF NO WORKAREA AVAILABLE,     */ 00114000
*     THEN                          /* RETURN TO CALLER WITH RETURN  */ 00115000
         LTR   R1,R1                                               0085 00116000
         BNZ   @RF00085                                            0085 00117000
*     RETURN CODE(28);              /* CODE 28.                      */ 00118000
         LA    @15,28                                              0086 00119000
         L     @14,12(,@13)                                        0086 00120000
         LM    @00,@12,20(@13)                                     0086 00121000
         BR    @14                                                 0086 00122000
*   WKAREG=R1;                      /* GET POINTER TO WORK AREA      */ 00123000
@RF00085 LR    WKAREG,R1                                           0087 00124000
*   EPAOWKA=R1;                     /* SAVE WORK AREA PTR FOR      0088 00125000
*                                      RECOVERY                      */ 00126000
         ST    R1,EPAOWKA(,EPAPTR)                                 0088 00127000
*   GEN REFS(WKAREG,R12) SETS(R12,R13);                            0089 00128000
         USING @DATD,WKAREG       /* ESTABLISH ADDR TO WORK AREA    */  00129000
         LA    R12,SAVEREGS      /* OBTAIN ADDR OF RLG00'S SAVE AREA*/  00130000
         ST    R13,4(R12)        /* STORE ADDR OF PREVIOUS SAVE AREA*/  00131000
         ST    R12,8(R13)         /* STORE ADDR OF RLG00'S SAVE AREA*/  00132000
         LR    R13,R12            /* SET R13 TO RLG00'S SAVE AREA   */  00133000
*   RFY                                                            0090 00134000
*     R1 UNRSTD;                    /* FREE REG 1 FOR USE            */ 00135000
*   WORKDCLS=WORKDCLS&&WORKDCLS;    /* ZERO OUT WORK AREA            */ 00136000
         XC    WORKDCLS(17),WORKDCLS                               0091 00137000
*   ACEPTR=R2;                      /* GET POINTER TO ACE FROM REG 2 */ 00138000
         ST    R2,ACEPTR                                           0092 00139000
*   RFY                                                            0093 00140000
*     R2 UNRSTD;                    /* FREE REG 2 FOR USE            */ 00141000
*   IF ACEUSYM=ON                   /* LG BEEN SAVED?                */ 00142000
*     THEN                                                         0094 00143000
         L     @12,ACEPTR                                          0094 00144000
         TM    ACEUSYM(@12),B'10000000'                            0094 00145000
         BNO   @RF00094                                            0094 00146000
*     LGSAVED=ON;                   /* SET LG -SAVED FLAG ON         */ 00147000
         OI    LGSAVED,B'10000000'                                 0095 00148000
*   LGEPTR=ACELGE;                  /* GET ADDRESSABILITY TO LGE     */ 00149000
@RF00094 L     @12,ACEPTR                                          0096 00150000
         L     @12,ACELGE(,@12)                                    0096 00151000
         ST    @12,LGEPTR                                          0096 00152000
*   IF LGEASPCT^=ZERO               /* IF THERE IS AN ACTIVE ASPCT,  */ 00153000
*     THEN                          /* THEN                          */ 00154000
         L     @12,LGEASPCT(,@12)                                  0097 00155000
         LTR   @12,@12                                             0097 00156000
         BZ    @RF00097                                            0097 00157000
*     DO;                                                          0098 00158000
*       ASPCTPTR=LGEASPCT;          /* GET POINTER TO ASPCT FROM LGE */ 00159000
         ST    @12,ASPCTPTR                                        0099 00160000
*       EPAAASP=ASPCTPTR;           /* RECORD PTR TO ACTIVE ASPCT    */ 00161000
         ST    @12,EPAAASP(,EPAPTR)                                0100 00162000
*       BACKSLOT=ASPBKSLT;          /* SAVE # OF BACKUP SLOTS        */ 00163000
         L     @11,ASPBKSLT(,@12)                                  0101 00164000
         ST    @11,BACKSLOT                                        0101 00165000
*       SAVESLOT=ASPSAVCT;          /* SAVE # OF SAVED SLOTS         */ 00166000
         L     @12,ASPSAVCT(,@12)                                  0102 00167000
         ST    @12,SAVESLOT                                        0102 00168000
*     END;                                                         0103 00169000
*                                                                  0104 00170000
*/********************************************************************/ 00171000
*/*                                                                  */ 00172000
*/* RLGSAVED: PROCESS A SAVED ASPCT                                  */ 00173000
*/*                                                                  */ 00174000
*/*          THE RETURN ADDRESS IS SAVED. IF THE ASPCT IS INACTIVE,  */ 00175000
*/*          TURN THE ACENOACT FLAG ON IN THE ACE. IF THE ASPCT IS   */ 00176000
*/*          ACTIVE, INVOKE ILRALS00 TO RELEASE THE UNSAVED SLOTS,   */ 00177000
*/*          INVOKE ILRAFS00 TO FREE THE LSQA STORAGE USED BY THIS   */ 00178000
*/*          ASPCT AND ITS EXTENSIONS.                               */ 00179000
*/*          IF STGINDEX IS NOT OPEN OR ILRTMRLG IS NOT ACTIVE, AND  */ 00180000
*/*         THIS IS NOT A DEACTIVATE REQUEST, SET THE RETURN CODE TO */ 00181000
*/*         16, AND RETURN. IF THIS IS A RELEASE REQUEST, AND        */ 00182000
*/*         STGINDEX IS OPEN AND ILRTMRLG IS ACTIVE, ADD THE ACE     */ 00183000
*/*         TO THE RELEASE QUEUE AND POST ILRTMRLG. RETURN TO CALLER */ 00184000
*/*                                                                  */ 00185000
*/*         ACTIVE ASPCT STORAGE FREED                               */ 00186000
*/*         UNSAVED SLOTS RELEASED                                   */ 00187000
*/*         IF ILRTMRLG IS ACTIVE, AND THIS IS A RELEASE REQUEST,    */ 00188000
*/*         THE ACE IS QUEUED TO THE ASMVT RELEASE QUEUE. IF THIS WAS*/ 00189000
*/*         A RELEASE REQUEST AND ILRTMRLG IS INACTIVE, REGISTER 15  */ 00190000
*/*         WILL CONTAIN A RETURN CODE OF 16.                        */ 00191000
*/*                                                                  */ 00192000
*/********************************************************************/ 00193000
*                                                                  0104 00194000
*   IF LGSAVED=ON                   /* IF LG -SAVED FLAG IS ON       */ 00195000
*     THEN                                                         0104 00196000
@RF00097 TM    LGSAVED,B'10000000'                                 0104 00197000
         BNO   @RF00104                                            0104 00198000
*     DO;                                                          0105 00199000
*       IF LGEASPCT=ZERO THEN                                      0106 00200000
         L     @12,LGEPTR                                          0106 00201000
         L     @12,LGEASPCT(,@12)                                  0106 00202000
         LTR   @12,@12                                             0106 00203000
         BNZ   @RF00106                                            0106 00204000
*         ACENOACT=ON;              /* IF ASPCT NOT ACTIVE,SET     0107 00205000
*                                      ACENOACT                      */ 00206000
         L     @12,ACEPTR                                          0107 00207000
         OI    ACENOACT(@12),B'00010000'                           0107 00208000
*       ELSE                        /* IF ASPCT IS ACTIVE            */ 00209000
*         DO;                                                      0108 00210000
         B     @RC00106                                            0108 00211000
@RF00106 DS    0H                                                  0109 00212000
*           RFY                                                    0109 00213000
*            (R0,                                                  0109 00214000
*             R1,                                                  0109 00215000
*             R15) RSTD;            /* RESTRICT USE OF REG 0         */ 00216000
*           R0=ASPCTPTR;            /* LOAD REG0 WITH ADDR(ASPCT)    */ 00217000
         L     R0,ASPCTPTR                                         0110 00218000
*           LGEASPCT=ZERO;          /* ZERO THE LGE PTR TO THE ASPCT */ 00219000
         L     @14,LGEPTR                                          0111 00220000
         SLR   @12,@12                                             0111 00221000
         ST    @12,LGEASPCT(,@14)                                  0111 00222000
*           CALL ILRALS00;          /* FREE THE UNSAVED SLOTS        */ 00223000
         BAL   @14,ILRALS00                                        0112 00224000
*           R1=ASPCTPTR;            /* LOAD REG1 WITH ADDR(ASPCT)    */ 00225000
         L     R1,ASPCTPTR                                         0113 00226000
*           R0=LENGTH(ASPCT);       /* SET REG0 TO LENGTH OF ACTIVE     00227000
*                                      ASPCT                         */ 00228000
         LA    R0,1088                                             0114 00229000
*           EPAAASP=ZERO;           /* ZERO EPATH PTR TO ASPCT       */ 00230000
         SLR   @14,@14                                             0115 00231000
         ST    @14,EPAAASP(,EPAPTR)                                0115 00232000
*           CALL ILRAFS00;          /* FREE THE ACTIVE ASPCT STORAGE */ 00233000
         BAL   @14,ILRAFS00                                        0116 00234000
*           IF R15^=ZERO            /* FOR NON-ZERO RETURN CODE FROM    00235000
*                                      AFS, ABEND                    */ 00236000
*             THEN                                                 0117 00237000
         LTR   R15,R15                                             0117 00238000
         BZ    @RF00117                                            0117 00239000
*             DO;                                                  0118 00240000
*               EPARTYRG=R13+12;    /* SAVED REGISTERS FOR RECOVERY  */ 00241000
         LA    @14,12                                              0119 00242000
         ALR   @14,R13                                             0119 00243000
         ST    @14,EPARTYRG(,EPAPTR)                               0119 00244000
*               GEN REFS(R0,R13,R14);                              0120 00245000
         STM   R0,R14,12(R13)                                           00246000
         ABEND X'087',,,SYSTEM                                          00247000
*             END;                                                 0121 00248000
*           RFY                                                    0122 00249000
*            (R0,                                                  0122 00250000
*             R1,                                                  0122 00251000
*             R15) UNRSTD;                                         0122 00252000
@RF00117 DS    0H                                                  0123 00253000
*         END;                      /* END ACTIVE ASPCT PROCESSING   */ 00254000
*       IF(ASMSTGXA=ZERO            /* IF STORAGE INDEX NOT OPEN     */ 00255000
*           |ASMNOTMR=ON)           /* OR ILRTMRLG NOT OPERATING     */ 00256000
*         THEN                                                     0124 00257000
@RC00106 L     @12,ASMSTGXA(,ASMVTPTR)                             0124 00258000
         LTR   @12,@12                                             0124 00259000
         BZ    @RT00124                                            0124 00260000
         TM    ASMNOTMR(ASMVTPTR),B'00100000'                      0124 00261000
         BNO   @RF00124                                            0124 00262000
@RT00124 DS    0H                                                  0125 00263000
*         DO;                                                      0125 00264000
*           RETACE=ON;              /* SET RETURN ACE BIT ON         */ 00265000
         OI    RETACE,B'01000000'                                  0126 00266000
*           RETCODE=20;             /* SET RETURN CODE TO 20         */ 00267000
         MVC   RETCODE(4),@CF00213                                 0127 00268000
*         END;                                                     0128 00269000
*       ELSE                                                       0129 00270000
*         IF ACEOP=ACERELLG         /* IF THIS IS A RELEEASE LG      */ 00271000
*           THEN                    /* REQUEST                       */ 00272000
         B     @RC00124                                            0129 00273000
@RF00124 L     @12,ACEPTR                                          0129 00274000
         CLI   ACEOP(@12),12                                       0129 00275000
         BNE   @RF00129                                            0129 00276000
*           DO;                     /* THEN                          */ 00277000
*             ACEFQPA=ZERO;         /* ZERO FORWARD POINTER IN ACE   */ 00278000
         SLR   @11,@11                                             0131 00279000
         ST    @11,ACEFQPA(,@12)                                   0131 00280000
*             RFY                                                  0132 00281000
*              (R8,                                                0132 00282000
*               R9) RSTD;           /* RESTRICT USE OF C&S REGISTERS */ 00283000
*             R8=ACEPTR;                                           0133 00284000
         LR    R8,@12                                              0133 00285000
*             R9=ASMRLGRQ;                                         0134 00286000
         L     R9,ASMRLGRQ(,ASMVTPTR)                              0134 00287000
*TRYOVER0:                                                         0135 00288000
*             ACEFQPA=R9;                                          0135 00289000
TRYOVER0 L     @12,ACEPTR                                          0135 00290000
         ST    R9,ACEFQPA(,@12)                                    0135 00291000
*             CS(R9,R8,ASMRLGRQ);   /* COMPARE AND SWAP THE ACE ONTO */ 00292000
         CS    R9,@08,ASMRLGRQ(ASMVTPTR)                           0136 00293000
*             BC(7,TRYOVER0);       /* THE HEAD OF THE RELEASE QUEUE */ 00294000
         BC    7,TRYOVER0                                          0137 00295000
*             RFY                                                  0138 00296000
*              (R8,                                                0138 00297000
*               R9) UNRSTD;         /* FREE C&S REGISTERS FOR USE    */ 00298000
*             GEN REFS(R8,R10,R13)(STM   R10,R8,12(R13));          0139 00299000
         STM   R10,R8,12(R13)                                           00300000
*             RFY                                                  0140 00301000
*              (R9,                                                0140 00302000
*               R10,                                               0140 00303000
*               R11,                                               0140 00304000
*               R12) RSTD;          /* RESTRICT POST REGS            */ 00305000
*             R9=R13;               /* SAVE ADDRESS OF ILRRLG'S    0141 00306000
*                                      SAVEAREA                      */ 00307000
         LR    R9,R13                                              0141 00308000
*             R11=ADDR(ASMTMECB);   /* ADDR OF ECB TO BE POSTED      */ 00309000
         LA    R11,ASMTMECB(,ASMVTPTR)                             0142 00310000
*             R11=R11|'80000000'X;  /* SET HI-ORDER BIT ON FOR XMPOST*/ 00311000
         O     R11,@CF01355                                        0143 00312000
*             R10=ZERO;             /* INITIALIZE COMPLETION CODE    */ 00313000
         SLR   R10,R10                                             0144 00314000
*             R12=ADDR(CVTBRET);    /* SYSTEM ERRET ENTRY PT ADDRESS */ 00315000
         L     @08,CVTPTR                                          0145 00316000
         LA    R12,CVTBRET(,@08)                                   0145 00317000
*             R13=ASMTASCB;         /* ADDR OF TMRLG'S ASCB          */ 00318000
         L     R13,ASMTASCB(,ASMVTPTR)                             0146 00319000
*             CALL XMPOST;          /* BRANCH ENTRY CROSS-MEMORY POST*/ 00320000
         L     @15,CVT0PT01(,@08)                                  0147 00321000
         BALR  @14,@15                                             0147 00322000
*             RFY                                                  0148 00323000
*              (R9,                                                0148 00324000
*               R10,                                               0148 00325000
*               R11,                                               0148 00326000
*               R12) UNRSTD;        /* FREE POST REGS                */ 00327000
*             GEN REFS(R8,R9,R10)(LM    R10,R8,12(R9));            0149 00328000
         LM    R10,R8,12(R9)                                            00329000
*           END;                    /* END DEACTIVATE REQ PROCESSING */ 00330000
*     END;                          /* END EPATH-SAVED PROCESSING    */ 00331000
*                                                                  0152 00332000
*/********************************************************************/ 00333000
*/*                                                                  */ 00334000
*/*  WHEN LG IS NOT SAVED (LGSAVED=OFF) THE FOLLOWING BLOCK OF       */ 00335000
*/*  CODE WILL BE EXECUTED.                                          */ 00336000
*/*                                                                  */ 00337000
*/********************************************************************/ 00338000
*                                                                  0152 00339000
*   ELSE                                                           0152 00340000
*     IF LGEASPCT^=ZERO THEN                                       0152 00341000
         B     @RC00104                                            0152 00342000
@RF00104 SLR   @12,@12                                             0152 00343000
         L     @11,LGEPTR                                          0152 00344000
         C     @12,LGEASPCT(,@11)                                  0152 00345000
         BE    @RF00152                                            0152 00346000
*       DO;                                                        0153 00347000
*         RFY                                                      0154 00348000
*          (R0,                                                    0154 00349000
*           R1,                                                    0154 00350000
