*                                      LG                            */ 00357000
         BAL   @14,ILRALS00                                        0157 00358000
*         R1=ASPCTPTR;              /* LOAD REG1 WITH ADDR OF ASPCT  */ 00359000
         L     R1,ASPCTPTR                                         0158 00360000
*         R0=LENGTH(ASPCT);         /* LOAD REG0 WITH LENGTH OF ASPCT*/ 00361000
         LA    R0,1088                                             0159 00362000
*         EPAAASP=ZERO;             /* ZERO EPATH PTR TO ACTIVE ASPCT*/ 00363000
         SLR   @14,@14                                             0160 00364000
         ST    @14,EPAAASP(,EPAPTR)                                0160 00365000
*         CALL ILRAFS00;            /* RELEASE STORAGE FOR THIS ASPCT*/ 00366000
         BAL   @14,ILRAFS00                                        0161 00367000
*         IF R15^=ZERO              /* IF NON-ZERO RETURN FROM AFS,  */ 00368000
*           THEN                                                   0162 00369000
         LTR   R15,R15                                             0162 00370000
         BZ    @RF00162                                            0162 00371000
*           DO;                     /* THEN ABEND 807                */ 00372000
