         ST    @14,EPARTYRG(,EPAPTR)                               0164 00376000
*             GEN REFS(R0,R13,R14);                                0165 00377000
         STM   R0,R14,12(R13)                                           00378000
         ABEND X'087',,,SYSTEM                                          00379000
*           END;                                                   0166 00380000
*         RFY                                                      0167 00381000
*          (R0,                                                    0167 00382000
*           R1,                                                    0167 00383000
*           R15) UNRSTD;            /* FREE REGS 0 AND 15 FOR USE    */ 00384000
@RF00162 DS    0H                                                  0168 00385000
*       END;                        /* END EPATH-SAVED PROCESSING    */ 00386000
*                                                                  0169 00387000
*/********************************************************************/ 00388000
*/*                                                                  */ 00389000
*/*      CHECK SLOT COUNTERS FOR NON-ZERO                            */ 00390000
*/*                                                                  */ 00391000
*/********************************************************************/ 00392000
*                                                                  0169 00393000
*   IF RETACE=OFF                   /* IF RETURN ACE BIT IS OFF      */ 00394000
*       &BACKSLOT^=ZERO             /* AND BACKSLOT IS NON-ZERO      */ 00395000
*     THEN                                                         0169 00396000
@RF00152 DS    0H                                                  0169 00397000
@RC00104 TM    RETACE,B'01000000'                                  0169 00398000
         BNZ   @RF00169                                            0169 00399000
         L     @12,BACKSLOT                                        0169 00400000
         LTR   @12,@12                                             0169 00401000
         BZ    @RF00169                                            0169 00402000
*     DO;                                                          0170 00403000
*       RFY                                                        0171 00404000
*        (R8,                                                      0171 00405000
*         R9) RSTD;                 /* THEN ADD BACKSLOT TO ASMBKSLT */ 00406000
*       R8=ASMBKSLT;                                               0172 00407000
         L     R8,ASMBKSLT(,ASMVTPTR)                              0172 00408000
*TRYOVER1:                                                         0173 00409000
*       R9=R8+BACKSLOT;                                            0173 00410000
TRYOVER1 LR    R9,R8                                               0173 00411000
         AL    R9,BACKSLOT                                         0173 00412000
*       CS(R8,R9,ASMBKSLT);                                        0174 00413000
         CS    R8,@09,ASMBKSLT(ASMVTPTR)                           0174 00414000
*       BC(7,TRYOVER1);                                            0175 00415000
