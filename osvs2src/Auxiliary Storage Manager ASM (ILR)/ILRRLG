*/*                                                                  */ 00640000
*/*                           RLGLPME                                */ 00641000
*/*  AN INTERNAL SUBROUTINE USED BY ILRALS00 TO RELEASE UNSAVED      */ 00642000
*/*  SLOTS.                                                          */ 00643000
*/*                                                                  */ 00644000
*/********************************************************************/ 00645000
*/*                                                                  */ 00646000
*/* INPUT: REGISTER 0 = CORRESPONDING RPN                            */ 00647000
*/*        REGISTER 1 = PTR TO LPME                                  */ 00648000
*/*                                                                  */ 00649000
*/* PROCESS: SAVE THE RETURN ADDRESS AND PTR TO LPME. IF THE LPME    */ 00650000
*/*          HAS NOT BEEN SAVED, FLAG THE LPME AS INVALID, CALL      */ 00651000
*/*          ILRFRSLT TO UPDATE THE PAT MAP, AND INCREMENT THE       */ 00652000
*/*          FREESLOT COUNTER.                                       */ 00653000
*/*                                                                  */ 00654000
*/* OUTPUT: THE UNSAVED SLOT RELEASED                                */ 00655000
*/*         THE FREESLOT COUNTER INCREMENTED IF THE SLOT WAS RELEASED*/ 00656000
*/*                                                                  */ 00657000
*/********************************************************************/ 00658000
*/*HEADER                                                            */ 00659000
*/*  ILRRLG                                                          */ 00660000
*/* RLGLPME: RELEASE UNSAVED SLOTS BACK TO SYSTEM                    */ 00661000
*/*RLGLPME: E  ENTRY                                                 */ 00662000
*/*         N  THIS ROUTINE IS USED BY ILRALS00 TO REL UNSAVED SLOTS */ 00663000
*/*         P  SAVE THE RETURN ADDRESS                               */ 00664000
*/*         P  LPMEPTR=R1                                            */ 00665000
*/*         D  (YES,%RLG16,NO,) LPME BEEN SAVED?                     */ 00666000
*/*         P  FLAG THE LPME AS INVALID                              */ 00667000
*/*         P  LOAD REG 1 WITH LSID TO BE FREED                      */ 00668000
*/*         S  ILRFRSLT: RELEASE THE SLOT                            */ 00669000
*/*         P  INCREMENT THE FREESLOT COUNTER                        */ 00670000
*/*%RLG16:  R  RETURN TO ILRALS00                                    */ 00671000
*                                                                  0239 00672000
*RLGLPME:                                                          0239 00673000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0239 00674000
RLGLPME  DS    0H                                                  0240 00675000
*   DCL                                                            0240 00676000
*     SAV14 PTR(31);                                               0240 00677000
*   RFY                                                            0241 00678000
*    (R0,                                                          0241 00679000
*     R1,                                                          0241 00680000
*     R14) RSTD;                    /* RESTRICT USE OF REGS 0,1,14   */ 00681000
*   SAV14=R14;                      /* SAVE RETURN ADDRESS           */ 00682000
         ST    R14,SAV14                                           0242 00683000
*   LPMEPTR=R1;                     /* GET POINTER TO LPME           */ 00684000
         LR    LPMEPTR,R1                                          0243 00685000
*   IF ASPLSAVE=OFF                 /* IF THE LPME HAS NOT BEEN    0244 00686000
*                                                              SAVED,*/ 00687000
*     THEN                                                         0244 00688000
         TM    ASPLSAVE(LPMEPTR),B'01000000'                       0244 00689000
         BNZ   @RF00244                                            0244 00690000
*     DO;                                                          0245 00691000
*       IF ASPLIOER=OFF             /* CHECK I/O ERROR FLAG          */ 00692000
*         THEN                                                     0246 00693000
         TM    ASPLIOER(LPMEPTR),B'00010000'                       0246 00694000
         BNZ   @RF00246                                            0246 00695000
*         DO;                       /* OFF,                          */ 00696000
*           ASPLVALD=OFF;           /* FLAG THE LPME AS INVALID      */ 00697000
         NI    ASPLVALD(LPMEPTR),B'01111111'                       0248 00698000
*           R1=ASPVLSID;            /* LOAD REG1 WITH LSID TO BE   0249 00699000
*                                      FREED                         */ 00700000
         L     R1,ASPVLSID-1(,LPMEPTR)                             0249 00701000
         LA    R1,0(,R1)                                           0249 00702000
*           CALL ILRFRSL1;          /* RELEASE THE SLOT              */ 00703000
         L     @15,ASMPFRSL(,ASMVTPTR)                             0250 00704000
         BALR  @14,@15                                             0250 00705000
*         END;                                                     0251 00706000
*       FREESLOT=FREESLOT+1;        /* INCREMENT THE FREESLOT COUNTER*/ 00707000
@RF00246 LA    @15,1                                               0252 00708000
         AL    @15,FREESLOT                                        0252 00709000
         ST    @15,FREESLOT                                        0252 00710000
*     END;                                                         0253 00711000
*   R14=SAV14;                      /* RESTORE RETURN ADDRESS        */ 00712000
@RF00244 L     R14,SAV14                                           0254 00713000
*   RFY                                                            0255 00714000
*    (R0,                                                          0255 00715000
*     R1,                                                          0255 00716000
*     R14) UNRSTD;                  /* FREE REGS 0,1,14 FOR USE      */ 00717000
*   END RLGLPME;                                                   0256 00718000
@EL00002 DS    0H                                                  0256 00719000
@EF00002 DS    0H                                                  0256 00720000
@ER00002 BR    @14                                                 0256 00721000
*                                                                  0257 00722000
*/*  MACCOMP Y-4SC1CW/PZD81                                          */ 00723000
*/*  MACSTAT Y-475082/415304                                         */ 00724000
*/********************************************************************/ 00725000
*/*                                                                  */ 00726000
*/*  NAME:  ILRAFS00                                                 */ 00727000
*/*                                                                  */ 00728000
*/*  STATUS:  OS/VS2 RELEASE 4.1, LEVEL 0                            */ 00729000
*/*                                                                  */ 00730000
*/*  ENTRY POINT:  ILRAFS00                                          */ 00731000
*/*                                                                  */ 00732000
*/*  FUNCTION:  THIS SUBROUTINE FREES AN ASPCT.                      */ 00733000
*/*                                                                  */ 00734000
*/*  INPUT:  ADDRESS OF ASPCT IN REGISTER 1.                         */ 00735000
*/*          LENGTH OF ASPCT IN REGISTER 0.                          */ 00736000
*/*                                                                  */ 00737000
*/*  PROCESS:  TWO LOOPS, ONE TO FREE ASST EXTENSIONS AND ONE TO     */ 00738000
*/*            FREE LPME EXTENSIONS, ARE EXECUTED.  EACH USES THE    */ 00739000
*/*            SUBROUTINE AFSFREE TO INVOKE FREEMAIN.  A FINAL CALL  */ 00740000
*/*            TO AFSFREE FREES THE BASE ASPCT EXTN.                 */ 00741000
*/*                                                                  */ 00742000
*/*  OUTPUT:  RETURN CODE IN REGISTER 15.                            */ 00743000
*/*             00 = NORMAL RETURN                                   */ 00744000
*/*             44 = FREEMAIN FAILURE                                */ 00745000
*/*                                                                  */ 00746000
*/*  EXIT-NORMAL:  RETURN TO CALLER WITH RETURN CODE (SEE OUTPUT)    */ 00747000
*/*                                                                  */ 00748000
*/*  EXIT-ERROR:  NONE                                               */ 00749000
*/*                                                                  */ 00750000
*/*  EXTERNAL REFERENCES:  FREEMAIN                                  */ 00751000
*/*                                                                  */ 00752000
*/*  REGISTERS CLOBBERED:  0 AND 1, 14 AND 15                        */ 00753000
*/*                                                                  */ 00754000
*/*  MAPPING MACROS USED:  ILRASPCT                                  */ 00755000
*/*                                                                  */ 00756000
*/*  NOTES:  DUE TO PLS OPTIMIZATION, RESTRICT ALL NECESSARY         */ 00757000
*/*          REGISTERS AROUND THIS ROUTINE.                          */ 00758000
*/*          INVOKING ROUTINE MUST HOLD THE LOCAL LOCK, OR HAVE      */ 00759000
*/*          SPECIFIED %AFSSVC=YES.                                  */ 00760000
*/*          ALL LABELS AND DATA AREAS CREATED BY THIS ROUTINE BEGIN */ 00761000
*/*          WITH THE THREE CHARACTERS 'AFS'.  THE CALLING MODULE    */ 00762000
*/*          MUST INCLUDE SUFFICIENT WORKSPACE FOR THIS ROUTINE'S    */ 00763000
*/*          DATA AREAS.  THIS AMOUNT IS COMPILER DEPENDENT WITH A   */ 00764000
*/*          MAXIMUM REQUIREMENT OF 46 BYTES.  THIS ROUTINE ALSO     */ 00765000
*/*          REQUIRES THAT THE CALLING MODULE DECLARES REGISTERS     */ 00766000
*/*          R0-R15.  THE MAPPING MACRO LISTED ABOVE MUST BE         */ 00767000
*/*          INCLUDED.  THE BASE POINTER FOR THE ASPCT IS NOT        */ 00768000
*/*          REQUIRED BY THIS SUBROUTINE.                            */ 00769000
*/*                                                                  */ 00770000
*/*  METHOD OF ACCESS:                                               */ 00771000
*/*          %AFSSVC='YES' IF THE INVOKING ROUTINE MUST USE AN SVC   */ 00772000
*/*          %AFSSVC='NO' IF BRANCH ENTRY REQUIRED                   */ 00773000
*/*          %INCLUDE SYSLIB(ILRAFS00)                               */ 00774000
*/********************************************************************/ 00775000
*/*ILRAFS00: CHART (AREF)                                            */ 00776000
*/*HEADER                                                          0257 00777000
*/*            ILRAFS00 - FREE ASPCT STORAGE                         */ 00778000
*/*ILRAFS00: E ENTRY                                                 */ 00779000
*/*         P  SAVE THE RETURN ADDRESS                               */ 00780000
*/*         P  SAVE PTR TO ASPCT BASE                                */ 00781000
*/*         P  SAVE THE LENGTH OF ASPCT                              */ 00782000
*/*         D  (YES,,NO,%AFS01A) SPECIFIED LENGTH THAT OF ASPCT?     */ 00783000
*/*         P  (,%AFS01B) SET SUBPOOL ID TO 255                      */ 00784000
*/*%AFS01A: P  SET SUBPOOL ID TO 230                                 */ 00785000
*/*%AFS01B: D  (YES,,NO,%AFS06) ARE THERE ANY EXTENSIONS?            */ 00786000
*/*         P  INITIALIZE BASE ARRAY SUBSCRIPT TO ONE                */ 00787000
*/*         P  INITIALIZE ASST EXT ARRAY SUBSCRIPT TO TWO            */ 00788000
*/*%AFS01:  P  GET PTR TO ASST EXTENSION                             */ 00789000
*/*%AFS02:  P  GET PTR TO LPME EXTENSION                             */ 00790000
*/*%AFS02A: S  AFSFREE: FREE AN LPME EXTENSION                       */ 00791000
*/*         D  (YES,%AFS03,NO,) RETURN CODE ZERO?                    */ 00792000
*/*         P  INCREMENT THE ASST EXT ARRAY SUBSCRIPT                */ 00793000
*/*         D  (YES,%AFS04,NO,) REACHED END OF ASST EXT ARRAY?       */ 00794000
*/*         D  (YES,,NO,%AFS04) ANYMORE LPME EXTENSIONS?             */ 00795000
*/*         P  SET PTR TO LPME EXTENSION                             */ 00796000
*/*         D  (YES,%AFS02A,NO,%AFS04) IS THIS AN LPME EXTENSION?    */ 00797000
*/*%AFS03:  P  INCREMENT THE ASST EXT ARRAY SUBSCRIPT                */ 00798000
*/*         D  (YES,%AFS04,NO,) REACHED END OF ASST EXT ARRAY?       */ 00799000
*/*         D  (YES,%AFS02,NO,) ANYMORE LPME EXTENSIONS?             */ 00800000
*/*%AFS04:  P  SET WORK PTR TO ASST EXT                              */ 00801000
*/*         S  AFSFREE: FREE AN ASST EXTENSION                       */ 00802000
*/*         D  (YES,%AFS05,NO,) RETURN CODE ZERO?                    */ 00803000
*/*         P  INCREMENT THE BASE ARRAY SUBSCRIPT                    */ 00804000
*/*         D  (YES,%AFS06,NO,) REACHED END OF BASE ARRAY?           */ 00805000
*/*         D  (YES,,NO,%AFS06) ANYMORE ASST EXTENSIONS?             */ 00806000
*/*         P  GET PTR TO ASST EXTENSION                             */ 00807000
*/*         D  (YES,,NO,%AFS06) IS THIS AN ASST EXTENSION            */ 00808000
*/*         P  (,%AFS02) RESET ASST EXT ARRAY SUBSCRIPT TO ONE       */ 00809000
*/*%AFS05:  P  INCREMENT THE BASE ARRAY SUBSCRIPT                    */ 00810000
*/*         D  (YES,%AFS06,NO,) REACHED END OF BASE ARRAY            */ 00811000
*/*         D  (YES,,NO,%AFS06) ANYMORE ASST EXTENSIONS?             */ 00812000
*/*         P  (,%AFS01) RESET ASST EXT ARRAY SUBSCRIPT TO ONE       */ 00813000
*/*%AFS06:  P  SET THE WORK PTR TO ASPCT BASE                        */ 00814000
*/*         S  AFSFREE: FREE THE ASPCT BASE                          */ 00815000
*/*         R  RETURN TO CALLER                                      */ 00816000
*                                                                  0257 00817000
         EJECT                                                          00818000
*ILRAFS00:                                                         0257 00819000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0257 00820000
*                                                                  0257 00821000
ILRAFS00 DS    0H                                                  0258 00822000
*/*A-000000-999999                                           @Z40WPWH*/ 00823000
*                                                                  0258 00824000
*   DECLARE                                                        0258 00825000
