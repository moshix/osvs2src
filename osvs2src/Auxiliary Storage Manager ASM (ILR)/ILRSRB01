ILRSRB01 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ILRSRB01  76.114'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @10,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@10                                         0001 00012000
*   SRBDREG=R0;                     /* ESTABLISH DATA REG            */ 00013000
         LR    SRBDREG,R0                                          0049 00014000
*   FRRWORKA=''B;                   /* CLEAR WORKAREA STORAGE        */ 00015000
