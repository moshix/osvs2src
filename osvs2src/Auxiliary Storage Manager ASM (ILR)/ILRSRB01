         L     AIAPTR,ATAAIAQ(,ATAPTR)                             0207 00545000
*   DO WHILE AIAPTR^=ZERO;          /* PROCESS EACH AIA              */ 00546000
         B     @DE00208                                            0208 00547000
@DL00208 DS    0H                                                  0209 00548000
*     AIALPMEP->ASPLOPIN=ON;        /* CLEAR LPME IN PROCESS FLAG    */ 00549000
         L     @12,AIALPMEP(,AIAPTR)                               0209 00550000
         OI    ASPLOPIN(@12),B'00100000'                           0209 00551000
*     AIALGE->LGEWRKPD=ON;          /* LGE WILL BE CHECKED           */ 00552000
         L     @12,AIALGE(,AIAPTR)                                 0210 00553000
         OI    LGEWRKPD(@12),B'10000000'                           0210 00554000
*     AIAPRINO=OFF;                 /* AIA WILL BE REPROCESSED       */ 00555000
         NI    AIAPRINO(AIAPTR),B'10111111'                        0211 00556000
*     AIAPTR=AIANXAIA;              /* GET NEXT AIA                  */ 00557000
         L     AIAPTR,AIANXAIA(,AIAPTR)                            0212 00558000
*   END;                            /* ATA AIA QUEUE LOOP            */ 00559000
@DE00208 LTR   AIAPTR,AIAPTR                                       0213 00560000
         BNZ   @DL00208                                            0213 00561000
*   RFY                                                            0214 00562000
*    (AIAPTR,                                                      0214 00563000
*     VERFREG,                                                     0214 00564000
*     R5,                                                          0214 00565000
*     R6,                                                          0214 00566000
*     R7) UNRSTD;                                                  0214 00567000
*                                                                  0215 00568000
*   /*****************************************************************/ 00569000
*   /*                                                               */ 00570000
*   /* SCHEDULE SRB CONTROLLER'S SRB, IF NECESSARY, TO PROCESS WORK  */ 00571000
*   /* LEFT ON THE ASMHD LGE QUEUE BY THE RECOVERY ROUTINE.          */ 00572000
*   /*                                                               */ 00573000
*   /*****************************************************************/ 00574000
*                                                                  0215 00575000
*SCHED:                                                            0215 00576000
*   IF ASHSCHED=OFF THEN            /* SRBC REQUIRES A SCHEDULE ?    */ 00577000
