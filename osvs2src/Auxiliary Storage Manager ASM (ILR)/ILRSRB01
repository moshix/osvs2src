*   RFY                                                            0102 00178000
*     R11 UNRSTD;                                                  0102 00179000
*                                                                  0102 00180000
*   /*****************************************************************/ 00181000
*   /*                                                               */ 00182000
*   /* SRB CONTROLLER'S MODID WILL BE CHECKPOINTED IF THE ERROR DID  */ 00183000
*   /* NOT OCCUR DURING ENTRY PROCESSING.                            */ 00184000
*   /*                                                               */ 00185000
*   /*****************************************************************/ 00186000
*                                                                  0103 00187000
*   IF ATAMODID^=ATAMSRBC THEN      /* MODID CHECKPOINTED ?          */ 00188000
         CLI   ATAMODID(ATAPTR),8                                  0103 00189000
         BE    @RF00103                                            0103 00190000
*     DO;                           /* NO- ERROR IN ENTRY PROCESSING */ 00191000
*       ASHSCHED=OFF;               /* INSURE ILRSRBC IS RESCHEDULED */ 00192000
         NI    ASHSCHED(RSMHDPTR),B'01111111'                      0105 00193000
