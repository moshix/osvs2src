*         ACEPTR) UNRSTD;                                          0149 00342000
*     END;                          /* NON-ZERO ATA ACE QUEUE        */ 00343000
*                                                                  0151 00344000
*   /*****************************************************************/ 00345000
*   /*                                                               */ 00346000
*   /* PROCESS A CURRENT AIA OR ACE CHECKPOINTED IN THE ATA. THE     */ 00347000
*   /* ATAAIACE WILL BE ZEROED BY ILRCQIOE FOR ERRORS WHICH OCCUR IN */ 00348000
*   /* ILRQIOE.                                                      */ 00349000
*   /*                                                               */ 00350000
*   /*****************************************************************/ 00351000
*                                                                  0151 00352000
*   RFY                                                            0151 00353000
*    (VERFREG,                                                     0151 00354000
*     R5,                                                          0151 00355000
*     R6,                                                          0151 00356000
*     R7,                                                          0151 00357000
*     RTCODE) RSTD;                                                0151 00358000
@RF00135 DS    0H                                                  0152 00359000
*   VERFREG=ATAAIACE;               /* VERIFY CURRENT AIA/ACE        */ 00360000
