         TITLE 'ILRSRBC - SRB CONTROLLER                               *00001000
                        '                                               00002000
ILRSRBC  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ILRSRBC   76.114'                                 0001 00009000
ILRSRBRM DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00241-ILRSRBRM)                                   00013000
         ENTRY ILRSRBRM                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  BALR  @06,0                                               0001 00016000
@PSTART  DS    0H                                                  0001 00017000
         USING @PSTART,@06                                         0001 00018000
