*       /*                                                           */ 00701000
*       /*************************************************************/ 00702000
*                                                                  0237 00703000
*     END;                          /* END GMA SUCCESSFUL            */ 00704000
*   RFY                                                            0238 00705000
*     R14 RSTD;                                                    0238 00706000
@RC00079 DS    0H                                                  0239 00707000
*   R14=CVTSRBRT;                   /* OBTAIN RETURN ADDR FROM CVT   */ 00708000
         L     @12,CVTPTR                                          0239 00709000
         L     R14,CVTSRBRT(,@12)                                  0239 00710000
*   GEN REFS(R14);                                                 0240 00711000
*              /* DELETE RECOVERY AND RETURN TO DISPATCHER */           00712000
        SETFRR  D,WRKREGS=(R11,R12),RELATED=(SETFRR,ILRSRBC)            00713000
        BR      R14   RETURN TO DISPATCHER                              00714000
*ILRSRBRM:                                                         0241 00715000
*   ENTRY;                                                         0241 00716000
*                                                                  0241 00717000
@EP00241 DS    0H                                                  0242 00718000
*/********************************************************************/ 00719000
*/*                                                                  */ 00720000
*/* ILRSRBRM IS A RESOURCE MANAGER TERMINATION ROUTINE INVOKED BY    */ 00721000
*/* PURGE DEQUEUE. IF THE RSM HEADER EXISTS THE ASM HEADER SRB       */ 00722000
*/* SCHEDULED FLAG IS RESET, OTHERWISE, THE SRB IS FREED.            */ 00723000
*/*                                                                  */ 00724000
*/* INPUT: R1 CONTAINS THE SRB ADDRESS                               */ 00725000
*/*        R14 CONTAINS THE RETURN ADDRESS                           */ 00726000
*/*                                                                  */ 00727000
*/* OUTPUT: ASHSCHED RESET OR SRB FREED                              */ 00728000
*/*                                                                  */ 00729000
*/********************************************************************/ 00730000
*/********************************************************************/ 00731000
*/*SRBRM:   CHART (AREF)                                             */ 00732000
*/*         HEADER                                                 0242 00733000
*/*         ILRSRBC                                                0242 00734000
*/* ILRSRBRM - SRBC RESOURCE MANAGER                                 */ 00735000
*/*ILRSRBRM: E ENTRY                                                 */ 00736000
*/*         P SAVE RETURN ADDR                                       */ 00737000
*/*         P MAINTAIN SRBPTR (SRBPTR=R1)                            */ 00738000
*/*         P OBTAIN ASCBPTR, ASCBPTR = SRBASCB                      */ 00739000
*/*         P OBTAIN ADDRESSIBILITY TO ASMH BY RSMHDPTR = ASCBRSM    */ 00740000
*/*         D (Y,,N,%SRBC26) RSMHDPTR = 0, NO RSMHD                  */ 00741000
*/*         L (,%SRBC27) FREEMAIN: FREE SQA SPACE CONTAINING THE SRB */ 00742000
*/*%SRBC26: P RESET SRB SCHEDULED FLAG (ASHSCHED)                    */ 00743000
*/*%SRBC27: P RESTORE RETURN ADDR                                    */ 00744000
*/*         R RETURN                                                 */ 00745000
*/*SRBRM:   END                                                      */ 00746000
*/********************************************************************/ 00747000
*                                                                  0242 00748000
*   RFY                                                            0242 00749000
*    (R1,                                                          0242 00750000
*     R10,                                                         0242 00751000
*     R11,                                                         0242 00752000
*     R14,                                                         0242 00753000
*     R15) RSTD;                                                   0242 00754000
*   R10=R14;                        /* MAINTAIN RETURN ADDR          */ 00755000
         LR    R10,R14                                             0243 00756000
*   SRBPTR=R1;                      /* OBTAIN ADDRESSIBILITY TO THE     00757000
*                                      SRB                           */ 00758000
         LR    SRBPTR,R1                                           0244 00759000
*   IF SRBASCB->ASCBRSM=ZERO THEN                                  0245 00760000
         L     @12,SRBASCB(,SRBPTR)                                0245 00761000
         L     @12,ASCBRSM(,@12)                                   0245 00762000
         LTR   @12,@12                                             0245 00763000
         BNZ   @RF00245                                            0245 00764000
*     DO;                           /* RSM HEADER DOES NOT EXIST,  0246 00765000
*                                      FREE THE SRB                  */ 00766000
*       R11=LENGTH(SRB);            /* OBTAIN LENGTH OF THE SRB      */ 00767000
         LA    R11,44                                              0247 00768000
*       GEN REFS(R11,SRBPTR);                                      0248 00769000
*                     /* FREE THE SQA SPACE CONTAINING THE SRB */       00770000
         FREEMAIN  RC,SP=245,A=(SRBPTR),LV=(R11)                        00771000
*     END;                          /* RSM HEADER DOES NOT EXIST     */ 00772000
*   ELSE                                                           0250 00773000
*     SRBASCB->ASCBRSM->ASHSCHED=OFF;/* RESET SRB SCHEDULED FLAG     */ 00774000
         B     @RC00245                                            0250 00775000
@RF00245 L     @12,SRBASCB(,SRBPTR)                                0250 00776000
         L     @12,ASCBRSM(,@12)                                   0250 00777000
         NI    ASHSCHED(@12),B'01111111'                           0250 00778000
*   R15=ZERO;                       /* ZERO RETURN CODE              */ 00779000
@RC00245 SLR   R15,R15                                             0251 00780000
*   R14=R10;                        /* RESTORE RETURN ADDR           */ 00781000
         LR    R14,R10                                             0252 00782000
*   RFY                                                            0253 00783000
*    (R1,                                                          0253 00784000
*     R10,                                                         0253 00785000
*     R11,                                                         0253 00786000
*     R15) UNRSTD;                                                 0253 00787000
*SRBRMEND:                                                         0254 00788000
*   GEN(BR     R14      RETURN TO CALLER);/* RETURN                  */ 00789000
SRBRMEND BR     R14      RETURN TO CALLER                               00790000
*ACEDEQFR:                                                         0255 00791000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0255 00792000
*                                                                  0255 00793000
ACEDEQFR DS    0H                                                  0256 00794000
*/********************************************************************/ 00795000
*/*                                                                  */ 00796000
*/* ACEDEQFR WILL DEQUEUE AN ACE FROM THE LGE PROCESS QUEUE AND THEN */ 00797000
*/* FREE THE ACE VIA THE ILRGMA MACRO. THE LGE PROCESS QUEUE IS A    */ 00798000
*/* DOUBLE HEADED, DOUBLE THREADED QUEUE.                            */ 00799000
*/*                                                                  */ 00800000
*/* INPUT:  ACE ADDR IN ACEPTR                                       */ 00801000
*/*         LGE ADDR IN LGEPTR                                       */ 00802000
*/*         R2 THRU R4 STANDARD AS IN MAPPING MACRO POINTERS         */ 00803000
*/*                                                                  */ 00804000
*/* OUTPUT:  LGEPROCF, LGEPROCL, ACEFQPA, AND/OR ACEBQPA UPDATED.    */ 00805000
*/*                                                                  */ 00806000
*/********************************************************************/ 00807000
*/********************************************************************/ 00808000
*/*ACEDEQFR: CHART (AREF)                                            */ 00809000
*/*         HEADER                                                 0256 00810000
*/*         ILRSRBC                                                0256 00811000
*/* ACEDEQFR - SUBROUTINE TO DEQUEUE AND FREE AN ACE                 */ 00812000
*/*         E ENTRY                                                  */ 00813000
*/*         P SAVE RETURN ADDRESS                                    */ 00814000
*/*         D (Y,,N,%SRBC11) ACE IS FIRST AND LAST ON Q              */ 00815000
*/*         P (,%SRBC14) FIRST AND LAST PTRS = 0                     */ 00816000
*/*%SRBC11: D (Y,,N,%SRBC12) ACE IS FIRST ON Q (LGEPROCF)            */ 00817000
*/*         P FIRST AIA/ACE PTR = ACE FORWARD PTR                    */ 00818000
*/*         P (,%SRBC14) BACK PTR OF ACE FORWARD PTR = 0             */ 00819000
*/*%SRBC12: D (Y,,N,%SRBC13) ACE IS LAST ON Q (LGEPROCL)             */ 00820000
*/*         P LAST AIA/ACE PTR = ACE BACK PTR                        */ 00821000
*/*         P (,%SRBC14) FORWARD PTR OF ACE BACK PTR = 0             */ 00822000
*/*%SRBC13: P BACK PTR OF ACE FORWARD PTR = BACK PTR OF ACE          */ 00823000
*/*         P FORWARD PTR OF ACE BACK PTR = FORWARD PTR OF ACE       */ 00824000
*/*%SRBC14: P INITIALIZE CELL@ FOR GMA MACRO                         */ 00825000
*/*         L ILRGMA: RETURN THE ACE TO THE POOL                     */ 00826000
*/*         R RETURN                                                 */ 00827000
*/*ACEDEQFR: END                                                     */ 00828000
*/********************************************************************/ 00829000
*                                                                  0256 00830000
*                                                                  0256 00831000
*   /*****************************************************************/ 00832000
*   /*                                                               */ 00833000
*   /* DEQUEUES ACE FROM LGE PROCESS QUEUE AND THEN FREES ACE VIA THE*/ 00834000
*   /* ILRGMA MACRO                                                  */ 00835000
*   /*                                                               */ 00836000
*   /*****************************************************************/ 00837000
*                                                                  0256 00838000
*   RFY                                                            0256 00839000
*    (R10,                                                         0256 00840000
*     R14) RSTD;                                                   0256 00841000
*   R10=R14;                        /* MAINTAIN THE RETURN ADDR      */ 00842000
         LR    R10,R14                                             0257 00843000
*   RFY                                                            0258 00844000
*     R14 UNRSTD;                                                  0258 00845000
*   IF ACEPTR=LGEPROCF&ACEPTR=LGEPROCL THEN                        0259 00846000
         C     ACEPTR,LGEPROCF(,LGEPTR)                            0259 00847000
         BNE   @RF00259                                            0259 00848000
         C     ACEPTR,LGEPROCL(,LGEPTR)                            0259 00849000
         BNE   @RF00259                                            0259 00850000
