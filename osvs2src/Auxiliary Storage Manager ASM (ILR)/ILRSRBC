*             /*                                                     */ 00428000
*             /* IF GROUP-OP ACE WAS FOUND TO BE FIRST ON THE LGE    */ 00429000
*             /* PROCESS QUEUE THEN QUEUE IT TO ATAACEQ. WHETHER     */ 00430000
*             /* FOUND OR NOT CONTINUE TO LOOP THRU THE LGES.        */ 00431000
*             /*                                                     */ 00432000
*             /*******************************************************/ 00433000
*                                                                  0164 00434000
*             IF ACEPTR^=ZERO THEN  /* GROUP-OP ACE                  */ 00435000
         SLR   @12,@12                                             0164 00436000
         CR    ACEPTR,@12                                          0164 00437000
         BE    @RF00164                                            0164 00438000
*               IF ACEPTR=LGEPROCF THEN/* GROUP-OP ACE IS FIRST ON 0165 00439000
*                                      PROCESS Q                     */ 00440000
         C     ACEPTR,LGEPROCF(,LGEPTR)                            0165 00441000
         BNE   @RF00165                                            0165 00442000
*                 DO;               /* QUEUE THE GROUP-OP ACE, LIFO     00443000
*                                      QUEUE                         */ 00444000
*                   LGEGRINP=ON;    /* SET GROUP-OP IN PROCESS FLAG  */ 00445000
         OI    LGEGRINP(LGEPTR),B'01000000'                        0167 00446000
*                   ACESRBWK=ATAACEQ;/* QUEUE ATAACEQ TO ACE         */ 00447000
         L     @15,ATAACEQ(,ATAPTR)                                0168 00448000
         ST    @15,ACESRBWK(,ACEPTR)                               0168 00449000
*                   ATAACEQ=ACEPTR; /* RRR-MAKE ACE FIRST ELEMENT ON    00450000
*                                      QUEUE-RRR                     */ 00451000
         ST    ACEPTR,ATAACEQ(,ATAPTR)                             0169 00452000
*                   IF ACEFQPA^=ZERO THEN                          0170 00453000
         C     @12,ACEFQPA(,ACEPTR)                                0170 00454000
         BE    @RF00170                                            0170 00455000
*                     LGEWRKPD=ON;  /* MORE ELEMENTS ARE ON THE LGE     00456000
*                                      PROCESS QUEUE, SET THE WORK 0171 00457000
*                                      PENDING FLAG                  */ 00458000
         OI    LGEWRKPD(LGEPTR),B'10000000'                        0171 00459000
*                 END;              /* QUEUE GROUP-OP ACE            */ 00460000
*               ELSE                                               0173 00461000
*                 LGEWRKPD=ON;      /* SET WORK PENDING FLAG IN LGE  */ 00462000
         B     @RC00165                                            0173 00463000
@RF00165 OI    LGEWRKPD(LGEPTR),B'10000000'                        0173 00464000
*           END;                    /* WORK PENDING AND NO GROUP-OP     00465000
*                                      IN PROCESS                    */ 00466000
@RC00165 DS    0H                                                  0174 00467000
@RF00164 DS    0H                                                  0175 00468000
*         LGEPTR=LGENEXT;           /* OBTAIN ADDR NEXT LGE          */ 00469000
@RF00098 L     LGEPTR,LGENEXT(,LGEPTR)                             0175 00470000
*       END;                        /* END LOOP THRU ALL LGES        */ 00471000
@DE00097 LTR   LGEPTR,LGEPTR                                       0176 00472000
         BNZ   @DL00097                                            0176 00473000
*                                                                  0177 00474000
*       /*************************************************************/ 00475000
*       /*                                                           */ 00476000
*       /* RELEASE THE ASM LOCK, CALL ILRESTRT IF NECESSARY AND PASS */ 00477000
*       /* THE QUEUE OF PAGING AIAS, CALL ILRRLG FOR RELEASE AND     */ 00478000
*       /* DEACTIVATE ACES AND POST THE ACE ECB FOR OTHER GROUP-OP   */ 00479000
*       /* ACES IF THERE ARE ANY ON THE ACE QUEUE.                   */ 00480000
*       /*                                                           */ 00481000
*       /*************************************************************/ 00482000
*                                                                  0177 00483000
*       RFY                                                        0177 00484000
*         R11 RSTD;                                                0177 00485000
*       R11=ADDR(ASHLOCK);          /* OBTAIN ADDR ASM LOCK WORD     */ 00486000
         LA    R11,ASHLOCK(,RSMHDPTR)                              0178 00487000
*       GEN REFS(R11);                                             0179 00488000
