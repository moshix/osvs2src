*                                      REQUEST                       */ 00610000
         B     @RC00194                                            0212 00611000
@RF00194 DS    0H                                                  0213 00612000
*                 RFY                                              0213 00613000
*                  (R11,                                           0213 00614000
*                   R10) RSTD;                                     0213 00615000
*                 R11=ADDR(ACEECB); /* OBTAIN ADDR ECB TO BE POSTED  */ 00616000
         LA    R11,ACEECB(,ACEPTR)                                 0214 00617000
*                 R10=ZERO;         /* CLEAR COMPLETION CODE FOR POST*/ 00618000
         SLR   R10,R10                                             0215 00619000
*                 CALL BRENPOST;    /* INVOKE INTRA-MEMORY BRANCH  0216 00620000
*                                      ENTRY POST                    */ 00621000
         L     @12,CVTPTR                                          0216 00622000
         L     @15,CVT0PT02(,@12)                                  0216 00623000
         BALR  @14,@15                                             0216 00624000
*                 RFY                                              0217 00625000
*                  (R11,                                           0217 00626000
*                   R10) UNRSTD;                                   0217 00627000
*                 ATAACEQ=ACESRBWK; /* DEQUEUE ACE AND UPDATE ANCHOR    00628000
*                                      TO NEXT ELEMENT               */ 00629000
         L     @12,ACESRBWK(,ACEPTR)                               0218 00630000
         ST    @12,ATAACEQ(,ATAPTR)                                0218 00631000
*               END;                /* NOT RELEASE, DEACTIVATE     0219 00632000
*                                      REQUEST                       */ 00633000
*             ACEPTR=ATAACEQ;       /* OBTAIN ADDR NEXT ACE ON THE Q */ 00634000
@RC00194 L     ACEPTR,ATAACEQ(,ATAPTR)                             0220 00635000
*           END;                    /* END LOOP THRU ACES            */ 00636000
@DE00193 LTR   ACEPTR,ACEPTR                                       0221 00637000
         BNZ   @DL00193                                            0221 00638000
*           GEN;                                                   0222 00639000
FRELOC  SETLOCK  RELEASE,TYPE=LOCAL,RELATED=(VIO,ILRSRBC(GETLOC))       00640000
*         END;                      /* GROUP-OP ACES FOUND           */ 00641000
*                                                                  0224 00642000
*       /*************************************************************/ 00643000
*       /*                                                           */ 00644000
*       /* FREE THE WORK-SAVE AREA, DELETE RECOVERY, AND RETURN TO   */ 00645000
*       /* THE DISPATCHER                                            */ 00646000
*       /*                                                           */ 00647000
*       /*************************************************************/ 00648000
*                                                                  0224 00649000
*       RFY                                                        0224 00650000
*         R1 RSTD;                                                 0224 00651000
@RF00189 DS    0H                                                  0225 00652000
*       R1=WKSAV@;                  /* OBTAIN ADDR WORK AREA TO BE 0225 00653000
