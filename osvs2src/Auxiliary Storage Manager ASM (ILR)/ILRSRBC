               RELATED=(VIO,ILRSRBC(FRELOC))                            00522000
*           ACEPTR=ATAACEQ;         /* OBTAIN ADDR FIRST ACE ON QUEUE*/ 00523000
         L     ACEPTR,ATAACEQ(,ATAPTR)                             0192 00524000
*           DO UNTIL ACEPTR=ZERO;   /* LOOP THRU ALL ACES OF ATAACEQ */ 00525000
@DL00193 DS    0H                                                  0194 00526000
*                                                                  0194 00527000
*             /*******************************************************/ 00528000
*             /*                                                     */ 00529000
*             /* PROCESS RELEASE AND DEACTIVATE REQUESTS BY CALLING  */ 00530000
*             /* RELEASE GROUP OPERATOR                              */ 00531000
*             /*                                                     */ 00532000
