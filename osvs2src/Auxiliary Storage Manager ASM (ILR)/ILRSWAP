*   R1=ADDR(SCCWSK);                /* GET 8 BYTE ANSWER FIELD ADDR  */ 01336000
         LA    R1,SCCWSK(,SCCWPTR)                                 0462 01337000
*   R13=ADDR(ASMSAVE);              /* GET SAVE AREA ADDRESS         */ 01338000
*                                                                  0463 01339000
         LA    R13,ASMSAVE(,ASMVTPTR)                              0463 01340000
*   /*****************************************************************/ 01341000
*   /*                                                               */ 01342000
*   /* CALL ILRCNVRT TO CONVERT RBA TO MBBCCHHR.                     */ 01343000
*   /*                                                               */ 01344000
*   /*****************************************************************/ 01345000
*                                                                  0464 01346000
*   CALL ILRCNVRT;                  /* GET MBBCCHHR                  */ 01347000
         BAL   @14,ILRCNVRT                                        0464 01348000
*   RFY                                                            0465 01349000
*    (R0,                                                          0465 01350000
*     R1,                                                          0465 01351000
*     R13,                                                         0465 01352000
*     QREG) UNRSTD;                                                0465 01353000
*                                                                  0466 01354000
*   /*****************************************************************/ 01355000
*   /*                                                               */ 01356000
*   /* DETERMINE IF CONVERSION WAS SUCCESSFUL. IF NOT, ABEND AND     */ 01357000
*   /* RETURN RESOURCES.                                             */ 01358000
*   /*                                                               */ 01359000
*   /*****************************************************************/ 01360000
*                                                                  0466 01361000
*   RFY                                                            0466 01362000
*    (R15) RSTD;                                                   0466 01363000
*   IF R15^=CODE0 THEN              /* IF CONVERSION FAILED          */ 01364000
