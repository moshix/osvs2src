         TITLE 'ILRSWAP, ASM SWAP CONTROLLER                           *00001000
                        '                                               00002000
ILRSWAP  CSECT                                                          00003000
         USING *,R15                                                    00004000
         MODID                                                          00005000
         DROP  R15                                                      00006000
         L     R15,CVTPTR          GET CVT ADDRESS                      00007000
         L     R15,CVTASMVT(,R15)  GET ASMVT ADDRESS                    00008000
         STM   R2,R14,ASMWKSA4(R15) STORE CALLERS REGS                  00009000
         ENTRY ILRCBSP1             FRR REFERENCED LABEL                00010000
         BALR  R12,0               ESTABLISH BASE                       00011000
         USING *,R12               TELL ASSEMBLER                       00012000
*ILRCBSP1:                                                         0122 00013000
*   ASMVTPTR=R15;                   /* LOAD PERMANENT ASMVT POINTER  */ 00014000
ILRCBSP1 LR    ASMVTPTR,R15                                        0122 00015000
*   RFY                                                            0123 00016000
*     R15 UNRSTD;                                                  0123 00017000
*                                                                  0123 00018000
*   /*****************************************************************/ 00019000
*   /*                                                               */ 00020000
*   /* LOAD THE FRR STACK FOR RECOVERY, AND SETUP TO PROCESS.        */ 00021000
*   /*                                                               */ 00022000
*   /*****************************************************************/ 00023000
*                                                                  0124 00024000
*   FADDR=ASMPIOFR;                 /* GET ADDRESS OF IOCTL FRR      */ 00025000
         L     FADDR,ASMPIOFR(,ASMVTPTR)                           0124 00026000
*   GENERATE REFS(FADDR) SETS(ATAPTR,R7,R8);                       0125 00027000
         SETFRR A,FRRAD=(FADDR),PARMAD=(ATAPTR),WRKREGS=(R7,R8)         00028000
*   ATAMODID=ATAMSWAP;              /* SWAP SET THE FRR              */ 00029000
         MVI   ATAMODID(ATAPTR),X'03'                              0126 00030000
*   ATASWAP='1'B;                   /* SWAP IS IN CONTROL            */ 00031000
         OI    ATASWAP(ATAPTR),B'10000000'                         0127 00032000
*   IF R1->AIALSQA='0'B THEN        /* IF THE FIRST AIA IS NOT FOR 0128 00033000
*                                      LSQA                          */ 00034000
         TM    AIALSQA(R1),B'00001000'                             0128 00035000
         BNZ   @RF00128                                            0128 00036000
*     DO;                                                          0129 00037000
*       RFY                                                        0130 00038000
*        (R15) RSTD;                                               0130 00039000
*       R1->AIAERROR='1'B;          /* SIGNAL AN ERROR               */ 00040000
         OI    AIAERROR(R1),B'00000010'                            0131 00041000
*       R15=CODE4;                  /* LOAD ERROR RETURN CODE        */ 00042000
         LA    R15,4                                               0132 00043000
*       GOTO DELETFRR;              /* AND RETURN IT TO RSM          */ 00044000
         B     DELETFRR                                            0133 00045000
*       RFY                                                        0134 00046000
*        (R15) UNRSTD;                                             0134 00047000
*     END;                                                         0135 00048000
*   ASHSWAPQ=R1;                    /* QUE AIA'S TO SWAP QUEUE       */ 00049000
@RF00128 ST    R1,ASHSWAPQ(,RSMHDPTR)                              0136 00050000
*   IF R1->AIAWRITE='1'B THEN       /* IF THIS IS A SWAP OUT REQUEST */ 00051000
         TM    AIAWRITE(R1),B'10000000'                            0137 00052000
         BNO   @RF00137                                            0137 00053000
*     ASHSWPOT='1'B;                /* TURN ON THE SWAP OUT FLAG     */ 00054000
         OI    ASHSWPOT(RSMHDPTR),B'10000000'                      0138 00055000
*   ELSE                            /* IF THIS IS A SWAP IN REQUEST  */ 00056000
*     ASHSWPOT='0'B;                /* RESET IN CASE LEFT ON IN ERROR*/ 00057000
         B     @RC00137                                            0139 00058000
@RF00137 NI    ASHSWPOT(RSMHDPTR),B'01111111'                      0139 00059000
*   PREVAIA=R1;                     /* REMEMBER FIRST AIA            */ 00060000
@RC00137 LR    PREVAIA,R1                                          0140 00061000
*   AIAPTR=R1;                      /* LOAD POINTER TO FIRST AIA     */ 00062000
         LR    AIAPTR,R1                                           0141 00063000
*                                                                  0142 00064000
*   /*****************************************************************/ 00065000
*   /*                                                               */ 00066000
*   /* SEARCH THE AIA CHAIN LOOKING FOR A NON-LSQA REQUEST. WHEN     */ 00067000
*   /* FOUND, PASS THE NON-LSQA REQUESTS TO ILRPAGIO FOR NORMAL PAGE */ 00068000
*   /* PROCESSING. AFTER THE NON-LSQA AIA'S HAVE BEEN PROCESSED: 1). */ 00069000
*   /* IF THE REQUEST IS FOR A SWAP-IN CALL ILRSLSQA TO PROCESS THE  */ 00070000
*   /* LSQA PAGES. 2). IF THE REQUEST IS FOR A SWAP-OUT AND ALL      */ 00071000
*   /* NON-LSQA I/O OPERATIONS ARE COMPLETE CALL ILRSLSQA TO PROCESS */ 00072000
*   /* THE LSQA PAGES. IF AN ERROR IS DETECTED AT ANY TIME, THE AIA'S*/ 00073000
*   /* NOT YET PROCESSED ARE RETURNED TO THE CALLER. THIS MODULE     */ 00074000
*   /* ALWAYS ASSUMES THAT THERE IS AT LEAST ONE AIA PASSED AND THAT */ 00075000
*   /* IT IS AN LSQA AIA.                                            */ 00076000
*   /*                                                               */ 00077000
*   /*****************************************************************/ 00078000
*                                                                  0142 00079000
*   DO WHILE AIAPTR^=0;             /* LOOP WHILE AIA'S TO PROCESS   */ 00080000
         B     @DE00142                                            0142 00081000
@DL00142 DS    0H                                                  0143 00082000
*     IF(AIAWRITE='1'B&ASHSWPOT='0'B)|/* IF MIXED IN AND OUT         */ 00083000
*         (AIAWRITE='0'B&ASHSWPOT='1'B) THEN/* REQUESTS              */ 00084000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0143 00085000
         BNO   @GL00001                                            0143 00086000
         TM    ASHSWPOT(RSMHDPTR),B'10000000'                      0143 00087000
         BZ    @RT00143                                            0143 00088000
@GL00001 TM    AIAWRITE(AIAPTR),B'10000000'                        0143 00089000
         BNZ   @RF00143                                            0143 00090000
         TM    ASHSWPOT(RSMHDPTR),B'10000000'                      0143 00091000
         BNO   @RF00143                                            0143 00092000
@RT00143 DS    0H                                                  0144 00093000
*       DO;                                                        0144 00094000
*         AIAERROR='1'B;            /* MARK AIA AS AN ERROR          */ 00095000
         OI    AIAERROR(AIAPTR),B'00000010'                        0145 00096000
*         GOTO ILRCRSP1;            /* AND RETURN REQUEST TO CALLER  */ 00097000
         B     ILRCRSP1                                            0146 00098000
*       END;                                                       0147 00099000
*     IF AIALSQA='0'B THEN          /* IF THIS IS FIRST NON-LSQA AIA */ 00100000
@RF00143 TM    AIALSQA(AIAPTR),B'00001000'                         0148 00101000
         BNZ   @RF00148                                            0148 00102000
*       DO;                         /* HANDLE NON-LSQA AIA'S         */ 00103000
*         PREVAIA->AIANXAIA=0;      /* DEQUEUE FROM LSQA AIA'S       */ 00104000
         SLR   @07,@07                                             0150 00105000
         ST    @07,AIANXAIA(,PREVAIA)                              0150 00106000
*         R1=AIAPTR;                /* LOAD PARM REG                 */ 00107000
         LR    R1,AIAPTR                                           0151 00108000
*         CALL ILRPAGIO;            /* PASS NON-LSQA AIA'S TO      0152 00109000
*                                      ILRPAGIO                      */ 00110000
         L     @15,@CV01874                                        0152 00111000
         BALR  @14,@15                                             0152 00112000
*         RFY                                                      0153 00113000
*          (R15) RSTD;                                             0153 00114000
*         IF R15^=0 THEN            /* IF ILRPAGI0 HAD AN ERROR      */ 00115000
         LTR   R15,R15                                             0154 00116000
         BZ    @RF00154                                            0154 00117000
*           DO;                     /* HANDLE ERROR                  */ 00118000
*             PREVAIA->AIANXAIA=R1; /* PUT ERROR CHAIN ON END OF LSQA*/ 00119000
         ST    R1,AIANXAIA(,PREVAIA)                               0156 00120000
*ILRCRSP1:                                                         0157 00121000
*             R1=ASHSWAPQ;          /* LOAD PARM REG - 1ST AIA PASSED*/ 00122000
ILRCRSP1 L     R1,ASHSWAPQ(,RSMHDPTR)                              0157 00123000
*             ASHSWAPQ=0;           /* RESET SWAP QUEUE              */ 00124000
         SLR   @14,@14                                             0158 00125000
         ST    @14,ASHSWAPQ(,RSMHDPTR)                             0158 00126000
*             R15=CODE4;            /* SET BAD RETURN CODE THEN      */ 00127000
         LA    R15,4                                               0159 00128000
*             GOTO DELETFRR;        /* RETURN REQUEST TO CALLER      */ 00129000
         B     DELETFRR                                            0160 00130000
*           END;                                                   0161 00131000
*         AIAPTR=0;                 /* GET OUT OF LOOP               */ 00132000
@RF00154 SLR   AIAPTR,AIAPTR                                       0162 00133000
*         RFY                                                      0163 00134000
*          (R15) UNRSTD;                                           0163 00135000
*       END;                        /* AIALSQA^='1'B                 */ 00136000
*     ELSE                                                         0165 00137000
*       DO;                         /* IF STILL LSQA AIA'S           */ 00138000
         B     @RC00148                                            0165 00139000
@RF00148 DS    0H                                                  0166 00140000
*         PREVAIA=AIAPTR;           /* REMEMBER PREVIOUS             */ 00141000
         LR    PREVAIA,AIAPTR                                      0166 00142000
*         AIAPTR=AIANXAIA;          /* UPDATE AIA POINTER            */ 00143000
         L     AIAPTR,AIANXAIA(,AIAPTR)                            0167 00144000
*       END;                                                       0168 00145000
*   END;                            /* DO WHILE END                  */ 00146000
@RC00148 DS    0H                                                  0169 00147000
@DE00142 SLR   @07,@07                                             0169 00148000
         CR    AIAPTR,@07                                          0169 00149000
         BNE   @DL00142                                            0169 00150000
*                                                                  0170 00151000
*   /*****************************************************************/ 00152000
*   /*                                                               */ 00153000
*   /* DETERMINE IF LSQA REQUESTS CAN BE STARTED, AND IF SO CALL     */ 00154000
*   /* ILRSLSQA TO PROCESS.                                          */ 00155000
*   /*                                                               */ 00156000
*   /*****************************************************************/ 00157000
*                                                                  0170 00158000
*   IF ASHSWPOT='0'B|ASHIOCNT=0 THEN/* IF SWAPIN OR NO I/O IN      0170 00159000
*                                      PROGRESS                      */ 00160000
         TM    ASHSWPOT(RSMHDPTR),B'10000000'                      0170 00161000
         BZ    @RT00170                                            0170 00162000
         C     @07,ASHIOCNT(,RSMHDPTR)                             0170 00163000
         BNE   @RF00170                                            0170 00164000
@RT00170 DS    0H                                                  0171 00165000
*     CALL ILRSLSQA;                /* START THE LSQA REQUESTS, RSMHD   00166000
*                                      POINTER IS VALID              */ 00167000
         L     @15,@CA01901                                        0171 00168000
         BALR  @14,@15                                             0171 00169000
*   RFY                                                            0172 00170000
*    (R15) RSTD;                                                   0172 00171000
@RF00170 DS    0H                                                  0173 00172000
*   R15=CODE0;                      /* SET RETURN CODE TO ZERO       */ 00173000
         SLR   R15,R15                                             0173 00174000
*   R1=0;                           /* SET PARM REG TO ZERO          */ 00175000
*                                                                  0174 00176000
         SLR   R1,R1                                               0174 00177000
*   /*****************************************************************/ 00178000
*   /*                                                               */ 00179000
*   /* DELETE THE RECOVERY FRR STACK, RESTORE REGS, AND RETURN TO THE*/ 00180000
*   /* CALLER.                                                       */ 00181000
*   /*                                                               */ 00182000
*   /*****************************************************************/ 00183000
*                                                                  0175 00184000
*DELETFRR:                                                         0175 00185000
*   GENERATE SETS(R7,R8) REFS(ASMWKSA4,ASMVTPTR);                  0175 00186000
DELETFRR DS    0H                                                  0175 00187000
         SETFRR D,WRKREGS=(R7,R8)                                       00188000
         LM    R2,R14,ASMWKSA4(ASMVTPTR) RESTORE CALLERS REGS, R1       00189000
*                                    AND R15 HAVE BEEN SET PREVIOUSLY   00190000
         BR    R14               RETURN TO CALLER                       00191000
*   RFY                                                            0176 00192000
*    (R1,                                                          0176 00193000
*     R15) UNRSTD;                                                 0176 00194000
*ILRSLSQA:                                                         0177 00195000
*   ENTRY;                                                         0177 00196000
*                                                                  0177 00197000
*   /*****************************************************************/ 00198000
*   /*                                                               */ 00199000
*   /* ENTRY TO ILRSLSQA, THE LSQA PROCESSOR FOR SWAP. THIS ROUTINE  */ 00200000
*   /* WILL BUILD & QUEUE SCCW'S FOR LSQA AIA'S FOR EITHER A SWAP-OUT*/ 00201000
*   /* OR SWAP-IN. AN FRR IS ALWAYS EXPECTED TO HAVE BEEN SET BY THE */ 00202000
*   /* CALLER.                                                       */ 00203000
*   /*                                                               */ 00204000
*   /*****************************************************************/ 00205000
*                                                                  0178 00206000
*   GENERATE REFS(ASMWKSA5,ASMVTPTR);                              0178 00207000
         ENTRY ILRSLSQA                                                 00208000
ILRSLSQA STM   R2,R14,ASMWKSA5(ASMVTPTR) SAVE CALLERS REGS              00209000
         ENTRY ILRCBSP2                                                 00210000
         BALR  R12,0               ESTABLISH BASE                       00211000
         USING *,R12               TELL ASSEMBLER                       00212000
*ILRCBSP2:                                                         0179 00213000
*   ATASLSQA='1'B;                  /* ILRSLSQA IS IN CONTROL        */ 00214000
ILRCBSP2 OI    ATASLSQA(ATAPTR),B'01000000'                        0179 00215000
*   SWPFLGS=0;                      /* CLEAR INTERNAL FLAGS          */ 00216000
*                                                                  0180 00217000
         MVI   SWPFLGS(ASMVTPTR),X'00'                             0180 00218000
*   /*****************************************************************/ 00219000
*   /*                                                               */ 00220000
*   /* IF THIS MODULE IS ENTERED WITH A VALID RSMHD POINTER,         */ 00221000
*   /* PROCESSING IS REQUESTED FOR A SPECIFIC ADDRESS SPACE. IF THE  */ 00222000
*   /* RSMHD POINTER IS ZERO, THE ENTRY WAS MADE FOR A REDRIVE       */ 00223000
*   /* SITUATION AND THE WAIT QUEUE IS THE ONLY POSSIBLE SOURCE OF   */ 00224000
*   /* WORK. IN THIS CASE PROCESSING STARTS AT WORKCK.               */ 00225000
*   /*                                                               */ 00226000
*   /*****************************************************************/ 00227000
*                                                                  0181 00228000
*   RFY                                                            0181 00229000
*    (GROUPAIA) RSTD;                                              0181 00230000
*   SARTPTR=ASMSART;                /* LOAD POINTER TO SART          */ 00231000
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0182 00232000
*   IF RSMHDPTR^=0 THEN             /* IF ENTRY WAS MADE TO PROCESS  */ 00233000
         SLR   @15,@15                                             0183 00234000
         CR    RSMHDPTR,@15                                        0183 00235000
         BE    @RF00183                                            0183 00236000
*     DO;                           /* A SPECIFIC MEMORY             */ 00237000
*       AIAPTR=ASHSWAPQ;            /* GET AIA'S FROM SWAP QUEUE     */ 00238000
         L     AIAPTR,ASHSWAPQ(,RSMHDPTR)                          0185 00239000
*       GROUPAIA=AIAPTR;            /* SAVE POINTER TO THE FIRST AIA    00240000
*                                      OF A GROUP TO BE BUILT        */ 00241000
         LR    GROUPAIA,AIAPTR                                     0186 00242000
*                                                                  0187 00243000
*       /*************************************************************/ 00244000
*       /*                                                           */ 00245000
*       /* IF A SWAP-OUT REQUEST & SETS ARE AVAILABLE, OR IF A       */ 00246000
*       /* SWAP-IN REQUEST FOR A SET OF PAGES WRITTEN TO A SWAP DATA */ 00247000
*       /* SET, THE AIA'S MUST BE PUT INTO GROUPS. OTHERWISE, THE    */ 00248000
*       /* AIA'S MUST BE COUNTED AND SENT TO ILRQIOE TO BE PROCESSED */ 00249000
*       /* AS REGULAR PAGES. ANY WORK THAT IS QUEUED TO THE WAIT     */ 00250000
*       /* QUEUE WILL BE STARTED IF RESOURCES ARE AVAILABLE.         */ 00251000
*       /*                                                           */ 00252000
*       /*************************************************************/ 00253000
*                                                                  0187 00254000
*       IF(AIAWRITE='1'B&SARSETCT>0)|/* IF A SWAP-OUT REQUEST AND A     00255000
*                                      SET IS AVAILABLE              */ 00256000
*           (AIAWRITE='0'B&AIAPAGDS='0'B) THEN/* OR IF A SWAP-IN FOR    00257000
*                                      A SWAP DATA SET               */ 00258000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0187 00259000
         BNO   @GL00005                                            0187 00260000
         C     @15,SARSETCT(,SARTPTR)                              0187 00261000
         BL    @RT00187                                            0187 00262000
@GL00005 TM    AIAWRITE(AIAPTR),B'10000100'                        0187 00263000
         BNZ   @RF00187                                            0187 00264000
@RT00187 DS    0H                                                  0188 00265000
*         DO;                       /* GROUP PROCESSING              */ 00266000
*           RFY                                                    0189 00267000
*            (PAGECTR) RSTD;                                       0189 00268000
*           PAGECTR=1;              /* INIT PAGE COUNTER             */ 00269000
         LA    @15,1                                               0190 00270000
         LR    PAGECTR,@15                                         0190 00271000
*           ASM5GCTR=1;             /* INIT GROUP COUNT              */ 00272000
*                                                                  0191 00273000
         ST    @15,ASM5GCTR(,ASMVTPTR)                             0191 00274000
*           /*********************************************************/ 00275000
*           /*                                                       */ 00276000
*           /* PUT AIA'S INTO GROUPS. THE NUMBER OF AIA'S IN A GROUP */ 00277000
*           /* IS DEPENDENT ON SARSETSZ - THE NUMBER OF SLOTS IN A   */ 00278000
*           /* SWAP SET.                                             */ 00279000
*           /*                                                       */ 00280000
*           /*********************************************************/ 00281000
*                                                                  0192 00282000
*           DO WHILE AIANXAIA^=0;   /* LOOP TILL OUT OF AIA'S        */ 00283000
         B     @DE00192                                            0192 00284000
@DL00192 DS    0H                                                  0193 00285000
*             IF PAGECTR^=SARSETSZ THEN/* IF GROUP IS NOT FULL       */ 00286000
         C     PAGECTR,SARSETSZ(,SARTPTR)                          0193 00287000
         BE    @RF00193                                            0193 00288000
*               DO;                 /* CONTINUE GROUPING             */ 00289000
*                 AIAFQPA=AIANXAIA; /* CHAIN LATERALLY               */ 00290000
         L     @09,AIANXAIA(,AIAPTR)                               0195 00291000
         ST    @09,AIAFQPA(,AIAPTR)                                0195 00292000
*                 AIANXAIA=0;       /* RESET NEXT CHAIN POINTER      */ 00293000
         SLR   @15,@15                                             0196 00294000
         ST    @15,AIANXAIA(,AIAPTR)                               0196 00295000
*                 AIAPTR=AIAFQPA;   /* GET NEXT AIA                  */ 00296000
         LR    AIAPTR,@09                                          0197 00297000
*                 PAGECTR=PAGECTR+1;/* ADD ONE TO COUNTER            */ 00298000
         AL    PAGECTR,@CF00061                                    0198 00299000
*               END;                /* STILL GROUPING                */ 00300000
*             ELSE                  /* IF GROUP IS FULL              */ 00301000
*               DO;                 /* FINISH GROUP AND START NEXT   */ 00302000
         B     @RC00193                                            0200 00303000
@RF00193 DS    0H                                                  0201 00304000
*                 GROUPAIA->AIANXAIA=AIANXAIA;/* CHAIN NEXT AIA AS A    00305000
*                                      GROUP HEADER                  */ 00306000
         L     @09,AIANXAIA(,AIAPTR)                               0201 00307000
         ST    @09,AIANXAIA(,GROUPAIA)                             0201 00308000
*                 AIANXAIA=0;       /* RESET NEXT CHAIN POINTER      */ 00309000
         SLR   @15,@15                                             0202 00310000
         ST    @15,AIANXAIA(,AIAPTR)                               0202 00311000
*                 GROUPAIA->AIAGRPSZ=PAGECTR;/* SET SET SIZE IN    0203 00312000
*                                      HEADER AIA                    */ 00313000
         ST    PAGECTR,AIAGRPSZ(,GROUPAIA)                         0203 00314000
*                 ASM5GCTR=ASM5GCTR+1;/* ADD ONE TO GROUP COUNT      */ 00315000
         LA    @15,1                                               0204 00316000
         L     @14,ASM5GCTR(,ASMVTPTR)                             0204 00317000
         ALR   @14,@15                                             0204 00318000
         ST    @14,ASM5GCTR(,ASMVTPTR)                             0204 00319000
*                 AIAPTR=GROUPAIA->AIANXAIA;/* GET NEXT AIA POINTER  */ 00320000
         LR    AIAPTR,@09                                          0205 00321000
*                 GROUPAIA=AIAPTR;  /* UPDATE GROUP HEADER           */ 00322000
         LR    GROUPAIA,AIAPTR                                     0206 00323000
*                 PAGECTR=1;        /* RE-INIT THE PAGE COUNTER      */ 00324000
         LR    PAGECTR,@15                                         0207 00325000
*               END;                /* NEXT GROUP STARTED            */ 00326000
*           END;                    /* LOOP END                      */ 00327000
@RC00193 DS    0H                                                  0209 00328000
@DE00192 L     @09,AIANXAIA(,AIAPTR)                               0209 00329000
         LTR   @09,@09                                             0209 00330000
         BNZ   @DL00192                                            0209 00331000
*                                                                  0210 00332000
*           /*********************************************************/ 00333000
*           /*                                                       */ 00334000
*           /* FINISH UP GROUP PROCESSING AND DETERMINE COURSE OF    */ 00335000
*           /* ACTION.                                               */ 00336000
*           /*                                                       */ 00337000
*           /*********************************************************/ 00338000
*                                                                  0210 00339000
*           GROUPAIA->AIAGRPSZ=PAGECTR;/* THE COUNT MUST BE INSERTED    00340000
*                                      IN CASE THE SET WAS NOT FULL  */ 00341000
         ST    PAGECTR,AIAGRPSZ(,GROUPAIA)                         0210 00342000
*           RFY                                                    0211 00343000
*            (GROUPAIA,                                            0211 00344000
*             PAGECTR) UNRSTD;                                     0211 00345000
*                                                                  0211 00346000
*           /*********************************************************/ 00347000
*           /*                                                       */ 00348000
*           /* FOR SWAP OUT PROCESSING ASIGNSET MUST BE CALLED TO    */ 00349000
*           /* ASSIGN LSID'S TO THE SETS. IF SETS ARE NOT AVAILABLE, */ 00350000
*           /* ASIGNSET WILL RECHAIN THE GROUPS BACK INTO A SINGLE   */ 00351000
*           /* CHAIN. IF A SINGLE CHAIN WAS BUILT, THE AIA'S WILL BE */ 00352000
*           /* TREATED AS REQULAR PAGES AND SENT TO ILRQIOE. IN THE  */ 00353000
*           /* NORMAL CASE HOWEVER, THEY WILL BE PROCESSED AS A GROUP*/ 00354000
*           /* BY SCCWPROC. SWAP IN REQUESTS ARE ALWAYS TREATED AS A */ 00355000
*           /* GROUP AT THIS POINT.                                  */ 00356000
*           /*                                                       */ 00357000
*           /*********************************************************/ 00358000
*                                                                  0212 00359000
*           IF ASHSWPOT='1'B THEN   /* IF THIS IS A SWAP-OUT REQUEST */ 00360000
         TM    ASHSWPOT(RSMHDPTR),B'10000000'                      0212 00361000
         BNO   @RF00212                                            0212 00362000
*             DO;                                                  0213 00363000
*               ASHSWPCT=SPCTLSQA;  /* SET THE SWAP COUNT            */ 00364000
*                                                                  0214 00365000
         L     @07,SPCTPTR(,RSMHDPTR)                              0214 00366000
         LH    @07,SPCTLSQA(,@07)                                  0214 00367000
         STH   @07,ASHSWPCT(,RSMHDPTR)                             0214 00368000
*               /*****************************************************/ 00369000
*               /*                                                   */ 00370000
*               /* CALL ASIGNSET TO OBTAIN SWAP SETS. UPON RETURN ,  */ 00371000
*               /* THE AIA POINTER IS POINTING TO THE LAST AIA ON THE*/ 00372000
*               /* CHAIN IF R.C. = 4                                 */ 00373000
*               /*                                                   */ 00374000
*               /*****************************************************/ 00375000
*                                                                  0215 00376000
*               CALL ASIGNSET;      /* GET LSID'S ASSIGNED           */ 00377000
         BAL   @14,ASIGNSET                                        0215 00378000
*               RFY                                                0216 00379000
*                (R15) RSTD;                                       0216 00380000
*               IF R15^=0 THEN      /* IF AIA'S WERE RECHAINED       */ 00381000
         LTR   R15,R15                                             0217 00382000
         BNZ   @RT00217                                            0217 00383000
*                 GOTO QIOEPREP;    /* PREPARE TO CALL ILRQIOE.    0218 00384000
*                                      AIAPTR WAS SET TO LAST AIA ON    00385000
*                                      CHAIN BY ASIGNSET.            */ 00386000
*               RFY                                                0219 00387000
*                (R15) UNRSTD;                                     0219 00388000
*             END;                                                 0220 00389000
*                                                                  0221 00390000
*           /*********************************************************/ 00391000
*           /*                                                       */ 00392000
*           /* PREPARE TO CALL SCCWPROC TO BUILD & QUQUE SCCW'S.     */ 00393000
*           /* PRIOR TO CALLING SCCWPROC, INSURE THAT RESOURCES ARE  */ 00394000
*           /* AVAILABLE. IF THERE IS WORK ON THE WAIT QUEUE, IT MUST*/ 00395000
*           /* BE DONE PRIOR TO ANY NEW WORK, I.E. SERIALIZATION.    */ 00396000
*           /*                                                       */ 00397000
*           /*********************************************************/ 00398000
*                                                                  0221 00399000
*           AIAPTR=ASHSWAPQ;        /* NEW WORK IS ON THE SWAP QUE   */ 00400000
