FINDONE  DS    0H                                                  0396 01045000
         LR    R7,R2               SAVE RSMHD POINTER                   01046000
         LH    R8,SATMAPLN(SATPTR) GET LENGTH OF SATMAP                 01047000
         LH    R9,SATOFFST(SATPTR) GET OFFSET OF LAST USED MAP          01048000
         SR    R8,R9               GET LENGTH TO SCAN                   01049000
         LA    R15,256             GET LENGTH TRT CAN HANDLE            01050000
         L     R9,SATSCAN(SATPTR)  GET START ADDRESS                    01051000
REDO1    DS    0H                                                       01052000
         SR    R8,R15              REMOVE LENGTH TRT CAN HANDLE         01053000
         BP    TRTONE              BRANCH IF FULL SCAN                  01054000
         AR    R8,R15              RESTORE IF NOT FULL SCAN (256)       01055000
         BCTR  R8,R0               CORRECT LENGTH (0 REFERANCED)        01056000
         EX    R8,TRTQUICK         TRANSLATE                            01057000
         BC    6,FOUNDONE          BRANCH IF SUCCESSFUL TRANSLATION     01058000
         LH    R8,SATMAPLN(SATPTR) GET FULL MAP LENGTH                  01059000
         LA    R9,SATMAPS(SATPTR)  GET START OF MAP                     01060000
*                                                                       01061000
REDO2    DS    0H                                                       01062000
         SR    R8,R15              REMOVE LENGTH TRT CAN HANDLE         01063000
         BP    TRTTWO              BRANCH IF FULL SCAN                  01064000
         AR    R8,R15              RESTORE IF NOT FULL SCAN             01065000
         BCTR  R8,0                CORRECT LENGTH (0 REFERANCED)        01066000
         EX    R8,TRTQUICK         TRANSLATE                            01067000
         BC    6,FOUNDONE          BRANCH IF SUCCESSFUL                 01068000
         ST    SAREPTR,ATASARTE(ATAPTR) STORE POINTER FOR RECOVERY      01069000
         LR    R2,R7               RESTORE RSMHD POINTER                01070000
*                                                                       01071000
         MVI   ATARCODE(ATAPTR),4  LOAD REASON CODE FOR FRR             01072000
         ABEND X'C0D',,,SYSTEM     ABEND - NO SET FOUND                 01073000
         B     ASGNRTRY            TRY TO GET ANOTHER SET               01074000
TRTONE   TRT   0(256,R9),FUBYTAB   SCAN FOR FREE BIT                    01075000
         BC    6,FOUNDONE          BRANCH IF SUCCESSFUL                 01076000
         LA    R9,256(R9)          GET NEXT 256 BYTES OF MAP TO SCAN    01077000
         B     REDO1               TRY AGAIN                            01078000
*                                                                       01079000
TRTTWO   TRT   0(256,R9),FUBYTAB   SCAN FOR FREE BIT                    01080000
         BC    6,FOUNDONE          BRANCH IF SUCCESSFUL                 01081000
         LA    R9,256(R9)          GET NEXT 256 BYTES TO SCAN           01082000
         B     REDO2               TRY AGAIN                            01083000
*                                                                       01084000
TRTQUICK TRT   0(0,R9),FUBYTAB     SCAN FOR FREE BIT                    01085000
*                                                                       01086000
*     A SWAP SET WAS FOUND. CB S MUST BE UPDATED FOR NEXT ALLOCATION.   01087000
*     RESULTS OF TRT -- R1= ADDR OF SELECTED BYTE, R2= FUNCTION         01088000
*                                                                       01089000
FOUNDONE LA    R1,0(R1)            CLEAR HI BYTE AFTER TRT              01090000
         SLL   R2,24               CLEAR 3 HI BYTES AFTER TRT           01091000
         SRL   R2,24               REPOSITION                           01092000
         BCTR  R2,0                BIT 1= 0 -CORRECT REFERENCE          01093000
         LR    R8,R2               SAVE FUNCTION BYTE                   01094000
         LA    R9,128(0)           LOAD MASK OF HEX 80                  01095000
         SRL   R9,0(R2)            SHIFT MASK BIT INTO POSITION         01096000
         IC    R14,0(R1)           GET MAP                              01097000
         OR    R14,R9              TURN ON BIT                          01098000
         STC   R14,0(R1)           STORE UPDATED MAP                    01099000
         LR    R2,R7               RESTORE RSMHD POINTER                01100000
*                                                                       01101000
         ST    R1,SATSCAN(SATPTR)  SAVE ADDRESS OF MAP FOR NEXT TRY     01102000
         LA    R9,SATMAPS(SATPTR)  GET START OF MAP                     01103000
         SR    R1,R9               GET OFFSET                           01104000
         STH   R1,SATOFFST(SATPTR) SAVE IT                              01105000
         LR    R14,R1              GET READY TO CALCULATE CYLINDER      01106000
*                                                                       01107000
         LH    R9,SATBYTCL(SATPTR) GET NUMBER OF BYTES TO MAP 1 CYL     01108000
         SR    R15,R15             CLEAR REG                            01109000
         SRDA  R14,0(R9)           DIVIDE BY BYTES TO MAP 1 CYL         01110000
         LR    R7,R14              SAVE CYLINDER NUMBER                 01111000
*                                                                       01112000
         SR    R14,R14             CLEAR REG                            01113000
         SLDL  R14,3(R9)           MULTIPLY ADDITIONAL BYTES BY 8 BITS  01114000
         AR    R8,R14              GET SWAP GROUP WITHIN CYLINDER       01115000
*                                                                       01116000
         SR    R6,R6               CLEAR REG                            01117000
         L     R14,SRESDCTE(SAREPTR) LOAD SDCT POINTER                  01118000
         LH    R14,SDCCYLSZ(R14)   GET NUMBER OF SLOTS PER CYLINDER     01119000
         MR    R6,R14              GET SLOTS X CYLINDER VALUE           01120000
         SRDL  R8,32               R9 = R8, R8 = 0 - READY TO MULTIPLY  01121000
         L     R14,SARSETSZ(SARTPTR) GET NUMBER OF SLOTS PER SWAP SET   01122000
         MR    R8,R14              GET TOTAL SLOTS FOR SETS             01123000
         AR    R7,R9               GET Q PORTION OF LSID                01124000
*                                                                       01125000
LOADAIA  L     R9,SARSETCT(SARTPTR) GET TOTAL SETS AVAILABLE COUNT      01126000
         BCTR  R9,0                SUBTRACT ONE                         01127000
         ST    R9,SARSETCT(SARTPTR) RETURN UPDATED COUNT                01128000
         L     R9,SREAVLSL(SAREPTR) PICK UP AVAIL SLOT CTR              01129000
         BCTR  R9,0                DECREMENT BY ONE                     01130000
         ST    R9,SREAVLSL(SAREPTR)  STORE NEW COUNT                    01131000
         LR    R9,R7               PUT QQQQ IN ODD REG OF PAIR          01132000
         SLL   R9,16               LEFT JUSTIFY                         01133000
         LH    R8,SRENN(SAREPTR)   GET NN PORTION OF LSID               01134000
         SRDL  R8,16               SHIFT NN TO R9 & JUSTIFY QQ IN R9    01135000
*   RFY                                                            0397 01136000
*    (R7,                                                          0397 01137000
*     R8,                                                          0397 01138000
*     R14,                                                         0397 01139000
*     R15) UNRSTD;                                                 0397 01140000
*                                                                  0398 01141000
*   /*****************************************************************/ 01142000
*   /*                                                               */ 01143000
*   /* INSERT LSIDS INTO AIA'S IN THE SWAP GROUPS AND PROCESS NEXT   */ 01144000
*   /* GROUP IF ANY EXIST.                                           */ 01145000
*   /*                                                               */ 01146000
*   /*****************************************************************/ 01147000
*                                                                  0398 01148000
*   RFY                                                            0398 01149000
*    (TEMPAIA) RSTD;                                               0398 01150000
*   AIAPTR=ASM5AIA;                 /* LOAD POINTER TO AIA GROUP     */ 01151000
         L     AIAPTR,ASM5AIA(,ASMVTPTR)                           0399 01152000
*   TEMPAIA=AIANXAIA;               /* SAVE NEXT AIA GROUP           */ 01153000
         L     TEMPAIA,AIANXAIA(,AIAPTR)                           0400 01154000
*   DO UNTIL AIAPTR=0;              /* PUT LSIDS IN ENTIRE GROUP     */ 01155000
