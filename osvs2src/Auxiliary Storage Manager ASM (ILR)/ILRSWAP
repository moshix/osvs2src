         CS    TEMPSCCW,@09,SRESCCW(SAREPTR)                       0528 01549000
*       BC(NE,CSLP2);               /* IF C&S FAILED, TRY AGAIN      */ 01550000
         BC    7,CSLP2                                             0529 01551000
*       SCHEDRVR='1'B;              /* SWAP DRIVER NEEDS TO BE     0530 01552000
*                                      SCHEDULED BY THE CALLER       */ 01553000
         OI    SCHEDRVR(ASMVTPTR),B'10000000'                      0530 01554000
*     END;                                                         0531 01555000
*   ATASCCWP='0'B;                  /* INDICATE CONTROL BEING      0532 01556000
*                                      RELINQUISHED                  */ 01557000
@RC00467 NI    ATASCCWP(ATAPTR),B'10111111'                        0532 01558000
*   RFY                                                            0533 01559000
*    (R14) RSTD;                                                   0533 01560000
*   R14=ASM5SR14;                   /* RESTORE RETURN REGISTER       */ 01561000
         L     R14,ASM5SR14(,ASMVTPTR)                             0534 01562000
*   RFY                                                            0535 01563000
*    (TEMPSCCW,                                                    0535 01564000
*     R14,                                                         0535 01565000
*     SAREPTR,                                                     0535 01566000
*     SCCWPTR) UNRSTD;                                             0535 01567000
*   END SCCWPROC;                                                  0536 01568000
@EL00003 DS    0H                                                  0536 01569000
@EF00003 DS    0H                                                  0536 01570000
@ER00003 BR    @14                                                 0536 01571000
*                                                                  0537 01572000
*/* MACCOMP Y-4SCICW/PZD81                                           */ 01573000
*/* MACSTAT Y-475152/415304                                          */ 01574000
*/*A 000000-999999                                           @Z40WPWF*/ 01575000
*/********************************************************************/ 01576000
*/*                                                                  */ 01577000
*/* NAME: ILRCNVRT                                                   */ 01578000
*/*                                                                  */ 01579000
*/* STATUS: OS/VS2 RELEASE 4.1, LEVEL 0                              */ 01580000
*/*                                                                  */ 01581000
*/* ENTRY POINT: ILRCNVRT                                            */ 01582000
*/*                                                                  */ 01583000
*/* FUNCTION: CONVERT AN RBA TO A FULL SEEK ADDRESS- MBBCCHHR        */ 01584000
*/*                                                                  */ 01585000
*/* INPUT: R0- EDB HEADER ADDRESS                                    */ 01586000
*/*        R1- POINTER TO 8 BYTE AREA TO STORE SEEK ADDRESS          */ 01587000
*/*            WORD 1- RBA TO BE CONVERTED                           */ 01588000
*/*        R13- SAVE AREA POINTER                                    */ 01589000
*/*        R14- RETURN ADDRESS                                       */ 01590000
*/*                                                                  */ 01591000
*/* PROCESS: CONVERTS AN RBA TO MBBCCHHR USING THE EDB AND LPMB.     */ 01592000
*/*                                                                  */ 01593000
*/* OUTPUT: MBBCCHHR IN 8 BYTE AREA POINTED TO BY REG 1              */ 01594000
*/*         R15- RETURN CODE                                         */ 01595000
*/*              0 - CONVERSION OK                                   */ 01596000
*/*              4- EDB NOT FOUND FOR THIS RBA                       */ 01597000
*/*                                                                  */ 01598000
*/* EXIT-NORMAL: RETURN TO CALLER                                    */ 01599000
*/*              R15= 0                                              */ 01600000
*/*                                                                  */ 01601000
*/* EXIT-ERROR: RETURN TO CALLER                                     */ 01602000
*/*              R15= 4                                              */ 01603000
*/*                                                                  */ 01604000
*/* EXTERNAL REFERENCES: NONE                                        */ 01605000
*/*                                                                  */ 01606000
*/* REGISTERS CLOBBERED: REGISTERS SAVED UPON ENTRY                  */ 01607000
*/*                      THEN REGS 0-5 USED FOR PROCESSING           */ 01608000
*/*                      UPON RETURN, ALL REGS RESTORED EXCEPT       */ 01609000
*/*                      R15 WHICH CONTAINS THE RETURN CODE          */ 01610000
*/*                                                                  */ 01611000
*/* MAPPING MACROS USED:  IDAEDB                                     */ 01612000
*/*                       IDALPMB                                    */ 01613000
*/*                                                                  */ 01614000
*/* NOTES: THE CALLING MODULE MUST INCLUDE THE MAPPING MACROS LISTED */ 01615000
*/*        ABOVE. ADDRESSABILITY FOR THESE MACROS WILL BE SET UP IN  */ 01616000
*/*        THIS ROUTINE.                                             */ 01617000
*/*        ALL LABELS AND DATA AREAS CREATED BY THIS ROUTINE BEGIN   */ 01618000
*/*        WITH THE FIVE CHARACTERS 'CNVRT'.                         */ 01619000
*/*        THIS ROUTINE REQUIRES THAT THE CALLING MODULE DECLARE     */ 01620000
*/*        REGISTERS AS R0-R15.                                      */ 01621000
*/*                                                                  */ 01622000
*/********************************************************************/ 01623000
*                                                                  0537 01624000
*                                                                  0537 01625000
*/*ILRCNVRT: CHART                                                   */ 01626000
*/* HEADER                                                         0537 01627000
*/*  ILRCNVRT:  ASM CONVERT ROUTINE                                  */ 01628000
*/*ILRCNVRT:  E CONVERT RBA TO SEEK ADDR                             */ 01629000
*/* P SET UP PTR TO EDB HDR                                          */ 01630000
*/* P SET UP PTR TO 1ST EDB ENTRY                                    */ 01631000
*/* P PICK UP RBA FROM INPUT PARMS                                   */ 01632000
*/* P INITIALIZE RETURN CODE TO 4                                    */ 01633000
*/* N LOOP WHILE R15 = 4                                             */ 01634000
*/*%CNV1: D (NO,,YES,%CNV4) ALL EDB ENTRIES TRIED?                   */ 01635000
*/*       D (YES,,NO,%CNV4) RETURN CODE STILL 4?                     */ 01636000
*/*       D (YES,,NO,%CNV2) RBA IN THIS EDB ENTRY                    */ 01637000
*/* P (,%CNV1) SET RETURN CODE TO ZERO                               */ 01638000
*/*%CNV2: P (,%CNV1) UPDATE TO NEXT EDB ENTRY                        */ 01639000
*/* N END OF LOOP                                                    */ 01640000
*/*%CNV4: D (YES,,NO,%CNVEXT)  EDB ENTRY FOUND?                      */ 01641000
*/* P GET LPMB ADDRESS                                               */ 01642000
*/* P ZERO 8 BYTE OUTPUT AREA                                        */ 01643000
*/* P GET RELATIVE RBA PER THIS EXTENT                               */ 01644000
*/* P SET UP EXTENT NUMBER- M                                        */ 01645000
*/* P CALCULATE RELATIVE CYLINDER FOR DATA SET                       */ 01646000
*/* P CALCULATE RELATIVE TRACK FOR DATA SET                          */ 01647000
*/* P CALCULATE RECORD NUMBER- R                                     */ 01648000
*/* P CALCULATE ACTUAL CYLINDER FOR DEVICE                           */ 01649000
*/* P CALCULATE ACTUAL TRACK FOR DEVICE                              */ 01650000
*/*%CNVEXT: R RETURN TO CALLER                                       */ 01651000
*/*ILRCNVRT: END                                                     */ 01652000
*                                                                  0537 01653000
*ILRCNVRT:                                                         0537 01654000
*   PROC OPTIONS(NOSAVEAREA);                                      0537 01655000
ILRCNVRT STM   @14,@12,12(@13)                                     0537 01656000
*   DCL                                                            0538 01657000
*     1 CNVRTSKA CHAR(8) BASED(R1)  /* FULL SEEK ADDRESS             */ 01658000
*           BDY(DWORD),                                            0538 01659000
*      2 CNVRTM CHAR(1),            /* EXTENT NUMBER                 */ 01660000
*      2 * CHAR(2),                 /* BIN NUMBER                    */ 01661000
*      2 CNVRTCC CHAR(2),           /* CYLINDER NUMBER               */ 01662000
*      2 CNVRTHH CHAR(2),           /* TRACK (HEAD) NUMBER           */ 01663000
*      2 CNVRTR CHAR(1);            /* RECORD NUMBER                 */ 01664000
*   DCL                                                            0539 01665000
*     CNVRTBAD FIXED(31) CONSTANT(4);/* BAD RETURN CODE              */ 01666000
*   DCL                                                            0540 01667000
*     CNVRTOK FIXED(31) CONSTANT(0);/* GOOD RETURN CODE              */ 01668000
*   DCL                                                            0541 01669000
*     CNVRTRBA FIXED(31) DEFINED(CNVRTSKA);                        0541 01670000
*   DCL                                                            0542 01671000
*     CNVRTONE FIXED(31) CONSTANT(1);/* CONSTANT OF ONE              */ 01672000
*   RFY                                                            0543 01673000
*    (R0,                                                          0543 01674000
*     R1,                                                          0543 01675000
*     R2) RSTD;                     /* SAVE PARM REGISTERS           */ 01676000
*   R2=R0;                          /* SET UP EDBHDPTR IN REG2       */ 01677000
         LR    R2,R0                                               0544 01678000
*   RFY                                                            0545 01679000
*     R0 UNRSTD;                    /* NO NEED FOR R0                */ 01680000
*   RFY                                                            0546 01681000
*     R3 RSTD;                                                     0546 01682000
*   R3=R2+LENGTH(IDAEDBHD);         /* POINT R3 TO FIRST EDB ENTRY   */ 01683000
         LA    R3,8                                                0547 01684000
         ALR   R3,R2                                               0547 01685000
*   RFY                                                            0548 01686000
*     R5 RSTD;                                                     0548 01687000
*   R5=CNVRTRBA;                    /* RBA TO BE CONVERTED           */ 01688000
         L     R5,CNVRTRBA(,R1)                                    0549 01689000
*   RFY                                                            0550 01690000
*    (R4,                                                          0550 01691000
*     R15) RSTD;                                                   0550 01692000
*   R15=CNVRTBAD;                   /* INITIALIZE RETURN CODE        */ 01693000
         LA    R15,4                                               0551 01694000
*   DO R4=R2->EDBNO TO 1 BY-1       /* LOOP THROUGH EDB ENTRIES      */ 01695000
*         WHILE R15=CNVRTBAD;                                      0552 01696000
         SLR   R4,R4                                               0552 01697000
         IC    R4,EDBNO(,R2)                                       0552 01698000
         B     @DE00552                                            0552 01699000
@DL00552 C     R15,@CF00053                                        0552 01700000
         BNE   @DC00552                                            0552 01701000
*     IF R5=>R3->EDBLORBA&          /* IF RBA IN THIS EXTENT         */ 01702000
*         R5<=R3->EDBHIRBA THEN     /* THEN RESET RETURN CODE        */ 01703000
         CL    R5,EDBLORBA(,R3)                                    0553 01704000
         BL    @RF00553                                            0553 01705000
         CL    R5,EDBHIRBA(,R3)                                    0553 01706000
         BH    @RF00553                                            0553 01707000
*       R15=CNVRTOK;                                               0554 01708000
         SLR   R15,R15                                             0554 01709000
*     ELSE                          /* ELSE, CHECK NEXT ENTRY        */ 01710000
*       R3=R3+R2->EDBLEN;                                          0555 01711000
         B     @RC00553                                            0555 01712000
@RF00553 AH    R3,EDBLEN(,R2)                                      0555 01713000
*   END;                                                           0556 01714000
@RC00553 BCTR  R4,0                                                0556 01715000
@DE00552 LTR   R4,R4                                               0556 01716000
         BP    @DL00552                                            0556 01717000
@DC00552 DS    0H                                                  0557 01718000
*   IF R15=CNVRTOK                  /* IF FOUND EDB ENTRY,           */ 01719000
*     THEN                                                         0557 01720000
         LTR   R15,R15                                             0557 01721000
         BNZ   @RF00557                                            0557 01722000
*     DO;                           /* THEN DO CONVERSION            */ 01723000
*/*                                                                  */ 01724000
*/*                 FOUND EDB ENTRY                                  */ 01725000
*/*                                                                  */ 01726000
*                                                                  0559 01727000
*       R2=R3->EDBLPMBA;            /* GET LPMB ADDRESS              */ 01728000
         L     R2,EDBLPMBA(,R3)                                    0559 01729000
*       CNVRTSKA=''B;               /* ZERO FULL SEEK ADDRESS        */ 01730000
         XC    CNVRTSKA(8,R1),CNVRTSKA(R1)                         0560 01731000
*       R5=R5-R3->EDBLORBA;         /* RELATIVE RBA THIS EXTENT      */ 01732000
         SL    R5,EDBLORBA(,R3)                                    0561 01733000
*       CNVRTM=R3->EDBM;            /* SET UP EXTENT NUMBER          */ 01734000
         IC    @14,EDBM(,R3)                                       0562 01735000
         STC   @14,CNVRTM(,R1)                                     0562 01736000
*/*                                                                  */ 01737000
*/*          OPTIMIZE CODE BY USING ASSEMBLER FOR DIVISION           */ 01738000
*/*                                                                  */ 01739000
*                                                                  0563 01740000
*       GENERATE REFS(LPMAUSZ,LPMBPTRK,LPMBLKSZ,R2,R4,R5,CNVRTCC,  0563 01741000
*           CNVRTHH);                                              0563 01742000
*                                                                       01743000
*           CNVRTCC = REL RBA / BYTES PER ALLOCATION UNIT               01744000
*           R5= R5 / LPMAUSZ                                            01745000
*           R4= R5 // LPMAUSZ                                           01746000
*                                                                       01747000
            SR   R4,R4                 ZERO EVEN REGISTER               01748000
            D    R4,LPMAUSZ(,R2)       RBA / BYTES PER ALLOC UNIT       01749000
            STCM R5,3,CNVRTCC(R1)      RELATIVE CYL VALVE               01750000
            LR   R5,R4                 REMAINDER= RELATIVE TRACK        01751000
*                                                                       01752000
*           CNVRTHH = RELATIVE TRACK / BYTES PER TRACK                  01753000
*           R5= R5 / LPMBPTRK                                           01754000
*           R4= R5 // LPMBPTRK                                          01755000
*                                                                       01756000
            SR   R4,R4                 ZERO EVEN REGISTER               01757000
            D    R4,LPMBPTRK(,R2)      REL TRACK / BYTES PER TRACK      01758000
            STCM R5,3,CNVRTHH(R1)      RELATIVE TRACK VALVE             01759000
*                                                                       01760000
*           CNVRTR= (( RELATIVE RECORD + BLOCKSIZE -1) / BLOCKSIZE)+1   01761000
*                R7= ( R6 + LPMBLKSZ - 1 ) / LPMBLKSZ                   01762000
*                                                                       01763000
            A    R4,LPMBLKSZ(,R2)      ADD PHYSICAL BLOCKSIZE           01764000
            BCTR R4,0                  SUBTRACT 1 FOR ROUNDING          01765000
            LR   R5,R4                 SET UP FOR DIVISION              01766000
            SR   R4,R4                 ZERO EVEN REGISTER               01767000
            D    R4,LPMBLKSZ(,R2)      DIVIDE BY BLOCKSIZE              01768000
*       CNVRTR=R5+CNVRTONE;         /* RECORD NUMBER                 */ 01769000
         LA    @14,1                                               0564 01770000
         ALR   @14,R5                                              0564 01771000
         STC   @14,CNVRTR(,R1)                                     0564 01772000
*/*                                                                  */ 01773000
*/*      CNVRTCC=(CNVRTCC * LPMTRKAU + CNVRTHH + EDBSTTRK)/ LPMTPC   */ 01774000
*/*      CNVRTHH=(CNVRTCC * LPMTRKAU + CNVRTHH + EDBSTTRK)// LPMTPC  */ 01775000
*/*                                                                  */ 01776000
*                                                                  0565 01777000
*       R5=CNVRTCC*(R2->LPMTRKAU)+CNVRTHH+(R3->EDBSTTRK);          0565 01778000
         SLR   R5,R5                                               0565 01779000
         ICM   R5,3,CNVRTCC(R1)                                    0565 01780000
         MH    R5,LPMTRKAU(,R2)                                    0565 01781000
         SLR   @14,@14                                             0565 01782000
         ICM   @14,3,CNVRTHH(R1)                                   0565 01783000
         ALR   R5,@14                                              0565 01784000
         AL    R5,EDBSTTRK(,R3)                                    0565 01785000
*       GENERATE REFS(LPMTPC,R3,R4,R5,R2,R1,CNVRTCC,CNVRTHH);      0566 01786000
*                                                                       01787000
*           CNVRTCC= R5 / LPMTPC                                        01788000
*           CNVRTHH= R5 // LPMTPC                                       01789000
*                                                                       01790000
            SR    R4,R4                ZERO EVEN REGISTER               01791000
            LH    R3,LPMTPC(,R2)       PICK UP TRACKS PER CYLINDER      01792000
            DR   R4,R3                 R5 / LPMTPC                      01793000
            STCM R5,3,CNVRTCC(R1)      ACTUAL CYLINDER ADDRESS          01794000
            STCM R4,3,CNVRTHH(R1)      ACTUAL HEAD ADDRESS              01795000
*     END;                          /* END OF CONVERSION PROCESS     */ 01796000
*   RETURN CODE(R15);               /* RETURN TO CALLER              */ 01797000
@RF00557 L     @14,12(,@13)                                        0568 01798000
         LM    @00,@12,20(@13)                                     0568 01799000
         BR    @14                                                 0568 01800000
*   END ILRCNVRT;                                                  0569 01801000
@PB00004 DS    0H                                                  0569 01802000
*   RFY                                                            0570 01803000
*    (R1,                                                          0570 01804000
*     R2,                                                          0570 01805000
*     R3,                                                          0570 01806000
*     R4,                                                          0570 01807000
*     R5,                                                          0570 01808000
*     R15) UNRSTD;                                                 0570 01809000
*   END ILRSWAP                                                    0571 01810000
