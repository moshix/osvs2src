         ST    @11,ATAAIA(,ATAPTR)                                 0477 01391000
*       TEMPAIA=PARTAIAE;           /* GET ERROR QUEUE               */ 01392000
         L     @11,PARTPTR(,ASMVTPTR)                              0478 01393000
         L     TEMPAIA,PARTAIAE(,@11)                              0478 01394000
*CSLP:                                                             0479 01395000
*       AIANXAIA=TEMPAIA;           /* CHAIN TO AIA                  */ 01396000
