@EL00002 DS    0H                                                  0419 01210000
@EF00002 DS    0H                                                  0419 01211000
@ER00002 BR    @14                                                 0419 01212000
*SCCWPROC:                                                         0420 01213000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0420 01214000
*                                                                  0420 01215000
SCCWPROC DS    0H                                                  0421 01216000
*   /*****************************************************************/ 01217000
*   /*                                                               */ 01218000
*   /* THIS ROUTINE BUILDS AND QUEUES SCCW'S FOR THE SWAP DRIVER. AIA*/ 01219000
*   /* GROUPS ARE PASSED TO THIS ROUTINE ONE AT A TIME. IF ANY WORK  */ 01220000
*   /* IS ADDED TO A QUEUE FOR SWAP DRIVER, A FLAG WILL BE SET PRIOR */ 01221000
*   /* TO EXIT TO NOTIFY THE MAINLINE THAT A SCHEDULE MAY BE         */ 01222000
*   /* REQUIRED. THIS ROUTINE DEPENDS ON THE FACT THAT IT IS CALLED  */ 01223000
*   /* ONLY IF AN SCCW IS ON THE AVAILABLE QUEUE & WILL BE           */ 01224000
*   /* OBTAINABLE.                                                   */ 01225000
*   /*                                                               */ 01226000
*   /*****************************************************************/ 01227000
*                                                                  0421 01228000
*   RFY                                                            0421 01229000
*    (R14,                                                         0421 01230000
*     SCCWPTR,                                                     0421 01231000
*     SAREPTR) RSTD;                                               0421 01232000
*   ATASCCWP='1'B;                  /* INDICATE SCCWPROC IN CONTROL  */ 01233000
         OI    ATASCCWP(ATAPTR),B'01000000'                        0422 01234000
*   ASM5SR14=R14;                   /* SAVE RETURN ADDRESS           */ 01235000
         ST    R14,ASM5SR14(,ASMVTPTR)                             0423 01236000
*   SCCWPTR=SARSCCWQ;               /* GET SCCW POINTER              */ 01237000
         L     SCCWPTR,SARSCCWQ(,SARTPTR)                          0424 01238000
*CSRETRY:                                                          0425 01239000
*   R14=SCCWSCCW;                   /* GET NEXT SCCW POINTER         */ 01240000
CSRETRY  L     R14,SCCWSCCW(,SCCWPTR)                              0425 01241000
*   CS(SCCWPTR,R14,SARSCCWQ);       /* C&S SCCW OFF QUEUE            */ 01242000
         CS    SCCWPTR,@14,SARSCCWQ(SARTPTR)                       0426 01243000
*   BC(NE,CSRETRY);                 /* IF IT FAILS, RETRY - FAILURE     01244000
*                                      CAN ONLY BE FROM A SCCW BEING    01245000
*                                      ADDED SINCE ONLY SALLOCED   0427 01246000
*                                      MODULES CAN REMOVE SCCWS.     */ 01247000
         BC    7,CSRETRY                                           0427 01248000
*   ATASCCW=SCCWPTR;                /* SAVE FOR RECOVERY             */ 01249000
         ST    SCCWPTR,ATASCCW(,ATAPTR)                            0428 01250000
*   SCCWSCCW=0;                     /* CLEAR OLD POINTER             */ 01251000
         SLR   @15,@15                                             0429 01252000
         ST    @15,SCCWSCCW(,SCCWPTR)                              0429 01253000
*   RFY                                                            0430 01254000
*    (R14) UNRSTD;                                                 0430 01255000
*                                                                  0431 01256000
*   /*****************************************************************/ 01257000
*   /*                                                               */ 01258000
*   /* FIND PROPER SRE BASED ON NN PORTION OF LSID.                  */ 01259000
*   /*                                                               */ 01260000
*   /*****************************************************************/ 01261000
*                                                                  0431 01262000
*   RFY                                                            0431 01263000
*    (NREG,                                                        0431 01264000
*     QREG) RSTD;                                                  0431 01265000
*   NREG=AIALSID;                   /* GET LSID TO BE PROCESSED      */ 01266000
         L     NREG,AIALSID(,AIAPTR)                               0432 01267000
*   QREG=NREG;                      /* GET LSID FOR QQQQ PORTION     */ 01268000
         LR    QREG,NREG                                           0433 01269000
*   SRL(NREG,TWOBYTES);             /* ISOLATE NN PORTION            */ 01270000
         SRL   NREG,16                                             0434 01271000
*   IF NREG>=SARUSE THEN            /* IF THIS IS AN INVALID ENTRY   */ 01272000
         C     NREG,SARUSE(,SARTPTR)                               0435 01273000
         BL    @RF00435                                            0435 01274000
*     DO;                                                          0436 01275000
*       IF AIAWRITE='0'B THEN       /* IF THIS IS A READ REQUEST     */ 01276000
         TM    AIAWRITE(AIAPTR),B'10000000'                        0437 01277000
         BNZ   @RF00437                                            0437 01278000
*         AIABADID='1'B;            /* SET BAD ID FLAG               */ 01279000
         OI    AIABADID(AIAPTR),B'00000001'                        0438 01280000
*       ELSE                        /* IF THIS IS A WRITE REQUEST    */ 01281000
*         AIAPRIER='1'B;            /* SET NORMAL PRIMARY ERROR FLAG */ 01282000
