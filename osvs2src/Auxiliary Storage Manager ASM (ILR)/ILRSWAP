*       GOTO QTOERRQ;               /* GO TO PUT AIA ON ERROR QUEUE  */ 01285000
         B     QTOERRQ                                             0440 01286000
*     END;                                                         0441 01287000
*   RFY                                                            0442 01288000
*    (TEMPLCCW) RSTD;                                              0442 01289000
@RF00435 DS    0H                                                  0443 01290000
*   SAREPTR=ADDR(SARENTS(NREG+1));  /* GET SARTE POINTER             */ 01291000
         LR    @15,NREG                                            0443 01292000
         MH    @15,@CH01024                                        0443 01293000
         LA    SAREPTR,SARENTS(@15,SARTPTR)                        0443 01294000
*   IF SRENUSE='1'B THEN            /* IF SARTE IS NOT IN USE        */ 01295000
         TM    SRENUSE(SAREPTR),B'10000000'                        0444 01296000
         BNO   @RF00444                                            0444 01297000
*     DO;                                                          0445 01298000
*       AIAPRIER='1'B;              /* SET ERROR FLAG                */ 01299000
         OI    AIAPRIER(AIAPTR),B'00001000'                        0446 01300000
*       GOTO QTOERRQ;               /* GO TO PUT AIA ON ERROR QUEUE  */ 01301000
         B     QTOERRQ                                             0447 01302000
*     END;                                                         0448 01303000
*   TEMPLCCW=SCCWLCCW;              /* GET TEMPORARY POINTER         */ 01304000
@RF00444 L     TEMPLCCW,SCCWLCCW(,SCCWPTR)                         0449 01305000
*   TEMPLCCW->LASTRWFL=CHAINCMD;    /* RESTORE CHAIN FLG AFTER LAST     01306000
*                                      USE                           */ 01307000
         MVI   LASTRWFL(TEMPLCCW),X'40'                            0450 01308000
*   TEMPLCCW=TEMPLCCW+8;            /* UPDATE TO NEW POINTER         */ 01309000
         AL    TEMPLCCW,@CF00059                                   0451 01310000
*   TEMPLCCW->LASTSROA=SCCWSVOA;    /* RESTORE SEARCH AFTER LAST USE */ 01311000
         L     @11,SCCWSVOA(,SCCWPTR)                              0452 01312000
         ST    @11,LASTSROA(,TEMPLCCW)                             0452 01313000
*   RFY                                                            0453 01314000
*    (TEMPLCCW) UNRSTD;                                            0453 01315000
*   RFY                                                            0454 01316000
*    (R0,                                                          0454 01317000
*     R1,                                                          0454 01318000
