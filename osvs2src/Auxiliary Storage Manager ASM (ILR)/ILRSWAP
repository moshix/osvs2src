*ASIGNSET:                                                         0318 00748000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0318 00749000
*                                                                  0318 00750000
ASIGNSET DS    0H                                                  0319 00751000
*   /*****************************************************************/ 00752000
*   /*                                                               */ 00753000
*   /* ASIGNSET ROUTINE OF ILRSLSQA. THIS ROUTINE IS CALLED ON WRITE */ 00754000
*   /* REQUESTS TO ASSIGN A FREE SWAP SET(S) TO A GROUP(S).THIS      */ 00755000
*   /* ROUTINE ASSIGNS THE SWAP SET BASED ON THE FOLLOWING CRITERIA: */ 00756000
*   /* 1). USE FIXED HEAD DEVICES FIRST IF AT ALL POSSIBLE. 2). USE  */ 00757000
*   /* THE SET THAT WAS LAST FREED IF AT ALL POSSIBLE. IF ASSIGNMENT */ 00758000
*   /* IS IMPOSSIBLE (THAT IS NOT ENOUGH SETS ARE FREE) THE ENTIRE   */ 00759000
*   /* GROUP(S) OF AIA'S ARE RECHAINED BY AIANXAIA AND RETURNED TO   */ 00760000
*   /* THE CALLER WITH R.C.=4. FOR THE NORMAL CASE (R.C.=0) THE LSID */ 00761000
*   /* WILL BE PLACED IN EACH AIA WITHIN THE GROUP.                  */ 00762000
*   /*                                                               */ 00763000
*   /*****************************************************************/ 00764000
*                                                                  0319 00765000
*   RFY                                                            0319 00766000
*    (SAREPTR,                                                     0319 00767000
*     R14) RSTD;                                                   0319 00768000
*   ATASGNST='1'B;                  /* SET TRACKING BIT FOR RECOVERY */ 00769000
         OI    ATASGNST(ATAPTR),B'10000000'                        0320 00770000
*   ASM5SR14=R14;                   /* SAVE RETURN REGISTER          */ 00771000
         ST    R14,ASM5SR14(,ASMVTPTR)                             0321 00772000
*   RFY                                                            0322 00773000
*    (R14) UNRSTD;                                                 0322 00774000
*   ASM5AIA=ASHSWAPQ;               /* SAVE POINTER TO FIRST GROUP   */ 00775000
         L     @11,ASHSWAPQ(,RSMHDPTR)                             0323 00776000
         ST    @11,ASM5AIA(,ASMVTPTR)                              0323 00777000
*ASGNRTRY:                                                         0324 00778000
*   IF SARSETCT>=ASM5GCTR THEN      /* IF ENOUGH SETS ARE AVAILABLE  */ 00779000
