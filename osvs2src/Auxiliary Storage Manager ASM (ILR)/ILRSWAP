@DE00401 SLR   @08,@08                                             0405 01164000
         CR    AIAPTR,@08                                          0405 01165000
         BNE   @DL00401                                            0405 01166000
*   RFY                                                            0406 01167000
*    (R9) UNRSTD;                                                  0406 01168000
*                                                                  0406 01169000
*   /*****************************************************************/ 01170000
*   /*                                                               */ 01171000
*   /* IF MORE SWAP GROUPS TO PROCESS RETURN TO TOP.                 */ 01172000
*   /*                                                               */ 01173000
*   /*****************************************************************/ 01174000
*                                                                  0407 01175000
*   IF TEMPAIA^=0 THEN              /* IF ANOTHER GROUP TO PROCESS   */ 01176000
         CR    TEMPAIA,@08                                         0407 01177000
         BE    @RF00407                                            0407 01178000
*     DO;                           /* SET UP                        */ 01179000
*       ASM5AIA=TEMPAIA;            /* SAVE NEXT AIA POINTER         */ 01180000
         ST    TEMPAIA,ASM5AIA(,ASMVTPTR)                          0409 01181000
*       ASM5GCTR=ASM5GCTR-1;        /* ONE LESS GROUP TO PROCESS     */ 01182000
         L     @07,ASM5GCTR(,ASMVTPTR)                             0410 01183000
         BCTR  @07,0                                               0410 01184000
         ST    @07,ASM5GCTR(,ASMVTPTR)                             0410 01185000
*       GOTO ASGNRTRY;              /* DO ENTIRE ASIGN OPERATION   0411 01186000
*                                      AGAIN                         */ 01187000
         B     ASGNRTRY                                            0411 01188000
*     END;                                                         0412 01189000
*   RFY                                                            0413 01190000
*    (R15) RSTD;                                                   0413 01191000
@RF00407 DS    0H                                                  0414 01192000
*   R15=CODE0;                      /* ZERO RETURN CODE              */ 01193000
         SLR   R15,R15                                             0414 01194000
*ASGNRTN:                                                          0415 01195000
*   ATASGNST='0'B;                  /* RESET TRACKING BIT FOR      0415 01196000
*                                      RECOVERY                      */ 01197000
ASGNRTN  NI    ATASGNST(ATAPTR),B'01111111'                        0415 01198000
*   RFY                                                            0416 01199000
*    (R14) RSTD;                                                   0416 01200000
*   R14=ASM5SR14;                   /* RESTORE RETURN REGISTER       */ 01201000
         L     R14,ASM5SR14(,ASMVTPTR)                             0417 01202000
*   RFY                                                            0418 01203000
*    (R14,                                                         0418 01204000
*     R15,                                                         0418 01205000
*     TEMPAIA,                                                     0418 01206000
*     SATPTR,                                                      0418 01207000
*     SAREPTR) UNRSTD;                                             0418 01208000
