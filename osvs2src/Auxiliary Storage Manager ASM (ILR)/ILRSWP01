*       DO;                         /* YES - PROCESS EDB ERROR       */ 01160000
*         RFY                                                      0392 01161000
*           SAREPTR RSTD;                                          0392 01162000
*         RECSDWA(1:LENGTH(AIA))=ATAAIA->AIA;/* COPY ERROR AIA       */ 01163000
         LA    @11,SDWAVRA(,SDWAPTR)                               0393 01164000
         LR    @05,@11                                             0393 01165000
         ALR   @05,@12                                             0393 01166000
         L     @15,ATAAIA(,ATAPTR)                                 0393 01167000
         MVC   RECSDWA(28,@05),AIA(@15)                            0393 01168000
*         SDWAURAL=SDWAURAL+LENGTH(AIA);/* AMOUNT OF RECORD AREA USED*/ 01169000
         AH    @12,@CH00386                                        0394 01170000
         STC   @12,SDWAURAL(,SDWAPTR)                              0394 01171000
*         SAREPTR=ATASARTE;         /* ADDRESSIBILITY TO SART ENTRY  */ 01172000
         L     SAREPTR,ATASARTE(,ATAPTR)                           0395 01173000
*         RECSDWA(1:LENGTH(IDAEDB))=IDAEDB;/* COPY ERROR EDB         */ 01174000
         ALR   @11,@12                                             0396 01175000
         L     @05,SREEDB(,SAREPTR)                                0396 01176000
         MVC   RECSDWA(24,@11),IDAEDB+8(@05)                       0396 01177000
*         SDWAURAL=SDWAURAL+LENGTH(IDAEDB);/* UPDATE SPACE USED      */ 01178000
         AH    @12,@CH00350                                        0397 01179000
         STC   @12,SDWAURAL(,SDWAPTR)                              0397 01180000
*         RFY                                                      0398 01181000
*           SAREPTR UNRSTD;                                        0398 01182000
*       END;                                                       0399 01183000
*     ELSE                                                         0400 01184000
*       ;                           /* NOT ENOUGH RECORDING AREA     */ 01185000
@RF00390 DS    0H                                                  0401 01186000
*   ELSE                                                           0401 01187000
*     DO;                           /* UNEXPECTED ERROR              */ 01188000
*                                                                  0401 01189000
         B     @RC00389                                            0401 01190000
@RF00389 DS    0H                                                  0402 01191000
*       /*************************************************************/ 01192000
*       /*                                                           */ 01193000
*       /* CURRENT AIA PROCESSING. AN AIA WITH ERROR FLAGS IS IGNORED*/ 01194000
*       /* TO PREVENT ADDING IT TO THE PART ERROR QUEUE TWICE.       */ 01195000
*       /*                                                           */ 01196000
*       /*************************************************************/ 01197000
*                                                                  0402 01198000
*       RFY                                                        0402 01199000
*         SARTPTR RSTD;                                            0402 01200000
*       IF ATAAIA^=ZERO THEN        /* AIA CHECKPOINTED IN ATA ?     */ 01201000
         ICM   @12,15,ATAAIA(ATAPTR)                               0403 01202000
         BZ    @RF00403                                            0403 01203000
*         DO;                       /* YES- PROCESS AIA              */ 01204000
*           RFY                                                    0405 01205000
*            (VERFREG,                                             0405 01206000
*             R5,                                                  0405 01207000
*             R6,                                                  0405 01208000
*             R7,                                                  0405 01209000
*             RTCODE) RSTD;                                        0405 01210000
*           CALL ILRVSWTQ;          /* VERIFY SART WAIT QUEUE        */ 01211000
         L     @15,@CV02466                                        0406 01212000
         BALR  @14,@15                                             0406 01213000
*           VERFREG=ATAAIA;         /* ADDRESS OF AIA TO BE VERIFIED */ 01214000
         L     VERFREG,ATAAIA(,ATAPTR)                             0407 01215000
*           CALL ILRVAIA;           /* VERIFY CHECKPOINTED AIA       */ 01216000
         L     @15,@CV02461                                        0408 01217000
         BALR  @14,@15                                             0408 01218000
*           RFY                                                    0409 01219000
*            (VERFREG,                                             0409 01220000
*             R5,                                                  0409 01221000
*             R6,                                                  0409 01222000
*             R7) UNRSTD;           /* VERIFY RTN REGISTERS          */ 01223000
*           LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED ACROSS CALL       */ 01224000
         LM    SDWAPTR,ATAPTR,GNSAVE                               0410 01225000
*           IF RTCODE=SUCCESS THEN  /* IS AIA VALID ?                */ 01226000
         SLR   @14,@14                                             0411 01227000
         CR    RTCODE,@14                                          0411 01228000
         BNE   @RF00411                                            0411 01229000
*             DO;                   /* YES- PROCESS AIA              */ 01230000
*               AIAPTR=ATAAIA;      /* ADDRESSIBILITY TO AIA FIELDS  */ 01231000
         L     AIAPTR,ATAAIA(,ATAPTR)                              0413 01232000
*               IF AIAPRIER=OFF&AIABADID=OFF THEN/* ERROR FLAG ON ?  */ 01233000
         TM    AIAPRIER(AIAPTR),B'00001001'                        0414 01234000
         BNZ   @RF00414                                            0414 01235000
*                 DO;               /* NO- RETURN AIA TO WAIT QUEUE  */ 01236000
*                   SARTPTR=ASMSART;/* ADDRESSIBILITY TO SART        */ 01237000
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0416 01238000
*                   AIANXAIA=SARWAITF;/* GET CURRENT WAIT QUEUE      */ 01239000
         L     @12,SARWAITF(,SARTPTR)                              0417 01240000
         ST    @12,AIANXAIA(,AIAPTR)                               0417 01241000
*                   SARWAITF=AIAPTR;/* UPDATE SART WAIT QUEUE ANCHOR */ 01242000
         ST    AIAPTR,SARWAITF(,SARTPTR)                           0418 01243000
*                   IF AIANXAIA=ZERO THEN/* WAS WAIT QUEUE EMPTY ?   */ 01244000
         CR    @12,@14                                             0419 01245000
         BNE   @RF00419                                            0419 01246000
*                     SARWAITL=AIAPTR;/* YES- UPDATE LAST POINTER    */ 01247000
         ST    AIAPTR,SARWAITL(,SARTPTR)                           0420 01248000
*                 END;              /* NON-ERROR FLAG PROCESSING     */ 01249000
@RF00419 DS    0H                                                  0422 01250000
*             END;                  /* AIA VALID PROCESSING          */ 01251000
@RF00414 DS    0H                                                  0423 01252000
*                                                                  0423 01253000
*           /*********************************************************/ 01254000
*           /*                                                       */ 01255000
*           /* CURRENT SCCW PROCESSING. A ZERO AIA POINTER INDICATES */ 01256000
*           /* THAT THE SCCW HAS ALREADY BEEN USED BY SCCWPROC.      */ 01257000
*           /*                                                       */ 01258000
*           /*********************************************************/ 01259000
*                                                                  0423 01260000
*           RFY                                                    0423 01261000
*            (VERFREG,                                             0423 01262000
*             R5,                                                  0423 01263000
*             R6,                                                  0423 01264000
*             R7) RSTD;             /* VERIFY RTN REGISTERS          */ 01265000
@RF00411 DS    0H                                                  0424 01266000
*           VERFREG=ATASCCW;        /* ADDRESS OF SCCW TO BE VERIFIED*/ 01267000
         L     VERFREG,ATASCCW(,ATAPTR)                            0424 01268000
*           CALL ILRVSCCW;          /* VERIFY CHECKPOINTED SCCW      */ 01269000
         L     @15,@CV02463                                        0425 01270000
         BALR  @14,@15                                             0425 01271000
*           LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED ACROSS CALL       */ 01272000
         LM    SDWAPTR,ATAPTR,GNSAVE                               0426 01273000
*           RFY                                                    0427 01274000
*            (VERFREG,                                             0427 01275000
*             R5,                                                  0427 01276000
*             R6,                                                  0427 01277000
*             R7) UNRSTD;           /* VERIFY RTN REGISTERS          */ 01278000
*           IF RTCODE=SUCCESS THEN  /* SCCW VAILD ?                  */ 01279000
         LTR   RTCODE,RTCODE                                       0428 01280000
         BNZ   @RF00428                                            0428 01281000
*             DO;                   /* YES- RETURN TO SART SCCW Q    */ 01282000
*               RFY                                                0430 01283000
*                (COMPREG,                                         0430 01284000
*                 REPLREG) RSTD;    /* C&S REGISTERS                 */ 01285000
*               SARTPTR=ASMSART;    /* ADDRESSIBILITY TO SART        */ 01286000
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0431 01287000
*               COMPREG=SARSCCWQ;   /* COMPARE VALUE- SART SCCW QUEUE*/ 01288000
         L     COMPREG,SARSCCWQ(,SARTPTR)                          0432 01289000
*               REPLREG=ATASCCW;    /* REPLACE VALUE- ATA SCCW       */ 01290000
         L     REPLREG,ATASCCW(,ATAPTR)                            0433 01291000
*RTRYCS3:                                                          0434 01292000
*               ATASCCW->SCCWSCCW=COMPREG;/* GET SARSCCWQ ANCHOR     */ 01293000
RTRYCS3  L     @14,ATASCCW(,ATAPTR)                                0434 01294000
         ST    COMPREG,SCCWSCCW(,@14)                              0434 01295000
*               CS(COMPREG,REPLREG,SARSCCWQ);/* UPDATE SART SCCW Q   */ 01296000
         CS    COMPREG,@07,SARSCCWQ(SARTPTR)                       0435 01297000
*               BC(NE,RTRYCS3);     /* TRY AGAIN                     */ 01298000
         BC    7,RTRYCS3                                           0436 01299000
*               RFY                                                0437 01300000
*                (COMPREG,                                         0437 01301000
*                 REPLREG) UNRSTD;                                 0437 01302000
*             END;                  /* VALID SCCW PROCESSING         */ 01303000
*           RFY                                                    0439 01304000
*             RTCODE UNRSTD;                                       0439 01305000
@RF00428 DS    0H                                                  0440 01306000
*         END;                      /* CHECKPOINTED AIA PROCESSING   */ 01307000
*       RFY                                                        0441 01308000
*         SARTPTR UNRSTD;                                          0441 01309000
@RF00403 DS    0H                                                  0442 01310000
*     END;                          /* UNEXPECTED ERROR PROCESSING   */ 01311000
*   RFY                                                            0443 01312000
*     R14 RSTD;                                                    0443 01313000
@RC00389 DS    0H                                                  0444 01314000
*   R14=SAVE2R14;                   /* RESTORE RETURN ADDRESS        */ 01315000
         L     R14,SAVE2R14                                        0444 01316000
*   RFY                                                            0445 01317000
*     R14 UNRSTD;                   /* FREE RETURN REGISTER          */ 01318000
*   END SCCWRCVY;                   /* RETURN TO ILRCSLSQ            */ 01319000
@EL00003 DS    0H                                                  0446 01320000
@EF00003 DS    0H                                                  0446 01321000
@ER00003 BR    @14                                                 0446 01322000
*                                                                  0447 01323000
*/********************************************************************/ 01324000
*/*                                                                  */ 01325000
*/*  ASETRCVY: SWAP LSQA PROCESSOR ASIGNSET SUBROUTINE RECOVERY      */ 01326000
*/*                                                                  */ 01327000
*/*  INPUT: REGISTER INTERFACE  REG 1 - SDWA ADDRESS                 */ 01328000
*/*                             REG 4 - ATA ADDRESS                  */ 01329000
*/*         THIS ROUTINE IS ENTERED WITH THE SALLOC LOCK HELD. THE   */ 01330000
*/*         SART ENTRY CONNECTED WITH THE ERROR WILL BE CHECKPOINTED */ 01331000
*/*         IN THE ATA.                                              */ 01332000
*/*                                                                  */ 01333000
*/*  PROCESS: SPECIAL PROCESSING FOR EITHER OF THE TWO POSSIBLE C0D  */ 01334000
*/*           ABENDS FROM ASIGNSET SUBROUTINE OF ILRSLSQA.           */ 01335000
*/*                                                                  */ 01336000
*/*  OUTPUT: NO STREAM OR LIST OUTPUT                                */ 01337000
*/*                                                                  */ 01338000
*/********************************************************************/ 01339000
*/*ASETRCVY: CHART ASSIGN SET SUBROUTINE  RECOVERY                   */ 01340000
*/*ASETRCVY:E ENTRY TO ASSIGN SET RECOVERY                           */ 01341000
*/*         P ZERO SART AVAILABLE SWAP SET COUNT                     */ 01342000
*/*         D (YES,,NO,%RTN3) SARTE CHECKPOINTED ?                   */ 01343000
*/*         P ZERO SARTE AVAILABLE SET COUNT                         */ 01344000
*/*         P GET FIRST SARTE (SAREPTR= @SARENTS)                    */ 01345000
*/*%SRECT:  P ADD SARTE AVAILABLE SETS TO SART SET COUNT             */ 01346000
*/*         D (YES,%RTN3,NO,) LAST SARTE IN USE ?                    */ 01347000
*/*         P (,%SRECT) GET NEXT SARTE                               */ 01348000
*/*%RTN3:   R RETURN TO CALLER (ILRCSLSQ)                            */ 01349000
*/*ASETRCVY: END                                                     */ 01350000
*                                                                  0447 01351000
*ASETRCVY:                                                         0447 01352000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* ASSIGN SET RECOVERY           */ 01353000
ASETRCVY DS    0H                                                  0448 01354000
*   RFY                                                            0448 01355000
*    (SARTPTR,                                                     0448 01356000
*     R14) RSTD;                    /* RESTRICT RETURN REGISTER      */ 01357000
*   SARTPTR=ASMSART;                /* ADDRESSIBILITY TO SART        */ 01358000
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0449 01359000
*   SARSETCT=ZERO;                  /* COUNT MUST BE RECALCULATED    */ 01360000
         SLR   @15,@15                                             0450 01361000
         ST    @15,SARSETCT(,SARTPTR)                              0450 01362000
*   IF ATASARTE^=ZERO THEN          /* PROBLEM WITH SART ENTRY ?     */ 01363000
         L     @12,ATASARTE(,ATAPTR)                               0451 01364000
         CR    @12,@15                                             0451 01365000
         BE    @RF00451                                            0451 01366000
*     DO;                           /* CLEAR SARTE SWAP SET COUNT    */ 01367000
*       RFY                                                        0453 01368000
*         SAREPTR RSTD;                                            0453 01369000
*       ATASARTE->SREAVLSL=ZERO;    /* COUNT WAS INCORRECT           */ 01370000
         ST    @15,SREAVLSL(,@12)                                  0454 01371000
*       SAREPTR=ADDR(SARENTS);      /* GET FIRST SART ENTRY          */ 01372000
         LA    SAREPTR,SARENTS(,SARTPTR)                           0455 01373000
*       DO I=1 TO SARUSE;           /* LOPP THRU SART ENTRIES        */ 01374000
         LA    I,1                                                 0456 01375000
         B     @DE00456                                            0456 01376000
@DL00456 DS    0H                                                  0457 01377000
*         SARSETCT=SARSETCT+SREAVLSL;/* ADD SARTE'S SETS TO TOTAL    */ 01378000
         L     @15,SARSETCT(,SARTPTR)                              0457 01379000
         AL    @15,SREAVLSL(,SAREPTR)                              0457 01380000
         ST    @15,SARSETCT(,SARTPTR)                              0457 01381000
*         SAREPTR=SAREPTR+LENGTH(SARTE);/* GET NEXT SARTE            */ 01382000
         AH    SAREPTR,@CH00408                                    0458 01383000
*       END;                        /* LOOP THRU SART ENTRIES        */ 01384000
         AH    I,@CH00043                                          0459 01385000
@DE00456 C     I,SARUSE(,SARTPTR)                                  0459 01386000
         BNH   @DL00456                                            0459 01387000
*       RFY                                                        0460 01388000
*         SAREPTR UNRSTD;                                          0460 01389000
*     END;                          /* SART ENTRY ERROR              */ 01390000
*   RFY                                                            0462 01391000
*    (R14,                                                         0462 01392000
*     SARTPTR) UNRSTD;                                             0462 01393000
*   END ASETRCVY;                   /* RETURN TO ILRCSLSQ            */ 01394000
@EL00004 DS    0H                                                  0463 01395000
@EF00004 DS    0H                                                  0463 01396000
@ER00004 BR    @14                                                 0463 01397000
*   DECLARE                         /* COMMON VARIABLES              */ 01398000
*     I256C CHAR(256) BASED,                                       0464 01399000
*     I031F FIXED(31) BASED,                                       0464 01400000
*     I031P PTR(31) BASED,                                         0464 01401000
*     I015F FIXED(15) BASED,                                       0464 01402000
*     I015P PTR(15) BASED,                                         0464 01403000
*     I008P PTR(8) BASED,                                          0464 01404000
