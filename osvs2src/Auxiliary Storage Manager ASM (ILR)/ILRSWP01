         TITLE 'ILRSWP01:  ASM SWAP RECOVERY                           *00001000
                        '                                               00002000
ILRSWP01 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ILRSWP01  76.091'                                 0001 00009000
ILRCSWAP DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00277-ILRCSWAP)                                   00013000
         ENTRY ILRCSWAP                                                 00014000
ILRCSLSQ DS    0H                                                  0001 00015000
         USING *,@15                                               0001 00016000
         B     @PROLOG                                             0001 00017000
         DC    AL2(@EP00304-ILRCSLSQ)                                   00018000
         ENTRY ILRCSLSQ                                                 00019000
         DROP  @15                                                      00020000
@PROLOG  BALR  @10,0                                               0001 00021000
@PSTART  DS    0H                                                  0001 00022000
         USING @PSTART,@10                                         0001 00023000
         AH    @15,4(,@15)                                         0001 00024000
         BR    @15                                                 0001 00025000
@EP00001 DS    0H                                                  0002 00026000
         USING @DATD,SWPDREG                                            00027000
*   SAVER14=R14;                    /* SAVE RETURN ADDRESS           */ 00028000
         ST    R14,SAVER14                                         0102 00029000
*   RFY                                                            0103 00030000
*     R14 UNRSTD;                                                  0103 00031000
*   SAREG=WSACASMS;                 /* SWAP DRIVER REG SAVEAREA      */ 00032000
         L     @11,LCCAPTR                                         0104 00033000
         L     @11,LCCACPUS(,@11)                                  0104 00034000
         L     SAREG,WSACASMS(,@11)                                0104 00035000
*   RFY                                                            0105 00036000
*     SAREPTR RSTD;                                                0105 00037000
*   SAREPTR=ATASARTE;               /* ADDRESSIBILITY TO SART ENTRY  */ 00038000
         L     SAREPTR,ATASARTE(,ATAPTR)                           0106 00039000
*   STM(SDWAPTR,ATAPTR,GNSAVE);     /* DESTROYED BY G/M & VERIFY RTNS*/ 00040000
*                                                                  0107 00041000
         STM   SDWAPTR,ATAPTR,GNSAVE                               0107 00042000
*   /*****************************************************************/ 00043000
*   /*                                                               */ 00044000
*   /* A SART ENTRY IS CHECKPOINTED IN THE ATA WHENEVER SWAP DRIVER  */ 00045000
*   /* IS PROCESSING A SARTE WITH ASSOCIATED SCCWS AND IORB. IF A    */ 00046000
*   /* SARTE IS NOT CHECKPOINTED, RECOVERY CONSISTS OF RESCHEDULING  */ 00047000
*   /* THE SWAP DRIVER.                                              */ 00048000
*   /*                                                               */ 00049000
*   /*****************************************************************/ 00050000
*                                                                  0108 00051000
*   IF SAREPTR^=ZERO THEN           /* SART ENTRY CHECKPOINTED ?     */ 00052000
         LTR   SAREPTR,SAREPTR                                     0108 00053000
         BZ    @RF00108                                            0108 00054000
*     DO;                           /* YES - PROCESS SARTE           */ 00055000
*                                                                  0109 00056000
*       /*************************************************************/ 00057000
*       /*                                                           */ 00058000
*       /* IF SWAP DRIVER FOUND A SARTE WITH A ZERO IORB POINTER, A  */ 00059000
*       /* '084' ABEND IS ISSUED. AN ATTEMPT IS MADE TO REBUILD AN   */ 00060000
*       /* IORB.                                                     */ 00061000
*       /*                                                           */ 00062000
*       /*************************************************************/ 00063000
*                                                                  0110 00064000
*       IF SDWACMPC=NOIORB THEN     /* SWAP DRIVER '084' ABEND ?     */ 00065000
         CLC   SDWACMPC(3,SDWAPTR),@CB02426                        0110 00066000
         BNE   @RF00110                                            0110 00067000
*         CALL RBDIORB;             /* ATTEMPT TO REBUILD IORB       */ 00068000
         BAL   @14,RBDIORB                                         0111 00069000
*       ELSE                                                       0112 00070000
*         DO;                       /* NO- NORMAL ERROR PATH         */ 00071000
*                                                                  0112 00072000
         B     @RC00110                                            0112 00073000
@RF00110 DS    0H                                                  0113 00074000
*           /*********************************************************/ 00075000
*           /*                                                       */ 00076000
*           /* PROCESS A CHECKPOINTED IORB                           */ 00077000
*           /*                                                       */ 00078000
*           /*********************************************************/ 00079000
*                                                                  0113 00080000
*           RFY                                                    0113 00081000
*            (VERFREG,                                             0113 00082000
*             R5,                                                  0113 00083000
*             R6,                                                  0113 00084000
*             R7,                                                  0113 00085000
*             RTCODE) RSTD;                                        0113 00086000
*           IORBPTR=ATAIORB;        /* ADDRESSIBILITY TO IORB        */ 00087000
         L     IORBPTR,ATAIORB(,ATAPTR)                            0114 00088000
*           VERFREG=IORBPTR;        /* ADDR OF IORB TO VERIFY        */ 00089000
         LR    VERFREG,IORBPTR                                     0115 00090000
*           CALL ILRVIORB;          /* VERIFY IORB                   */ 00091000
         L     @15,@CV02462                                        0116 00092000
         BALR  @14,@15                                             0116 00093000
*           LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED ACROSS CALL       */ 00094000
         LM    SDWAPTR,ATAPTR,GNSAVE                               0117 00095000
*           IF RTCODE=SUCCESS THEN  /* IORB VALID ?                  */ 00096000
         LTR   RTCODE,RTCODE                                       0118 00097000
         BNZ   @RF00118                                            0118 00098000
*             DO;                   /* YES- PROCESS IORB             */ 00099000
*                                                                  0119 00100000
*               /*****************************************************/ 00101000
*               /*                                                   */ 00102000
*               /* THE QUEUE OF SCCWS ANCHORED IN THE IORB IS        */ 00103000
*               /* VALIDITY CHECKED. THOSE SCCWS, WHICH ARE NOT      */ 00104000
*               /* ALREADY PART OF THE ATA SCCW QUEUE, ARE ADDED TO  */ 00105000
*               /* THE ATA SCCW QUEUE.                               */ 00106000
*               /*                                                   */ 00107000
*               /*****************************************************/ 00108000
*                                                                  0120 00109000
*               VERFREG=ADDR(IORSCCW);/* ADDRESS OF QUEUE TO VERIFY  */ 00110000
         LA    VERFREG,IORSCCW(,IORBPTR)                           0120 00111000
*               CALL ILRVSCWQ;      /* VERIFY IORB SCCW QUEUE        */ 00112000
         L     @15,@CV02464                                        0121 00113000
         BALR  @14,@15                                             0121 00114000
*               IF IORSCCW^=ZERO&IORSCCW^=ATASCCW THEN/* UNIQUE SCCW    00115000
*                                      QUEUE?                        */ 00116000
         L     @14,IORSCCW(,IORBPTR)                               0122 00117000
         LTR   @14,@14                                             0122 00118000
         BZ    @RF00122                                            0122 00119000
         C     @14,ATASCCW(,ATAPTR)                                0122 00120000
         BE    @RF00122                                            0122 00121000
*                 DO;               /* YES- FIND LAST UNIQUE SCCW    */ 00122000
*                   RFY                                            0124 00123000
*                     SCCWPTR RSTD;                                0124 00124000
*                   SCCWPTR=IORSCCW;/* GET QUEUE ANCHOR              */ 00125000
         LR    SCCWPTR,@14                                         0125 00126000
*                   DO WHILE(SCCWSCCW^=ATASCCW)&(SCCWSCCW^=ZERO);  0126 00127000
         B     @DE00126                                            0126 00128000
@DL00126 DS    0H                                                  0127 00129000
*                     SCCWPTR=SCCWSCCW;/* GET NEXT SCCW              */ 00130000
         L     SCCWPTR,SCCWSCCW(,SCCWPTR)                          0127 00131000
*                   END;            /* SEARCH FOR LAST UNIQUE SCCW   */ 00132000
@DE00126 L     @14,SCCWSCCW(,SCCWPTR)                              0128 00133000
         C     @14,ATASCCW(,ATAPTR)                                0128 00134000
         BE    @DC00126                                            0128 00135000
         LTR   @14,@14                                             0128 00136000
         BNZ   @DL00126                                            0128 00137000
@DC00126 DS    0H                                                  0129 00138000
*                   SCCWSCCW=ATASCCW;/* COMBINE ATA AND IORB QUEUES  */ 00139000
         MVC   SCCWSCCW(4,SCCWPTR),ATASCCW(ATAPTR)                 0129 00140000
*                   ATASCCW=IORSCCW;/* UPDATE ATA SCCW QUEUE ANCHOR  */ 00141000
         MVC   ATASCCW(4,ATAPTR),IORSCCW(IORBPTR)                  0130 00142000
*                   RFY                                            0131 00143000
*                     SCCWPTR UNRSTD;                              0131 00144000
*                 END;              /* UNIQUE IORB SCCW QUEUE        */ 00145000
*               IORSCCW=ZERO;       /* CLEAR IORB SCCW ANCHOR        */ 00146000
@RF00122 SLR   @14,@14                                             0133 00147000
         ST    @14,IORSCCW(,IORBPTR)                               0133 00148000
*               IORFUSE=OFF;        /* RELEASE IORB                  */ 00149000
         NI    IORFUSE(IORBPTR),B'01111111'                        0134 00150000
*             END;                  /* VALID IORB PROCESSING         */ 00151000
*           ELSE                                                   0136 00152000
*             DO;                   /* IORB IS INVALID               */ 00153000
*                                                                  0136 00154000
         B     @RC00118                                            0136 00155000
@RF00118 DS    0H                                                  0137 00156000
*               /*****************************************************/ 00157000
*               /*                                                   */ 00158000
*               /* THE CHECKPOINTED IORB IS INVALID. IF IT IS THE    */ 00159000
*               /* LAST IORB ON THIS SART ENTRY, ATTEMPT TO REBUILD  */ 00160000
*               /* AN IORB. OTHERWISE, REMOVE THE INVALID IORB FROM  */ 00161000
*               /* THE SARTE IORB QUEUE.                             */ 00162000
*               /*                                                   */ 00163000
*               /*****************************************************/ 00164000
*                                                                  0137 00165000
*               VERFREG=SREIORB;    /* ADDR OF ANCHOR IORB           */ 00166000
         L     VERFREG,SREIORB(,SAREPTR)                           0137 00167000
*               CALL ILRVIORB;      /* VERIFY IORB                   */ 00168000
         L     @15,@CV02462                                        0138 00169000
         BALR  @14,@15                                             0138 00170000
*               LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED ACROSS CALL   */ 00171000
         LM    SDWAPTR,ATAPTR,GNSAVE                               0139 00172000
*               IF RTCODE^=SUCCESS THEN/* IORB ANCHOR INVALID ?      */ 00173000
         LTR   RTCODE,RTCODE                                       0140 00174000
         BZ    @RF00140                                            0140 00175000
*                 CALL RBDIORB;     /* YES - ATTEMPT TO REBUILD IORB */ 00176000
         BAL   @14,RBDIORB                                         0141 00177000
*               ELSE                                               0142 00178000
*                 DO;               /* LOCATE INVALID IORB ON QUEUE  */ 00179000
         B     @RC00140                                            0142 00180000
@RF00140 DS    0H                                                  0143 00181000
*                   IORBPTR=SREIORB;/* GET SARTE IORB QUEUE ANCHOR   */ 00182000
         L     IORBPTR,SREIORB(,SAREPTR)                           0143 00183000
*                   DO WHILE IORIORB^=ZERO;/* VERIFY REST OF IORB Q  */ 00184000
         B     @DE00144                                            0144 00185000
@DL00144 DS    0H                                                  0145 00186000
*                     VERFREG=IORIORB;/* VERIFY INPUT REGISTER       */ 00187000
         L     VERFREG,IORIORB(,IORBPTR)                           0145 00188000
*                     CALL ILRVIORB;/* VERIFY THIS IORB              */ 00189000
         L     @15,@CV02462                                        0146 00190000
         BALR  @14,@15                                             0146 00191000
*                     LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED BY CALL */ 00192000
         LM    SDWAPTR,ATAPTR,GNSAVE                               0147 00193000
*                     IF RTCODE=SUCCESS THEN/* IORB VALID ?          */ 00194000
         LTR   RTCODE,RTCODE                                       0148 00195000
         BNZ   @RF00148                                            0148 00196000
*                       IORBPTR=IORIORB;/* YES- GET NEXT IORB        */ 00197000
         L     IORBPTR,IORIORB(,IORBPTR)                           0149 00198000
*                     ELSE                                         0150 00199000
*                       IORIORB=ZERO;/* NO- TRUNCATE IORB Q          */ 00200000
         B     @RC00148                                            0150 00201000
@RF00148 SLR   @14,@14                                             0150 00202000
         ST    @14,IORIORB(,IORBPTR)                               0150 00203000
*                   END;            /* IORB QUEUE LOOP               */ 00204000
@RC00148 DS    0H                                                  0151 00205000
@DE00144 ICM   @14,15,IORIORB(IORBPTR)                             0151 00206000
         BNZ   @DL00144                                            0151 00207000
*                 END;              /* IORB NOT SREIORB              */ 00208000
*             END;                  /* END OF INVALID IORB PROCESSING*/ 00209000
@RC00140 DS    0H                                                  0154 00210000
*           RFY                                                    0154 00211000
*            (VERFREG,                                             0154 00212000
*             R5,                                                  0154 00213000
*             R6,                                                  0154 00214000
*             R7,                                                  0154 00215000
*             RTCODE) UNRSTD;                                      0154 00216000
@RC00118 DS    0H                                                  0155 00217000
*         END;                      /* END OF NON 084 PROCESSING     */ 00218000
*     END;                          /* END OF SARTE PROCESSING       */ 00219000
@RC00110 DS    0H                                                  0157 00220000
*                                                                  0157 00221000
*   /*****************************************************************/ 00222000
*   /*                                                               */ 00223000
*   /* THE SARTE CHECKPOINTED IN THE ATA MAY HAVE BEEN ZEROED BY     */ 00224000
*   /* RBDIORB SUBROUTINE. IF THE SARTE IS STILL USABLE, VALIDITY    */ 00225000
*   /* CHECK THE ATA SCCW QUEUE.                                     */ 00226000
*   /*                                                               */ 00227000
*   /*****************************************************************/ 00228000
*                                                                  0157 00229000
*   IF ATASARTE^=ZERO THEN          /* SARTE STILL CHECKPOINTED ?    */ 00230000
@RF00108 SLR   @12,@12                                             0157 00231000
         C     @12,ATASARTE(,ATAPTR)                               0157 00232000
         BE    @RF00157                                            0157 00233000
*     DO;                           /* UNLOCK IT                     */ 00234000
*       RFY                                                        0159 00235000
*         COMPREG RSTD;                                            0159 00236000
*       SREDRIVE=OFF;               /* RESET SARTE REDRIVE FLAG      */ 00237000
         NI    SREDRIVE(SAREPTR),B'11011111'                       0160 00238000
*       COMPREG=ONE;                /* COMPARE VALUE FOR C&S         */ 00239000
         LA    COMPREG,1                                           0161 00240000
*       CS(COMPREG,ZERO,SRELOCK);   /* UNLOCK SARTE ENTRY            */ 00241000
         CS    COMPREG,@12,SRELOCK(SAREPTR)                        0162 00242000
*       RFY                                                        0163 00243000
*        (VERFREG,                                                 0163 00244000
*         R5,                                                      0163 00245000
*         R6,                                                      0163 00246000
*         R7) RSTD;                 /* VERIFY RTN REGISTERS          */ 00247000
*       VERFREG=ADDR(ATASCCW);      /* ADDR OF QUEUE TO VERIFY       */ 00248000
         LA    VERFREG,ATASCCW(,ATAPTR)                            0164 00249000
*       CALL ILRVSCWQ;              /* VERIFY ATA SCCW QUEUE         */ 00250000
         L     @15,@CV02464                                        0165 00251000
         BALR  @14,@15                                             0165 00252000
*       RFY                                                        0166 00253000
*        (VERFREG,                                                 0166 00254000
*         R5,                                                      0166 00255000
*         R6,                                                      0166 00256000
*         R7) UNRSTD;               /* VERIFY RTN REGISTERS          */ 00257000
*       IF ATASCCW^=ZERO THEN       /* QUEUE EMPTY ?                 */ 00258000
         L     @12,ATASCCW(,ATAPTR)                                0167 00259000
         LTR   @12,@12                                             0167 00260000
         BZ    @RF00167                                            0167 00261000
*         DO;                       /* NO- FIRST SCCW IS VALID       */ 00262000
*                                                                  0168 00263000
*           /*********************************************************/ 00264000
*           /*                                                       */ 00265000
*           /* LOOP THROUGH THE ATA SCCW QUEUE SEARCHING FOR THE LAST*/ 00266000
*           /* SCCW. SINCE SWAP DRIVER MAY HAVE SET COMMAND CHAINING */ 00267000
*           /* FLAGS IN EVERY SCCW LAST CCW EXCEPT IN THE LAST SCCW, */ 00268000
*           /* CLEAR THE FLAG HERE.                                  */ 00269000
*           /*                                                       */ 00270000
*           /*********************************************************/ 00271000
*                                                                  0169 00272000
*           RFY                                                    0169 00273000
*             SCCWPTR RSTD;                                        0169 00274000
*           SCCWPTR=ATASCCW;        /* GET FIRST SCCW                */ 00275000
         LR    SCCWPTR,@12                                         0170 00276000
*           DO WHILE SCCWSCCW^=ZERO;/* FIND LAST SCCW ON QUEUE       */ 00277000
         B     @DE00171                                            0171 00278000
@DL00171 DS    0H                                                  0172 00279000
*             CCFLAG=NCHAIN;        /* CLEAR COMMAND CHAIN FLAG      */ 00280000
         L     @12,SCCWLCCW(,SCCWPTR)                              0172 00281000
         MVI   CCFLAG(@12),X'00'                                   0172 00282000
*             SCCWPTR=SCCWSCCW;     /* GET NEXT SCCW                 */ 00283000
         L     SCCWPTR,SCCWSCCW(,SCCWPTR)                          0173 00284000
*           END;                    /* END OF QUEUE SEARCH           */ 00285000
@DE00171 ICM   @12,15,SCCWSCCW(SCCWPTR)                            0174 00286000
         BNZ   @DL00171                                            0174 00287000
*           RFY                                                    0175 00288000
*             REPLREG RSTD;         /* C&S REGISTER                  */ 00289000
*           COMPREG=SRESCCW;        /* COMPARE VALUE- SARTE SCCW Q   */ 00290000
         L     COMPREG,SRESCCW(,SAREPTR)                           0176 00291000
*           REPLREG=ATASCCW;        /* REPLACE VALUE- ATA SCCW QUEUE */ 00292000
         L     REPLREG,ATASCCW(,ATAPTR)                            0177 00293000
*RTRYCS1:                                                          0178 00294000
*           SCCWSCCW=COMPREG;       /* GET SARTE SCCW QUEUE ANCHOR   */ 00295000
RTRYCS1  ST    COMPREG,SCCWSCCW(,SCCWPTR)                          0178 00296000
*           CS(COMPREG,REPLREG,SRESCCW);/* UPDATE SARTE SCCW QUEUE   */ 00297000
         CS    COMPREG,@07,SRESCCW(SAREPTR)                        0179 00298000
*           BC(NE,RTRYCS1);         /* TRY AGAIN                     */ 00299000
         BC    7,RTRYCS1                                           0180 00300000
*           RFY                                                    0181 00301000
*            (SCCWPTR,                                             0181 00302000
*             REPLREG) UNRSTD;      /* FREE REGISTERS                */ 00303000
*         END;                      /* NON-ZERO ATA SCCW QUEUE       */ 00304000
*       RFY                                                        0183 00305000
*         COMPREG UNRSTD;                                          0183 00306000
@RF00167 DS    0H                                                  0184 00307000
*     END;                          /* UNLOCKING SARTE               */ 00308000
*   RFY                                                            0185 00309000
*     SAREPTR UNRSTD;                                              0185 00310000
@RF00157 DS    0H                                                  0186 00311000
*                                                                  0186 00312000
*   /*****************************************************************/ 00313000
*   /*                                                               */ 00314000
*   /* RESCHEDULE THE SWAP DRIVER TO PROCESS SCCWS RETURNED TO THE   */ 00315000
*   /* SART ENTRY WORK QUEUES BY THIS RECOVERY.                      */ 00316000
*   /*                                                               */ 00317000
*   /*****************************************************************/ 00318000
*                                                                  0186 00319000
*   RFY                                                            0186 00320000
*     SARTPTR RSTD;                                                0186 00321000
*   SARTPTR=ASMSART;                /* ADDRESSIBILITY TO SART        */ 00322000
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0187 00323000
*   IF SARSRBCT=ZERO THEN           /* SCHEDULE REQUIRED ?           */ 00324000
         ICM   @12,15,SARSRBCT(SARTPTR)                            0188 00325000
         BNZ   @RF00188                                            0188 00326000
*     DO;                           /* YES- ATTEMPT SCHEDULE         */ 00327000
*       RFY                                                        0190 00328000
*        (COMPREG,                                                 0190 00329000
*         SRBPTR) RSTD;                                            0190 00330000
*       COMPREG=ZERO;               /* COMPARE VALUE FOR SRB COUNT   */ 00331000
         SLR   COMPREG,COMPREG                                     0191 00332000
*       CS(COMPREG,ONE,SARSRBCT);   /* UPDATE SRB COUNT              */ 00333000
         LA    @12,1                                               0192 00334000
         CS    COMPREG,@12,SARSRBCT(SARTPTR)                       0192 00335000
*       BC(NE,SWPDCRTN);            /* BEAT TO THE PUNCH             */ 00336000
         BC    7,SWPDCRTN                                          0193 00337000
*       SRBPTR=SARSRBP;             /* SWAP DRIVER'S SRB             */ 00338000
         L     SRBPTR,SARSRBP(,SARTPTR)                            0194 00339000
*       GENERATE REFS(SRBPTR,CVTPTR,CVTLSMQ,SRBFLNK) SETS(SRBFLNK) 0195 00340000
*           (SCHEDULE SRB=(SRBPTR),SCOPE=LOCAL);                   0195 00341000
         SCHEDULE SRB=(SRBPTR),SCOPE=LOCAL                              00342000
*       SDWAPTR=SAVSDWA;            /* SDWAPTR DESTROYED BY SCHEDULE */ 00343000
         L     SDWAPTR,SAVSDWA                                     0196 00344000
*       RFY                                                        0197 00345000
*        (COMPREG,                                                 0197 00346000
*         SRBPTR) UNRSTD;                                          0197 00347000
*     END;                                                         0198 00348000
*   RFY                                                            0199 00349000
*     SARTPTR UNRSTD;                                              0199 00350000
@RF00188 DS    0H                                                  0200 00351000
*SWPDCRTN:                                                         0200 00352000
*   RFY                                                            0200 00353000
*     R14 RSTD;                                                    0200 00354000
SWPDCRTN DS    0H                                                  0201 00355000
*   R14=SAVER14;                    /* RESTORE RETURN ADDRESS        */ 00356000
         L     R14,SAVER14                                         0201 00357000
*   RFY                                                            0202 00358000
*     R14 UNRSTD;                                                  0202 00359000
*   RETURN;                                                        0203 00360000
@EL00001 DS    0H                                                  0203 00361000
@EF00001 DS    0H                                                  0203 00362000
@ER00001 BR    @14                                                 0203 00363000
*                                                                  0204 00364000
*/********************************************************************/ 00365000
*/*                                                                  */ 00366000
*/*  RBDIORB : SUBROUTINE TO REBUILD AN IORB                         */ 00367000
*/*                                                                  */ 00368000
*/*  INPUT: REGISTER INTERFACE  REG 1 - SDWA ADDRESS                 */ 00369000
*/*                             REG 3 - ASMVT ADDRESS                */ 00370000
*/*                             REG 4 - ATA ADDRESS                  */ 00371000
*/*                             REG 8 - VERIFICATION WORKAREA ADDR   */ 00372000
*/*                             REG 9 - FRR WORKAREA ADDRESS         */ 00373000
*/*         THIS ROUTINE IS ENTERED WITHOUT THE SALLOC LOCK. THE     */ 00374000
*/*         SARTE TO BE PROCESSED IS CHECKPOINTED IN THE ATA.        */ 00375000
*/*                                                                  */ 00376000
*/*  PROCESS: THE SALLOC LOCK IS OBTAINED, A GETMAIN IS ISSUED FOR   */ 00377000
*/*           THE LENGTH OF AN IORB, AND THE IORB IS REINITIALIZED.  */ 00378000
*/*           IF SUCCESSFUL, THE IORB IS ANCHORED IN THE SARTE. IF   */ 00379000
*/*           THE GETMAIN FAILS, THE WORK QUEUE OF SCCWS IS          */ 00380000
*/*           CLEANED UP, A MESSAGE IS SENT TO THE OPERATOR, AND THE */ 00381000
*/*           SARTE POINTER IN THE ATA IS ZEROED TO PREVENT ILRSWP01 */ 00382000
*/*           FROM UNLOCKING THE SARTE.                              */ 00383000
*/*                                                                  */ 00384000
*/*  OUTPUT: NO STREAM OR LIST OUTPUT                                */ 00385000
*/*                                                                  */ 00386000
*/********************************************************************/ 00387000
*/*RBDIORB: CHART RBDIORB SUBROUTINE OF ILRSWP01                     */ 00388000
*/*RBDIORB: E ENTRY TO REBUILD IORB SUBROUTINE                       */ 00389000
*/*         L SETLOCK: OBTAIN SALLOC LOCK (UNC)                      */ 00390000
*/*         L GETMAIN: SQA FOR LENGTH OF IORB-IOSB-SRB STRING        */ 00391000
*/*         D (YES,,NO,%BDSRE) STORAGE ACQUIRED ?                    */ 00392000
*/*         P INIT FIELDS CHECKED BY ILRVIORB                        */ 00393000
*/*         S ILRVIORB: REINITIALIZE IORB-IOSB-SRB                   */ 00394000
*/*         P (,%RBDRTN) ANCHOR IORB IN SARTE                        */ 00395000
*/*%BDSRE:  P TURN ON SARTE UNUSABLE FLAG                            */ 00396000
*/*         P SET UP REGISTER FOR ILRVSCWQ                           */ 00397000
*/*         S ILRVSCWQ: VERIFY SARTE SCCW QUEUE                      */ 00398000
*/*         D (YES,%CLRCT,NO,) SARTE SCCW Q EMPTY ?                  */ 00399000
*/*         P GET 1ST SCCW (SCCWPTR= SRESCCW)                        */ 00400000
*/*%SCCWLP: D (YES,%SQUE,NO,) LAST SCCW ?                            */ 00401000
*/*         P (,%SCCWLP) GET NEXT SCCW (SCCWPTR= SCCWSCCW)           */ 00402000
*/*%SQUE:   P GET SART SCCW QUEUE (SCCWSCCW= SARSCCWQ)               */ 00403000
*/*         P C&S SAVSCCW TO SART SCCW Q ANCHOR                      */ 00404000
*/*         D (YES,,NO,%SQUE) C&S SUCCESSFUL                         */ 00405000
*/*%CLRCT:  P ZERO SARTE AVAILABLE SET COUNT                         */ 00406000
*/*         P ZERO SART AVAILABLE SET COUNT                          */ 00407000
*/*         P GET FIRST SARTE (SREPTR= SARENTS@)                     */ 00408000
*/*%SRELP:  P ADD SARTE AVAILABLE SETS TO SART SET COUNT             */ 00409000
*/*         D (YES,%SMSG,NO,) LAST SARTE IN USE ?                    */ 00410000
*/*         P (,%SRELP) GET NEXT SARTE                               */ 00411000
*/*%SMSG:   P SET UP PARAMETER LIST FOR ILRMSG00                     */ 00412000
*/*         S ILRMSG00: UNUSABLE SWAP DATA SET MESSAGE               */ 00413000
*/*         P CLEAR ATA SARTE POINTER                                */ 00414000
*/*%RBDRTN: R RETURN TO CALLER (ILRSWP01)                            */ 00415000
*/*RBDIORB: END                                                      */ 00416000
*                                                                  0204 00417000
*RBDIORB:                                                          0204 00418000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* REBUILD IORB ROUTINE          */ 00419000
