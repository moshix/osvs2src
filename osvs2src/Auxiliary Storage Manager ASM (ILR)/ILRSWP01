*                                      VERIFIED                      */ 00700000
         LA    VERFREG,ASHSWAPQ(,RSMHDPTR)                         0284 00701000
*   CALL ILRVSPAQ;                  /* VERIFY SWAP QUEUE             */ 00702000
         L     @15,@CV02465                                        0285 00703000
         BALR  @14,@15                                             0285 00704000
*   RFY                                                            0286 00705000
*    (VERFREG,                                                     0286 00706000
*     R5,                                                          0286 00707000
*     R6,                                                          0286 00708000
*     R7) UNRSTD;                   /* VERIFY RTN REGISTERS          */ 00709000
*   IF ASHSWAPQ^=ZERO THEN          /* IS QUEUE EMPTY ?              */ 00710000
         L     @12,ASHSWAPQ(,RSMHDPTR)                             0287 00711000
         LTR   @12,@12                                             0287 00712000
         BZ    @RF00287                                            0287 00713000
*     DO;                           /* NO- SETUP FOR RETRY           */ 00714000
*       ASHSWAPQ->AIAERROR=ON;      /* ERROR FLAG IN FIRST AIA       */ 00715000
         OI    AIAERROR(@12),B'00000010'                           0289 00716000
*       IF ATARCRF6=OFF THEN        /* RECURSIVE ERROR ?             */ 00717000
         TM    ATARCRF6(ATAPTR),B'00000100'                        0290 00718000
         BNZ   @RF00290                                            0290 00719000
*         DO;                       /* NO- ALLOW RETRY               */ 00720000
*                                                                  0291 00721000
*           /*********************************************************/ 00722000
*           /*                                                       */ 00723000
*           /* SET UP FOR RETRY. A NON-ZERO RETRY ADDRESS IN SDWARTYA*/ 00724000
*           /* INDICATES TO ILRIOFRR THAT RETRY IS REQUESTED.        */ 00725000
*           /*                                                       */ 00726000
*           /*********************************************************/ 00727000
*                                                                  0292 00728000
*           ATARCRF6=ON;            /* SET RECURSION FLAG            */ 00729000
         OI    ATARCRF6(ATAPTR),B'00000100'                        0292 00730000
