         BNE   @RF00721                                            0721 01951000
*                       DO;         /* FAILURE IN TERMVIO            */ 01952000
*                         RFY                                      0723 01953000
*                           LGE BASED(R0);                         0723 01954000
*                         IF ASHLGEQ^=0 THEN                       0724 01955000
         L     @14,ASHLGEQ(,RSMHDPTR)                              0724 01956000
         LTR   @14,@14                                             0724 01957000
         BZ    @RF00724                                            0724 01958000
*                           DO;     /* IF SOMETHING ON LGE QUEUE     */ 01959000
*                             R0=ASHLGEQ;/* POINT TO LGE QUEUE       */ 01960000
         LR    R0,@14                                              0726 01961000
*                             DO WHILE R0^=0&R15^=24;/* DO WHILE   0727 01962000
*                                      THERE ARE LGES & THE QUEUE IS    01963000
*                                      VALID RC(^=24)                */ 01964000
         B     @DE00727                                            0727 01965000
@DL00727 DS    0H                                                  0728 01966000
*                               CALL ILRVLPRQ;/* VERIFY QUEUE        */ 01967000
         L     @15,@CV02632                                        0728 01968000
         BALR  @14,@15                                             0728 01969000
*                               R0=LGENEXT;/* GET NEXT LGE ADDR      */ 01970000
         LR    @14,R0                                              0729 01971000
         L     R0,LGENEXT(,@14)                                    0729 01972000
*                             END;  /* END DO WHILE                  */ 01973000
@DE00727 LTR   R0,R0                                               0730 01974000
         BZ    @DC00727                                            0730 01975000
         C     R15,@CF00175                                        0730 01976000
         BNE   @DL00727                                            0730 01977000
@DC00727 DS    0H                                                  0731 01978000
*                             RFY                                  0731 01979000
*                               R0 UNRSTD;                         0731 01980000
*                           END;    /* END THERE ARE LGES ON Q       */ 01981000
*                         ELSE                                     0733 01982000
*                           WKLSTKX=0;/* CLEAR LOOP RETRY INDEX      */ 01983000
         B     @RC00724                                            0733 01984000
@RF00724 MVI   WKLSTKX,X'00'                                       0733 01985000
*                       END;        /* END TERMVIO PROCESSING        */ 01986000
*                     ELSE                                         0735 01987000
*                       WKLSTKX=0;  /* CLEAR LOOP RETRY INDEX        */ 01988000
         B     @RC00721                                            0735 01989000
