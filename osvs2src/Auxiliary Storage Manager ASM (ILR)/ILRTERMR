*/*                                                                  */ 01777000
*/* TERMRFRR                                                         */ 01778000
*/* ENTERED FROM RTM ON ERRORS                                       */ 01779000
*/* ILRTERMR FRR ROUTINE - ATTEMPTS TO RETRY AT NEXT RETRY POINT     */ 01780000
*/* INPUT - R0 FRR WORKAREA (200 BYTES) ADDRESS                      */ 01781000
*/*         R1 ADDRESS OF SDWA                                       */ 01782000
*/*         SDWAPARM IS ADDRESS OF TERMR'S PARAMETER LIST            */ 01783000
*/*         THE PARAMETER LIST HAS THE ADDRESSES OF THE RMPL AND     */ 01784000
*/*         TERMR'S AUTO DATA WORK AREA                              */ 01785000
*/*                                                                  */ 01786000
*/********************************************************************/ 01787000
*                                                                  0656 01788000
*TERMRFRR:                                                         0656 01789000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0656 01790000
TERMRFRR DS    0H                                                  0657 01791000
*   RFY                                                            0657 01792000
*    (R14,                                                         0657 01793000
*     R8,                                                          0657 01794000
*     R0,                                                          0657 01795000
*     R1,                                                          0657 01796000
*     SAVERTRN) RSTD;                                              0657 01797000
*   GEN REFS(R12,R8) SETS(R12,R8);                                 0658 01798000
         BALR  R12,0           SET UP ADDRESSABILITY TO THIS ROUTINE    01799000
TFRR     DS    0H                                                       01800000
         LA    R8,TFRR-@PSTART GET CURRENT OFFSET FROM START OF MODULE  01801000
         SR    R12,R8          SUBTRACT FROM BALR TO GET START ADDR     01802000
*   RFY                                                            0659 01803000
*     R8 UNRSTD;                                                   0659 01804000
*   SAVERTRN=R14;                   /* SAVE CALLERS RETURN ADDRESS   */ 01805000
         LR    SAVERTRN,R14                                        0660 01806000
*   RFY                                                            0661 01807000
*     FRRWKAPT RSTD;                                               0661 01808000
*   RFY                                                            0662 01809000
*     R14 UNRSTD;                                                  0662 01810000
*   RFY                                                            0663 01811000
*     SDWA BASED(R1);                                              0663 01812000
*   FRRWKAPT=R0;                    /* POINT TO FRRS WORKAREA        */ 01813000
         LR    FRRWKAPT,R0                                         0664 01814000
*   RFY                                                            0665 01815000
*     R0 UNRSTD;                                                   0665 01816000
*   FRRWKA=''B;                     /* CLEAR FRR WORKAREA            */ 01817000
         XC    FRRWKA(200,FRRWKAPT),FRRWKA(FRRWKAPT)               0666 01818000
*   RFY                                                            0667 01819000
*     ATAPTR RSTD;                                                 0667 01820000
*   RFY                                                            0668 01821000
*     ATA BASED(ATAPTR);                                           0668 01822000
*   ATAPTR=SDWAPARM;                /* OBTAIN ATA ADDRESS            */ 01823000
         L     ATAPTR,SDWAPARM(,R1)                                0669 01824000
*   RSMHDPTR=ATARMPL->RMPLASCB->ASCBRSM;/* OBTAIN RSMHD PTR          */ 01825000
         L     @09,ATARMPL(,ATAPTR)                                0670 01826000
         L     @09,RMPLASCB(,@09)                                  0670 01827000
         L     RSMHDPTR,ASCBRSM(,@09)                              0670 01828000
*   ASMVTPTR=CVTASMVT;              /* OBTAIN ASMVT ADDR             */ 01829000
         L     @09,CVTPTR                                          0671 01830000
         L     ASMVTPTR,CVTASMVT(,@09)                             0671 01831000
*   WORKAPTR=ATAWORKA;              /* OBTAIN AUTO DATA ADDRESS      */ 01832000
         L     WORKAPTR,ATAWORKA(,ATAPTR)                          0672 01833000
*   R13=ADDR(ASMSAVE);              /* POINT TO SAVE AREA            */ 01834000
         LA    R13,ASMSAVE(,ASMVTPTR)                              0673 01835000
*   DO;                             /* ILRTRAS BACK                  */ 01836000
*     RFY                                                          0675 01837000
*       R15 RSTD;                                                  0675 01838000
*     R15=PSAAOLD;                                                 0676 01839000
         L     R15,PSAAOLD                                         0676 01840000
*     GEN(LCTL  1,1,ASCBSTOR(15))                                  0677 01841000
*     REFS(ASCBSTOR);                                              0677 01842000
         LCTL  1,1,ASCBSTOR(15)                                         01843000
*     PSALCCAV->LCCAASID=0;                                        0678 01844000
