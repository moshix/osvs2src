*                   END;            /* END INDEX NOT 1 OR 2          */ 01991000
@RC00721 DS    0H                                                  0737 01992000
*               END;                /* END INDEX NOT 1               */ 01993000
@RC00716 DS    0H                                                  0738 01994000
*             IF WKLSTKX^=0 THEN                                   0738 01995000
@RC00706 CLI   WKLSTKX,0                                           0738 01996000
         BE    @RF00738                                            0738 01997000
*               DO;                 /* LOOP RETRY INDEX NOT ZERO     */ 01998000
*                 IF R15^=0 THEN                                   0740 01999000
         LTR   R15,R15                                             0740 02000000
         BZ    @RF00740                                            0740 02001000
*                   WKLSTKX=WKLSTKX-DECR;/* IF QUEUE BAD CLEAR LOOP     02002000
*                                      RETRY ADDRESS                 */ 02003000
         SLR   @14,@14                                             0741 02004000
         IC    @14,WKLSTKX                                         0741 02005000
         BCTR  @14,0                                               0741 02006000
         STC   @14,WKLSTKX                                         0741 02007000
*                 RFY                                              0742 02008000
*                   R15 UNRSTD;                                    0742 02009000
@RF00740 DS    0H                                                  0743 02010000
*               END;                /* END INDEX STILL NOT ZERO      */ 02011000
*           END;                    /* END INDEX NOT ZERO            */ 02012000
@RF00738 DS    0H                                                  0745 02013000
*                                                                  0745 02014000
*         /***********************************************************/ 02015000
*         /*                                                         */ 02016000
*         /* SET UP RETRY ADDRESS                                    */ 02017000
*         /*                                                         */ 02018000
*         /***********************************************************/ 02019000
*                                                                  0745 02020000
*         IF WKLSTKX^=0|WKRETRY^=0 THEN                            0745 02021000
@RF00704 CLI   WKLSTKX,0                                           0745 02022000
         BNE   @RT00745                                            0745 02023000
         CLI   WKRETRY,0                                           0745 02024000
         BE    @RF00745                                            0745 02025000
@RT00745 DS    0H                                                  0746 02026000
*           DO;                     /* IF THERE IS A RETRY PT        */ 02027000
*             IF WKLSTKX^=0 THEN                                   0747 02028000
         CLI   WKLSTKX,0                                           0747 02029000
         BE    @RF00747                                            0747 02030000
*               SDWARTYA=WKLSTACK(WKLSTKX);/* SET UP RETRY ADDR      */ 02031000
         SLR   @09,@09                                             0748 02032000
         IC    @09,WKLSTKX                                         0748 02033000
         SLA   @09,2                                               0748 02034000
         L     @09,WKLSTACK-4(@09)                                 0748 02035000
         ST    @09,SDWARTYA(,R1)                                   0748 02036000
*             ELSE                                                 0749 02037000
*               SDWARTYA=RETRYTAB(WKRETRY);/* MAIN RETRY ADDRESS     */ 02038000
         B     @RC00747                                            0749 02039000
@RF00747 SLR   @09,@09                                             0749 02040000
         IC    @09,WKRETRY                                         0749 02041000
         SLA   @09,2                                               0749 02042000
         L     @09,RETRYTAB-4(@09)                                 0749 02043000
         ST    @09,SDWARTYA(,R1)                                   0749 02044000
*             RMPLPTR=ATARMPL;      /* SET UP RMPL ADDRESS FOR RETRY */ 02045000
@RC00747 L     @09,ATARMPL(,ATAPTR)                                0750 02046000
         ST    @09,RMPLPTR                                         0750 02047000
*             STM(R0,R15,SDWASRSV); /* PUT RETRY REGS IN SDWA        */ 02048000
         STM   R0,R15,SDWASRSV(R1)                                 0751 02049000
*             RFY                                                  0752 02050000
*               RETRYPTR BASED(SDWARTYA);                          0752 02051000
*             DO;                   /* SETRP RC(4)RECORD(YES)RETREGS(   02052000
*                                      YES)RETADDR(RETRYPTR)         */ 02053000
*               RESPECIFY                                          0754 02054000
*                (GPR00P,                                          0754 02055000
*                 GPR01P,                                          0754 02056000
*                 GPR14P,                                          0754 02057000
*                 GPR15P) RSTD;                                    0754 02058000
*               GPR01P->SDWARCDE=4; /* STORE RC INTO SDWA            */ 02059000
         MVI   SDWARCDE(GPR01P),X'04'                              0755 02060000
*               GPR01P->SDWARTYA=ADDR(RETRYPTR);/* SAVE RETRY ADDRESS*/ 02061000
         L     @09,SDWARTYA(,R1)                                   0756 02062000
         ST    @09,SDWARTYA(,GPR01P)                               0756 02063000
*               GPR01P->SDWARCRD='1'B;/* TURN ON RECORD INDICATOR    */ 02064000
*               GPR01P->SDWAUPRG='1'B;/* TURN ON RETREGS INDICATOR   */ 02065000
         OI    SDWARCRD(GPR01P),B'10001000'                        0758 02066000
*               RESPECIFY                                          0759 02067000
*                (GPR00P,                                          0759 02068000
*                 GPR01P,                                          0759 02069000
*                 GPR14P,                                          0759 02070000
*                 GPR15P) UNRSTD;                                  0759 02071000
*             END;                                                 0760 02072000
*           END;                    /* END OF RETRY POSSIBLE         */ 02073000
*       END;                        /* END PERCD TO OR NOT C0D       */ 02074000
@RF00745 DS    0H                                                  0763 02075000
*   IF SDWARCDE^=RTRY THEN                                         0763 02076000
@RF00696 DS    0H                                                  0763 02077000
@RC00685 CLI   SDWARCDE(R1),X'04'                                  0763 02078000
         BE    @RF00763                                            0763 02079000
*     DO;                           /* IF RETRY NOT POSSIBLE         */ 02080000
*       SDWARCRD=ON;                /* INDICATE RECORD               */ 02081000
         OI    SDWARCRD(R1),B'10000000'                            0765 02082000
*       R1=ATAWORKA;                /* RESET R1 TO WORK AREA @ZA25692*/ 02083000
*                                                                  0766 02084000
         L     R1,ATAWORKA(,ATAPTR)                                0766 02085000
*       /*************************************************************/ 02086000
*       /*                                                           */ 02087000
*       /* DONT USE SDWA AFTER GMA- GMA DESTROYS SDWA ADDRESS BEGIN  */ 02088000
*       /* ILRGMA MACRO EXPANSION                                    */ 02089000
*       /*                                                           */ 02090000
*       /*************************************************************/ 02091000
*                                                                  0767 02092000
*       DO;                         /* ILRGMA FREE                 0767 02093000
*                                      CELL(BWK)WORKREGS(R5,R6)      */ 02094000
*         RESPECIFY                                                0768 02095000
*          (R0,                                                    0768 02096000
*           R1,                                                    0768 02097000
*           R5,                                                    0768 02098000
*           R6,                                                    0768 02099000
*           R14,                                                   0768 02100000
*           R15) RESTRICTED;                                       0768 02101000
*         RESPECIFY                                                0769 02102000
*           ASMPOOLS BASED(R5);                                    0769 02103000
*         RESPECIFY                                                0770 02104000
*           GMACELL BASED(R1);                                     0770 02105000
*         R5=ADDR(ASMBWKPC);                                       0771 02106000
         LA    R5,ASMBWKPC(,ASMVTPTR)                              0771 02107000
*         R14=ASMCPAVL;                                            0772 02108000
         L     R14,ASMCPAVL(,R5)                                   0772 02109000
*GMA00182:                                                         0773 02110000
*         GMANEXT=R14;              /* PUT ADDR CURRENT FIRST IN   0773 02111000
*                                      INPUT CELL                    */ 02112000
GMA00182 ST    R14,GMANEXT(,R1)                                    0773 02113000
*         CS(R14,R1,ASMCPAVL);      /* ADD INPUT CELL TO POOL        */ 02114000
         CS    R14,@01,ASMCPAVL(R5)                                0774 02115000
*         BC(7,GMA00182);                                          0775 02116000
         BC    7,GMA00182                                          0775 02117000
*         RESPECIFY                                                0776 02118000
*          (R0,                                                    0776 02119000
*           R1,                                                    0776 02120000
*           R5,                                                    0776 02121000
*           R6,                                                    0776 02122000
*           R14,                                                   0776 02123000
*           R15) UNRESTRICTED;                                     0776 02124000
*       END;                        /* ILRGMA FREE                 0777 02125000
*                                      CELL(BWK)WORKREGS(R5,R6)      */ 02126000
*                                                                  0777 02127000
*       /*************************************************************/ 02128000
*       /*                                                           */ 02129000
*       /* END ILRGMA MACRO EXPANSION                                */ 02130000
*       /*                                                           */ 02131000
*       /*************************************************************/ 02132000
*                                                                  0778 02133000
*       RFY                                                        0778 02134000
*         R14 RSTD;                                                0778 02135000
*     END;                          /* END RETRY NOT POSSIBLE        */ 02136000
*   R14=SAVERTRN;                   /* RESTORE RETURN ADDRESS        */ 02137000
@RF00763 LR    R14,SAVERTRN                                        0780 02138000
*   RETURN;                                                        0781 02139000
@EL00006 DS    0H                                                  0781 02140000
@EF00006 DS    0H                                                  0781 02141000
@ER00006 BR    @14                                                 0781 02142000
