         TITLE 'ILRTERMR-ASM MEMORY TERMINATION ROUTINE                *00001000
                        '                                               00002000
ILRTERMR CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'ILRTERMR  77.343'                                 0001 00009000
ILRSLTRV DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00464-ILRSLTRV)                                   00013000
         ENTRY ILRSLTRV                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @12,0                                               0001 00017000
@PSTART  DS    0H                                                  0001 00018000
         USING @PSTART,@12                                         0001 00019000
         AH    @15,4(,@15)                                         0001 00020000
         BR    @15                                                 0001 00021000
@EP00001 DS    0H                                                  0002 00022000
*   R2=PARMPTR;                     /* SAVE INPUT OVER GMA           */ 00023000
         LR    R2,PARMPTR                                          0130 00024000
*   ASMVTPTR=CVTASMVT;              /* GMA NEEDS ASMVT ADDRESS       */ 00025000
         L     @10,CVTPTR                                          0131 00026000
         L     ASMVTPTR,CVTASMVT(,@10)                             0131 00027000
*   RFY                                                            0132 00028000
*    (R4,                                                          0132 00029000
*     R5) RSTD;                                                    0132 00030000
*                                                                  0132 00031000
*   /*****************************************************************/ 00032000
*   /*                                                               */ 00033000
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */ 00034000
*   /*                                                               */ 00035000
*   /*****************************************************************/ 00036000
*                                                                  0133 00037000
*   DO;                             /* ILRGMA GET                  0133 00038000
*                                      CELL(BWK)WORKREGS(R4,R5)      */ 00039000
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */ 00040000
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0134 00041000
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */ 00042000
*       1 GMACELL BASED BDY(WORD),                                 0135 00043000
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */ 00044000
*     RESPECIFY                                                    0136 00045000
*      (R0,                                                        0136 00046000
*       R1,                                                        0136 00047000
*       R4,                                                        0136 00048000
*       R5,                                                        0136 00049000
*       R14,                                                       0136 00050000
*       R15) RESTRICTED;                                           0136 00051000
*     RESPECIFY                                                    0137 00052000
*       ASMPOOLS BASED(R4);                                        0137 00053000
*     RESPECIFY                                                    0138 00054000
*       GMACELL BASED(R14);                                        0138 00055000
*     R4=ADDR(ASMBWKPC);                                           0139 00056000
         LA    R4,ASMBWKPC(,ASMVTPTR)                              0139 00057000
*     LM(R14,R15,ASMCPAVQ);         /* GET ADDR OF 1ST CELL          */ 00058000
         LM    R14,R15,ASMCPAVQ(R4)                                0140 00059000
*GMA00040:                                                         0141 00060000
*     IF R14^=0 THEN                /* TEST CELL POOL                */ 00061000
GMA00040 LTR   R14,R14                                             0141 00062000
         BZ    @RF00141                                            0141 00063000
*       DO;                         /* CELL AVAILABLE, REMOVE CELL   */ 00064000
*         R0=GMANEXT;                                              0143 00065000
         L     R0,GMANEXT(,R14)                                    0143 00066000
*         R1=R15-1;                                                0144 00067000
         LR    R1,R15                                              0144 00068000
         BCTR  R1,0                                                0144 00069000
*         CDS(R14,R0,ASMCPAVQ);                                    0145 00070000
         CDS   R14,R0,ASMCPAVQ(R4)                                 0145 00071000
*         BC(7,GMA00040);                                          0146 00072000
         BC    7,GMA00040                                          0146 00073000
*         R1=R14;                                                  0147 00074000
         LR    R1,R14                                              0147 00075000
*       END;                                                       0148 00076000
*     ELSE                                                         0149 00077000
*       DO;                         /* CELL POOL EMPTY               */ 00078000
         B     @RC00141                                            0149 00079000
@RF00141 DS    0H                                                  0150 00080000
*         R1=R4;                    /* PREPARE TO CALL PEX           */ 00081000
         LR    R1,R4                                               0150 00082000
*         RFY                                                      0151 00083000
*          (R11,                                                   0151 00084000
*           R12) RSTD;                                             0151 00085000
*         R4=R11;                   /* SAVE R11                      */ 00086000
         LR    R4,R11                                              0152 00087000
*         R5=R12;                   /* SAVE R12                      */ 00088000
         LR    R5,R12                                              0153 00089000
*         CALL ILRPEX;              /* EXTEND THE POOL               */ 00090000
         L     @15,ASMPEX(,ASMVTPTR)                               0154 00091000
         BALR  @14,@15                                             0154 00092000
*         R11=R4;                   /* RESTORE R11                   */ 00093000
         LR    R11,R4                                              0155 00094000
*         R12=R5;                   /* RESTORE R12                   */ 00095000
         LR    R12,R5                                              0156 00096000
*         RFY                                                      0157 00097000
*          (R11,                                                   0157 00098000
*           R12) UNRSTD;                                           0157 00099000
*       END;                                                       0158 00100000
*     RESPECIFY                                                    0159 00101000
*      (R0,                                                        0159 00102000
*       R1,                                                        0159 00103000
*       R4,                                                        0159 00104000
*       R5,                                                        0159 00105000
*       R14,                                                       0159 00106000
*       R15) UNRESTRICTED;                                         0159 00107000
@RC00141 DS    0H                                                  0160 00108000
*   END;                            /* ILRGMA GET                  0160 00109000
*                                      CELL(BWK)WORKREGS(R4,R5)      */ 00110000
*                                                                  0160 00111000
*   /*****************************************************************/ 00112000
*   /*                                                               */ 00113000
*   /* END ILRGMA MACRO EXPANSION                                    */ 00114000
*   /*                                                               */ 00115000
*   /*****************************************************************/ 00116000
*                                                                  0161 00117000
*                                   /* OBTAIN WORK AREA              */ 00118000
*   IF PARMPTR^=0 THEN                                             0161 00119000
         LTR   PARMPTR,PARMPTR                                     0161 00120000
         BZ    @RF00161                                            0161 00121000
*     DO;                           /* IF WORKAREA RETURNED          */ 00122000
*       WORKAPTR=PARMPTR;           /* SAVE WORK AREA ADDRESS        */ 00123000
         LR    WORKAPTR,PARMPTR                                    0163 00124000
*       RFY                                                        0164 00125000
*        (PARMPTR,                                                 0164 00126000
*         R4,                                                      0164 00127000
*         R5) UNRSTD;                                              0164 00128000
*       GEN REFS(WORKAPTR)(USING @DATD,WORKAPTR);/* ADDRESS WORK AREA*/ 00129000
         USING @DATD,WORKAPTR                                           00130000
*       WORKAR=''B;                 /* INITIALIZE WORK AREA          */ 00131000
         XC    WORKAR(256,WORKAPTR),WORKAR(WORKAPTR)               0166 00132000
*       RMPLPTR=R2->RMPLADR;        /* OBTAIN RMPL ADDRESS           */ 00133000
         L     @10,RMPLADR(,R2)                                    0167 00134000
         ST    @10,RMPLPTR                                         0167 00135000
*       RFY                                                        0168 00136000
*         R2 UNRSTD;                                               0168 00137000
*       RFY                                                        0169 00138000
*        (ATAPTR,                                                  0169 00139000
*         R1,                                                      0169 00140000
*         R5,                                                      0169 00141000
*         R6) RSTD;                                                0169 00142000
*       R6=ADDR(TERMRFRR);          /* SET UP FRR ADDRESS            */ 00143000
         LA    R6,TERMRFRR                                         0170 00144000
*       GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRSELEN,FRRSPARM,  00145000
*           PSALITA,FLC) SETS(FRRSFRRA,FRRSENTR);                  0171 00146000
SETLO   SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,REGS=USE,               X00147000
               RELATED=(PAGE/SWAP,ILRTERMR(SETLR))                      00148000
        SETFRR A,FRRAD=(6),WRKREGS=(5,1),PARMAD=(4)                     00149000
*       ATAADDR=ATAPTR;             /* SAVE ADDRESS OF ATA           */ 00150000
         LR    ATAADDR,ATAPTR                                      0172 00151000
*       ATARMPL=RMPLPTR;            /* PUT RMPL ADDR IN FRR PARMS    */ 00152000
         L     @10,RMPLPTR                                         0173 00153000
         ST    @10,ATARMPL(,ATAPTR)                                0173 00154000
*       ATAWORKA=WORKAPTR;          /* PUT WORK AREA @ IN FRR PARM   */ 00155000
         ST    WORKAPTR,ATAWORKA(,ATAPTR)                          0174 00156000
*       RFY                                                        0175 00157000
*         ATA BASED(ATAADDR);                                      0175 00158000
*       RFY                                                        0176 00159000
*        (ATAPTR,                                                  0176 00160000
*         R1,                                                      0176 00161000
*         R5,                                                      0176 00162000
*         R6) UNRSTD;                                              0176 00163000
*       SAVER13=SAVEPTR;            /* SAVE CALLERS REG 13           */ 00164000
         ST    SAVEPTR,SAVER13                                     0177 00165000
*       SAVEPTR=ADDR(ASMSAVE);      /* PERFORM SAVE AREA LINKAGE     */ 00166000
         LA    SAVEPTR,ASMSAVE(,ASMVTPTR)                          0178 00167000
*       RFY                                                        0179 00168000
*         SAVEA BASED(SAVER13);     /* POINT TO CALLERS SAVE AREA    */ 00169000
*       FWDCHAIN=SAVEPTR;           /* CHAIN UP CALLERS SAVE AREA    */ 00170000
         L     @06,SAVER13                                         0180 00171000
         ST    SAVEPTR,FWDCHAIN(,@06)                              0180 00172000
*       RFY                                                        0181 00173000
*         SAVEA BASED(SAVEPTR);     /* POINT TO MY SAVE AREA         */ 00174000
*       BCKCHAIN=SAVER13;           /* CHAIN MY SAVE AREA            */ 00175000
         ST    @06,BCKCHAIN(,SAVEPTR)                              0182 00176000
*       RSMHDPTR=ASCBRSM;           /* SET UP RSMHD ADDRESS          */ 00177000
         L     @10,ASCBPTR(,@10)                                   0183 00178000
         L     RSMHDPTR,ASCBRSM(,@10)                              0183 00179000
*       IF RSMHDPTR^=0 THEN                                        0184 00180000
         LTR   RSMHDPTR,RSMHDPTR                                   0184 00181000
         BZ    @RF00184                                            0184 00182000
*         DO;                       /* IF RSMHD ADDR IS 0 RETURN     */ 00183000
*           IF ASCBMCC=MEMCREAT THEN                               0186 00184000
         CLC   ASCBMCC(4,@10),@CF02584                             0186 00185000
         BNE   @RF00186                                            0186 00186000
*             WKLSQA=ON;            /* MEMORY CREATE ERROR           */ 00187000
         OI    WKLSQA,B'01000000'                                  0187 00188000
*           ELSE                                                   0188 00189000
*             DO;                   /* NOT MEMORY CREATE ERROR       */ 00190000
         B     @RC00186                                            0188 00191000
@RF00186 DS    0H                                                  0189 00192000
*               WKRETRY=TRSOUT;     /* SET RETRY INDEX TO ONE        */ 00193000
         MVI   WKRETRY,X'01'                                       0189 00194000
*               IF RMPLTYPE=OFF THEN                               0190 00195000
         L     @10,RMPLPTR                                         0190 00196000
         TM    RMPLTYPE(@10),B'10000000'                           0190 00197000
         BNZ   @RF00190                                            0190 00198000
*                 WKLSQA=ON;        /* IF NORMAL MEM TERM LSQA IS  0191 00199000
*                                      AVAILABLE                     */ 00200000
         OI    WKLSQA,B'01000000'                                  0191 00201000
*               ELSE                                               0192 00202000
*                 DO;               /* NOT NORMAL MEM TERM           */ 00203000
         B     @RC00190                                            0192 00204000
@RF00190 DS    0H                                                  0193 00205000
*                   IF RSMFAIL=OFF THEN                            0193 00206000
         TM    RSMFAIL(RSMHDPTR),B'00000010'                       0193 00207000
         BNZ   @RF00193                                            0193 00208000
*                     DO;           /* IF NOT LSQA SWAP IN ERROR     */ 00209000
*                       IF RSMLSQAF=0 THEN                         0195 00210000
         LH    @10,RSMLSQAF(,RSMHDPTR)                             0195 00211000
         N     @10,@CF02800                                        0195 00212000
         LTR   @10,@10                                             0195 00213000
         BNZ   @RF00195                                            0195 00214000
*                         DO;       /* IF NO LSQA FRAMES             */ 00215000
*                           ATARCODE=16;/* SET CODE 16 IN ATA        */ 00216000
         MVI   ATARCODE(ATAADDR),X'10'                             0197 00217000
*                           GEN(ABEND X'C0D',,,SYSTEM);/* ISSUE C0D     00218000
*                                      ABEND                         */ 00219000
         ABEND X'C0D',,,SYSTEM                                          00220000
*                         END;      /* END NO LSQA FRAMES            */ 00221000
*                       ELSE                                       0200 00222000
*                         DO;       /* LSQA FRAMES AVAILABLE         */ 00223000
         B     @RC00195                                            0200 00224000
@RF00195 DS    0H                                                  0201 00225000
*                           IF SPCTPTR^=0 THEN                     0201 00226000
         L     @10,SPCTPTR(,RSMHDPTR)                              0201 00227000
         LTR   @10,@10                                             0201 00228000
         BZ    @RF00201                                            0201 00229000
*                             DO;   /* SPCT ADDR NON ZERO            */ 00230000
*                               IF SPCTSWIN=ON THEN                0203 00231000
         TM    SPCTSWIN(@10),B'10000000'                           0203 00232000
         BNO   @RF00203                                            0203 00233000
*                                 DO;/* IF SWAP IN IN PROGRESS       */ 00234000
*                                   ATARCODE=16;/* SET CODE OF 16 IN    00235000
*                                      ATA                           */ 00236000
         MVI   ATARCODE(ATAADDR),X'10'                             0205 00237000
*                                   GEN(ABEND X'C0D',,,SYSTEM);/*  0206 00238000
*                                      ISSUE ABEND                   */ 00239000
         ABEND X'C0D',,,SYSTEM                                          00240000
*                                 END;/* END SWAP IN IN PROGRESS     */ 00241000
*                               ELSE                               0208 00242000
*                                 DO;/* SWAP IN NOT IN PROGRESS      */ 00243000
         B     @RC00203                                            0208 00244000
@RF00203 DS    0H                                                  0209 00245000
*                                   IF ASHSWPOT=ON THEN/* IF SWAP OUT   00246000
*                                      IN PROGRESS                   */ 00247000
         TM    ASHSWPOT(RSMHDPTR),B'10000000'                      0209 00248000
         BNO   @RF00209                                            0209 00249000
*                                    CALL TERMSOUT;/* CALL SWAP OUT     00250000
*                                      PROCESSING                    */ 00251000
         BAL   @14,TERMSOUT                                        0210 00252000
*                                   IF FIRSTPT=SEGINDX&/* VALIDATE 0211 00253000
*                                      SEGMENT ORIGIN                */ 00254000
*                                       SECPART='000000'B THEN/*   0211 00255000
*                                      ADDRESS (ASCBSTOR)            */ 00256000
@RF00209 L     @10,RMPLPTR                                         0211 00257000
         L     @10,ASCBPTR(,@10)                                   0211 00258000
         CLI   FIRSTPT(@10),X'0F'                                  0211 00259000
         BNE   @RF00211                                            0211 00260000
         TM    SECPART(@10),B'00111111'                            0211 00261000
         BNZ   @RF00211                                            0211 00262000
*                                    WKLSQA=ON;/* INDICATE LSQA    0212 00263000
*                                      AVAILABLE                     */ 00264000
         OI    WKLSQA,B'01000000'                                  0212 00265000
*                                 END;/* END NOT SWAP IN             */ 00266000
@RF00211 DS    0H                                                  0214 00267000
*                             END;  /* END SPCT ADDR NON ZERO        */ 00268000
@RC00203 DS    0H                                                  0215 00269000
*                         END;      /* END LSQA FRAMES AVAILABLE     */ 00270000
@RF00201 DS    0H                                                  0216 00271000
*                     END;          /* END NOT LSQA SWAP IN ERROR    */ 00272000
@RC00195 DS    0H                                                  0217 00273000
*                 END;              /* END ABNORMAL MEM TERM         */ 00274000
@RF00193 DS    0H                                                  0218 00275000
*RETRY1:                                                           0218 00276000
*               WKRETRY=TRAIA1;     /* INCREMENT RETRY INDEX         */ 00277000
@RC00190 DS    0H                                                  0218 00278000
RETRY1   MVI   WKRETRY,X'02'                                       0218 00279000
*                                                                  0219 00280000
*/********************************************************************/ 00281000
*/*                                                                  */ 00282000
*/* TERMAIA1:                                                        */ 00283000
*/* UPDATE THE AIAS ON THE LOCAL I/O QUEUE SO THAT SLOTS WILL BE     */ 00284000
*/* FREED WHEN IN-PROCESS REQUESTS COMPLETE                          */ 00285000
*/*       PCBPTR   = ADDRESS OF CURRENT PCB/AIA                      */ 00286000
*/*       SAVEPTR1 = ADDRESS OF NEXT PCB/AIA                         */ 00287000
*/*                                                                  */ 00288000
*/********************************************************************/ 00289000
*                                                                  0219 00290000
*               STACK1=ADDR(AIA1NEXT);/* PUSH RETRY ADDR INTO STACK  */ 00291000
         LA    @10,AIA1NEXT                                        0219 00292000
         ST    @10,STACK1                                          0219 00293000
*               WKLSTKX=1;          /* INCREMENT LOOP RTRY INDEX     */ 00294000
         MVI   WKLSTKX,X'01'                                       0220 00295000
*               PCBPTR=RSMLIOQF;    /* GET ADDR OF ACTIVE PCB        */ 00296000
         L     PCBPTR,RSMLIOQF(,RSMHDPTR)                          0221 00297000
*               RFY                                                0222 00298000
*                 PCB BASED(PCBPTR);                               0222 00299000
*               DO WHILE PCBPTR^=0; /* LOOP THRU PCBS                */ 00300000
         B     @DE00223                                            0223 00301000
@DL00223 DS    0H                                                  0224 00302000
*                 SAVEPTR1=PCBFQPA; /* GET NEXT PCB/AIA ADDR         */ 00303000
         L     @10,PCBFQPA-1(,PCBPTR)                              0224 00304000
         LA    @10,0(,@10)                                         0224 00305000
         ST    @10,SAVEPTR1                                        0224 00306000
*                 RFY                                              0225 00307000
*                   AIA BASED(ADDR(PCBAIA));                       0225 00308000
*                 AIATERMR=ON;      /* INDICATE ADDR SPACE TERM      */ 00309000
         OI    AIATERMR+36(PCBPTR),B'01000000'                     0226 00310000
*AIA1NEXT:                                                         0227 00311000
*                 PCBPTR=SAVEPTR1;  /* POINT TO NEXT PCB/AIA         */ 00312000
AIA1NEXT L     PCBPTR,SAVEPTR1                                     0227 00313000
*               END;                /* END PCB LOOP                  */ 00314000
@DE00223 LTR   PCBPTR,PCBPTR                                       0228 00315000
         BNZ   @DL00223                                            0228 00316000
*               WKLSTKX=0;          /* POP RETRY ADDR FROM STACK     */ 00317000
         MVI   WKLSTKX,X'00'                                       0229 00318000
*RETRY2:                                                           0230 00319000
*               WKRETRY=TRAVIO;     /* INCREMENT RETRY INDEX         */ 00320000
RETRY2   MVI   WKRETRY,X'03'                                       0230 00321000
*                                                                  0231 00322000
*/********************************************************************/ 00323000
*/*                                                                  */ 00324000
*/* TERMAVIO:                                                        */ 00325000
*/* ASPCT VIO PROCESSING                                             */ 00326000
*/*        LGEPTR   = ADDRESS OF LGE BEING PROCESSED                 */ 00327000
*/*                                                                  */ 00328000
*/********************************************************************/ 00329000
*                                                                  0231 00330000
*               IF WKLSQA=ON THEN                                  0231 00331000
         TM    WKLSQA,B'01000000'                                  0231 00332000
         BNO   @RF00231                                            0231 00333000
*                 DO;               /* LSQA AVAILABLE                */ 00334000
*                   WKLTRAS=ON;     /* INDICATE IN TRAS MODE         */ 00335000
         OI    WKLTRAS,B'10000000'                                 0233 00336000
*                   RFY                                            0234 00337000
*                     R4 RSTD;                                     0234 00338000
*                   R4=RMPLASCB;    /* PUT ASCB @ IN REG FOR TRAS    */ 00339000
         L     @10,RMPLPTR                                         0235 00340000
         L     R4,RMPLASCB(,@10)                                   0235 00341000
*                   DO;             /* ILRTRAS TO(R4)                */ 00342000
*                     DCL                                          0237 00343000
*                       TRASASID PTR(15) DEFINED(ASCBASID);        0237 00344000
*                     PSALCCAV->LCCAASID=R4->TRASASID;             0238 00345000
         LH    @10,TRASASID(,R4)                                   0238 00346000
         L     @01,PSALCCAV                                        0238 00347000
         ST    @10,LCCAASID(,@01)                                  0238 00348000
*                     GEN(LCTL  1,1,ASCBSTOR(R4))                  0239 00349000
*                     REFS(ASCBSTOR,R4);                           0239 00350000
         LCTL  1,1,ASCBSTOR(R4)                                         00351000
*                   END;            /* ILRTRAS TO(R4) TRAS TO      0240 00352000
*                                      TERMINATING MEMORY            */ 00353000
*                   RFY                                            0241 00354000
*                     R4 UNRSTD;                                   0241 00355000
*                   LGEPTR=ASHLGEQ; /* GET ADDR OF LGE QUEUE         */ 00356000
         L     LGEPTR,ASHLGEQ(,RSMHDPTR)                           0242 00357000
*                   DO WHILE LGEPTR^=0;/* DO WHILE THERE ARE LGES    */ 00358000
         B     @DE00243                                            0243 00359000
@DL00243 DS    0H                                                  0244 00360000
*                     RFY                                          0244 00361000
*                       LGE BASED(LGEPTR);                         0244 00362000
*                     RFY                                          0245 00363000
*                       R0 RSTD;                                   0245 00364000
*                     R0=LGEASPCT;  /* OBTAIN ASPCT ADDR             */ 00365000
         L     R0,LGEASPCT(,LGEPTR)                                0246 00366000
*                     IF R0^=0 THEN                                0247 00367000
         LTR   R0,R0                                               0247 00368000
         BZ    @RF00247                                            0247 00369000
*                       DO;         /* ASPCT ADDR^= 0                */ 00370000
*                         CALL ILRALS00;/* SCAN LPMES FOR THIS ASPCT */ 00371000
         BAL   @14,ILRALS00                                        0249 00372000
*                         IF RMPLTYPE=ON THEN/* ABNORMAL MEMORY    0250 00373000
*                                      TERMINATION                   */ 00374000
         L     @10,RMPLPTR                                         0250 00375000
         TM    RMPLTYPE(@10),B'10000000'                           0250 00376000
         BNO   @RF00250                                            0250 00377000
*                           IF ASPSAVED=OFF|/* ASPCT HAS NOT BEEN  0251 00378000
*                                      SAVED                         */ 00379000
*                               ASPSSYM=ZERO THEN/* ZERO S SYMBOL    */ 00380000
         L     @10,ASPCTPTR(,LGEPTR)                               0251 00381000
         TM    ASPSAVED(@10),B'10000000'                           0251 00382000
         BZ    @RT00251                                            0251 00383000
         CLC   ASPSSYM(8,@10),ZERO                                 0251 00384000
         BNE   @RF00251                                            0251 00385000
@RT00251 DS    0H                                                  0252 00386000
*                             WKVIOSC=WKVIOSC+ASPBKSLT;/* INCREMENT     00387000
*                                      INTERNAL VIO CNT              */ 00388000
         L     @01,ASPCTPTR(,LGEPTR)                               0252 00389000
         L     @10,WKVIOSC                                         0252 00390000
         AL    @10,ASPBKSLT(,@01)                                  0252 00391000
         ST    @10,WKVIOSC                                         0252 00392000
*                         RFY                                      0253 00393000
*                           R0 UNRSTD;                             0253 00394000
@RF00251 DS    0H                                                  0253 00395000
@RF00250 DS    0H                                                  0254 00396000
*                       END;        /* END NON ZERO ASPCT ADDR       */ 00397000
*                     LGEPTR=LGENEXT;/* GET NEXT LGE                 */ 00398000
@RF00247 L     LGEPTR,LGENEXT(,LGEPTR)                             0255 00399000
*                   END;            /* END DO WHILE THERE ARE LGES   */ 00400000
@DE00243 LTR   LGEPTR,LGEPTR                                       0256 00401000
         BNZ   @DL00243                                            0256 00402000
*                   DO;             /* ILRTRAS BACK                  */ 00403000
*                     RFY                                          0258 00404000
*                       R15 RSTD;                                  0258 00405000
*                     R15=PSAAOLD;                                 0259 00406000
         L     R15,PSAAOLD                                         0259 00407000
*                     GEN(LCTL  1,1,ASCBSTOR(15))                  0260 00408000
*                     REFS(ASCBSTOR);                              0260 00409000
         LCTL  1,1,ASCBSTOR(15)                                         00410000
*                     PSALCCAV->LCCAASID=0;                        0261 00411000
         L     @14,PSALCCAV                                        0261 00412000
         SLR   @10,@10                                             0261 00413000
         ST    @10,LCCAASID(,@14)                                  0261 00414000
*                     RFY                                          0262 00415000
*                       R15 UNRSTD;                                0262 00416000
*                   END;            /* ILRTRAS BACK TRAS BACK        */ 00417000
