*ALSPROC:                                                          0554 01484000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0554 01485000
*                                                                  0554 01486000
ALSPROC  DS    0H                                                  0555 01487000
*/********************************************************************/ 01488000
*/*                                                                  */ 01489000
*/* ALSPROC IS CALLED BY ILRALS00 (MACRO INCLUDED IN ILRTERMR)       */ 01490000
*/* PURPOSE - FOR EACH LPME PASSED BY ILRALS00, THE SLOT IS FREED &  */ 01491000
*/*           THE VIO ALLOCATED SLOT COUNT IS DECREMENTED            */ 01492000
*/* INPUT -   REGISTER 0 = RPN                                       */ 01493000
*/*           REGISTER 1 = ADDRESS OF LPME                           */ 01494000
*/*                                                                  */ 01495000
*/********************************************************************/ 01496000
*                                                                  0555 01497000
*   RFY                                                            0555 01498000
*    (PARMPTR,                                                     0555 01499000
*     R14) RSTD;                                                   0555 01500000
*   RFY                                                            0556 01501000
*     ASPLPME BASED(PARMPTR);                                      0556 01502000
*   SAVERET1=R14;                   /* SAVE CALLERS ADDR             */ 01503000
         ST    R14,SAVERET1                                        0557 01504000
*   RFY                                                            0558 01505000
*     R14 UNRSTD;                                                  0558 01506000
*   IF ASPLOPIN=OFF&                /* PAGE OPER NOT IN PROCESS      */ 01507000
*       ASPLSAVE=OFF THEN                                          0559 01508000
         TM    ASPLOPIN(PARMPTR),B'01100000'                       0559 01509000
         BNZ   @RF00559                                            0559 01510000
*     DO;                           /* OR PAGE HAS NOT BEEN SAVED    */ 01511000
*       IF ASPLIOER=OFF THEN                                       0561 01512000
         TM    ASPLIOER(PARMPTR),B'00010000'                       0561 01513000
         BNZ   @RF00561                                            0561 01514000
*         DO;                       /* IF NO IO ERROR                */ 01515000
*           RFY                                                    0563 01516000
*             PARMPTR RSTD;                                        0563 01517000
*           PARMPTR=ASPVLSID;       /* PARMPTR = LSID                */ 01518000
         L     @09,ASPVLSID-1(,PARMPTR)                            0564 01519000
         LA    @09,0(,@09)                                         0564 01520000
         LR    PARMPTR,@09                                         0564 01521000
*           CALL ILRFRSL1;          /* FREE THE LSID                 */ 01522000
         L     @15,ASMPFRSL(,ASMVTPTR)                             0565 01523000
         BALR  @14,@15                                             0565 01524000
*           RFY                                                    0566 01525000
*             PARMPTR UNRSTD;                                      0566 01526000
*         END;                      /* END PGE OP OR SAVED           */ 01527000
*       WORKPTR1=ASMVSC;            /* DECREMENT VIO ALLOCATED       */ 01528000
@RF00561 L     WORKPTR1,ASMVSC(,ASMVTPTR)                          0568 01529000
*ALSAGAIN:                                                         0569 01530000
*       WORKPTR2=WORKPTR1-DECR;     /* SLOT COUNT                    */ 01531000
ALSAGAIN LR    WORKPTR2,WORKPTR1                                   0569 01532000
         BCTR  WORKPTR2,0                                          0569 01533000
*       CS(WORKPTR1,WORKPTR2,ASMVSC);/* DECREMENT COUNT              */ 01534000
         CS    WORKPTR1,@06,ASMVSC(ASMVTPTR)                       0570 01535000
*       BC(7,ALSAGAIN);             /* REPEAT TILL CSP WORKS         */ 01536000
         BC    7,ALSAGAIN                                          0571 01537000
*     END;                          /* END NO IO ERROR               */ 01538000
*   RFY                                                            0573 01539000
*     R14 RSTD;                     /* SAVED                         */ 01540000
@RF00559 DS    0H                                                  0574 01541000
*   R14=SAVERET1;                   /* RESTORE CALLERS ADDRESS       */ 01542000
         L     R14,SAVERET1                                        0574 01543000
*   RFY                                                            0575 01544000
*    (PARMPTR,                                                     0575 01545000
*     R14) UNRSTD;                                                 0575 01546000
*   END ALSPROC;                                                   0576 01547000
@EL00003 DS    0H                                                  0576 01548000
@EF00003 DS    0H                                                  0576 01549000
@ER00003 BR    @14                                                 0576 01550000
*                                                                  0577 01551000
*/********************************************************************/ 01552000
*/*                                                                  */ 01553000
*/* TERMSIDE  CALLED BY ILRTERMR                                     */ 01554000
*/* PURPOSE - GO DOWN AIA SIDE QUEUE AND CLEAN UP THE SWAP SET       */ 01555000
*/*           AIAPTR   = ADDRESS OF AIA BEING PROCESSED              */ 01556000
*/*           SAVEPTR1 = ADDRESS OF NEXT AIA                         */ 01557000
*/*                                                                  */ 01558000
*/********************************************************************/ 01559000
*                                                                  0577 01560000
*TERMSIDE:                                                         0577 01561000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0577 01562000
TERMSIDE DS    0H                                                  0578 01563000
*   RFY                                                            0578 01564000
*     R14 RSTD;                                                    0578 01565000
*   SAVERET2=R14;                   /* SAVE RETURN ADDR              */ 01566000
         ST    R14,SAVERET2                                        0579 01567000
*   RFY                                                            0580 01568000
*     R14 UNRSTD;                                                  0580 01569000
*   STACK2=ADDR(SIDENEXT);          /* PUSH LOOP RETRY ADDR INTO     */ 01570000
         LA    @10,SIDENEXT                                        0581 01571000
         ST    @10,STACK2                                          0581 01572000
*   WKLSTKX=2;                      /* STACK                         */ 01573000
         MVI   WKLSTKX,X'02'                                       0582 01574000
*   DO WHILE AIAPTR^=0;             /* DO WHILE PTR TO AIAS NOT 0    */ 01575000
         B     @DE00583                                            0583 01576000
@DL00583 DS    0H                                                  0584 01577000
*     RFY                                                          0584 01578000
*       AIA BASED(AIAPTR);                                         0584 01579000
*     SAVEPTR1=AIAFQPA;             /* GET NEXT AIA IN SWAP SET      */ 01580000
         L     @10,AIAFQPA(,AIAPTR)                                0585 01581000
         ST    @10,SAVEPTR1                                        0585 01582000
*     AIANXAIA=WKPIOPQ;             /* QUEUE AIA TO INTERNAL PIOP    */ 01583000
         L     @10,WKPIOPQ                                         0586 01584000
         ST    @10,AIANXAIA(,AIAPTR)                               0586 01585000
*     WKPIOPQ=AIAPTR;               /* QUEUE                         */ 01586000
         ST    AIAPTR,WKPIOPQ                                      0587 01587000
*SIDENEXT:                                                         0588 01588000
*     AIAPTR=SAVEPTR1;              /* AIAPTR =NEXT AIA IN SWP SET   */ 01589000
SIDENEXT L     AIAPTR,SAVEPTR1                                     0588 01590000
*   END;                            /* END DO WHILE                  */ 01591000
@DE00583 LTR   AIAPTR,AIAPTR                                       0589 01592000
         BNZ   @DL00583                                            0589 01593000
*   WKLSTKX=1;                      /* POP LOOP RETRY ADDR FROM STACK*/ 01594000
         MVI   WKLSTKX,X'01'                                       0590 01595000
*   RFY                                                            0591 01596000
*     R14 RSTD;                                                    0591 01597000
*   R14=SAVERET2;                   /* RESTORE CALLERS ADDR          */ 01598000
         L     R14,SAVERET2                                        0592 01599000
*   RFY                                                            0593 01600000
*     R14 UNRSTD;                                                  0593 01601000
*   END TERMSIDE;                                                  0594 01602000
@EL00004 DS    0H                                                  0594 01603000
@EF00004 DS    0H                                                  0594 01604000
@ER00004 BR    @14                                                 0594 01605000
*TERMSOUT:                                                         0595 01606000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0595 01607000
*                                                                  0595 01608000
TERMSOUT DS    0H                                                  0596 01609000
*/********************************************************************/ 01610000
*/*                                                                  */ 01611000
*/* TERMSOUT:                                                        */ 01612000
*/* FOR SWAP OUT IN PROGRESS CLEAN UP THE SWAP AND CAPTURE QUEUES    */ 01613000
*/*          AIAPTR   = ADDRESS OF CURRENT AIA ON SWAP/CAPTURE Q     */ 01614000
*/*                                                                  */ 01615000
*/********************************************************************/ 01616000
*                                                                  0596 01617000
*   RFY                                                            0596 01618000
*     R14 RSTD;                                                    0596 01619000
*   SAVERET1=R14;                   /* SAVE CALLERS ADDRESS          */ 01620000
