*                 R0=ADDR(ASHSWAPQ);/* GET ADDR OF SWAP QUEUE        */ 01927000
         LA    R0,ASHSWAPQ(,RSMHDPTR)                              0712 01928000
*                 CALL ILRVSPAQ;    /* VALIDITY CHECK SWAP QUEUE     */ 01929000
         L     @15,@CV02630                                        0713 01930000
         BALR  @14,@15                                             0713 01931000
*               END;                /* END TERMSOUT                  */ 01932000
*             ELSE                                                 0715 01933000
*               DO;                 /* WKRETRY NOT 1                 */ 01934000
         B     @RC00706                                            0715 01935000
@RF00706 DS    0H                                                  0716 01936000
*                 IF WKRETRY=TRAIA1 THEN                           0716 01937000
         CLI   WKRETRY,2                                           0716 01938000
         BNE   @RF00716                                            0716 01939000
*                   DO;             /* FAILURE IN TERMAIA1           */ 01940000
*                     CALL ILRVPCBQ;/* VERIFY PCBS                   */ 01941000
         L     @15,@CV02631                                        0718 01942000
         BALR  @14,@15                                             0718 01943000
*                   END;            /* END TERMAIA1                  */ 01944000
*                 ELSE                                             0720 01945000
*                   DO;             /* WKRETRY NOT 1 OR 2            */ 01946000
         B     @RC00716                                            0720 01947000
