         DC    C'COMMAND OR A SPECIFIC OR GENERAL POLL.'                12650068
MSG8     DC    AL2(MSG9-*-2)                                            12700068
         DC    C'SS=40C5',C'  '                                         12750068
         DC    CL29'DATA CHECK,OPERATION CHECK'                         12800068
         DC    C'                                        '              12850068
         DC    C'THE 3271 DETECTS A PARITY CHECK ON THE  '              12900068
