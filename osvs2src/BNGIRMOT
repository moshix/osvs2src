M14      DC    AL4(MSG14),CL4'4E40'                                     07750068
M15      DC    AL4(MSG15),CL4'C140'                                     07800068
M16      DC    AL4(MSG16),CL4'C240'                                     07850068
M17      DC    AL4(MSG17),CL4'C250'                                     07900068
M18      DC    AL4(MSG18),CL4'C2C4'                                     07950068
