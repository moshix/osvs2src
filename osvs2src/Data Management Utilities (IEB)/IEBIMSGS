         TITLE 'IEBIMSGS - PRINTER IMAGE LIBRARY UTILITY MESSAGE HANDLE*00001000
               ER       '                                               00002000
IEBIMSGS CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEBIMSGS  76.222'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*/********************************************************************* 00018000
**                                                                    * 00019000
**  THIS PROCEDURE WILL ACCEPT AS A PARAMETER A NUMBER OF A MESSAGE.  * 00020000
**  THE NUMBER WILL BE USED TO SELECT A MESSAGE WHICH WILL BE MOVED   * 00021000
**  INTO THE WORK AREA ALONG WITH A LIST OF DISPLACEMENT(S) IF        * 00022000
**  APPLICABLE. THE DISPLACEMENT(S) CAN BE USED AS OFFSETS TO FIELDS  * 00023000
**  WHICH MAY BE EDITED WITH APPROPRIATE INFORMATION.                 * 00024000
**                                                                    * 00025000
**********************************************************************/ 00026000
*                                                                  0021 00027000
*              MSGPTR = ADDR(MSGA)     /* GET APPROPRIATE MESSAGE    */ 00028000
*                     + MSGINDX(MSGN); /* FROM TABLE USING OFFSET    */ 00029000
         LA    MSGPTR,MSGA                                         0021 00030000
         SLR   @09,@09                                             0021 00031000
         IC    @09,MSGN(,BUFAD)                                    0021 00032000
         ALR   @09,@09                                             0021 00033000
         LA    @06,MSGINDX-2(@09)                                  0021 00034000
         SLR   @15,@15                                             0021 00035000
         ICM   @15,3,0(@06)                                        0021 00036000
         ALR   MSGPTR,@15                                          0021 00037000
*                                      /* GET LENGTH OF MESSAGE, USE */ 00038000
*              MSGLEN = MSGINDX(MSGN + ONE)                        0022 00039000
*                     - MSGINDX(MSGN)  /* NEXT MSG MINUS THIS MSG    */ 00040000
*                     - DIM(MSGDISP);  /* MINUS DISPLACEMENT FIELDS. */ 00041000
         LCR   @15,@15                                             0022 00042000
         LA    @06,MSGINDX(@09)                                    0022 00043000
         SLR   MSGLEN,MSGLEN                                       0022 00044000
         ICM   MSGLEN,3,0(@06)                                     0022 00045000
         ALR   @15,MSGLEN                                          0022 00046000
         SH    @15,@CH00041                                        0022 00047000
         LR    MSGLEN,@15                                          0022 00048000
*                                      /* SET MESSAGE TO OUTPUT AREA */ 00049000
*              MSGR = MSGREAL(ONE:MSGLEN);                         0023 00050000
         MVI   MSGR+1(BUFAD),C' '                                  0023 00051000
         MVC   MSGR+2(114,BUFAD),MSGR+1(BUFAD)                     0023 00052000
         LR    @09,MSGLEN                                          0023 00053000
         BCTR  @09,0                                               0023 00054000
         EX    @09,@SM00833                                        0023 00055000
*              PREFX = MSGFX;          /* SET PREFIX -IEBA- IN MSG   */ 00056000
         MVC   PREFX(4,BUFAD),MSGFX                                0024 00057000
*              SPACE = SINGL;          /* INSERT SINGLE SPACE.       */ 00058000
         MVI   SPACE(BUFAD),C' '                                   0025 00059000
*              DO                      /* NOW EDIT MESSAGE.          */ 00060000
*               CNT = ONE                                          0026 00061000
*               TO DIM(MSGEDIT)        /* EDIT ONLY INTO BLANK AREAS.*/ 00062000
*               WHILE MSGDISP(CNT) ^= ZERO;                        0026 00063000
         LA    CNT,1                                               0026 00064000
@DL00026 LR    @09,MSGPTR                                          0026 00065000
         BCTR  @09,0                                               0026 00066000
         SLR   @06,@06                                             0026 00067000
         IC    @06,MSGDISP(CNT,@09)                                0026 00068000
         LTR   @06,@06                                             0026 00069000
         BZ    @DC00026                                            0026 00070000
*                IF MDATALEN ^= ZERO   /* EDIT ANY DATA INTO MESSAGE.*/ 00071000
*                  THEN                /*                            */ 00072000
         LR    @09,CNT                                             0027 00073000
         SLA   @09,2                                               0027 00074000
         LA    @15,EDITLEN-4(@09,BUFAD)                            0027 00075000
         CLI   MDATALEN(@15),0                                     0027 00076000
         BE    @RF00027                                            0027 00077000
*                    MSGOF(MSGDISP(CNT):MSGDISP(CNT)+MDATALEN-ONE) 0028 00078000
*                       = MSGDATA(ONE:MDATALEN);                   0028 00079000
         ALR   @06,BUFAD                                           0028 00080000
         SLR   @14,@14                                             0028 00081000
         IC    @14,MDATALEN(,@15)                                  0028 00082000
         BCTR  @14,0                                               0028 00083000
         L     @09,EDITDATA-5(@09,BUFAD)                           0028 00084000
         EX    @14,@SM00835                                        0028 00085000
*                MDATALEN = ZERO;      /* RESET EDIT LENGTH.         */ 00086000
@RF00027 LR    @09,CNT                                             0029 00087000
         SLA   @09,2                                               0029 00088000
         LA    @09,EDITLEN-4(@09,BUFAD)                            0029 00089000
         MVI   MDATALEN(@09),X'00'                                 0029 00090000
*              END;                    /* END OF EDITING.            */ 00091000
         AH    CNT,@CH00051                                        0030 00092000
         CH    CNT,@CH00041                                        0030 00093000
         BNH   @DL00026                                            0030 00094000
@DC00026 DS    0H                                                  0031 00095000
*              DB = SYSPRTAD;          /* GET SYSPRINT DCB ADDRESS.  */ 00096000
         L     DB,SYSPRTAD(,BUFAD)                                 0031 00097000
*              GEN REFS(DB,MSG)                                    0032 00098000
*               (PUT  (DB),MSG(BUFAD));/* PRINT MESSAGE.             */ 00099000
         PUT  (DB),MSG(BUFAD)                                           00100000
*  END IEBIMSGS                        /* RETURN TO CALLER.          */ 00101000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.      * 00102000
*/*%INCLUDE SYSLIB  (IEBIBLKS)                                        * 00103000
*;                                                                 0033 00104000
@EL00001 L     @13,4(,@13)                                         0033 00105000
@EF00001 DS    0H                                                  0033 00106000
@ER00001 LM    @14,@12,12(@13)                                     0033 00107000
         BR    @14                                                 0033 00108000
@DATA    DS    0H                                                       00109000
@CH00051 DC    H'1'                                                     00110000
@CH00041 DC    H'4'                                                     00111000
@SM00833 MVC   MSGR(0,BUFAD),MSGREAL(MSGPTR)                            00112000
@SM00835 MVC   MSGOF-1(0,@06),MSGDATA(@09)                              00113000
         DS    0F                                                       00114000
