         ST    @10,@AL00001+8                                      0192 00115000
         L     @10,@PC00001+20                                     0192 00116000
         ST    @10,@AL00001+12                                     0192 00117000
         LA    @10,RETCODE                                         0192 00118000
         ST    @10,@AL00001+16                                     0192 00119000
         L     @15,@CV00514                                        0192 00120000
         LA    @01,@AL00001                                        0192 00121000
         BALR  @14,@15                                             0192 00122000
*/*  IF NOT SUCCESSFUL                                               */ 00123000
*    IF RETCODE ^= SUCCESS                                         0193 00124000
*                                                                  0193 00125000
*/*    THEN SET ERROR CODE (CONDCODE=1)                              */ 00126000
*      THEN                                                        0193 00127000
         L     @10,RETCODE                                         0193 00128000
         LTR   @10,@10                                             0193 00129000
         BZ    @RF00193                                            0193 00130000
*        DO;                                                       0194 00131000
*                                                                  0194 00132000
*          CONDCODE = NWTVLBL;                                     0195 00133000
         L     @10,@PC00001+24                                     0195 00134000
         MVC   CONDCODE(4,@10),@CF00169                            0195 00135000
*        END;                                                      0196 00136000
*                                                                  0196 00137000
*/*  END-PROCEDURE CREATE_VOLUME_LABEL                               */ 00138000
