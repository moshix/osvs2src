*        /************************************************************/ 01050000
*                                                                  0200 01051000
*                                                                  0200 01052000
*      /************  INCLUDED MAPPING MACROS: IOB & ECB  ************/ 01053000
*                                                                  0200 01054000
*                                                                  0200 01055000
*                                                                  0200 01056000
*/* **************************************************************** */ 01057000
*/*                                                                  */ 01058000
*/*            EVENT CONTROL BLOCK (ECB) (OS/VS1 AND OS/VS2) AND     */ 01059000
*/*            EVENT CONTROL BLOCK EXTENSION (ECBE) (OS/VS2 ONLY)    */ 01060000
*/*                                                                  */ 01061000
*/*  OS/VS2 RELEASE 4, OS/VS1 RELEASE 5, 04/24/75, LEVEL=1           */ 01062000
*/*                                                                  */ 01063000
*/*  METHOD OF ACCESS                                                */ 01064000
*/*      BAL  - A DSECT IS PRODUCED FOR ECB UNLESS DSECT=NO IS       */ 01065000
*/*             SPECIFIED.  USING ON ECB GIVES ADDRESSABILITY FOR    */ 01066000
*/*             ALL ECB SYMBOLS.                                     */ 01067000
*/*             IF EXT=YES IS SPECIFIED, A DSECT IS PRODUCED FOR     */ 01068000
*/*             ECB EXTENSION (ECBE).  USING ON ECBE GIVES           */ 01069000
*/*             ADDRESSABILITY FOR ALL ECBE SYMBOLS.                 */ 01070000
*/*      PL/S - DCL ECBPTR PTR                                       */ 01071000
*/*             SPECIFY %ECBEXT='YES' BEFORE INCLUDING IHAECB IF     */ 01072000
*/*             THE ECB EXTENSION (ECBE) IS TO BE MAPPED.            */ 01073000
*/*             IF A MODULE USES THE ECB EXTENSION, A RESPECIFY      */ 01074000
*/*             STATEMENT MUST BE ISSUED ON ECBE.  ECBE IS BASED     */ 01075000
*/*             ON ECBEXTB, BUT THIS ADDRESS HAS THE LOW-ORDER 2     */ 01076000
*/*             BITS ON TO INDICATE THAT AN ECB EXTENSION EXISTS.    */ 01077000
*/*             THE ADDRESS IN ECBEXTB MUST BE PUT IN ANOTHER        */ 01078000
*/*             POINTER FIELD AND THE LOW-ORDER 2 BITS TURNED OFF    */ 01079000
*/*             IN ORDER TO OBTAIN PROPER ADDRESSABILITY TO THE      */ 01080000
*/*             EXTENSION.                                           */ 01081000
*/*                                                                  */ 01082000
*/*  NOTE                                                            */ 01083000
*/*      THE ECB IS USED FOR COMMUNICATION BETWEEN VARIOUS           */ 01084000
*/*      COMPONENTS OF THE CONTROL PROGRAM, AS WELL AS BETWEEN       */ 01085000
*/*      PROCESSING PROGRAMS AND THE CONTROL PROGRAM.  AN ECB IS     */ 01086000
*/*      THE SUBJECT OF WAIT AND POST MACRO INSTRUCTIONS.            */ 01087000
*/* **************************************************************** */ 01088000
*                                                                  0200 01089000
*DECLARE                                                           0200 01090000
*  1 ECB      BASED(ECBPTR) BDY(WORD),                             0200 01091000
*   2 ECBRB       FIXED(31),             /* REQUEST BLOCK ADDRESS  0200 01092000
*                                           (WHILE AWAITING        0200 01093000
*                                           COMPLETION OF AN EVENT)  */ 01094000
*    3 ECBEVTB     PTR(31),              /* ADDRESS OF EVENT TABLE 0200 01095000
*                                           (MDC300)         @X31ED9A*/ 01096000
*     4 ECBEXTB     PTR(31),             /* ADDRESS OF ECB EXTENSION    01097000
*                                           (OS/VS2)               0200 01098000
*                                           (MDC305)         @Z40EP9A*/ 01099000
*      5 ECBCC       BIT(8),             /* COMPLETION CODE BYTE     */ 01100000
*       6 ECBWAITB BIT(1),               /* WAIT BIT                 */ 01101000
*       6 ECBPOSTB BIT(1),               /* POST BIT                 */ 01102000
*       6 *        BIT(6),               /* COMPLETION CODE BITS     */ 01103000
*      5 ECBCCCNT    CHAR(3) BDY(WORD,2),/* ZEROES OR REMAINDER OF 0200 01104000
*                                           COMPLETION CODE  (AFTER     01105000
*                                           COMPLETION OF THE EVENT) */ 01106000
*       6 ECBRBA      PTR(24),           /* REQUEST BLOCK ADDRESS  0200 01107000
*                                           (WHILE AWAITING        0200 01108000
*                                           COMPLETION OF AN EVENT)  */ 01109000
*        7 ECBEVTBA    PTR(24),          /* ADDRESS OF EVENT TABLE 0200 01110000
*                                           (MDC302)         @X31ED9A*/ 01111000
*         8 ECBEXTBA    PTR(24),         /* ADDRESS OF ECB EXTENSION    01112000
*                                           (OS/VS2)               0200 01113000
*                                           (MDC306)         @Z40EP9A*/ 01114000
*          9 *           CHAR(2),        /* FIRST TWO BYTES OF     0200 01115000
*                                           ECBEVTBA         @XM06072*/ 01116000
*          9 ECBBYTE3    BIT(8),         /* THIRD BYTE OF ECBEVTBA 0200 01117000
*                                           (MDC303)         @XM06072*/ 01118000
*           10 *        BIT(6),          /* BITS 0-5 OF            0200 01119000
*                                           ECBBYTE3         @Z40EP9A*/ 01120000
*           10 ECBEXTND BIT(2),          /* ECB EXTENSION EXISTS   0200 01121000
*                                           (OS/VS2)               0200 01122000
*                                           (MDC307)         @Z40EP9A*/ 01123000
*            11 *        BIT(1),         /* BIT 6 OF               0200 01124000
*                                           ECBBYTE3         @Z40EP9A*/ 01125000
*            11 ECBEVNT  BIT(1);         /* EXTENDED FORMAT ECB    0200 01126000
*                                           (MDC304)         @XM06072*/ 01127000
*                                                                  0200 01128000
*                                                                  0200 01129000
*/*            INPUT/OUTPUT BLOCK                                    */ 01130000
*/*                                                                  */ 01131000
*/*  OS/VS2 RELEASE 4, OS/VS1 RELEASE 5, 10/30/75, LEVEL=4           */ 01132000
*/*                                                                  */ 01133000
*/*  METHOD OF ACCESS                                                */ 01134000
*/*      BAL  - DSECT IS PRODUCED UNLESS DSECT=NO IS SPECIFIED.      */ 01135000
*/*             USING ON IOB GIVES ADDRESSABILITY FOR ALL SYMBOLS.   */ 01136000
*/*      PL/S - DCL (IOBPRPTR,IOBSTPTR) PTR                          */ 01137000
*/*                                                                  */ 01138000
*                                                                  0201 01139000
* DECLARE                                                          0201 01140000
*   1 IOBPREFX BASED(IOBPRPTR) BDY(WORD),                          0201 01141000
*                                                                  0201 01142000
* /* *************************************************************** */ 01143000
* /*                                                                 */ 01144000
* /*                 PREFIX SECTIONS OF THE IOB                      */ 01145000
* /*                                                                 */ 01146000
* /* *************************************************************** */ 01147000
*                                                                  0201 01148000
*                        /* **************************************** */ 01149000
*                        /*    QSAM,BSAM,BPAM PREFIX                 */ 01150000
*                        /*    CHAINED SCHEDULING *** 16 BYTES       */ 01151000
*                        /* **************************************** */ 01152000
*                                                                  0201 01153000
*    2 IOBCFLG1 CHAR(1),             /* I/O INDICATORS               */ 01154000
*     3 IOBRSV01 BIT(1),             /* RESERVED                     */ 01155000
*     3 IOBRSV02 BIT(1),             /* RESERVED                     */ 01156000
*     3 IOBRSV03 BIT(1),             /* RESERVED                     */ 01157000
