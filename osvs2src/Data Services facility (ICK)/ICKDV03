*/*  EXECUTE CHANNEL PROGRAM                                         */ 09075000
*    RESPECIFY (REG1) RSTD;                                        0578 09076000
*    REG1 = ADDR(CBIOB);                                           0579 09077000
         L     @15,@PC00001+4                                      0579 09078000
         L     REG1,VOLIBPTR(,@15)                                 0579 09079000
         LA    REG1,CBIOB(,REG1)                                   0579 09080000
*    GEN CODE;                                                     0580 09081000
*                                                                  0580 09082000
LEXCP1 EXCP (1)                                                         09083000
*/*  ESTABLISH ADDRESSABILITY FOR ECB CONTROL BLOCK                  */ 09084000
*    ECBPTR = ADDR(CBECB);                                         0581 09085000
*                                                                  0581 09086000
         L     @15,@PC00001+4                                      0581 09087000
         L     ECBPTR,VOLIBPTR(,@15)                               0581 09088000
         LA    ECBPTR,CBECB(,ECBPTR)                               0581 09089000
*/*  WAIT FOR COMPLETION                                             */ 09090000
*    REG1 = ECBPTR;                                                0582 09091000
         LR    REG1,ECBPTR                                         0582 09092000
*    GEN CODE;                                                     0583 09093000
LWAIT1  WAIT 1,ECB=(1)                                                  09094000
*    RESPECIFY (REG1) UNRSTD;                                      0584 09095000
*                                                                  0584 09096000
*/*  IF CHANNEL PROGRAM EXECUTED CORRECTLY                           */ 09097000
*    IF ECBCC = CPSUCC                                             0585 09098000
*                                                                  0585 09099000
*/*    THEN SET INDICATOR THAT CHANNEL PROGRAM WAS SUCCESSFUL        */ 09100000
*      THEN DO;                                                    0585 09101000
         CLI   ECBCC(ECBPTR),X'7F'                                 0585 09102000
         BNE   @RF00585                                            0585 09103000
*        CONDCODE = RCSUCC;                                        0587 09104000
         SLR   @02,@02                                             0587 09105000
         L     @15,@PC00001+32                                     0587 09106000
         ST    @02,CONDCODE(,@15)                                  0587 09107000
*        IF GDTDBG = NULLPTR                                       0588 09108000
*          THEN;                                                   0588 09109000
         L     @15,@PC00001                                        0588 09110000
         C     @02,GDTDBG(,@15)                                    0588 09111000
         BE    @RT00588                                            0588 09112000
*          ELSE                                                    0590 09113000
*            CALL ICKDB010 (GDTTBL, 'DV32');                       0590 09114000
         L     @02,@PC00001                                        0590 09115000
