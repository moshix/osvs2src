         MVC   OLDERID2(95,@02),@TS00001                           0557 08989000
*    NEWID2 = 'DVEP';                                              0558 08990000
*                                                                  0558 08991000
         MVC   NEWID2(4,@02),@CC04729                              0558 08992000
*/*  ESTABLISH ADDRESSIBILITY TO IOB                                 */ 08993000
*    IOBSTPTR = ADDR (CBIOB);                                      0559 08994000
         L     @15,@PC00001+4                                      0559 08995000
         L     IOBSTPTR,VOLIBPTR(,@15)                             0559 08996000
         LA    IOBSTPTR,CBIOB(,IOBSTPTR)                           0559 08997000
*/*  IF FIRST PASS THROUGH FOR THIS ACTION                           */ 08998000
*    IF DESCRNUM = 1                                               0560 08999000
*/*    THEN INITIALIZE CHANNEL PROGRAM                               */ 09000000
*      THEN DO;                                                    0560 09001000
         CH    DESCRNUM,@CH00180                                   0560 09002000
         BNE   @RF00560                                            0560 09003000
*/*      MOVE CHANNEL PROGRAM SKELETON INTO DYNAMIC WORK-AREA        */ 09004000
*        CPAREA (1:DOPECBYT) = CPSKEL (1:DOPECBYT);                0562 09005000
         LH    @02,DOPECBYT(,CLSTRPTR)                             0562 09006000
         BCTR  @02,0                                               0562 09007000
         L     @01,DOPECPAD(,CLSTRPTR)                             0562 09008000
         EX    @02,@SM04898                                        0562 09009000
*/*      SET ADDRESS OF CHANNEL PROGRAM INTO IOB                     */ 09010000
*        IOBSTART = ADDR (CPAREA);                                 0563 09011000
         LA    @02,CPAREA                                          0563 09012000
         ST    @02,IOBSTART(,IOBSTPTR)                             0563 09013000
*/*      END-THEN                                                    */ 09014000
*        END;                                                      0564 09015000
*/*    ELSE SIMPLY UPDATE THE CHANNEL PROGRAM SECTION ADDRESS        */ 09016000
*      ELSE DO;                                                    0565 09017000
         B     @RC00560                                            0565 09018000
@RF00560 DS    0H                                                  0566 09019000
*        IOBSTART = ADDR (CPAREA) + (DESCCCWN - 1)*8;              0566 09020000
         LA    @02,CPAREA                                          0566 09021000
         L     @01,DESCRPTR                                        0566 09022000
         LH    @15,DESCCCWN(,@01)                                  0566 09023000
         BCTR  @15,0                                               0566 09024000
         SLA   @15,3                                               0566 09025000
         ALR   @02,@15                                             0566 09026000
         ST    @02,IOBSTART(,IOBSTPTR)                             0566 09027000
*        END;                                                      0567 09028000
*                                                                  0567 09029000
*/*  MODIFY_CHANNEL_PROGRAM TO INCORPORATE INVOKER'S ARGUMENTS, ETC. */ 09030000
*    CALL MODCHPGM;                                                0568 09031000
*                                                                  0568 09032000
@RC00560 BAL   @14,MODCHPGM                                        0568 09033000
*/*  PLACE USER-SUPPLIED SEEK TRACK-ADDRESS INTO IOB                 */ 09034000
*    IOBSEEK = ''B;                                                0569 09035000
