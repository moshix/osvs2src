*               CALL PRINT;         /* SPACE A LINE                  */ 00693000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0327 00694000
         BALR  @14,@15                                             0327 00695000
*               CALL IECDADCB;      /* DCB FORMATTER                 */ 00696000
         L     @15,@CV02686                                        0328 00697000
         BALR  @14,@15                                             0328 00698000
*               IF RCREG=ZERO THEN  /* FORMATTED DEB OK              */ 00699000
         LTR   RCREG,RCREG                                         0329 00700000
