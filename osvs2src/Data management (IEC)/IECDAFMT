*                                                                  0658 01754000
*                                                                    */ 01755000
*/********************************************************************/ 01756000
*                                                                  0658 01757000
*   IF LOCKHELD=ON THEN             /* RELEASE LOCAL LOCK            */ 01758000
         TM    LOCKHELD,B'01000000'                                0658 01759000
         BNO   @RF00658                                            0658 01760000
*     DO;                                                          0659 01761000
*       PARMSAVE=ABDPLPTR;          /* SAVE REGISTER 1               */ 01762000
*                                                                  0660 01763000
         LR    PARMSAVE,ABDPLPTR                                   0660 01764000
*       /*************************************************************/ 01765000
*       /*                                                           */ 01766000
*       /* ONLY REG 1, WHICH GETS MODIFIED BY SETLOCK, NEEDS TO BE   */ 01767000
*       /* SAVED                                                     */ 01768000
*       /*                                                           */ 01769000
*       /*************************************************************/ 01770000
*                                                                  0661 01771000
*       DO;                         /* SETLOCK (RELEASE) TYPE(LOCAL)    01772000
*                                      REGS(USE) RELATED(IECDAFMT)   */ 01773000
*         RFY                                                      0662 01774000
*          (GPR11P,                                                0662 01775000
*           GPR12P,                                                0662 01776000
*           GPR13P,                                                0662 01777000
*           GPR14P) RSTD;                                          0662 01778000
*         RFY                                                      0663 01779000
*          (GPR15P,                                                0663 01780000
*           GPR00P,                                                0663 01781000
*           GPR01P) RSTD;                                          0663 01782000
*         GPR15P=GPR11P;            /* SAVE REG. 11 IN REG. 15.      */ 01783000
         LR    GPR15P,GPR11P                                       0664 01784000
*         GPR00P=GPR12P;            /* SAVE REG. 12 IN REG. 0.       */ 01785000
         LR    GPR00P,GPR12P                                       0665 01786000
*         GPR01P=GPR13P;            /* SAVE REG. 13 IN REG. 1.       */ 01787000
         LR    GPR01P,GPR13P                                       0666 01788000
*         GPR13P=PSALITA;           /* ADDRESS OF LOCK INTERFACE   0667 01789000
*                                      TABLE.                        */ 01790000
         L     GPR13P,PSALITA                                      0667 01791000
*         GEN(LM GPR11P,GPR13P,588(GPR13P));/* LOAD LOCKWORD, CLHT 0668 01792000
*                                      AND ENTRY ADRS.               */ 01793000
         LM GPR11P,GPR13P,588(GPR13P)                                   01794000
*         GEN(BALR GPR14P,GPR13P);  /* BALR TO SETLOCK RTN.          */ 01795000
         BALR GPR14P,GPR13P                                             01796000
*         GPR11P=GPR15P;            /* RESTORE REG. 11.              */ 01797000
         LR    GPR11P,GPR15P                                       0670 01798000
*         GPR15P=GPR13P;            /* SAVE RETURN CODE.             */ 01799000
         LR    GPR15P,GPR13P                                       0671 01800000
*         GPR12P=GPR00P;            /* RESTORE REG. 12               */ 01801000
         LR    GPR12P,GPR00P                                       0672 01802000
*         GPR13P=GPR01P;            /* RESTORE REG. 13.              */ 01803000
         LR    GPR13P,GPR01P                                       0673 01804000
*         RFY                                                      0674 01805000
*          (GPR15P,                                                0674 01806000
*           GPR00P,                                                0674 01807000
*           GPR01P) UNRSTD;                                        0674 01808000
*         RFY                                                      0675 01809000
*          (GPR11P,                                                0675 01810000
*           GPR12P,                                                0675 01811000
*           GPR13P,                                                0675 01812000
*           GPR14P) UNRSTD;                                        0675 01813000
*       END;                        /* SETLOCK (RELEASE) TYPE(LOCAL)    01814000
*                                      REGS(USE) RELATED(IECDAFMT)   */ 01815000
*       LOCKHELD=OFF;               /* INDICATE LOCK NO LONGER HELD  */ 01816000
         NI    LOCKHELD,B'10111111'                                0677 01817000
*       ABDPLPTR=PARMSAVE;          /* RESTORE REGISTER 1            */ 01818000
         LR    ABDPLPTR,PARMSAVE                                   0678 01819000
*     END;                          /* END LOCAL LOCK RELEASED       */ 01820000
*   ELSE                                                           0680 01821000
*     ;                             /* NO NEED TO RELEASE LOCK       */ 01822000
@RF00658 DS    0H                                                  0681 01823000
*   END RELLOCK;                                                   0681 01824000
