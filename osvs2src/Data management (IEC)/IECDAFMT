*               BUFFER(1:LENGTH(DEBUS))=DEBUS;/* UNDER SCORE         */ 00652000
         L     @14,ADPLBUF(,ABDPLPTR)                              0312 00653000
         MVC   BUFFER(19,@14),DEBUS                                0312 00654000
*               CALL PRINT;         /* PRINT IT                      */ 00655000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0313 00656000
         BALR  @14,@15                                             0313 00657000
*               CALL PRINT;         /* BLANK                         */ 00658000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0314 00659000
         BALR  @14,@15                                             0314 00660000
*             END;                  /* END SPECIAL HDR FOR AMDPRDMP  */ 00661000
*           ELSE                                                   0316 00662000
*             ;                     /* SPECIAL HEADER NOT NEEDED     */ 00663000
@RF00302 DS    0H                                                  0317 00664000
*           IF DCBPTR=NEGONE THEN   /* ERROR ACCESSING DCB ADDRESS   */ 00665000
         C     DCBPTR,NEGONE                                       0317 00666000
         BNE   @RF00317                                            0317 00667000
*             DO;                                                  0318 00668000
*               BUFFER(1:LENGTH(DCBERR))=DCBERR;                   0319 00669000
         L     @14,ADPLBUF(,ABDPLPTR)                              0319 00670000
         MVC   BUFFER(61,@14),DCBERR                               0319 00671000
*               CALL PRINT;         /* ERROR MSG                     */ 00672000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0320 00673000
         BALR  @14,@15                                             0320 00674000
*             END;                  /* END ERROR ACCESSING DCB ADDR  */ 00675000
*           ELSE                                                   0322 00676000
*             DO;                   /* DCB ADDRESS OK                */ 00677000
         B     @RC00317                                            0322 00678000
@RF00317 DS    0H                                                  0323 00679000
*               ADPLCOM4=ADDR(ESTAPARM);/* PASS ESTAE PARM LIST TO 0323 00680000
