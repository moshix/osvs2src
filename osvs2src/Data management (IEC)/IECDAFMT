         TITLE 'IECDAFMT-MAINLINE FOR DATA MGMT CONTROL BLOCK FORMATTER*00001000
               R        '                                               00002000
IECDAFMT CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IECDAFMT  77.299'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  LA    @12,4095(,@11)                                      0001 00012000
         USING @PSTART,@11                                         0001 00013000
         USING @PSTART+4095,@12                                    0001 00014000
*                                                                  0153 00015000
*/********************************************************************/ 00016000
*/* FUNCTION -                                                     0153 00017000
*      ROUTINE TO DETERMINE IF THE DATA MANAGEMENT CONTROL BLOCKS ARE   00018000
*      TO BE FORMATTED. IF THEY ARE, IT SCANS THE DEB CHAIN (ALWAYS     00019000
*      STARTING AT THE TCB) TO PROCESS EACH DEB.                   0153 00020000
*                                                                  0153 00021000
*   INPUT -                                                        0153 00022000
*      REGISTER 1 MUST POINT TO THE PARMLIST PASSED BY THE CALLER  0153 00023000
*                                                                  0153 00024000
*   CALLS -                                                        0153 00025000
*      ACCESRTN, FORMATIT, RELLOCK, AND PRINT                      0153 00026000
*                                                                  0153 00027000
*   CALLED BY -                                                    0153 00028000
*      AMDPRDMP OR SNAP/ABEND TO FORMAT THE DATA MGMT CONTROL BLOCKS    00029000
*                                                                  0153 00030000
*   RETURNS TO -                                                   0153 00031000
*      CALLER WITH RETURN CODE 0                                   0153 00032000
*                                                                    */ 00033000
*/********************************************************************/ 00034000
*                                                                  0153 00035000
*   TREG2=ABDPLPTR;                 /* SAVE PARM LIST POINTER /*   0153 00036000
*                                                            @ZA26256*/ 00037000
         LR    TREG2,ABDPLPTR                                      0153 00038000
*   IF ADPLSNPR=OFF THEN            /* SNAP CALLER           @ZA26256*/ 00039000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0154 00040000
         BNZ   @RF00154                                            0154 00041000
*     DO;                           /* YES.....              @ZA26256*/ 00042000
*       PARMPTR=ADDR(ADPLCOM2);     /* ADDRESS OF GETMAIN LIST     0156 00043000
*                                                            @ZA26256*/ 00044000
         LA    @14,ADPLCOM2(,ABDPLPTR)                             0156 00045000
         LR    PARMPTR,@14                                         0156 00046000
*       GENERATE REFS(DREG,ADPLCOM1);                              0157 00047000
*                                                         /* @ZA26256*/ 00048000
    GETMAIN EC,LV=@ENDDATD-@DATD,SP=253,A=ADPLCOM1(TREG2),   @ZA26256  X00049000
               MF=(E,(1))                                    @ZA26256   00050000
*     END;                          /* END... SNAP CALLER    @ZA26256*/ 00051000
*   ELSE                                                           0159 00052000
*     DO;                           /*                       @ZA26256*/ 00053000
         B     @RC00154                                            0159 00054000
@RF00154 DS    0H                                                  0160 00055000
*       TREG6=0;                    /*                       @ZA26256*/ 00056000
         SLR   TREG6,TREG6                                         0160 00057000
*       GENERATE REFS(TREG6,ADPLSBPL);                             0161 00058000
*                                                         /* @ZA26256*/ 00059000
    IC TREG6,ADPLSBPL(TREG2)                              /* @ZA26256*/ 00060000
*       PARMPTR=ADDR(ADPLCOM2);     /* ADDRESS OF GETMAIN LIST     0162 00061000
*                                                            @ZA26256*/ 00062000
         LA    @14,ADPLCOM2(,ABDPLPTR)                             0162 00063000
         LR    PARMPTR,@14                                         0162 00064000
*       GENERATE REFS(TREG6,ADPLSBPL,ADPLCOM1);                    0163 00065000
*                                                         /* @ZA26256*/ 00066000
    GETMAIN EC,LV=@ENDDATD-@DATD,SP=(TREG6),                 @ZA26256  X00067000
               A=ADPLCOM1(TREG2),MF=(E,(1))                  @ZA26256   00068000
*     END;                                                         0164 00069000
*   IF RCREG^=0 THEN                /* RETURN CODE FROM GM TEST /* 0165 00070000
*                                                            @ZA26256*/ 00071000
@RC00154 LTR   RCREG,RCREG                                         0165 00072000
         BZ    @RF00165                                            0165 00073000
*     DO;                           /*                       @ZA26256*/ 00074000
*       IF ADPLSNPR=OFF THEN        /* WHICH ERROR CODE TO USE?    0167 00075000
*                                                            @ZA26256*/ 00076000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0167 00077000
         BNZ   @RF00167                                            0167 00078000
*         RETURN CODE(8);           /* RETURN WITH RC 8      @ZA26256*/ 00079000
         LA    @15,8                                               0168 00080000
         L     @14,12(,@13)                                        0168 00081000
         LM    @00,@12,20(@13)                                     0168 00082000
         BR    @14                                                 0168 00083000
*       ELSE                        /*                       @ZA26256*/ 00084000
*         RETURN CODE(4);           /* RETURN WITH RC 4      @ZA26256*/ 00085000
@RF00167 LA    @15,4                                               0169 00086000
         L     @14,12(,@13)                                        0169 00087000
         LM    @00,@12,20(@13)                                     0169 00088000
         BR    @14                                                 0169 00089000
*     END;                          /*                       @ZA26256*/ 00090000
*   ABDPLPTR=TREG2;                 /* RESTORE PARM POINTER  @ZA26256*/ 00091000
@RF00165 LR    ABDPLPTR,TREG2                                      0171 00092000
*   DREG=ADPLCOM1;                  /* SAVE ADDRESS OF GETMAINED AREA   00093000
*                                                            @ZA26256*/ 00094000
         L     DREG,ADPLCOM1(,ABDPLPTR)                            0172 00095000
*   GENERATE REFS(SAVEA1,REG13);                                   0173 00096000
*                                                                  0173 00097000
*                                                         /* @ZA26256*/ 00098000
      USING @DATD,DREG           BASE FOR DSECT              @ZA26256   00099000
      ST REG13,SAVEA1+4          SAVE OLD SAVEAREA ADDRESS   @ZA26256   00100000
      LA R5,SAVEA1               GET NEW SAVEAREA ADDRESS    @ZA26256   00101000
      ST R5,8(REG13)             SAVE NEW SAVEAREA ADDRESS   @ZA26256   00102000
      LR REG13,R5                NEW SAVEAREA ADDRESS IN R13 @ZA26256   00103000
*   /*****************************************************************/ 00104000
*   /*                                                               */ 00105000
*   /* ESTABLISH THE ESTAE                                           */ 00106000
*   /*                                                               */ 00107000
*   /*****************************************************************/ 00108000
*                                                                  0174 00109000
*   ESTAERR=ZERO;                   /* TURN OFF ESTAE BITS   @ZM45067*/ 00110000
         SLR   @14,@14                                             0174 00111000
         ST    @14,ESTAERR                                         0174 00112000
*   STM(VADR,RCREG,RTYREGS);        /* SAVE REGS FOR THE ESTAE     0175 00113000
*                                                            @ZM45067*/ 00114000
         STM   VADR,RCREG,RTYREGS                                  0175 00115000
*   LCALREGS=RTYREGS;               /* REGS FOR EXPECTED RETRY     0176 00116000
*                                                            @ZM45067*/ 00117000
         MVC   LCALREGS(64),RTYREGS                                0176 00118000
*   SAVEREG1=ABDPLPTR;              /* SAVE REGISTER 1 ACCROSS ESTAE */ 00119000
         LR    SAVEREG1,ABDPLPTR                                   0177 00120000
*   ESTAEXEC(1:ESTALEN)=ESTALIST(1:ESTALEN);/* MOVE LIST INTO      0178 00121000
*                                      AUTOMATIC AREA                */ 00122000
         L     @14,ESTALEN                                         0178 00123000
         BCTR  @14,0                                               0178 00124000
         EX    @14,@SM02961                                        0178 00125000
*   GEN FLOWS(ESTAERTN)(ESTAE  ESTAERTN,PARAM=(DREG),MF=(E,ESTAEXEC));  00126000
         ESTAE  ESTAERTN,PARAM=(DREG),MF=(E,ESTAEXEC)                   00127000
*                                   /* ESTAE WITH PARAM POINTING TO     00128000
*                                      ALL THE AUTOMATIC DATA FOR  0180 00129000
*                                      THIS MODULE                   */ 00130000
*   IF RCREG=ZERO THEN              /* ESTAE RESOLVED FINE           */ 00131000
         SLR   @14,@14                                             0180 00132000
         CR    RCREG,@14                                           0180 00133000
         BNE   @RF00180                                            0180 00134000
*     DO;                                                          0181 00135000
*       ABDPLPTR=SAVEREG1;          /* RESTORE REG 1                 */ 00136000
*                                                                  0182 00137000
         LR    ABDPLPTR,SAVEREG1                                   0182 00138000
*       /*************************************************************/ 00139000
*       /*                                                           */ 00140000
*       /* FORMAT THE CONTROL BLOCKS. CHECK IF THE DUMP LEVEL IS     */ 00141000
*       /* COMPATIBLE WITH THE FORMATTER AND THAT THE NEEDED INFO WAS*/ 00142000
*       /* PASSED                                                    */ 00143000
*       /*                                                           */ 00144000
*       /*************************************************************/ 00145000
*                                                                  0183 00146000
*       IF ADPLTCB=ZERO|ADPLCVT=ZERO THEN/* CALLER DID NOT PASS TCB     00147000
*                                      OR CVT                        */ 00148000
         C     @14,ADPLTCB(,ABDPLPTR)                              0183 00149000
         BE    @RT00183                                            0183 00150000
         C     @14,ADPLCVT(,ABDPLPTR)                              0183 00151000
         BNE   @RF00183                                            0183 00152000
@RT00183 DS    0H                                                  0184 00153000
*         BUFFER(1:LENGTH(NOINF))=NOINF;/* ERROR MESSAGE             */ 00154000
         L     @14,ADPLBUF(,ABDPLPTR)                              0184 00155000
         MVC   BUFFER(99,@14),NOINF                                0184 00156000
*       ELSE                        /* NEEDED TCB AND CVT WERE PASSED*/ 00157000
*         DO;                                                      0185 00158000
         B     @RC00183                                            0185 00159000
@RF00183 DS    0H                                                  0186 00160000
*           VADR=ADDR(CVTDCB);      /* OPERATING SYSTEM FLAGS        */ 00161000
         L     @14,CVTPTR(,ABDPLPTR)                               0186 00162000
         LA    VADR,CVTDCB(,@14)                                   0186 00163000
*           CALL ACCESRTN;          /* ACCESS THEM                   */ 00164000
         BAL   @14,ACCESRTN                                        0187 00165000
*           IF RCREG^=ZERO THEN     /* ACCESS ERROR                  */ 00166000
         LTR   RCREG,RCREG                                         0188 00167000
         BZ    @RF00188                                            0188 00168000
*             BUFFER(1:LENGTH(INCOMP))=INCOMP;/* ERROR MESSAGE       */ 00169000
         L     @14,ADPLBUF(,ABDPLPTR)                              0189 00170000
         MVC   BUFFER(74,@14),INCOMP                               0189 00171000
*           ELSE                    /* FOUND THEM                    */ 00172000
*             DO;                                                  0190 00173000
         B     @RC00188                                            0190 00174000
@RF00188 DS    0H                                                  0191 00175000
*               IF ADPLSYTM=ON|(DATABYTE&LOWBIT)^=LOWBIT THEN/* DUMP    00176000
*                                      SYSTEM IS NOT MVS             */ 00177000
         TM    ADPLSYTM(ABDPLPTR),B'01000000'                      0191 00178000
         BO    @RT00191                                            0191 00179000
         SLR   @14,@14                                             0191 00180000
         IC    @14,LOWBIT                                          0191 00181000
         SLR   @10,@10                                             0191 00182000
         IC    @10,DATABYTE(,DATAPTR)                              0191 00183000
         NR    @10,@14                                             0191 00184000
         CR    @10,@14                                             0191 00185000
         BE    @RF00191                                            0191 00186000
@RT00191 DS    0H                                                  0192 00187000
*                 BUFFER(1:LENGTH(INCOMP))=INCOMP;                 0192 00188000
         L     @14,ADPLBUF(,ABDPLPTR)                              0192 00189000
         MVC   BUFFER(74,@14),INCOMP                               0192 00190000
*               ELSE                /* WAS AN MVS SYSTEM             */ 00191000
*                 DO;                                              0193 00192000
*                                                                  0193 00193000
         B     @RC00191                                            0193 00194000
@RF00191 DS    0H                                                  0194 00195000
*                   /*************************************************/ 00196000
*                   /*                                               */ 00197000
*                   /* DETERMINE IF THE DATA MGMT CONTROL BLOCKS ARE */ 00198000
*                   /* WANTED. IF THE CALLER IS AMDPRDMP, THE TASK   */ 00199000
*                   /* MUST BE AN ABENDING TASK OR HAVE NON-ZERO     */ 00200000
*                   /* COMPLETION CODE.                              */ 00201000
*                   /*                                               */ 00202000
*                   /*************************************************/ 00203000
*                                                                  0194 00204000
*                   VADR=ADDR(TCBCMP);/* TCB COMPLETION CODE         */ 00205000
         L     @14,ADPLTCB(,ABDPLPTR)                              0194 00206000
         LA    VADR,TCBCMP(,@14)                                   0194 00207000
*                   CALL ACCESRTN;  /* ACCESS IT                     */ 00208000
         BAL   @14,ACCESRTN                                        0195 00209000
*                   IF RCREG^=ZERO THEN/* COULD NOT ACCESS TCB       */ 00210000
         LTR   RCREG,RCREG                                         0196 00211000
         BZ    @RF00196                                            0196 00212000
*                     BUFFER(1:LENGTH(TCBERR))=TCBERR;             0197 00213000
         L     @14,ADPLBUF(,ABDPLPTR)                              0197 00214000
         MVC   BUFFER(72,@14),TCBERR                               0197 00215000
*                   ELSE            /* FOUND TCB COMPLETION CODE     */ 00216000
*                     DO;                                          0198 00217000
         B     @RC00196                                            0198 00218000
@RF00196 DS    0H                                                  0199 00219000
*                       COMPC=DATAWORD;/* SAVE COMPLETION CODE       */ 00220000
         MVC   COMPC(4),DATAWORD(DATAPTR)                          0199 00221000
*                       VADR=ADDR(TCBFLGS1);/* ABEND CODE BYTES      */ 00222000
         L     @14,ADPLTCB(,ABDPLPTR)                              0200 00223000
         LA    VADR,TCBFLGS1(,@14)                                 0200 00224000
*                       CALL ACCESRTN;/* ACCESS IT                   */ 00225000
         BAL   @14,ACCESRTN                                        0201 00226000
*                       IF RCREG^=ZERO THEN/* ACCESS ERROR           */ 00227000
         LTR   RCREG,RCREG                                         0202 00228000
         BZ    @RF00202                                            0202 00229000
*                         BUFFER(1:LENGTH(TCBERR))=TCBERR;         0203 00230000
         L     @14,ADPLBUF(,ABDPLPTR)                              0203 00231000
         MVC   BUFFER(72,@14),TCBERR                               0203 00232000
*                       ELSE        /* FOUND ABEND FLAGS             */ 00233000
*                         DO;                                      0204 00234000
         B     @RC00202                                            0204 00235000
@RF00202 DS    0H                                                  0205 00236000
*                           IF COMPC^=ZERO|/* NON -ZERO COMPLETION 0205 00237000
*                                      CODE                          */ 00238000
*                               (FLGS1&HIGHON)=HIGHON|/* ABEND IN  0205 00239000
*                                      PROGRESS FLAG ON              */ 00240000
*                               ADPLSNPR=OFF THEN/* SNAP WAS INVOKER    00241000
*                                      - CONTROL BLOCKS WANTED       */ 00242000
         ICM   @14,15,COMPC                                        0205 00243000
         BNZ   @RT00205                                            0205 00244000
         SLR   @14,@14                                             0205 00245000
         IC    @14,HIGHON                                          0205 00246000
         SLR   @10,@10                                             0205 00247000
         IC    @10,FLGS1(,DATAPTR)                                 0205 00248000
         NR    @10,@14                                             0205 00249000
         CR    @10,@14                                             0205 00250000
         BE    @RT00205                                            0205 00251000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0205 00252000
         BNZ   @RF00205                                            0205 00253000
@RT00205 DS    0H                                                  0206 00254000
*                             DO;                                  0206 00255000
*                                                                  0206 00256000
*                               /*************************************/ 00257000
*                               /*                                   */ 00258000
*                               /* LOOP THROUGH THE DEB CHAIN        */ 00259000
*                               /* STARTING EACH TIME AT THE TCB AND */ 00260000
*                               /* FINDING ONE DEB FURTHER. THIS     */ 00261000
*                               /* ELIMINATES ERROR IF THE DEB CHAIN */ 00262000
*                               /* CHANGES BETWEEN SCANS             */ 00263000
*                               /*                                   */ 00264000
*                               /*************************************/ 00265000
*                                                                  0207 00266000
*                               DEBPTR=1;/* DUMMY VALUE TO ALLOW   0207 00267000
*                                      FIRST PASS THROUGH LOOP       */ 00268000
         LA    DEBPTR,1                                            0207 00269000
*                               DO DEBCNTR=0 TO DEBMAX WHILE(DEBPTR^=   00270000
*                                     ZERO);/* LOOP THROUGH DEB CHAIN*/ 00271000
         SLR   @14,@14                                             0208 00272000
         ST    @14,DEBCNTR                                         0208 00273000
@DL00208 LTR   DEBPTR,DEBPTR                                       0208 00274000
         BZ    @DC00208                                            0208 00275000
*                                 IF ADPLSNPR=OFF THEN/* SNAP WAS  0209 00276000
*                                      CALLER-LOCAL LOCK IS NEEDED   */ 00277000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0209 00278000
         BNZ   @RF00209                                            0209 00279000
*                                   DO;                            0210 00280000
*                                                                  0210 00281000
*                                    /********************************/ 00282000
*                                    /*                              */ 00283000
*                                    /* ONLY REG 1 WHICH GETS        */ 00284000
*                                    /* MODIFIED BY SETLOCK NEEDS TO */ 00285000
*                                    /* BE SAVED                     */ 00286000
*                                    /*                              */ 00287000
*                                    /********************************/ 00288000
*                                                                  0211 00289000
*                                    PARMSAVE=ABDPLPTR;/* SAVE PTR 0211 00290000
*                                      ACCROSS SETLOCK               */ 00291000
         LR    PARMSAVE,ABDPLPTR                                   0211 00292000
*                                    DO;/* SETLOCK (OBTAIN)        0212 00293000
*                                      MODE(UNCOND) TYPE(LOCAL)    0212 00294000
*                                      REGS(USE) RELATED(IECDAFMT)   */ 00295000
*                                     RFY                          0213 00296000
*                                      (GPR11P,                    0213 00297000
*                                       GPR12P,                    0213 00298000
*                                       GPR13P,                    0213 00299000
*                                       GPR14P) RSTD;              0213 00300000
*                                     RFY                          0214 00301000
*                                      (GPR15P,                    0214 00302000
*                                       GPR00P,                    0214 00303000
*                                       GPR01P) RSTD;              0214 00304000
*                                     GPR15P=GPR11P;/* SAVE REG. 11     00305000
*                                      IN REG. 15.                   */ 00306000
         LR    GPR15P,GPR11P                                       0215 00307000
*                                     GPR00P=GPR12P;/* SAVE REG. 12     00308000
*                                      IN REG. 0.                    */ 00309000
         LR    GPR00P,GPR12P                                       0216 00310000
*                                     GPR01P=GPR13P;/* SAVE REG. 13     00311000
*                                      IN REG. 1.                    */ 00312000
         LR    GPR01P,GPR13P                                       0217 00313000
*                                     GPR13P=PSALITA;/* ADDRESS OF 0218 00314000
*                                      LOCK INTERFACE TABLE.         */ 00315000
         L     GPR13P,PSALITA                                      0218 00316000
*                                     GEN(LM GPR11P,GPR13P,576(GPR13P)) 00317000
*                                         ;/* LOAD LOCKWORD, CLHT AND   00318000
*                                      ENTRY ADRS.                   */ 00319000
         LM GPR11P,GPR13P,576(GPR13P)                                   00320000
*                                     GEN(BALR GPR14P,GPR13P);/* BALR   00321000
*                                      TO SETLOCK RTN.               */ 00322000
         BALR GPR14P,GPR13P                                             00323000
*                                     GPR11P=GPR15P;/* RESTORE REG.     00324000
*                                      11.                           */ 00325000
         LR    GPR11P,GPR15P                                       0221 00326000
*                                     GPR15P=GPR13P;/* SAVE RETURN 0222 00327000
*                                      CODE.                         */ 00328000
         LR    GPR15P,GPR13P                                       0222 00329000
*                                     GPR12P=GPR00P;/* RESTORE REG.     00330000
*                                      12                            */ 00331000
         LR    GPR12P,GPR00P                                       0223 00332000
*                                     GPR13P=GPR01P;/* RESTORE REG.     00333000
*                                      13.                           */ 00334000
         LR    GPR13P,GPR01P                                       0224 00335000
*                                     RFY                          0225 00336000
*                                      (GPR15P,                    0225 00337000
*                                       GPR00P,                    0225 00338000
*                                       GPR01P) UNRSTD;            0225 00339000
*                                     RFY                          0226 00340000
*                                      (GPR11P,                    0226 00341000
*                                       GPR12P,                    0226 00342000
*                                       GPR13P,                    0226 00343000
*                                       GPR14P) UNRSTD;            0226 00344000
*                                    END;/* SETLOCK (OBTAIN)       0227 00345000
*                                      MODE(UNCOND) TYPE(LOCAL)    0227 00346000
*                                      REGS(USE) RELATED(IECDAFMT)   */ 00347000
*                                    ABDPLPTR=PARMSAVE;/* RESTORE  0228 00348000
*                                      REGISTER 1                    */ 00349000
         LR    ABDPLPTR,PARMSAVE                                   0228 00350000
*                                    LOCKHELD=ON;/* INDICATE LOCK  0229 00351000
*                                      HELD TO ESTAE                 */ 00352000
         OI    LOCKHELD,B'01000000'                                0229 00353000
*                                   END;/* END LOCAL LOCK NEEDED     */ 00354000
*                                 ELSE                             0231 00355000
*                                   ;/* LOCK NOT NEEDED FOR AMDPRDMP */ 00356000
@RF00209 DS    0H                                                  0232 00357000
*                                 VADR=ADDR(TCBDEB);/* PTR TO DEB  0232 00358000
*                                      CHAIN FROM TCB                */ 00359000
         L     @14,ADPLTCB(,ABDPLPTR)                              0232 00360000
         LA    VADR,TCBDEB(,@14)                                   0232 00361000
*                                 CALL ACCESRTN;/* ACCESS IT         */ 00362000
         BAL   @14,ACCESRTN                                        0233 00363000
*                                 IF RCREG^=ZERO THEN/* ERROR      0234 00364000
*                                      ACCESSING TCB                 */ 00365000
         LTR   RCREG,RCREG                                         0234 00366000
         BZ    @RF00234                                            0234 00367000
*                                   DO;                            0235 00368000
*                                    BUFFER(1:LENGTH(TCBERR))=TCBERR;   00369000
         L     @14,ADPLBUF(,ABDPLPTR)                              0236 00370000
         MVC   BUFFER(72,@14),TCBERR                               0236 00371000
*                                    /* ERROR MSG                    */ 00372000
*                                    CALL PRINT;/* PRINT IT          */ 00373000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0237 00374000
         BALR  @14,@15                                             0237 00375000
*                                    DEBPTR=ZERO;/* END THE LOOP     */ 00376000
         SLR   DEBPTR,DEBPTR                                       0238 00377000
*                                   END;/* END ACCESS ERROR          */ 00378000
*                                 ELSE/* FOUND DEB ADDRESS           */ 00379000
*                                   DO;                            0240 00380000
         B     @RC00234                                            0240 00381000
@RF00234 DS    0H                                                  0241 00382000
*                                    DEBPTR=DATAWORD;/* SAVE PTR TO     00383000
*                                      DEB                           */ 00384000
*                                                                  0241 00385000
         L     DEBPTR,DATAWORD(,DATAPTR)                           0241 00386000
*                                    /********************************/ 00387000
*                                    /*                              */ 00388000
*                                    /* INNER LOOP TO FIND THE NEXT  */ 00389000
*                                    /* DEB ON THE CHAIN. IF THIS IS */ 00390000
*                                    /* THE FIRST TIME DEBPTR ALREADY*/ 00391000
*                                    /* CONTAINS THE DEB WANTED. THE */ 00392000
*                                    /* FIRST TIME THROUGH THE       */ 00393000
*                                    /* DEBCNTR IS ZERO - INDICATING */ 00394000
*                                    /* THE PTR FROM THE TCB IS THE  */ 00395000
*                                    /* ONE WANTED. THE LOOP IS NOT  */ 00396000
*                                    /* ENTERED. THE NEXT TIME       */ 00397000
*                                    /* THROUGH, DEBCNTR = 1,THE LOOP*/ 00398000
*                                    /* IS ENTERED ONCE TO GET THE   */ 00399000
*                                    /* POINTER TO THE NEXT DEB      */ 00400000
*                                    /*                              */ 00401000
*                                    /********************************/ 00402000
*                                                                  0242 00403000
*                                    DO LOOPCNTR=DEBCNTR TO 1 BY-1 0242 00404000
*                                         WHILE(DEBPTR^=ZERO);     0242 00405000
         L     @14,DEBCNTR                                         0242 00406000
         ST    @14,LOOPCNTR                                        0242 00407000
         B     @DE00242                                            0242 00408000
@DL00242 LTR   DEBPTR,DEBPTR                                       0242 00409000
         BZ    @DC00242                                            0242 00410000
*                                     VADR=ADDR(DEBDEBAD);/* NEXT DEB   00411000
*                                      ADDR                          */ 00412000
         LA    VADR,DEBDEBAD(,DEBPTR)                              0243 00413000
*                                     CALL ACCESRTN;/* ACCESS IT     */ 00414000
         BAL   @14,ACCESRTN                                        0244 00415000
*                                     IF RCREG^=ZERO THEN/* ACCESS 0245 00416000
*                                      ERROR                         */ 00417000
         LTR   RCREG,RCREG                                         0245 00418000
         BZ    @RF00245                                            0245 00419000
*                                      DO;                         0246 00420000
*                                       BUFFER(1:LENGTH(DEBERR))=DEBERR 00421000
*                                           ;/* ERROR MESSAGE        */ 00422000
