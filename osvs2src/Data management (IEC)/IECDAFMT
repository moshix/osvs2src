*                                       DEBPTR=ZERO;/* TERMINATES  0249 00428000
*                                      LOOP                          */ 00429000
         SLR   DEBPTR,DEBPTR                                       0249 00430000
*                                      END;/* END ACCESS ERROR       */ 00431000
*                                     ELSE/* FOUND NEXT DEB          */ 00432000
*                                      DEBPTR=DATAADR;/* SET DEB   0251 00433000
*                                      ADDRESS                       */ 00434000
         B     @RC00245                                            0251 00435000
@RF00245 SLR   DEBPTR,DEBPTR                                       0251 00436000
