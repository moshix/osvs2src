*IOBRTN:                                                           0338 00721000
*   PROC OPTIONS(SAVE(14));                                        0338 00722000
*                                                                  0338 00723000
IOBRTN   ST    @14,@SA00003                                        0338 00724000
*/********************************************************************/ 00725000
*/* FUNCTION -                                                     0339 00726000
*      ROUTINE TO FIND THE ADDRESS OF THE IOB'S (ICB'S,LCB'S) POINTED   00727000
*      TO BY THE DCB. IT PLACES THIS ADDRESS INTO THE PARMLIST AT  0339 00728000
*      ADPLFMT2 AND CALLS THE IOB FORMATTER(IECDAIOB) TO FORMAT EACH    00729000
*      OF THE AVAILABLE IOBS.                                      0339 00730000
*                                                                  0339 00731000
*   INPUT -                                                        0339 00732000
*      REGISTER 1 MUST POINT TO THE PARMLIST AND ADPLCOM2 MUST CONTAIN  00733000
*      THE ACCESS METHOD TYPE SET UP BY THE DCB FORMATTER.         0339 00734000
*                                                                  0339 00735000
*   CALLS -                                                        0339 00736000
*      ACCESRTN, IECDAIOB, AND PRINT                               0339 00737000
*                                                                  0339 00738000
*   CALLED BY -                                                    0339 00739000
*      FORMATIT TO FIND THE FIRST IOB AND TO CALL THE IOB FORMATTER     00740000
*                                                                  0339 00741000
*   RETURNS TO -                                                   0339 00742000
*      CALLER WITHOUT SETTING A RETURN CODE                        0339 00743000
*                                                                    */ 00744000
*/********************************************************************/ 00745000
*                                                                  0339 00746000
*   ERRSW=OFF;                      /* INITIALIZE ERROR INDICATOR    */ 00747000
         NI    ERRSW,B'01111111'                                   0339 00748000
*   ADPLCOM4=ADDR(ESTAPARM);        /* PASS ESTAE PARM LIST TO     0340 00749000
*                                      INDIVIDUAL FORMAT ROUTINES TO    00750000
*                                      USE                           */ 00751000
         LA    @14,ESTAPARM                                        0340 00752000
         ST    @14,ADPLCOM4(,ABDPLPTR)                             0340 00753000
*   ADPLCOM3=DCBPTR;                /* PASS DCB ADDR FOR IOB VALIDITY   00754000
*                                      CHECK                         */ 00755000
*                                                                  0341 00756000
         ST    DCBPTR,ADPLCOM3(,ABDPLPTR)                          0341 00757000
*   /*****************************************************************/ 00758000
*   /*                                                               */ 00759000
*   /* ISAM LOAD TYPE IOB                                            */ 00760000
*   /*                                                               */ 00761000
*   /*****************************************************************/ 00762000
*                                                                  0342 00763000
*   IF ISAMLOAD=ON THEN             /* ISAM LOAD TYPE IOB            */ 00764000
         TM    ISAMLOAD(ABDPLPTR),B'01000000'                      0342 00765000
         BNO   @RF00342                                            0342 00766000
*     DO;                                                          0343 00767000
*       VADR=ADDR(DCBWKPT1);        /* FIRST IOB ADDR                */ 00768000
         LA    VADR,DCBWKPT1+52(,DCBPTR)                           0344 00769000
*       CALL ACCESRTN;              /* ACCESS IT                     */ 00770000
         BAL   @14,ACCESRTN                                        0345 00771000
*       IF RCREG^=ZERO THEN         /* ACCESS ERROR                  */ 00772000
         LTR   RCREG,RCREG                                         0346 00773000
         BZ    @RF00346                                            0346 00774000
*         DO;                                                      0347 00775000
*           BUFFER(1:LENGTH(IOBERR))=IOBERR;/* ERROR MESSAGE         */ 00776000
         L     @14,ADPLBUF(,ABDPLPTR)                              0348 00777000
         MVC   BUFFER(64,@14),IOBERR                               0348 00778000
*           CALL PRINT;             /* PRINT IT                      */ 00779000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0349 00780000
         BALR  @14,@15                                             0349 00781000
*         END;                      /* END ACCESS ERROR              */ 00782000
*       ELSE                        /* FOUND IOB ADDR                */ 00783000
*         DO;                                                      0351 00784000
         B     @RC00346                                            0351 00785000
@RF00346 DS    0H                                                  0352 00786000
*           IOBPRPTR=DATAADR;       /* IOB PREFIX SECTION    @ZM45056*/ 00787000
         SLR   IOBPRPTR,IOBPRPTR                                   0352 00788000
         ICM   IOBPRPTR,7,DATAADR(DATAPTR)                         0352 00789000
*           DO ARRAYCNT=1 TO LOADMAX WHILE(ERRSW=OFF);             0353 00790000
         LA    ARRAYCNT,1                                          0353 00791000
@DL00353 TM    ERRSW,B'10000000'                                   0353 00792000
         BNZ   @DC00353                                            0353 00793000
*             ADPLFMT2=IOBPRPTR+QISMPREF;/* FILL IN PARMLIST WITH PTR   00794000
*                                      TO IOB STANDARD SECTION     0354 00795000
*                                                            @ZM45056*/ 00796000
         LA    @14,4                                               0354 00797000
         LR    @09,IOBPRPTR                                        0354 00798000
         ALR   @09,@14                                             0354 00799000
         ST    @09,ADPLFMT2(,ABDPLPTR)                             0354 00800000
*             OFFSET=QISMPREF;      /* LENGTH OF PREFIX      @ZM45056*/ 00801000
         ST    @14,OFFSET(,ABDPLPTR)                               0355 00802000
*             CALL PRINT;           /* SPACE                         */ 00803000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0356 00804000
         BALR  @14,@15                                             0356 00805000
*             CALL PRINT;           /* SPACE                         */ 00806000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0357 00807000
         BALR  @14,@15                                             0357 00808000
*             CALL IECDAIOB;        /* IOB FORMATTER                 */ 00809000
         L     @15,@CV02687                                        0358 00810000
         BALR  @14,@15                                             0358 00811000
*             IF RCREG=ZERO THEN    /* FORMATTED IOB FINE            */ 00812000
         LTR   RCREG,RCREG                                         0359 00813000
         BNZ   @RF00359                                            0359 00814000
*               IOBPRPTR=IOBPRPTR+LOADLNTH;/* NEXT IOB       @ZM45056*/ 00815000
         AH    IOBPRPTR,@CH01689                                   0360 00816000
*             ELSE                                                 0361 00817000
*               ERRSW=ON;           /* OTHERWISE END LLOP            */ 00818000
         B     @RC00359                                            0361 00819000
@RF00359 OI    ERRSW,B'10000000'                                   0361 00820000
*           END;                    /* END FORMAT LOOP               */ 00821000
@RC00359 AH    ARRAYCNT,@CH00041                                   0362 00822000
         CH    ARRAYCNT,@CH00315                                   0362 00823000
         BNH   @DL00353                                            0362 00824000
@DC00353 DS    0H                                                  0363 00825000
*         END;                      /* END FOUND IOBADDR OK          */ 00826000
*     END;                          /* END ISAM LOAD IOB             */ 00827000
*   ELSE                            /* NOT ISAM LOAD                 */ 00828000
*                                                                  0365 00829000
*     /***************************************************************/ 00830000
*     /*                                                             */ 00831000
*     /* ISAM SCAN TYPE IOB                                          */ 00832000
*     /*                                                             */ 00833000
*     /***************************************************************/ 00834000
*                                                                  0365 00835000
*     IF ISAMSCAN=ON THEN           /* ISAM SCAN TYPE IOB            */ 00836000
         B     @RC00342                                            0365 00837000
@RF00342 TM    ISAMSCAN(ABDPLPTR),B'00100000'                      0365 00838000
         BNO   @RF00365                                            0365 00839000
*       DO;                                                        0366 00840000
*         VADR=ADDR(DCBWKPT1);      /* FIRST IOB ADDR                */ 00841000
         LA    VADR,DCBWKPT1+52(,DCBPTR)                           0367 00842000
*         CALL ACCESRTN;            /* ACCESS IT                     */ 00843000
         BAL   @14,ACCESRTN                                        0368 00844000
*         IF RCREG^=ZERO THEN       /* ACCESS ERROR                  */ 00845000
         LTR   RCREG,RCREG                                         0369 00846000
         BZ    @RF00369                                            0369 00847000
*           DO;                                                    0370 00848000
*             BUFFER(1:LENGTH(IOBERR))=IOBERR;/* ERROR MESSAGE       */ 00849000
         L     @14,ADPLBUF(,ABDPLPTR)                              0371 00850000
         MVC   BUFFER(64,@14),IOBERR                               0371 00851000
*             CALL PRINT;           /* PRINT IT                      */ 00852000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0372 00853000
         BALR  @14,@15                                             0372 00854000
*           END;                    /* END ACCESS ERROR              */ 00855000
*         ELSE                      /* FOUND IOB ADDR                */ 00856000
*           DO;                                                    0374 00857000
         B     @RC00369                                            0374 00858000
@RF00369 DS    0H                                                  0375 00859000
*             IOBPRPTR=DATAADR;     /* IOB PREFIX SECTION            */ 00860000
         SLR   IOBPRPTR,IOBPRPTR                                   0375 00861000
         ICM   IOBPRPTR,7,DATAADR(DATAPTR)                         0375 00862000
*             DO ARRAYCNT=1 TO SCANMAX WHILE(ERRSW=OFF);           0376 00863000
         LA    ARRAYCNT,1                                          0376 00864000
@DL00376 TM    ERRSW,B'10000000'                                   0376 00865000
         BNZ   @DC00376                                            0376 00866000
*               ADPLFMT2=IOBPRPTR+QISMPREF;/* FILL IN PARMLIST WITH     00867000
*                                      PTR TO STANDARD SECTION     0377 00868000
*                                                            @ZM45056*/ 00869000
         LA    @14,4                                               0377 00870000
         LR    @09,IOBPRPTR                                        0377 00871000
         ALR   @09,@14                                             0377 00872000
         ST    @09,ADPLFMT2(,ABDPLPTR)                             0377 00873000
*               OFFSET=QISMPREF;    /* LENGTH OF PREFIX      @ZM45056*/ 00874000
         ST    @14,OFFSET(,ABDPLPTR)                               0378 00875000
*               CALL PRINT;         /* SPACE                         */ 00876000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0379 00877000
         BALR  @14,@15                                             0379 00878000
*               CALL PRINT;         /* SPACE                         */ 00879000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0380 00880000
         BALR  @14,@15                                             0380 00881000
*               CALL IECDAIOB;      /* IOB FORMATTER                 */ 00882000
         L     @15,@CV02687                                        0381 00883000
         BALR  @14,@15                                             0381 00884000
*               IF RCREG=ZERO THEN  /* FORMATTED IOB FINE            */ 00885000
         LTR   RCREG,RCREG                                         0382 00886000
         BNZ   @RF00382                                            0382 00887000
*                 IOBPRPTR=IOBPRPTR+SCANLNTH;/* NEXT IOB             */ 00888000
         AH    IOBPRPTR,@CH00924                                   0383 00889000
*               ELSE                                               0384 00890000
*                 ERRSW=ON;         /* OTHERWISE END LLOP            */ 00891000
         B     @RC00382                                            0384 00892000
@RF00382 OI    ERRSW,B'10000000'                                   0384 00893000
*             END;                  /* END FORMAT LOOP               */ 00894000
@RC00382 AH    ARRAYCNT,@CH00041                                   0385 00895000
         CH    ARRAYCNT,@CH00099                                   0385 00896000
         BNH   @DL00376                                            0385 00897000
@DC00376 DS    0H                                                  0386 00898000
*           END;                    /* END FOUND IOBADDR OK          */ 00899000
*       END;                        /* END ISAM LOAD IOB             */ 00900000
*     ELSE                          /* NOT ISAM LOAD                 */ 00901000
*                                                                  0388 00902000
*       /*************************************************************/ 00903000
*       /*                                                           */ 00904000
*       /* SAM OR BPAM NORMAL SCHEDULING - THE IOBS CHAIN IN A LOOP  */ 00905000
*       /*                                                           */ 00906000
*       /*************************************************************/ 00907000
*                                                                  0388 00908000
*       IF SAMNMAL=ON THEN          /* SAM NORMAL SCHEDULING IOB     */ 00909000
         B     @RC00365                                            0388 00910000
@RF00365 TM    SAMNMAL(ABDPLPTR),B'00001000'                       0388 00911000
         BNO   @RF00388                                            0388 00912000
*         DO;                                                      0389 00913000
*           VADR=ADDR(DCBIOBA);     /* FIRST IOB ADDR                */ 00914000
         LA    VADR,DCBIOBA+52(,DCBPTR)                            0390 00915000
*           CALL ACCESRTN;          /* ACCESS IT                     */ 00916000
         BAL   @14,ACCESRTN                                        0391 00917000
*           IF RCREG^=ZERO THEN     /* ACCESS ERROR                  */ 00918000
         LTR   RCREG,RCREG                                         0392 00919000
         BZ    @RF00392                                            0392 00920000
*             DO;                                                  0393 00921000
*               BUFFER(1:LENGTH(IOBERR))=IOBERR;/* ERROR MESSAGE     */ 00922000
         L     @14,ADPLBUF(,ABDPLPTR)                              0394 00923000
         MVC   BUFFER(64,@14),IOBERR                               0394 00924000
*               CALL PRINT;         /* PRINT IT                      */ 00925000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0395 00926000
         BALR  @14,@15                                             0395 00927000
*             END;                  /* END ACCESS ERROR              */ 00928000
*           ELSE                    /* FOUND IOB ADDR                */ 00929000
*             DO;                                                  0397 00930000
         B     @RC00392                                            0397 00931000
@RF00392 DS    0H                                                  0398 00932000
*               IOBPRPTR=DATAADR;   /* IOB PREFIX SECTION            */ 00933000
         SLR   IOBPRPTR,IOBPRPTR                                   0398 00934000
         ICM   IOBPRPTR,7,DATAADR(DATAPTR)                         0398 00935000
*               COMPARE=ZERO;       /* ADR TO CHECK FOR LOOP         */ 00936000
         SLR   @14,@14                                             0399 00937000
         ST    @14,COMPARE                                         0399 00938000
*               FIRSTIOB=IOBPRPTR;  /* ADDR OF FIRST IOB             */ 00939000
         ST    IOBPRPTR,FIRSTIOB                                   0400 00940000
*               DO ARRAYCNT=1 TO LOOPMAX WHILE(ERRSW=OFF&COMPARE^= 0401 00941000
*                     FIRSTIOB);                                   0401 00942000
         LA    ARRAYCNT,1                                          0401 00943000
@DL00401 TM    ERRSW,B'10000000'                                   0401 00944000
         BNZ   @DC00401                                            0401 00945000
         CLC   COMPARE(4),FIRSTIOB                                 0401 00946000
         BE    @DC00401                                            0401 00947000
*                 IOBSTPTR=IOBPRPTR+NSAMPREF;/* STANDARD SECTION     */ 00948000
         LA    @14,8                                               0402 00949000
         LR    IOBSTPTR,IOBPRPTR                                   0402 00950000
         ALR   IOBSTPTR,@14                                        0402 00951000
*                 ADPLFMT2=IOBSTPTR;/* FILL IN PARMLIST              */ 00952000
         ST    IOBSTPTR,ADPLFMT2(,ABDPLPTR)                        0403 00953000
*                 OFFSET=NSAMPREF;  /* LENGTH OF PREFIX              */ 00954000
         ST    @14,OFFSET(,ABDPLPTR)                               0404 00955000
*                 CALL PRINT;       /* SPACE                         */ 00956000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0405 00957000
         BALR  @14,@15                                             0405 00958000
*                 CALL PRINT;       /* SPACE                         */ 00959000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0406 00960000
         BALR  @14,@15                                             0406 00961000
*                 CALL IECDAIOB;    /* IOB FORMATTER                 */ 00962000
         L     @15,@CV02687                                        0407 00963000
         BALR  @14,@15                                             0407 00964000
*                 IF RCREG=ZERO THEN/* FORMATTED IOB FINE            */ 00965000
         LTR   RCREG,RCREG                                         0408 00966000
         BNZ   @RF00408                                            0408 00967000
*                   DO;                                            0409 00968000
*                     IOBPRPTR=IOBPRPTR-PREFLEN;/* PTR TO BEGINNING     00969000
*                                      OF FULL PREFIX, FOR BASING  0410 00970000
*                                      IOBNIOB CORRECTLY             */ 00971000
         SH    IOBPRPTR,@CH00086                                   0410 00972000
*                     VADR=ADDR(IOBNIOBA);/* NEXT IOB                */ 00973000
         LA    VADR,IOBNIOBA(,IOBPRPTR)                            0411 00974000
*                     CALL ACCESRTN;/* ACCESS IT                     */ 00975000
         BAL   @14,ACCESRTN                                        0412 00976000
*                     IF RCREG^=ZERO THEN/* ACCESS ERROR             */ 00977000
         LTR   RCREG,RCREG                                         0413 00978000
         BZ    @RF00413                                            0413 00979000
*                       DO;                                        0414 00980000
*                         BUFFER(1:LENGTH(IOBCHN))=IOBCHN;/* ERROR 0415 00981000
*                                      MESSAGE                       */ 00982000
         L     @14,ADPLBUF(,ABDPLPTR)                              0415 00983000
         MVC   BUFFER(55,@14),IOBCHN                               0415 00984000
*                         CALL PRINT;/* PRINT IT                     */ 00985000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0416 00986000
         BALR  @14,@15                                             0416 00987000
*                         ERRSW=ON; /* END LOOP                      */ 00988000
         OI    ERRSW,B'10000000'                                   0417 00989000
*                       END;        /* END ACCESS ERROR              */ 00990000
*                     ELSE          /* FOUND IOB ADDR                */ 00991000
*                       DO;                                        0419 00992000
         B     @RC00413                                            0419 00993000
@RF00413 DS    0H                                                  0420 00994000
*                         IOBPRPTR=DATAADR;/* FILL IN IOB PTR        */ 00995000
         SLR   IOBPRPTR,IOBPRPTR                                   0420 00996000
         ICM   IOBPRPTR,7,DATAADR(DATAPTR)                         0420 00997000
*                         COMPARE=IOBPRPTR;/* TO CHECK FOR LOOP      */ 00998000
         ST    IOBPRPTR,COMPARE                                    0421 00999000
*                       END;        /* END FOUND IOB ADDR            */ 01000000
*                   END;            /* END FORMATTED IOB FINE        */ 01001000
*                 ELSE                                             0424 01002000
*                   ERRSW=ON;       /* ERROR FORMATTING IOB          */ 01003000
         B     @RC00408                                            0424 01004000
@RF00408 OI    ERRSW,B'10000000'                                   0424 01005000
*               END;                /* END FORMAT LOOP               */ 01006000
@RC00408 AH    ARRAYCNT,@CH00041                                   0425 01007000
         CH    ARRAYCNT,@CH02766                                   0425 01008000
         BNH   @DL00401                                            0425 01009000
@DC00401 DS    0H                                                  0426 01010000
*             END;                  /* END FOUND IOBADDR OK          */ 01011000
*         END;                      /* END SAM NORMAL IOB            */ 01012000
*       ELSE                        /* NOT SAM NORMAL IOB            */ 01013000
*                                                                  0428 01014000
*         /***********************************************************/ 01015000
*         /*                                                         */ 01016000
*         /* SAM OR BPAM CHAINED SCHEDULING - THERE IS ONE IOB AND A */ 01017000
*         /* CHAIN OF ICBS. THE ICBS CHAIN IN A LOOP                 */ 01018000
*         /*                                                         */ 01019000
*         /***********************************************************/ 01020000
*                                                                  0428 01021000
*         IF SAMCHAIN=ON THEN       /* SAM CHAIN SCHEDULING IOB AND     01022000
*                                      ICB                           */ 01023000
         B     @RC00388                                            0428 01024000
@RF00388 TM    SAMCHAIN(ABDPLPTR),B'00000100'                      0428 01025000
         BNO   @RF00428                                            0428 01026000
*           DO;                                                    0429 01027000
*                                                                  0429 01028000
*             /*******************************************************/ 01029000
*             /*                                                     */ 01030000
*             /* FORMAT THE MAIN IOB                                 */ 01031000
*             /*                                                     */ 01032000
*             /*******************************************************/ 01033000
*                                                                  0430 01034000
*             VADR=ADDR(DCBIOBAD);  /* MAIN IOB ADDR                 */ 01035000
         LA    VADR,DCBIOBAD+16(,DCBPTR)                           0430 01036000
*             CALL ACCESRTN;        /* ACCESS IT                     */ 01037000
         BAL   @14,ACCESRTN                                        0431 01038000
*             IF RCREG^=ZERO THEN   /* ACCESS ERROR                  */ 01039000
         LTR   RCREG,RCREG                                         0432 01040000
         BZ    @RF00432                                            0432 01041000
*               DO;                                                0433 01042000
*                 BUFFER(1:LENGTH(IOBERR))=IOBERR;/* ERROR MESSAGE   */ 01043000
         L     @14,ADPLBUF(,ABDPLPTR)                              0434 01044000
         MVC   BUFFER(64,@14),IOBERR                               0434 01045000
*                 CALL PRINT;       /* PRINT IT                      */ 01046000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0435 01047000
         BALR  @14,@15                                             0435 01048000
*               END;                /* END ACCESS ERROR              */ 01049000
*             ELSE                  /* FOUND IOB ADDR                */ 01050000
*               DO;                                                0437 01051000
         B     @RC00432                                            0437 01052000
@RF00432 DS    0H                                                  0438 01053000
*                 IOBPRPTR=DATAADR; /* IOB PREFIX SECTION            */ 01054000
         SLR   IOBPRPTR,IOBPRPTR                                   0438 01055000
         ICM   IOBPRPTR,7,DATAADR(DATAPTR)                         0438 01056000
*                 IOBSTPTR=IOBPRPTR+CSAMPREF;/* STANDARD SECTION     */ 01057000
         LA    @14,16                                              0439 01058000
         LR    IOBSTPTR,IOBPRPTR                                   0439 01059000
         ALR   IOBSTPTR,@14                                        0439 01060000
*                 ADPLFMT2=IOBSTPTR;/* FILL IN PARMLIST              */ 01061000
         ST    IOBSTPTR,ADPLFMT2(,ABDPLPTR)                        0440 01062000
*                 CHAINIOB=ON;      /* INDICATE IOB BEING PROCESSED  */ 01063000
         OI    CHAINIOB(ABDPLPTR),B'00001000'                      0441 01064000
*                 OFFSET=CSAMPREF;  /* LENGTH OF PREFIX              */ 01065000
         ST    @14,OFFSET(,ABDPLPTR)                               0442 01066000
*                 CALL PRINT;       /* SPACE                         */ 01067000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0443 01068000
         BALR  @14,@15                                             0443 01069000
*                 CALL PRINT;       /* SPACE                         */ 01070000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0444 01071000
         BALR  @14,@15                                             0444 01072000
*                 CALL IECDAIOB;    /* IOB FORMATTER                 */ 01073000
         L     @15,@CV02687                                        0445 01074000
         BALR  @14,@15                                             0445 01075000
*                 IF RCREG=ZERO THEN/* FORMATTED IOB FINE            */ 01076000
         LTR   RCREG,RCREG                                         0446 01077000
         BNZ   @RF00446                                            0446 01078000
*                   DO;                                            0447 01079000
*                                                                  0447 01080000
*                     /***********************************************/ 01081000
*                     /*                                             */ 01082000
*                     /* IF IOB FORMATTED OK - FORMAT THE ICB CHAIN  */ 01083000
*                     /*                                             */ 01084000
*                     /***********************************************/ 01085000
*                                                                  0448 01086000
*                     VADR=ADDR(DCBIOBA);/* FIRST ICB ADDR           */ 01087000
         LA    VADR,DCBIOBA+52(,DCBPTR)                            0448 01088000
*                     CALL ACCESRTN;/* ACCESS IT                     */ 01089000
         BAL   @14,ACCESRTN                                        0449 01090000
*                     IF RCREG^=ZERO THEN/* ACCESS ERROR             */ 01091000
         LTR   RCREG,RCREG                                         0450 01092000
         BZ    @RF00450                                            0450 01093000
*                       DO;                                        0451 01094000
*                         BUFFER(1:LENGTH(ICBERR))=ICBERR;/* ERROR 0452 01095000
*                                      MESSAGE                       */ 01096000
         L     @14,ADPLBUF(,ABDPLPTR)                              0452 01097000
         MVC   BUFFER(64,@14),ICBERR                               0452 01098000
*                         CALL PRINT;/* PRINT IT                     */ 01099000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0453 01100000
         BALR  @14,@15                                             0453 01101000
*                       END;        /* END ACCESS ERROR              */ 01102000
*                     ELSE          /* FOUND ICB ADDR                */ 01103000
*                       DO;                                        0455 01104000
         B     @RC00450                                            0455 01105000
@RF00450 DS    0H                                                  0456 01106000
*                         IOBSTPTR=DATAADR;/* ICB                    */ 01107000
         SLR   IOBSTPTR,IOBSTPTR                                   0456 01108000
         ICM   IOBSTPTR,7,DATAADR(DATAPTR)                         0456 01109000
*                         COMPARE=ZERO;/* LOOP CONTROL               */ 01110000
         SLR   @14,@14                                             0457 01111000
         ST    @14,COMPARE                                         0457 01112000
*                         FIRSTIOB=IOBSTPTR;/* ADDR OF FIRST ICB     */ 01113000
         ST    IOBSTPTR,FIRSTIOB                                   0458 01114000
*                         CHAINIOB=OFF;/* INDICATE ICB BEING       0459 01115000
*                                      PROCESSED                     */ 01116000
         NI    CHAINIOB(ABDPLPTR),B'11110111'                      0459 01117000
*                         DO ARRAYCNT=1 TO LOOPMAX WHILE(ERRSW=OFF&     01118000
*                               COMPARE^=FIRSTIOB);                0460 01119000
*                                                                  0460 01120000
         LA    ARRAYCNT,1                                          0460 01121000
@DL00460 TM    ERRSW,B'10000000'                                   0460 01122000
         BNZ   @DC00460                                            0460 01123000
         CLC   COMPARE(4),FIRSTIOB                                 0460 01124000
         BE    @DC00460                                            0460 01125000
*                           /*****************************************/ 01126000
*                           /*                                       */ 01127000
*                           /* CHECK THAT THE ICB TO BE FORMATTED IS */ 01128000
*                           /* ADDRESSABLE BY THE USER. THIS WILL    */ 01129000
*                           /* PREVENT FORMATTING OF SECURE DATA BY  */ 01130000
*                           /* SNAP/ABEND.                   @ZM45048*/ 01131000
*                           /*                                       */ 01132000
*                           /*****************************************/ 01133000
*                                                                  0461 01134000
*                           IF ADPLSNPR=OFF THEN/* SNAP IS CALLER -     01135000
*                                      PROTECT DATA BY ACCESSING ICB    01136000
*                                      IN CALLERS KEY        @ZM45048*/ 01137000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0461 01138000
         BNZ   @RF00461                                            0461 01139000
*                             DO;   /*                       @ZM45048*/ 01140000
*                               RTYADR=ADDR(ICBRETRY);/* ADDRESS TO     01141000
*                                      RETRY AT TO PRINT A MESSAGE IF   01142000
*                                      THE DATA IS NOT ADDRESSABLE 0463 01143000
*                                                            @ZM45048*/ 01144000
         LA    @14,ICBRETRY                                        0463 01145000
         ST    @14,RTYADR                                          0463 01146000
*                               VADR=IOBSTPTR;/* ADDRESS TO BE     0464 01147000
*                                      VERIFIED              @ZM45048*/ 01148000
         LR    VADR,IOBSTPTR                                       0464 01149000
*                               ERREXP=ON;/* AN ACCESS ERROR IS    0465 01150000
*                                      EXPECTED              @ZM45048*/ 01151000
         OI    ERREXP,B'10000000'                                  0465 01152000
*                               RFY                                0466 01153000
*                                (KEYREG2) RSTD;/*           @ZM45048*/ 01154000
*                               DO; /* MODESET SAVEKEY(KEYSAVE)    0467 01155000
*                                      EXTKEY(TCB) WORKREG(KEYREG2)  */ 01156000
*                                 RESPECIFY                        0468 01157000
*                                  (GPR02F) RESTRICTED;            0468 01158000
*                                 KEYREG2=GPR02F;/* SAVE REGISTER 2  */ 01159000
*                                 IPK;/* SAVE CURRENT PSW KEY        */ 01160000
         IPK                                                       0470 01161000
*                                 KEYSAVE=GPR02F;                  0471 01162000
         STC   GPR02F,KEYSAVE                                      0471 01163000
*                                 GPR02F=KEYREG2;/* RESTORE REGISTER    01164000
*                                      2                             */ 01165000
*                                 KEYREG2=TCBPKF;/* GET KEY FROM TCB */ 01166000
         L     @14,ADPLTCB(,ABDPLPTR)                              0473 01167000
         SLR   KEYREG2,KEYREG2                                     0473 01168000
         IC    KEYREG2,TCBPKF(,@14)                                0473 01169000
*                                 SPKA(KEYREG2);/* SET PSW KEY       */ 01170000
         SPKA  0(KEYREG2)                                          0474 01171000
*                                 RESPECIFY                        0475 01172000
*                                  (GPR02F) UNRESTRICTED;          0475 01173000
*                               END;/* MODESET SAVEKEY(KEYSAVE)    0476 01174000
*                                      EXTKEY(TCB) WORKREG(KEYREG2)  */ 01175000
*                               ICBPTR=VADR;/* STORE ICB ADDRESS IN     01176000
*                                      REG2 SINCE REG0 CANNOT BE USED   01177000
*                                      AS A BASE             @ZM45048*/ 01178000
         LR    ICBPTR,VADR                                         0477 01179000
*                               RFY                                0478 01180000
*                                 ICB BASED(ICBPTR);/* REBASE THE  0478 01181000
*                                      MAPPING TO VERIFY THAT THE  0478 01182000
*                                      USER CAN ACCESS IT    @ZM45048*/ 01183000
*                                                                  0478 01184000
*                               /*************************************/ 01185000
*                               /*                                   */ 01186000
*                               /* THE CODE IS NOW IN THE USER KEY.  */ 01187000
*                               /* THE FOLLOWING STATEMENTS WILL     */ 01188000
*                               /* VERIFY IF THE USER IS ALLOWED TO  */ 01189000
*                               /* ACCESS THE ICB. IF THE            */ 01190000
*                               /* INSTRUCTIONS COMPLETE WITHOUT     */ 01191000
*                               /* ABEND, THE ICB WILL BE FORMATTED  */ 01192000
*                               /* JUST AS BY AMDPRDMP. IF THE       */ 01193000
*                               /* INSTRUCTIONS CAUSE AN ABEND (0C4),*/ 01194000
*                               /* THEN THE ESTAE WILL RETRY AT      */ 01195000
*                               /* ICBRETRY. THIS RETRY ADDRESS WILL */ 01196000
*                               /* PRINT AN ABEND MESSAGE AND INHIBIT*/ 01197000
*                               /* FURTHER ICB PROCESSING    @ZM45048*/ 01198000
*                               /*                                   */ 01199000
*                               /*************************************/ 01200000
*                                                                  0479 01201000
*                               VADR=TESTBYTE;/* VERIFY ACCESSIBILITY   01202000
*                                      OF THE FIRST BYTE OF THE ICB     01203000
*                                                            @ZM45048*/ 01204000
         SLR   VADR,VADR                                           0479 01205000
         IC    VADR,TESTBYTE(,ICBPTR)                              0479 01206000
*                               ICBPTR=ICBPTR+LENGTH(ICB)-1;/* REBASE   01207000
*                                      TESTBYTE ON LAST BYTE OF THE     01208000
*                                      ICB                   @ZM45048*/ 01209000
         SH    ICBPTR,@CH02958                                     0480 01210000
*                               VADR=TESTBYTE;/* VERIFY ACCESSIBILITY   01211000
*                                      OF THE LAST BYTE OF THE ICB 0481 01212000
*                                                            @ZM45048*/ 01213000
*                                                                  0481 01214000
         SLR   VADR,VADR                                           0481 01215000
         IC    VADR,TESTBYTE(,ICBPTR)                              0481 01216000
*                               /*************************************/ 01217000
*                               /*                                   */ 01218000
*                               /* THE KEY CAN BE RESET NOW THAT THE */ 01219000
*                               /* ICB HAS BEEN ACCESSED     @ZM45048*/ 01220000
*                               /*                                   */ 01221000
*                               /*************************************/ 01222000
*                                                                  0482 01223000
*                               DO; /* MODESET KEYADDR(KEYSAVE)    0482 01224000
*                                      WORKREG(KEYREG2)              */ 01225000
*                                 KEYREG2=KEYSAVE;/* GET KEY FORM  0483 01226000
*                                      SAVE LOCATION                 */ 01227000
         SLR   @14,@14                                             0483 01228000
         IC    @14,KEYSAVE                                         0483 01229000
         LR    KEYREG2,@14                                         0483 01230000
*                                 SPKA(KEYREG2);/* SET PSW KEY       */ 01231000
         SPKA  0(KEYREG2)                                          0484 01232000
*                               END;/* MODESET KEYADDR(KEYSAVE)    0485 01233000
*                                      WORKREG(KEYREG2)              */ 01234000
*                               ERREXP=OFF;/* AN ERROR IS NO LONGER     01235000
*                                      EXPECTED              @ZM45048*/ 01236000
         NI    ERREXP,B'01111111'                                  0486 01237000
*                               RFY                                0487 01238000
