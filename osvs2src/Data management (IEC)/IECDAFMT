         BO    @RT00298                                            0298 00616000
         TM    ADPLDMGT(ABDPLPTR),B'00100000'                      0298 00617000
         BNO   @RF00298                                            0298 00618000
         LTR   RCREG,RCREG                                         0298 00619000
         BNZ   @RF00298                                            0298 00620000
@RT00298 DS    0H                                                  0299 00621000
*     DO;                                                          0299 00622000
*       IF DCBPTR^=ZERO             /* THE DCB POINTER WAS NOT ZERO  */ 00623000
*         THEN                                                     0300 00624000
         LTR   DCBPTR,DCBPTR                                       0300 00625000
         BZ    @RF00300                                            0300 00626000
*         DO;                                                      0301 00627000
*           IF ADPLSNPR=ON THEN     /* SPECIAL HEADER UNDER AMDPRDMP */ 00628000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0302 00629000
         BNO   @RF00302                                            0302 00630000
*             DO;                                                  0303 00631000
*               CALL PRINT;         /* BLANK LINE                    */ 00632000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0304 00633000
         BALR  @14,@15                                             0304 00634000
*               CALL PRINT;         /* BLANK LINE                    */ 00635000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0305 00636000
         BALR  @14,@15                                             0305 00637000
*               BUFFER(1:LENGTH(DEBHDR))=DEBHDR;/* HEADER LINE       */ 00638000
         L     @14,ADPLBUF(,ABDPLPTR)                              0306 00639000
         MVC   BUFFER(20,@14),DEBHDR                               0306 00640000
*               PACKAREA=DEBPTR;    /* MOVE INTO CHAR AREA           */ 00641000
         ST    DEBPTR,PACKAREA                                     0307 00642000
*               GARBAGE='04'X;      /* GARBAGE BYTE FOR UNPK         */ 00643000
         MVI   GARBAGE,X'04'                                       0308 00644000
*               UNPK(ADDRESS,UNPACK);/* PUT IN BUFFER LINE           */ 00645000
         UNPK  ADDRESS(9,@14),UNPACK(5)                            0309 00646000
*               TR(ADDRESS,TRANS);  /* MAKE PRINTABLE                */ 00647000
         TR    ADDRESS(9,@14),TRANS                                0310 00648000
*               CALL PRINT;         /* PRINT HDR                     */ 00649000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0311 00650000
