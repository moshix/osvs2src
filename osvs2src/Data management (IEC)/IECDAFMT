*ACCESRTN:                                                         0682 01829000
*   PROC OPTIONS(SAVE(14));                                        0682 01830000
*                                                                  0682 01831000
ACCESRTN ST    @14,@SA00005                                        0682 01832000
*/********************************************************************/ 01833000
*/* FUNCTION-                                                      0683 01834000
*       ROUTINE WHICH CALLS THE ACCESS SERVICE ROUTINE AND MOVES THE    01835000
*     VALUE RETURNED IN R0 INTO DATAPTR.                           0683 01836000
*                                                                  0683 01837000
*                                                                  0683 01838000
*   INPUT -                                                        0683 01839000
*         VADR(REGISTER 0) CONTAINS THE VIRTUAL ADDRESS OF THE FIELD    01840000
*         TO BE ACCESSED.                                          0683 01841000
*                                                                  0683 01842000
*    CALLS-                                                        0683 01843000
*         ACCESS SERVICE ROUTINE PASSED IN PARMLIST                0683 01844000
*                                                                  0683 01845000
*    CALLED BY -                                                   0683 01846000
*         ALL ROUTINES IN THIS MODULE                              0683 01847000
*                                                                  0683 01848000
*    RETURNS TO -                                                  0683 01849000
*         CALLER WITH THE RETURN CODE SET BY THE ACCESS            0683 01850000
*         SERVICE ROUTINE.                                         0683 01851000
*                                                                  0683 01852000
*                                                                    */ 01853000
*/********************************************************************/ 01854000
*                                                                  0683 01855000
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS THE CALLER           */ 01856000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0683 01857000
         BNZ   @RF00683                                            0683 01858000
*     DO;                                                          0684 01859000
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN CASE     01860000
*                                      OF EXPECTED ERROR-SIMULATES 0685 01861000
*                                      PRINT DUMP                    */ 01862000
         LA    @14,RETRY                                           0685 01863000
         ST    @14,RTYADR                                          0685 01864000
*       DATAPTR=VADR;               /* PREPARE FOR A LOAD FROM THE 0686 01865000
*                                      DUMP INTO AN INTERNAL BUFFER  */ 01866000
         LR    DATAPTR,VADR                                        0686 01867000
*       RCREG=ZERO;                 /* SUCCESS UNLESS OTHERWISE    0687 01868000
*                                      CHANGED                       */ 01869000
         SLR   RCREG,RCREG                                         0687 01870000
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */ 01871000
         OI    ERREXP,B'10000000'                                  0688 01872000
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO THE 0689 01873000
*                                      BUFFER                        */ 01874000
*                                                                  0689 01875000
         MVC   VERIFY(8),DATADWRD(DATAPTR)                         0689 01876000
*       /*************************************************************/ 01877000
*       /*                                                           */ 01878000
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/ 01879000
*       /* THIS AREA OR IF IT IS PROTECTED. IF AN ABEND OCCURS THE   */ 01880000
*       /* ESTAE WILL GET CONTROL AND RETRY AT THE POINT WHICH SETS A*/ 01881000
*       /* BAD RETURN CODE AND CONTINUES IN THE MAIN LINE. THIS THEN */ 01882000
*       /* SIMULATES THE PRINT DUMP ACCESS ROUTINE. IF THE AREA ASKED*/ 01883000
*       /* FOR IS IN THE DUMP. A DOUBLE WORD IS MOVED INTO AN        */ 01884000
*       /* INTERNAL BUFFER TO PREVENT AN UNEXPECTED ERROR WHEN       */ 01885000
*       /* REFERENCING THE AREA AGAIN                                */ 01886000
*       /*                                                           */ 01887000
*       /*************************************************************/ 01888000
*                                                                  0690 01889000
*       ERREXP=OFF;                 /* ERROR NO LONGER EXPECTED      */ 01890000
         NI    ERREXP,B'01111111'                                  0690 01891000
*       DATAPTR=ADDR(VERIFY);       /* BASE DATAWORD ON THE BUFFER   */ 01892000
         LA    DATAPTR,VERIFY                                      0691 01893000
*     END;                          /* END SNAP WAS CALLER           */ 01894000
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE */ 01895000
*     DO;                                                          0693 01896000
         B     @RC00683                                            0693 01897000
@RF00683 DS    0H                                                  0694 01898000
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED    01899000
*                                      BY CALLER                     */ 01900000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0694 01901000
         BALR  @14,@15                                             0694 01902000
*       IF RCREG=ZERO THEN                                         0695 01903000
         LTR   RCREG,RCREG                                         0695 01904000
         BNZ   @RF00695                                            0695 01905000
*         DATAPTR=VADR;             /* SAVE PTR RETURNED BY ACCESS 0696 01906000
*                                      SERVICE ROUTINE               */ 01907000
         LR    DATAPTR,VADR                                        0696 01908000
*       ELSE                        /* BAD RETURN CODE               */ 01909000
*RETRY:                             /* RETRY ADDR FOR THE ESTAE      */ 01910000
*         RCREG=4;                  /* SET BAD RETURN FOR THE      0697 01911000
*                                      MAINLINE                      */ 01912000
         B     @RC00695                                            0697 01913000
@RF00695 DS    0H                                                  0697 01914000
RETRY    LA    RCREG,4                                             0697 01915000
*     END;                          /* END CALLER NOT SNAP           */ 01916000
@RC00695 DS    0H                                                  0699 01917000
*   RCCODE=RCREG;                   /* SAVE RETURN CODE              */ 01918000
@RC00683 ST    RCREG,RCCODE                                        0699 01919000
*   RETURN CODE(RCCODE);                                           0700 01920000
         L     @14,RCCODE                                          0700 01921000
         LR    @15,@14                                             0700 01922000
@EL00005 DS    0H                                                  0700 01923000
@EF00005 DS    0H                                                  0700 01924000
@ER00005 L     @14,@SA00005                                        0700 01925000
         BR    @14                                                 0700 01926000
*   END ACCESRTN;                                                  0701 01927000
*ESTAERTN:                                                         0702 01928000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0702 01929000
*                                                                  0702 01930000
ESTAERTN DS    0H                                                  0703 01931000
*/********************************************************************/ 01932000
*/* FUNCTION-                                                      0703 01933000
*       ROUTINE TO DETERMINE IF THE ERROR WAS EXPECTED AND CONTINUE     01934000
*     PROCESSING OR TO CLEAN UP AND EXIT. IF THE ERROR WAS EXPECTED     01935000
*     THEN THE CONTROL IS RETURNED TO THE ADDRESS PASSED IN THE ESTAE   01936000
*     PARM LIST. THIS RETURN ACTS LIKE THE BAD RETURN FROM THE ACCESS   01937000
*     SERVICE ROUTINE. IF THE ERROR WAS NOT EXPECTED, REQUIRED CLEAN UP 01938000
*     OF RESOURCES IS DONE. A MESSAGE IS FILLED INTO THE BUFFER AND     01939000
*     CONTROL IS RETURNED AT THE END OF THE FORMATTER.             0703 01940000
*                                                                  0703 01941000
*    CALLS-                                                        0703 01942000
*         MAKES NO OUTSIDE CALLS                                   0703 01943000
*                                                                  0703 01944000
*    CALLED BY -                                                   0703 01945000
*         RTM IN AN ABEND SITUATION                                0703 01946000
*                                                                  0703 01947000
*    RETURNS TO -                                                  0703 01948000
*         CALLER                                                   0703 01949000
*                                                                    */ 01950000
*/********************************************************************/ 01951000
*                                                                  0703 01952000
*   /*****************************************************************/ 01953000
*   /*                                                               */ 01954000
*   /* NO RESTORE REGS SPECIFIED                                     */ 01955000
*   /*                                                               */ 01956000
*   /*****************************************************************/ 01957000
*                                                                  0703 01958000
*   DO;                             /* ENTESTAE PARMREG(DREG)        */ 01959000
*/*  ENTRY CODE FOR ESTAE ROUTINE.                                   */ 01960000
*/*  IF RTCA ALLOCATED THEN RESTORE CODE AND                         */ 01961000
*/*  DATA REGS FROM START OF USERS PARM LIST,                        */ 01962000
*/*  AND ESTABLISH ADDR TO ESTAE SAVE AREA                           */ 01963000
*/*  IF REQUIRED.                                                    */ 01964000
*/*  IF NO RTCA IS AVAILABLE THEN JUST RETURN                        */ 01965000
*/*  TO RTM TO CONTINUE TERMINATION                                  */ 01966000
*                                                                  0704 01967000
*     GEN(USING *,GPR15P);                                         0704 01968000
         USING *,GPR15P                                                 01969000
*     GEN(CH    GPR00F,*+14);       /* CHECK IF RTCA ALLOC FAILED    */ 01970000
         CH    GPR00F,*+14                                              01971000
*     GEN(BNE   *+12);              /* NO,SAVE RTM REGS              */ 01972000
         BNE   *+12                                                     01973000
*     GEN(DROP  GPR15P);            /* DROP LOCAL ADDR               */ 01974000
         DROP  GPR15P                                                   01975000
*     GEN(LA    GPR15P,0);          /* YES, SET CONT TERM RC         */ 01976000
         LA    GPR15P,0                                                 01977000
*     GEN(BR    GPR14P);            /* RETURN TO RTM                 */ 01978000
         BR    GPR14P                                                   01979000
*     GEN(DC    H'12');             /* NO RTCA AVAIL RC              */ 01980000
         DC    H'12'                                                    01981000
*     GEN(STM   GPR14P,GPR12F,12(GPR13P));/* GOOD R13, SAVE RTM'S  0711 01982000
*                                      REGS                          */ 01983000
         STM   GPR14P,GPR12F,12(GPR13P)                                 01984000
*     GEN(L     DREG,0(GPR01P));    /* PTR TO USER PARM LIST         */ 01985000
         L     DREG,0(GPR01P)                                           01986000
*   END;                            /* ENTESTAE PARMREG(DREG)        */ 01987000
*                                                                  0713 01988000
*   /*****************************************************************/ 01989000
*   /*                                                               */ 01990000
*   /* THIS CODE SET DREG TO BE THE POINTER TO ALL THE AUTOMATIC DATA*/ 01991000
*   /* FOR THIS MODULE                                               */ 01992000
*   /*                                                               */ 01993000
*   /*****************************************************************/ 01994000
*                                                                  0714 01995000
*   LM(GPR02P,GPR12F,REG2TO12);     /* RESTORE THE REGS SAVED FROM 0714 01996000
*                                      THE MAINLINE                  */ 01997000
         LM    GPR02P,GPR12F,REG2TO12                              0714 01998000
*   SDWAPTR=GPR01P;                 /* SDWA ADDR PASSED TO THIS    0715 01999000
*                                      ROUTINE                       */ 02000000
*                                                                  0715 02001000
         ST    GPR01P,SDWAPTR                                      0715 02002000
*   /*****************************************************************/ 02003000
*   /*                                                               */ 02004000
*   /* TO PREVENT MORE THAN ONE DUMP BEING TAKEN FOR AN X37 TYPE     */ 02005000
*   /* ABEND, PERCOLATE TO THE CALLERS RECOVERY. THIS WILL PREVENT   */ 02006000
*   /* THIS ESTAE FROM TAKING A DUMP AND LET THE CALLERS RECOVERY    */ 02007000
*   /* TAKE IT.                                              @ZM43805*/ 02008000
*   /*                                                               */ 02009000
*   /*****************************************************************/ 02010000
*                                                                  0716 02011000
*   IF ABNDTYPE^='37'X THEN         /* PERCOLATE FOR ALL X37 TYPE  0716 02012000
*                                      ABEND CODES           @ZM43805*/ 02013000
         L     @14,SDWAPTR                                         0716 02014000
         MVC   @TS00001(2),ABNDTYPE(@14)                           0716 02015000
         NC    @TS00001(2),@CB02969                                0716 02016000
         CLC   @TS00001(2),@CB02971                                0716 02017000
         BE    @RF00716                                            0716 02018000
*     DO;                           /* NOT AN X37 ABEND      @ZM43805*/ 02019000
*       IF ABND^='0C4'X THEN        /* ONLY EXPECTED ERROR CODE      */ 02020000
         L     @14,SDWAPTR                                         0718 02021000
         MVC   @TS00001(2),ABND(@14)                               0718 02022000
         NC    @TS00001(2),@CB02973                                0718 02023000
         CLC   @TS00001(2),@CB02975                                0718 02024000
         BE    @RF00718                                            0718 02025000
*         ERREXP=OFF;               /* EVEN IF AN ERROR WAS EXPECTED,   02026000
*                                      THIS ABEND CODE WAS NOT THE 0719 02027000
*                                      EXPECTED ONE                  */ 02028000
         NI    ERREXP,B'01111111'                                  0719 02029000
*       ELSE                                                       0720 02030000
*         ;                         /* SEE IF THE ERROR WAS EXPECTED */ 02031000
@RF00718 DS    0H                                                  0721 02032000
*       IF ERREXP=ON THEN           /* AN ERROR WAS EXPECTED BY THE     02033000
*                                      CODE                          */ 02034000
         TM    ERREXP,B'10000000'                                  0721 02035000
         BNO   @RF00721                                            0721 02036000
*         DO;                                                      0722 02037000
*           ERREXP=OFF;             /* TURN OFF EXPECTED ERROR       */ 02038000
*           LOCKHELD=OFF;           /* RTM RELEASED LOCAL LOCK       */ 02039000
         NI    ERREXP,B'00111111'                                  0724 02040000
*           SDWASRSV=LCALREGS;      /* REGS AT ABEND, USED AT RETRY  */ 02041000
*                                                                  0725 02042000
         L     @14,SDWAPTR                                         0725 02043000
         MVC   SDWASRSV(64,@14),LCALREGS                           0725 02044000
*           /*********************************************************/ 02045000
*           /*                                                       */ 02046000
*           /* RETRY AT THE ACCESS RTN WHICH TOOK THE ERROR,AT THAT  */ 02047000
*           /* POINT THE RETURN CODE WILL BE SET TO 4 TO SIMULATE THE*/ 02048000
*           /* PRINT DUMP ACCESS SERVICE ROUTINE.                    */ 02049000
*           /*                                                       */ 02050000
*           /*********************************************************/ 02051000
*                                                                  0726 02052000
*           DO;                     /* SETRP REGS(14,12)DUMP(NO)RC(4)   02053000
*                                      RETADDR(RETRYPT)RETREGS(YES)FR   02054000
*                                      ESDWA(YES)                    */ 02055000
*             RESPECIFY                                            0727 02056000
*              (GPR00P,                                            0727 02057000
*               GPR01P,                                            0727 02058000
*               GPR14P,                                            0727 02059000
*               GPR15P) RSTD;                                      0727 02060000
*             GPR01P->SDWARCDE=4;   /* STORE RC INTO SDWA            */ 02061000
         MVI   SDWARCDE(GPR01P),X'04'                              0728 02062000
*             GPR01P->SDWARTYA=ADDR(RETRYPT);/* SAVE RETRY ADDRESS   */ 02063000
         MVC   SDWARTYA(4,GPR01P),RTYADR                           0729 02064000
*             GPR01P->SDWAFREE='1'B;/* SET FLAG TO FREE SDWA         */ 02065000
         OI    SDWAFREE(GPR01P),B'00000100'                        0730 02066000
*             GPR01P->SDWAREQ='0'B; /* TURN OFF DUMP INDICATOR       */ 02067000
         NI    SDWAREQ(GPR01P),B'01111111'                         0731 02068000
*             GPR01P->SDWAUPRG='1'B;/* TURN ON RETREGS INDICATOR     */ 02069000
         OI    SDWAUPRG(GPR01P),B'00001000'                        0732 02070000
*             GEN(RETURN (14,12));                                 0733 02071000
         RETURN (14,12)                                                 02072000
*             RESPECIFY                                            0734 02073000
*              (GPR00P,                                            0734 02074000
*               GPR01P,                                            0734 02075000
*               GPR14P,                                            0734 02076000
*               GPR15P) UNRSTD;                                    0734 02077000
*           END;                                                   0735 02078000
*         END;                      /* END AN ERROR WAS EXPECTED     */ 02079000
*       ELSE                        /* AN UNEXPECTED ERROR - RETRY AT   02080000
*                                      RETURN TO CALLER              */ 02081000
*         DO;                                                      0737 02082000
         B     @RC00721                                            0737 02083000
@RF00721 DS    0H                                                  0738 02084000
*           IF UNEXP2=OFF THEN      /* THE FIRST UNEXPECTED ERROR    */ 02085000
         TM    UNEXP2,B'00100000'                                  0738 02086000
         BNZ   @RF00738                                            0738 02087000
*             DO;                                                  0739 02088000
*               UNEXP2=ON;          /* RETRY ONLY ONCE - PREVENT LOOP*/ 02089000
*               LOCKHELD=OFF;       /* RTM RELEASED LOCAL LOCK       */ 02090000
         OI    UNEXP2,B'00100000'                                  0741 02091000
         NI    LOCKHELD,B'10111111'                                0741 02092000
*               RTYADR=ADDR(TROUBLE);/* RETRY POINT IN MAIN LINE FOR    02093000
*                                      UNEXPECTED ERRORS             */ 02094000
         LA    @14,TROUBLE                                         0742 02095000
         ST    @14,RTYADR                                          0742 02096000
*               SDWASRSV=RTYREGS;   /* SET RETRY REGS FOR MAINLINE 0743 02097000
*                                      RETRY                         */ 02098000
         L     @10,SDWAPTR                                         0743 02099000
         MVC   SDWASRSV(64,@10),RTYREGS                            0743 02100000
*               ABDPLPTR=RTYREG1;   /* RESTORE PTR TO PRINT DUMP   0744 02101000
*                                      PARMLIST-REG1                 */ 02102000
         L     ABDPLPTR,RTYREG1                                    0744 02103000
*               BUFFER=ENDMSG;      /* ABEND MESSAGE                 */ 02104000
         L     @04,ADPLBUF(,ABDPLPTR)                              0745 02105000
         MVI   BUFFER+53(@04),C' '                                 0745 02106000
         MVC   BUFFER+54(66,@04),BUFFER+53(@04)                    0745 02107000
         MVC   BUFFER(53,@04),ENDMSG                               0745 02108000
*               PACKAREA(3:4)=ABNDCMPC;/* MOVE COMP CODE TO CHAR AREA*/ 02109000
         MVC   PACKAREA+2(2),ABNDCMPC(@10)                         0746 02110000
*               GARBAGE='04'X;      /* GARBAGE BYTE FOR THE UNPK     */ 02111000
         MVI   GARBAGE,X'04'                                       0747 02112000
*               UNPK(BUFFER(54:58),UNPACK(3:5));/* UNPACK INTO THE 0748 02113000
*                                      BUFFER                        */ 02114000
         UNPK  BUFFER+53(5,@04),UNPACK+2(3)                        0748 02115000
*               TR(BUFFER(54:58),TRANS);/* TRANSLATE TO PRINTABLE HEX*/ 02116000
         TR    BUFFER+53(5,@04),TRANS                              0749 02117000
*               BUFFER(57:57)=' ';  /* BLANK OUT SECOND HALF BYTE    */ 02118000
         MVI   BUFFER+56(@04),C' '                                 0750 02119000
*               GPR01P=SDWAPTR;     /* RESTORE REGISTER 1 FOR SETRP  */ 02120000
         LR    GPR01P,@10                                          0751 02121000
*               DO;                 /* SETRP REGS(14,12)DUMP(YES)RC(4   02122000
*                                      )RETADDR(RETRYPT)RETREGS(YES)F   02123000
*                                      RESDWA(YES)                   */ 02124000
*                 RESPECIFY                                        0753 02125000
*                  (GPR00P,                                        0753 02126000
*                   GPR01P,                                        0753 02127000
*                   GPR14P,                                        0753 02128000
*                   GPR15P) RSTD;                                  0753 02129000
*                 GPR01P->SDWARCDE=4;/* STORE RC INTO SDWA           */ 02130000
         MVI   SDWARCDE(GPR01P),X'04'                              0754 02131000
*                 GPR01P->SDWARTYA=ADDR(RETRYPT);/* SAVE RETRY     0755 02132000
*                                      ADDRESS                       */ 02133000
         L     @10,RTYADR                                          0755 02134000
         ST    @10,SDWARTYA(,GPR01P)                               0755 02135000
*                 GPR01P->SDWAFREE='1'B;/* SET FLAG TO FREE SDWA     */ 02136000
         OI    SDWAFREE(GPR01P),B'00000100'                        0756 02137000
*                 GPR01P->SDWAREQ='1'B;/* TURN ON DUMP INDICATOR     */ 02138000
         OI    SDWAREQ(GPR01P),B'10000000'                         0757 02139000
*                 GPR01P->SDWAUPRG='1'B;/* TURN ON RETREGS INDICATOR */ 02140000
         OI    SDWAUPRG(GPR01P),B'00001000'                        0758 02141000
*                 GEN(RETURN (14,12));                             0759 02142000
         RETURN (14,12)                                                 02143000
*                 RESPECIFY                                        0760 02144000
*                  (GPR00P,                                        0760 02145000
*                   GPR01P,                                        0760 02146000
*                   GPR14P,                                        0760 02147000
*                   GPR15P) UNRSTD;                                0760 02148000
*               END;                                               0761 02149000
*             END;                  /* END FIRST UNEXPECTED ERROR    */ 02150000
*           ELSE                    /* PERCOLATE FOR SECOND        0763 02151000
*                                      UNEXPECTED ERROR              */ 02152000
*             DO;                   /* SETRP                       0763 02153000
*                                      REGS(14,12)DUMP(YES)RC(0)     */ 02154000
         B     @RC00738                                            0763 02155000
@RF00738 DS    0H                                                  0764 02156000
*               RESPECIFY                                          0764 02157000
*                (GPR00P,                                          0764 02158000
*                 GPR01P,                                          0764 02159000
*                 GPR14P,                                          0764 02160000
*                 GPR15P) RSTD;                                    0764 02161000
*               GPR01P->SDWARCDE=0; /* SAVE RC VALUE                 */ 02162000
         MVI   SDWARCDE(GPR01P),X'00'                              0765 02163000
*               GPR01P->SDWAREQ='1'B;/* TURN ON DUMP INDICATOR       */ 02164000
         OI    SDWAREQ(GPR01P),B'10000000'                         0766 02165000
*               GEN(RETURN (14,12));                               0767 02166000
         RETURN (14,12)                                                 02167000
*               RESPECIFY                                          0768 02168000
*                (GPR00P,                                          0768 02169000
*                 GPR01P,                                          0768 02170000
*                 GPR14P,                                          0768 02171000
*                 GPR15P) UNRSTD;                                  0768 02172000
*             END;                  /*                       @ZM43805*/ 02173000
*         END;                      /* END UNEXPECTED ERROR          */ 02174000
*     END;                          /* END NOT AN X37 TYPE ABEND   0771 02175000
*                                                            @ZM43805*/ 02176000
*   ELSE                            /* THIS IS AN X37 ABEND -      0772 02177000
*                                      PERCOLATE             @ZM43805*/ 02178000
*     DO;                           /* SETRP                       0772 02179000
*                                      REGS(14,12)DUMP(YES)RC(0)     */ 02180000
         B     @RC00716                                            0772 02181000
@RF00716 DS    0H                                                  0773 02182000
*       RESPECIFY                                                  0773 02183000
*        (GPR00P,                                                  0773 02184000
*         GPR01P,                                                  0773 02185000
*         GPR14P,                                                  0773 02186000
*         GPR15P) RSTD;                                            0773 02187000
*       GPR01P->SDWARCDE=0;         /* SAVE RC VALUE                 */ 02188000
         MVI   SDWARCDE(GPR01P),X'00'                              0774 02189000
*       GPR01P->SDWAREQ='1'B;       /* TURN ON DUMP INDICATOR        */ 02190000
         OI    SDWAREQ(GPR01P),B'10000000'                         0775 02191000
*       GEN(RETURN (14,12));                                       0776 02192000
         RETURN (14,12)                                                 02193000
*       RESPECIFY                                                  0777 02194000
*        (GPR00P,                                                  0777 02195000
*         GPR01P,                                                  0777 02196000
*         GPR14P,                                                  0777 02197000
*         GPR15P) UNRSTD;                                          0777 02198000
*     END;                          /*                       @ZM43805*/ 02199000
*   END ESTAERTN;                                                  0779 02200000
@EL00006 DS    0H                                                  0779 02201000
@EF00006 DS    0H                                                  0779 02202000
@ER00006 BR    @14                                                 0779 02203000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 02204000
*     GPR00F FIXED(31) REG(0),                                     0780 02205000
*     GPR02F FIXED(31) REG(2),                                     0780 02206000
*     GPR12F FIXED(31) REG(12),                                    0780 02207000
*     GPR00P PTR(31) REG(0),                                       0780 02208000
*     GPR01P PTR(31) REG(1),                                       0780 02209000
*     GPR02P PTR(31) REG(2),                                       0780 02210000
*     GPR11P PTR(31) REG(11),                                      0780 02211000
*     GPR12P PTR(31) REG(12),                                      0780 02212000
*     GPR13P PTR(31) REG(13),                                      0780 02213000
*     GPR14P PTR(31) REG(14),                                      0780 02214000
*     GPR15P PTR(31) REG(15);                                      0780 02215000
*   DECLARE                         /* COMMON VARIABLES              */ 02216000
*     I256C CHAR(256) BASED,                                       0781 02217000
*     I031F FIXED(31) BASED,                                       0781 02218000
*     I031P PTR(31) BASED,                                         0781 02219000
*     I015F FIXED(15) BASED,                                       0781 02220000
*     I015P PTR(15) BASED,                                         0781 02221000
*     I008P PTR(8) BASED,                                          0781 02222000
*     I001C CHAR(1) BASED;                                         0781 02223000
*   END IECDAFMT                                                   0782 02224000
*                                                                  0782 02225000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02226000
*/*%INCLUDE SYSLIB  (IHAABDPL)                                       */ 02227000
