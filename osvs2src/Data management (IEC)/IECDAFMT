*               ELSE                                               0331 00704000
*                 ;                 /* IOBS NOT WANTED               */ 00705000
@RF00329 DS    0H                                                  0332 00706000
*             END;                  /* END DCB ADDRESS OK            */ 00707000
*         END;                      /* END DCB ADDRESS WAS NOT ZERO  */ 00708000
*       ELSE                                                       0334 00709000
*         ;                         /* DCB ADDRESS WAS ZERO          */ 00710000
@RF00300 DS    0H                                                  0335 00711000
*     END;                          /* END CONTROL BLOCKS WANTED     */ 00712000
*   ELSE                                                           0336 00713000
*     ;                             /* CONTROL BLOCKS NOT WANTED     */ 00714000
@RF00298 DS    0H                                                  0337 00715000
*   END FORMATIT;                                                  0337 00716000
@EL00002 DS    0H                                                  0337 00717000
@EF00002 DS    0H                                                  0337 00718000
