*                                 END;/* END FOUND ICB ADDR          */ 01296000
*                             END;  /* END FORMATTED ICB FINE        */ 01297000
*                           ELSE                                   0511 01298000
*                             ERRSW=ON;/* ERROR FORMATTING ICB       */ 01299000
         B     @RC00496                                            0511 01300000
