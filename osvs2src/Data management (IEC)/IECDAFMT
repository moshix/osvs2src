*                                   ERRSW=ON;/* END LOOP             */ 01282000
         OI    ERRSW,B'10000000'                                   0504 01283000
*                                 END;/* END ACCESS ERROR            */ 01284000
*                               ELSE/* FOUND ICB ADDR                */ 01285000
