         BP    @DL00242                                            0252 00444000
@DC00242 DS    0H                                                  0253 00445000
*                                    IF DEBPTR=ZERO THEN/* VALID DEB    00446000
*                                      WAS NOT FOUND                 */ 00447000
         LTR   DEBPTR,DEBPTR                                       0253 00448000
         BNZ   @RF00253                                            0253 00449000
*                                     CALL RELLOCK;/* RELEASE LOCK IF   00450000
*                                      HELD                          */ 00451000
         BAL   @14,RELLOCK                                         0254 00452000
*                                    ELSE/* VALID DEB FOUND          */ 00453000
*                                     DO;                          0255 00454000
*                                                                  0255 00455000
         B     @RC00253                                            0255 00456000
@RF00253 DS    0H                                                  0256 00457000
*                                      /******************************/ 00458000
*                                      /*                            */ 00459000
*                                      /* A VALID DEB WAS FOUND TO BE*/ 00460000
*                                      /* PROCESSED                  */ 00461000
*                                      /*                            */ 00462000
*                                      /******************************/ 00463000
*                                                                  0256 00464000
*                                      VADR=ADDR(DEBDCBAD);/* PTR TO    00465000
*                                      ASSOCIATED DCB                */ 00466000
         LA    VADR,DEBDCBAD(,DEBPTR)                              0256 00467000
*                                      CALL ACCESRTN;/* ACCESS IT    */ 00468000
         BAL   @14,ACCESRTN                                        0257 00469000
*                                      IF RCREG^=ZERO THEN/* ACCESS     00470000
*                                      ERROR                         */ 00471000
