         BALR  @14,@15                                             0273 00519000
*       GEN(ESTAE 0);               /* DROP THE ESTAE                */ 00520000
         ESTAE 0                                                        00521000
*     END;                          /* END ESTAE ESTABLISHED FINE    */ 00522000
*   ELSE                                                           0276 00523000
*     ;                             /* ESATE COULD NOT BE ESTABLISHED*/ 00524000
@RF00180 DS    0H                                                  0277 00525000
*   IF ADPLSNPR=OFF THEN            /* SNAP CALLER           @ZA26256*/ 00526000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0277 00527000
