         MVC   NUMLCB(1),DATABYTE(DATAPTR)                         0622 01620000
*                         VADR=ADDR(DCBEIOBX);/* SIZE OF THE LCB     */ 01621000
         LA    VADR,DCBEIOBX+28(,DCBPTR)                           0623 01622000
*                         CALL ACCESRTN;/* ACCESS IT                 */ 01623000
         BAL   @14,ACCESRTN                                        0624 01624000
*                         IF RCREG^=ZERO THEN/* ACCESS ERROR         */ 01625000
         LTR   RCREG,RCREG                                         0625 01626000
         BZ    @RF00625                                            0625 01627000
*                           DO;                                    0626 01628000
*                             BUFFER(1:LENGTH(LCBERR))=LCBERR;/*   0627 01629000
*                                      ERROR MSG                     */ 01630000
         L     @14,ADPLBUF(,ABDPLPTR)                              0627 01631000
         MVC   BUFFER(64,@14),LCBERR                               0627 01632000
*                             CALL PRINT;/* PRINT IT                 */ 01633000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0628 01634000
         BALR  @14,@15                                             0628 01635000
*                           END;    /* END ACCESS ERROR              */ 01636000
*                         ELSE      /* FOUND LCB SIZE                */ 01637000
*                           DO;                                    0630 01638000
         B     @RC00625                                            0630 01639000
@RF00625 DS    0H                                                  0631 01640000
*                             SIZE=DATABYTE;/* SAVE LCB SIZE         */ 01641000
         MVC   SIZE(1),DATABYTE(DATAPTR)                           0631 01642000
*                             VADR=ADDR(DCBIOBAD);/* ADDR OF IOB BASE   01643000
*                                                            @ZM45056*/ 01644000
         LA    VADR,DCBIOBAD+16(,DCBPTR)                           0632 01645000
*                             CALL ACCESRTN;/* ACCESS IT             */ 01646000
         BAL   @14,ACCESRTN                                        0633 01647000
*                             IF RCREG^=ZERO THEN/* ACCESS ERROR     */ 01648000
         LTR   RCREG,RCREG                                         0634 01649000
         BZ    @RF00634                                            0634 01650000
*                               DO;                                0635 01651000
*                                 BUFFER(1:LENGTH(LCBERR))=LCBERR;/*    01652000
*                                      ERROR MESSAGE                 */ 01653000
         L     @14,ADPLBUF(,ABDPLPTR)                              0636 01654000
         MVC   BUFFER(64,@14),LCBERR                               0636 01655000
*                                 CALL PRINT;/* PRINT IT             */ 01656000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0637 01657000
         BALR  @14,@15                                             0637 01658000
*                               END;/* END ACCESS ERROR              */ 01659000
*                             ELSE  /* FOUND IOB BASE ADDR   @ZM45056*/ 01660000
*                               DO;                                0639 01661000
         B     @RC00634                                            0639 01662000
@RF00634 DS    0H                                                  0640 01663000
*                                 IOBSTPTR=DATAADR+SIZE-OFSETIOB;/*     01664000
*                                      FIND THE POINTER TO THE FIRST    01665000
*                                      LCB BY ADDING THE LCB SIZE TO    01666000
*                                      THE IOB BASE ADDR. THIS NOW 0640 01667000
*                                      POINTS TO THE FIRST IOB, WHICH   01668000
*                                      IS EMBEDDED AT X'20' INTO THE    01669000
*                                      FIRST LCB. BACK UP THE 32   0640 01670000
*                                      BYTES TO POINT TO THE FIRST 0640 01671000
*                                      LCB                   @ZM45056*/ 01672000
         SLR   IOBSTPTR,IOBSTPTR                                   0640 01673000
         ICM   IOBSTPTR,7,DATAADR(DATAPTR)                         0640 01674000
         SLR   @14,@14                                             0640 01675000
         IC    @14,SIZE                                            0640 01676000
         ALR   IOBSTPTR,@14                                        0640 01677000
         SH    IOBSTPTR,@CH00133                                   0640 01678000
*                                 DO ARRAYCNT=1 TO NUMLCB WHILE(ERRSW=  01679000
*                                       OFF);                      0641 01680000
         LA    ARRAYCNT,1                                          0641 01681000
