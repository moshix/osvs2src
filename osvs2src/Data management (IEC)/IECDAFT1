*FMTRTN:                                                           0842 02436000
*   PROC OPTIONS(SAVE(14));                                        0842 02437000
*                                                                  0842 02438000
FMTRTN   ST    @14,@SA00004                                        0842 02439000
*/****************************************************************** */ 02440000
*/* FUNCTION                                                       0843 02441000
*       ROUTINE TO SET UP CALLING OF THE FORMAT SERVICE ROUTINE PASSED  02442000
*     IN THE PARMLIST AND CHECKS RETURN CODE.  IF A RETURN CODE IS BAD  02443000
*     IT TAKES THE MESSAGE WHOSE ADDRESS IS IN ADPLCOM1. AND PUTS IT    02444000
*     IN THE BUFFER AND PRINTS IT. IT SETS THE ERRSW TO INDICATE BAD    02445000
*     RETURN CODE AND RETURNS. IF THE RETURN CODE WAS GOOD, IT PRINTS   02446000
*     THE FORMATTED LINE.                                          0843 02447000
*                                                                  0843 02448000
*   CALLS-                                                         0843 02449000
*       CALLS FORMAT AND PRINT                                     0843 02450000
*                                                                  0843 02451000
*   INPUT -                                                        0843 02452000
*     LINEPTR(REGISTER 0) HAS THE ADDRESS OF THE FORMAT PATTERN TO BE   02453000
*    PASSED TO THE FORMAT SERVICE ROUTINE. ADPLFMT2 CONTAINS THE CURREN 02454000
*    ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED AND ,IN MOST CASES,   02455000
*    ADPLFMT1 CONTAINS THE ADDRESS OF THE FIELD 'CONVERTD' WHICH IS     02456000
*    THE EBCDIC REPRESENTATION OF THE OFFSET TO BE PLACED ALONG SIDE    02457000
*    THE OUTPUT LINE.                                              0843 02458000
*                                                                  0843 02459000
*   CALLED BY -                                                    0843 02460000
*     IECIOEXD,IECIOUCB                                            0843 02461000
*                                                                  0843 02462000
*   RETURNS CONTROL TO -                                           0843 02463000
*     CALLER WITH RETURN CODE RECIEVED FROM FORMAT                 0843 02464000
*     SERVICE ROUTINE                                              0843 02465000
*                                                                    */ 02466000
*/****************************************************************** */ 02467000
*                                                                  0843 02468000
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS THE INVOKER          */ 02469000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0843 02470000
         BNZ   @RF00843                                            0843 02471000
*     DO;                           /* PROTECTION IS NEEDED FROM THE    02472000
*                                      FORMAT SERVICE ROUTINE        */ 02473000
*       RTYADR=ADDR(FMTRTY);        /* WHERE TO RETRY FROM THE ESTAE */ 02474000
         LA    @05,FMTRTY                                          0845 02475000
         ST    @05,RTYADR(,ESTAPTR)                                0845 02476000
*       ERREXP=ON;                  /* INDICATE ERROR IS EXPECTED    */ 02477000
         OI    ERREXP(ESTAPTR),B'10000000'                         0846 02478000
*     END;                          /* END SNAP WAS INVOKER          */ 02479000
*   ELSE                                                           0848 02480000
*     ;                             /* SNAP WAS NOT INVOKER          */ 02481000
@RF00843 DS    0H                                                  0849 02482000
*   CALL FORMAT;                    /* FORMAT ROUTINE PASSED BY    0849 02483000
*                                      CALLER                        */ 02484000
         L     @15,ADPLFRMT(,ABDPLPTR)                             0849 02485000
         BALR  @14,@15                                             0849 02486000
*   ERREXP=OFF;                     /* ERROR IS NO LONGER EXPECTED   */ 02487000
         NI    ERREXP(ESTAPTR),B'01111111'                         0850 02488000
*   IF RCREG^=ZERO THEN                                            0851 02489000
         LTR   RCREG,RCREG                                         0851 02490000
         BZ    @RF00851                                            0851 02491000
*     DO;                           /* FORMAT ROUTINE ERROR          */ 02492000
*FMTRTY:                            /* RETRY ADDR FROM ESTAE         */ 02493000
*       ERRSW=ON;                   /* SET ERROR INDICATOR           */ 02494000
FMTRTY   OI    ERRSW,B'10000000'                                   0853 02495000
*       CALL PRINT;                 /* SPACE A LINE                  */ 02496000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0854 02497000
         BALR  @14,@15                                             0854 02498000
*       BUFFER=MSG;                 /* ERROR MSG PASSED BY CALLER  0855 02499000
*                                      WHOSE ADDR IS PLACED IN     0855 02500000
*                                      ADPLCOM1                      */ 02501000
         L     @05,ADPLBUF(,ABDPLPTR)                              0855 02502000
         L     @04,ADPLCOM1(,ABDPLPTR)                             0855 02503000
         MVC   BUFFER(120,@05),MSG(@04)                            0855 02504000
*       RCREG=4;                    /* INDICATE FORMAT ROUTINE DID 0856 02505000
*                                      NOT COMPLETE SUCESSFULLY      */ 02506000
         LA    RCREG,4                                             0856 02507000
*     END;                          /* END FORMAT ROUTINE ERROR      */ 02508000
*   ELSE                                                           0858 02509000
*     ;                             /* FORMATTED LINE IS IN BUFFER   */ 02510000
@RF00851 DS    0H                                                  0859 02511000
*   RCCODE=RCREG;                   /* SAVE RETURN CODE FROM FORMAT     02512000
*                                      SERVICE ROUTINE               */ 02513000
         LR    RCCODE,RCREG                                        0859 02514000
*   CALL PRINT;                     /* PRINT THE ERROR MSG OR      0860 02515000
*                                      FORMATTED LINE                */ 02516000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0860 02517000
         BALR  @14,@15                                             0860 02518000
*   RETURN CODE(RCCODE);                                           0861 02519000
         LR    @15,RCCODE                                          0861 02520000
@EL00004 DS    0H                                                  0861 02521000
@EF00004 DS    0H                                                  0861 02522000
@ER00004 L     @14,@SA00004                                        0861 02523000
         BR    @14                                                 0861 02524000
*   END FMTRTN;                                                    0862 02525000
*ACCESRTN:                                                         0863 02526000
*   PROC OPTIONS(SAVE(14));                                        0863 02527000
*                                                                  0863 02528000
ACCESRTN ST    @14,@SA00005                                        0863 02529000
*/********************************************************************/ 02530000
*/* FUNCTION-                                                      0864 02531000
*       ROUTINE WHICH CALLS THE ACCESS SERVICE ROUTINE AND MOVES THE    02532000
*     VALUE RETURNED IN R0 INTO DATAPTR.                           0864 02533000
*                                                                  0864 02534000
*                                                                  0864 02535000
*   INPUT -                                                        0864 02536000
*         VADR(REGISTER 0) CONTAINS THE VIRTUAL ADDRESS OF THE FIELD    02537000
*         TO BE ACCESSED.                                          0864 02538000
*                                                                  0864 02539000
*    CALLS-                                                        0864 02540000
*         ACCESS SERVICE ROUTINE PASSED IN PARMLIST                0864 02541000
*                                                                  0864 02542000
*    CALLED BY -                                                   0864 02543000
*         IECIOEXD,IECIOUCB                                        0864 02544000
*                                                                  0864 02545000
*    RETURNS TO -                                                  0864 02546000
*         CALLER WITH THE RETURN CODE SET BY THE ACCESS            0864 02547000
*         SERVICE ROUTINE.                                         0864 02548000
*                                                                    */ 02549000
*/********************************************************************/ 02550000
*                                                                  0864 02551000
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS THE CALLER           */ 02552000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0864 02553000
         BNZ   @RF00864                                            0864 02554000
*     DO;                                                          0865 02555000
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN CASE     02556000
*                                      OF EXPECTED ERROR-SIMULATES 0866 02557000
*                                      PRINT DUMP                    */ 02558000
         LA    @05,RETRY                                           0866 02559000
         ST    @05,RTYADR(,ESTAPTR)                                0866 02560000
*       DATAPTR=VADR;               /* PREPARE FOR A LOAD FROM THE 0867 02561000
*                                      DUMP INTO AN INTERNAL BUFFER  */ 02562000
         ST    VADR,DATAPTR                                        0867 02563000
*       RCREG=ZERO;                 /* SUCCESS UNLESS OTHERWISE    0868 02564000
*                                      CHANGED                       */ 02565000
         SLR   RCREG,RCREG                                         0868 02566000
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */ 02567000
         OI    ERREXP(ESTAPTR),B'10000000'                         0869 02568000
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO THE 0870 02569000
*                                      BUFFER                        */ 02570000
*                                                                  0870 02571000
         L     @05,DATAPTR                                         0870 02572000
         MVC   VERIFY(8),DATADWRD(@05)                             0870 02573000
*       /*************************************************************/ 02574000
*       /*                                                           */ 02575000
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/ 02576000
*       /* THIS AREA OR IF IT IS PROTECTED. IF AN ABEND OCCURS THE   */ 02577000
*       /* ESTAE WILL GET CONTROL AND RETRY AT THE POINT WHICH SETS A*/ 02578000
*       /* BAD RETURN CODE AND CONTINUES IN THE MAIN LINE. THIS THEN */ 02579000
*       /* SIMULATES THE PRINT DUMP ACCESS ROUTINE. IF THE AREA ASKED*/ 02580000
*       /* FOR IS IN THE DUMP. A DOUBLE WORD IS MOVED INTO AN        */ 02581000
*       /* INTERNAL BUFFER TO PREVENT AN UNEXPECTED ERROR WHEN       */ 02582000
*       /* REFERENCING THE AREA AGAIN                                */ 02583000
*       /*                                                           */ 02584000
*       /*************************************************************/ 02585000
*                                                                  0871 02586000
*       ERREXP=OFF;                 /* AN ERROR IS NO LONGER EXPECTED*/ 02587000
         NI    ERREXP(ESTAPTR),B'01111111'                         0871 02588000
*       DATAPTR=ADDR(VERIFY);       /* BASE DATAWORD ON THE BUFFER   */ 02589000
         LA    @05,VERIFY                                          0872 02590000
         ST    @05,DATAPTR                                         0872 02591000
*     END;                          /* END SNAP WAS CALLER           */ 02592000
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE */ 02593000
*     DO;                                                          0874 02594000
         B     @RC00864                                            0874 02595000
@RF00864 DS    0H                                                  0875 02596000
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED    02597000
*                                      BY CALLER                     */ 02598000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0875 02599000
         BALR  @14,@15                                             0875 02600000
*       IF RCREG=ZERO THEN                                         0876 02601000
         LTR   RCREG,RCREG                                         0876 02602000
         BNZ   @RF00876                                            0876 02603000
*         DATAPTR=VADR;             /* SAVE PTR RETURNED BY ACCESS 0877 02604000
*                                      SERVICE ROUTINE               */ 02605000
         ST    VADR,DATAPTR                                        0877 02606000
*       ELSE                        /* BAD RETURN CODE               */ 02607000
*RETRY:                             /* RETRY ADDR FOR THE ESTAE      */ 02608000
*         RCREG=4;                  /* SET BAD RETURN FOR THE      0878 02609000
*                                      MAINLINE                      */ 02610000
         B     @RC00876                                            0878 02611000
@RF00876 DS    0H                                                  0878 02612000
RETRY    LA    RCREG,4                                             0878 02613000
*     END;                          /* END CALLER NOT SNAP           */ 02614000
@RC00876 DS    0H                                                  0880 02615000
*   RCCODE=RCREG;                   /* SAVE RETURN CODE              */ 02616000
@RC00864 LR    RCCODE,RCREG                                        0880 02617000
*   RETURN CODE(RCCODE);                                           0881 02618000
         LR    @15,RCCODE                                          0881 02619000
@EL00005 DS    0H                                                  0881 02620000
@EF00005 DS    0H                                                  0881 02621000
@ER00005 L     @14,@SA00005                                        0881 02622000
         BR    @14                                                 0881 02623000
*   END ACCESRTN;                                                  0882 02624000
*   END IECDAFT1                                                   0883 02625000
*                                                                  0883 02626000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02627000
