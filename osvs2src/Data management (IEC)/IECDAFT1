         TITLE 'IECDAFT1-DATA MGMT CONTROL BLOCK FORMAT MODULE         *00001000
                        '                                               00002000
IECDAFT1 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IECDAFT1  77.066'                                 0001 00009000
IECDADEB DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00213-IECDADEB)                                   00013000
         ENTRY IECDADEB                                                 00014000
IECDADCB DS    0H                                                  0001 00015000
         USING *,@15                                               0001 00016000
         B     @PROLOG                                             0001 00017000
         DC    AL2(@EP00284-IECDADCB)                                   00018000
         ENTRY IECDADCB                                                 00019000
IECDAIOB DS    0H                                                  0001 00020000
         USING *,@15                                               0001 00021000
         B     @PROLOG                                             0001 00022000
         DC    AL2(@EP00626-IECDAIOB)                                   00023000
         ENTRY IECDAIOB                                                 00024000
         DROP  @15                                                      00025000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00026000
         BALR  @11,0                                               0001 00027000
@PSTART  LA    @12,4095(,@11)                                      0001 00028000
         USING @PSTART,@11                                         0001 00029000
         USING @PSTART+4095,@12                                    0001 00030000
         L     @00,@SIZDATD                                        0001 00031000
         GETMAIN  R,LV=(0)                                              00032000
         LR    @10,@01                                             0001 00033000
         USING @DATD,@10                                           0001 00034000
         ST    @13,@SA00001+4                                      0001 00035000
         LM    @15,@01,16(@13)                                     0001 00036000
         ST    @10,8(,@13)                                         0001 00037000
         LR    @13,@10                                             0001 00038000
         AH    @15,4(,@15)                                         0001 00039000
         BR    @15                                                 0001 00040000
@EP00001 DS    0H                                                  0002 00041000
*IECDADEB:                                                         0213 00042000
*   ENTRY;                                                         0213 00043000
*                                                                  0213 00044000
@EP00213 DS    0H                                                  0214 00045000
*/********************************************************************/ 00046000
*/* FUNCTION -                                                     0214 00047000
*      ROUTINE TO FORMAT THE DEB WITH THE CORRECT PREFIX LENGTH.   0214 00048000
*                                                                  0214 00049000
*   INPUT -                                                        0214 00050000
*      REGISTER 1 MUST POINT TO THE PARMLIST PASSED BY THE CALLER  0214 00051000
*      AND THE ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED IS IN  0214 00052000
*      ADPLFMT2, THE OFFSET TO BE PLACED ALONG SIDE THE CONTROL    0214 00053000
*      BLOCK IS IN ADPLFMT1 ALSO DEFINED AS START, AND ADPLCOM4    0214 00054000
*      CONTAINS THE ADDRESS OF THE ESTAE PARAMETER LIST.           0214 00055000
*                                                                  0214 00056000
*   CALLS -                                                        0214 00057000
*      ACCESRTN, CONVERT, FMTRTN, AND PRINT                        0214 00058000
*                                                                  0214 00059000
*   CALLED BY -                                                    0214 00060000
*      IECDAFMT TO FORMAT THE DEB                                  0214 00061000
*                                                                  0214 00062000
*   RETURNS TO -                                                   0214 00063000
*      CALLER WITH RETURN CODE SET BY THE ACCESS OR FORMAT SERVICE 0214 00064000
*      ROUTINES                                                    0214 00065000
*                                                                    */ 00066000
*/********************************************************************/ 00067000
*                                                                  0214 00068000
*   ESTAPTR=ADPLCOM4;               /* SAVE ESTAE PARM LIST ADDR     */ 00069000
         L     ESTAPTR,ADPLCOM4(,ABDPLPTR)                         0214 00070000
*   STM(LINEPTR,RCREG,LCALREGS);    /* SAVE REGS FOR EXPECTED RETRY  */ 00071000
         STM   LINEPTR,RCREG,LCALREGS(ESTAPTR)                     0215 00072000
*   DEBPTR=ADPLFMT2;                /* DEB PTR PASSED IN PARM LIST   */ 00073000
         L     DEBPTR,ADPLFMT2(,ABDPLPTR)                          0216 00074000
*   SAVEPTR=DEBPTR-START;           /* ADDRESS TO GENERATE OFFSETS 0217 00075000
*                                      FROM                          */ 00076000
         LR    @07,DEBPTR                                          0217 00077000
         SL    @07,START(,ABDPLPTR)                                0217 00078000
         ST    @07,SAVEPTR                                         0217 00079000
*   ADPLCOM1=ADDR(DEBMSG);          /* FORMAT ERROR MESSAGE          */ 00080000
         LA    @07,DEBMSG                                          0218 00081000
         ST    @07,ADPLCOM1(,ABDPLPTR)                             0218 00082000
*   ERRSW=OFF;                      /* INITIALIZE ERROR INDICATOR    */ 00083000
         NI    ERRSW,B'01111111'                                   0219 00084000
*   RFY                                                            0220 00085000
*     DEB BASED(PREFXPTR);          /* REBASE THE DEB PREFIX         */ 00086000
*                                                                  0220 00087000
*   /*****************************************************************/ 00088000
*   /*                                                               */ 00089000
*   /* PUT OUT THE DEB HEADER                                        */ 00090000
*   /*                                                               */ 00091000
*   /*****************************************************************/ 00092000
*                                                                  0221 00093000
*   BUFFER(1:LENGTH(DEBHDR))=DEBHDR;/* HEADER LINE                   */ 00094000
         L     @07,ADPLBUF(,ABDPLPTR)                              0221 00095000
         MVC   BUFFER(54,@07),DEBHDR                               0221 00096000
*   PACKAREA=DEBPTR;                /* MOVE PTR INTO CHAR AREA FOR 0222 00097000
*                                      THE UNPK                      */ 00098000
         ST    DEBPTR,PACKAREA                                     0222 00099000
*   GARBAGE='04'X;                  /* GARBAGE BYTE FOR THE UNPK     */ 00100000
         MVI   GARBAGE,X'04'                                       0223 00101000
*   UNPK(ADDRESS,UNPACK);           /* MOVE INTO BUFFER LINE         */ 00102000
         UNPK  ADDRESS(9,@07),UNPACK(5)                            0224 00103000
*   TR(ADDRESS,TRANS);              /* TRANSLATE TO PRINTABLE HEX    */ 00104000
         TR    ADDRESS(9,@07),TRANS                                0225 00105000
*   CALL PRINT;                     /* PRINT THE HEADER LINE         */ 00106000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0226 00107000
         BALR  @14,@15                                             0226 00108000
*   BUFFER(1:LENGTH(UNDER))=UNDER;  /* UNDER SCORE                   */ 00109000
         L     @07,ADPLBUF(,ABDPLPTR)                              0227 00110000
         MVC   BUFFER(5,@07),UNDER                                 0227 00111000
*   CALL PRINT;                     /* PRINT IT                      */ 00112000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0228 00113000
         BALR  @14,@15                                             0228 00114000
*   CALL PRINT;                     /* BLANK LINE                    */ 00115000
*                                                                  0229 00116000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0229 00117000
         BALR  @14,@15                                             0229 00118000
*   /*****************************************************************/ 00119000
*   /*                                                               */ 00120000
*   /* FIND CORRECT DEB PREFIX AND THE DEB LENGTH                    */ 00121000
*   /*                                                               */ 00122000
*   /*****************************************************************/ 00123000
*                                                                  0230 00124000
*   PREFXPTR=DEBPTR-LONGPREF;       /* PTR TO LONG PREFIX            */ 00125000
         LA    @07,36                                              0230 00126000
         LR    @05,DEBPTR                                          0230 00127000
         SLR   @05,@07                                             0230 00128000
         ST    @05,PREFXPTR                                        0230 00129000
*   PREFIX=LONGPREF;                /* SAVE PREFIX LENGTH            */ 00130000
         ST    @07,PREFIX                                          0231 00131000
*   VADR=ADDR(DEBLNGTH);            /* DEB LENGTH IN DOUBLE WORDS    */ 00132000
         LA    VADR,DEBLNGTH(,@05)                                 0232 00133000
*   CALL ACCESRTN;                  /* ACCESS IT                     */ 00134000
         BAL   @14,ACCESRTN                                        0233 00135000
*   IF RCREG^=ZERO THEN             /* ACCESS ERROR                  */ 00136000
         LTR   RCREG,RCREG                                         0234 00137000
         BZ    @RF00234                                            0234 00138000
*     BUFFER(1:LENGTH(DEBMSG))=DEBMSG;/* ERROR MSG                   */ 00139000
         L     @07,ADPLBUF(,ABDPLPTR)                              0235 00140000
         MVC   BUFFER(120,@07),DEBMSG                              0235 00141000
*   ELSE                            /* FOUND DEB LENGTH              */ 00142000
*     DO;                                                          0236 00143000
         B     @RC00234                                            0236 00144000
@RF00234 DS    0H                                                  0237 00145000
*       DEBSIZE=(DATABYTE*8);       /* LENGTH OF DEB IN BYTES        */ 00146000
         L     @07,DATAPTR                                         0237 00147000
         SLR   @05,@05                                             0237 00148000
         IC    @05,DATABYTE(,@07)                                  0237 00149000
         SLA   @05,3                                               0237 00150000
         STH   @05,DEBSIZE                                         0237 00151000
*       IF DEBPTR=((DEBPTR/8)*8) THEN/* DEB DOES NOT HAVE FULL PREFIX*/ 00152000
         LR    @07,DEBPTR                                          0238 00153000
         SRL   @07,3                                               0238 00154000
         SLA   @07,3                                               0238 00155000
         CR    DEBPTR,@07                                          0238 00156000
         BNE   @RF00238                                            0238 00157000
*         PREFIX=SHORTPRF;          /* SAVE LENGTH OF SHORT PREFIX   */ 00158000
         MVC   PREFIX(4),@CF00082                                  0239 00159000
*       ELSE                                                       0240 00160000
*         ;                         /* NOT THE SHORT PREFIX          */ 00161000
@RF00238 DS    0H                                                  0241 00162000
*       VADR=ADDR(DEBAMTYP);        /* ACCESS METHOD TYPE            */ 00163000
         L     @07,PREFXPTR                                        0241 00164000
         LA    VADR,DEBAMTYP(,@07)                                 0241 00165000
*       CALL ACCESRTN;              /* ACCESS IT                     */ 00166000
         BAL   @14,ACCESRTN                                        0242 00167000
*       IF RCREG^=ZERO THEN         /* ACCESS ERROR                  */ 00168000
         LTR   RCREG,RCREG                                         0243 00169000
         BZ    @RF00243                                            0243 00170000
*         BUFFER(1:LENGTH(DEBMSG))=DEBMSG;/* ERROR MSG               */ 00171000
         L     @07,ADPLBUF(,ABDPLPTR)                              0244 00172000
         MVC   BUFFER(120,@07),DEBMSG                              0244 00173000
*       ELSE                        /* FOUND ACCESS METHOD TYPE      */ 00174000
*         DO;                                                      0245 00175000
         B     @RC00243                                            0245 00176000
@RF00243 DS    0H                                                  0246 00177000
*           IF DATABYTE=HASPTYPE THEN/* IS IT A HASP DEB             */ 00178000
         L     @07,DATAPTR                                         0246 00179000
         CLC   DATABYTE(1,@07),HASPTYPE                            0246 00180000
         BNE   @RF00246                                            0246 00181000
*             PREFIX=HASPPREF;      /* LENGTH OF HASP PREFIX         */ 00182000
         MVC   PREFIX(4),@CF00070                                  0247 00183000
*           ELSE                                                   0248 00184000
*             ;                     /* NOT A HASP DEB                */ 00185000
@RF00246 DS    0H                                                  0249 00186000
*           ADPLFMT2=DEBPTR-PREFIX; /* FORMAT STARTING AT THE PREFIX */ 00187000
         L     @07,PREFIX                                          0249 00188000
         LR    @05,DEBPTR                                          0249 00189000
         SLR   @05,@07                                             0249 00190000
         ST    @05,ADPLFMT2(,ABDPLPTR)                             0249 00191000
*           ENDPTR=ADPLFMT2+DEBSIZE;/* ADDRESS OF END OF THE DEB     */ 00192000
         AH    @05,DEBSIZE                                         0250 00193000
         ST    @05,ENDPTR                                          0250 00194000
*           IF PREFIX=LONGPREF THEN /* FORMAT THE DEB PREFIX AND   0251 00195000
*                                      COMMON SECTION FOR THE NORMAL    00196000
*                                      PREFIX                        */ 00197000
         CH    @07,@CH01703                                        0251 00198000
         BNE   @RF00251                                            0251 00199000
*             DO ARRAYCNT=1 TO DIM(NORMLDEB) WHILE(ERRSW=OFF);     0252 00200000
         LA    ARRAYCNT,1                                          0252 00201000
@DL00252 TM    ERRSW,B'10000000'                                   0252 00202000
         BNZ   @DC00252                                            0252 00203000
*               LINEPTR=NORMLDEB(ARRAYCNT);/* FORMAT PATTERN ADDR    */ 00204000
         LR    @07,ARRAYCNT                                        0253 00205000
         SLA   @07,2                                               0253 00206000
         L     LINEPTR,NORMLDEB-4(@07)                             0253 00207000
*               CALL CONVERT;       /* GENERATE THE OFFSET           */ 00208000
         BAL   @14,CONVERT                                         0254 00209000
*               CALL FMTRTN;        /* FORMAT THE LINE               */ 00210000
         BAL   @14,FMTRTN                                          0255 00211000
*             END;                  /* END FORMAT LOOP               */ 00212000
         AH    ARRAYCNT,@CH00041                                   0256 00213000
         CH    ARRAYCNT,@CH00068                                   0256 00214000
         BNH   @DL00252                                            0256 00215000
@DC00252 DS    0H                                                  0257 00216000
*           ELSE                    /* NOT THE NORMAL PREFIX         */ 00217000
*             IF PREFIX=SHORTPRF THEN/* FORMAT THE DEB PREFIX AND  0257 00218000
*                                      COMMON SECTION FOR THE SHORT     00219000
*                                      PREFIX                        */ 00220000
         B     @RC00251                                            0257 00221000
@RF00251 CLC   PREFIX(4),@CF00082                                  0257 00222000
         BNE   @RF00257                                            0257 00223000
*               DO ARRAYCNT=1 TO DIM(SHORTDEB) WHILE(ERRSW=OFF);   0258 00224000
         LA    ARRAYCNT,1                                          0258 00225000
@DL00258 TM    ERRSW,B'10000000'                                   0258 00226000
         BNZ   @DC00258                                            0258 00227000
*                 LINEPTR=SHORTDEB(ARRAYCNT);/* FORMAT PATTERN ADDR  */ 00228000
         LR    @07,ARRAYCNT                                        0259 00229000
         SLA   @07,2                                               0259 00230000
         L     LINEPTR,SHORTDEB-4(@07)                             0259 00231000
*                 CALL CONVERT;     /* GENERATE THE OFFSET           */ 00232000
         BAL   @14,CONVERT                                         0260 00233000
*                 CALL FMTRTN;      /* FORMAT THE LINE               */ 00234000
         BAL   @14,FMTRTN                                          0261 00235000
*               END;                /* END FORMAT LOOP               */ 00236000
         AH    ARRAYCNT,@CH00041                                   0262 00237000
         CH    ARRAYCNT,@CH00130                                   0262 00238000
         BNH   @DL00258                                            0262 00239000
@DC00258 DS    0H                                                  0263 00240000
*             ELSE                  /* NOT THE SHORT PREFIX          */ 00241000
*               DO ARRAYCNT=1 TO DIM(HASPDEB) WHILE(ERRSW=OFF);    0263 00242000
         B     @RC00257                                            0263 00243000
@RF00257 LA    ARRAYCNT,1                                          0263 00244000
@DL00263 TM    ERRSW,B'10000000'                                   0263 00245000
         BNZ   @DC00263                                            0263 00246000
*                 LINEPTR=HASPDEB(ARRAYCNT);/* FORMAT PATTERN ADDR   */ 00247000
         LR    @07,ARRAYCNT                                        0264 00248000
         SLA   @07,2                                               0264 00249000
         L     LINEPTR,HASPDEB-4(@07)                              0264 00250000
*                 CALL CONVERT;     /* GENERATE THE OFFSET           */ 00251000
         BAL   @14,CONVERT                                         0265 00252000
*                 CALL FMTRTN;      /* FORMAT THE LINE               */ 00253000
         BAL   @14,FMTRTN                                          0266 00254000
*               END;                /* END FORMAT LOOP               */ 00255000
*                                                                  0267 00256000
         AH    ARRAYCNT,@CH00041                                   0267 00257000
         CH    ARRAYCNT,@CH00130                                   0267 00258000
         BNH   @DL00263                                            0267 00259000
@DC00263 DS    0H                                                  0268 00260000
*           /*********************************************************/ 00261000
*           /*                                                       */ 00262000
*           /* IF THE PREFIX AND BASIC SECTION WERE FORMATTED WITHOUT*/ 00263000
*           /* PROBLEM, THE REST OF THE DEB WILL BE FORMATTED        */ 00264000
*           /*                                                       */ 00265000
*           /*********************************************************/ 00266000
*                                                                  0268 00267000
*           DO WHILE((ADPLFMT2<ENDPTR)&(ERRSW=OFF));               0268 00268000
@RC00257 DS    0H                                                  0268 00269000
@RC00251 B     @DE00268                                            0268 00270000
@DL00268 DS    0H                                                  0269 00271000
*             IF ADPLFMT2+FOURWORD<=ENDPTR THEN/* THE OUTPUT LINE CAN   00272000
*                                      BE 4 WORDS LONG               */ 00273000
         LA    @07,16                                              0269 00274000
         AL    @07,ADPLFMT2(,ABDPLPTR)                             0269 00275000
         C     @07,ENDPTR                                          0269 00276000
         BH    @RF00269                                            0269 00277000
*               LINEPTR=ADDR(DEBDAT4);/* FORMAT PATTERN FOR 4 WORDS     00278000
*                                      OF DATA                       */ 00279000
         LA    LINEPTR,DEBDAT4                                     0270 00280000
*             ELSE                  /* THE REMAINDER OF DEB IS LESS     00281000
*                                      THAN 4 WORDS                  */ 00282000
*               IF ADPLFMT2+THREWORD<=ENDPTR THEN/* THE OUTPUT LINE     00283000
*                                      CAN BE 3 WORDS LONG           */ 00284000
         B     @RC00269                                            0271 00285000
@RF00269 LA    @07,12                                              0271 00286000
         AL    @07,ADPLFMT2(,ABDPLPTR)                             0271 00287000
         C     @07,ENDPTR                                          0271 00288000
         BH    @RF00271                                            0271 00289000
*                 LINEPTR=ADDR(DEBDAT3);/* FORMAT PATTERN FOR 3 WORDS   00290000
*                                      OF DATA                       */ 00291000
         LA    LINEPTR,DEBDAT3                                     0272 00292000
*               ELSE                /* THE REMAINDER OF DEB IS LESS     00293000
*                                      THAN 3 WORDS                  */ 00294000
*                 IF ADPLFMT2+TWOWORD<=ENDPTR THEN/* THE OUTPUT LINE    00295000
*                                      CAN BE 2 WORDS LONG           */ 00296000
         B     @RC00271                                            0273 00297000
@RF00271 LA    @07,8                                               0273 00298000
         AL    @07,ADPLFMT2(,ABDPLPTR)                             0273 00299000
         C     @07,ENDPTR                                          0273 00300000
         BH    @RF00273                                            0273 00301000
*                   LINEPTR=ADDR(DEBDAT2);/* FORMAT PATTERN FOR 2  0274 00302000
*                                      WORDS OF DATA                 */ 00303000
         LA    LINEPTR,DEBDAT2                                     0274 00304000
*                 ELSE              /* THE REMAINDER OF DEB IS LESS     00305000
*                                      THAN 2 WORDS                  */ 00306000
*                   LINEPTR=ADDR(DEBDAT1);/* FORMAT PATTERN FOR 1  0275 00307000
*                                      WORD OF DATA                  */ 00308000
         B     @RC00273                                            0275 00309000
@RF00273 LA    LINEPTR,DEBDAT1                                     0275 00310000
*             CALL CONVERT;         /* GENERATE OFFSET               */ 00311000
@RC00273 DS    0H                                                  0276 00312000
@RC00271 DS    0H                                                  0276 00313000
@RC00269 BAL   @14,CONVERT                                         0276 00314000
*             CALL FMTRTN;          /* FORMAT THE LINE               */ 00315000
         BAL   @14,FMTRTN                                          0277 00316000
*           END;                    /* END FORMAT LOOP               */ 00317000
@DE00268 CLC   ADPLFMT2(4,ABDPLPTR),ENDPTR                         0278 00318000
         BNL   @DC00268                                            0278 00319000
         TM    ERRSW,B'10000000'                                   0278 00320000
         BZ    @DL00268                                            0278 00321000
@DC00268 DS    0H                                                  0279 00322000
*         END;                      /* END FOUND ACCESS METHOD       */ 00323000
*     END;                          /* END FOUND DEB SIZE            */ 00324000
@RC00243 DS    0H                                                  0281 00325000
*   CALL PRINT;                     /* ERROR MESSAGE OR BLANK        */ 00326000
@RC00234 L     @15,ADPLPRNT(,ABDPLPTR)                             0281 00327000
         BALR  @14,@15                                             0281 00328000
*   LCALREGS=RTYREGS;               /* RESTORE CALLERS RETRY REGS    */ 00329000
         MVC   LCALREGS(64,ESTAPTR),RTYREGS(ESTAPTR)               0282 00330000
*   RETURN CODE(RCCODE);            /* RETURN THE RETURN CODE SAVED     00331000
*                                      BY THE ACCESS OR FORMAT     0283 00332000
*                                      SERVICE ROUTINE               */ 00333000
         L     @13,4(,@13)                                         0283 00334000
         L     @00,@SIZDATD                                        0283 00335000
         LR    @01,@10                                             0283 00336000
         FREEMAIN R,LV=(0),A=(1)                                        00337000
         LR    @15,@09                                             0283 00338000
         L     @14,12(,@13)                                        0283 00339000
         LM    @00,@12,20(@13)                                     0283 00340000
         BR    @14                                                 0283 00341000
*IECDADCB:                                                         0284 00342000
*   ENTRY;                                                         0284 00343000
*                                                                  0284 00344000
@EP00284 DS    0H                                                  0285 00345000
*/********************************************************************/ 00346000
*/* FUNCTION -                                                     0285 00347000
*      ROUTINE TO FORMAT THE DCB ACCORDING TO THE ACCESS METHOD TYPE    00348000
*      AND SEND THE TYPE BACK IN THE PARAMETER LIST PASSED BY THE CALLE 00349000
*                                                                  0285 00350000
*   INPUT -                                                        0285 00351000
*      REGISTER 1 MUST POINT TO THE PARMLIST PASSED BY THE CALLER  0285 00352000
*      AND THE ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED IS IN  0285 00353000
*      ADPLFMT2, THE OFFSET TO BE PLACED ALONG SIDE THE CONTROL    0285 00354000
*      BLOCK IS IN ADPLFMT1 ALSO DEFINED AS START, AND ADPLCOM4 CONTAIN 00355000
*      THE ADDRESS OF THE ESTAE PARAMETER LIST.                    0285 00356000
*                                                                  0285 00357000
*   OUTPUT-                                                        0285 00358000
*      PASSES BACK THE ACCESS METHOD TYPE IN ADPLCOM2 AND A RETURN 0285 00359000
*      CODE IN REGISTER 15 INDICATING WHETHER TO FORMAT THE ASSOCIATED  00360000
*      IOBS. RETURN CODE 4 INDICATES NO IOB.                       0285 00361000
*                                                                  0285 00362000
*   CALLS -                                                        0285 00363000
*      ACCESRTN, CONVERT, FMTRTN, AND PRINT                        0285 00364000
*                                                                  0285 00365000
*   CALLED BY -                                                    0285 00366000
*      IECDAFMT TO FORMAT THE DCB                                  0285 00367000
*                                                                  0285 00368000
*   RETURNS TO -                                                   0285 00369000
*      CALLER WITH RETURN CODE SET BY THE ACCESS OR FORMAT SERVICE 0285 00370000
*      ROUTINES,OR IF THE IOBS ARE NOT TO BE FORMATTED             0285 00371000
*                                                                    */ 00372000
*/********************************************************************/ 00373000
*                                                                  0285 00374000
*   DCBTYPES=ZERO;                  /* INITIALIZE THE ACCESS METHOD     00375000
*                                      TYPE FIELD                    */ 00376000
         SLR   @06,@06                                             0285 00377000
         ST    @06,DCBTYPES(,ABDPLPTR)                             0285 00378000
*   ESTAPTR=ADPLCOM4;               /* SAVE ESTAE PARM LIST ADDR     */ 00379000
         L     ESTAPTR,ADPLCOM4(,ABDPLPTR)                         0286 00380000
*   STM(LINEPTR,RCREG,LCALREGS);    /* SAVE REGS FOR EXPECTED RETRY  */ 00381000
         STM   LINEPTR,RCREG,LCALREGS(ESTAPTR)                     0287 00382000
*   DCBPTR=ADPLFMT2;                /* POINTER TO DCB PASSED IN    0288 00383000
*                                      CONTROL BLOCK                 */ 00384000
         L     DCBPTR,ADPLFMT2(,ABDPLPTR)                          0288 00385000
*   SAVEPTR=DCBPTR-START;           /* OFFSET OF FIRST FIELD         */ 00386000
         LR    @06,DCBPTR                                          0289 00387000
         SL    @06,START(,ABDPLPTR)                                0289 00388000
         ST    @06,SAVEPTR                                         0289 00389000
*   ADPLCOM1=ADDR(DCBMSG);          /* FORMAT ERROR MESSAGE          */ 00390000
         LA    @06,DCBMSG                                          0290 00391000
         ST    @06,ADPLCOM1(,ABDPLPTR)                             0290 00392000
*   ERRSW=OFF;                      /* INITIALIZE ERROR SWITCH       */ 00393000
*                                                                  0291 00394000
         NI    ERRSW,B'01111111'                                   0291 00395000
*   /*****************************************************************/ 00396000
*   /*                                                               */ 00397000
*   /* GET THE MACRF BITS TO DETERMINE IF THE ACCESS METHOD IS EXCP  */ 00398000
*   /*                                                               */ 00399000
*   /*****************************************************************/ 00400000
*                                                                  0292 00401000
*   VADR=ADDR(DCBMACRF);            /* MACREF BITS                   */ 00402000
         LA    VADR,DCBMACRF+40(,DCBPTR)                           0292 00403000
*   CALL ACCESRTN;                  /* ACCESS THEM                   */ 00404000
         BAL   @14,ACCESRTN                                        0293 00405000
*   IF RCREG^=ZERO THEN             /* ACCESS ERROR                  */ 00406000
         LTR   RCREG,RCREG                                         0294 00407000
         BZ    @RF00294                                            0294 00408000
*     BUFFER(1:LENGTH(NOACCESS))=NOACCESS;                         0295 00409000
         L     @07,ADPLBUF(,ABDPLPTR)                              0295 00410000
         MVC   BUFFER(70,@07),NOACCESS                             0295 00411000
*   ELSE                            /* FOUND MACREF BITS             */ 00412000
*     DO;                                                          0296 00413000
         B     @RC00294                                            0296 00414000
