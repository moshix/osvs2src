         B     @RC00831                                            0833 02404000
@RF00831 L     @05,CONVRT                                          0833 02405000
         CH    @05,@CH01664                                        0833 02406000
         BNL   @RF00833                                            0833 02407000
*         UNPK(CONVERTD(1:5),UNPACK(3:5));/* UNPACK LAST 3 DIGITS &     02408000
*                                      DUMMY                         */ 02409000
         UNPK  CONVERTD(5),UNPACK+2(3)                             0834 02410000
*       ELSE                        /* 4 DIGITS (MAX)                */ 02411000
*         UNPK(DIGIT(1:5),UNPACK(3:5));/* LAST 4 DIGITS & DUMMY BYTE */ 02412000
         B     @RC00833                                            0835 02413000
