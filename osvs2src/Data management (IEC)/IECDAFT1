*                                   DO ARRAYCNT=1 TO DIM(QSAMARY) WHILE 01140000
*                                        (ERRSW=OFF);              0492 01141000
         LA    ARRAYCNT,1                                          0492 01142000
@DL00492 TM    ERRSW,B'10000000'                                   0492 01143000
         BNZ   @DC00492                                            0492 01144000
*                                    LINEPTR=QSAMARY(ARRAYCNT);/*  0493 01145000
*                                      FORMAT PATTERN ADDR           */ 01146000
         LR    @07,ARRAYCNT                                        0493 01147000
         SLA   @07,2                                               0493 01148000
         L     LINEPTR,QSAMARY-4(@07)                              0493 01149000
*                                    CALL CONVERT;/* GENERATE OFFSET */ 01150000
         BAL   @14,CONVERT                                         0494 01151000
*                                    CALL FMTRTN;/* FORMAT           */ 01152000
         BAL   @14,FMTRTN                                          0495 01153000
*                                   END;/* END FORMAT LOOP           */ 01154000
         AH    ARRAYCNT,@CH00041                                   0496 01155000
         CH    ARRAYCNT,@CH00130                                   0496 01156000
         BNH   @DL00492                                            0496 01157000
@DC00492 DS    0H                                                  0497 01158000
*                                 END;/* END FORMAT QSAM             */ 01159000
*                           END;    /* END FOUND CHAIN BIT           */ 01160000
*                       END;        /* END SAM ACCESS METHOD         */ 01161000
*                     ELSE          /* NOT SAM METHOD                */ 01162000
*                                                                  0500 01163000
*                       /*********************************************/ 01164000
*                       /*                                           */ 01165000
*                       /* BDAM TYPE DCB                             */ 01166000
*                       /*                                           */ 01167000
*                       /*********************************************/ 01168000
*                                                                  0500 01169000
*                       IF DSGDA=ON THEN/* BDAM ACCESS METHOD        */ 01170000
         B     @RC00450                                            0500 01171000
@RF00450 TM    DSGDA,B'00100000'                                   0500 01172000
         BNO   @RF00500                                            0500 01173000
*                         DO;                                      0501 01174000
*                           HDRTYPE(1:LENGTH(BDAM))=BDAM;/* FILL IN     01175000
*                                      TYPE                          */ 01176000
         L     @07,ADPLBUF(,ABDPLPTR)                              0502 01177000
         MVC   HDRTYPE(6,@07),BDAM                                 0502 01178000
*                           CALL PRINT;/* PRINT HEADER               */ 01179000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0503 01180000
         BALR  @14,@15                                             0503 01181000
*                           BUFFER(1:LENGTH(UNDER))=UNDER;/* UNDER 0504 01182000
*                                      SCORE                         */ 01183000
         L     @07,ADPLBUF(,ABDPLPTR)                              0504 01184000
