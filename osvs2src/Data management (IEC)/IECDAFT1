*               LINEPTR=EXAPARY(ARRAYCNT);/* FORMAT PATTERN          */ 00831000
         LR    @07,ARRAYCNT                                        0406 00832000
         SLA   @07,2                                               0406 00833000
         L     LINEPTR,EXAPARY-4(@07)                              0406 00834000
*               CALL CONVERT;       /* GENERATE OFFSET               */ 00835000
         BAL   @14,CONVERT                                         0407 00836000
*               CALL FMTRTN;        /* FORMAT THE LINE               */ 00837000
         BAL   @14,FMTRTN                                          0408 00838000
*             END;                  /* END FORMAT LOOP               */ 00839000
         AH    ARRAYCNT,@CH00041                                   0409 00840000
         CH    ARRAYCNT,@CH00041                                   0409 00841000
         BNH   @DL00405                                            0409 00842000
@DC00405 DS    0H                                                  0410 00843000
*           ELSE                                                   0410 00844000
*             ;                     /* APPENDAGES NOT WANTED         */ 00845000
@RF00404 DS    0H                                                  0411 00846000
*           EXCPDCB=ON;             /* SET EXCP INDICATOR            */ 00847000
         OI    EXCPDCB(ABDPLPTR),B'10000000'                       0411 00848000
*           RCCODE=NOIOB;           /* INHIBIT IOB PROCESSING        */ 00849000
         LA    RCCODE,4                                            0412 00850000
*         END;                      /* END EXCP                      */ 00851000
*                                                                  0414 00852000
*       /*************************************************************/ 00853000
*       /*                                                           */ 00854000
*       /* THE ACCESS METHOD WAS NOT EXCP- DETERMINE WHAT IT WAS AND */ 00855000
*       /* FORMAT APPROPRIATELY                                      */ 00856000
*       /*                                                           */ 00857000
*       /*************************************************************/ 00858000
*                                                                  0414 00859000
*       ELSE                        /* NOT EXCP                      */ 00860000
*         DO;                                                      0414 00861000
         B     @RC00298                                            0414 00862000
@RF00298 DS    0H                                                  0415 00863000
*           VADR=ADDR(DCBDSORG);    /* DATA SET ORGANIZATION BITS    */ 00864000
         LA    VADR,DCBDSORG+16(,DCBPTR)                           0415 00865000
*           CALL ACCESRTN;          /* ACCESS THEM                   */ 00866000
         BAL   @14,ACCESRTN                                        0416 00867000
*           IF RCREG^=ZERO THEN     /* ACCESS ERROR                  */ 00868000
         LTR   RCREG,RCREG                                         0417 00869000
         BZ    @RF00417                                            0417 00870000
*             BUFFER(1:LENGTH(NOACCESS))=NOACCESS;                 0418 00871000
         L     @07,ADPLBUF(,ABDPLPTR)                              0418 00872000
         MVC   BUFFER(70,@07),NOACCESS                             0418 00873000
*           ELSE                    /* FOUND DSORG BITS              */ 00874000
*             DO;                                                  0419 00875000
         B     @RC00417                                            0419 00876000
@RF00417 DS    0H                                                  0420 00877000
*               DSORGBIT=DATAHALF;  /* SAVE DSORG BITS               */ 00878000
*                                                                  0420 00879000
         L     @06,DATAPTR                                         0420 00880000
         MVC   DSORGBIT(2),DATAHALF(@06)                           0420 00881000
*               /*****************************************************/ 00882000
*               /*                                                   */ 00883000
*               /* THIS ROUTINE SKIPS FORMATTING IF THE CONTROL BLOCK*/ 00884000
*               /* WAS AN ACB. IT FORMATES ONLY DCBS                 */ 00885000
*               /*                                                   */ 00886000
*               /*****************************************************/ 00887000
*                                                                  0421 00888000
*               IF ACBM=OFF THEN    /* THIS FORMATTER DOES NOT     0421 00889000
*                                      HANDLER ACBS                  */ 00890000
         TM    ACBM,B'00001000'                                    0421 00891000
         BNZ   @RF00421                                            0421 00892000
*                 DO;                                              0422 00893000
*                   ADPLFMT2=ADDR(DCBRELB);/* WHERE TO START       0423 00894000
*                                      FORMATTING                    */ 00895000
         LA    @06,16                                              0423 00896000
         ALR   @06,DCBPTR                                          0423 00897000
         ST    @06,ADPLFMT2(,ABDPLPTR)                             0423 00898000
*                   BUFFER(1:LENGTH(DCBHDR))=DCBHDR;/* FILL IN HEADER   00899000
*                                      LINE                          */ 00900000
         L     @06,ADPLBUF(,ABDPLPTR)                              0424 00901000
         MVC   BUFFER(55,@06),DCBHDR                               0424 00902000
*                   PACKAREA=DCBPTR;/* CHAR AREA TO UPACK FROM       */ 00903000
         ST    DCBPTR,PACKAREA                                     0425 00904000
*                   GARBAGE='04'X;  /* GARBAGE BYTE FOR THE UNPK     */ 00905000
         MVI   GARBAGE,X'04'                                       0426 00906000
*                   UNPK(ADDRESS,UNPACK);/* PLACE INTO BUFFER        */ 00907000
         UNPK  ADDRESS(9,@06),UNPACK(5)                            0427 00908000
*                   TR(ADDRESS,TRANS);/* MAKE PRINTABLE              */ 00909000
*                                                                  0428 00910000
         TR    ADDRESS(9,@06),TRANS                                0428 00911000
*                   /*************************************************/ 00912000
*                   /*                                               */ 00913000
*                   /* ISAM TYPE DCB                                 */ 00914000
*                   /*                                               */ 00915000
*                   /*************************************************/ 00916000
*                                                                  0429 00917000
*                   IF DSGIS=ON THEN/* ISAM ACCESS METHOD            */ 00918000
         TM    DSGIS,B'10000000'                                   0429 00919000
         BNO   @RF00429                                            0429 00920000
*                     DO;                                          0430 00921000
*                       HDRTYPE(1:LENGTH(ISAM))=ISAM;/* FILL IN TYPE */ 00922000
         MVC   HDRTYPE(6,@06),ISAM                                 0431 00923000
*                       CALL PRINT; /* PRINT HEADER                  */ 00924000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0432 00925000
         BALR  @14,@15                                             0432 00926000
*                       BUFFER(1:LENGTH(UNDER))=UNDER;/* UNDER SCORE */ 00927000
         L     @07,ADPLBUF(,ABDPLPTR)                              0433 00928000
         MVC   BUFFER(5,@07),UNDER                                 0433 00929000
*                       CALL PRINT; /* PRINT IT                      */ 00930000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0434 00931000
         BALR  @14,@15                                             0434 00932000
*                       CALL PRINT; /* BLANK LINE                    */ 00933000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0435 00934000
         BALR  @14,@15                                             0435 00935000
*                       DO ARRAYCNT=1 TO DIM(ISAMARY) WHILE(ERRSW=OFF); 00936000
         LA    ARRAYCNT,1                                          0436 00937000
@DL00436 TM    ERRSW,B'10000000'                                   0436 00938000
         BNZ   @DC00436                                            0436 00939000
*                         LINEPTR=ISAMARY(ARRAYCNT);/* FORMAT PATTERN   00940000
*                                      ADDR                          */ 00941000
         LR    @07,ARRAYCNT                                        0437 00942000
         SLA   @07,2                                               0437 00943000
         L     LINEPTR,ISAMARY-4(@07)                              0437 00944000
*                         CALL CONVERT;/* GENERATE OFFSET            */ 00945000
         BAL   @14,CONVERT                                         0438 00946000
*                         CALL FMTRTN;/* FORMAT                      */ 00947000
         BAL   @14,FMTRTN                                          0439 00948000
*                       END;        /* END FORMAT LOOP               */ 00949000
         AH    ARRAYCNT,@CH00041                                   0440 00950000
         CH    ARRAYCNT,@CH00070                                   0440 00951000
         BNH   @DL00436                                            0440 00952000
@DC00436 DS    0H                                                  0441 00953000
*                       IF MFMVP=ON|MFLCP=ON THEN/* QISAM LOAD BITS     00954000
*                                                            @ZM45069*/ 00955000
         TM    MFMVP,B'00011000'                                   0441 00956000
         BZ    @RF00441                                            0441 00957000
*                         ISAMLOAD=ON;/* SET QISAM LOAD TYPE BIT     */ 00958000
         OI    ISAMLOAD(ABDPLPTR),B'01000000'                      0442 00959000
*                       ELSE        /* NOT QISAM LOAD                */ 00960000
*                         IF MFRD=ON|MFDBF=ON|MFCHK=ON|MFWRT=ON THEN    00961000
         B     @RC00441                                            0443 00962000
@RF00441 TM    MFRD,B'00100110'                                    0443 00963000
         BNZ   @RT00443                                            0443 00964000
         TM    MFWRT,B'00100000'                                   0443 00965000
         BNO   @RF00443                                            0443 00966000
@RT00443 DS    0H                                                  0444 00967000
*                                   /* BISAM METHOD                  */ 00968000
*                           DO;                                    0444 00969000
*                             BISAMDCB=ON;/* INDICATE BISAM          */ 00970000
         OI    BISAMDCB(ABDPLPTR),B'00010000'                      0445 00971000
*                             RCCODE=NOIOB;/* INHIBIT IOB PROCESSING */ 00972000
         LA    RCCODE,4                                            0446 00973000
*                           END;    /* END BISAM                     */ 00974000
*                         ELSE      /* QISAM SCAN TYPE               */ 00975000
*                           ISAMSCAN=ON;/* SET QISAM SCAN TYPE       */ 00976000
         B     @RC00443                                            0448 00977000
@RF00443 OI    ISAMSCAN(ABDPLPTR),B'00100000'                      0448 00978000
*                     END;          /* END ISAM                      */ 00979000
*                   ELSE            /* NOT ISAM                      */ 00980000
*                                                                  0450 00981000
*                     /***********************************************/ 00982000
*                     /*                                             */ 00983000
*                     /* PARTITIONED OR SAM TYPE DCB                 */ 00984000
*                     /*                                             */ 00985000
*                     /***********************************************/ 00986000
*                                                                  0450 00987000
*                     IF DSGPS=ON|DSGPO=ON THEN/* PHYICAL SEQUENTIAL    00988000
*                                      ORGANIZATION OR PARTITIONED 0450 00989000
*                                      ORGANIZATION                  */ 00990000
         B     @RC00429                                            0450 00991000
@RF00429 TM    DSGPS,B'01000010'                                   0450 00992000
         BZ    @RF00450                                            0450 00993000
*                       DO;                                        0451 00994000
*                         VADR=ADDR(DCBCIND2);/* TO GET CHAINED    0452 00995000
*                                      SCHEDULING BIT                */ 00996000
         LA    VADR,DCBCIND2+52(,DCBPTR)                           0452 00997000
*                         CALL ACCESRTN;/* ACCESS IT                 */ 00998000
         BAL   @14,ACCESRTN                                        0453 00999000
*                         IF RCREG^=ZERO THEN/* ACCESS ERROR         */ 01000000
         LTR   RCREG,RCREG                                         0454 01001000
         BZ    @RF00454                                            0454 01002000
*                           BUFFER(1:LENGTH(DCBMSG))=DCBMSG;/* ERROR    01003000
*                                      MESSAGE                       */ 01004000
         L     @07,ADPLBUF(,ABDPLPTR)                              0455 01005000
         MVC   BUFFER(120,@07),DCBMSG                              0455 01006000
*                         ELSE      /* FOUND SCHEDULING BIT          */ 01007000
*                           DO;                                    0456 01008000
         B     @RC00454                                            0456 01009000
@RF00454 DS    0H                                                  0457 01010000
*                             IF(DATABYTE&CHAINMSK)=ZEROBYTE THEN/*     01011000
*                                      NOT CHAIN SCHEDULING          */ 01012000
         L     @07,DATAPTR                                         0457 01013000
         SLR   @06,@06                                             0457 01014000
         IC    @06,DATABYTE(,@07)                                  0457 01015000
         SLR   @07,@07                                             0457 01016000
         IC    @07,CHAINMSK                                        0457 01017000
         NR    @06,@07                                             0457 01018000
         SLR   @07,@07                                             0457 01019000
         IC    @07,ZEROBYTE                                        0457 01020000
         CR    @06,@07                                             0457 01021000
         BNE   @RF00457                                            0457 01022000
*                               SAMNMAL=ON;/* NO CHAINING BIT        */ 01023000
         OI    SAMNMAL(ABDPLPTR),B'00001000'                       0458 01024000
*                             ELSE                                 0459 01025000
*                               SAMCHAIN=ON;/* CHAIN SCHEDULING IS ON*/ 01026000
         B     @RC00457                                            0459 01027000
@RF00457 OI    SAMCHAIN(ABDPLPTR),B'00000100'                      0459 01028000
*                             IF DSGPO=ON THEN/* IF PARTITIONED    0460 01029000
*                                      ORGANIZATION                  */ 01030000
@RC00457 TM    DSGPO,B'00000010'                                   0460 01031000
         BNO   @RF00460                                            0460 01032000
*                               DO;                                0461 01033000
*                                 HDRTYPE(1:LENGTH(BPAM))=BPAM;/*  0462 01034000
*                                      FILL IN TYPE                  */ 01035000
         L     @07,ADPLBUF(,ABDPLPTR)                              0462 01036000
         MVC   HDRTYPE(6,@07),BPAM                                 0462 01037000
*                                 CALL PRINT;/* PRINT HEADER         */ 01038000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0463 01039000
         BALR  @14,@15                                             0463 01040000
*                                 BUFFER(1:LENGTH(UNDER))=UNDER;/* 0464 01041000
*                                      UNDER SCORE                   */ 01042000
         L     @07,ADPLBUF(,ABDPLPTR)                              0464 01043000
         MVC   BUFFER(5,@07),UNDER                                 0464 01044000
*                                 CALL PRINT;/* PRINT IT             */ 01045000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0465 01046000
         BALR  @14,@15                                             0465 01047000
*                                 CALL PRINT;/* BLANK LINE           */ 01048000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0466 01049000
         BALR  @14,@15                                             0466 01050000
*                                 DO ARRAYCNT=1 TO DIM(BPAMARY) WHILE(  01051000
*                                       ERRSW=OFF);                0467 01052000
         LA    ARRAYCNT,1                                          0467 01053000
@DL00467 TM    ERRSW,B'10000000'                                   0467 01054000
         BNZ   @DC00467                                            0467 01055000
*                                   LINEPTR=BPAMARY(ARRAYCNT);/*   0468 01056000
*                                      FORMAT PATTERN ADDR           */ 01057000
         LR    @07,ARRAYCNT                                        0468 01058000
         SLA   @07,2                                               0468 01059000
         L     LINEPTR,BPAMARY-4(@07)                              0468 01060000
*                                   CALL CONVERT;/* GENERATE OFFSET  */ 01061000
         BAL   @14,CONVERT                                         0469 01062000
*                                   CALL FMTRTN;/* FORMAT            */ 01063000
         BAL   @14,FMTRTN                                          0470 01064000
*                                 END;/* END FORMAT LOOP             */ 01065000
         AH    ARRAYCNT,@CH00041                                   0471 01066000
         CH    ARRAYCNT,@CH00130                                   0471 01067000
         BNH   @DL00467                                            0471 01068000
@DC00467 DS    0H                                                  0472 01069000
*                               END;/* END PARTITIONED               */ 01070000
*                             ELSE  /* NOT PARTITIONED ORGANIZATION  */ 01071000
*                               IF MFRD=ON|MFWRT=ON THEN/* BSAM    0473 01072000
*                                      METHOD                        */ 01073000
         B     @RC00460                                            0473 01074000
@RF00460 TM    MFRD,B'00100000'                                    0473 01075000
         BO    @RT00473                                            0473 01076000
         TM    MFWRT,B'00100000'                                   0473 01077000
         BNO   @RF00473                                            0473 01078000
@RT00473 DS    0H                                                  0474 01079000
*                                 DO;/* FORMAT BSAM DCB              */ 01080000
*                                   HDRTYPE(1:LENGTH(BSAM))=BSAM;/*     01081000
*                                      FILL IN TYPE                  */ 01082000
         L     @07,ADPLBUF(,ABDPLPTR)                              0475 01083000
         MVC   HDRTYPE(6,@07),BSAM                                 0475 01084000
*                                   CALL PRINT;/* PRINT HEADER       */ 01085000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0476 01086000
         BALR  @14,@15                                             0476 01087000
*                                   BUFFER(1:LENGTH(UNDER))=UNDER;/*    01088000
*                                      UNDER SCORE                   */ 01089000
         L     @07,ADPLBUF(,ABDPLPTR)                              0477 01090000
         MVC   BUFFER(5,@07),UNDER                                 0477 01091000
*                                   CALL PRINT;/* PRINT IT           */ 01092000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0478 01093000
         BALR  @14,@15                                             0478 01094000
*                                   CALL PRINT;/* BLANK LINE         */ 01095000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0479 01096000
         BALR  @14,@15                                             0479 01097000
*                                   DO ARRAYCNT=1 TO DIM(BSAMARY) WHILE 01098000
*                                        (ERRSW=OFF);              0480 01099000
         LA    ARRAYCNT,1                                          0480 01100000
@DL00480 TM    ERRSW,B'10000000'                                   0480 01101000
         BNZ   @DC00480                                            0480 01102000
*                                    LINEPTR=BSAMARY(ARRAYCNT);/*  0481 01103000
*                                      FORMAT PATTERN ADDR           */ 01104000
         LR    @07,ARRAYCNT                                        0481 01105000
         SLA   @07,2                                               0481 01106000
         L     LINEPTR,BSAMARY-4(@07)                              0481 01107000
*                                    CALL CONVERT;/* GENERATE OFFSET */ 01108000
         BAL   @14,CONVERT                                         0482 01109000
*                                    CALL FMTRTN;/* FORMAT           */ 01110000
         BAL   @14,FMTRTN                                          0483 01111000
*                                   END;/* END FORMAT LOOP END FORMAT   01112000
*                                      BSAM                          */ 01113000
         AH    ARRAYCNT,@CH00041                                   0484 01114000
         CH    ARRAYCNT,@CH00130                                   0484 01115000
         BNH   @DL00480                                            0484 01116000
@DC00480 DS    0H                                                  0485 01117000
*                                 END;/* END BSAM METHOD             */ 01118000
*                               ELSE/* QSAM DCB                      */ 01119000
*                                 DO;                              0486 01120000
         B     @RC00473                                            0486 01121000
@RF00473 DS    0H                                                  0487 01122000
*                                   HDRTYPE(1:LENGTH(QSAM))=QSAM;/*     01123000
*                                      FILL IN TYPE                  */ 01124000
         L     @07,ADPLBUF(,ABDPLPTR)                              0487 01125000
         MVC   HDRTYPE(6,@07),QSAM                                 0487 01126000
*                                   CALL PRINT;/* PRINT HEADER       */ 01127000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0488 01128000
         BALR  @14,@15                                             0488 01129000
*                                   BUFFER(1:LENGTH(UNDER))=UNDER;/*    01130000
*                                      UNDER SCORE                   */ 01131000
         L     @07,ADPLBUF(,ABDPLPTR)                              0489 01132000
         MVC   BUFFER(5,@07),UNDER                                 0489 01133000
*                                   CALL PRINT;/* PRINT IT           */ 01134000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0490 01135000
         BALR  @14,@15                                             0490 01136000
*                                   CALL PRINT;/* BLANK LINE         */ 01137000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0491 01138000
