*                           BDAMDCB=ON;/* SET BIT IN PARMLIST        */ 01211000
         OI    BDAMDCB(ABDPLPTR),B'00000010'                       0512 01212000
*                         END;      /* END BDAM METHOD               */ 01213000
*                       ELSE        /* NOT BDAM METHOD               */ 01214000
*                                                                  0514 01215000
*                         /*******************************************/ 01216000
*                         /*                                         */ 01217000
*                         /* BTAM TYPE DCB                           */ 01218000
*                         /*                                         */ 01219000
*                         /*******************************************/ 01220000
*                                                                  0514 01221000
*                         IF DSGCX=ON THEN/* BTAM METHOD             */ 01222000
         B     @RC00500                                            0514 01223000
@RF00500 TM    DSGCX,B'00010000'                                   0514 01224000
         BNO   @RF00514                                            0514 01225000
*                           DO;                                    0515 01226000
*                             HDRTYPE(1:LENGTH(BTAM))=BTAM;/* FILL IN   01227000
*                                      TYPE                          */ 01228000
         L     @07,ADPLBUF(,ABDPLPTR)                              0516 01229000
