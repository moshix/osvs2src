         L     @15,ADPLPRNT(,ABDPLPTR)                             0517 01232000
         BALR  @14,@15                                             0517 01233000
*                             BUFFER(1:LENGTH(UNDER))=UNDER;/* UNDER    01234000
*                                      SCORE                         */ 01235000
         L     @07,ADPLBUF(,ABDPLPTR)                              0518 01236000
         MVC   BUFFER(5,@07),UNDER                                 0518 01237000
*                             CALL PRINT;/* PRINT IT                 */ 01238000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0519 01239000
         BALR  @14,@15                                             0519 01240000
