         L     LINEPTR,BDAMARY-4(@07)                              0508 01201000
*                             CALL CONVERT;/* GENERATE OFFSET        */ 01202000
         BAL   @14,CONVERT                                         0509 01203000
*                             CALL FMTRTN;/* FORMAT                  */ 01204000
         BAL   @14,FMTRTN                                          0510 01205000
*                           END;    /* END FORMAT LOOP               */ 01206000
