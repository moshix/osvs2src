*                           CALL PRINT;/* PRINT IT                   */ 01186000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0505 01187000
         BALR  @14,@15                                             0505 01188000
*                           CALL PRINT;/* BLANK LINE                 */ 01189000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0506 01190000
         BALR  @14,@15                                             0506 01191000
*                           DO ARRAYCNT=1 TO DIM(BDAMARY) WHILE(ERRSW=  01192000
*                                 OFF);                            0507 01193000
         LA    ARRAYCNT,1                                          0507 01194000
