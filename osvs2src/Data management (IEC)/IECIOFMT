         BR    @14                                                 0214 00313000
EXCPDRTN ST    @14,@SA00002                                        0214 00314000
*/********************************************************************/ 00315000
*/* FUNCTION -                                                     0215 00316000
*      ROUTINE TO SET THE POINTER TO THE EXCPD(XDBA) INTO THE PARAMETER 00317000
*      LIST AND CALL THE EXCPD(XDBA) FORMATTER(IECIOEXD).          0215 00318000
*                                                                  0215 00319000
*   INPUT -                                                        0215 00320000
*      REGISTER 1 POINTS TO THE PARMLIST PASSED BY THE CALLER      0215 00321000
*                                                                  0215 00322000
*   CALLS -                                                        0215 00323000
*      ACCESRTN,IECIOEXD, AND PRINT                                0215 00324000
*                                                                  0215 00325000
*   CALLED BY -                                                    0215 00326000
*      IECIOFMT                                                    0215 00327000
*                                                                  0215 00328000
*   RETURNS TO -                                                   0215 00329000
*      CALLER                                                      0215 00330000
*                                                                    */ 00331000
*/********************************************************************/ 00332000
*                                                                  0215 00333000
*   VADR=ADDR(TCBEXCPD);            /* POINTER TO EXCPD              */ 00334000
         L     @14,ADPLTCB(,ABDPLPTR)                              0215 00335000
         LA    VADR,TCBEXCPD(,@14)                                 0215 00336000
*   CALL ACCESRTN;                  /* ACCESS IT                     */ 00337000
         BAL   @14,ACCESRTN                                        0216 00338000
*   IF RCREG^=ZERO THEN             /* ACCESS ERROR                  */ 00339000
         LTR   RCREG,RCREG                                         0217 00340000
         BZ    @RF00217                                            0217 00341000
*     DO;                                                          0218 00342000
*       BUFFER(1:LENGTH(EXDERR))=EXDERR;/* ERROR MESSAGE             */ 00343000
         L     @14,ADPLBUF(,ABDPLPTR)                              0219 00344000
         MVC   BUFFER(67,@14),EXDERR                               0219 00345000
*       CALL PRINT;                 /* PRINT IT                      */ 00346000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0220 00347000
         BALR  @14,@15                                             0220 00348000
*     END;                          /* END ACCESS ERROR              */ 00349000
*   ELSE                            /* FOUND EXCPD PTR               */ 00350000
*     DO;                                                          0222 00351000
         B     @RC00217                                            0222 00352000
@RF00217 DS    0H                                                  0223 00353000
*       IF DATAADR^=ZERO THEN       /* NON-ZERO PTR                  */ 00354000
         ICM   @14,7,DATAADR(DATAPTR)                              0223 00355000
         BZ    @RF00223                                            0223 00356000
*         DO;                                                      0224 00357000
*           CALL PRINT;             /* SPACE 3 BLANKS                */ 00358000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0225 00359000
         BALR  @14,@15                                             0225 00360000
*           CALL PRINT;             /* BLANK                         */ 00361000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0226 00362000
         BALR  @14,@15                                             0226 00363000
*           CALL PRINT;             /* BLANK                         */ 00364000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0227 00365000
         BALR  @14,@15                                             0227 00366000
*           ADPLCOM1=ADDR(ESTAPARM);/* PASS ESTAE PARM LIST TO     0228 00367000
*                                      INDIVIDUAL FORMAT ROUTINES    */ 00368000
         LA    @14,ESTAPARM                                        0228 00369000
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0228 00370000
*           ADPLFMT2=DATAADR;       /* PLACE INFORMATION INTO      0229 00371000
*                                      PARMLIST                      */ 00372000
         SLR   @14,@14                                             0229 00373000
         ICM   @14,7,DATAADR(DATAPTR)                              0229 00374000
         ST    @14,ADPLFMT2(,ABDPLPTR)                             0229 00375000
*           OFFSET=ZERO;            /* SET OFFSET                    */ 00376000
         SLR   @14,@14                                             0230 00377000
         ST    @14,OFFSET(,ABDPLPTR)                               0230 00378000
*           CALL IECIOEXD;          /* EXCPD FORMATTER               */ 00379000
         L     @15,@CV02383                                        0231 00380000
         BALR  @14,@15                                             0231 00381000
*         END;                      /* END EXCPD                     */ 00382000
*       ELSE                                                       0233 00383000
*         ;                         /* WAS NO EXCPD-ZERO PTR         */ 00384000
@RF00223 DS    0H                                                  0234 00385000
*     END;                          /* END FOUND EXCPD POINTER       */ 00386000
*   END EXCPDRTN;                                                  0235 00387000
@EL00002 DS    0H                                                  0235 00388000
@EF00002 DS    0H                                                  0235 00389000
@ER00002 L     @14,@SA00002                                        0235 00390000
         BR    @14                                                 0235 00391000
*UCBRTN:                                                           0236 00392000
*   PROC OPTIONS(SAVE(14));                                        0236 00393000
*                                                                  0236 00394000
