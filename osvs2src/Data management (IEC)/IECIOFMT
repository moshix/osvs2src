*               ELSE                /* SYSTEM IS MVS                 */ 00173000
*                 DO;                                              0177 00174000
*                                                                  0177 00175000
         B     @RC00175                                            0177 00176000
@RF00175 DS    0H                                                  0178 00177000
*                   /*************************************************/ 00178000
*                   /*                                               */ 00179000
*                   /* DETERMINE IF THE IOS CONTROL BLOCKS ARE       */ 00180000
*                   /* WANTED. THEY ARE ALWAYS FORMATTED IF THE      */ 00181000
*                   /* CALLER IS SNAP. IF AMDPRDMP IS CALLER, THE    */ 00182000
*                   /* TASK MUST BE AN ABENDING TASK.                */ 00183000
*                   /*                                               */ 00184000
*                   /*************************************************/ 00185000
*                                                                  0178 00186000
*                   VADR=ADDR(TCBCMP);/* TCB COMPLETION CODE         */ 00187000
         L     @14,ADPLTCB(,ABDPLPTR)                              0178 00188000
         LA    VADR,TCBCMP(,@14)                                   0178 00189000
*                   CALL ACCESRTN;  /* ACCESS IT                     */ 00190000
         BAL   @14,ACCESRTN                                        0179 00191000
*                   IF RCREG^=ZERO THEN                            0180 00192000
         LTR   RCREG,RCREG                                         0180 00193000
         BZ    @RF00180                                            0180 00194000
*                     BUFFER(1:LENGTH(TCBERR))=TCBERR;/* COULD NOT 0181 00195000
*                                      ACCESS TCB TO FIND COMPLETION    00196000
*                                      CODE                          */ 00197000
         L     @14,ADPLBUF(,ABDPLPTR)                              0181 00198000
         MVC   BUFFER(61,@14),TCBERR                               0181 00199000
*                   ELSE            /* FOUND COMPLETION CODE         */ 00200000
*                     DO;                                          0182 00201000
         B     @RC00180                                            0182 00202000
@RF00180 DS    0H                                                  0183 00203000
*                       COMPC=DATAWORD;/* SAME COMPLETION CODE       */ 00204000
         L     COMPC,DATAWORD(,DATAPTR)                            0183 00205000
*                       VADR=ADDR(TCBFLGS1);/* ABEND CODES BYTE      */ 00206000
         L     @14,ADPLTCB(,ABDPLPTR)                              0184 00207000
         LA    VADR,TCBFLGS1(,@14)                                 0184 00208000
*                       CALL ACCESRTN;/* ACCESS IT                   */ 00209000
         BAL   @14,ACCESRTN                                        0185 00210000
*                       IF RCREG^=ZERO THEN                        0186 00211000
         LTR   RCREG,RCREG                                         0186 00212000
         BZ    @RF00186                                            0186 00213000
*                         BUFFER(1:LENGTH(TCBERR))=TCBERR;/* TROUBLE    00214000
*                                      ACCESSING TCB                 */ 00215000
         L     @14,ADPLBUF(,ABDPLPTR)                              0187 00216000
         MVC   BUFFER(61,@14),TCBERR                               0187 00217000
*                       ELSE        /* FOUND ABEND FLAGS             */ 00218000
*                         DO;                                      0188 00219000
