*                                                                  0496 01384000
*                                                                    */ 01385000
*/********************************************************************/ 01386000
*                                                                  0496 01387000
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS THE CALLER           */ 01388000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0496 01389000
         BNZ   @RF00496                                            0496 01390000
*     DO;                                                          0497 01391000
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN CASE     01392000
*                                      OF EXPECTED ERROR-SIMULATES 0498 01393000
*                                      PRINT DUMP                    */ 01394000
         LA    @14,RETRY                                           0498 01395000
         ST    @14,RTYADR                                          0498 01396000
*       DATAPTR=VADR;               /* PREPARE FOR A LOAD FROM THE 0499 01397000
*                                      DUMP INTO AN INTERNAL BUFFER  */ 01398000
         LR    DATAPTR,VADR                                        0499 01399000
*       RCREG=ZERO;                 /* SUCCESS UNLESS OTHERWISE    0500 01400000
*                                      CHANGED                       */ 01401000
         SLR   RCREG,RCREG                                         0500 01402000
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */ 01403000
         OI    ERREXP,B'10000000'                                  0501 01404000
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO THE 0502 01405000
*                                      BUFFER                        */ 01406000
*                                                                  0502 01407000
         MVC   VERIFY(8),DATADWRD(DATAPTR)                         0502 01408000
*       /*************************************************************/ 01409000
*       /*                                                           */ 01410000
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/ 01411000
*       /* THIS AREA OR IF THE AREA IS FETCH PROTECTED. IF AN ABEND  */ 01412000
*       /* OCCURS, THE ESTAE WILL GET CONTROL AND RETRY AT THE POINT */ 01413000
*       /* WHICH SETS A BAD RETURN CODE AND CONTINUES IN THE         */ 01414000
*       /* MAINLINE. THIS THEN SIMULATES THE PRINT DUMP ACCESS       */ 01415000
*       /* ROUTINE. IF THE AREA ASKED FOR IS IN THE DUMP, A DOUBLE   */ 01416000
*       /* WORD IS MOVED INTO AN INTERNAL BUFFER TO PREVENT AN       */ 01417000
*       /* UNEXPECTED ERROR WHEN REFERENCING THE AREA AGAIN          */ 01418000
*       /*                                                           */ 01419000
*       /*************************************************************/ 01420000
*                                                                  0503 01421000
*       ERREXP=OFF;                 /* AN ERROR IS NO LONGER EXPECTED*/ 01422000
         NI    ERREXP,B'01111111'                                  0503 01423000
*       DATAPTR=ADDR(VERIFY);       /* BASE DATAWORD ON THE BUFFER   */ 01424000
         LA    DATAPTR,VERIFY                                      0504 01425000
*     END;                          /* END SNAP WAS CALLER           */ 01426000
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE */ 01427000
*     DO;                                                          0506 01428000
         B     @RC00496                                            0506 01429000
@RF00496 DS    0H                                                  0507 01430000
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED    01431000
*                                      BY CALLER                     */ 01432000
         L     @15,ADPLMEMA(,ABDPLPTR)                             0507 01433000
         BALR  @14,@15                                             0507 01434000
*       IF RCREG=ZERO THEN                                         0508 01435000
