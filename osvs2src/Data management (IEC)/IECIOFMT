*                             END;  /* END CONTROL BLOCKS WANTED     */ 00247000
*                           ELSE                                   0194 00248000
*                             ;     /* CONTROL BLOCKS NOT WANTED     */ 00249000
@RF00189 DS    0H                                                  0195 00250000
*                         END;      /* END FOUND ABEND FLAGS         */ 00251000
*                     END;          /* END FOUND COMPLETION CODE     */ 00252000
@RC00186 DS    0H                                                  0197 00253000
*                 END;              /* END MVS SYSTEM                */ 00254000
@RC00180 DS    0H                                                  0198 00255000
*             END;                  /* END OPERATING SYSTEM BYTE   0198 00256000
*                                      FOUND                         */ 00257000
@RC00175 DS    0H                                                  0199 00258000
*         END;                      /* END NEEDED TCB & CVT WERE   0199 00259000
*                                      PASSED                        */ 00260000
@RC00172 DS    0H                                                  0200 00261000
*TROUBLE:                           /* ENTRY FROM ESTAE TO PRINT   0200 00262000
*                                      MESSAGE                       */ 00263000
*       CALL PRINT;                 /* EITHER BLANK OR ERROR MSG     */ 00264000
@RC00167 DS    0H                                                  0200 00265000
TROUBLE  L     @15,ADPLPRNT(,ABDPLPTR)                             0200 00266000
         BALR  @14,@15                                             0200 00267000
*       GEN(ESTAE 0);               /* DROP THE ESTAE                */ 00268000
         ESTAE 0                                                        00269000
*     END;                          /* END ESTAE ESTABLISHED         */ 00270000
*   ELSE                                                           0203 00271000
*     ;                             /* ESTAE COULD NOT BE ESTABLISHED*/ 00272000
@RF00164 DS    0H                                                  0204 00273000
*   IF ADPLSNPR=OFF THEN            /* SNAP THE CALLER?      @ZA26256*/ 00274000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0204 00275000
         BNZ   @RF00204                                            0204 00276000
*     DO;                           /* YES...                @ZA26256*/ 00277000
*       GENERATE;                                                  0206 00278000
*                                                         /* @ZA26256*/ 00279000
      L REG13,4(REG13)               RESTORE SAVE AREA PTR   @ZA26256   00280000
      FREEMAIN R,LV=@ENDDATD-@DATD,SP=253,A=(DREG)        /* @ZA26256*/ 00281000
*     END;                          /* END... SNAP CALLER    @ZA26256*/ 00282000
*   ELSE                            /* NO... PRINT DUMP CALLER     0208 00283000
*                                                            @ZA26256*/ 00284000
*     DO;                           /*                       @ZA26256*/ 00285000
         B     @RC00204                                            0208 00286000
@RF00204 DS    0H                                                  0209 00287000
*       RFY                                                        0209 00288000
*        (TREG6) RSTD;              /*                       @ZA26256*/ 00289000
*       GENERATE;                                                  0210 00290000
*                                                         /* @ZA26256*/ 00291000
       L REG13,4(REG13)              RESTORE SAVE AREA PTR   @ZA26256   00292000
       IC TREG6,ADPLSBPL(ABDPLPTR)   GET SUBPOOL             @ZA26256   00293000
       FREEMAIN R,LV=@ENDDATD-@DATD,SP=(TREG6),A=(DREG)      @ZA26256   00294000
*     END;                          /* END... PRINT DUMP CALL      0211 00295000
*                                                            @ZA26256*/ 00296000
*   RFY                                                            0212 00297000
*    (TREG6) UNRSTD;                /*                       @ZA26256*/ 00298000
@RC00204 DS    0H                                                  0213 00299000
*   RETURN CODE(0);                 /* RETURN TO CALLER WITH A RETURN   00300000
*                                      CODE OF 0 SO THE CALLER WILL     00301000
*                                      CONTINUE FORMATTING THE DUMP  */ 00302000
         SLR   @15,@15                                             0213 00303000
         L     @14,12(,@13)                                        0213 00304000
         LM    @00,@12,20(@13)                                     0213 00305000
         BR    @14                                                 0213 00306000
*EXCPDRTN:                                                         0214 00307000
*   PROC OPTIONS(SAVE(14));                                        0214 00308000
*                                                                  0214 00309000
@EL00001 DS    0H                                                  0214 00310000
@EF00001 DS    0H                                                  0214 00311000
