*           GPR15P=GPR13P;          /* SAVE RETURN CODE.             */ 00481000
         LR    GPR15P,GPR13P                                       0253 00482000
*           GPR12P=GPR00P;          /* RESTORE REG. 12               */ 00483000
         LR    GPR12P,GPR00P                                       0254 00484000
*           GPR13P=GPR01P;          /* RESTORE REG. 13.              */ 00485000
