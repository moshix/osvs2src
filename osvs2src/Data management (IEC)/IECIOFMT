@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @10,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@10                                         0001 00013000
*   TREG2=ABDPLPTR;                 /* SAVE PARM LIST POINTER      0136 00014000
*                                                            @ZA26256*/ 00015000
         LR    TREG2,ABDPLPTR                                      0136 00016000
*   IF ADPLSNPR=OFF THEN            /* SNAP CALLER?          @ZA26256*/ 00017000
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0137 00018000
         BNZ   @RF00137                                            0137 00019000
*     DO;                           /* YES....               @ZA26256*/ 00020000
*       PARMPTR=ADDR(ADPLCOM2);     /* ADDRESS OF GETMAIN LIST     0139 00021000
*                                                            @ZA26256*/ 00022000
         LA    @14,ADPLCOM2(,ABDPLPTR)                             0139 00023000
         LR    PARMPTR,@14                                         0139 00024000
*       GENERATE REFS(DREG,ADPLCOM1);                              0140 00025000
*                                                         /* @ZA26256*/ 00026000
