*    RFY (WRK4,WRK5) UNRSTD;                                       0367 00847000
*/********************************************************************/ 00848000
*/*    ENTRY POINT FROM RTM ON FRR RETRY                             */ 00849000
*/********************************************************************/ 00850000
*CRHARTRY: ;                                                       0368 00851000
CRHARTRY DS    0H                                                  0369 00852000
*    RETCODE=EIGHT;                    /* SET UP RETURN CODE         */ 00853000
         LA    RETCODE,8                                           0369 00854000
*    GO TO CRHAEXIT;                   /* GO TO EXIT ROUTINE         */ 00855000
         B     CRHAEXIT                                            0370 00856000
*/********************************************************************* 00857000
**                                                                 0371 00858000
** ENTRY NAME - IECVCRHD                                           0371 00859000
**                                                                 0371 00860000
** DESCRIPTIVE NAME - CRH DEACTIVATION ROUTINE                     0371 00861000
**                                                                 0371 00862000
** FUNCTION - THIS ENTRY DEACTIVATES THE CHANNEL RECONFIGURATION   0371 00863000
**            HARDWARE FUNCTION IN THE OS/VS2 SYSTEM. IT REMOVES   0371 00864000
**            HOOKS IN IOS MAINLINE MODULE IECIOSCN, POINTS THE    0371 00865000
**            I/O FLIH BACK TO THE IOS SLIH AND PROCESSES THE UCBS 0371 00866000
**            FOR USE WITHOUT CRH. THE CRH FUNCTION IS LOGICALLY   0371 00867000
**            DEACTIVATED WHEN THE POINTER IN THE CVT (CVTCRCA)    0371 00868000
**            TO THE CRH COMMUNICATION AREA HAS BEEN ZEROED.       0371 00869000
**                                                                 0371 00870000
** LINKAGE - STANDARD LINKAGE                                      0371 00871000
**                                                                 0371 00872000
** INPUT                                                           0371 00873000
**                                                                 0371 00874000
**      REGISTERS - SAVEPTR  (REGISTER 13) POINTS TO 18 WORD SAVE AREA  00875000
**                  RETURN   (REGISTER 14) ADDRESS OF RETURN POINT 0371 00876000
**                  ENTRYREG (REGISTER 15) ADDRESS OF IECVCRHA     0371 00877000
**                                                                 0371 00878000
** OUTPUT - NONE                                                   0371 00879000
**                                                                 0371 00880000
** EXIT - NORMAL - RETURN TO CALLER WITH RETURN CODE OF 0          0371 00881000
**                                                                 0371 00882000
** EXIT - ERROR - RETURN TO CALLER WITH RETURN CODE                0371 00883000
**                  RC=4  CRH ALREADY ACITVE                       0371 00884000
**********************************************************************/ 00885000
*/********************************************************************/ 00886000
*/*                                                                  */ 00887000
*/*           ESTABLISH ADRESSABILITY AND ENVIRONMENT                */ 00888000
*/*                                                                  */ 00889000
*/********************************************************************/ 00890000
*/* IECVCRHD: ENTRY                       CRH DEACTIVATION ROUTINE   */ 00891000
*/********************************************************************/ 00892000
*/*      STANDARD ENTRY LINKAGE                                      */ 00893000
*/********************************************************************/ 00894000
*    GEN CODE DEFS(IECVCRHD)                                       0371 00895000
*        REFS(RETRNREG,BASE,SAVEPTR,ENTRYREG,MYSAVE,CALLER);       0371 00896000
IECVCRHD   DS  0F                          ENTRY POINT                  00897000
     STM   RETRNREG,BASE,SAVREGS(SAVEPTR)  SAVE CALLERS REGISTERS       00898000
     LR    BASE,ENTRYREG                   LOAD BASE REGISTER           00899000
     USING IECVCRHD,BASE                   INDICATE ADDRESSABILITY      00900000
     LA    RETRNREG,MYSAVE                 GET ADDRESS OF MY SAVE AREA  00901000
     ST    RETRNREG,CALLED(SAVEPTR)        STORE IN CALLERS SAVE AREA   00902000
     ST    SAVEPTR,CALLER(RETRNREG)        SAVE PTR TO CALLERS AREA     00903000
     LR    SAVEPTR,RETRNREG                POINT REG13 TO MY SAVE AREA  00904000
*/********************************************************************/ 00905000
*/*  DISABLE FOR INTERRUPTS AND PROVIDE FRR COVERAGE                 */ 00906000
*/********************************************************************/ 00907000
*    RFY (WRK4,WRK5) RSTD;                                         0372 00908000
*    GEN CODE REFS(DISABLE,SAVEMASK,WRK4,WRK5);                    0373 00909000
     STNSM SAVEMASK,DISABLE            DISABLE FOR I/O AND EXT. INTER.  00910000
     SETFRR  A,FRRAD=CRHDF,WRKREGS=(WRK4,WRK5)                          00911000
*    RFY (WRK4,WRK5) UNRSTD;                                       0374 00912000
*    PSAIOSUP=ON;                      /* TURN ON IOS SUPER BIT SO 0375 00913000
*                                       THAT MY FRR WILL GET CONTROL    00914000
*                                       INSTEAD OF THE VARY ESTAE    */ 00915000
         OI    PSAIOSUP,B'10000000'                                0375 00916000
*    SETFRRD=ON;                       /* INDICATE FRR COVERAGE      */ 00917000
         OI    SETFRRD,B'01000000'                                 0376 00918000
*/********************************************************************/ 00919000
*/*                                                                  */ 00920000
*/*       MAKE SURE CRH FUNCTION IS ACTIVE AND THEN DEACTIVATE       */ 00921000
*/*                                                                  */ 00922000
*/********************************************************************/ 00923000
*                                                                  0377 00924000
*/********************************************************************/ 00925000
*/*  CVTCRCA=0 INDICATES CRH NOT ACTIVE                              */ 00926000
*/********************************************************************/ 00927000
*    IF CVTCRCA=ZERO                   /* CHECK IF CRH ACTIVE        */ 00928000
*       THEN DO;                       /* CRH NOT ACTIVE             */ 00929000
         L     @10,CVTPTR                                          0377 00930000
         ICM   @03,15,CVTCRCA(@10)                                 0377 00931000
         BNZ   @RF00377                                            0377 00932000
*              RETCODE=FOUR;           /* INDICATE CRH NOT ACTIVE    */ 00933000
         LA    RETCODE,4                                           0379 00934000
*              GOTO CRHDEXIT;          /* CALL EXIT ROUTINE          */ 00935000
         B     CRHDEXIT                                            0380 00936000
*            END;                                                  0381 00937000
*    CVTCRCA=ZERO;                     /* INDICATE CRH NO LONGER   0382 00938000
*                                         ACTIVE                     */ 00939000
@RF00377 L     @10,CVTPTR                                          0382 00940000
         SLR   @03,@03                                             0382 00941000
         ST    @03,CVTCRCA(,@10)                                   0382 00942000
*/********************************************************************/ 00943000
*/*      ISSUE DEACTIVATION MESSAGE                                  */ 00944000
*/********************************************************************/ 00945000
*    GEN CODE REFS(IEA972I);                                       0383 00946000
     RECORD TYPE=WTO,DATAADR=IEA972I,BUFFER=NO,RCVRY=SETFRR,LENGTH=LIEAX00947000
               972I                                                     00948000
*/********************************************************************/ 00949000
*/*  CALL SUBROUTINE TO BACK OUT CHANGES TO OS/VS SYSTEM FOR CRH     */ 00950000
*/********************************************************************/ 00951000
*    LOOKUP=CVTILK2;                   /* POINT TO FIRST ENTRY IN UCB   00952000
*                                         LOOK UP TABLE              */ 00953000
