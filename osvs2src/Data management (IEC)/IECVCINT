         ST    @10,SDWASR13(,SDWAPTR)                              0445 01166000
*    RETRNREG=SAVERET;                 /* RESTORE RETURN REGISTER    */ 01167000
         L     RETRNREG,SAVERET                                    0446 01168000
*    GEN CODE (SETFRR D,WRKREGS=(WRK4,WRK5)); /* DELETE SECOND LEVEL */ 01169000
         SETFRR D,WRKREGS=(WRK4,WRK5)                                   01170000
*    FRRENTRA=OFF;                     /* SECOND LEVEL DELETED       */ 01171000
*    RETRYA=ON;                        /* INDICATE RETRY ATTEMPTED   */ 01172000
