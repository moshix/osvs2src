         TITLE 'IECVCINT - CHANNEL RECONFIGURATION HARDWARE MODULE     *00001000
                        '                                               00002000
*                                                                  0141 00003000
IECVCINT   CSECT                                                        00004000
     ENTRY IECVCRHA                    DEFINE                           00005000
     ENTRY IECVCRHD                        ENTRY                        00006000
     ENTRY IECVCRHT                            POINTS                   00007000
     ENTRY IECVCRHV                                FOR                  00008000
     ENTRY IECCRHAF                                  ROUTINES           00009000
     ENTRY IECCRHDF                                                     00010000
     ENTRY IECCINTF                                                     00011000
     ENTRY IECVCRHS                                                     00012000
     ENTRY IECCRHTF                                                     00013000
     LR    BASE,CINTENT                LOAD BASE REGISTER               00014000
     USING IECVCINT,BASE               INDICATE ADRESSABILITY           00015000
*/********************************************************************/ 00016000
*/* IF CRH NOT ACTIVE PASS INTERRUPT ON TO IOS SLIH WHICH PROCESSES  */ 00017000
*/* THE INTERRUPT AND THEN RETURNS DIRECTLY TO I/O FLIH.             */ 00018000
*/********************************************************************/ 00019000
*    IF CVTCRCA=ZERO                   /* IS CRH ACTIVE ?            */ 00020000
*       THEN DO;                       /*NO, PASS INTERRUPT TO SLIH  */ 00021000
         L     @10,CVTPTR                                          0142 00022000
         ICM   @03,15,CVTCRCA(@10)                                 0142 00023000
         BNZ   @RF00142                                            0142 00024000
*            CINTENT=ADDR(IECINT);     /* GET ADDRESS OF IOS SLIH    */ 00025000
         L     @10,IOCOMPTR(,@10)                                  0144 00026000
         L     CINTENT,IOCINT(,@10)                                0144 00027000
*            GEN( BR CINTENT);         /* CALL IOS SLIH - RETURN   0145 00028000
*                                         REGISTER 4 IS PASSED TO IOS   00029000
*                                         SLIH THEREFOR IECINT RETURNS  00030000
*                                         DIRECTLY TO THE I/O FLIH   */ 00031000
          BR CINTENT                                                    00032000
*            END;                                                  0146 00033000
*/********************************************************************/ 00034000
*/*  PROVIDE FRR COVERAGE                                            */ 00035000
*/********************************************************************/ 00036000
*    RFY (WRK1,WRK3) RSTD;                                         0147 00037000
@RF00142 DS    0H                                                  0148 00038000
*    IF SETFRRC=OFF                    /* ALREADY COVERED BY FRR ?   */ 00039000
*       THEN DO;                                                   0148 00040000
         TM    SETFRRC,B'00100000'                                 0148 00041000
         BNZ   @RF00148                                            0148 00042000
*            GEN CODE(SETFRR A,FRRAD=CINTF,WRKREGS=(WRK1,WRK3))    0150 00043000
*                     SETS(WRK1,WRK3) REFS(CINTF); /* SET FRR        */ 00044000
         SETFRR A,FRRAD=CINTF,WRKREGS=(WRK1,WRK3)                       00045000
*            SETFRRC=ON;               /* INDICATE FRR COVERAGE      */ 00046000
         OI    SETFRRC,B'00100000'                                 0151 00047000
*            END;                                                  0152 00048000
*    RFY (WRK1,WRK3) UNRSTD;                                       0153 00049000
@RF00148 DS    0H                                                  0154 00050000
*/********************************************************************/ 00051000
*/*       DO SOME INITITALIZATION                                    */ 00052000
*/********************************************************************/ 00053000
*    SAVERET=CINTRET;                  /* SAVE RETURN REGISTER 4     */ 00054000
         ST    CINTRET,SAVERET                                     0154 00055000
*    CRCAPTR=CVTCRCA;                  /* SET UP CRCA BASE REGISTER  */ 00056000
         L     @10,CVTPTR                                          0155 00057000
         L     CRCAPTR,CVTCRCA(,@10)                               0155 00058000
*    CRCADINT=OFF;                     /* INITIALIZE FLAG TO OFF     */ 00059000
         NI    CRCADINT(CRCAPTR),B'10111111'                       0156 00060000
*    GEN CODE REFS(CRCAPTR) SETS(CRCAITIM);                        0157 00061000
     STCK  CRCAITIM(CRCAPTR)           SAVE TIME OF THIS INTERRUPT      00062000
*/********************************************************************/ 00063000
*/* CHECK WHETHER INTERRUPT WAS SOLICITED ACROSS CRH CONNECTION      */ 00064000
*/********************************************************************/ 00065000
*    IF CRCADIAG=ON                    /* CRH CONNECTION OUTSTANDING?*/ 00066000
*       THEN DO;                       /* YES-INTERRUPT ACROSS CRH   */ 00067000
         TM    CRCADIAG(CRCAPTR),B'10000000'                       0158 00068000
         BNO   @RF00158                                            0158 00069000
*            BUILDIOA=FLCIOAA;         /* UPDATE I/O INTERRUPT       */ 00070000
         MVC   BUILDIOA(3),FLCIOAA                                 0160 00071000
*            IOAA(1:1)=CRCACHAN(1:1);  /* ADDRESS IN FIXED LOW CORE  */ 00072000
         MVC   IOAA(1),CRCACHAN(CRCAPTR)                           0161 00073000
*            FLCIOAA=BUILDIOA;         /* TO REFLECT ACTUAL CHANNEL  */ 00074000
         MVC   FLCIOAA(3),BUILDIOA                                 0162 00075000
*            CATINDEX=IOAA(1:1);       /* GET CHANNEL ADDRESS        */ 00076000
         SLR   CATINDEX,CATINDEX                                   0163 00077000
         IC    CATINDEX,IOAA                                       0163 00078000
*            SLL(CATINDEX,CATELP2);    /*SHIFT TO MULTIPLY BY LENGTH */ 00079000
         LA    @10,3                                               0164 00080000
         SLL   CATINDEX,0(@10)                                     0164 00081000
*            CATPTR=CRCACAT+CATINDEX;  /* GET POINTER TO CAT ENTRY   */ 00082000
         LR    CATPTR,CATINDEX                                     0165 00083000
         AL    CATPTR,CRCACAT(,CRCAPTR)                            0165 00084000
*            CATBSY=OFF;               /* MAKE SURE CHANNEL NOT BUSY */ 00085000
         NI    CATBSY(CATPTR),B'01111111'                          0166 00086000
*            END;                                                  0167 00087000
*/********************************************************************/ 00088000
*/*                                                                  */ 00089000
*/*         FIND UCB FOR DEVICE WHICH CAUSED INTERRUPT               */ 00090000
*/*                                                                  */ 00091000
*/********************************************************************/ 00092000
*                                                                  0168 00093000
*/********************************************************************/ 00094000
*/*  VALIDITY CHECK THE INTERRUPT ADDRESS                            */ 00095000
*/********************************************************************/ 00096000
*    RFY (WRK0,WRK1,WRK3,WRK5) RSTD;                               0168 00097000
@RF00158 DS    0H                                                  0169 00098000
*    WRK0=FLCIOA;                      /* GET INTERRUPT ADDRESS      */ 00099000
         L     WRK0,FLCIOA                                         0169 00100000
*    GEN CODE(  SRDL WRK0,3 );         /* SHIFT 3 LOW ORDER BITS INTO   00101000
*                                         NEXT REGISTER              */ 00102000
           SRDL WRK0,3                                                  00103000
*    WRK3=(IOCUCBBM+WRK0)->BITMASK;    /* PICK UP UCB BIT MASK USING    00104000
*                                         CONTENTS OF WRK0 AS INDEX  */ 00105000
         L     @10,CVTPTR                                          0171 00106000
         L     @10,IOCOMPTR(,@10)                                  0171 00107000
         LR    @09,WRK0                                            0171 00108000
         AL    @09,IOCUCBBM(,@10)                                  0171 00109000
         SLR   WRK3,WRK3                                           0171 00110000
         IC    WRK3,BITMASK(,@09)                                  0171 00111000
*    GEN CODE( SRL WRK1,29);           /* SHIFT 3 BITS TO LOW END    */ 00112000
          SRL WRK1,29                                                   00113000
*    WRK5=X80;                         /* GET BIT READY POSITION     */ 00114000
         L     WRK5,X80                                            0173 00115000
*    GEN CODE( SRL WRK5,0(WRK1));      /* SHIFT BIT INTO AND POSITION*/ 00116000
          SRL WRK5,0(WRK1)                                              00117000
*    WRK5=WRK5&WRK3;                   /* CHECK IF BIT ON IN MASK    */ 00118000
         NR    WRK5,WRK3                                           0175 00119000
*    BC( SEVEN,VCINT100);              /* BRANCH IF VALID DEVICE     */ 00120000
         BC    7,VCINT100                                          0176 00121000
*/********************************************************************/ 00122000
*/*  INVALID DEVICE - ZERO UCB POINTER                               */ 00123000
*/********************************************************************/ 00124000
*    UCBPTR=ZERO;                      /* ZERO UCB POINTER           */ 00125000
         SLR   UCBPTR,UCBPTR                                       0177 00126000
*    GOTO VCINT200;                    /* GO CHECK IF CALLED BY HIO  */ 00127000
         B     VCINT200                                            0178 00128000
*/********************************************************************/ 00129000
*/*  VALID DEVICE - DO UCB LOOK UP                                   */ 00130000
*/********************************************************************/ 00131000
*VCINT100: ;                                                       0179 00132000
VCINT100 DS    0H                                                  0180 00133000
*    GEN CODE( SRDL WRK0,5);           /* SHIFT CU TO NEXT REGISTER  */ 00134000
          SRDL WRK0,5                                                   00135000
*    GEN CODE( SRL  WRK1,28);          /* SHIFT TO LOW ORDER END AND    00136000
*                                         DROP LEFT OVER DEVICE BIT  */ 00137000
          SRL  WRK1,28                                                  00138000
*    WRK3=(CVTILK1+WRK0)->CHANCON;     /* GET CHANNEL CONSTANT       */ 00139000
         L     @09,CVTPTR                                          0182 00140000
         L     @08,CVTILK1(,@09)                                   0182 00141000
         LR    @07,WRK0                                            0182 00142000
         ALR   @07,@08                                             0182 00143000
         SLR   WRK3,WRK3                                           0182 00144000
         IC    WRK3,CHANCON(,@07)                                  0182 00145000
*    WRK1=WRK3+WRK1;                   /* GET TWO TIMES CONTROL UNIT */ 00146000
         ALR   WRK1,WRK3                                           0183 00147000
*    WRK1=WRK1+WRK1;                   /* PLUS CHANNEL CONST @ZA08721*/ 00148000
         ALR   WRK1,WRK1                                           0184 00149000
*    WRK3=FLCIOA;                      /* GET INTERRUPT ADDRESS      */ 00150000
         L     WRK3,FLCIOA                                         0185 00151000
*    WRK5=WRK3&ONEFOX;                 /* ISOLATE DEVICE ADDRESS     */ 00152000
         LR    WRK5,WRK3                                           0186 00153000
         N     WRK5,ONEFOX                                         0186 00154000
*    WRK3=(CVTILK1+WRK1)->CUCON;       /* GET CU CONSTANT            */ 00155000
         ALR   @08,WRK1                                            0187 00156000
         SLR   WRK3,WRK3                                           0187 00157000
         ICM   WRK3,3,CUCON(@08)                                   0187 00158000
*    WRK3=WRK3+WRK5;                   /* ADD TO DEVICE NUMBER       */ 00159000
         ALR   WRK3,WRK5                                           0188 00160000
*    WRK3=WRK3+WRK3;                   /* DOUBLE SUM                 */ 00161000
         ALR   WRK3,WRK3                                           0189 00162000
*    UCBPTR=(CVTILK2+WRK3)->UCBADDR;   /* GET ADDRESS OF UCB         */ 00163000
         LR    @08,WRK3                                            0190 00164000
         AL    @08,CVTILK2(,@09)                                   0190 00165000
         SLR   UCBPTR,UCBPTR                                       0190 00166000
         ICM   UCBPTR,3,UCBADDR(@08)                               0190 00167000
*/********************************************************************/ 00168000
*/*         SET UP ACTIVE IOQ FOR FRR                                */ 00169000
*/********************************************************************/ 00170000
*    IRTIOQ=UCBIOQ;                    /* PUT ACTIVE IOQ IN IRT      */ 00171000
         LA    @09,736                                             0191 00172000
         AL    @09,LCCAPTR                                         0191 00173000
         LR    @08,UCBPTR                                          0191 00174000
         AL    @08,@CF03026                                        0191 00175000
         MVC   IRTIOQ(4,@09),UCBIOQ-508(@08)                       0191 00176000
*    IRTIOQA=ON;                       /* INDICATE ACTIVE IOQ        */ 00177000
         OI    IRTIOQA(@09),B'00000100'                            0192 00178000
*/********************************************************************/ 00179000
*/********************************************************************/ 00180000
*/*                                                                  */ 00181000
*/*      IF CALLED BY HIO PASS INTERRUPT TO HIO ENTRY OF IOS SLIH    */ 00182000
*/*                                                                  */ 00183000
*/********************************************************************/ 00184000
*VCINT200: ;                                                       0193 00185000
VCINT200 DS    0H                                                  0194 00186000
*    IF CRCAHIO=ON                     /* HIO ENTRY ?                */ 00187000
*       THEN DO;                       /* YES - PROCESS HIO INTERRUPT*/ 00188000
         TM    CRCAHIO(CRCAPTR),B'00010000'                        0194 00189000
         BNO   @RF00194                                            0194 00190000
*            CRCAMCWM=OFF;             /* SET UP MCW TO BREAK CRH  0196 00191000
*                                         CONNECTION                 */ 00192000
         NI    CRCAMCWM(CRCAPTR),B'11101111'                       0196 00193000
*            GEN CODE REFS(CRCAMCW,CRCAPTR,CRCA);                  0197 00194000
             USING CRCA,CRCAPTR        ESTABLISH ADDRESSABILITY         00195000
             DC X'8300',S(CRCAMCW)     ISSUE DIAGNOSE                   00196000
             DROP CRCAPTR                                               00197000
*            CRCADIAG=OFF;             /* INDICATE NO CRH CONNECTION */ 00198000
         NI    CRCADIAG(CRCAPTR),B'01111111'                       0198 00199000
*            IF UCBPTR=0               /* CHECK IF VALID DEVICE      */ 00200000
*               THEN CRCADINT=ON;      /* NO- INVALID CRH INTERRUPT  */ 00201000
         LTR   UCBPTR,UCBPTR                                       0199 00202000
         BNZ   @RF00199                                            0199 00203000
         OI    CRCADINT(CRCAPTR),B'01000000'                       0200 00204000
*               ELSE UCBIORST=ON;      /* YES-INDICATE CRH INTERRUPT */ 00205000
         B     @RC00199                                            0201 00206000
@RF00199 OI    UCBIORST(UCBPTR),B'10000000'                        0201 00207000
*            CRCAHIO=OFF;              /* TURN OFF HIO ENTRY FLAG    */ 00208000
@RC00199 NI    CRCAHIO(CRCAPTR),B'11101111'                        0202 00209000
*            GEN (SETFRR D,WRKREGS=(WRK1,WRK3));  /* DELETE FRR      */ 00210000
         SETFRR D,WRKREGS=(WRK1,WRK3)                                   00211000
*            SETFRRC=OFF;              /* INDICATE NO FRR            */ 00212000
         NI    SETFRRC,B'11011111'                                 0204 00213000
*            CINTENT=ADDR(IECINT);     /* GET ADDRESS OF IOS SLIH  0205 00214000
*                                         HIO ENTRY          @ZA27164*/ 00215000
         L     @09,CVTPTR                                          0205 00216000
         L     @09,IOCOMPTR(,@09)                                  0205 00217000
         L     CINTENT,IOCINT(,@09)                                0205 00218000
*/********************************************************************/ 00219000
*/*  NOTE: REGISTER 11 CONTAINS AN ENVIRONMENT CODE WHICH MUST BE    */ 00220000
*/*        MUST BE PASSED TO HIO ENTRY OF SLIH.                      */ 00221000
*/********************************************************************/ 00222000
*            GEN( B 4(CINTENT));       /* CALL IOS SLIH - RETURN   0206 00223000
*                                         REGISTER 4 IS PASSED TO IOS   00224000
*                                         SLIH THEREFOR IECINT RETURNS  00225000
*                                         DIRECTLY TO HIO    @ZA27164*/ 00226000
          B 4(CINTENT)                                                  00227000
*            END;                                                  0207 00228000
*    RFY (WRK1,WRK3) UNRSTD;                                       0208 00229000
@RF00194 DS    0H                                                  0209 00230000
*/********************************************************************/ 00231000
*/*                                                                  */ 00232000
*/*        NORMAL ENTRY - PROCESS INTERRUPT AND PASS TO IOS SLIH     */ 00233000
*/*                                                                  */ 00234000
*/********************************************************************/ 00235000
*                                                                  0209 00236000
*    IF CRCADIAG=ON                    /* CRH CONNECTION OUTSTANDING?*/ 00237000
*/********************************************************************/ 00238000
*/*          INTERRUPT WAS SOLICITED ACROSS CRH CONNECTION           */ 00239000
*/********************************************************************/ 00240000
*       THEN DO;                       /* YES-INTERRUPT ACROSS CRH   */ 00241000
         TM    CRCADIAG(CRCAPTR),B'10000000'                       0209 00242000
         BNO   @RF00209                                            0209 00243000
*            CRCAMCWM=OFF;             /* SET UP MCW TO BREAK CRH  0211 00244000
*                                         CONNECTION                 */ 00245000
         NI    CRCAMCWM(CRCAPTR),B'11101111'                       0211 00246000
*            GEN CODE REFS(CRCAMCW,CRCAPTR,CRCA);                  0212 00247000
             USING CRCA,CRCAPTR        ESTABLISH ADDRESSABILITY         00248000
             DC X'8300',S(CRCAMCW)     ISSUE DIAGNOSE                   00249000
             DROP CRCAPTR                                               00250000
*            CRCADIAG=OFF;             /* INDICATE NO CRH CONNECTION */ 00251000
         NI    CRCADIAG(CRCAPTR),B'01111111'                       0213 00252000
*            GEN CODE( LCTL CTL2,CTL2,SAVECTL2 );  /* RESTORE SYSTEM    00253000
*                                                     CONTROL REG2   */ 00254000
          LCTL CTL2,CTL2,SAVECTL2                                       00255000
*            IF UCBPTR^=0              /* CHECK IF VALID DEVICE      */ 00256000
*               THEN DO;               /* VALID DEVICE               */ 00257000
         LTR   UCBPTR,UCBPTR                                       0215 00258000
         BZ    @RF00215                                            0215 00259000
*                    IF UCK=ON|        /* WILL SENSE BE REQUIRED ?   */ 00260000
*                       CDC=ON|                                    0217 00261000
*                       CCC=ON|                                    0217 00262000
*                       ICC=ON                                     0217 00263000
*                       THEN UCBCRHSN=ON; /* YES, INDICATE IN UCB    */ 00264000
         TM    UCK,B'00000010'                                     0217 00265000
         BO    @RT00217                                            0217 00266000
         TM    CDC,B'00001110'                                     0217 00267000
         BZ    @RF00217                                            0217 00268000
@RT00217 DS    0H                                                  0218 00269000
         OI    UCBCRHSN(UCBPTR),B'00000100'                        0218 00270000
*                       ELSE;                                      0219 00271000
@RF00217 DS    0H                                                  0220 00272000
*                    IF (UCBBSY=ON) &  /* EXPECTED I/O INTERRUPT ?   */ 00273000
*                       (UCBIORST=OFF) /*BUT NOT STARTED ACROSS CRH ?*/ 00274000
*                       THEN IF CDC=ON|  /* YES, CHANNEL DATA CHECK? */ 00275000
         TM    UCBBSY(UCBPTR),B'10000000'                          0220 00276000
         BNO   @RF00220                                            0220 00277000
         TM    UCBIORST(UCBPTR),B'10000000'                        0220 00278000
         BNZ   @RF00220                                            0220 00279000
*                               CCC=ON|  /* CHANNEL CONTROL CHECK ?  */ 00280000
*                               ICC=ON   /* INTERFACE CHANNEL CHECK ?*/ 00281000
*                               THEN CRCACCH=ON; /*INDICATE ERRORS ON   00282000
*                                                  INTERRUPT TO IOS  */ 00283000
         TM    CDC,B'00001110'                                     0221 00284000
         BZ    @RF00221                                            0221 00285000
         OI    CRCACCH(CRCAPTR),B'00100000'                        0222 00286000
*                               ELSE;  /*                    @ZA16176*/ 00287000
@RF00221 DS    0H                                                  0224 00288000
*                       ELSE;                                      0224 00289000
@RF00220 DS    0H                                                  0225 00290000
*                    END;              /* END VALID DEVICE           */ 00291000
*               ELSE CRCADINT=ON;      /* INDICATE INVALID CRH INT.  */ 00292000
         B     @RC00215                                            0226 00293000
@RF00215 OI    CRCADINT(CRCAPTR),B'01000000'                       0226 00294000
*            ENVCODE=CRHINT;           /* SET UP REG 11 FOR CRH    0227 00295000
*                                         INTERRUPT          @ZA16177*/ 00296000
@RC00215 L     ENVCODE,CRHINT                                      0227 00297000
*            END;                      /* END INTERRUPT ACROSS CRH   */ 00298000
*/********************************************************************/ 00299000
*/*          INTERRUPT WAS NOT SOLICITED ACROSS CRH CONNECTION       */ 00300000
*/********************************************************************/ 00301000
*       ELSE DO;                       /* INTERRUPT NOT ACROSS CRH   */ 00302000
         B     @RC00209                                            0229 00303000
@RF00209 DS    0H                                                  0230 00304000
*            GEN CODE(STCTL CTL2,CTL2,SAVECTL2 ); /* SAVE SYSTEM   0230 00305000
*                                                    CONTROL REG2    */ 00306000
         STCTL CTL2,CTL2,SAVECTL2                                       00307000
*            IF UCBPTR^=0              /* CHECK IF VALID DEVICE      */ 00308000
*               THEN DO;               /* VALID DEVICE               */ 00309000
         LTR   UCBPTR,UCBPTR                                       0231 00310000
         BZ    @RF00231                                            0231 00311000
*                    IF UCK=ON|        /* WILL SENSE BE REQUIRED ?   */ 00312000
*                       CDC=ON|                                    0233 00313000
*                       CCC=ON|                                    0233 00314000
*                       ICC=ON                                     0233 00315000
*                       THEN UCBCRHSN=OFF; /* ENSURE FLAG OFF        */ 00316000
         TM    UCK,B'00000010'                                     0233 00317000
         BO    @RT00233                                            0233 00318000
         TM    CDC,B'00001110'                                     0233 00319000
         BZ    @RF00233                                            0233 00320000
@RT00233 DS    0H                                                  0234 00321000
         NI    UCBCRHSN(UCBPTR),B'11111011'                        0234 00322000
*                       ELSE;                                      0235 00323000
@RF00233 DS    0H                                                  0236 00324000
*                                      /* 8 LINES DELETED    @ZA16176*/ 00325000
*                    END;              /* END VALID DEVICE           */ 00326000
*               ELSE;                  /*INVALID DEVICE-PASS TO SLIH */ 00327000
@RF00231 DS    0H                                                  0238 00328000
*            ENVCODE=NORMINT;          /* SET UP REG 11 FOR NORMAL 0238 00329000
*                                         INTERRUPT          @ZA16177*/ 00330000
         L     ENVCODE,NORMINT                                     0238 00331000
*            END;                      /* END NOT CRH INTERRUPT      */ 00332000
*                                                                  0239 00333000
*/********************************************************************/ 00334000
*/*  PASS I/O INTERRUPT TO IOS SLIH IECINT VIA BALR 4,5              */ 00335000
*/********************************************************************/ 00336000
*    CINTENT=ADDR(IECINT);             /* GET ADDRESS OF IOS SLIH (REG  00337000
*                                         11 ALREADY SET UP) @ZA27164*/ 00338000
@RC00209 L     @10,CVTPTR                                          0240 00339000
         L     @01,IOCOMPTR(,@10)                                  0240 00340000
         L     CINTENT,IOCINT(,@01)                                0240 00341000
*    GEN CODE ( BAL CINTRET,4(CINTENT) )                           0241 00342000
*              REFS( CINTRET,CINTENT); /* CALL I/O SLIH      @ZA27164*/ 00343000
          BAL CINTRET,4(CINTENT)                                        00344000
*    CRCAPTR=CVTCRCA;                  /* RESTORE POINTER TO CRCA    */ 00345000
         L     @10,CVTPTR                                          0242 00346000
         L     CRCAPTR,CVTCRCA(,@10)                               0242 00347000
*    BASE=CRCACINT;                    /* RESTORE BASE REGISTER      */ 00348000
         L     BASE,CRCACINT(,CRCAPTR)                             0243 00349000
*/********************************************************************/ 00350000
*/*                                                                  */ 00351000
*/*     POLL ALL CHANNELS ON DEAD CPU THRU CRH BY SELECTIVELY        */ 00352000
*/*     ENABLING FOR INTERRUPTS FROM EACH CHANNEL.                   */ 00353000
*/*                                                                  */ 00354000
*/********************************************************************/ 00355000
*                                                                  0244 00356000
*    POLLCHAN=ZERO;                    /* BEGIN POLLING WITH CHAN. 0 */ 00357000
         MVI   POLLCHAN,X'00'                                      0244 00358000
*    GEN CODE ( LCTL  CTL2,CTL2,ENABLE6 )                          0245 00359000
*    REFS(ENABLE6) SETS(CTL2);        /* ENABLE INTERRUPTS ONLY    0245 00360000
*                                         ACROSS CHANNEL 6           */ 00361000
          LCTL  CTL2,CTL2,ENABLE6                                       00362000
*/********************************************************************/ 00363000
*/*  INDEX THRU ALL VALID CHANNELS ON THIS CPU                       */ 00364000
*/********************************************************************/ 00365000
*    DO WHILE POLLCHAN<=IOXHICH;       /* HIGHEST CHANNEL SYSGENED ? */ 00366000
         B     @DE00246                                            0246 00367000
@DL00246 DS    0H                                                  0247 00368000
*       CATPTR=CRCACAT+(POLLCHAN*EIGHT); /* INDEX TO CAT           0247 00369000
*                                                    ENTRY FOR CHAN. */ 00370000
         SLR   CATPTR,CATPTR                                       0247 00371000
         IC    CATPTR,POLLCHAN                                     0247 00372000
         SLA   CATPTR,3                                            0247 00373000
         AL    CATPTR,CRCACAT(,CRCAPTR)                            0247 00374000
*       IF CATNGEN=ON|                 /* CHANNEL NOT SYSGENED ?     */ 00375000
*          CATNCPU=ON                  /* CHANNEL NOT ON DEAD CPU ?  */ 00376000
*          THEN;                       /* YES - BUMP TO NEXT CHANNEL */ 00377000
         TM    CATNGEN(CATPTR),B'00110000'                         0248 00378000
         BNZ   @RT00248                                            0248 00379000
*          ELSE DO;                    /* CHANNEL VALID - POLL CHAN. */ 00380000
*               /*****************************************************/ 00381000
*               /*  MAKE CRH CONNECTION TO CHANNEL ON DEAD CPU       */ 00382000
*               /*****************************************************/ 00383000
*               CRCAMCWF=POLLCHAN;     /* SET UP MCW TO USE THIS CHAN*/ 00384000
         MVC   CRCAMCWF(1,CRCAPTR),POLLCHAN                        0251 00385000
*               CRCACHAN=POLLADDR;     /* SAVE REMOTE CHANNEL ADDRESS*/ 00386000
         MVC   CRCACHAN(2,CRCAPTR),POLLADDR                        0252 00387000
*               CRCAMCWI=ON;           /* INDICATE MCW FOR CRH       */ 00388000
*               CRCAMCWM=ON;           /* INDICATE MAKE CONNECTION   */ 00389000
         OI    CRCAMCWI(CRCAPTR),B'00110000'                       0254 00390000
*               CRCADIAG=ON;           /* INDICATE CRH CONNECTION  0255 00391000
*                                         OUTSTANDING                */ 00392000
         OI    CRCADIAG(CRCAPTR),B'10000000'                       0255 00393000
*               GEN CODE REFS(CRCAMCW,CRCAPTR,CRCA);               0256 00394000
                USING CRCA,CRCAPTR        ESTABLISH ADDRESSABILITY      00395000
                DC X'8300',S(CRCAMCW)     ISSUE DIAGNOSE                00396000
                DROP CRCAPTR                                            00397000
*               /*****************************************************/ 00398000
*               /*  SOLICIT AN I/O INTERRUPT ACROSS CRH CONNECTION   */ 00399000
*               /*****************************************************/ 00400000
*               GEN CODE REFS(SAVEMASK,ENABLEIO,DISABLE);          0257 00401000
                STOSM SAVEMASK,ENABLEIO       ENABLE                    00402000
                STNSM SAVEMASK,DISABLE        DISABLE                   00403000
*               END;                                               0258 00404000
*       POLLCHAN=POLLCHAN+ONE;         /* BUMP TO NEXT CHANNEL NUMBER*/ 00405000
@RT00248 IC    @01,POLLCHAN                                        0259 00406000
         LA    @01,1(,@01)                                         0259 00407000
         STC   @01,POLLCHAN                                        0259 00408000
*    END;                                                          0260 00409000
@DE00246 L     @10,CVTPTR                                          0260 00410000
         L     @01,IOCOMPTR(,@10)                                  0260 00411000
         L     @01,IOCXPTR(,@01)                                   0260 00412000
         CLC   POLLCHAN(1),IOXHICH(@01)                            0260 00413000
         BNH   @DL00246                                            0260 00414000
*/********************************************************************/ 00415000
*/*                                                                  */ 00416000
*/*                   EXIT ROUTINE                                   */ 00417000
*/*                                                                  */ 00418000
*/********************************************************************/ 00419000
*    IF CRCADIAG=ON                    /* CRH CONNECTION OUTSTANDING?*/ 00420000
*       THEN DO;                       /* YES                        */ 00421000
         TM    CRCADIAG(CRCAPTR),B'10000000'                       0261 00422000
         BNO   @RF00261                                            0261 00423000
*            CRCAMCWM=OFF;             /* INDICATE BREAK CONNECTION  */ 00424000
         NI    CRCAMCWM(CRCAPTR),B'11101111'                       0263 00425000
*            GEN CODE REFS(CRCAMCW,CRCAPTR,CRCA,SAVECTL2) SETS(CTL2);   00426000
             LCTL CTL2,CTL2,SAVECTL2      RESTORE SYSTEM CONTROL REG2   00427000
             USING CRCA,CRCAPTR           ESTABLISH ADDRESSABILITY      00428000
             DC X'8300',S(CRCAMCW)        ISSUE DIAGNOSE                00429000
             DROP CRCAPTR                                               00430000
*            CRCADIAG=OFF;             /* INDICATE NO CRH CONNECTION */ 00431000
         NI    CRCADIAG(CRCAPTR),B'01111111'                       0265 00432000
*            END;                                                  0266 00433000
*    RFY (WRK1,WRK3) RSTD;                                         0267 00434000
@RF00261 DS    0H                                                  0268 00435000
*    GEN CODE (SETFRR D,WRKREGS=(WRK1,WRK3))                       0268 00436000
*             SETS(WRK1,WRK3);         /* DELETE FRR                 */ 00437000
         SETFRR D,WRKREGS=(WRK1,WRK3)                                   00438000
*    SETFRRC=OFF;                      /* INDICATE NO FRR            */ 00439000
         NI    SETFRRC,B'11011111'                                 0269 00440000
*    CINTRET=SAVERET;                  /* RESTORE RETURN REGISTER 4  */ 00441000
         L     CINTRET,SAVERET                                     0270 00442000
*    GEN CODE REFS(CINTRET);                                       0271 00443000
     BR  CINTRET                       RETURN TO CALLER                 00444000
*    RFY (CINTRET,CINTENT,WRK1,WRK3) UNRSTD;                       0272 00445000
*/********************************************************************* 00446000
**                                                                 0273 00447000
** ENTRY NAME - IECVCRHA                                           0273 00448000
**                                                                 0273 00449000
** DESCRIPTIVE NAME - CRH ACTIVATION ROUTINE.                      0273 00450000
**                                                                 0273 00451000
** FUNCTION - THIS ENTRY WILL PERFORM THE INITIALIZATION REQUIRED TO    00452000
**            ALLOW THE OS/VS2 SYSTEM TO USE THE CHANNEL           0273 00453000
**            RECONFIGURATION HARDWARE IN ORDER TO ACCESS A DEVICE 0273 00454000
**            ON A DEAD CPU FROM THE CPU WHICH IS STILL RUNNING IN 0273 00455000
**            A 168MP ENVIRONMENT.                                 0273 00456000
**                                                                 0273 00457000
**  LINKAGE - STANDARD LINKAGE IS USED ON ENTRY AND EXIT           0273 00458000
**            NON-STANDARD LINKAGE IS USED FOR CALL TO IECVSMGR    0273 00459000
**                                                                 0273 00460000
**  INPUT                                                          0273 00461000
**      REGISTERS - SAVEPTR  (REGISTER 13) POINTS TO 18 WORD SAVE AREA  00462000
**                  RETRNREG (REGISTER 14) ADDRESS OF RETURN POINT 0273 00463000
**                  ENTRYREG (REGISTER 15) ADDRESS OF IECVCRHA     0273 00464000
**                                                                 0273 00465000
**  OUTPUT                                                         0273 00466000
**      REGISTERS - RETCODE  (REGISTER 15) SEE EXIT                0273 00467000
**                                                                 0273 00468000
**   EXIT - NORMAL - RETURN TO CALLER WITH RETURN CODE OF 0        0273 00469000
**                                                                 0273 00470000
**   EXIT - ERROR - RETURN TO CALLER WITH RETURN CODE              0273 00471000
**                  RC=4  CRH ALREADY ACTIVE                       0273 00472000
**                  RC=8  UNABLE TO ACTIVATE                       0273 00473000
**                  RC=12 NOT ON A 168MP SYSTEM                    0273 00474000
**                                                                 0273 00475000
**********************************************************************/ 00476000
*/********************************************************************/ 00477000
*/*                                                                  */ 00478000
*/*           ESTABLISH ADRESSABILITY AND ENVIRONMENT                */ 00479000
*/*                                                                  */ 00480000
*/********************************************************************/ 00481000
*/* IECVCRHA: ENTRY                       ENTRY POINT FOR ACTIVATION */ 00482000
*/********************************************************************/ 00483000
*/*      STANDARD ENTRY LINKAGE                                      */ 00484000
*/********************************************************************/ 00485000
*    GEN   CODE DEFS(IECVCRHA)                                     0273 00486000
*          REFS(IECVCRHA,RETRNREG,BASE,SAVEPTR,ENTRYREG,MYSAVE,CALLER); 00487000
IECVCRHA   DS 0F                           ENTRY POINT                  00488000
     STM   RETRNREG,BASE,SAVREGS(SAVEPTR)  SAVE CALLERS REGISTERS       00489000
     LR    BASE,ENTRYREG                   LOAD BASE REGISTER           00490000
     USING IECVCRHA,BASE                   INDICATE ADDRESSABILITY      00491000
     LA    RETRNREG,MYSAVE                 GET ADDRESS OF MY SAVE AREA  00492000
     ST    RETRNREG,CALLED(SAVEPTR)        STORE IN CALLERS SAVE AREA   00493000
     ST    SAVEPTR,CALLER(RETRNREG)        SAVE PTR TO CALLERS AREA     00494000
     LR    SAVEPTR,RETRNREG                POINT REG13 TO MY SAVE AREA  00495000
*/********************************************************************/ 00496000
*/*  DISABLE FOR INTERRUPTS AND PROVIDE FRR COVERAGE                 */ 00497000
*/********************************************************************/ 00498000
*    FOOTPRNT=ZERO;                    /* ZERO PROGRESS FLAGS        */ 00499000
         SLR   @10,@10                                             0274 00500000
         ST    @10,FOOTPRNT                                        0274 00501000
*    RFY (WRK4,WRK5) RSTD;                                         0275 00502000
*    GEN CODE REFS(DISABLE,SAVEMASK,WRK4,WRK5);                    0276 00503000
     STNSM SAVEMASK,DISABLE            DISABLE FOR I/O AND EXT. INTER.  00504000
     SETFRR  A,FRRAD=CRHAF,WRKREGS=(WRK4,WRK5)                          00505000
*/********************************************************************/ 00506000
*/*  WHEN CALLED BY VARY IECVCRHA NEEDS THE IOS SUPER BIT ON SO THAT */ 00507000
*/*  MY FRR WILL GET CONTROL RATHER THAN THE VARY ESTAE EXIT.        */ 00508000
*/********************************************************************/ 00509000
*    IF PSAIOSUP=OFF                   /* CHECK IF IOS SUPER BIT ON  */ 00510000
*       THEN DO;                       /* NEED SUPER BIT FOR FRR     */ 00511000
         TM    PSAIOSUP,B'10000000'                                0277 00512000
         BNZ   @RF00277                                            0277 00513000
*            PSAIOSUP=ON;              /* TURN IT ON                 */ 00514000
         OI    PSAIOSUP,B'10000000'                                0279 00515000
*            IOSUP=ON;                 /* INDICATE I TURNED IT ON    */ 00516000
         OI    IOSUP,B'10000000'                                   0280 00517000
*            END;                                                  0281 00518000
*    SETFRRA=ON;                       /* INDICATE FRR COVERAGE      */ 00519000
@RF00277 OI    SETFRRA,B'10000000'                                 0282 00520000
*    RFY (WRK4,WRK5) UNRSTD;                                       0283 00521000
*/********************************************************************/ 00522000
*/*                                                                  */ 00523000
*/*               CHECK FOR ERROR CONDITIONS                         */ 00524000
*/*                                                                  */ 00525000
*/********************************************************************/ 00526000
*                                                                  0284 00527000
*/********************************************************************/ 00528000
*/*  MAKE SURE WE ARE RUNNING ON A 168MP SYSTEM                      */ 00529000
*/********************************************************************/ 00530000
*    IF PSACPUSA=ZERO                  /* ARE WE RUNNING ON CPU 0?   */ 00531000
*       THEN INDEX=ZERO;               /* YES - CPU ID IS ZERO       */ 00532000
         ICM   @10,3,PSACPUSA                                      0284 00533000
         BNZ   @RF00284                                            0284 00534000
         SLR   INDEX,INDEX                                         0285 00535000
*       ELSE INDEX=FOUR;               /* NO - CPU ID IS FOUR        */ 00536000
         B     @RC00284                                            0286 00537000
@RF00284 LA    INDEX,4                                             0286 00538000
*    IF (PCCACPID(9:12)^=CPU168)|(CSDMP=OFF)                       0287 00539000
*       THEN DO;                                                   0287 00540000
@RC00284 L     @10,CVTPTR                                          0287 00541000
         LR    @01,INDEX                                           0287 00542000
         AL    @01,CVTPCCAT(,@10)                                  0287 00543000
         L     @01,PCCAPTR(,@01)                                   0287 00544000
         CLC   PCCACPID+8(4,@01),CPU168                            0287 00545000
         BNE   @RT00287                                            0287 00546000
         L     @01,CVTCSD(,@10)                                    0287 00547000
         TM    CSDMP(@01),B'10000000'                              0287 00548000
         BNZ   @RF00287                                            0287 00549000
@RT00287 DS    0H                                                  0288 00550000
*            IOXCRHA=ZERO;             /* ZERO POINTER TO IECVCRHA   */ 00551000
         L     @10,CVTPTR                                          0289 00552000
         L     @01,IOCOMPTR(,@10)                                  0289 00553000
         L     @01,IOCXPTR(,@01)                                   0289 00554000
         SLR   @10,@10                                             0289 00555000
         ST    @10,IOXCRHA(,@01)                                   0289 00556000
*            RETCODE = TWELVE;         /* SETUP RETURN CODE          */ 00557000
         LA    RETCODE,12                                          0290 00558000
*            GOTO CRHAEXIT;            /* CALL EXIT SUBROUTINE       */ 00559000
         B     CRHAEXIT                                            0291 00560000
*            END;                                                  0292 00561000
*/********************************************************************/ 00562000
*/*  CHECK WHETHER DSS IS ACTIVE                                     */ 00563000
*/********************************************************************/ 00564000
*    IF CVTDSSAC=FF                    /* TEST WHETHER DSS ACTIVE    */ 00565000
*       THEN DO;                       /* YES, DO NOT ACTIVATE CRH   */ 00566000
@RF00287 L     @10,CVTPTR                                          0293 00567000
         CLC   CVTDSSAC(1,@10),FF                                  0293 00568000
         BNE   @RF00293                                            0293 00569000
*            GEN CODE REFS(IEA989I);                               0295 00570000
             RECORD  TYPE=WTO,DATAADR=IEA989I,BUFFER=NO,RCVRY=SETFRR,LEX00571000
                NGTH=LIEA989I                                           00572000
*            RETCODE=EIGHT;            /* INDICATE UNABLE TO ACTIVATE*/ 00573000
         LA    RETCODE,8                                           0296 00574000
*            GOTO CRHAEXIT;            /* GO RETURN TO CALLER        */ 00575000
         B     CRHAEXIT                                            0297 00576000
*            END;                                                  0298 00577000
*/********************************************************************/ 00578000
*/*  CHECK WHETHER CRH HAS ALREADY BEEN ACTIVATED                    */ 00579000
*/********************************************************************/ 00580000
*    IF CVTCRCA^=ZERO                  /* IS CRCA POINTER ZERO       */ 00581000
*       THEN DO;                                                   0299 00582000
@RF00293 L     @10,CVTPTR                                          0299 00583000
         ICM   @03,15,CVTCRCA(@10)                                 0299 00584000
         BZ    @RF00299                                            0299 00585000
*            RETCODE=FOUR;             /* SET UP RETURN CODE         */ 00586000
         LA    RETCODE,4                                           0301 00587000
*            GOTO CRHAEXIT;            /* CALL EXIT SUBROUTINE       */ 00588000
         B     CRHAEXIT                                            0302 00589000
*            END;                                                  0303 00590000
*    CVTCRCA = ADDR(IECVCRCA);         /* A NON-ZERO POINTER TO CRCA    00591000
*                                         INDICATES CRH IS ACTIVE    */ 00592000
@RF00299 L     @10,@CA02779                                        0304 00593000
         L     @03,CVTPTR                                          0304 00594000
         ST    @10,CVTCRCA(,@03)                                   0304 00595000
*    CRCAPTR=CVTCRCA;                  /* SET UP ADDRESSABILITY      */ 00596000
         LR    CRCAPTR,@10                                         0305 00597000
*    CRCAFTIM=ON;                        /* TURN ON 1ST TIME @ZM30496   00598000
*                                           SWITCH FOR SWITCH        */ 00599000
         OI    CRCAFTIM(CRCAPTR),B'00001000'                       0306 00600000
*    CRHSADDR=CRHS;                      /* ADDR OF SRB SCHED@ZM30496   00601000
*                                           RTN FOR TIMER SLIH       */ 00602000
         L     @10,@CA02758                                        0307 00603000
         MVC   CRHSADDR-CRHSADDR(4,@10),CRHS                       0307 00604000
*/********************************************************************/ 00605000
*/*  INITIALIZE CRH COMMUNICATION AREA (CRCA)                        */ 00606000
*/********************************************************************/ 00607000
*    IF PSACPUSA=ZERO                  /* ARE WE RUNNING ON CPU 0 ?  */ 00608000
*       THEN INDEX=FOUR;               /* YES-DEAD CPU ID IS ONE     */ 00609000
         ICM   @10,3,PSACPUSA                                      0308 00610000
         BNZ   @RF00308                                            0308 00611000
         LA    INDEX,4                                             0309 00612000
*       ELSE INDEX=ZERO;               /* NO - DEAD CPU ID IS ZERO   */ 00613000
         B     @RC00308                                            0310 00614000
@RF00308 SLR   INDEX,INDEX                                         0310 00615000
*    CRCAPCCA=(CVTPCCAT+INDEX)->PCCAPTR; /* GET VIRTUAL ADDRESS OF 0311 00616000
*                                           PCCA FOR DEAD CPU        */ 00617000
@RC00308 L     @10,CVTPTR                                          0311 00618000
         LR    @01,INDEX                                           0311 00619000
         AL    @01,CVTPCCAT(,@10)                                  0311 00620000
         L     @01,PCCAPTR(,@01)                                   0311 00621000
         ST    @01,CRCAPCCA(,CRCAPTR)                              0311 00622000
*    CRCACAT=ADDR(PCCACAT);            /* GET CAT FOR DEAD CPU       */ 00623000
         LA    @01,PCCACAT(,@01)                                   0312 00624000
         ST    @01,CRCACAT(,CRCAPTR)                               0312 00625000
*/********************************************************************/ 00626000
*/*                                                                  */ 00627000
*/*                 HOOK INTO OS/VS2 SYSTEM                          */ 00628000
*/*                                                                  */ 00629000
*/********************************************************************/ 00630000
*                                                                  0313 00631000
*/********************************************************************/ 00632000
*/*  CHANGE ADDRESS OF SLIH IN THE I/O FLIH TO POINT TO IECVCINT     */ 00633000
*/********************************************************************/ 00634000
*    SLIHPTR=ADDR(IECVCINT);           /* POINT TO CRH SLIH-IECVCINT */ 00635000
         L     @03,@CA02782                                        0313 00636000
         L     @01,@CA00028                                        0313 00637000
         ST    @01,SLIHPTR-IEAVEIOI(,@03)                          0313 00638000
*    SLIHCHNG=ON;                      /* INDICATE POINTER TO SLIH 0314 00639000
*                                         WAS CHANGED                */ 00640000
         OI    SLIHCHNG,B'00010000'                                0314 00641000
*/********************************************************************/ 00642000
*/*  PUT HOOKS IN IOS MAINLINE - IECIOSCN                            */ 00643000
*/********************************************************************/ 00644000
*    SAVEHK1=IECVHK1A;                 /* SAVE OVERLAID INSTRUCTION  */ 00645000
         L     @03,@CA02759                                        0315 00646000
         MVC   SAVEHK1(4),IECVHK1A-IECVHK1A(@03)                   0315 00647000
*    IECVHK1A=IECVHK1B;                /* OVERLAY INSTRUCTION        */ 00648000
         L     @01,@CA02760                                        0316 00649000
         MVC   IECVHK1A-IECVHK1A(4,@03),IECVHK1B-IECVHK1B(@01)     0316 00650000
*    TCHHOOK=ON;                       /* INDICATE TCH HOOK IN       */ 00651000
         OI    TCHHOOK,B'00001000'                                 0317 00652000
*    SAVEHK2=IECVHK2A;                 /* SAVE OVERLAID INSTRUCTION  */ 00653000
         L     @03,@CA02761                                        0318 00654000
         MVC   SAVEHK2(4),IECVHK2A-IECVHK2A(@03)                   0318 00655000
*    IECVHK2A=IECVHK2B;                /* OVERLAY INSTRUCTION        */ 00656000
         L     @01,@CA02762                                        0319 00657000
         MVC   IECVHK2A-IECVHK2A(4,@03),IECVHK2B-IECVHK2B(@01)     0319 00658000
*    SIOHOOK=ON;                       /* INDICATE SIO HOOK IN       */ 00659000
         OI    SIOHOOK,B'00000100'                                 0320 00660000
*    SAVEHK3=IECVHK3A;                 /* SAVE OVERLAID INSTRUCTION  */ 00661000
         L     @03,@CA02763                                        0321 00662000
         MVC   SAVEHK3(4),IECVHK3A-IECVHK3A(@03)                   0321 00663000
*    IECVHK3A=IECVHK3B;                /* OVERLAY INSTRUCTION        */ 00664000
         L     @01,@CA02764                                        0322 00665000
         MVC   IECVHK3A-IECVHK3A(4,@03),IECVHK3B-IECVHK3B(@01)     0322 00666000
*    SNSHOOK=ON;                       /* INDICATE SENSE HOOK IN     */ 00667000
         OI    SNSHOOK,B'00000010'                                 0323 00668000
*/********************************************************************/ 00669000
*/*                                                                  */ 00670000
*/*        SCAN THRU ALL UCBS FOR I/O ON DEAD CPU                    */ 00671000
*/*                                                                  */ 00672000
*/********************************************************************/ 00673000
*    RFY UCBOB BASED(NXTUCB);          /* BASE UCBS ON UCB LOOKUP  0324 00674000
*                                         TABLE ENTRIES              */ 00675000
*    RFY UCB BASED(NXTUCB-512);        /* BASE UCB PREFIX ON NXTUCB  */ 00676000
*    RFY IOQ BASED(UCBIOQ);            /* BASE IOQ ON POINTER IN UCB */ 00677000
*    LOOKUP=CVTILK2;                   /* GET POINTER TO UCB LOOK UP    00678000
*                                         TABLE                      */ 00679000
         MVC   LOOKUP(4),CVTILK2(@10)                              0327 00680000
*    IF PSACPUSA=ZERO                  /* ARE WE RUNNING ON CPU 0 ?  */ 00681000
*       THEN DEADCPU=ONE;              /* YES-DEAD CPU ID IS ONE     */ 00682000
         ICM   @10,3,PSACPUSA                                      0328 00683000
         BNZ   @RF00328                                            0328 00684000
         MVI   DEADCPU,X'01'                                       0329 00685000
*       ELSE DEADCPU=ZERO;             /* NO - DEAD CPU ID IS ZERO   */ 00686000
         B     @RC00328                                            0330 00687000
@RF00328 MVI   DEADCPU,X'00'                                       0330 00688000
*    SCANUCBS=ON;                      /* INDICATE UCBS BEING SCANNED   00689000
*                                         FOR I/O ON DEAD CPU        */ 00690000
@RC00328 OI    SCANUCBS,B'00000001'                                0331 00691000
*    DO WHILE NXTUCB^=TABLEND;         /* CHECK FOR END OF TABLE     */ 00692000
         B     @DE00332                                            0332 00693000
@DL00332 DS    0H                                                  0333 00694000
*       IF NXTUCB^=ZERO                /* ENSURE NONZERO UCB POINTER */ 00695000
*          THEN IF UCBCPU=DEADCPU      /* LAST I/O STARTED TO DEAD ? */ 00696000
         L     @10,LOOKUP                                          0333 00697000
         ICM   @03,3,NXTUCB(@10)                                   0333 00698000
         BZ    @RF00333                                            0333 00699000
*                  THEN DO;            /* YES                        */ 00700000
         L     @10,LOOKUP                                          0334 00701000
         SLR   @01,@01                                             0334 00702000
         ICM   @01,3,NXTUCB(@10)                                   0334 00703000
         CLC   UCBCPU(1,@01),DEADCPU                               0334 00704000
         BNE   @RF00334                                            0334 00705000
*                       UCBCPU=UCBCPU&&ONE;/* SWITCH CPU ID          */ 00706000
         L     @10,LOOKUP                                          0336 00707000
         SLR   @01,@01                                             0336 00708000
         ICM   @01,3,NXTUCB(@10)                                   0336 00709000
         L     @10,LOOKUP                                          0336 00710000
         SLR   @03,@03                                             0336 00711000
         ICM   @03,3,NXTUCB(@10)                                   0336 00712000
         SLR   @10,@10                                             0336 00713000
         IC    @10,UCBCPU(,@03)                                    0336 00714000
         X     @10,@CF00036                                        0336 00715000
         STC   @10,UCBCPU(,@01)                                    0336 00716000
*                       UCBIORST=ON;   /* INDICATE LAST PATH STARTED    00717000
*                                         WAS THRU DEAD CPU          */ 00718000
         L     @10,LOOKUP                                          0337 00719000
         SLR   @01,@01                                             0337 00720000
         ICM   @01,3,NXTUCB(@10)                                   0337 00721000
         OI    UCBIORST(@01),B'10000000'                           0337 00722000
*                       IF UCBRESVH=ON /*CHECK IF RESERVE OUTSTANDING*/ 00723000
*                          THEN UCBCRHRV=ON;/*YES,RESERVE ON DEAD CPU*/ 00724000
         L     @10,LOOKUP                                          0338 00725000
         SLR   @01,@01                                             0338 00726000
         ICM   @01,3,NXTUCB(@10)                                   0338 00727000
         TM    UCBRESVH(@01),B'00010000'                           0338 00728000
         BNO   @RF00338                                            0338 00729000
         L     @10,LOOKUP                                          0339 00730000
         SLR   @01,@01                                             0339 00731000
         ICM   @01,3,NXTUCB(@10)                                   0339 00732000
         OI    UCBCRHRV(@01),B'00001000'                           0339 00733000
*                          ELSE;                                   0340 00734000
@RF00338 DS    0H                                                  0341 00735000
*                       IF UCBSPST=ON  /* ASSOCIATED SENSE IOQ ?     */ 00736000
*                          THEN IF IOQENQ=ON&  /* IOQ ON A QUEUE ?   */ 00737000
         L     @10,LOOKUP                                          0341 00738000
         SLR   @01,@01                                             0341 00739000
         ICM   @01,3,NXTUCB(@10)                                   0341 00740000
         TM    UCBSPST(@01),B'00100000'                            0341 00741000
         BNO   @RF00341                                            0341 00742000
*                                  IOQSLCH=ON  /*QUEUED ON SENSE LCH?*/ 00743000
*                                  THEN UCBCRHSN=ON;/*NEED CRH SENSE */ 00744000
         L     @10,LOOKUP                                          0342 00745000
         SLR   @01,@01                                             0342 00746000
         ICM   @01,3,NXTUCB(@10)                                   0342 00747000
         AL    @01,@CF03026                                        0342 00748000
         L     @01,UCBIOQ-508(,@01)                                0342 00749000
         TM    IOQENQ(@01),B'11000000'                             0342 00750000
         BNO   @RF00342                                            0342 00751000
         L     @10,LOOKUP                                          0343 00752000
         SLR   @01,@01                                             0343 00753000
         ICM   @01,3,NXTUCB(@10)                                   0343 00754000
         OI    UCBCRHSN(@01),B'00000100'                           0343 00755000
*                                  ELSE;                           0344 00756000
@RF00342 DS    0H                                                  0345 00757000
*                          ELSE;                                   0345 00758000
@RF00341 DS    0H                                                  0346 00759000
*                       END;                                       0346 00760000
*                  ELSE;               /*NOT LAST I/O STARTED TO DEAD*/ 00761000
@RF00334 DS    0H                                                  0348 00762000
*          ELSE;                       /* ZERO UCB PTR,BUMP TO NEXT  */ 00763000
@RF00333 DS    0H                                                  0349 00764000
*       LOOKUP=LOOKUP+TWO;             /* BUMP TO NEXT ENTRY IN UCB     00765000
*                                         LOOKUP TABLE - POINT TO  0349 00766000
*                                         NEXT UCB                   */ 00767000
         LA    @10,2                                               0349 00768000
         AL    @10,LOOKUP                                          0349 00769000
         ST    @10,LOOKUP                                          0349 00770000
*    END;                                                          0350 00771000
@DE00332 L     @10,LOOKUP                                          0350 00772000
         CLC   NXTUCB(2,@10),TABLEND                               0350 00773000
         BNE   @DL00332                                            0350 00774000
*    IRTCHMSK=IOXCHMSK;                /* INSURE THAT ALL LOGICAL  0351 00775000
*                                         CHANNEL QUEUES ARE       0351 00776000
*                                         SEARCHED           @ZA16177*/ 00777000
         L     @10,CVTPTR                                          0351 00778000
         L     @01,IOCOMPTR(,@10)                                  0351 00779000
         L     @01,IOCXPTR(,@01)                                   0351 00780000
         LH    @10,IOXCHMSK(,@01)                                  0351 00781000
         L     @03,LCCAPTR                                         0351 00782000
         ST    @10,IRTCHMSK+736(,@03)                              0351 00783000
