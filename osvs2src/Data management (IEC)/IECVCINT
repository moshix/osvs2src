*/*                                                                  */ 00808000
*/*                       EXIT ROUTINE                               */ 00809000
*/*                                                                  */ 00810000
*/********************************************************************/ 00811000
*                                                                  0357 00812000
*/********************************************************************/ 00813000
*/*  ISSUE CRH ACTIVATION MESSAGE VIA RECORD FACILITY                */ 00814000
*/********************************************************************/ 00815000
*    GEN CODE REFS(IEA970I);                                       0357 00816000
     RECORD   TYPE=WTO,DATAADR=IEA970I,BUFFER=NO,RCVRY=SETFRR,LENGTH=LIX00817000
               EA970I                                                   00818000
*    RETCODE=ZERO;                     /* RETURN CODE = 0 ACTIVATION    00819000
*                                         COMPLETED SUCCESSFULLY     */ 00820000
         SLR   RETCODE,RETCODE                                     0358 00821000
*/********************************************************************/ 00822000
*/*  CLEANUP, SET UP RETURN CODE AND RETURN                          */ 00823000
*/********************************************************************/ 00824000
*CRHAEXIT: ;                           /* EXIT LINKAGE               */ 00825000
CRHAEXIT DS    0H                                                  0360 00826000
*    IF IOSUP=ON                       /* DID I TURN ON IOS SUPER    */ 00827000
*       THEN DO;                       /* YES I DID                  */ 00828000
         TM    IOSUP,B'10000000'                                   0360 00829000
         BNO   @RF00360                                            0360 00830000
*            PSAIOSUP=OFF;             /* TURN IT OFF                */ 00831000
         NI    PSAIOSUP,B'01111111'                                0362 00832000
*            IOSUP=OFF;                /* INDICATE IOSUP OFF         */ 00833000
         NI    IOSUP,B'01111111'                                   0363 00834000
*            END;                                                  0364 00835000
*    RFY (WRK4,WRK5) RSTD;                                         0365 00836000
