         BNO   @RF00535                                            0535 01546000
*               CRCAFTIM=OFF;          /* TURN OF 1ST TIME SW@ZM30496*/ 01547000
         NI    CRCAFTIM(CRCAPTR),B'11110111'                       0537 01548000
*               CRCAITIM=CRCAITIM&&CRCAITIM;         /*      @ZM30496*/ 01549000
         XC    CRCAITIM(8,CRCAPTR),CRCAITIM(CRCAPTR)               0538 01550000
*                                       /*ZERO INTERRUPT TIME@ZM30496*/ 01551000
*               TQEOFF=ON;             /* TURN ON TQE FLAGS  @ZM30496*/ 01552000
         L     @10,TQEPTR                                          0539 01553000
         OI    TQEOFF(@10),B'10000000'                             0539 01554000
*               TQETYPE=REALTQE;       /*                    @ZM30496*/ 01555000
         OI    TQETYPE(@10),B'00000011'                            0540 01556000
*               TQECRH=ON;             /*                    @ZM30496*/ 01557000
         OI    TQECRH(@10),B'00100000'                             0541 01558000
*            END;                      /*                    @ZM30496*/ 01559000
*/********************************************************************/ 01560000
*/*                                                          @ZM30496*/ 01561000
*/*     THIS SEGMENT COMPARES THE TIME OF THE LAST INTERRUPT TO THE  */ 01562000
*/*     CURRENT TIME.  IF THE DIFFERENCE IS LESS THAN TWO SECONDS,   */ 01563000
*/*     THE TIME INTERVAL WE NEXT WANT TO BE DISPATCHED IN IS SET.   */ 01564000
*/*     OTHERWISE, AN INTERRUPT IS SIMULATED AND THEN THE TIME       */ 01565000
*/*     INTERVAL IS SET.                                             */ 01566000
*/********************************************************************/ 01567000
*                                                                  0543 01568000
*    IF CWORD1-IWORD1<TWO              /* DIFF<2 SEC?        @ZM30496*/ 01569000
*       THEN;                          /*   DO NOT DO SIMINT @ZM30496*/ 01570000
@RF00535 L     @10,CWORD1(,CRCAPTR)                                0543 01571000
         SL    @10,IWORD1(,CRCAPTR)                                0543 01572000
         CL    @10,@CF00052                                        0543 01573000
         BL    @RT00543                                            0543 01574000
*       ELSE DO;                       /*                    @ZM30496*/ 01575000
*                                                                  0545 01576000
*    RFY   (WRK4,WRK5) RSTD;                                       0546 01577000
*CRHT100:                                                          0547 01578000
*            WRK4=SIMADDR;             /* LOAD DEVICE ADDRESS FOR  0547 01579000
*                                         SIMULATED INTERRUPT        */ 01580000
CRHT100  L     WRK4,SIMADDR                                        0547 01581000
*            GEN CODE(IOSINTRP UCB=(WRK4),VAR=2,BRANCH=YES);       0548 01582000
         IOSINTRP UCB=(WRK4),VAR=2,BRANCH=YES                           01583000
*            END;                                                  0549 01584000
*                                                                  0549 01585000
*    TIMEWRK1=IWORD1+TWO;              /* FOR NEXT TIME TO   @ZM30496*/ 01586000
@RT00543 LA    @10,2                                               0550 01587000
         AL    @10,IWORD1(,CRCAPTR)                                0550 01588000
         ST    @10,TIMEWRK1                                        0550 01589000
*                                      /* GET DISPATCHED             */ 01590000
*    RFY   (WRK4,WRK5) UNRSTD;                                     0551 01591000
*/********************************************************************/ 01592000
*/* ENQ TQE TO GET US DISPATCHED AGAIN IN 2.5 SECONDS.       @ZM30496*/ 01593000
*/********************************************************************/ 01594000
*    RFY (WRK1,WRK4,WRK5) RSTD;             /*               @ZM30496*/ 01595000
*    TQEVAL=TIMEWORK;                       /* TIMER VALUE   @ZM30496*/ 01596000
         L     @10,TQEPTR                                          0553 01597000
         MVC   TQEVAL(8,@10),TIMEWORK                              0553 01598000
*/********************************************************************/ 01599000
*/*    GET LOCK AND ENQ TQE   NOTE: NON-STANDARD LINKAGE     @ZM30496*/ 01600000
*/*                                RETURN ON REGISTER 2,             */ 01601000
*/*                                NO SAVE AREA PASSED.              */ 01602000
*/********************************************************************/ 01603000
*    GEN CODE DEFS(CRHT150,CRHT160)                                0554 01604000
*REFS(CVTQTE00,TQEAREA,PARMSREG,TQERET,ENTRYREG,WRK4,WRK5);/*@ZM30496*/ 01605000
CRHT150  SETLOCK OBTAIN,TYPE=DISP,MODE=UNCOND,REGS=SAVE,               C01606000
               RELATED=(IECVCINT,CRHT160)                      @ZM30496 01607000
         L     WRK4,FLCCVT-PSA(REG0)        CVT ADDRESS        @ZM30496 01608000
         L     ENTRYREG,CVTQTE00-CVT(WRK4)  ADDR OF TQE ENQ RTN@ZM30496 01609000
         LA    PARMSREG,TQEAREA             TQE ADDRESS        @ZM30496 01610000
         BALR  TQERET,ENTRYREG         GO TO TIMER SLIH ENQ RTN@ZM30496 01611000
         LA    SAVEPTR,MYSAVE2         RESTORE SAVE AREA ADDR  @ZM30496 01612000
CRHT160  SETLOCK RELEASE,TYPE=DISP,RELATED=(IECVCINT,CRHT150),         C01613000
               REGS=SAVE                                       @ZM30496 01614000
*    RFY (WRK4,WRK5,WRK1) UNRSTD;                                  0555 01615000
*/********************************************************************/ 01616000
*/*                  EXIT LINKAGE                                    */ 01617000
*/********************************************************************/ 01618000
*CRHTEXIT:                                                         0556 01619000
*    GEN CODE;                                                     0556 01620000
CRHTEXIT DS    0H                                                  0556 01621000
         L     RETRNREG,SAVERET2         RESTORE RETURN REG    @ZM30496 01622000
     BR    RETRNREG                    RETURN TO CALLER                 01623000
*/********************************************************************* 01624000
**                                                                 0557 01625000
** ENTRY NAME - IECVCRHV                                           0557 01626000
**                                                                 0557 01627000
** DESCRIPTIVE NAME - CRH INTERFACE FOR VARYING REMOTE CHANNEL ONLINE   01628000
**                                                                 0557 01629000
** FUNCTION - THIS ENTRY IS CALLED BY VARY MODULE IEEVCPU TO TEST  0557 01630000
**            WHETHER A REMOTE CHANNEL (A CHANNEL ON THE DEAD CPU) 0557 01631000
**            IS OPERATIONAL BY MEANS OF THE STIDC INSTRUCTION.    0557 01632000
**                                                                 0557 01633000
** LINKAGE - STANDARD LINKAGE IS USED                              0557 01634000
**                                                                 0557 01635000
** INPUT                                                           0557 01636000
**     REGISTERS - SAVEPTR  (REGISTER 13) POINTS TO 18 WORD SAVE AREA   01637000
**                 RETRNREG (REGISTER 14) ADDRESS OF RETURN POINT  0557 01638000
**                 ENTRYREG (REGISTER 15) ADDRESS OF IECVCRHT      0557 01639000
**                 PARMSREG (REGISTER  1) CHANNEL ID FOR STIDC     0557 01640000
**                                                                 0557 01641000
** OUTPUT                                                          0557 01642000
**                                                                 0557 01643000
**     REGISTERS - PARMSREG (REGISTER 1)  HIGH ORDER BYTE WILL     0557 01644000
**                            CONTAIN CONDITION CODE FROM THE STIDC     01645000
**     DATA AREAS - LOCATION X'A8' WILL CONTAIN THE INFORMATION STORED  01646000
**                            AS A RESULT OF THE STIDC.            0557 01647000
**                                                                 0557 01648000
** EXIT - NORMAL - RETURN TO CALLER WITH CONDITION CODE IN REGISTER     01649000
**                                                                 0557 01650000
** EXIT - ERROR - NONE                                             0557 01651000
**                                                                 0557 01652000
**********************************************************************/ 01653000
*/* IECVCRHV: ENTRY                   ENTRY POINT FOR VARY INTERFACE */ 01654000
*/********************************************************************/ 01655000
*/*      STANDARD ENTRY LINKAGE                                      */ 01656000
*/********************************************************************/ 01657000
*    RFY   (PARMSREG) RSTD;                                        0557 01658000
*    GEN   CODE DEFS(IECVCRHV)                                     0558 01659000
*          REFS(IECVCRHV,RETRNREG,BASE,SAVEPTR,ENTRYREG,MYSAVE,CALLER); 01660000
IECVCRHV   DS 0F                           ENTRY POINT                  01661000
     STM   RETRNREG,BASE,SAVREGS(SAVEPTR)  SAVE CALLERS REGISTERS       01662000
     LR    BASE,ENTRYREG                   LOAD BASE REGISTER           01663000
     USING IECVCRHV,BASE                   INDICATE ADDRESSABILITY      01664000
     LA    RETRNREG,MYSAVE                 GET ADDRESS OF MY SAVE AREA  01665000
     ST    RETRNREG,CALLED(SAVEPTR)        STORE IN CALLERS SAVE AREA   01666000
     ST    SAVEPTR,CALLER(RETRNREG)        SAVE PTR TO CALLERS AREA     01667000
     LR    SAVEPTR,RETRNREG                POINT REG13 TO MY SAVE AREA  01668000
     STNSM SAVEMASK,DISABLE                DISABLE FOR I/O AND EXT.     01669000
*/********************************************************************/ 01670000
*/*    VARY WILL CALL IECVCRHV BEFORE CRH HAS BEEN ACTIVATED IF      */ 01671000
*/*    HE IS VARYING THE FIRST REMOTE CHANNEL ONLINE. THEREFORE      */ 01672000
*/*    IECVCRHV CAN NOT DEPEND ON CVTCRCA TO POINT TO THE CRCA.      */ 01673000
*/********************************************************************/ 01674000
*    CRCAPTR=ADDR(IECVCRCA);           /* GET POINTER TO CRCA        */ 01675000
         L     CRCAPTR,@CA02779                                    0559 01676000
*/********************************************************************/ 01677000
*/*    MAKE CRH CONNECTION TO REQUESTED CHANNEL AND ISSUE STORE      */ 01678000
*/*    CHANNEL ID ACROSS CRH CONNECTION USING CHANNEL 6              */ 01679000
*/********************************************************************/ 01680000
*    CRCACHAN=PARMSREG;                 /* INDICATE CHANNEL ID       */ 01681000
         STH   PARMSREG,CRCACHAN(,CRCAPTR)                         0560 01682000
*    CRCAMCWF=CRCACHAN(ONE:ONE);        /* SET UP MCW TO MAKE CRH  0561 01683000
*                                          CONNECTION TO CHANNEL     */ 01684000
         MVC   CRCAMCWF(1,CRCAPTR),CRCACHAN(CRCAPTR)               0561 01685000
*    CRCAMCWI=ON;                       /* INDICATE CRH MCW          */ 01686000
*    CRCAMCWM=ON;                       /* INDICATE MAKE CONNECTION  */ 01687000
         OI    CRCAMCWI(CRCAPTR),B'00110000'                       0563 01688000
*    CRCADIAG=ON;                       /* INDICATE CRH CONNECTION 0564 01689000
*                                          OUTSTANDING               */ 01690000
         OI    CRCADIAG(CRCAPTR),B'10000000'                       0564 01691000
*    PARMSREG=SIMADDR;                  /* USE CHANNEL 6 FOR STIDC   */ 01692000
         L     PARMSREG,SIMADDR                                    0565 01693000
*    GEN CODE REFS(CRCA,CRCAPTR,CRCAMCW) SETS(PARMSREG);           0566 01694000
     USING CRCA,CRCAPTR                                                 01695000
     DC  X'8300',S(CRCAMCW)             ISSUE DIAGNOSE                  01696000
     DROP  CRCAPTR                                                      01697000
     STIDC  AZERO(PARMSREG)             ISSUE STORE CHANNEL ID          01698000
     BALR   PARMSREG,AZERO              SAVE CONDITION CODE             01699000
*/********************************************************************/ 01700000
*/*    BREAK CRH CONNECTION                                          */ 01701000
*/********************************************************************/ 01702000
*    CRCAMCWM=OFF;                      /* INDICATE BREAK CONNECTION */ 01703000
         NI    CRCAMCWM(CRCAPTR),B'11101111'                       0567 01704000
*    GEN CODE REFS(CRCA,CRCAPTR,CRCAMCW);                          0568 01705000
     USING CRCA,CRCAPTR                                                 01706000
     DC  X'8300',S(CRCAMCW)             ISSUE DIAGNOSE                  01707000
     DROP  CRCAPTR                                                      01708000
*    CRCADIAG=OFF;                      /* INDICATE NO CRH CONNECTION   01709000
*                                          OUTSTANDING               */ 01710000
         NI    CRCADIAG(CRCAPTR),B'01111111'                       0569 01711000
*/********************************************************************/ 01712000
*/*                  EXIT LINKAGE                                    */ 01713000
*/********************************************************************/ 01714000
*CRHVEXIT:                                                         0570 01715000
*    RFY WRK4 RSTD;                                                0570 01716000
CRHVEXIT DS    0H                                                  0571 01717000
*    GEN CODE;                                                     0571 01718000
     IC    WRK4,SAVEMASK               GET SAVED SYSTEM MASK            01719000
     EX    WRK4,STOSMV                 RESTORE SYSTEM MASK              01720000
     L     SAVEPTR,CALLER(SAVEPTR)     GET PTR TO CALLERS SAVE AREA     01721000
     LM    RETRNREG,REG0,SAVREG14(SAVEPTR) RESTORE REGISTERS 14,15,0    01722000
     LM    CRCAPTR,BASE,SAVREG2(SAVEPTR)  RESTORE REGISTERS,SKIP REG 1  01723000
     BR    RETRNREG                    RETURN TO CALLER WITH RC         01724000
STOSMV  STOSM  SAVEMASK,AZERO          RESTORE SYSTEM MASK              01725000
*    RFY (PARMSREG,WRK4) UNRSTD;                                   0572 01726000
*/********************************************************************* 01727000
**                                                                 0573 01728000
** PROCEDURE NAME - BACKOUT                                        0573 01729000
**                                                                 0573 01730000
** DESCRIPTIVE NAME - CRH INTERNAL BACK OUT ROUTINE                0573 01731000
**                                                                 0573 01732000
** FUNCTION - THIS INTERNAL PROCEDURE IS CALLED TO BACK OUT THE    0573 01733000
**            CHANGES MADE TO THE OS/VS2 SYSTEM BY CRH ACTIVATION  0573 01734000
**            ROUTINE IECVCRHA.                                    0573 01735000
**                                                                 0573 01736000
** INPUT                                                           0573 01737000
**                                                                 0573 01738000
**    REGISTERS - RETRNREG ( REGISTER 14) RETURN ADDRESS           0573 01739000
**                ENTRYREG ( REGISTER 15) ADDRESS OF BACKOUT       0573 01740000
**                                                                 0573 01741000
**    DATA FIELDS - LOOKUP (POINTER TO UCB LOOK UP TABLE) MUST     0573 01742000
**                  CONTAIN THE ADDRESS OF THE UCB LOOK UP TABLE   0573 01743000
**                  OR ONE OF ITS ENTRIES.                         0573 01744000
**                                                                 0573 01745000
** OUTPUT - NONE                                                   0573 01746000
**                                                                 0573 01747000
** EXIT - NORMAL - RETURN TO CALLER VIA BRANCH ON RETURN REGISTER  0573 01748000
**                                                                 0573 01749000
** EXIT - ERROR - NONE                                             0573 01750000
**********************************************************************/ 01751000
*BACKOUT: PROCEDURE OPTIONS(NOSAVEAREA);                           0573 01752000
@EL00001 L     @13,4(,@13)                                         0573 01753000
@EF00001 DS    0H                                                  0573 01754000
@ER00001 LM    @14,@12,12(@13)                                     0573 01755000
         BR    @14                                                 0573 01756000
BACKOUT  STM   @14,@12,12(@13)                                     0573 01757000
*/********************************************************************/ 01758000
*/*       ESTABLISH ADDRESSABILITY                                   */ 01759000
*/********************************************************************/ 01760000
*    GEN CODE SETS(BASE) REFS(ENTRYREG);                           0574 01761000
     LR  BASE,ENTRYREG                 ESTABLISH                        01762000
     USING BACKOUT,BASE                     ADDRESSABILITY              01763000
*/********************************************************************/ 01764000
*/*  CHANGE ADDRESS OF SLIH IN THE I/O FLIH TO POINT BACK TO IOS SLIH*/ 01765000
*/********************************************************************/ 01766000
*    IF SLIHCHNG=ON                    /* FLIH POINT TO CRH SLIH ?   */ 01767000
*       THEN DO;                       /* YES                        */ 01768000
         TM    SLIHCHNG,B'00010000'                                0575 01769000
         BNO   @RF00575                                            0575 01770000
*            SLIHPTR=ADDR(IECINT);     /* RESTORE POINTER TO I/O SLIH*/ 01771000
         L     @10,@CA02782                                        0577 01772000
         L     @03,CVTPTR                                          0577 01773000
         L     @01,IOCOMPTR(,@03)                                  0577 01774000
         MVC   SLIHPTR-IEAVEIOI(4,@10),IOCINT(@01)                 0577 01775000
*            SLIHCHNG=OFF;             /* TURN OFF INDICATOR         */ 01776000
         NI    SLIHCHNG,B'11101111'                                0578 01777000
*            END;                                                  0579 01778000
*/********************************************************************/ 01779000
*/*  TAKE HOOKS OUT OF IOS MAINLINE IECIOSCN                         */ 01780000
*/********************************************************************/ 01781000
*    IF TCHHOOK=ON                     /* HOOK STILL IN ?            */ 01782000
*       THEN DO;                       /*  YES                       */ 01783000
@RF00575 TM    TCHHOOK,B'00001000'                                 0580 01784000
         BNO   @RF00580                                            0580 01785000
*            IECVHK1A=SAVEHK1;         /*RESTORE OVERLAID INSTRUCTION*/ 01786000
         L     @10,@CA02759                                        0582 01787000
         MVC   IECVHK1A-IECVHK1A(4,@10),SAVEHK1                    0582 01788000
*            TCHHOOK=OFF;              /* INDICATE HOOK OUT          */ 01789000
         NI    TCHHOOK,B'11110111'                                 0583 01790000
*            END;                                                  0584 01791000
*    IF SIOHOOK=ON                     /* HOOK STILL IN ?            */ 01792000
*       THEN DO;                       /*  YES                       */ 01793000
@RF00580 TM    SIOHOOK,B'00000100'                                 0585 01794000
         BNO   @RF00585                                            0585 01795000
*            IECVHK2A=SAVEHK2;         /*RESTORE OVERLAID INSTRUCTION*/ 01796000
         L     @10,@CA02761                                        0587 01797000
         MVC   IECVHK2A-IECVHK2A(4,@10),SAVEHK2                    0587 01798000
*            SIOHOOK=OFF;              /* INDICATE HOOK OUT          */ 01799000
         NI    SIOHOOK,B'11111011'                                 0588 01800000
*            END;                                                  0589 01801000
*    IF SNSHOOK=ON                     /* HOOK STILL IN ?            */ 01802000
*       THEN DO;                       /*  YES                       */ 01803000
@RF00585 TM    SNSHOOK,B'00000010'                                 0590 01804000
         BNO   @RF00590                                            0590 01805000
*            IECVHK3A=SAVEHK3;         /*RESTORE OVERLAID INSTRUCTION*/ 01806000
         L     @10,@CA02763                                        0592 01807000
         MVC   IECVHK3A-IECVHK3A(4,@10),SAVEHK3                    0592 01808000
*            SNSHOOK=OFF;              /* INDICATE HOOK OUT          */ 01809000
         NI    SNSHOOK,B'11111101'                                 0593 01810000
*            END;                                                  0594 01811000
*/********************************************************************/ 01812000
*/*  SCAN THROUGH ALL UCBS FOR I/O ON DEAD CPU                       */ 01813000
*/********************************************************************/ 01814000
*    RFY UCBOB BASED(NXTUCB);         /* BASE UCBS ON UCB LOOKUP   0595 01815000
*                                         TABLE ENTRIES              */ 01816000
@RF00590 DS    0H                                                  0596 01817000
*    IF SCANUCBS=OFF                  /* NEED TO SWITCH UCBS BACK ?  */ 01818000
*       THEN RETURN;                  /* NO, RETURN TO CALLER        */ 01819000
         TM    SCANUCBS,B'00000001'                                0596 01820000
         BZ    @RT00596                                            0596 01821000
*       ELSE;                         /* YES, SCANUCBS               */ 01822000
*    IF DOINSCAN=ON                   /* RECURSIVE CALL BY FRR?      */ 01823000
*       THEN LOOKUP=LOOKUP+TWO;       /* YES,BUMP TO NEXT UCB        */ 01824000
         TM    DOINSCAN,B'10000000'                                0599 01825000
         BNO   @RF00599                                            0599 01826000
         LA    @10,2                                               0600 01827000
         AL    @10,LOOKUP                                          0600 01828000
         ST    @10,LOOKUP                                          0600 01829000
*       ELSE DOINSCAN=ON;             /* INDICATE FIRST ENTRY        */ 01830000
         B     @RC00599                                            0601 01831000
@RF00599 OI    DOINSCAN,B'10000000'                                0601 01832000
*    DO WHILE NXTUCB^=TABLEND;         /*CHECK FOR END OF UCB LOOKUP */ 01833000
@RC00599 B     @DE00602                                            0602 01834000
@DL00602 DS    0H                                                  0603 01835000
*       IF NXTUCB^=ZERO                /* ENSURE NONZERO UCB POINTER */ 01836000
*         THEN DO;                     /* PROCESS UCB                */ 01837000
         L     @10,LOOKUP                                          0603 01838000
         ICM   @03,3,NXTUCB(@10)                                   0603 01839000
         BZ    @RF00603                                            0603 01840000
*              IF UCBIORST=ON | UCBCRHRV=ON | UCBCRHSN=ON          0605 01841000
*                                      /* CHECK IF LAST I/O STARTED     01842000
*                                    WAS THRU CRH CONNECTION OR IF CRH  01843000
*                                    PATH RESERVED @ZA15805  @ZA16177*/ 01844000
*                THEN DO;              /* YES                        */ 01845000
         L     @10,LOOKUP                                          0605 01846000
         SLR   @01,@01                                             0605 01847000
         ICM   @01,3,NXTUCB(@10)                                   0605 01848000
         TM    UCBIORST(@01),B'10001100'                           0605 01849000
         BZ    @RF00605                                            0605 01850000
*                     UCBCPU=UCBCPU&&ONE;    /* SWITCH CPUID         */ 01851000
         L     @10,LOOKUP                                          0607 01852000
         SLR   @01,@01                                             0607 01853000
         ICM   @01,3,NXTUCB(@10)                                   0607 01854000
         L     @10,LOOKUP                                          0607 01855000
         SLR   @03,@03                                             0607 01856000
         ICM   @03,3,NXTUCB(@10)                                   0607 01857000
         SLR   @10,@10                                             0607 01858000
         IC    @10,UCBCPU(,@03)                                    0607 01859000
         X     @10,@CF00036                                        0607 01860000
         STC   @10,UCBCPU(,@01)                                    0607 01861000
*                     UCBIORST=OFF;     /* TURN OFF INDICATOR THAT I/O  01862000
*                                           WAS STARTED THRU CRH     */ 01863000
*                     UCBCRHRV=OFF;     /* ENSURE THAT INDICATOR FOR    01864000
*                                          RESERVE ACROSS CRH IS OFF */ 01865000
*                     UCBCRHSN=OFF;    /* ENSURE CRH SENSE BIT OFF 0610 01866000
*                                                            @ZA16177*/ 01867000
         L     @10,LOOKUP                                          0610 01868000
         SLR   @01,@01                                             0610 01869000
         ICM   @01,3,NXTUCB(@10)                                   0610 01870000
         NI    UCBIORST(@01),B'01110011'                           0610 01871000
*                     END;                                         0611 01872000
*                ELSE;                 /* LAST I/O STARTED NOT CRH   */ 01873000
@RF00605 DS    0H                                                  0613 01874000
*              END;                                                0613 01875000
*          ELSE;                       /* ZERO UCB PTR,BUMP TO NEXT  */ 01876000
@RF00603 DS    0H                                                  0615 01877000
*       LOOKUP=LOOKUP+TWO;             /* BUMP TO NEXT ENTRY IN UCB     01878000
*                                         LOOK UP TABLE              */ 01879000
         LA    @10,2                                               0615 01880000
         AL    @10,LOOKUP                                          0615 01881000
         ST    @10,LOOKUP                                          0615 01882000
*    END;                              /* END DO WHILE               */ 01883000
@DE00602 L     @10,LOOKUP                                          0616 01884000
         CLC   NXTUCB(2,@10),TABLEND                               0616 01885000
         BNE   @DL00602                                            0616 01886000
*    SCANUCBS=OFF;                     /* INDICATE UCB SCAN DONE     */ 01887000
         NI    SCANUCBS,B'11111110'                                0617 01888000
*    DOINSCAN=OFF;                     /* INDICATE UCB SCAN DONE     */ 01889000
         NI    DOINSCAN,B'01111111'                                0618 01890000
*/********************************************************************/ 01891000
*/*               RETURN TO CALLER                                   */ 01892000
*/********************************************************************/ 01893000
*    RETURN;                           /* RETURN TO CALLER           */ 01894000
@EL00002 DS    0H                                                  0619 01895000
@EF00002 DS    0H                                                  0619 01896000
@ER00002 LM    @14,@12,12(@13)                                     0619 01897000
         BR    @14                                                 0619 01898000
*    END  BACKOUT;                                                 0620 01899000
         B     @EL00002                                            0620 01900000
*                                                                  0621 01901000
*                                                                  0621 01902000
*    DECLARE /*GENERAL PURPOSE REGISTERS */                        0621 01903000
*      GPR00P PTR(31) REG(0),                                      0621 01904000
*      GPR01P PTR(31) REG(1),                                      0621 01905000
*      GPR14P PTR(31) REG(14),                                     0621 01906000
*      GPR15P PTR(31) REG(15);                                     0621 01907000
*                                                                  0621 01908000
*    DECLARE /* COMMON VARIABLES */                                0622 01909000
*      I256C CHAR(256) BASED,                                      0622 01910000
*      I031F FIXED(31) BASED,                                      0622 01911000
*      I031P PTR(31)   BASED,                                      0622 01912000
*      I015F FIXED(15) BASED,                                      0622 01913000
*      I015P PTR(15)   BASED,                                      0622 01914000
*      I008P PTR(8)    BASED,                                      0622 01915000
*      I001C CHAR(1)   BASED;                                      0622 01916000
*      END  IECVCINT                                               0623 01917000
