         TITLE 'IECVHDET - HOT I/O DETECTION'                           00001000
IECVHDET CSECT ,                                                01S0001 00002000
@PROLOG  BALR  @12,0                                            01S0001 00003000
@PSTART  DS    0H                                               01S0001 00004000
         USING @PSTART,@12                                      01S0001 00005000
           MODID BR=YES                                                 00006000
*   STM(R0,R15,SVAREA);             /* SAVE REGS- REG 12 IS DESTROYED*/ 00007000
         STM   R0,R15,SVAREA(SVAREAP)                           01S0059 00008000
*   RFY                                                         01S0060 00009000
*    (R0,                                                       01S0060 00010000
*     R15) UNRSTD;                                              01S0060 00011000
*   PCCAPTR=PSAPCCAV;               /* ESTABLISH PCCA ADDRESSABILITY */ 00012000
         L     PCCAPTR,PSAPCCAV                                 01S0061 00013000
*   RFY                                                         01S0062 00014000
*     R15 RSTD;                                                 01S0062 00015000
*   R15=ADDR(CR2CONT);                                          01S0063 00016000
         LA    R15,CR2CONT(,SVAREAP)                            01S0063 00017000
*   GEN SETS(CR2CONT);                                          01S0064 00018000
   STCTL 2,2,0(R15)                                                     00019000
*   RFY                                                         01S0065 00020000
*     R15 UNRSTD;                                               01S0065 00021000
*                                                               01S0065 00022000
*/********************************************************************/ 00023000
*/*                                                                  */ 00024000
*/* GET ADDRESSABILITY TO THE SCD WHICH CONTAINS INFORMATION ON THE  */ 00025000
*/* PREVIOUS INTERRUPT(S) FROM THE CURRENT CHANNEL                   */ 00026000
*/*                                                                  */ 00027000
*/********************************************************************/ 00028000
*                                                               01S0066 00029000
*   IF IRTCRHIN=ON THEN             /* CRH INTERRUPT                 */ 00030000
         TM    IRTCRHIN+736(LCCAPTR),B'00010000'                01S0066 00031000
         BNO   @RF00066                                         01S0066 00032000
*     DO;                                                       01S0067 00033000
*                                                               01S0067 00034000
*       /*************************************************************/ 00035000
*       /*                                                           */ 00036000
*       /* IF THE INTERRUPT BEING PROCESSED IS A CRH INTERRUPT,      */ 00037000
*       /* ADDRESSABILITY TO THE OTHER CPU'S PCCA MUST BE            */ 00038000
*       /* ESTABLISHED. THIS PTR IS USED TO GET ADDRESSABILITY TO THE*/ 00039000
*       /* OTHER CPU'S CAT.                                          */ 00040000
*       /*                                                           */ 00041000
*       /*************************************************************/ 00042000
*                                                               01S0068 00043000
*       IF PSACPUSA=0 THEN          /* CPU 0?                        */ 00044000
         ICM   @02,3,PSACPUSA                                   01S0068 00045000
         BNZ   @RF00068                                         01S0068 00046000
*         DO;                       /* GET PCCA AND SCD PTRS FOR CPU    00047000
*                                      1                             */ 00048000
*           PCCAPTR=PCCAT00P(2);    /* LOAD PCCA PTR FOR CPU 1       */ 00049000
         L     @08,CVTPTR                                       01S0070 00050000
         L     @08,CVTPCCAT(,@08)                               01S0070 00051000
         L     PCCAPTR,PCCAT00P+4(,@08)                         01S0070 00052000
*           SCDPTR=ADDR(SCDS1);     /* LOAD SCD PTR FOR CPU 1 CHANNEL*/ 00053000
         LA    SCDPTR,SCDS1                                     01S0071 00054000
*         END;                                                  01S0072 00055000
*       ELSE                        /* CPU 1                         */ 00056000
*         DO;                       /* GET PCCA AND SCD PTRS FOR CPU    00057000
*                                      0                             */ 00058000
         B     @RC00068                                         01S0073 00059000
@RF00068 DS    0H                                               01S0074 00060000
*           PCCAPTR=PCCAT00P(1);    /* LOAD PCCA PTR FOR CPU 0       */ 00061000
         L     @08,CVTPTR                                       01S0074 00062000
         L     @08,CVTPCCAT(,@08)                               01S0074 00063000
         L     PCCAPTR,PCCAT00P(,@08)                           01S0074 00064000
*           SCDPTR=ADDR(SCDS0);     /* LOAD SCD PTR FOR CPU 0 CHANNEL*/ 00065000
         LA    SCDPTR,SCDS0                                     01S0075 00066000
*         END;                                                  01S0076 00067000
*     END;                                                      01S0077 00068000
*   ELSE                            /* NOT CRH-PCCA PTR CORRECT AS IS*/ 00069000
*     IF PSACPUSA=0 THEN            /* CPU 0?                        */ 00070000
         B     @RC00066                                         01S0078 00071000
@RF00066 ICM   @15,3,PSACPUSA                                   01S0078 00072000
         BNZ   @RF00078                                         01S0078 00073000
*       SCDPTR=ADDR(SCDS0);         /* POINT TO CPU 0'S SCDS         */ 00074000
         LA    SCDPTR,SCDS0                                     01S0079 00075000
*     ELSE                          /* CPU 1                         */ 00076000
*       SCDPTR=ADDR(SCDS1);         /* POINT TO CPU 1'S SCDS         */ 00077000
         B     @RC00078                                         01S0080 00078000
@RF00078 LA    SCDPTR,SCDS1                                     01S0080 00079000
*   IF SCDPTR^=0 THEN               /* SCD EXISTS                    */ 00080000
@RC00078 DS    0H                                               01S0081 00081000
@RC00066 LTR   SCDPTR,SCDPTR                                    01S0081 00082000
         BZ    @RF00081                                         01S0081 00083000
*     DO;                           /* CONTINUE PROCESSING           */ 00084000
*       SCDPTR=SCDPTR+(EVAL(DEVAD/256)*LENGTH(SCD));/* GET SCD       */ 00085000
*                                                               01S0083 00086000
         LR    @00,DEVAD                                        01S0083 00087000
         SRDA  @00,32                                           01S0083 00088000
         D     @00,@CF00076                                     01S0083 00089000
         MH    @01,@CH00117                                     01S0083 00090000
         ALR   SCDPTR,@01                                       01S0083 00091000
*       /*************************************************************/ 00092000
*       /*                                                           */ 00093000
*       /* ZERO SCD COUNTS IF SUCCESSFUL SIO (CC=0) DONE TO CHANNEL  */ 00094000
*       /* SINCE LAST INVOCATION OF THIS MODULE.                     */ 00095000
*       /*                                                           */ 00096000
*       /*************************************************************/ 00097000
*                                                               01S0084 00098000
*       WORKREG=DEVAD;              /* GET DEVICE ADDRESS            */ 00099000
         LR    WORKREG,DEVAD                                    01S0084 00100000
*       SRL(WORKREG,8);             /* ISOLATE CHANNEL ADDRESS       */ 00101000
         SRL   WORKREG,8                                        01S0085 00102000
*       RFY                                                     01S0086 00103000
*         CAT BASED(ADDR(PCCACAT)+WORKREG*LENGTH(CAT));         01S0086 00104000
*       IF CATSOL=ON THEN           /* SIO CC=0 DONE                 */ 00105000
         LA    @08,PCCACAT(,PCCAPTR)                            01S0087 00106000
         LR    @15,WORKREG                                      01S0087 00107000
         SLA   @15,3                                            01S0087 00108000
         ALR   @08,@15                                          01S0087 00109000
         TM    CATSOL(@08),B'00001000'                          01S0087 00110000
         BNO   @RF00087                                         01S0087 00111000
*         DO;                                                   01S0088 00112000
*           SCDCNTS=0;              /* ZERO SCD COUNTERS             */ 00113000
         SLR   @15,@15                                          01S0089 00114000
         ST    @15,SCDCNTS(,SCDPTR)                             01S0089 00115000
*           CATSOL=OFF;             /* RESET SUCCESSFUL SIO BIT      */ 00116000
         NI    CATSOL(@08),B'11110111'                          01S0090 00117000
*         END;                                                  01S0091 00118000
*                                                               01S0091 00119000
*       /*************************************************************/ 00120000
*       /*                                                           */ 00121000
*       /* IF HOT DEVICE IS DASD, MODIFY TO LOOK LIKE HOT CHANNEL SO */ 00122000
*       /* THAT RECOVERY WILL BE PERFORMED ON A FULL CHANNEL BASIS.  */ 00123000
*       /* THIS IS NECESSARY FOR SHARED DEVICES THAT ARE RESERVED.   */ 00124000
*       /*                                                           */ 00125000
*       /*************************************************************/ 00126000
*                                                               01S0092 00127000
*       UCBPTR=UCBPTR+LENGTH(UCB);  /* ADJUST UCB PTR PAST PREFIX    */ 00128000
@RF00087 AH    UCBPTR,@CH01644                                  01S0092 00129000
*                                                               02S0093 00130000
*/********************************************************************/ 00131000
*/*                                                                  */ 00132000
*/* SEGMENT NAME - CHECKINT                                          */ 00133000
*/*                                                                  */ 00134000
*/* DESCRIPTIVE NAME - EXAMINE CURRENT INTERRUPT                     */ 00135000
*/*                                                                  */ 00136000
*/* FUNCTION -                                                       */ 00137000
*/*   THIS SEGMENT DETERMINES IF THE CURRENT INTERRUPT IS A REPEAT   */ 00138000
*/*   OF THE PREVIOUS INTERRUPT. IF NOT, THE SCD IS SET UP TO        */ 00139000
*/*   REFLECT THE CURRENT INTERRUPT. IF SO, THE REPEAT COUNT FIELD   */ 00140000
*/*   IN THE SCD IS INCREMENTED BY 1, THE TIMEOUT COUNT FIELD IS     */ 00141000
*/*   INCREMENTED IF NECESSARY, AND THE SCD FLAGS AND STATUS ARE     */ 00142000
*/*   ADJUSTED. IF EITHER THE REPEAT COUNT OR TIMEOUT COUNT          */ 00143000
*/*   THRESHOLDS HAVE BEEN EXCEEDED, SEGMENT SCHEDREC IS USED TO     */ 00144000
*/*   SET UP FOR AND SCHEDULE HOT I/O RECOVERY.                      */ 00145000
*/*                                                                  */ 00146000
*/********************************************************************/ 00147000
*                                                               02S0093 00148000
*       IF(IRTHOTD=OFF&             /* HOT CHANNEL AND               */ 00149000
*           SCDHOTD=OFF&            /* PREVIOUS INTERRUPT ALSO HOT      00150000
*                                      CHANNEL AND...                */ 00151000
*           SCDTIME1^=0)|           /* SCD IS INITIALIZED OR...      */ 00152000
*           (IRTHOTD=ON&            /* HOT DEVICE AND                */ 00153000
*           SCDHOTD=ON&             /* PREVIOUS INTERRUPT ALSO HOT      00154000
*                                      DEVICE AND                    */ 00155000
*           DEVAD=SCDADDR) THEN     /* DEVICE ADDRESSES MATCH        */ 00156000
         TM    IRTHOTD+736(LCCAPTR),B'00001000'                 02S0093 00157000
         BNZ   @GL00001                                         02S0093 00158000
         TM    SCDHOTD(SCDPTR),B'00001000'                      02S0093 00159000
         BNZ   @GL00001                                         02S0093 00160000
         ICM   @08,15,SCDTIME1(SCDPTR)                          02S0093 00161000
         BNZ   @RT00093                                         02S0093 00162000
@GL00001 TM    IRTHOTD+736(LCCAPTR),B'00001000'                 02S0093 00163000
         BNO   @RF00093                                         02S0093 00164000
         TM    SCDHOTD(SCDPTR),B'00001000'                      02S0093 00165000
         BNO   @RF00093                                         02S0093 00166000
         CH    DEVAD,SCDADDR(,SCDPTR)                           02S0093 00167000
         BNE   @RF00093                                         02S0093 00168000
@RT00093 DS    0H                                               02S0094 00169000
*         DO;                       /* PROCESS REPEATED INTERRUPT    */ 00170000
*           SCDRCNT=SCDRCNT+1;      /* INCREMENT REPEAT COUNT        */ 00171000
         LA    @08,1                                            02S0095 00172000
         AH    @08,SCDRCNT(,SCDPTR)                             02S0095 00173000
         STH   @08,SCDRCNT(,SCDPTR)                             02S0095 00174000
*           SCDRSC1=SCDRSC1|IRTDETEC;/* OR IN DETECTION FLAGS        */ 00175000
         OC    SCDRSC1(1,SCDPTR),IRTDETEC+736(LCCAPTR)          02S0096 00176000
*           RFY                                                 02S0097 00177000
*             WORKREG RSTD;                                     02S0097 00178000
*           OLDTIME=SCDTIME1;       /* SAVE TIME OF PREVIOUS    02S0098 00179000
*                                      INTERRUPT                     */ 00180000
         L     OLDTIME,SCDTIME1(,SCDPTR)                        02S0098 00181000
*           WORKREG=ADDR(SCDTIME);  /* SET UP FOR STCK INSTRUCTION   */ 00182000
         LA    WORKREG,SCDTIME(,SCDPTR)                         02S0099 00183000
*           GEN SETS(SCDTIME);                                  02S0100 00184000
*                                  /*STORE CURRENT TIME              */ 00185000
       STCK 0(WORKREG)                                                  00186000
*           IF SCDTIME1-OLDTIME>HIDTINTV THEN/* ELAPSED TIME INTERVAL   00187000
*                                      EXCEEDED?                     */ 00188000
         L     @08,SCDTIME1(,SCDPTR)                            02S0101 00189000
         SLR   @08,OLDTIME                                      02S0101 00190000
         L     @05,@CV00080                                     02S0101 00191000
         LH    @05,HIDTINTV(,@05)                               02S0101 00192000
         CLR   @08,@05                                          02S0101 00193000
         BNH   @RF00101                                         02S0101 00194000
*             SCDTRCNT=SCDTRCNT+1;  /* YES-INCREMENT TIMEOUT COUNT   */ 00195000
         LA    @08,1                                            02S0102 00196000
         AH    @08,SCDTRCNT(,SCDPTR)                            02S0102 00197000
         STH   @08,SCDTRCNT(,SCDPTR)                            02S0102 00198000
*           RFY                                                 02S0103 00199000
*             WORKREG UNRSTD;                                   02S0103 00200000
@RF00101 DS    0H                                               02S0104 00201000
*           IF(SCDHOTD=OFF&         /* POSSIBLE HOT CHANNEL AND      */ 00202000
*               SCDRCNT>HIDTCHAN)|  /* CHANNEL THRESHOLD EXCEEDED       00203000
*                                      OR...                         */ 00204000
*               (SCDHOTD=ON&        /* POSSIBLE HOT DEVICE AND       */ 00205000
*               SCDRCNT>HIDTDEV)|   /* DEVICE THRESHOLD EXCEEDED        00206000
*                                      OR...                         */ 00207000
*               (SCDTRCNT>HIDTTIME) THEN/* TIMEOUT COUNT THRESHOLD      00208000
*                                      EXCEEDED                      */ 00209000
         TM    SCDHOTD(SCDPTR),B'00001000'                      02S0104 00210000
         BNZ   @GL00007                                         02S0104 00211000
         L     @08,@CV00080                                     02S0104 00212000
         LH    @15,SCDRCNT(,SCDPTR)                             02S0104 00213000
         CH    @15,HIDTCHAN(,@08)                               02S0104 00214000
         BH    @RT00104                                         02S0104 00215000
@GL00007 TM    SCDHOTD(SCDPTR),B'00001000'                      02S0104 00216000
         BNO   @GL00006                                         02S0104 00217000
         L     @08,@CV00080                                     02S0104 00218000
         LH    @15,SCDRCNT(,SCDPTR)                             02S0104 00219000
         CH    @15,HIDTDEV(,@08)                                02S0104 00220000
         BH    @RT00104                                         02S0104 00221000
@GL00006 L     @08,@CV00080                                     02S0104 00222000
         LH    @15,SCDTRCNT(,SCDPTR)                            02S0104 00223000
         CH    @15,HIDTTIME(,@08)                               02S0104 00224000
         BNH   @RF00104                                         02S0104 00225000
@RT00104 DS    0H                                               02S0105 00226000
*             DO;                   /* PROCESS HOT I/O DETECTED      */ 00227000
*               IF SCDTRCNT>HIDTTIME THEN/* TIMEOUT THRESHOLD   02S0106 00228000
*                                      EXCEEDED                      */ 00229000
         L     @08,@CV00080                                     02S0106 00230000
         LH    @15,SCDTRCNT(,SCDPTR)                            02S0106 00231000
         CH    @15,HIDTTIME(,@08)                               02S0106 00232000
         BNH   @RF00106                                         02S0106 00233000
*                 SCDTMOUT=ON;      /* INDICATE TIMEOUT IN SCD       */ 00234000
         OI    SCDTMOUT(SCDPTR),B'10000000'                     02S0107 00235000
*               SCDCSWST=FLCCSW(5:6);/* MOVE IN CSW STATUS           */ 00236000
@RF00106 MVC   SCDCSWST(2,SCDPTR),FLCCSW+4                      02S0108 00237000
*                                                               03S0109 00238000
*/********************************************************************/ 00239000
*/*                                                                  */ 00240000
*/* SEGMENT NAME - SCHEDREC                                          */ 00241000
*/*                                                                  */ 00242000
*/* DESCRIPTIVE NAME - SCHEDULE HOT I/O RECOVERY                     */ 00243000
*/*                                                                  */ 00244000
*/* FUNCTION -                                                       */ 00245000
*/*   THIS SEGMENT SETS UP FOR AND SCHEDULES HOT I/O RECOVERY.       */ 00246000
*/*   PROCESSING INCLUDES:                                           */ 00247000
*/*     . DISABLING THE CHANNEL OVER WHICH THE HOT INTERRUPTS        */ 00248000
*/*       HAVE BEEN RECEIVED. (THIS STEP IS BYPASSED FOR CRH         */ 00249000
*/*       INTERRUPTS)                                                */ 00250000
*/*     . SETTING THE CATNOP AND CATDSABL FLAGS FOR THE CHANNEL      */ 00251000
*/*     . ISSUING SETFRR AND OBTAINING SALLOC LOCK                   */ 00252000
*/*     . GETTING STORAGE FOR AND BUILDING AN SRB                    */ 00253000
*/*     . FREEING SALLOC LOCK AND DELETING FRR                       */ 00254000
*/*     . SCHEDULING HOT I/O RECOVERY WITH CPU AFFINITY              */ 00255000
*/*                                                                  */ 00256000
*/********************************************************************/ 00257000
*                                                               03S0109 00258000
*               SRL(DEVAD,8);       /* ISOLATE CHANNEL NUMBER        */ 00259000
         SRL   DEVAD,8                                          03S0109 00260000
*               RFY                                             03S0110 00261000
*                 WORKREG RSTD;                                 03S0110 00262000
*               WORKREG=CR2MASK;    /* INITIALIZE WORKREG MASK       */ 00263000
         L     WORKREG,CR2MASK                                  03S0111 00264000
*               SRL(WORKREG,DEVAD); /* GET MASK FOR DISABLING CHANNEL*/ 00265000
         SRL   WORKREG,0(DEVAD)                                 03S0112 00266000
*               CR2CONT=CR2CONT&&WORKREG;/* MARK CHANNEL DISABLED IN    00267000
*                                      PCCA                          */ 00268000
         L     @08,CR2CONT(,SVAREAP)                            03S0113 00269000
         XR    @08,WORKREG                                      03S0113 00270000
         ST    @08,CR2CONT(,SVAREAP)                            03S0113 00271000
*               WORKREG=ADDR(CR2CONT);/* SET UP FOR LCTL INSTRUCTION */ 00272000
         LA    WORKREG,CR2CONT(,SVAREAP)                        03S0114 00273000
*               IF IRTCRHIN=OFF THEN/* NOT CRH INTERRUPT             */ 00274000
         TM    IRTCRHIN+736(LCCAPTR),B'00010000'                03S0115 00275000
         BNZ   @RF00115                                         03S0115 00276000
*                 DO;               /* DISABLE CHANNEL               */ 00277000
*                   GEN REFS(CR2CONT);                          03S0117 00278000
     LCTL 2,2,0(WORKREG)                                                00279000
*                 END;                                          03S0118 00280000
*               RFY                                             03S0119 00281000
*                 WORKREG UNRSTD;                               03S0119 00282000
@RF00115 DS    0H                                               03S0120 00283000
*               RFY                                             03S0120 00284000
*                 CAT BASED(ADDR(PCCACAT)+DEVAD*LENGTH(CAT));   03S0120 00285000
*               CATNOP=ON;          /* SET INOPERATIVE FLAG          */ 00286000
         LA    @08,PCCACAT(,PCCAPTR)                            03S0121 00287000
         LR    @15,DEVAD                                        03S0121 00288000
         SLA   @15,3                                            03S0121 00289000
         ALR   @08,@15                                          03S0121 00290000
         OI    CATNOP(@08),B'01000000'                          03S0121 00291000
*               IF CATDSABL=OFF THEN/* CAT NOT ALREADY MARKED        */ 00292000
         TM    CATDSABL(@08),B'00000001'                        03S0122 00293000
         BNZ   @RF00122                                         03S0122 00294000
*                 DO;               /* SET UP AND SCHEDULE RECOVERY  */ 00295000
*                   CATDSABL=ON;    /* SET DISABLED FLAG             */ 00296000
         OI    CATDSABL(@08),B'00000001'                        03S0124 00297000
*                   SCDCNTS=0;      /* ZERO SCD REPEAT COUNTS        */ 00298000
         SLR   @08,@08                                          03S0125 00299000
         ST    @08,SCDCNTS(,SCDPTR)                             03S0125 00300000
*                   GEN REFS(ADDRFRR) SETS(R4,R7,R8) FLOWS(HDETFRR);    00301000
*                                                     /*ADD FRR      */ 00302000
     SETFRR A,FRRAD=ADDRFRR,WRKREGS=(R4,R7),PARMAD=(R8)                 00303000
*                   GEN SETS(R0,R1,R14,R15) REFS(R11,R12,R13);  03S0127 00304000
*                                               /*GET SALLOC LOCK    */ 00305000
GETSAL SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,DISABLED,REGS=USE,       X00306000
               RELATED=(FREESAL,'OBTAIN SALLOC LOCK')                   00307000
*                   RFY                                         03S0128 00308000
*                    (R0,                                       03S0128 00309000
*                     R1,                                       03S0128 00310000
*                     R2,                                       03S0128 00311000
*                     R4) RSTD;                                 03S0128 00312000
*                   R2=LCCAPTR;     /* SAVE LCCA PTR                 */ 00313000
         LR    R2,LCCAPTR                                       03S0129 00314000
*                   R0=LENGTH(SRB)+WORKLENG;/* LENGTH FOR GETMAIN    */ 00315000
         LA    R0,300                                           03S0130 00316000
*                   GEN SETS(R0,R1,LCCAPTR,R4,R14,R15);         03S0131 00317000
*                                        /*GET STORAGE FOR SRB       */ 00318000
     GETMAIN RU,LV=(0),BRANCH=(YES,GLOBAL),SP=245                       00319000
*                   SRBPTR=R1;      /* PTR TO STORAGE FOR SRB        */ 00320000
         LR    SRBPTR,R1                                        03S0132 00321000
*                   LCCAPTR=R2;     /* RESTORE LCCA PTR              */ 00322000
         LR    LCCAPTR,R2                                       03S0133 00323000
*                   RFY                                         03S0134 00324000
*                    (R0,                                       03S0134 00325000
*                     R1,                                       03S0134 00326000
*                     R2,                                       03S0134 00327000
*                     R4) UNRSTD;                               03S0134 00328000
*                   SRB=SRB&&SRB;   /* CLEAR SRB                     */ 00329000
         XC    SRB(44,SRBPTR),SRB(SRBPTR)                       03S0135 00330000
*                   SRBID='SRB ';   /* IDENTIFY SRB                  */ 00331000
         MVC   SRBID(4,SRBPTR),@CC02424                         03S0136 00332000
*                   SRBASCB=ADDR(IEAMASCB);/* MASTER ADDRESS SPACE   */ 00333000
         L     @11,@CV00082                                     03S0137 00334000
         ST    @11,SRBASCB(,SRBPTR)                             03S0137 00335000
*                   SRBCPAFF=PSAPCCAV->PCCACAFM;/* REQUEST CPU  03S0138 00336000
*                                      AFFINITY                      */ 00337000
         L     @11,PSAPCCAV                                     03S0138 00338000
         MVC   SRBCPAFF(2,SRBPTR),PCCACAFM(@11)                 03S0138 00339000
*                   SRBRMTR=ADDR(CVTBRET);/* RESOURCE MGR-BR 14 03S0139 00340000
*                                      INSTRUCTION                   */ 00341000
         L     @11,CVTPTR                                       03S0139 00342000
         LA    @11,CVTBRET(,@11)                                03S0139 00343000
         ST    @11,SRBRMTR(,SRBPTR)                             03S0139 00344000
*                   SRBPARM=SCDPTR; /* PTR TO SCD IS PARM            */ 00345000
         ST    SCDPTR,SRBPARM(,SRBPTR)                          03S0140 00346000
*                   SRBEP=ADDR(IECVHREC);/* HOT I/O RECOVERY RTN     */ 00347000
         L     @11,@CV00081                                     03S0141 00348000
         ST    @11,SRBEP(,SRBPTR)                               03S0141 00349000
*                   RFY                                         03S0142 00350000
*                     WORKREG RSTD;                             03S0142 00351000
*                   WORKREG=SRBPTR; /* SET UP FOR SCHEDULE           */ 00352000
         LR    WORKREG,SRBPTR                                   03S0143 00353000
*                   GEN REFS(WORKREG) SETS(R0,R1,R15);          03S0144 00354000
*                                       /*SCHEDULE HOT I/O RECOVERY  */ 00355000
     SCHEDULE SRB=(WORKREG),SCOPE=GLOBAL                                00356000
*                   RFY                                         03S0145 00357000
*                     WORKREG UNRSTD;                           03S0145 00358000
*                   GEN SETS(R0,R1,R14,R15) REFS(R11,R12,R13);  03S0146 00359000
*                                               /*FREE SALLOC LOCK   */ 00360000
FREESAL SETLOCK RELEASE,TYPE=SALLOC,DISABLED,REGS=USE,                 X00361000
               RELATED=(GETSAL,'RELEASE SALLOC LOCK')                   00362000
     SETFRR D,WRKREGS=(R1,R15)       DELETE FRR                         00363000
*                 END;              /* END SCHEDULE RECOVERY         */ 00364000
*               ELSE                                            03S0148 00365000
*                 ;                 /* CAT MARKED-RECOVERY DONE 03S0148 00366000
*                                      EARLIER                       */ 00367000
@RF00122 DS    0H                                               03S0149 00368000
*             END;                                              02S0149 00369000
*           ELSE                                                02S0150 00370000
*             ;                     /* THRESHOLD NOT EXCEEDED        */ 00371000
@RF00104 DS    0H                                               02S0151 00372000
*         END;                      /* END INTERRUPT IS REPEAT       */ 00373000
*       ELSE                        /* INTERRUPT IS NOT REPEAT       */ 00374000
*         DO;                       /* REFLECT CURRENT INTERRUPT IN     00375000
*                                      SCD                           */ 00376000
         B     @RC00093                                         02S0152 00377000
@RF00093 DS    0H                                               02S0153 00378000
*           SCDADDR=DEVAD;          /* UPDATE DEVICE ADDRESS         */ 00379000
         STH   DEVAD,SCDADDR(,SCDPTR)                           02S0153 00380000
*           SCDRSC1=IRTDETEC;       /* UPDATE DETECTION FLAGS        */ 00381000
         MVC   SCDRSC1(1,SCDPTR),IRTDETEC+736(LCCAPTR)          02S0154 00382000
*           SCDCSWST=FLCCSW(5:6);   /* UPDATE CSW STATUS             */ 00383000
         MVC   SCDCSWST(2,SCDPTR),FLCCSW+4                      02S0155 00384000
*           SCDCNTS=0;              /* ZERO REPEAT COUNTS            */ 00385000
         SLR   @08,@08                                          02S0156 00386000
         ST    @08,SCDCNTS(,SCDPTR)                             02S0156 00387000
*           SCDCPUID=PCCACPUA;      /* SET CPU ID                    */ 00388000
         MVC   SCDCPUID(2,SCDPTR),PCCACPUA(PCCAPTR)             02S0157 00389000
*           RFY                                                 02S0158 00390000
*             WORKREG RSTD;                                     02S0158 00391000
*           WORKREG=ADDR(SCDTIME);  /* SET UP FOR STCK INSTRUCTION   */ 00392000
         LA    WORKREG,SCDTIME(,SCDPTR)                         02S0159 00393000
*           GEN SETS(SCDTIME);                                  02S0160 00394000
*                                  /*STORE NEW TIME                  */ 00395000
       STCK 0(WORKREG)                                                  00396000
*           RFY                                                 02S0161 00397000
*             WORKREG UNRSTD;                                   02S0161 00398000
*         END;                                                  02S0162 00399000
*     END;                                                      01S0163 00400000
@RC00093 DS    0H                                               01S0164 00401000
*   RFY                                                         01S0164 00402000
*    (R0,                                                       01S0164 00403000
*     R15) RSTD;                                                01S0164 00404000
@RF00081 DS    0H                                               01S0165 00405000
*   LM(R0,R15,SVAREA);              /* RESTORE REGISTERS             */ 00406000
         LM    R0,R15,SVAREA(SVAREAP)                           01S0165 00407000
