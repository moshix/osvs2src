         TITLE 'IECVIRST - IOS LOST CHANNEL RECOVERY PROCESSOR         *00001000
                        '                                               00002000
IECVIRST CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(24)                                             0001 00007000
         DC    C'IECVIRST 78009  UZ85100 '                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @09,0                                               0001 00010000
@PSTART  LA    @10,4095(,@09)                                      0001 00011000
         USING @PSTART,@09                                         0001 00012000
         USING @PSTART+4095,@10                                    0001 00013000
*   R11=R0;                         /* ADDR OF SRB IN REG FOR      0206 00014000
*                                      ADDRESSABILITY                */ 00015000
         LR    R11,R0                                              0206 00016000
*   GEN REFS(R11,DISABLE);                                         0207 00017000
         STNSM 0(R11),DISABLE          DISABLE, ST CURR MASK IN SRB     00018000
*   PSAIOSUP='1'B;                  /* IOS SUPER BIT, DISABLED     0208 00019000
*                                      WITHOUT HOLDING A LOCK        */ 00020000
         OI    PSAIOSUP,B'10000000'                                0208 00021000
*   R11=ADDR(IRSTFRR);              /* ADDR OF FRR FOR SETFRR MACRO  */ 00022000
         LA    R11,IRSTFRR                                         0209 00023000
*   GEN REFS(R11) SETS(R14,R4,FRRWPTR) FLOWS(IRSTFRR);             0210 00024000
*                                                  /*ADD FRR TO STACK*/ 00025000
ADDRFRR1 SETFRR    A,FRRAD=(R11),WRKREGS=(R14,R4),PARMAD=(FRRWPTR)      00026000
*   FRRBASE1=R9;                    /* SAVE BASE REG IN FRR WORK     */ 00027000
         ST    R9,FRRBASE1(,FRRWPTR)                               0211 00028000
*   FRRBASE2=R10;                   /* SAVE BASE REG IN FRR WORK     */ 00029000
         ST    R10,FRRBASE2(,FRRWPTR)                              0212 00030000
*   FRRINPUT=R1;                    /* SAVE INPUT PARAMETER          */ 00031000
         ST    R1,FRRINPUT(,FRRWPTR)                               0213 00032000
*   RFY                                                            0214 00033000
*     R15 RSTD;                                                    0214 00034000
*   GEN SETS(R14,R15,R0,R1) REFS(R11,R12,R13);                     0215 00035000
*                                           /*OBTAIN SALLOC LOCK     */ 00036000
GETSALC1 SETLOCK   OBTAIN,TYPE=SALLOC,MODE=UNCOND,DISABLED,REGS=USE,   X00037000
               RELATED=(FRESALC1,'OBTAIN SALLOC LOCK')                  00038000
*   FRRSALOC='1'B;                  /* INDICATE SALLOC LOCK HELD     */ 00039000
         OI    FRRSALOC(FRRWPTR),B'10000000'                       0216 00040000
*   GEN(L     R15,@SIZDATD);        /* LEN, SUBPOOL FOR GETMAIN      */ 00041000
         L     R15,@SIZDATD                                             00042000
*   GEN(LA    R0,0(R15));           /* CLEAR HIGH ORDER BYTE         */ 00043000
         LA    R0,0(R15)                                                00044000
*   RFY                                                            0219 00045000
*     R3 RSTD;                                                     0219 00046000
*   GEN REFS(R11) SETS(R4,R1,R3,R14,R15);                          0220 00047000
*                                      /*OBTAIN GENERAL WORK AREA    */ 00048000
OBTMAIN1 GETMAIN   RU,LV=(0),BRANCH=(YES,GLOBAL),SP=245                 00049000
*   RFY                                                            0221 00050000
*     R3 UNRSTD;                                                   0221 00051000
*   IRSTWPTR=R1;                    /* BASE FOR AUTO DATA AREA       */ 00052000
         LR    IRSTWPTR,R1                                         0222 00053000
*   GEN;                                                           0223 00054000
         USING @DATD,IRSTWPTR                                           00055000
*   IRSTSTRT=IRSTSTRT&&IRSTSTRT;    /* CLEAR FIRST RESERVE TAB SEG   */ 00056000
         XC    IRSTSTRT(208),IRSTSTRT                              0224 00057000
*   IRSTFLG1=IRSTFLG1&&IRSTFLG1;    /* CLEAR FLAGS                   */ 00058000
         XC    IRSTFLG1(2),IRSTFLG1                                0225 00059000
*   FRRGWPTR=R1;                    /* SAVE WORK AREA PTR IN FRR WK  */ 00060000
         ST    R1,FRRGWPTR(,FRRWPTR)                               0226 00061000
*   GEN REFS(R11,R12,R13) SETS(R14,R15,R0,R1);                     0227 00062000
*                                           /*RELEASE SALLOC LOCK    */ 00063000
FRESALC1 SETLOCK   RELEASE,TYPE=SALLOC,DISABLED,REGS=USE,              X00064000
               RELATED=(GETSALC1,'RELEASE SALLOC LOCK')                 00065000
*   RFY                                                            0228 00066000
*     R15 UNRSTD;                                                  0228 00067000
*   FRRSALOC='0'B;                  /* INDICATE SALLOC LOCK FREED    */ 00068000
         NI    FRRSALOC(FRRWPTR),B'01111111'                       0229 00069000
*   IF FRRINCOD=MACHCKIN THEN       /* IF ERR DETECTED ON MACH CHK   */ 00070000
         CLI   FRRINCOD(FRRWPTR),1                                 0230 00071000
         BNE   @RF00230                                            0230 00072000
*     DO;                                                          0231 00073000
*       PCCAIRST='1'B;              /* IND IECVIRST RECOVERING LOST     00074000
*                                      CHANNELS DETECTED ON MACH CK.    00075000
*                                      IF IGFCCHED DETECTS 'MORE'  0232 00076000
*                                      LOST CHANNELS WHILE PCCAIRST     00077000
*                                      IS SET HE WILL SET PCCAEXDM.  */ 00078000
         L     @15,PCCAPTR                                         0232 00079000
         OI    PCCAIRST(@15),B'10000000'                           0232 00080000
*       PWAEDSRB='00'X;             /* MACH CK SRB IS AVAILABLE      */ 00081000
         L     @15,PWAPTR(,@15)                                    0233 00082000
         MVI   PWAEDSRB(@15),X'00'                                 0233 00083000
*     END;                                                         0234 00084000
*   ELSE                                                           0235 00085000
*     PCCASRBL='00'X;               /* FREE CCH MAINLINE SRB         */ 00086000
         B     @RC00230                                            0235 00087000
@RF00230 L     @15,PCCAPTR                                         0235 00088000
         MVI   PCCASRBL(@15),X'00'                                 0235 00089000
*   FRRSRBLK='1'B;                  /* IND SRB AVAILABLE             */ 00090000
@RC00230 OI    FRRSRBLK(FRRWPTR),B'00100000'                       0236 00091000
*   IRSTCAT=PCCACAT;                /* COPY CAT INCASE A HUNG INTER-    00092000
*                                      FACE OCCURS WHILE IECVIRST IS    00093000
*                                      PROCESSING THIS ERROR         */ 00094000
         L     @15,PCCAPTR                                         0237 00095000
         MVC   IRSTCAT(128),PCCACAT(@15)                           0237 00096000
*   RFY                                                            0238 00097000
*    (R0,                                                          0238 00098000
*     R1,                                                          0238 00099000
*     R11,                                                         0238 00100000
*     R14,                                                         0238 00101000
*     R4) UNRSTD;                                                  0238 00102000
*                                                                  0238 00103000
*   /*****************************************************************/ 00104000
*   /*                                                               */ 00105000
*   /* THIS SEGMENT BUILDS THE RESERVE TABLE. THE COPIED CAT, IN     */ 00106000
*   /* IRSTCAT IS SCANNED FOR CHANNELS SUFFERING AN ERR THAT MATCHES */ 00107000
*   /* THE INPUT PARAM: MACHCKIN->CATMCRST,CHANCKIN->CATCCRST. WHEN  */ 00108000
*   /* SUCH A CHANNEL IS FOUND, INTERNAL PROCEDURE FINDUCB IS CALLED */ 00109000
*   /* TO LOCATE THE UCB FOR EACH DEVICE ON THE CHANNEL. IF THE      */ 00110000
*   /* DEVICE IS RESERVED IT WILL BE INSERTED INTO THE RESERVE TABLE.*/ 00111000
*   /* IF A RESERVE TABLE SEGMENT IS FILLED, ANOTHER SEGMENT WILL BE */ 00112000
*   /* OBTAINED.                                                     */ 00113000
*   /*                                                               */ 00114000
*   /*****************************************************************/ 00115000
*                                                                  0239 00116000
*   RFY                                                            0239 00117000
*    (UCBPTR,                                                      0239 00118000
*     R11,                                                         0239 00119000
*     R14) RSTD;                                                   0239 00120000
*   IRSTRPTR=ADDR(IRSTRESV);        /* ADDR OF FIRST SEG             */ 00121000
         LA    @15,IRSTRESV                                        0240 00122000
         ST    @15,IRSTRPTR                                        0240 00123000
*   RESVCNT=1;                      /* FIRST ENTRY IN SEG            */ 00124000
         LA    @05,1                                               0241 00125000
         ST    @05,RESVCNT(,@15)                                   0241 00126000
*   IRSTCATP=ADDR(IRSTCAT);         /* ADDRESS COPIED CAT            */ 00127000
         LA    IRSTCATP,IRSTCAT                                    0242 00128000
*   RFY                                                            0243 00129000
*     CAT BASED(IRSTCATP);          /* SCAN COPIED CAT               */ 00130000
*   IRSTCHMK=IRSTCHMK&&IRSTCHMK;    /* ZERO CHAN MASK                */ 00131000
         XC    IRSTCHMK(4),IRSTCHMK                                0244 00132000
*   DO IRSTLOOP=1 TO LENGTH(IRSTCAT)/LENGTH(CAT);/* CHECK EACH ENTRY    00133000
*                                      IN CAT *NOTE* WE ARE SCANNING    00134000
*                                      COPIED CAT                    */ 00135000
         STC   @05,IRSTLOOP                                        0245 00136000
@DL00245 DS    0H                                                  0246 00137000
*     IRSTCHAN=IRSTLOOP-1;          /* CHAN ADDRESS                  */ 00138000
         BCTR  @05,0                                               0246 00139000
         STC   @05,IRSTCHAN                                        0246 00140000
*     IF(FRRINCOD=MACHCKIN&CATMCRST='1'B)|/* MACH CK DETECTED        */ 00141000
*         (FRRINCOD=CHANCKIN&CATCCRST='1'B) THEN/* CHAN CK DETECTED  */ 00142000
         CLI   FRRINCOD(FRRWPTR),1                                 0247 00143000
         BNE   @GL00001                                            0247 00144000
         TM    CATMCRST(IRSTCATP),B'00010000'                      0247 00145000
         BO    @RT00247                                            0247 00146000
@GL00001 CLI   FRRINCOD(FRRWPTR),2                                 0247 00147000
         BNE   @RF00247                                            0247 00148000
         TM    CATCCRST(IRSTCATP),B'00100000'                      0247 00149000
         BNO   @RF00247                                            0247 00150000
@RT00247 DS    0H                                                  0248 00151000
*       DO;                         /* SCAN LOST CHAN FOR RESERVED 0248 00152000
*                                      DEVICES                       */ 00153000
*         IRSTTEMP=HIGHORDR;        /* BIT 0 IS ON                   */ 00154000
         MVC   IRSTTEMP(4),@CB02987                                0249 00155000
*         SRL(IRSTTEMP,IRSTCHAN);   /* CORRESPONDING MASK BIT TO CHAN   00156000
*                                      IS ON                         */ 00157000
         L     @15,IRSTTEMP                                        0250 00158000
         SLR   @05,@05                                             0250 00159000
         IC    @05,IRSTCHAN                                        0250 00160000
         SRL   @15,0(@05)                                          0250 00161000
         ST    @15,IRSTTEMP                                        0250 00162000
*         IRSTCHMK=IRSTCHMK|IRSTTEMP;/* ACCUMULATE LOST CH           */ 00163000
         OC    IRSTCHMK(4),IRSTTEMP                                0251 00164000
*         IRSTCALL=ADDR(FICALL1);   /* RTN FINDUCB IS TO CALL        */ 00165000
         LA    @15,FICALL1                                         0252 00166000
         ST    @15,IRSTCALL                                        0252 00167000
*         RFY                                                      0253 00168000
*          (R0,                                                    0253 00169000
*           R1,                                                    0253 00170000
*           R3,                                                    0253 00171000
*           R4,                                                    0253 00172000
*           R13,                                                   0253 00173000
*           R15) RSTD;              /* FINDUCB PATH SETS             */ 00174000
*         CALL FINDUCB;             /* LOCATE UCB                    */ 00175000
         BAL   @14,FINDUCB                                         0254 00176000
*         RFY                                                      0255 00177000
*          (R0,                                                    0255 00178000
*           R1,                                                    0255 00179000
*           R3,                                                    0255 00180000
*           R4,                                                    0255 00181000
*           R13,                                                   0255 00182000
*           R15) UNRSTD;                                           0255 00183000
*FICALL1:                                                          0256 00184000
*         PROC OPTIONS(NOSAVE,NOSAVEAREA);/* FICALL1 PROCEEDURE IS 0256 00185000
*                                      CALLED BY THE FINDUCB       0256 00186000
*                                      PROCEDURE FOR EACH DEVICE ON     00187000
*                                      THE LOST CHAN. FICALL1 WILL 0256 00188000
*                                      DETERMINE IF THE DEVICE IS  0256 00189000
*                                      RESERVED, ADD IT TO THE     0256 00190000
*                                      RESERVE TABLE AND RETURN TO 0256 00191000
*                                      FINDUCB. FINDUCB WILL CONTINUE   00192000
*                                      TO CALL FICALL1 UNTIL ALL THE    00193000
*                                      DEVICES ON THE LOST CHANNEL 0256 00194000
*                                      HAVE BEEN PROCESSED. FINDUCB     00195000
*                                      WILL THEN RETURN TO ITS     0256 00196000
*                                      CALLER.                       */ 00197000
         B     @PB00002                                            0256 00198000
FICALL1  DS    0H                                                  0257 00199000
*         IRSTFI14=R14;             /* RET ADDR-> FINDUCB            */ 00200000
         ST    R14,IRSTFI14                                        0257 00201000
*         IF UCBCPU=PSACPUSA&       /* LAST SIO THIS CPU     @ZM47978*/ 00202000
*             UCBCHA=IRSTCHAN&      /* LAST SIO LOST CHAN            */ 00203000
*             UCBIORST='0'B THEN    /* NOT ALREADY PROCES            */ 00204000
         SLR   @15,@15                                             0258 00205000
         IC    @15,UCBCPU(,UCBPTR)                                 0258 00206000
         CH    @15,PSACPUSA                                        0258 00207000
         BNE   @RF00258                                            0258 00208000
         CLC   UCBCHA(1,UCBPTR),IRSTCHAN                           0258 00209000
         BNE   @RF00258                                            0258 00210000
         TM    UCBIORST(UCBPTR),B'10000000'                        0258 00211000
         BNZ   @RF00258                                            0258 00212000
*           DO;                     /* PROCESS DEVICE                */ 00213000
*             IF UCB3DACC='1'B&     /* DASD                          */ 00214000
*                 UCBRR='1'B&       /* SHARABLE DEVICE               */ 00215000
*                 UCBRESVH='1'B&    /* RESERVED                      */ 00216000
*                 UCBSQC^=0 THEN    /* NO. RESERVE MACROS            */ 00217000
         TM    UCB3DACC(UCBPTR),B'00100000'                        0260 00218000
         BNO   @RF00260                                            0260 00219000
         TM    UCBRR(UCBPTR),B'00100000'                           0260 00220000
         BNO   @RF00260                                            0260 00221000
         TM    UCBRESVH(UCBPTR),B'00010000'                        0260 00222000
         BNO   @RF00260                                            0260 00223000
         CLI   UCBSQC(UCBPTR),0                                    0260 00224000
         BE    @RF00260                                            0260 00225000
*               DO;                 /* RESERVED DEVICE               */ 00226000
*                 FRRRESVE='1'B;    /* IND RESERVE FOUND             */ 00227000
         OI    FRRRESVE(FRRWPTR),B'00010000'                       0262 00228000
*                 RESVDEVA(RESVCNT)=UCBCHAN;/* DEVICE ADDRESS        */ 00229000
         L     @15,IRSTRPTR                                        0263 00230000
         L     @05,RESVCNT(,@15)                                   0263 00231000
         LR    @04,@05                                             0263 00232000
         SLA   @04,4                                               0263 00233000
         SLR   @03,@03                                             0263 00234000
         ICM   @03,3,UCBCHAN(UCBPTR)                               0263 00235000
         LA    @02,0(@04,@15)                                      0263 00236000
         AL    @02,@CF03300                                        0263 00237000
         STH   @03,RESVDEVA-8(,@02)                                0263 00238000
*                 RESVUCBA(RESVCNT)=UCBPTR;/* UCB ADDRESS            */ 00239000
         AL    @15,@CF03301                                        0264 00240000
         ST    UCBPTR,RESVUCBA-12(@04,@15)                         0264 00241000
*                 IF RESVCNT=MAXENTRS THEN/* RESERVE SEG FULL        */ 00242000
         CH    @05,@CH00796                                        0265 00243000
         BNE   @RF00265                                            0265 00244000
*                   DO;             /* GET ANOTHER SEG               */ 00245000
*                     R11=IRSTUCBL; /* FREE UCB LOCK                 */ 00246000
         L     R11,IRSTUCBL                                        0267 00247000
*                     RFY                                          0268 00248000
*                      (R15,                                       0268 00249000
*                       R0,                                        0268 00250000
*                       R1) RSTD;                                  0268 00251000
*                     GEN REFS(R11,R12,R13) SETS(R14,R15,R0,R1);   0269 00252000
FREUCB2  SETLOCK   RELEASE,TYPE=IOSUCB,ADDR=(11),DISABLED,REGS=USE,    X00253000
               RELATED=(GETUCB1,'FREE UCBLOCK')                         00254000
*                     RFY                                          0270 00255000
*                      (R15,                                       0270 00256000
*                       R0,                                        0270 00257000
*                       R1) UNRSTD;                                0270 00258000
*                     FRRSUCB='0'B; /* IND UCB LOCK FREED            */ 00259000
         NI    FRRSUCB(FRRWPTR),B'10111111'                        0271 00260000
*                     IRSTOBPN=LENGTH(IRSTRESV);/* LEN OF RESERE SEG */ 00261000
         LA    IRSTOBPN,200                                        0272 00262000
*                     RFY                                          0273 00263000
*                      (R0,                                        0273 00264000
*                       R1,                                        0273 00265000
*                       R3,                                        0273 00266000
*                       R4,                                        0273 00267000
*                       R15) RSTD;  /* OBTSTOR PATH SETS R0,R1,R3, 0273 00268000
*                                      R4,R14,R15                    */ 00269000
*                     CALL OBTSTOR; /* GET ANOTHER SEG               */ 00270000
         BAL   @14,OBTSTOR                                         0274 00271000
*                     RFY                                          0275 00272000
*                      (R0,                                        0275 00273000
*                       R1,                                        0275 00274000
*                       R3,                                        0275 00275000
*                       R4,                                        0275 00276000
*                       R15) UNRSTD;                               0275 00277000
*                     IRSTOBPN->RESVTAB=IRSTOBPN->RESVTAB&&IRSTOBPN->   00278000
*                         RESVTAB;  /* CLEAR                         */ 00279000
         XC    RESVTAB(200,IRSTOBPN),RESVTAB(IRSTOBPN)             0276 00280000
*                     RESVNEXT=IRSTOBPN;/* CHAIN TO PREVIOUS         */ 00281000
         L     @15,IRSTRPTR                                        0277 00282000
         ST    IRSTOBPN,RESVNEXT(,@15)                             0277 00283000
*                     IRSTRPTR=IRSTOBPN;/* ADDR NEW SEGMENT          */ 00284000
         ST    IRSTOBPN,IRSTRPTR                                   0278 00285000
*                     RESVCNT=1;    /* FIRST ENTRY                   */ 00286000
         L     @15,IRSTRPTR                                        0279 00287000
         MVC   RESVCNT(4,@15),@CF00103                             0279 00288000
*                   END;            /* END GET ANOTHER        SEGMENT*/ 00289000
*                 ELSE                                             0281 00290000
*                   RESVCNT=RESVCNT+1;/* SEGMENT NOT FULL, SETUP FOR    00291000
*                                      NEXT ENTRY                    */ 00292000
         B     @RC00265                                            0281 00293000
@RF00265 L     @15,IRSTRPTR                                        0281 00294000
         LA    @05,1                                               0281 00295000
         AL    @05,RESVCNT(,@15)                                   0281 00296000
         ST    @05,RESVCNT(,@15)                                   0281 00297000
*               END;                /* END RESERVE DEV PROCESS       */ 00298000
*             ELSE                                                 0283 00299000
*               UCBRESVH='0'B;      /* DEVICE NOT RESERVED, TURN OFF    00300000
*                                      RESERVE IND. IOS MAY HAVE A 0283 00301000
*                                      DEVICE RESERVED THAT DOES NOT    00302000
*                                      NEED TO BE RESERVED (UCBSQC=0)*/ 00303000
         B     @RC00260                                            0283 00304000
@RF00260 NI    UCBRESVH(UCBPTR),B'11101111'                        0283 00305000
*           END;                    /* END PROCESS DEVICE            */ 00306000
*         ELSE                                                     0285 00307000
*           ;                       /* LAST SIO NOT FROM THIS CPU    */ 00308000
@RF00258 DS    0H                                                  0286 00309000
*         R14=IRSTFI14;             /* RESTORE RETURN ADDR           */ 00310000
@RC00258 L     R14,IRSTFI14                                        0286 00311000
*       END FICALL1;                                               0287 00312000
@EL00002 DS    0H                                                  0287 00313000
@EF00002 DS    0H                                                  0287 00314000
@ER00002 BR    @14                                                 0287 00315000
@PB00002 DS    0H                                                  0288 00316000
*   END;                            /* FINISHED WITH CHANNEL         */ 00317000
*   ELSE                                                           0289 00318000
*     ;                             /* CHAN NOT LOST                 */ 00319000
@RF00247 DS    0H                                                  0290 00320000
*   IRSTCATP=IRSTCATP+LENGTH(CAT);  /* PTR TO NEXT ENTRY             */ 00321000
         AH    IRSTCATP,@CH00101                                   0290 00322000
*   END;                            /* DONE WITH ALL CHANNELS        */ 00323000
         LA    @05,1                                               0291 00324000
         SLR   @15,@15                                             0291 00325000
         IC    @15,IRSTLOOP                                        0291 00326000
         ALR   @05,@15                                             0291 00327000
         STC   @05,IRSTLOOP                                        0291 00328000
         CH    @05,@CH00830                                        0291 00329000
         BNH   @DL00245                                            0291 00330000
*   RFY                                                            0292 00331000
*     CAT BASED(CATPTR);                                           0292 00332000
*   RFY                                                            0293 00333000
*    (UCBPTR,                                                      0293 00334000
*     R11,                                                         0293 00335000
*     R14) UNRSTD;                                                 0293 00336000
*   IF FRRINCOD=MACHCKIN|           /* IF MACH CK DETECT ERR         */ 00337000
*       (FRRINCOD=CHANCKIN&FRRRESVE='1'B) THEN/* OR CHAN CK WITH   0294 00338000
*                                      RESERVE                       */ 00339000
         CLI   FRRINCOD(FRRWPTR),1                                 0294 00340000
         BE    @RT00294                                            0294 00341000
         CLI   FRRINCOD(FRRWPTR),2                                 0294 00342000
         BNE   @RF00294                                            0294 00343000
         TM    FRRRESVE(FRRWPTR),B'00010000'                       0294 00344000
         BNO   @RF00294                                            0294 00345000
@RT00294 DS    0H                                                  0295 00346000
*     DO;                           /* COMMUNICATE WITH OPER         */ 00347000
*                                                                  0295 00348000
*       /*************************************************************/ 00349000
*       /*                                                           */ 00350000
*       /* THIS SEGMENT COMMUNICATES WITH THE OPERATOR VIA           */ 00351000
*       /* RESTARTABLE WAIT STATES. IT IS INCLUDED BY SEGMENT        */ 00352000
*       /* RESVSCAN IF THE LOSS OF THE CHANNEL(S) WAS DETECTED DURING*/ 00353000
*       /* PROCESSING FOR AN EXTERNAL DAMAGE MACHINE CHECK OR DURING */ 00354000
*       /* PROCESSING FOR A CHANNEL CHECK AND RESERVED DEVICES EXIST */ 00355000
*       /* ON THE LOST CHANNEL. WAIT STATE CODE 04D IS FOR CHANNEL   */ 00356000
*       /* CHECK WITH RESERVES. WAIT STATE CODE 04C IS FOR MACHINE   */ 00357000
*       /* CHECK WITH RESERVES. WAIT STATE CODE 04B IS FOR MACHINE   */ 00358000
*       /* CHECK WITHOUT RESERVES. LOADWAIT IS CALLED TO ACTUALLY    */ 00359000
*       /* CAUSE THE WAIT STATE TO BE LOADED.                        */ 00360000
*       /*                                                           */ 00361000
*       /*************************************************************/ 00362000
*                                                                  0296 00363000
*       IF FRRINCOD=MACHCKIN THEN   /* IF DETECTED DURING MACH CK    */ 00364000
         CLI   FRRINCOD(FRRWPTR),1                                 0296 00365000
         BNE   @RF00296                                            0296 00366000
*         DO;                       /* DETERMINE CORRECT WAIT CODE   */ 00367000
*           IF FRRRESVE='1'B THEN   /* IF RESERVED DEVICES           */ 00368000
         TM    FRRRESVE(FRRWPTR),B'00010000'                       0298 00369000
         BNO   @RF00298                                            0298 00370000
*             IRSTWTCD=MCRESERV;    /* RESERVED CODE ON MACH CK      */ 00371000
         MVC   IRSTWTCD(4),@CF02948                                0299 00372000
*           ELSE                                                   0300 00373000
*             IRSTWTCD=MCNORESR;    /* CODE FOR NO RESERVES          */ 00374000
         B     @RC00298                                            0300 00375000
@RF00298 MVC   IRSTWTCD(4),@CF02946                                0300 00376000
*         END;                                                     0301 00377000
*       ELSE                                                       0302 00378000
*         IRSTWTCD=CCRESERV;        /* CHAN CK WITH RESERVED DEV     */ 00379000
         B     @RC00296                                            0302 00380000
@RF00296 MVC   IRSTWTCD(4),@CF02950                                0302 00381000
*RELOADWT:                                                         0303 00382000
*       ;                           /* IF OPER ACTION CODE REQ AND IS   00383000
*                                      INVALID OR REQ RE-LOAD WAIT,     00384000
*                                      BRANCH TO THIS LABEL          */ 00385000
@RC00296 DS    0H                                                  0303 00386000
RELOADWT DS    0H                                                  0304 00387000
*       IRSTWAIT='1'B;              /* IND WAIT IS RESTARTABLE       */ 00388000
         OI    IRSTWAIT,B'10000000'                                0304 00389000
*       RFY                                                        0305 00390000
*        (R0,                                                      0305 00391000
*         R1,                                                      0305 00392000
*         R3,                                                      0305 00393000
*         R4,                                                      0305 00394000
*         R13,                                                     0305 00395000
*         R14,                                                     0305 00396000
*         R15) RSTD;                /* LOADWAIT PATH SETS THESE      */ 00397000
*       CALL LOADWAIT;              /* CAUSE WAIT STATE TO BE LOAD   */ 00398000
         BAL   @14,LOADWAIT                                        0306 00399000
*       RFY                                                        0307 00400000
*        (R0,                                                      0307 00401000
*         R1,                                                      0307 00402000
*         R3,                                                      0307 00403000
*         R4,                                                      0307 00404000
*         R13,                                                     0307 00405000
*         R14,                                                     0307 00406000
*         R15) UNRSTD;                                             0307 00407000
*       IF FRRINCOD=MACHCKIN&       /* IF ACTION CODE REQ AND        */ 00408000
*           (IRSTOPAC^=ACDOWAIT&IRSTOPAC^=ACNOWAIT)/* INVALID OR REQ    00409000
*                                      RE-LOAD                       */ 00410000
*         THEN                                                     0308 00411000
         CLI   FRRINCOD(FRRWPTR),1                                 0308 00412000
         BNE   @RF00308                                            0308 00413000
