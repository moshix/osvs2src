*     R15,                                                         0689 01908000
*     R3) RSTD;                                                    0689 01909000
*   IRSTOBRT=R14;                   /* SAVE RETURN ADDRESS           */ 01910000
         LR    IRSTOBRT,R14                                        0690 01911000
*   GEN REFS(R11,R12,R13) SETS(R14,R15,R0,R1);                     0691 01912000
*                                           /*GETMAIN REQUIRES SALLOC*/ 01913000
