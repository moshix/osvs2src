         NI    FRRSALOC(FRRWPTR),B'01111111'                       0697 01928000
*   R14=IRSTOBRT;                   /* RESTORE RETURN ADDRESS        */ 01929000
         LR    R14,IRSTOBRT                                        0698 01930000
*   RFY                                                            0699 01931000
*    (R0,                                                          0699 01932000
*     R1,                                                          0699 01933000
*     R4,                                                          0699 01934000
*     R14,                                                         0699 01935000
*     R15,                                                         0699 01936000
*     R3) UNRSTD;                                                  0699 01937000
*   END OBTSTOR;                                                   0700 01938000
@EL00005 DS    0H                                                  0700 01939000
@EF00005 DS    0H                                                  0700 01940000
@ER00005 BR    @14                                                 0700 01941000
*RECORD:                                                           0701 01942000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* ISSUE RECORD MACRO            */ 01943000
*                                                                  0701 01944000
RECORD   DS    0H                                                  0702 01945000
*   /*****************************************************************/ 01946000
*   /*                                                               */ 01947000
*   /* THIS PROCEDURE ISSUES THE RECORD MACRO TO WRITE A RECORD TO   */ 01948000
*   /* THE LOGREC DATASET OR TO ISSUE A MESSAGE TO THE OPERATOR. THE */ 01949000
*   /* RECORD IS ALREADY BUILT IN THE MESSAGE BUFFER. THE LENGTH OF  */ 01950000
*   /* THE RECORD IS IN IRSTRDLN. IRSTRECD=0 INDICATE LOGREC.        */ 01951000
*   /* IRSTRECD=1 INDICATES WTO. IF THE RECORD MACRO RETURNS CODES OF*/ 01952000
*   /* 8 OR 12, OBTSTOR WILL BE CALLED TO GETMAIN ENOUGH STORAGE TO  */ 01953000
*   /* HOLD THE RECORD, THE RECORD WILL BE COPIED TO THE GETMAINED   */ 01954000
*   /* STORAGE AND THE RECORD MACRO WILL BE RE-ISSUED WITH BUFFER=NO.*/ 01955000
*   /*                                                               */ 01956000
*   /*****************************************************************/ 01957000
*                                                                  0702 01958000
*   RFY                                                            0702 01959000
*    (R14,                                                         0702 01960000
*     R15,                                                         0702 01961000
*     R0,                                                          0702 01962000
*     R1,                                                          0702 01963000
*     R13) RSTD;                                                   0702 01964000
*   IRSTRERT=R14;                   /* SAVE RETURN ADDRESS           */ 01965000
         LR    IRSTRERT,R14                                        0703 01966000
*   R1=ADDR(IRSTMESG);              /* ADDRESS OF RECORD             */ 01967000
         LA    R1,IRSTMESG                                         0704 01968000
*   R0=IRSTRDLN;                    /* LENGTH OF RECORD              */ 01969000
         LH    R0,IRSTRDLN                                         0705 01970000
*   R13=WSACRSTI;                   /* SAVE AREA FOR RECORD          */ 01971000
         L     @05,LCCAPTR                                         0706 01972000
         L     @05,LCCACPUS(,@05)                                  0706 01973000
         L     R13,WSACRSTI(,@05)                                  0706 01974000
*   IF IRSTRECD='0'B THEN           /* IF RECORDING WAIT STATE       */ 01975000
         TM    IRSTRECD,B'01000000'                                0707 01976000
         BNZ   @RF00707                                            0707 01977000
*     DO;                           /* RECORD TYPE IS LOGREC         */ 01978000
*       GEN REFS(R0,R1) SETS(R14,R15);                             0709 01979000
         RECORD    TYPE=LOGREC,RCVRY=SETFRR,PARMADR=(1),LENGTH=(0),    X01980000
               BUFFER=YES,HEADER=YES                                    01981000
*     END;                          /* END RECORD TYPE IS LOGREC     */ 01982000
*   ELSE                            /* RECORDING MESSAGE             */ 01983000
*     DO;                           /* RECORD TYPE IS WTO            */ 01984000
         B     @RC00707                                            0711 01985000
@RF00707 DS    0H                                                  0712 01986000
*       GEN REFS(R0,R1) SETS(R14,R15);                             0712 01987000
         RECORD    TYPE=WTO,RCVRY=SETFRR,DATAADR=(1),LENGTH=(0),       X01988000
               BUFFER=YES                                               01989000
*     END;                          /* END RECORD TYPE IS WTO        */ 01990000
*   IF R15=8|                       /* IF MESSAGE TRUNCATED          */ 01991000
*       R15=12 THEN                 /* MESSAGE NOT BUFFERED          */ 01992000
@RC00707 CH    R15,@CH00101                                        0714 01993000
         BE    @RT00714                                            0714 01994000
         CH    R15,@CH00796                                        0714 01995000
         BNE   @RF00714                                            0714 01996000
@RT00714 DS    0H                                                  0715 01997000
*     DO;                           /* RE-RECORD WITH BUFFER=NO      */ 01998000
*       IF IRSTRECD='0'B THEN       /* IF LOGREC RECORD              */ 01999000
         TM    IRSTRECD,B'01000000'                                0716 02000000
         BNZ   @RF00716                                            0716 02001000
*         IRSTOBPN=IRSTRDLN+LENGTH(LRB);/* GETMAIN LEN INCLUDES    0717 02002000
*                                      HEADER                        */ 02003000
         LA    IRSTOBPN,24                                         0717 02004000
         AH    IRSTOBPN,IRSTRDLN                                   0717 02005000
*       ELSE                                                       0718 02006000
*         IRSTOBPN=IRSTRDLN;        /* WTO LEN IS COMPLETE LENGTH    */ 02007000
         B     @RC00716                                            0718 02008000
@RF00716 LH    IRSTOBPN,IRSTRDLN                                   0718 02009000
*       RFY                                                        0719 02010000
*        (R3,                                                      0719 02011000
*         R4) RSTD;                 /* OBTSOR PATH SETS            0719 02012000
*                                      R0,R1,R3,R4,R14,R15           */ 02013000
@RC00716 DS    0H                                                  0720 02014000
*       CALL OBTSTOR;               /* GETMAIN STORAGE TO COPY RCD   */ 02015000
         BAL   @14,OBTSTOR                                         0720 02016000
*       RFY                                                        0721 02017000
*        (R3,                                                      0721 02018000
*         R4) UNRSTD;                                              0721 02019000
*       IF IRSTRECD='0'B THEN       /* IF LOGREC LENGTH              */ 02020000
         TM    IRSTRECD,B'01000000'                                0722 02021000
         BNZ   @RF00722                                            0722 02022000
*         DO;                                                      0723 02023000
*           R0=IRSTRDLN+LENGTH(LRB);/* ADD LEN OF HEADER             */ 02024000
         LA    R0,24                                               0724 02025000
         AH    R0,IRSTRDLN                                         0724 02026000
*           LRBHREL=0;              /* FIX LRB HDR AS RECORD MACRO 0725 02027000
*                                      WILL NOT FORMAT               */ 02028000
         LA    @03,IRSTMESG                                        0725 02029000
         MVI   LRBHREL(@03),X'00'                                  0725 02030000
*           LRBMSYST='1'B;          /* INDICATE SYSTEM TERMINATED    */ 02031000
         OI    LRBMSYST(@03),B'00100000'                           0726 02032000
*           LOGDATAA=0;             /* DATA IN PROPER POS IN RCD     */ 02033000
         SLR   @05,@05                                             0727 02034000
         ST    @05,LOGDATAA(,@03)                                  0727 02035000
*         END;                                                     0728 02036000
*       ELSE                                                       0729 02037000
*         R0=IRSTRDLN;              /* WTO LEN IS COMPLETE LENGTH    */ 02038000
         B     @RC00722                                            0729 02039000
@RF00722 LH    R0,IRSTRDLN                                         0729 02040000
*       IRSTOBPN->GETMBUFF(1:R0)=IRSTMESG(1:R0);/* COPY RECORD       */ 02041000
@RC00722 LR    @03,R0                                              0730 02042000
         BCTR  @03,0                                               0730 02043000
         EX    @03,@SM03316                                        0730 02044000
*       R1=IRSTOBPN;                /* ADDR OF NEW MESSAGE BUFFER    */ 02045000
*                                                                  0731 02046000
         LR    R1,IRSTOBPN                                         0731 02047000
*       /*************************************************************/ 02048000
*       /*                                                           */ 02049000
*       /* RECORD FROM NEW BUFFER                                    */ 02050000
*       /*                                                           */ 02051000
*       /*************************************************************/ 02052000
*                                                                  0732 02053000
*       IF IRSTRECD='0'B THEN       /* IF RECORDING WAIT STATE       */ 02054000
         TM    IRSTRECD,B'01000000'                                0732 02055000
         BNZ   @RF00732                                            0732 02056000
*         DO;                       /* RECORD MACRO TYPE IS LOGREC   */ 02057000
*           GEN REFS(R0,R1) SETS(R14,R15);                         0734 02058000
         RECORD    TYPE=LOGREC,RCVRY=SETFRR,DATAADR=(1),LENGTH=(0),    X02059000
               BUFFER=NO                                                02060000
*         END;                      /* END RECORD TYPE IS LOGREC     */ 02061000
*       ELSE                        /* RECORDING MESSAGE             */ 02062000
*         DO;                       /* RECORD MACRO TYPE IS WTO      */ 02063000
         B     @RC00732                                            0736 02064000
@RF00732 DS    0H                                                  0737 02065000
*           GEN REFS(R0,R1) SETS(R14,R15);                         0737 02066000
         RECORD    TYPE=WTO,RCVRY=SETFRR,DATAADR=(1),LENGTH=(0),       X02067000
               BUFFER=NO                                                02068000
*         END;                      /* END RECORD TYPE IS WTO        */ 02069000
*     END;                          /* END RC=8|12 FROM RECORD       */ 02070000
*   ELSE                                                           0740 02071000
*     ;                                                            0740 02072000
@RF00714 DS    0H                                                  0741 02073000
*   R14=IRSTRERT;                   /* RETURN ADDRESS                */ 02074000
@RC00714 LR    R14,IRSTRERT                                        0741 02075000
*   RFY                                                            0742 02076000
*    (R0,                                                          0742 02077000
*     R1,                                                          0742 02078000
*     R14,                                                         0742 02079000
*     R15,                                                         0742 02080000
*     R13) UNRSTD;                                                 0742 02081000
*   END RECORD;                                                    0743 02082000
@EL00006 DS    0H                                                  0743 02083000
@EF00006 DS    0H                                                  0743 02084000
@ER00006 BR    @14                                                 0743 02085000
*FINDUCB:                                                          0744 02086000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* LOCATE UCB                    */ 02087000
*                                                                  0744 02088000
FINDUCB  DS    0H                                                  0745 02089000
*   /*****************************************************************/ 02090000
*   /*                                                               */ 02091000
*   /* THIS SEGMENT LOCATES THE UCBS FOR EACH DEVICE ON A CHANNEL. A */ 02092000
*   /* DO LOOP IS SETUP FOR EVERY POSSIBLE DEVICE ADDRESS, X00 THRU  */ 02093000
*   /* XFF WHERE X IS THE CHANNEL ADDRESS. EACH POSSIBLE ADDRESS IS  */ 02094000
*   /* INPUTED TO THE UCBLOOK FUNCTION OF IOSGEN. ON RETURN FROM     */ 02095000
*   /* IOSGEN, A ZERO RETURN CODE INDICATES THE DEVICE EXISTS. UCBPTR*/ 02096000
*   /* WILL CONTAIN THE UCB ADDRESS. THE UCBLOCK WILL BE OBTAINED AND*/ 02097000
*   /* THE CALLER RETURNED TO. AFTER ALL POSSIBLE DEVICE ADDRESSES   */ 02098000
*   /* HAVE BEEN EXAMINED, THE CALLER IS RETURNED TO WITH A ZERO     */ 02099000
*   /* UCBPTR. INPUT: IRSTCHAN CONTAINS THE CHANNEL ADDRESS. R14     */ 02100000
*   /* CONTAINS THE RETURN ADDRESS. OUTPUT: UCBPTR CONTAINS A UCB    */ 02101000
*   /* ADDRESS OR ZERO. IRSTUCBL CONTAINS ADDRESS OF UCBLOCK. FRRSUCB*/ 02102000
*   /* INDICATES LOCK HELD. CALLERS: RESVSCAN, RESTARIO              */ 02103000
*   /*                                                               */ 02104000
*   /*****************************************************************/ 02105000
*                                                                  0745 02106000
*   RFY                                                            0745 02107000
*    (R14,                                                         0745 02108000
*     UCBPTR,                                                      0745 02109000
*     R13,                                                         0745 02110000
*     R15,                                                         0745 02111000
*     R11,                                                         0745 02112000
*     R0,                                                          0745 02113000
*     R1) RSTD;                                                    0745 02114000
*   IRSTFIRT=R14;                   /* SAVE RETURN ADDRESS           */ 02115000
         ST    R14,IRSTFIRT                                        0746 02116000
*   IRSTDOCH=IRSTCHAN;              /* CHANNEL ADDRESS               */ 02117000
         MVC   IRSTDOCH(1),IRSTCHAN                                0747 02118000
*   IRSTDODE=0;                     /* FIRST POSSIBLE DEVICE ADDR    */ 02119000
         MVI   IRSTDODE,X'00'                                      0748 02120000
*   IRSTLACH=IRSTCHAN;              /* END OF DO LOOP                */ 02121000
         MVC   IRSTLACH(1),IRSTCHAN                                0749 02122000
*   IRSTLADE='FF'X;                 /* LAST DEVICE ON CHANNEL        */ 02123000
         MVI   IRSTLADE,X'FF'                                      0750 02124000
*   DO IRSTDOLP=IRSTDOLP TO IRSTLAST;/* SCAN ALL POSSIBLE DEV ADDR   */ 02125000
         LH    @05,IRSTDOLP                                        0751 02126000
         B     @DE00751                                            0751 02127000
@DL00751 DS    0H                                                  0752 02128000
*     R13=WSACRSTI;                 /* SAVE AREA FOR IOSGEN          */ 02129000
         L     @04,LCCAPTR                                         0752 02130000
         L     @04,LCCACPUS(,@04)                                  0752 02131000
         L     R13,WSACRSTI(,@04)                                  0752 02132000
*     R11=IRSTDOLP;                 /* DEVICE ADDRESS                */ 02133000
         LR    R11,@05                                             0753 02134000
*     GEN REFS(CVTILK1,CVTILK2,CVTIXAVL,CVTMAP,IOCOMEX,CVTPTR,IOCOM,IOX 02135000
*         ,IOCUCBBM,IOXHICH) SETS(R15,UCBPTR)                      0754 02136000
*         (IOSGEN UCBLOOK,UCB=(UCBPTR),REG=(R11));/* LOCATE UCB      */ 02137000
         IOSGEN UCBLOOK,UCB=(UCBPTR),REG=(R11)                          02138000
*     IF R15=0 THEN                 /* IF UCB FOUND, DEV ADDR VALID  */ 02139000
         LTR   R15,R15                                             0755 02140000
         BNZ   @RF00755                                            0755 02141000
*       DO;                         /* PROCESS DEVICE                */ 02142000
*         IF UCBMTPXP='1'B THEN     /* IF MULTI-EXPOSURE DEVICE      */ 02143000
         TM    UCBMTPXP(UCBPTR),B'00001000'                        0757 02144000
         BNO   @RF00757                                            0757 02145000
*           R11=ADDR(UCBBASE->UCBLOCK);/* LOCK ON BASE UCB           */ 02146000
         L     R11,UCBBASE(,UCBPTR)                                0758 02147000
         LA    R11,UCBLOCK(,R11)                                   0758 02148000
*         ELSE                                                     0759 02149000
*           R11=ADDR(UCBLOCK);      /* ADDR OF UCB LOCK WORD         */ 02150000
         B     @RC00757                                            0759 02151000
@RF00757 LR    R11,UCBPTR                                          0759 02152000
         AL    R11,@CF03300                                        0759 02153000
*         IRSTUCBL=R11;             /* SAVE ADDR OF LOCK TO FREE     */ 02154000
@RC00757 ST    R11,IRSTUCBL                                        0760 02155000
*         GEN SETS(R14,R15,R0,R1) REFS(R11,R12,R13);               0761 02156000
*                                                   /*OBTAIN UCBLOCK */ 02157000
GETUCB1  SETLOCK   OBTAIN,TYPE=IOSUCB,ADDR=(11),MODE=UNCOND,DISABLED,  X02158000
               REGS=USE,RELATED=(FREUCB1,FREUCB2,'GETUCB LOCK')         02159000
*         FRRSUCB='1'B;             /* INDICATE UCB LOCK HELD        */ 02160000
         OI    FRRSUCB(FRRWPTR),B'01000000'                        0762 02161000
*         RFY                                                      0763 02162000
*          (R3,                                                    0763 02163000
*           R4,                                                    0763 02164000
*           R5) RSTD;               /* FICALLER PATH SETS R0,R1,R3 0763 02165000
*                                      R4,R5,R11,R13,R14,R15         */ 02166000
*         CALL FICALLER;            /* CALL CALLER, UCBPTR CONTAINS     02167000
*                                      ADDR OF UCB                   */ 02168000
         L     @15,IRSTCALL                                        0764 02169000
         BALR  @14,@15                                             0764 02170000
*         RFY                                                      0765 02171000
*          (R3,                                                    0765 02172000
*           R4,                                                    0765 02173000
*           R5) UNRSTD;                                            0765 02174000
*         IF FRRSUCB='1'B THEN      /* IF CALLER DID NOT FREE UCBLK  */ 02175000
         TM    FRRSUCB(FRRWPTR),B'01000000'                        0766 02176000
         BNO   @RF00766                                            0766 02177000
*           DO;                     /* FREE IT NOW                   */ 02178000
*             R11=IRSTUCBL;         /* ADDR OF UCB LOCK WORD         */ 02179000
         L     R11,IRSTUCBL                                        0768 02180000
*             GEN SETS(R14,R15,R0,R1) REFS(R11,R12,R13);           0769 02181000
FREUCB1  SETLOCK   RELEASE,TYPE=IOSUCB,ADDR=(11),DISABLED,REGS=USE,    X02182000
               RELATED=(GETUCB1,'FREE UCB LOCK')                        02183000
*             FRRSUCB='0'B;         /* IND UCB LOCK NOT HELD         */ 02184000
         NI    FRRSUCB(FRRWPTR),B'10111111'                        0770 02185000
*           END;                    /* END FREE UCB LOCK             */ 02186000
*         ELSE                                                     0772 02187000
*           ;                       /* UCBLOCK FREED BY CALLER       */ 02188000
@RF00766 DS    0H                                                  0773 02189000
*       END;                        /* END PROCESS DEVICE            */ 02190000
*     ELSE                                                         0774 02191000
*       ;                                                          0774 02192000
@RF00755 DS    0H                                                  0775 02193000
*   END;                            /* END DO LOOP, ALL DEV FOUND    */ 02194000
         LA    @05,1                                               0775 02195000
         AH    @05,IRSTDOLP                                        0775 02196000
         STH   @05,IRSTDOLP                                        0775 02197000
@DE00751 CH    @05,IRSTLAST                                        0775 02198000
         BNH   @DL00751                                            0775 02199000
*   UCBPTR=0;                       /* IND TO CALLER, RETURN         */ 02200000
         SLR   UCBPTR,UCBPTR                                       0776 02201000
*   R14=IRSTFIRT;                   /* RETURN ADDRESS                */ 02202000
         L     R14,IRSTFIRT                                        0777 02203000
*   RFY                                                            0778 02204000
*    (R14,                                                         0778 02205000
*     UCBPTR,                                                      0778 02206000
*     R13,                                                         0778 02207000
*     R15,                                                         0778 02208000
*     R11,                                                         0778 02209000
*     R0,                                                          0778 02210000
*     R1) UNRSTD;                                                  0778 02211000
*   END FINDUCB;                                                   0779 02212000
@EL00007 DS    0H                                                  0779 02213000
@EF00007 DS    0H                                                  0779 02214000
@ER00007 BR    @14                                                 0779 02215000
*RESVSIO:                                                          0780 02216000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,PARMREG);/* DO RESERVE SIOS       */ 02217000
*                                                                  0780 02218000
RESVSIO  DS    0H                                                  0781 02219000
*   /*****************************************************************/ 02220000
*   /*                                                               */ 02221000
*   /* THIS ROUTINE DOES THE ACTUAL I/O REQUIRED TO RESERVE DEVICES. */ 02222000
*   /* INPUT TO THIS ROUTINE IS A DEVICE ADDRESS TO BE USED FOR THE  */ 02223000
*   /* SIO INSTRUCTION. THIS IS PASSED IN REGISTER 1. IF THE RESERVE */ 02224000
*   /* IS SUCCESSFUL A RESERVE COMPLETE INDICATOR IS SET IN THE      */ 02225000
*   /* RESERVE TABLE AND THE UCB IS MARKED AS RESERVED. IF THE       */ 02226000
*   /* RESERVE IS NOT SUCCESSFUL THE RESERVE COMPLETE INDICATOR IS   */ 02227000
*   /* NOT TURNED ON.                                                */ 02228000
*   /*                                                               */ 02229000
*   /*****************************************************************/ 02230000
*                                                                  0781 02231000
*   RFY                                                            0781 02232000
*    (R1) RSTD;                                                    0781 02233000
*   RFY                                                            0782 02234000
*    (R2,                                                          0782 02235000
*     R7) RSTD;                     /* RESTRICT REGS FOR COUNTERS    */ 02236000
*   RFY                                                            0783 02237000
*    (R14) RSTD;                                                   0783 02238000
*   IRSTR14=R14;                    /* SAVE RETURN ADDRESS           */ 02239000
         ST    R14,IRSTR14                                         0784 02240000
*   RFY                                                            0785 02241000
*    (R14) UNRSTD;                                                 0785 02242000
*   IF IRSTUNCR='1'B THEN           /* IF UNCONDITIONAL RESERVE GET  */ 02243000
         TM    IRSTUNCR,B'00010000'                                0786 02244000
         BNO   @RF00786                                            0786 02245000
*     GEN REFS(URESVCCW)( LRA 7,URESVCCW);/* REAL ADDRESS OF UR CCW  */ 02246000
          LRA 7,URESVCCW                                                02247000
*   ELSE                                                           0788 02248000
*     GEN REFS(RESVCCW)( LRA 7,RESVCCW);/* GET REAL ADDR OF RESV CCW */ 02249000
         B     @RC00786                                            0788 02250000
@RF00786 DS    0H                                                  0788 02251000
          LRA 7,RESVCCW                                                 02252000
*   CALL IGFPSAD0;                  /* ALLOW MODIFICATION OF FLC     */ 02253000
@RC00786 L     @15,@CV02941                                        0789 02254000
         BALR  @14,@15                                             0789 02255000
*   FLCCAW=R7;                      /* INITIALIZE CAW WITH CCW ADDR  */ 02256000
         ST    R7,FLCCAW                                           0790 02257000
*   CALL IGFPSAE0;                  /* PREVENT MODIFICATION OF FLC   */ 02258000
         L     @15,@CV02942                                        0791 02259000
         BALR  @14,@15                                             0791 02260000
*   RFY                                                            0792 02261000
*    (R7) UNRSTD;                                                  0792 02262000
*   RESVSNS(R2)='0'B;               /* RESET SENSE REQUIRED INDICATOR*/ 02263000
         L     @11,IRSTRPTR                                        0793 02264000
         LR    @07,R2                                              0793 02265000
         SLA   @07,4                                               0793 02266000
         ALR   @07,@11                                             0793 02267000
         AL    @07,@CF03305                                        0793 02268000
         NI    RESVSNS-10(@07),B'10111111'                         0793 02269000
*   IRSTSIOC=0;                     /* SET SIO COUNTER TO ZERO       */ 02270000
         SLR   IRSTSIOC,IRSTSIOC                                   0794 02271000
*   IRSTTIOC=0;                     /* SET TIO COUNTER TO ZERO       */ 02272000
         SLR   IRSTTIOC,IRSTTIOC                                   0795 02273000
*   RESVXIO(R2)='1'B;               /* INDICATE SIO ISSUED           */ 02274000
         LR    @07,R2                                              0796 02275000
         SLA   @07,4                                               0796 02276000
         ALR   @11,@07                                             0796 02277000
         AL    @11,@CF03305                                        0796 02278000
         OI    RESVXIO-10(@11),B'00100000'                         0796 02279000
*DOSIO:                                                            0797 02280000
*   IRSTSIOC=IRSTSIOC+1;            /* INCREMENT SIO COUNTER         */ 02281000
DOSIO    AL    IRSTSIOC,@CF00103                                   0797 02282000
*   IF IRSTSIOC=SIOMAX THEN                                        0798 02283000
         C     IRSTSIOC,@CF02984                                   0798 02284000
         BE    @RT00798                                            0798 02285000
*     GO TO EXIT;                   /* IF COUNT=MAXIMUM-EXIT, RESERVE   02286000
*                                      FAILED                        */ 02287000
*   SIO(R1);                        /* ISSUE SIO FOR RESERVE         */ 02288000
         SIO   0(R1)                                               0800 02289000
*   BC(4,CC1);                      /* CC=1, CHECK STATUS            */ 02290000
         BC    4,CC1                                               0801 02291000
*   BC(2,BUSY);                     /* CC=2, ENABLE AND RETRY        */ 02292000
         BC    2,BUSY                                              0802 02293000
*   BC(1,EXIT);                     /* CC=3, EXIT-RESERVE FAILED     */ 02294000
         BC    1,EXIT                                              0803 02295000
*/*FALL THROUGH TO TIO LOOP ON SIO CC=0                              */ 02296000
*                                                                  0804 02297000
*DOTIO:                             /* TIO IS USED TO BRING IN     0804 02298000
*                                      INTERRUPT FROM SIO WITHOUT  0804 02299000
*                                      ENABLING                      */ 02300000
*   RESVXIO(R2)='0'B;               /* RESET SIO BIT TO INDICATE TIO    02301000
*                                      ISSUED                        */ 02302000
DOTIO    LR    @11,R2                                              0804 02303000
         SLA   @11,4                                               0804 02304000
         L     @07,IRSTRPTR                                        0804 02305000
         ALR   @07,@11                                             0804 02306000
         AL    @07,@CF03305                                        0804 02307000
         NI    RESVXIO-10(@07),B'11011111'                         0804 02308000
*   IRSTTIOC=IRSTTIOC+1;            /* INCREMENT TIO COUNT           */ 02309000
         AL    IRSTTIOC,@CF00103                                   0805 02310000
*   IF IRSTTIOC=TIOMAX THEN                                        0806 02311000
         C     IRSTTIOC,@CF02984                                   0806 02312000
         BE    @RT00806                                            0806 02313000
*     GO TO EXIT;                   /* IF TIO COUNT=MAXIMUM, RESERVE    02314000
*                                      FAILED                        */ 02315000
*   TIO(R1);                        /* ISSUE TIO TO BRING IN       0808 02316000
*                                      INTERRUPT                     */ 02317000
         TIO   0(R1)                                               0808 02318000
*   BC(9,EXIT);                     /* CC=3, CC=0, ERROR-EXIT, RESV     02319000
*                                      FAILED                        */ 02320000
         BC    9,EXIT                                              0809 02321000
*   BC(4,CC1);                      /* CC=1, CHECK STATUS            */ 02322000
         BC    4,CC1                                               0810 02323000
*   GO TO DOTIO;                    /* CC=2, LOOP ON TIO TILL COUNT     02324000
*                                      EXPIRES                       */ 02325000
         B     DOTIO                                               0811 02326000
*CC1:                               /* CHECK CSW STATUS ON CC=1 FROM    02327000
*                                                             SIO,TIO*/ 02328000
*   IF CDC='1'B|CCC='1'B|ICC='1'B THEN                             0812 02329000
CC1      TM    CDC,B'00001110'                                     0812 02330000
         BNZ   @RT00812                                            0812 02331000
*     ;                             /* CHANNEL ERRORS-RESV FAILED    */ 02332000
*   ELSE                                                           0814 02333000
*     IF UC='1'B THEN                                              0814 02334000
         TM    UC,B'00000010'                                      0814 02335000
         BNO   @RF00814                                            0814 02336000
*       RESVSNS(R2)='1'B;           /* UC-RESVERVE FAILED-SENSE    0815 02337000
*                                      NEEDED                        */ 02338000
         LR    @12,R2                                              0815 02339000
         SLA   @12,4                                               0815 02340000
         L     @11,IRSTRPTR                                        0815 02341000
         ALR   @11,@12                                             0815 02342000
         AL    @11,@CF03305                                        0815 02343000
         OI    RESVSNS-10(@11),B'01000000'                         0815 02344000
*     ELSE                                                         0816 02345000
*       IF RESVXIO(R2)='0'B&DE='1'B /* DE ON TIO-RESERVE SUCCESSFUL  */ 02346000
*         THEN                                                     0816 02347000
         B     @RC00814                                            0816 02348000
@RF00814 L     @11,IRSTRPTR                                        0816 02349000
         LR    @07,R2                                              0816 02350000
         SLA   @07,4                                               0816 02351000
         ALR   @07,@11                                             0816 02352000
         AL    @07,@CF03305                                        0816 02353000
         TM    RESVXIO-10(@07),B'00100000'                         0816 02354000
         BNZ   @RF00816                                            0816 02355000
         TM    DE,B'00000100'                                      0816 02356000
         BNO   @RF00816                                            0816 02357000
*         RESVCOMP(R2)='1'B;        /* SET DEVICE RESERVED INDICATOR */ 02358000
         LR    @12,R2                                              0817 02359000
         SLA   @12,4                                               0817 02360000
         ALR   @11,@12                                             0817 02361000
         AL    @11,@CF03305                                        0817 02362000
         OI    RESVCOMP-10(@11),B'10000000'                        0817 02363000
*       ELSE                                                       0818 02364000
*         IF RESVXIO(R2)='0'B       /* CC1 OCCURRED ON TIO           */ 02365000
*           THEN                                                   0818 02366000
         B     @RC00816                                            0818 02367000
@RF00816 L     @11,IRSTRPTR                                        0818 02368000
         LR    @07,R2                                              0818 02369000
         SLA   @07,4                                               0818 02370000
         ALR   @07,@11                                             0818 02371000
         AL    @07,@CF03305                                        0818 02372000
         TM    RESVXIO-10(@07),B'00100000'                         0818 02373000
         BZ    @RT00818                                            0818 02374000
*           GO TO DOTIO;            /* RETRY TIO                     */ 02375000
*         ELSE                                                     0820 02376000
*           IF BSY='1'B&DE='1'B THEN                               0820 02377000
         TM    BSY,B'00010100'                                     0820 02378000
         BNO   @RF00820                                            0820 02379000
*             DO;                   /* BUSY, DE ON SIO               */ 02380000
*               RFY                                                0822 02381000
*                (UCBPTR) RSTD;                                    0822 02382000
*               UCBPTR=RESVUCBA(R2);/* GET UCB ADDRESSABILITY        */ 02383000
         LR    @15,R2                                              0823 02384000
         SLA   @15,4                                               0823 02385000
         AL    @11,@CF03301                                        0823 02386000
         L     UCBPTR,RESVUCBA-12(@15,@11)                         0823 02387000
*               UCBUDE='1'B;        /* SET UNSOLICITED DE ON TO FORCE   02388000
*                                      DAVV                          */ 02389000
         OI    UCBUDE(UCBPTR),B'00100000'                          0824 02390000
*               UCBSAP='0'B;        /* TURN OF STAND ALONE PROCESS 0825 02391000
*                                      INDICATOR                     */ 02392000
         NI    UCBSAP(UCBPTR),B'11111011'                          0825 02393000
*               RFY                                                0826 02394000
*                (UCBPTR) UNRSTD;                                  0826 02395000
*               GO TO DOSIO;        /* RETRY SIO                     */ 02396000
         B     DOSIO                                               0827 02397000
*             END;                                                 0828 02398000
*           ELSE                                                   0829 02399000
*             IF BSY='1'B&STATM='1'B&CUE='1'B THEN                 0829 02400000
@RF00820 TM    BSY,B'01110000'                                     0829 02401000
         BO    @RT00829                                            0829 02402000
*               GO TO DOSIO;        /* SHORT BUSY-RETRY SIO          */ 02403000
*             ELSE                                                 0831 02404000
*               IF BSY='1'B         /* DEVICE BUSY?                  */ 02405000
*                 THEN                                             0831 02406000
         TM    BSY,B'00010000'                                     0831 02407000
         BO    @RT00831                                            0831 02408000
*                 GO TO BUSY;       /* ENABLE AND RETRY              */ 02409000
*               ELSE                                               0833 02410000
*                 IF CE='1'B&DE='1'B/* CE,DE ON-RESERVE GOOD         */ 02411000
*                   THEN                                           0833 02412000
         TM    CE,B'00001100'                                      0833 02413000
         BNO   @RF00833                                            0833 02414000
*                   RESVCOMP(R2)='1'B;/* INDICATE RESERVE DONE       */ 02415000
         LR    @12,R2                                              0834 02416000
         SLA   @12,4                                               0834 02417000
         L     @11,IRSTRPTR                                        0834 02418000
         ALR   @11,@12                                             0834 02419000
         AL    @11,@CF03305                                        0834 02420000
         OI    RESVCOMP-10(@11),B'10000000'                        0834 02421000
*                 ELSE                                             0835 02422000
*                   IF CE='1'B      /* CE, W/O DE?                   */ 02423000
*                     THEN                                         0835 02424000
         B     @RC00833                                            0835 02425000
@RF00833 TM    CE,B'00001000'                                      0835 02426000
         BO    @RT00835                                            0835 02427000
*                     GO TO DOTIO;  /* DO TIO TO GET DE INTERRUPT    */ 02428000
*EXIT:                              /* IF CC1 FALLS THRU-INVALID   0837 02429000
*                                      STATUS-RESERVE FAILED         */ 02430000
*   IF RESVSNS(R2)='1'B THEN        /* SENSE REQUIRED                */ 02431000
@RC00833 DS    0H                                                  0837 02432000
@RC00816 DS    0H                                                  0837 02433000
@RC00814 DS    0H                                                  0837 02434000
@RT00812 DS    0H                                                  0837 02435000
EXIT     LR    @12,R2                                              0837 02436000
         SLA   @12,4                                               0837 02437000
         L     @11,IRSTRPTR                                        0837 02438000
         ALR   @11,@12                                             0837 02439000
         AL    @11,@CF03305                                        0837 02440000
         TM    RESVSNS-10(@11),B'01000000'                         0837 02441000
         BNO   @RF00837                                            0837 02442000
*     DO;                           /* ISSUE SENSE                   */ 02443000
*       RFY                                                        0839 02444000
*        (R7) RSTD;                                                0839 02445000
*       GEN REFS(SNSCCW) SETS(R7)( LRA R7,SNSCCW);/* GET REAL ADDR OF   02446000
*                                      SENSE CCW                     */ 02447000
          LRA R7,SNSCCW                                                 02448000
*       CALL IGFPSAD0;              /* ALLOW MODIFICATION OF FLC     */ 02449000
         L     @15,@CV02941                                        0841 02450000
         BALR  @14,@15                                             0841 02451000
*       FLCCAW=R7;                  /* PUT CCW ADDR IN CAW,KEY=0     */ 02452000
         ST    R7,FLCCAW                                           0842 02453000
*       CALL IGFPSAE0;              /* PREVENT MODIFICATION OF FLC   */ 02454000
         L     @15,@CV02942                                        0843 02455000
         BALR  @14,@15                                             0843 02456000
*       RFY                                                        0844 02457000
*        (R7) UNRSTD;                                              0844 02458000
*       SIO(R1);                    /* ISSUE SENSE                   */ 02459000
         SIO   0(R1)                                               0845 02460000
*     END;                                                         0846 02461000
*   IF RESVCOMP(R2)='1'B THEN       /* RESERVE SUCCESSFUL ?          */ 02462000
@RF00837 L     @12,IRSTRPTR                                        0847 02463000
         LR    @11,R2                                              0847 02464000
         SLA   @11,4                                               0847 02465000
         ALR   @11,@12                                             0847 02466000
         AL    @11,@CF03305                                        0847 02467000
         TM    RESVCOMP-10(@11),B'10000000'                        0847 02468000
         BNO   @RF00847                                            0847 02469000
*     DO;                           /* INDICATE RESERVE IN UCB       */ 02470000
*       RFY                                                        0849 02471000
*        (UCBPTR) RSTD;                                            0849 02472000
*       UCBPTR=RESVUCBA(R2);        /* GET UCB ADDRESS               */ 02473000
         LR    @11,R2                                              0850 02474000
         SLA   @11,4                                               0850 02475000
         AL    @12,@CF03301                                        0850 02476000
         L     UCBPTR,RESVUCBA-12(@11,@12)                         0850 02477000
*       UCBRESVH='1'B;              /* TURN ON RESERVED INDICATOR    */ 02478000
         OI    UCBRESVH(UCBPTR),B'00010000'                        0851 02479000
*       UCBCPU=CURRCPU(2:2);        /* INDICATE CPU RESERVED TO      */ 02480000
         MVC   UCBCPU(1,UCBPTR),CURRCPU+1                          0852 02481000
*       UCBCHAN=R1;                 /* INDICATE ADDR RESERVED ON     */ 02482000
         STH   R1,UCBCHAN(,UCBPTR)                                 0853 02483000
*       RFY                                                        0854 02484000
*        (UCBPTR) UNRSTD;                                          0854 02485000
*     END;                                                         0855 02486000
*   RFY                                                            0856 02487000
*    (R14) RSTD;                                                   0856 02488000
@RF00847 DS    0H                                                  0857 02489000
*   R14=IRSTR14;                    /* RESTORE RETURN ADDRESS        */ 02490000
         L     R14,IRSTR14                                         0857 02491000
*   RETURN;                         /* RETURN TO CALLER              */ 02492000
@EL00008 DS    0H                                                  0858 02493000
@EF00008 DS    0H                                                  0858 02494000
@ER00008 BR    @14                                                 0858 02495000
*   RFY                                                            0859 02496000
*    (R14) UNRSTD;                                                 0859 02497000
*                                                                  0859 02498000
*   /*****************************************************************/ 02499000
*   /*                                                               */ 02500000
*   /* THIS SECTION OF CODE RECEIVES CONTROL IF A BUSY (CC=2 OR CC=1 */ 02501000
*   /* AND DEVICE BUSY) IS ENCOUNTERED ON THE RESERVE SIO. IT WILL   */ 02502000
*   /* ENABLE THE SYSTEM FOR INTERRUPTS FROM THE CHANNEL IN USE IN   */ 02503000
*   /* ORDER TO CLEAR THE INTERRUPT SO THAT THE SIO CAN BE REISSUED. */ 02504000
*   /* IT TAKES OVER THE I/O NEW PSW AND SELECTIVELY ENABLES THE     */ 02505000
*   /* CHANNEL BY MODIFYING CONTROL REGISTER 2. WHEN THE INTERRUPT   */ 02506000
*   /* OCCURS IT IS PASSED TO THE I/O SLIH. CONTROL REGISTER 2 AND   */ 02507000
*   /* THE I/O NEW PSW ARE RESTORED AND THE SIO IS RETRIED. IF NO    */ 02508000
*   /* INTERRUPT OCCURS THE RESERVE IS NOT RE-ATTEMPTED.             */ 02509000
*   /*                                                               */ 02510000
*   /*****************************************************************/ 02511000
*                                                                  0860 02512000
*BUSY:                                                             0860 02513000
*   IRSTIPSW=FLCINPSW(5:8);         /* SAVE THE IO NEW PSW           */ 02514000
BUSY     MVC   IRSTIPSW(4),FLCINPSW+4                              0860 02515000
*   CALL IGFPSAD0;                  /* ALLOW MODIFICATION OF FLC     */ 02516000
         L     @15,@CV02941                                        0861 02517000
         BALR  @14,@15                                             0861 02518000
*   FLCINPSW(5:8)=ADDR(INTERUPT);   /* U/D IO NEW PSW INSTRUCTION  0862 02519000
*                                      ADDRESS                       */ 02520000
         LA    @11,INTERUPT                                        0862 02521000
         ST    @11,FLCINPSW+4                                      0862 02522000
*   CALL IGFPSAE0;                  /* PREVENT MODIFICATION OF FLC   */ 02523000
         L     @15,@CV02942                                        0863 02524000
         BALR  @14,@15                                             0863 02525000
*   FRRIOPSW='1'B;                  /* INDICATE FOR FRR PSW IS     0864 02526000
*                                      MODIFIED                      */ 02527000
         OI    FRRIOPSW(FRRWPTR),B'00000001'                       0864 02528000
*   GEN REFS(IRSTCTLR);                                            0865 02529000
*                            /*SAVE CONTROL REGISTER 2               */ 02530000
          STCTL 2,2,IRSTCTLR                                            02531000
*   RFY                                                            0866 02532000
*    (R13,                                                         0866 02533000
*     R11) RSTD;                    /* RESTRICT REGS TO ENABLE     0866 02534000
*                                      CHANNEL                       */ 02535000
*   R13=R1/256;                     /* GET CHANNEL IN LOW ORDER BYTE    02536000
*                                      OF REG                        */ 02537000
         LR    R13,R1                                              0867 02538000
         SRL   R13,8                                               0867 02539000
*   R11=HIGHORDR;                   /* TURN ON HIGH ORDER BIT IN REG */ 02540000
         L     R11,@CF02987                                        0868 02541000
*   SRL(R11,R13);                   /* SET CORRECT BYTE TO ENABLE  0869 02542000
*                                      RIGHT CHAN                    */ 02543000
         SRL   R11,0(R13)                                          0869 02544000
*   IRSTCTLT=R11;                   /* PUT NEW CTL REG2 VALUE IN   0870 02545000
*                                      WORKAREA                      */ 02546000
         ST    R11,IRSTCTLT                                        0870 02547000
*   RFY                                                            0871 02548000
*    (R13,                                                         0871 02549000
*     R11) UNRSTD;                                                 0871 02550000
*   GEN REFS(IRSTCTLT);                                            0872 02551000
*                            /*UPDATE CTL REG 2 TO ENABLE 1 CHAN ONLY*/ 02552000
         LCTL  2,2,IRSTCTLT                                             02553000
*   FRRCTLR2='1'B;                  /* INDICATE FOR FRR CTL REG    0873 02554000
*                                      MODIFIED                      */ 02555000
         OI    FRRCTLR2(FRRWPTR),B'10000000'                       0873 02556000
*   DO IRSTM=1 TO ENBLMAX;          /* ENABLE TILL COUNT EXPIRES     */ 02557000
         LA    IRSTM,1                                             0874 02558000
@DL00874 DS    0H                                                  0875 02559000
*     GEN REFS(IRSTSYSM);                                          0875 02560000
*                            /*ENABLE FOR INTERRUPT                  */ 02561000
         STOSM IRSTSYSM,X'02'                                           02562000
*   END;                                                           0876 02563000
         AL    IRSTM,@CF00103                                      0876 02564000
         C     IRSTM,@CF02982                                      0876 02565000
         BNH   @DL00874                                            0876 02566000
*   GEN REFS(IRSTSYSM);                                            0877 02567000
*                              /*INTERRUPT DID NOT OCCUR-DISABLE     */ 02568000
         STNSM IRSTSYSM,X'FD'                                           02569000
*INTERUPT:                                                         0878 02570000
*   GEN REFS(IRSTCTLR);                                            0878 02571000
INTERUPT DS    0H                                                  0878 02572000
*                              /*RESTORE CTL REG 2                   */ 02573000
         LCTL  2,2,IRSTCTLR                                             02574000
*   FRRCTLR2='0'B;                  /* TURN OFF FRR INDICATOR        */ 02575000
         NI    FRRCTLR2(FRRWPTR),B'01111111'                       0879 02576000
*   FLCINPSW(5:8)=IRSTIPSW;         /* RESTORE I/O NEW PSW           */ 02577000
         MVC   FLCINPSW+4(4),IRSTIPSW                              0880 02578000
*   FRRIOPSW='0'B;                  /* TURN OFF FRR INDICATOR        */ 02579000
         NI    FRRIOPSW(FRRWPTR),B'11111110'                       0881 02580000
*   IF IRSTM^=ENBLMAX THEN          /* ENTERED DUE TO INTERRUPT ?    */ 02581000
         C     IRSTM,@CF02982                                      0882 02582000
         BE    @RF00882                                            0882 02583000
*     DO;                           /* HANDLE INTERRUPT              */ 02584000
*       RFY                                                        0884 02585000
*        (R5,                                                      0884 02586000
*         R11,                                                     0884 02587000
*         R13,                                                     0884 02588000
*         R4) RSTD;                 /* RESTRICT REGS FOR CALL TO SLIH*/ 02589000
*       R11='08'X;                  /* INDICATE IORESTART ENTERING 0885 02590000
*                                      SLIH                          */ 02591000
         LA    R11,8                                               0885 02592000
*       R13=WSACRSTI;               /* GET SAVE AREA ADDRESS         */ 02593000
         L     @07,LCCAPTR                                         0886 02594000
         L     @07,LCCACPUS(,@07)                                  0886 02595000
         L     R13,WSACRSTI(,@07)                                  0886 02596000
*       R5=ADDR(IECINT);            /* GET SLIH ADDRESS              */ 02597000
         L     R5,@CV02934                                         0887 02598000
*       GEN(STM 0,15,0(13));        /* SAVE REGS                     */ 02599000
         STM 0,15,0(13)                                                 02600000
*       GEN( BAL 4,4(5));           /* BRANCH ENTER SLIH TO HANDLE 0889 02601000
*                                      INTERRUPT                     */ 02602000
          BAL 4,4(5)                                                    02603000
*       GEN(LM 0,15,0(13));         /* RESTORE REGS                  */ 02604000
         LM 0,15,0(13)                                                  02605000
*       RFY                                                        0891 02606000
*        (R5,                                                      0891 02607000
*         R11,                                                     0891 02608000
*         R13,                                                     0891 02609000
*         R4) UNRSTD;                                              0891 02610000
*       GO TO DOSIO;                /* TRY SIO AGAIN                 */ 02611000
         B     DOSIO                                               0892 02612000
*     END;                                                         0893 02613000
*   ELSE                                                           0894 02614000
*     GO TO EXIT;                   /* NOT INTERRUPT - RESERVE FAILED*/ 02615000
*   RFY                                                            0895 02616000
*    (R1) UNRSTD;                                                  0895 02617000
*   END RESVSIO;                                                   0896 02618000
*                                                                  0897 02619000
*   /*****************************************************************/ 02620000
*   /*                                                               */ 02621000
*   /* THIS ROUTINE ISSUES THE MAP FUNCTION OF THE IOSGEN MACRO. ON  */ 02622000
*   /* ENTRY R4 MUST CONTAIN A UCB ADDRESS AND R5 MUST CONTAIN THE   */ 02623000
*   /* ADDRESS OF AN 8 BYTE WORK AREA WHERE IOSGEN WILL STORE THE    */ 02624000
*   /* RESULTS OF THE PATH MAPPING. R13 WILL BE DESTROYED. IOSGEN    */ 02625000
*   /* WILL RETURN DIRECTLY TO THE CALLER OF THIS ROUTINE.           */ 02626000
*   /*                                                               */ 02627000
*   /*****************************************************************/ 02628000
*                                                                  0897 02629000
*IOSGENPT:                                                         0897 02630000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0897 02631000
IOSGENPT DS    0H                                                  0898 02632000
*   RFY                                                            0898 02633000
*    (R4,                                                          0898 02634000
*     R5,                                                          0898 02635000
*     R13,                                                         0898 02636000
*     R14) RSTD;                                                   0898 02637000
*   R13=WSACRSTI;                                                  0899 02638000
         L     @15,LCCAPTR                                         0899 02639000
         L     @15,LCCACPUS(,@15)                                  0899 02640000
         L     R13,WSACRSTI(,@15)                                  0899 02641000
*   GEN REFS(UCBCHA,UCBCHM,UCBFL5,UCBLCI,UCBUA,CVTILCH,CVTCSD,CVTPCCAT, 02642000
*       PCCACPUA,PCCACAFM,PCCACAT,CSDCPUAL,CATFLG,LCHTCH,CVTPTR,UCBOB,  02643000
*       UCBWGT,UCBBASE)( IOSGEN MAP,VAR=1);                        0900 02644000
          IOSGEN MAP,VAR=1                                              02645000
*   RFY                                                            0901 02646000
*    (R4,                                                          0901 02647000
*     R5,                                                          0901 02648000
*     R13,                                                         0901 02649000
*     R14) UNRSTD;                                                 0901 02650000
*   END IOSGENPT;                                                  0902 02651000
@EL00009 DS    0H                                                  0902 02652000
@EF00009 DS    0H                                                  0902 02653000
@ER00009 BR    @14                                                 0902 02654000
*LOADWAIT:                                                         0903 02655000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* LOAD A WAIT STATE             */ 02656000
*                                                                  0903 02657000
LOADWAIT DS    0H                                                  0904 02658000
*   /*****************************************************************/ 02659000
*   /*                                                               */ 02660000
*   /* THIS ROUTINE WILL INTERFACE WITH THE STOP/RESTART ROUTINE     */ 02661000
*   /* (IEESTPRS) TO LOAD A WAIT STATE. THE WAIT WILL BE RECORDED BY */ 02662000
*   /* BUILDING A LOGREC RECORD AND CALLING THE RECORD PROCEDURE. THE*/ 02663000
*   /* RESTART LOCK WILL BE OBTAINED AND IEESTPRS WILL BE CALLED. IF */ 02664000
*   /* THE RESTART KEY IS PRESSED, THE CALLER OF THIS PROCEDURE WILL */ 02665000
*   /* BE RETURNED TO. IF THE RESTART LOCK IS NOT OBTAINABLE OR      */ 02666000
*   /* IEESTPRS CANNOT STOP THE SYSTEM, THIS PROCEDURE WILL ASSUME A */ 02667000
*   /* UP SYSTEM AND STOP THIS PROCESSOR BY LOADING A WAIT PSW WITH  */ 02668000
*   /* THE RESTART PSW SETUP TO A RESUME POINT IN THIS PROCEDURE.    */ 02669000
*   /* CALLED BY OPERINF SEG, THE TOP SEG AND THE FRR. INPUT IS THE  */ 02670000
*   /* WAIT STATE CODE IN IRSTWTCD.                                  */ 02671000
*   /*                                                               */ 02672000
*   /*****************************************************************/ 02673000
*                                                                  0904 02674000
*   RFY                                                            0904 02675000
*    (R14,                                                         0904 02676000
*     R0,                                                          0904 02677000
*     R1,                                                          0904 02678000
*     R13,                                                         0904 02679000
*     R15) RSTD;                                                   0904 02680000
*   IRSTLORT=R14;                   /* SAVE RETURN ADDRESS           */ 02681000
         LR    IRSTLORT,R14                                        0905 02682000
*   IRSTMESG=IRSTMESG&&IRSTMESG;    /* CLEAR LOGREC BUFFER           */ 02683000
         XC    IRSTMESG(128),IRSTMESG                              0906 02684000
*   IF IRSTWAIT='1'B THEN           /* IF WAIT RESTARTABLE           */ 02685000
         TM    IRSTWAIT,B'10000000'                                0907 02686000
         BNO   @RF00907                                            0907 02687000
*     LRBHTYPE=LOGRSHDR;            /* SET RESTARTABLE HEADER        */ 02688000
         LA    @12,IRSTMESG                                        0908 02689000
         MVI   LRBHTYPE(@12),X'84'                                 0908 02690000
*   ELSE                                                           0909 02691000
*     LRBHTYPE=LRBHTER;             /* SYSTEM TERMINATION HDR        */ 02692000
         B     @RC00907                                            0909 02693000
@RF00907 LA    @12,IRSTMESG                                        0909 02694000
         MVI   LRBHTYPE(@12),X'81'                                 0909 02695000
*   LRBMSYST='1'B;                  /* IND SYSTEM TERMINATED         */ 02696000
@RC00907 LA    @12,IRSTMESG                                        0910 02697000
         OI    LRBMSYST(@12),B'00100000'                           0910 02698000
*   LRBHSYS=LRBHSW1;                /* IN PROPER HEADER SPOT FOR   0911 02699000
*                                      RECORD MACRO                  */ 02700000
         MVC   LRBHSYS(1,@12),LRBHSW1(@12)                         0911 02701000
*   LRBMSYST='0'B;                  /* ZERO OTHER HEADER BYTE      0912 02702000
*                                      POSITIONS                     */ 02703000
         NI    LRBMSYST(@12),B'11011111'                           0912 02704000
*   LOGDATAA=ADDR(LRBTTER);         /* DATA PTR FOR RCD MACRO        */ 02705000
         LA    @07,LRBTTER(,@12)                                   0913 02706000
         ST    @07,LOGDATAA(,@12)                                  0913 02707000
*   LRBTLNH=(LENGTH(LRBTTER)+LENGTH(LOGTUSR));/* LEN OF DATA IN    0914 02708000
*                                      RECORD                        */ 02709000
         LA    @07,100                                             0914 02710000
         ST    @07,LRBTLNH(,@12)                                   0914 02711000
*   IRSTRDLN=(LENGTH(LRBTTER)+LENGTH(LOGTUSR));/* LEN FOR RECORD   0915 02712000
*                                      MACRO                         */ 02713000
         STH   @07,IRSTRDLN                                        0915 02714000
*   LRBTWSC=IRSTWTCD;               /* WAIT STATE CODE IN RCD        */ 02715000
         MVC   LRBTWSC(4,@12),IRSTWTCD                             0916 02716000
*   STM(R0,R15,LOGREGS);            /* REGS INTO USER FIELD          */ 02717000
         STM   R0,R15,LOGREGS(@12)                                 0917 02718000
*   LOGFRRWK=FRRWORK;               /* FRR WK AREA INTO USER         */ 02719000
         MVC   LOGFRRWK(24,@12),FRRWORK(FRRWPTR)                   0918 02720000
*   LOGCHMSK=IRSTCHMK;              /* LOST CHAN MASK, LOST=1        */ 02721000
         MVC   LOGCHMSK(4,@12),IRSTCHMK                            0919 02722000
*   IRSTRECD='0'B;                  /* IND RECORD(LOGREC) OPTION FOR    02723000
*                                      RECORD MACRO                  */ 02724000
         NI    IRSTRECD,B'10111111'                                0920 02725000
*   CALL RECORD;                    /* RECORD WAIT STATE             */ 02726000
         BAL   @14,RECORD                                          0921 02727000
*   R14=0;                          /* COMPARSION FOR CS INSTR       */ 02728000
         SLR   R14,R14                                             0922 02729000
*   IRSTCPUA=PSACPULA;              /* GET CPUID FOR RESTART WORD  0923 02730000
*                                                            @ZM47978*/ 02731000
         MVC   IRSTCPUA(2),PSACPULA                                0923 02732000
*   IRSTREID=RESTRTID;              /* I/O RESTART ID = IS           */ 02733000
         MVC   IRSTREID(2),@CC02960                                0924 02734000
*   R15=IRSTREST;                   /* LOCKED VALUE                  */ 02735000
         L     R15,IRSTREST                                        0925 02736000
*   CS(R14,R15,CVTRSTWD);           /* OBTAIN RESTART LOCK           */ 02737000
         L     @12,CVTPTR                                          0926 02738000
         CS    R14,@15,CVTRSTWD(@12)                               0926 02739000
*   BC(NOTZERO,LOADIT);             /* NOT OBTAINABLE-LOAD WT        */ 02740000
         BC    7,LOADIT                                            0927 02741000
*   FRRRESTL='1'B;                  /* IND IECVIRST HAS LOCK         */ 02742000
         OI    FRRRESTL(FRRWPTR),B'00000010'                       0928 02743000
*RELOADIT:                                                         0929 02744000
*   ;                               /* BRANCH HERE, RELOAD WT        */ 02745000
RELOADIT DS    0H                                                  0930 02746000
*   R0=IRSTWTCD;                    /* WAIT STATE CODE               */ 02747000
         L     R0,IRSTWTCD                                         0930 02748000
*   R1=0;                           /* NOT QUIESCE OPER CMD          */ 02749000
         SLR   R1,R1                                               0931 02750000
*   R13=WSACRSTI;                   /* POINT TO SAVE AREA            */ 02751000
         L     @07,LCCAPTR                                         0932 02752000
         L     @07,LCCACPUS(,@07)                                  0932 02753000
         L     R13,WSACRSTI(,@07)                                  0932 02754000
*   PSAWTCOD=ADDR(IRSTCHMK);        /* PARM FOR OPERATOR             */ 02755000
         LA    @12,IRSTCHMK                                        0933 02756000
         ST    @12,PSAWTCOD                                        0933 02757000
*   PSAACTCD=0;                     /* INITIAL(DEFAULT) VALUE OF OPER   02758000
*                                      SUPPLIED ACT CD               */ 02759000
         MVI   PSAACTCD,X'00'                                      0934 02760000
*   CALL IEESTPRS;                  /* STOP THE SYSTEM               */ 02761000
*                                                                  0935 02762000
         L     @12,CVTPTR                                          0935 02763000
         L     @15,CVTSTPRS(,@12)                                  0935 02764000
         BALR  @14,@15                                             0935 02765000
*   /*****************************************************************/ 02766000
*   /*                                                               */ 02767000
*   /* REACH HERE IF OPERATOR HITS RESTART KEY OR IEESTPRS CANNOT    */ 02768000
*   /* STOP THE SYSTEM                                               */ 02769000
*   /*                                                               */ 02770000
*   /*****************************************************************/ 02771000
*                                                                  0936 02772000
*   IF R15=0&                       /* IF OPER HIT RESTART BUT       */ 02773000
*       IRSTWAIT='0'B THEN          /* NOT RESTARTABLE WAIT          */ 02774000
         LTR   R15,R15                                             0936 02775000
         BNZ   @RF00936                                            0936 02776000
         TM    IRSTWAIT,B'10000000'                                0936 02777000
         BZ    @RT00936                                            0936 02778000
*     GO TO RELOADIT;               /* RELOAD WAIT STATE             */ 02779000
*   ELSE                                                           0938 02780000
*     ;                                                            0938 02781000
@RF00936 DS    0H                                                  0939 02782000
*   IF R15^=0 THEN                  /* IF IEESTPRS CANNOT STOP SYS   */ 02783000
         LTR   R15,R15                                             0939 02784000
         BZ    @RF00939                                            0939 02785000
*     DO;                           /* ASSUME UP, DO IT HERE         */ 02786000
*LOADIT:                                                           0941 02787000
*       ;                           /* COULD NOT OBTAIN RESTART LOCK    02788000
*                                      STOP SYSTEM HERE              */ 02789000
LOADIT   DS    0H                                                  0942 02790000
*       IRSTRPSW=FLCRNPSW;          /* SAVE SYS RESTART NEW PSW      */ 02791000
         MVC   IRSTRPSW(8),FLCRNPSW                                0942 02792000
*RELDIT:                                                           0943 02793000
*       ;                           /* BRANCH HERE TO RELOAD WAIT    */ 02794000
RELDIT   DS    0H                                                  0944 02795000
*       IRSTWSYS=HWAITPSW;          /* FIRST HALF OF WAIT PSW        */ 02796000
         MVC   IRSTWSYS(4),@CB02969                                0944 02797000
*       PSAWTCOD=ADDR(IRSTCHMK);    /* PARM FOR OPERATOR             */ 02798000
         LA    @12,IRSTCHMK                                        0945 02799000
         ST    @12,PSAWTCOD                                        0945 02800000
*       PSAACTCD=0;                 /* DEFAULT ACTION CODE           */ 02801000
         MVI   PSAACTCD,X'00'                                      0946 02802000
*       FRRRSPSW='1'B;              /* IND RESTART PSW CHANGED       */ 02803000
         OI    FRRRSPSW(FRRWPTR),B'00000100'                       0947 02804000
*       CALL IGFPSAD0;              /* ALLOW MODIFICATION OF FLC     */ 02805000
         L     @15,@CV02941                                        0948 02806000
         BALR  @14,@15                                             0948 02807000
*       FLCRNPSW(1:4)=HRESTPSW;     /* FIRST HALF RESTART PSW        */ 02808000
         MVC   FLCRNPSW(4),@CB02971                                0949 02809000
*       GEN(STOSM FLCRNPSW,X'00');  /* CONTINUE WITH CURR SYS MASK   */ 02810000
         STOSM FLCRNPSW,X'00'                                           02811000
*       FLCRNPSW(5:8)=ADDR(RESTARTP);/* RESTART POINT ADDRESS        */ 02812000
         LA    @12,RESTARTP                                        0951 02813000
         ST    @12,FLCRNPSW+4                                      0951 02814000
*       CALL IGFPSAE0;              /* PREVENT MODIFICATION OF FLC   */ 02815000
         L     @15,@CV02942                                        0952 02816000
         BALR  @14,@15                                             0952 02817000
*       LPSW(IRSTWPSW);             /* LOAD WAIT STATE PSW           */ 02818000
*                                                                  0953 02819000
         LPSW  IRSTWPSW                                            0953 02820000
*       /*************************************************************/ 02821000
*       /*                                                           */ 02822000
*       /* CONTROL REACHES HERE ONLY IF OPERATOR HITS THE RESTART KEY*/ 02823000
*       /*                                                           */ 02824000
*       /*************************************************************/ 02825000
*                                                                  0954 02826000
*RESTARTP:                                                         0954 02827000
*       ;                                                          0954 02828000
RESTARTP DS    0H                                                  0955 02829000
*       IF IRSTWAIT='0'B THEN       /* IF NON RESTARTABLE WAIT       */ 02830000
         TM    IRSTWAIT,B'10000000'                                0955 02831000
         BZ    @RT00955                                            0955 02832000
*         GO TO RELDIT;             /* GO RE LOAD WAIT STATE         */ 02833000
*       ELSE                                                       0957 02834000
*         ;                                                        0957 02835000
*     END;                          /* END IECVIRST STOP SYSTEM      */ 02836000
*   ELSE                                                           0959 02837000
*     ;                             /* IEESTPRS DID STOP SYSTEM      */ 02838000
@RF00939 DS    0H                                                  0960 02839000
*   IF FRRINCOD=MACHCKIN THEN       /* IF OPER ACT CODE REQUIRED     */ 02840000
         CLI   FRRINCOD(FRRWPTR),1                                 0960 02841000
         BNE   @RF00960                                            0960 02842000
*     IRSTOPAC=PSAACTCD;            /* SAVE FOR LATER PROCESSING     */ 02843000
         MVC   IRSTOPAC(1),PSAACTCD                                0961 02844000
*   ELSE                                                           0962 02845000
*     ;                                                            0962 02846000
@RF00960 DS    0H                                                  0963 02847000
*   IF FRRRSPSW='1'B THEN           /* IF RESTART PSW CHANGED        */ 02848000
         TM    FRRRSPSW(FRRWPTR),B'00000100'                       0963 02849000
         BNO   @RF00963                                            0963 02850000
*     DO;                                                          0964 02851000
*       CALL IGFPSAD0;              /* ALLOW MODIFICATION OF FLC     */ 02852000
         L     @15,@CV02941                                        0965 02853000
         BALR  @14,@15                                             0965 02854000
*       FLCRNPSW=IRSTRPSW;          /* RESTORE RESTART PSW           */ 02855000
         MVC   FLCRNPSW(8),IRSTRPSW                                0966 02856000
*       CALL IGFPSAE0;              /* PREVENT MODIFICATION OF FLC   */ 02857000
         L     @15,@CV02942                                        0967 02858000
         BALR  @14,@15                                             0967 02859000
*       FRRRSPSW='0'B;              /* RESTART PSW NOT CHANGED       */ 02860000
         NI    FRRRSPSW(FRRWPTR),B'11111011'                       0968 02861000
*     END;                                                         0969 02862000
*   ELSE                                                           0970 02863000
*     ;                                                            0970 02864000
@RF00963 DS    0H                                                  0971 02865000
*   IF FRRRESTL='1'B THEN           /* IF RESTART LOCK HELD          */ 02866000
         TM    FRRRESTL(FRRWPTR),B'00000010'                       0971 02867000
         BNO   @RF00971                                            0971 02868000
*     DO;                                                          0972 02869000
*       CVTRSTWD=0;                 /* FREE RESTART RESOURCE         */ 02870000
         L     @12,CVTPTR                                          0973 02871000
         SLR   @07,@07                                             0973 02872000
         ST    @07,CVTRSTWD(,@12)                                  0973 02873000
*       FRRRESTL='0'B;              /* IND RESTART LOCK NOT HELD     */ 02874000
         NI    FRRRESTL(FRRWPTR),B'11111101'                       0974 02875000
*     END;                                                         0975 02876000
*   ELSE                                                           0976 02877000
*     ;                                                            0976 02878000
@RF00971 DS    0H                                                  0977 02879000
*   R14=IRSTLORT;                   /* RESTORE RETURN ADDRESS        */ 02880000
         LR    R14,IRSTLORT                                        0977 02881000
*   RFY                                                            0978 02882000
*    (R14,                                                         0978 02883000
*     R0,                                                          0978 02884000
*     R1,                                                          0978 02885000
*     R13,                                                         0978 02886000
*     R15) UNRSTD;                                                 0978 02887000
*   END LOADWAIT;                                                  0979 02888000
@EL00010 DS    0H                                                  0979 02889000
@EF00010 DS    0H                                                  0979 02890000
@ER00010 BR    @14                                                 0979 02891000
*IRSTFRR:                                                          0980 02892000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0980 02893000
*                                                                  0980 02894000
IRSTFRR  DS    0H                                                  0981 02895000
*   /*****************************************************************/ 02896000
*   /*                                                               */ 02897000
*   /* THIS SEGMENT IS THE FUNCTIONAL RECOVERY ROUTINE FOR IECVIRST. */ 02898000
*   /* IT WILL RELEASE RESOURCES OBTAINED BY IECVIRST OR CAUSE THEN  */ 02899000
*   /* TO BE RELEASED BY RTM. IF ANY RESERVES HAVE BEEN RESET AND NOT*/ 02900000
*   /* RE-RESERVED THEN A NON RESTARTABLE WAIT STATE WILL BE LOADED. */ 02901000
*   /* OTHERWISE, CONTROL WILL BE RETURNED TO RTM AND IT WILL        */ 02902000
*   /* PERCOLATE.                                                    */ 02903000
*   /*                                                               */ 02904000
*   /*****************************************************************/ 02905000
*                                                                  0981 02906000
*   RFY                                                            0981 02907000
*    (SDWAPTR,                                                     0981 02908000
*     R11,                                                         0981 02909000
*     R14) RSTD;                                                   0981 02910000
*   FRRWPTR=SDWAPARM;               /* GET ADDRESSABILITY TO FRR   0982 02911000
*                                      PARMS                         */ 02912000
         L     FRRWPTR,SDWAPARM(,SDWAPTR)                          0982 02913000
*   R9=FRRBASE1;                    /* ESTABLISH BASE REGISTER 1     */ 02914000
         L     R9,FRRBASE1(,FRRWPTR)                               0983 02915000
*   R10=FRRBASE2;                   /* ESTABLISH BASE REGISTER 2     */ 02916000
         L     R10,FRRBASE2(,FRRWPTR)                              0984 02917000
*   IRSTWPTR=FRRGWPTR;              /* GET ADDRESSABILITY TO GENERAL    02918000
*                                      WORK AREA                     */ 02919000
         L     IRSTWPTR,FRRGWPTR(,FRRWPTR)                         0985 02920000
*   FRRSDWA=SDWAPTR;                /* ADDR OF SDWA IN FRRWORK IF  0986 02921000
*                                      WAIT STATE LOADED             */ 02922000
         ST    SDWAPTR,FRRSDWA(,FRRWPTR)                           0986 02923000
*   R11=R14;                        /* SAVE R14 TO RETURN TO RTM     */ 02924000
         LR    R11,R14                                             0987 02925000
*   RFY                                                            0988 02926000
*    (R14) UNRSTD;                                                 0988 02927000
*   SDWAMODN=MODNAME;                                              0989 02928000
         MVC   SDWAMODN(8,SDWAPTR),@CC02996                        0989 02929000
*   SDWACSCT=MODNAME;                                              0990 02930000
         MVC   SDWACSCT(8,SDWAPTR),@CC02996                        0990 02931000
*   SDWAREXN=FRRNAME;                                              0991 02932000
         MVC   SDWAREXN(8,SDWAPTR),@CC02998                        0991 02933000
*   SDWAURAL=LENGTH(RCDVRA);        /* LEN OF USER DATA              */ 02934000
         MVI   SDWAURAL(SDWAPTR),X'1C'                             0992 02935000
*   SDWAHEX='1'B;                   /* DUMP USER DATA IN HEX         */ 02936000
         OI    SDWAHEX(SDWAPTR),B'10000000'                        0993 02937000
*   RCDFRRW=FRRWORK;                /* RCD FRRWORK AREA              */ 02938000
         MVC   RCDFRRW(24,SDWAPTR),FRRWORK(FRRWPTR)                0994 02939000
*   RCDCHMSK=RCDCHMSK&&RCDCHMSK;    /* CLEAR LOST CHAN MASK          */ 02940000
         XC    RCDCHMSK(4,SDWAPTR),RCDCHMSK(SDWAPTR)               0995 02941000
*   SDWARCRD='1'B;                  /* RECORD ERROR                  */ 02942000
         OI    SDWARCRD(SDWAPTR),B'10000000'                       0996 02943000
*   IF FRRSALOC='1'B THEN           /* IF SALLOC LOCK HELD           */ 02944000
         TM    FRRSALOC(FRRWPTR),B'10000000'                       0997 02945000
         BNO   @RF00997                                            0997 02946000
*     SDWASALL='1'B;                /* INDICATE RTM SHOULD FREE IT   */ 02947000
         OI    SDWASALL(SDWAPTR),B'00000100'                       0998 02948000
*   IF FRRSUCB='1'B THEN            /* IF UCB LOCK HELD              */ 02949000
@RF00997 TM    FRRSUCB(FRRWPTR),B'01000000'                        0999 02950000
         BNO   @RF00999                                            0999 02951000
*     DO;                                                          1000 02952000
*       SDWAIUCB='1'B;              /* INDICATE RTM SHOULD FREE LOCK */ 02953000
         OI    SDWAIUCB(SDWAPTR),B'10000000'                       1001 02954000
*       SDWAIULW=IRSTUCBL;          /* GIVE LOCK ADDRESS TO RTM      */ 02955000
         MVC   SDWAIULW(4,SDWAPTR),IRSTUCBL                        1002 02956000
*     END;                                                         1003 02957000
*   IF FRRRESTL='1'B                /* IF RESTART LOCK IS HELD       */ 02958000
*     THEN                                                         1004 02959000
@RF00999 TM    FRRRESTL(FRRWPTR),B'00000010'                       1004 02960000
         BNO   @RF01004                                            1004 02961000
*     CVTRSTWD=0;                   /* RELEASE RESTART LOCK          */ 02962000
         L     @12,CVTPTR                                          1005 02963000
         SLR   @07,@07                                             1005 02964000
         ST    @07,CVTRSTWD(,@12)                                  1005 02965000
*   CALL IGFPSAD0;                  /* ALLOW MODIFICATION OF FLC     */ 02966000
@RF01004 L     @15,@CV02941                                        1006 02967000
         BALR  @14,@15                                             1006 02968000
*   IF FRRIOPSW='1'B                /* IF I/O NEW PSW MODIFIED       */ 02969000
*     THEN                                                         1007 02970000
         TM    FRRIOPSW(FRRWPTR),B'00000001'                       1007 02971000
         BNO   @RF01007                                            1007 02972000
*     FLCINPSW(5:8)=IRSTIPSW;       /* RESTORE I/O NEW PSW           */ 02973000
         MVC   FLCINPSW+4(4),IRSTIPSW                              1008 02974000
*   IF FRRCTLR2='1'B THEN           /* RESTORE CONTROL REG2 IF     1009 02975000
*                                      MODIFIED                      */ 02976000
@RF01007 TM    FRRCTLR2(FRRWPTR),B'10000000'                       1009 02977000
         BNO   @RF01009                                            1009 02978000
*     DO;                                                          1010 02979000
*       GEN REFS(IRSTCTLR)( LCTL 2,2,IRSTCTLR);                    1011 02980000
          LCTL 2,2,IRSTCTLR                                             02981000
*     END;                                                         1012 02982000
*   IF FRRRSPSW='1'B                /* IF RESTART PSW MODIFIED       */ 02983000
*     THEN                                                         1013 02984000
@RF01009 TM    FRRRSPSW(FRRWPTR),B'00000100'                       1013 02985000
         BNO   @RF01013                                            1013 02986000
*     FLCRNPSW=IRSTRPSW;            /* RESTORE RESTART PSW           */ 02987000
         MVC   FLCRNPSW(8),IRSTRPSW                                1014 02988000
*   FLCLCL='FFFFFFFF'X;             /* FOX OUT LIMITED CHANNEL LOGOUT*/ 02989000
@RF01013 MVC   FLCLCL(4),@CB03216                                  1015 02990000
*   CALL IGFPSAE0;                  /* PREVENT MODIFICATION OF FLC   */ 02991000
         L     @15,@CV02942                                        1016 02992000
         BALR  @14,@15                                             1016 02993000
*   PCCAELAD->LOGOUT='FFFFFFFF'X;   /* FOX OUT I/O EXTENDED LOGOUT   */ 02994000
         L     @12,PCCAPTR                                         1017 02995000
         L     @12,PCCAELAD(,@12)                                  1017 02996000
         MVC   LOGOUT(4,@12),@CB03216                              1017 02997000
*   IF IRSTWPTR^=0 THEN             /* IF WORK AREA EXISTS           */ 02998000
         LTR   IRSTWPTR,IRSTWPTR                                   1018 02999000
         BZ    @RF01018                                            1018 03000000
*     DO;                           /* LOAD WAIT/TAKE DUMP/FREEMAIN  */ 03001000
*       RCDCHMSK=IRSTCHMK;          /* RECORD LOST CHAN MASK         */ 03002000
         MVC   RCDCHMSK(4,SDWAPTR),IRSTCHMK                        1020 03003000
*       IF FRRRESVE='1'B|FRRCLRCH='0'B THEN/* IF RESERVES WERE FOUND    03004000
*                                      OR                            */ 03005000
         TM    FRRRESVE(FRRWPTR),B'00010000'                       1021 03006000
         BO    @RT01021                                            1021 03007000
         TM    FRRCLRCH(FRRWPTR),B'01000000'                       1021 03008000
         BNZ   @RF01021                                            1021 03009000
@RT01021 DS    0H                                                  1022 03010000
*         DO;                       /* CLRCH HAS NOT RESET RESERVES-    03011000
*                                      WAIT STATE                    */ 03012000
*           IRSTWTCD=CANTRESV;      /* SET WAIT STATE CODE TO 04E    */ 03013000
         MVC   IRSTWTCD(4),@CF02952                                1023 03014000
*           IRSTWAIT='0'B;          /* INDICATE NON-RESTARTABLE WAIT */ 03015000
         NI    IRSTWAIT,B'01111111'                                1024 03016000
*           FRRLDWT='1'B;           /* SET WAIT REQUIRED INDICATOR   */ 03017000
         OI    FRRLDWT(FRRWPTR),B'00001000'                        1025 03018000
*         END;                                                     1026 03019000
*       PCCAIRST='0'B;              /* IND DONE RECOVERING CHANNELS  */ 03020000
@RF01021 L     @12,PCCAPTR                                         1027 03021000
         NI    PCCAIRST(@12),B'01111111'                           1027 03022000
*       IF PCCAEXDM='1'B&           /* IF MORE CHANNELS LOST AND     */ 03023000
*           FRRLDWT='0'B THEN       /* WAIT STATE NOT ALREADY SET    */ 03024000
         TM    PCCAEXDM(@12),B'01000000'                           1028 03025000
         BNO   @RF01028                                            1028 03026000
         TM    FRRLDWT(FRRWPTR),B'00001000'                        1028 03027000
         BNZ   @RF01028                                            1028 03028000
*         DO;                                                      1029 03029000
*           IRSTWTCD=MORECHLT;      /* MORE CHANNELS LOST            */ 03030000
         MVC   IRSTWTCD(4),@CF02944                                1030 03031000
*           IRSTWAIT='0'B;          /* WAIT STATE NOT RESTARTABLE    */ 03032000
         NI    IRSTWAIT,B'01111111'                                1031 03033000
*           FRRLDWT='1'B;           /* SET WAIT REQUIRED INDICATOR   */ 03034000
         OI    FRRLDWT(FRRWPTR),B'00001000'                        1032 03035000
*         END;                                                     1033 03036000
*       ELSE                                                       1034 03037000
*         ;                                                        1034 03038000
@RF01028 DS    0H                                                  1035 03039000
*       IF FRRLDWT='1'B THEN                                       1035 03040000
         TM    FRRLDWT(FRRWPTR),B'00001000'                        1035 03041000
         BNO   @RF01035                                            1035 03042000
*         CALL LOADWAIT;            /* LOAD NON-RESTARTABLE WAIT   1036 03043000
*                                      STATE                         */ 03044000
         BAL   @14,LOADWAIT                                        1036 03045000
*       RFY                                                        1037 03046000
*        (R4,                                                      1037 03047000
*         R5) RSTD;                                                1037 03048000
@RF01035 DS    0H                                                  1038 03049000
*       R4=CVTSDBF&HIOFF;           /* GET DUMP BUFFER LOCKWORD      */ 03050000
         L     @12,CVTPTR                                          1038 03051000
         L     R4,CVTSDBF(,@12)                                    1038 03052000
         N     R4,@CF02989                                         1038 03053000
*       R5=R4|HIGHORDR;             /* TURN ON LOCK BIT IN REGISTER  */ 03054000
         LR    R5,R4                                               1039 03055000
         O     R5,@CF02987                                         1039 03056000
*       CS(R4,R5,CVTSDBF);          /* OBTAIN DUMP BUFFER LOCK IF  1040 03057000
*                                      AVAIL                         */ 03058000
         CS    R4,@05,CVTSDBF(@12)                                 1040 03059000
*       BC(7,NODUMP);               /* SKIP DUMP IF NO BUFFER        */ 03060000
         BC    7,NODUMP                                            1041 03061000
*       R4=ADDR(SDBDATA);           /* GET ADDR OF DATA AREA IN    1042 03062000
*                                      BUFFER                        */ 03063000
         L     @12,CVTPTR                                          1042 03064000
         L     @12,CVTSDBF(,@12)                                   1042 03065000
         LA    R4,SDBDATA(,@12)                                    1042 03066000
