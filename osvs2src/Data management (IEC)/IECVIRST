         BNE   @RT00308                                            0308 00417000
*         GO TO RELOADWT;           /* GO RE-LOAD WAIT STATE         */ 00418000
*       ELSE                                                       0310 00419000
*         ;                                                        0310 00420000
@RF00308 DS    0H                                                  0311 00421000
*     END;                          /* END COMM WITH OPER            */ 00422000
*   ELSE                                                           0312 00423000
*     ;                                                            0312 00424000
*                                                                  0312 00425000
@RF00294 DS    0H                                                  0313 00426000
*   /*****************************************************************/ 00427000
*   /*                                                               */ 00428000
*   /* THIS SEGMENT WILL ATTEMPT TO RECOVER THE LOST CHANNELS. IF THE*/ 00429000
*   /* LOST CHANNEL(S) WAS DETECTED DURING I/O INTERRUPT PROCESSING, */ 00430000
*   /* SEGMENT HUNGINTF IS INCLUDED TO RECOVER A HUNG INTERFACE      */ 00431000
*   /* CONDITION. ELSE, SEGMENT MACHCHK IS INCLUDED TO RECOVER LOST  */ 00432000
*   /* CHANNELS DETECTED DURING PROCESSING FOR AN EXTERNAL DAMAGE    */ 00433000
*   /* MACHINE CHECK. MESSAGE IEA410I IS ISSUED FOR ANY CHANNEL THAT */ 00434000
*   /* FAILED TO RECOVER.                                            */ 00435000
*   /*                                                               */ 00436000
*   /*****************************************************************/ 00437000
*                                                                  0313 00438000
*   FRRCLRCH='1'B;                  /* IND CHANNELS RESET FOR FRR    */ 00439000
         OI    FRRCLRCH(FRRWPTR),B'01000000'                       0313 00440000
*   IF FRRINCOD=CHANCKIN THEN       /* IF SCHED VIA CHAN CK HANDLER  */ 00441000
         CLI   FRRINCOD(FRRWPTR),2                                 0314 00442000
         BNE   @RF00314                                            0314 00443000
*     DO;                           /* RECOVER HUNG INTERFACE        */ 00444000
*                                                                  0315 00445000
*       /*************************************************************/ 00446000
*       /*                                                           */ 00447000
*       /* THIS SEGMENT WILL ATTEMPT TO RECOVER A HUNG INTERFACE     */ 00448000
*       /* CONDITION. IT IS INCLUDED BY SEGMENT RCVCHANS WHEN        */ 00449000
*       /* IECVIRST IS SCHEDULED BY THE CCH. THIS SEGMENT WILL SCAN  */ 00450000
*       /* THE COPIED CAT FOR A CHANNEL(S) SUFFERING A HUNG INTERFACE*/ 00451000
*       /* CONDITION. A CLEAR CHANNEL (CLRCH) INSTRUCTION WILL THEN  */ 00452000
*       /* BE ISSUED TO THE CHANNEL. A TCH INSTRUCTION WILL THEN BE  */ 00453000
*       /* ISSUED. A CC=0 RETURNED FROM THE CLRCH INSTRUCTION AND THE*/ 00454000
*       /* TCH INSTRUCTION INDICATES SUCCESSFUL RECOVERY OF THE      */ 00455000
*       /* CHANNEL. IF ANY OTHER CC IS RETURNED FROM EITHER          */ 00456000
*       /* INSTRUCTION, THE CHANNEL IS ASSUMED PERMANENTLY LOST AND  */ 00457000
*       /* WILL NOT BE REUSED.                                       */ 00458000
*       /*                                                           */ 00459000
*       /*************************************************************/ 00460000
*                                                                  0316 00461000
*       RFY                                                        0316 00462000
*         R15 RSTD;                                                0316 00463000
*       IRSTCATP=ADDR(IRSTCAT);     /* ADDRESS OF COPIED CAT         */ 00464000
         LA    IRSTCATP,IRSTCAT                                    0317 00465000
*       RFY                                                        0318 00466000
*         CAT BASED(IRSTCATP);                                     0318 00467000
*       DO IRSTLOOP=1 TO LENGTH(IRSTCAT)/LENGTH(CAT);/* SCAN COPIED     00468000
*                                      CAT                           */ 00469000
         LA    @14,1                                               0319 00470000
         STC   @14,IRSTLOOP                                        0319 00471000
@DL00319 DS    0H                                                  0320 00472000
*         IRSTCHAN=IRSTLOOP-1;      /* CHANNEL ADDRESS               */ 00473000
         BCTR  @14,0                                               0320 00474000
         STC   @14,IRSTCHAN                                        0320 00475000
*         IF CATCCRST='1'B THEN     /* IF HUNG INTERFACE CONDITION   */ 00476000
         TM    CATCCRST(IRSTCATP),B'00100000'                      0321 00477000
         BNO   @RF00321                                            0321 00478000
*           DO;                     /* RECOVER CHANNEL               */ 00479000
*             R15=IRSTCHAN*256;     /* CHAN ADDR IN POS FOR INSTR    */ 00480000
         SLA   @14,8                                               0323 00481000
         LR    R15,@14                                             0323 00482000
*             GEN REFS(R15);                                       0324 00483000
         DC    X'9F01F000'              CLEAR CHANNEL                   00484000
*             BC(NOTZERO,STILHUNG); /* IF CC^0, ASSUME STILL HUNG    */ 00485000
         BC    7,STILHUNG                                          0325 00486000
*             TCH(R15);             /* TEST CHANNEL                  */ 00487000
         TCH   0(R15)                                              0326 00488000
*             BC(CC3,STILHUNG);     /* IF CC=3, ASSUME STILL HUNG    */ 00489000
         BC    1,STILHUNG                                          0327 00490000
*             IF IRSTCHAN=FRRINCHN THEN/* IF THE CHAN PART OF THE  0328 00491000
*                                      DEVICE ADDR PASSED AS INPUT BY   00492000
*                                      CCH IS THE SAME AS THE ONE WE    00493000
*                                      ARE PROCESSING                */ 00494000
         CLC   IRSTCHAN(1),FRRINCHN(FRRWPTR)                       0328 00495000
         BNE   @RF00328                                            0328 00496000
*               DO;                 /* SEE IF STILL HUNG             */ 00497000
*                 R15=FRRINPUT;     /* DEV ADDR STORED BY HDWARE ON     00498000
*                                      I/O INT INDICATING HUNG INTF  */ 00499000
         L     R15,FRRINPUT(,FRRWPTR)                              0330 00500000
*                 TIO(R15);         /* GO OUT OVER INTERFACE         */ 00501000
         TIO   0(R15)                                              0331 00502000
*                 BC(NOTCC1,CHANRCOV);/* ASSUME ANYTHING BUT HUNG  0332 00503000
*                                      INTF INDICATES CHAN USABLE    */ 00504000
         BC    11,CHANRCOV                                         0332 00505000
*                 IF ICC='1'B&HUNGINF='1'B THEN                    0333 00506000
         TM    ICC,B'00000010'                                     0333 00507000
         BNO   @RF00333                                            0333 00508000
         TM    HUNGINF,B'00100000'                                 0333 00509000
         BO    @RT00333                                            0333 00510000
*                   GO TO STILHUNG;                                0334 00511000
*                 ELSE                                             0335 00512000
*                   ;                                              0335 00513000
@RF00333 DS    0H                                                  0336 00514000
*CHANRCOV:                                                         0336 00515000
*                 ;                 /* BRANCH HERE IF TIO DOES NOT 0336 00516000
*                                      STORE CSW                     */ 00517000
CHANRCOV DS    0H                                                  0337 00518000
*               END;                /* END SEE IF STILL HUNG         */ 00519000
*             ELSE                                                 0338 00520000
*               ;                   /* ASSUME CHANNEL RECOVERED      */ 00521000
@RF00328 DS    0H                                                  0339 00522000
*             CATNOP='0'B;          /* SET OPERATINAL IN COPIED CAT  */ 00523000
         NI    CATNOP(IRSTCATP),B'10111111'                        0339 00524000
*             EVAL(ADDR(PCCACAT)+(IRSTCHAN*LENGTH(CAT)))->CATNOP='0'B;  00525000
         L     @14,PCCAPTR                                         0340 00526000
         LA    @14,PCCACAT(,@14)                                   0340 00527000
         SLR   @11,@11                                             0340 00528000
         IC    @11,IRSTCHAN                                        0340 00529000
         SLA   @11,3                                               0340 00530000
         ALR   @14,@11                                             0340 00531000
         NI    CATNOP(@14),B'10111111'                             0340 00532000
*                                   /* SET OPERATIONAL IN REAL CAT   */ 00533000
*STILHUNG:                                                         0341 00534000
*             ;                                                    0341 00535000
STILHUNG DS    0H                                                  0342 00536000
*           END;                    /* END RECOVER CHANNEL           */ 00537000
*         ELSE                                                     0343 00538000
*           ;                                                      0343 00539000
@RF00321 DS    0H                                                  0344 00540000
*         IRSTCATP=IRSTCATP+LENGTH(CAT);/* ADR NEXT ENTRY IN COPIED     00541000
*                                      CAT                           */ 00542000
         AH    IRSTCATP,@CH00101                                   0344 00543000
*       END;                                                       0345 00544000
         LA    @14,1                                               0345 00545000
         SLR   @11,@11                                             0345 00546000
         IC    @11,IRSTLOOP                                        0345 00547000
         ALR   @14,@11                                             0345 00548000
         STC   @14,IRSTLOOP                                        0345 00549000
         CH    @14,@CH00830                                        0345 00550000
         BNH   @DL00319                                            0345 00551000
*       RFY                                                        0346 00552000
*         R15 UNRSTD;                                              0346 00553000
*       RFY                                                        0347 00554000
*         CAT BASED(CATPTR);                                       0347 00555000
*     END;                          /* END RECOVER HUNG INTERFACE    */ 00556000
*   ELSE                            /* SCHED VIA MACH CK HANDLER     */ 00557000
*     DO;                           /* RECOVER LOST CHANNELS         */ 00558000
*                                                                  0349 00559000
         B     @RC00314                                            0349 00560000
@RF00314 DS    0H                                                  0350 00561000
*       /*************************************************************/ 00562000
*       /*                                                           */ 00563000
*       /* THIS SEGMENT ATTEMPTS TO RECOVER LOST CHANNEL(S) WHICH    */ 00564000
*       /* WERE DETECTED DURING PROCESSING OF AN EXTERNAL DAMAGE     */ 00565000
*       /* MACHINE CHECK. IT IS INCLUDED BY SEGMENT RCVCHANS. A CLRCH*/ 00566000
*       /* INSTRUCTION IS ISSUED TO EVERY (***NOTE EVERY***) CHANNEL */ 00567000
*       /* THAT RETURNS CC=3 TO A TCH INSTRUCTION. THIS IS TO INSURE */ 00568000
*       /* THAT IN THE CASE OF A HARD GROUP ERROR, EACH CHANNEL IN   */ 00569000
*       /* THE GROUP IS RESET. A CLRCH INSTRUCTION MUST BE ISSUED TO */ 00570000
*       /* EVERY CHANNEL IN A GROUP IF A IMPL IS TO BE DONE. AFTER   */ 00571000
*       /* ALL THE NECESSARY CLRCH INSTRUCTIONS HAVE BEEN DONE, THE  */ 00572000
*       /* OPERATOR SUPPLIED ACTION CODE IS EXAMINED. IF THE CODE IS */ 00573000
*       /* 02, SEGMENT SETDIE IS INCLUDED TO WAIT FOR A POSSIBLE IMPL*/ 00574000
*       /* TO COMPLETE. THE LOST CHANNELS ARE THEN TESTED FOR        */ 00575000
*       /* AVAILABILITY. FOR EACH LOST CHANNEL THAT RETURNS CC=0 TO A*/ 00576000
*       /* TCH INSTRUCTION, THE CATNOP FLAG IS TURNED OFF. ANY LOST  */ 00577000
*       /* CHANNEL THAT RETURNS A CC OTHER THAN 0, IS CONSIDERED     */ 00578000
*       /* PERMANENTLY LOST AND ITS RELATED CATNOP FLAG IS LEFT ON.  */ 00579000
*       /* IF THE OPERATOR SUPPLIED ACTION CODE IS 01, THE OPERATOR  */ 00580000
*       /* DOES NOT WANT TO RE-USE THE CHANNELS AND THEY ARE         */ 00581000
*       /* CONSIDERED PERMANENTLY LOST.                              */ 00582000
*       /*                                                           */ 00583000
*       /*************************************************************/ 00584000
*                                                                  0350 00585000
*       RFY                                                        0350 00586000
*         R15 RSTD;                                                0350 00587000
*       DO IRSTLOOP=1 TO LENGTH(IRSTCAT)/LENGTH(CAT);/* CHECK ALL  0351 00588000
*                                      CHANNELS                      */ 00589000
         LA    @14,1                                               0351 00590000
         STC   @14,IRSTLOOP                                        0351 00591000
@DL00351 DS    0H                                                  0352 00592000
*         IRSTCHAN=IRSTLOOP-1;      /* CHANNEL ADDRESS               */ 00593000
         BCTR  @14,0                                               0352 00594000
         STC   @14,IRSTCHAN                                        0352 00595000
*         R15=IRSTCHAN*256;         /* OPERAND FOR TCH INSTRUCTION   */ 00596000
         SLA   @14,8                                               0353 00597000
         LR    R15,@14                                             0353 00598000
*         TCH(R15);                 /* TEST CHANNEL                  */ 00599000
         TCH   0(R15)                                              0354 00600000
*         BC(NOTCC3,CHANOK);        /* IF NOT CC=3, CHAN STILL OPER  */ 00601000
         BC    14,CHANOK                                           0355 00602000
*         GEN REFS(R15);                                           0356 00603000
    DC    X'9F01F000'                  CLEAR CHANNEL                    00604000
*CHANOK:                                                           0357 00605000
*         ;                         /* BRANCH HERE AFTER TEST CHANNEL   00606000
*                                      IF CHAN STILL OPERATIONAL     */ 00607000
CHANOK   DS    0H                                                  0358 00608000
*       END;                                                       0358 00609000
         LA    @14,1                                               0358 00610000
         SLR   @12,@12                                             0358 00611000
         IC    @12,IRSTLOOP                                        0358 00612000
         ALR   @14,@12                                             0358 00613000
         STC   @14,IRSTLOOP                                        0358 00614000
         CH    @14,@CH00830                                        0358 00615000
         BNH   @DL00351                                            0358 00616000
*       RFY                                                        0359 00617000
*         R15 UNRSTD;                                              0359 00618000
*       IF IRSTOPAC=ACDOWAIT&       /* IF OPER REQ WAIT, REUSE CHAN  */ 00619000
*           PCCAEXDM='0'B THEN      /* 'MORE' CHANS NOT LOST         */ 00620000
         CLI   IRSTOPAC,X'02'                                      0360 00621000
         BNE   @RF00360                                            0360 00622000
         L     @12,PCCAPTR                                         0360 00623000
         TM    PCCAEXDM(@12),B'01000000'                           0360 00624000
         BNZ   @RF00360                                            0360 00625000
*         DO;                       /* WAIT FOR IMPL, TEST RECOVERY  */ 00626000
*                                                                  0361 00627000
*           /*********************************************************/ 00628000
*           /*                                                       */ 00629000
*           /* THIS SEGMENT WILL WAIT 30 SECONDS FOR A POSSIBLE IMPL */ 00630000
*           /* OF THE FAILING CHANNEL GROUP TO COMPLETE. THE TIMER   */ 00631000
*           /* SETDIE FUNCTION WILL BE BRANCH ENTERED TO ESTABLISH A */ 00632000
*           /* DISABLED INTERRUPT EXIT. IF THE DIE IS SUCCESSFULLY   */ 00633000
*           /* ESTABLISHED, EXIT IS MADE TO THE SRB DISPATCHER.      */ 00634000
*           /* IECVIRST WILL AWAKEN AT DIERTN (DIE) AND RESCHEDULE   */ 00635000
*           /* IECVIRST AT SRBRTN ON THE CORRECT PROCESSOR. THE      */ 00636000
*           /* SRBRTN WILL RE-ESTABLISH IECVIRST AS IF THE WAIT HAD  */ 00637000
*           /* NOT OCCURRED. IF THE DIE WAS NOT ESTABLISHED,         */ 00638000
*           /* PROCESSING WILL CONTINUE WITHOUT THE WAIT.            */ 00639000
*           /*                                                       */ 00640000
*           /*********************************************************/ 00641000
*                                                                  0362 00642000
*           SRB=SRB&&SRB;           /* CLEAR SRB TO ZEROES           */ 00643000
         LA    @11,IRSTSRB                                         0362 00644000
         XC    SRB(44,@11),SRB(@11)                                0362 00645000
*           SRBID='SRB ';           /* SRB ID                        */ 00646000
         MVC   SRBID(4,@11),@CC03186                               0363 00647000
*           SRBASCB=ADDR(IEAMASCB); /* MASTER ADDR SP ASCB ADDRESS   */ 00648000
         L     @07,@CV02940                                        0364 00649000
         ST    @07,SRBASCB(,@11)                                   0364 00650000
*           SRBCPAFF=PCCACAFM;      /* PROCESSOR AFFINITY-BIT MASK   */ 00651000
         MVC   SRBCPAFF(2,@11),PCCACAFM(@12)                       0365 00652000
*           SRBRMTR=ADDR(CVTBRET);  /* RESOURCE MANAGER              */ 00653000
         L     @12,CVTPTR                                          0366 00654000
         LA    @07,CVTBRET(,@12)                                   0366 00655000
         ST    @07,SRBRMTR(,@11)                                   0366 00656000
*           SRBEP=ADDR(SRBRTN);     /* ENTRY POINT TO BE SCHEDULED   */ 00657000
         LA    @07,SRBRTN                                          0367 00658000
         ST    @07,SRBEP(,@11)                                     0367 00659000
*           SRBPARM=IRSTWPTR;       /* PARAM IS ADDRESS OF WORKAREA  */ 00660000
         ST    IRSTWPTR,SRBPARM(,@11)                              0368 00661000
*           IRSTTQE=IRSTTQE&&IRSTTQE;/* CLEAR TQE. TQEAID, TQETCB, 0369 00662000
*                                      AND TQEASCB ARE SET TO ZERO   */ 00663000
         XC    IRSTTQE(128),IRSTTQE                                0369 00664000
*           TQEEXIT=ADDR(DIERTN);   /* ADDRESS OF DIE                */ 00665000
         LA    @11,IRSTTQE                                         0370 00666000
         LA    @07,DIERTN                                          0370 00667000
         ST    @07,TQEEXIT(,@11)                                   0370 00668000
*           TQEVAL=WAITTIME;        /* WAIT 30 SECONDS               */ 00669000
         MVC   TQEVAL(8,@11),@CB02991                              0371 00670000
*           RFY                                                    0372 00671000
*            (R1,                                                  0372 00672000
*             R15,                                                 0372 00673000
*             R14) RSTD;                                           0372 00674000
*           R1=ADDR(IRSTTQE);       /* PARM IS ADDRESS OF TQE        */ 00675000
         LR    R1,@11                                              0373 00676000
*           IRSTFRRW=FRRWORK;       /* SAVE STATUS KEPT IN FRRWORK   */ 00677000
         MVC   IRSTFRRW(24),FRRWORK(FRRWPTR)                       0374 00678000
*           RFY                                                    0375 00679000
*            (R11,                                                 0375 00680000
*             R12,                                                 0375 00681000
*             R13,                                                 0375 00682000
*             R0) RSTD;                                            0375 00683000
*           CALL SETDIE;            /* ESTABLISH DIE. NOTE REGS 11-0    00684000
*                                      DESTROYED ON RETURN.          */ 00685000
         L     @07,CVTPTR                                          0376 00686000
         L     @07,TPCPTR(,@07)                                    0376 00687000
         L     @15,TPCSDIE(,@07)                                   0376 00688000
         BALR  @14,@15                                             0376 00689000
*           RFY                                                    0377 00690000
*            (R11,                                                 0377 00691000
*             R12,                                                 0377 00692000
*             R13,                                                 0377 00693000
*             R0) UNRSTD;                                          0377 00694000
*           IF R15=0 THEN           /* IF DIE SUCCESSFULLY ESTABLIH  */ 00695000
         LTR   R15,R15                                             0378 00696000
         BNZ   @RF00378                                            0378 00697000
*             DO;                   /* EXIT TO SRB DISPATCHER        */ 00698000
*               GEN SETS(R1,R15);                                  0380 00699000
*                                      /*DELETE FRR                  */ 00700000
         SETFRR    D,WRKREGS=(R1,R15)                                   00701000
*               PSAIOSUP='0'B;      /* TURN OFF SUPER BIT            */ 00702000
         NI    PSAIOSUP,B'01111111'                                0381 00703000
*               GEN REFS(IRSTWPTR,ENABLE) SETS(IRSTSYSM);          0382 00704000
         STOSM IRSTSYSM,ENABLE           ENABLE I/O,EXT INTERRUPTS      00705000
*               R14=CVTSRBRT;       /* EXIT ADDRESS                  */ 00706000
         L     @12,CVTPTR                                          0383 00707000
         L     R14,CVTSRBRT(,@12)                                  0383 00708000
*               GEN FLOWS(DIERTN);                                 0384 00709000
* INDICATE PROCESSING CONTINUES AT DIERTN                               00710000
*               RETURN;             /* EXIT TO SRB DISPATCHER        */ 00711000
@EL00001 DS    0H                                                  0385 00712000
@EF00001 DS    0H                                                  0385 00713000
@ER00001 BR    @14                                                 0385 00714000
*DIERTN:                                                           0386 00715000
*               PROC OPTIONS(NOSAVE,NOSAVEAREA);                   0386 00716000
*                                                                  0386 00717000
DIERTN   DS    0H                                                  0387 00718000
*               /*****************************************************/ 00719000
*               /*                                                   */ 00720000
*               /* THIS IS THE DIE ROUTINE. IT RUNS UNDER THE TIMER  */ 00721000
*               /* SLIH IN ANY ADDRESS SPACE ON ANY PROCESSOR. IT IS */ 00722000
*               /* DISABLED AND IN SUPERVISOR STATE. IT MUST NOT     */ 00723000
*               /* OBTAIN THE LOCAL LOCK OR ASSUME IT IS HELD OR NOT */ 00724000
*               /* HELD. REGISTERS 2-12 ARE THE SAME AS WHEN SETDIE  */ 00725000
*               /* WAS CALLED. THIS ROUTINE WILL SCHEDULE IECVIRST AS*/ 00726000
*               /* AN SRB AT SRBRTN ON THE PROCESSOR IECVIRST WAS    */ 00727000
*               /* ORIGINALLY RUNNING ON (ONE SUFFERING THE LOSS OF  */ 00728000
*               /* CHANNELS). R14 CONTAINS THE RETURN ADDRESS.       */ 00729000
*               /*                                                   */ 00730000
*               /*****************************************************/ 00731000
*                                                                  0387 00732000
*               R15=ADDR(IRSTFRR);  /* ADDRESS OF FRR ROUTINE        */ 00733000
         LA    R15,IRSTFRR                                         0387 00734000
*               RFY                                                0388 00735000
*                 R4 RSTD;                                         0388 00736000
*               GEN SETS(FRRWPTR,R1,R4) REFS(R15) FLOWS(IRSTFRR);  0389 00737000
         SETFRR    A,FRRAD=(R15),WRKREGS=(R1,R4),PARMAD=(FRRWPTR)       00738000
*               RFY                                                0390 00739000
*                 R4 UNRSTD;                                       0390 00740000
*               FRRWORK=IRSTFRRW;   /* RESTORE FRR WORK AREA         */ 00741000
         MVC   FRRWORK(24,FRRWPTR),IRSTFRRW                        0391 00742000
*               R1=ADDR(IRSTSRB);   /* SRB TO RE-SCHEDULE IECVIRST   */ 00743000
         LA    R1,IRSTSRB                                          0392 00744000
*               GEN REFS(R1) SETS(R15,R1);                         0393 00745000
         SCHEDULE  SRB=(1),SCOPE=GLOBAL     RE-SCHEDULE IECVIRST        00746000
         SETFRR    D,WRKREGS=(R1,R15)       DELETE FRR                  00747000
*               GEN FLOWS(SRBRTN);                                 0394 00748000
* INDICATE PROCESSING CONTINUES AT SRBRTN                               00749000
*             END DIERTN;           /* RETURN TO TIMER SLIH          */ 00750000
@EL00003 DS    0H                                                  0395 00751000
@EF00003 DS    0H                                                  0395 00752000
@ER00003 BR    @14                                                 0395 00753000
*SRBRTN:                                                           0396 00754000
*           ;                                                      0396 00755000
*                                                                  0396 00756000
SRBRTN   DS    0H                                                  0397 00757000
*           /*********************************************************/ 00758000
*           /*                                                       */ 00759000
*           /* THIS ROUTINE IS SCHEDULED BY THE DIE ROUTINE (DIERTN).*/ 00760000
*           /* IT WILL RE-ESTABLISH IECVIRST TO CONTINUE RECOVERY    */ 00761000
*           /* PROCESSING.                                           */ 00762000
*           /*                                                       */ 00763000
*           /*********************************************************/ 00764000
*                                                                  0397 00765000
*           IRSTWPTR=R1;            /* PARM IS ADDR OF GEN WK AREA   */ 00766000
         LR    IRSTWPTR,R1                                         0397 00767000
*           RFY                                                    0398 00768000
*             FRRWORK BASED(ADDR(IRSTFRRW));/* FRR WORK AREA IN GEN     00769000
*                                      WKAREA                        */ 00770000
*           R9=FRRBASE1;            /* RESTORE FIRST BASE REGISTER   */ 00771000
         LA    @12,IRSTFRRW                                        0399 00772000
         L     R9,FRRBASE1(,@12)                                   0399 00773000
*           R10=FRRBASE2;           /* RESTORE SECOND BASE REGISTER  */ 00774000
         L     R10,FRRBASE2(,@12)                                  0400 00775000
*           GEN REFS(IRSTWPTR,DISABLE) SETS(IRSTSYSM);             0401 00776000
         STNSM IRSTSYSM,DISABLE          DISABLE I/O, EXT INTERRUPTS    00777000
*           PSAIOSUP='1'B;          /* IND DISABLED WITHOUT A LOCK   */ 00778000
