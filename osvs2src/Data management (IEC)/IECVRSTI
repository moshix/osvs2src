*   IF((UCBBSY=ON1|UCBPSNS=ON1)&(UCBCPU=CPUID))/* IS THE DEVICE    1070 03035000
*                                      ACTIVE ON THE FAILING CPU     */ 03036000
*       &((INDICAT=ACR)|            /* AND ERR IS ACR OR     @G552PLM*/ 03037000
*       (INDICAT=LOSTCHAN&UCBCHA=CHANID&FRRLCRSV='0'B))/* ERR IS   1070 03038000
*                                      LOSTCHAN WITH LAST I/O OPER ON   03039000
*                                      LOSTCHAN AND NO RESERVES HAVE    03040000
*                                      YET BEEN FOUND. IF RESERVE  1070 03041000
*                                      FOUND DONT WASTE TIME       1070 03042000
*                                      SIMULATING INTERRUPT AS     1070 03043000
*                                      DISABLED WAIT WILL BE LOADED     03044000
*                                      AFTER ALL RESERVES HAVE BEEN     03045000
*                                      FOUND.                @G552PLM*/ 03046000
*     THEN                                                         1070 03047000
@RC01067 LA    @15,512                                             1070 03048000
         ALR   @15,UCBPTR                                          1070 03049000
         TM    UCBBSY(@15),B'10010000'                             1070 03050000
         BZ    @RF01070                                            1070 03051000
         CLC   UCBCPU(1,@15),CPUID(REG8)                           1070 03052000
         BNE   @RF01070                                            1070 03053000
         CLI   INDICAT(REG8),X'00'                                 1070 03054000
         BE    @RT01070                                            1070 03055000
         CLC   INDICAT(1,REG8),LOSTCHAN                            1070 03056000
         BNE   @RF01070                                            1070 03057000
         CLC   UCBCHA(1,@15),CHANID(REG8)                          1070 03058000
         BNE   @RF01070                                            1070 03059000
         TM    FRRLCRSV(REG9),B'00100000'                          1070 03060000
         BNZ   @RF01070                                            1070 03061000
@RT01070 DS    0H                                                  1071 03062000
*     DO;                           /* YES SET CHANNEL ERRS          */ 03063000
*       CALL IGFPSAD0;              /* OBTAIN AUTHORIZATION  @G51BPLG*/ 03064000
         L     @15,@CV03195                                        1072 03065000
         BALR  @14,@15                                             1072 03066000
*       STATUS=CCCICCST;            /* SET CCC ICC IN CSW            */ 03067000
         MVC   STATUS(2),@CB03054                                  1073 03068000
*       RSTIIOAA(TWO:THREE)=UCBCHAN;/* SET INTERRUPT ADDR    @G51BPLC*/ 03069000
         MVC   RSTIIOAA+1(2),UCBCHAN+512(UCBPTR)                   1074 03070000
*       CALL IGFPSAE0;              /* RELEASE AUTHORIZATION @G51BPLC*/ 03071000
         L     @15,@CV03196                                        1075 03072000
         BALR  @14,@15                                             1075 03073000
*       IF INDICAT=ACR THEN         /* IF ACR INDICATE       @G552PLM*/ 03074000
         CLI   INDICAT(REG8),X'00'                                 1076 03075000
         BNE   @RF01076                                            1076 03076000
*         PCCACCPU=ON1;             /* CPU FAIL IN ERPIB     @G552PLM*/ 03077000
         L     @15,PSAPCCAV                                        1077 03078000
         OI    PCCACCPU(@15),B'10000000'                           1077 03079000
*       ELSE                        /* FOR LOSTCHAN IND      @G552PLM*/ 03080000
*         PCCACCHA=ON1;             /* CHAN FAIL IN ERPIB    @G552PLM*/ 03081000
         B     @RC01076                                            1078 03082000
@RF01076 L     @15,PSAPCCAV                                        1078 03083000
         OI    PCCACCHA(@15),B'01000000'                           1078 03084000
*       RFY                                                        1079 03085000
*        (REG5,                                                    1079 03086000
*         REG12,                                                   1079 03087000
*         REG14) RSTD;                                             1079 03088000
@RC01076 DS    0H                                                  1080 03089000
*       GEN;                                                       1080 03090000
         SETLOCK RELEASE,TYPE=IOSUCB,ADDR=(11),RELATED=(UCB,IECVRSTI(GL*03091000
               OCK1)),DISABLED                                 @YM01377 03092000
*       LOCKUCB=OFF1;               /* UCBLOCK NOT HELD      @YM05487*/ 03093000
         NI    LOCKUCB(REG8),B'01111111'                           1081 03094000
*       RFY                                                        1082 03095000
*        (REG12,                                                   1082 03096000
*         REG14) UNRSTD;                                           1082 03097000
*       REG11=RSTIPINT;             /* SET ENVIRONMENT FOR IOS CALL  */ 03098000
         LA    REG11,10                                            1083 03099000
*       REG5=ADIECINT;              /* ADDRESS OF INT HANDLER        */ 03100000
         L     REG5,ADIECINT                                       1084 03101000
*       REG13=WSACRSTI;             /* SAVE AREA ADDRESS             */ 03102000
         L     @06,PSALCCAV                                        1085 03103000
         L     @06,LCCACPUS(,@06)                                  1085 03104000
         L     REG13,WSACRSTI(,@06)                                1085 03105000
*       GEN CODE;                                                  1086 03106000
         STM   @00,@15,0(@13)          SAVE REGS                        03107000
         BAL   @04,4(@05)              HANDLE SIMULATED INT             03108000
         LM    @00,@15,0(@13)          RESTORE REGS                     03109000
*     END;                                                         1087 03110000
*   ELSE                                                           1088 03111000
*     DO;                                                          1088 03112000
         B     @RC01070                                            1088 03113000
@RF01070 DS    0H                                                  1089 03114000
*FREELOCK:                                                         1089 03115000
*       ;                           /* BRANCH HERE IF DEVICE NOT TO     03116000
*                                      BE PROCESSED FOR LOSTCHAN   1089 03117000
*                                                            @G552PLM*/ 03118000
FREELOCK DS    0H                                                  1090 03119000
*       IF UCBMTPXP=ON1 THEN        /* MULTI EXPOSURE DEV    @G552PLM*/ 03120000
         LA    @12,512                                             1090 03121000
         ALR   @12,UCBPTR                                          1090 03122000
         TM    UCBMTPXP(@12),B'00001000'                           1090 03123000
         BNO   @RF01090                                            1090 03124000
*         REG11=ADDR(UCBBASE->UCBLOCK);/* YES-GET BASE LOCK  @G552PLM*/ 03125000
         L     REG11,UCBBASE(,@12)                                 1091 03126000
         LA    REG11,UCBLOCK(,REG11)                               1091 03127000
*       ELSE                                                       1092 03128000
*         REG11=ADDR(UCBLOCK);      /* NO -LOCK THIS UCB     @G552PLM*/ 03129000
         B     @RC01090                                            1092 03130000
@RF01090 LA    REG11,UCBLOCK(,UCBPTR)                              1092 03131000
*       RFY                                                        1093 03132000
*        (REG12,                                                   1093 03133000
*         REG14) RSTD;                                             1093 03134000
@RC01090 DS    0H                                                  1094 03135000
*       GEN;                                                       1094 03136000
         SETLOCK RELEASE,TYPE=IOSUCB,ADDR=(11),RELATED=(UCB,IECVRSTI(GL*03137000
               OCK1)),DISABLED                                 @YM01377 03138000
*       RFY                                                        1095 03139000
*        (REG5,                                                    1095 03140000
*         REG11,                                                   1095 03141000
*         REG12,                                                   1095 03142000
*         REG13,                                                   1095 03143000
*         REG14) UNRSTD;                                           1095 03144000
*       LOCKUCB=OFF1;               /* UCBLOCK NOT HELD      @YM05487*/ 03145000
         NI    LOCKUCB(REG8),B'01111111'                           1096 03146000
*     END;                                                         1097 03147000
*   PCCAWERP(ONE:EIGHT)=PCCAWERP&&PCCAWERP;/* ZERO THE PCCA ERPIB    */ 03148000
@RC01070 L     @12,PSAPCCAV                                        1098 03149000
         XC    PCCAWERP(8,@12),PCCAWERP(@12)                       1098 03150000
*   RFY                                                            1099 03151000
*     REG14 RSTD;                   /*                       @G552PLM*/ 03152000
*   REG14=UCBACTRT;                 /* RESTORE RET ADDR      @G552PLM*/ 03153000
