         TITLE ' IECVRSTI                                              *00001000
                        '                                               00002000
         TITLE 'IECVRSTI - BASIC ENTRY AND SETUP'                       00003000
IECVRSTI CSECT                                                          00004000
         ENTRY IECHK6                  DSS HOOK                         00005000
         ENTRY IECHK7                  DSS HOOK                         00006000
         ENTRY IORFRR                  FRR ENTRY               @YM05487 00007000
         ENTRY IECVRST1                LYLE TEST                        00008000
         BALR  @15,0               INITIAL BASE REG            @G552PLM 00009000
         BCTR  @15,0               BACK UP                     @G552PLM 00010000
         BCTR  @15,0                       TO ENTRY POINT      @G552PLM 00011000
         USING IECVRSTI,@15        USE R15 AS TEMP BASE        @G552PLM 00012000
         MODID                                                          00013000
         CLM   @01,8,LOSTCHAN      CALLED FOR LOST CHAN        @G552PLM 00014000
         BNE   DOSAVE              NO, WE ARE ALREADY DISABLD  @G552PLM 00015000
         LR    @02,@00             SRB IN AN ADDRESSABLE REG   @G552PLM 00016000
         STNSM 0(@02),X'FC'        STORE SYS MASK IN SRB       @G552PLM 00017000
         B     NOSAVE              NO SAVE AREA BYPASS SAVE    @G552PLM 00018000
LOSTCHAN DC    X'03000000'         IND LOSTCHAN CALLER         @G552PLM 00019000
DOSAVE   EQU   *                   TARGET IF NOT POS LOSTCHAN  @G552PLM 00020000
         STM   @14,@12,0(13)       SAVE REGISTERS              @G552PLM 00021000
NOSAVE   EQU   *                   TARGET IF POSSIBLE LOSTCHN  @G552PLM 00022000
         DROP  @15                 NO LONGER NEED R15 AS BASE  @G552PLM 00023000
         LR    @10,@15                 SET ADDRESSIBILITY               00024000
         USING IECVRSTI,@10                                             00025000
         LA     @02,4094(@10)           GET SECOND BASE        @OZ06051 00026000
         USING IECVRSTI+4094,@02                               @OZ06051 00027000
*   REG8=ZERO;                      /* ZERO PTR TO WORK AREA FOR FRR    00028000
*                                                            @YM05487*/ 00029000
         SLR   REG8,REG8                                           0269 00030000
*   REG11=ADDR(IORFRR);             /* FRR ADDR FOR FRR CALL         */ 00031000
         LA    REG11,IORFRR                                        0270 00032000
*   GEN CODE;                                                      0271 00033000
         SETFRR A,PARMAD=(@09),FRRAD=(@11),WRKREGS=(@03,@05)            00034000
*   RFY                                                            0272 00035000
*    (REG11) UNRSTD;                                               0272 00036000
*   RFY                                                            0273 00037000
*    (REG11,                                                       0273 00038000
*     REG13,                                                       0273 00039000
*     REG15) RSTD;                  /*                       @YM05487*/ 00040000
*   IF PSAIOSUP=ON1                 /* IF SUPER BIT ON WHEN ENTERRED    00041000
*                                                            @YM05487*/ 00042000
*     THEN                                                         0274 00043000
         TM    PSAIOSUP,B'10000000'                                0274 00044000
         BNO   @RF00274                                            0274 00045000
*     FRRIOSUP=OFF1;                /* I DID NOT TURN IT ON  @YM05487*/ 00046000
         NI    FRRIOSUP(REG9),B'01111111'                          0275 00047000
*   ELSE                                                           0276 00048000
*     DO;                           /* OTHERWISE             @YM05487*/ 00049000
         B     @RC00274                                            0276 00050000
@RF00274 DS    0H                                                  0277 00051000
*       FRRIOSUP=ON1;               /* INDICATE I TURNED IT ON     0277 00052000
*                                                            @YM05487*/ 00053000
         OI    FRRIOSUP(REG9),B'10000000'                          0277 00054000
*       PSAIOSUP=ON1;               /* TURN ON IOS SUPER BIT SO IOS     00055000
*                                      STORAGE MANAGER WILL NOT CAUSE   00056000
*                                      ENABLE                @YM05487*/ 00057000
         OI    PSAIOSUP,B'10000000'                                0278 00058000
*     END;                          /*                       @YM05487*/ 00059000
*   FRRSAV=REG13;                   /* SAVE SAVE AREA PTR            */ 00060000
@RC00274 ST    REG13,FRRSAV(,REG9)                                 0280 00061000
*   REG11=GET320;                   /* SET TO GET WORK AREA AND    0281 00062000
*                                      MESSAGE BUFFER                */ 00063000
         L     REG11,@CF03039                                      0281 00064000
*   REG13=WSACRSTI;                 /* GET IO RESTART SAVE           */ 00065000
         L     @14,PSALCCAV                                        0282 00066000
         L     @14,LCCACPUS(,@14)                                  0282 00067000
         L     REG13,WSACRSTI(,@14)                                0282 00068000
*   REG15=IOCORMGT;                 /* STORAGE MANAGER ENTRY         */ 00069000
         L     @06,CVTPTR                                          0283 00070000
         L     @06,CVTIXAVL(,@06)                                  0283 00071000
         L     REG15,IOCORMGT(,@06)                                0283 00072000
*   CALL SMGRGB;                    /* CALL STORAGE MANAGER FOR A 160   00073000
*                                      BYTE BLOCK                    */ 00074000
         BALR  @14,@15                                             0284 00075000
*   REG8=REG11;                     /* SET WORK AREA PTR             */ 00076000
         LR    REG8,REG11                                          0285 00077000
*   REG11=BLKCHAIN;                 /* PICK UP MESSBUF PTR           */ 00078000
         L     REG11,BLKCHAIN(,REG8)                               0286 00079000
*   RFY                                                            0287 00080000
*    (REG13) UNRSTD;                                               0287 00081000
*   RFY                                                            0288 00082000
*    (REG15) UNRSTD;                                               0288 00083000
*   IORWORK=IORWORK&&IORWORK;       /* ZERO THE WORK AREA            */ 00084000
         XC    IORWORK(120,REG8),IORWORK(REG8)                     0289 00085000
*   PWAPTR=PCCAPWAV;                /* ADDR OF PWA           @G552PLM*/ 00086000
         L     @06,PSAPCCAV                                        0290 00087000
         MVC   PWAPTR(4,REG8),PCCAPWAV(@06)                        0290 00088000
*   INPARM=REG1;                    /* SET PARM INDICATOR            */ 00089000
         ST    REG1,INPARM(,REG8)                                  0291 00090000
*   MESSPTR=REG11;                  /* SET MESSAGE BUF PTR           */ 00091000
         ST    REG11,MESSPTR(,REG8)                                0292 00092000
*   REG11->MSGENT=ONE;              /* SET INITIAL MESSAGE COUNT     */ 00093000
         MVC   MSGENT(2,REG11),@CH00039                            0293 00094000
*   REG11->MESSBUF=REG11->MESSBUF&&REG11->MESSBUF;/* CLEAR THE MSG 0294 00095000
*                                      BUFFER INDEX VALUE            */ 00096000
         XC    MESSBUF(152,REG11),MESSBUF(REG11)                   0294 00097000
*   IORSIOCM=CVTIXAVL;              /* IOCOM ADDR TO WORK            */ 00098000
         L     @06,CVTPTR                                          0295 00099000
         MVC   IORSIOCM(4,REG8),CVTIXAVL(@06)                      0295 00100000
*   RFY                                                            0296 00101000
*     IOCOM BASED(IORSIOCM);                                       0296 00102000
*   FRRIORWA=REG8;                  /* SAVE WORK AREA PTR    @G552PLM*/ 00103000
         ST    REG8,FRRIORWA(,REG9)                                0297 00104000
*   RFY                                                            0298 00105000
*    (REG1) UNRSTD;                                                0298 00106000
*   RFY                                                            0299 00107000
*    (REG11) UNRSTD;                                               0299 00108000
*                                                                  0299 00109000
*/***************************************************************  0300 00110000
**       THE FOLLOWING CODE DETERMINES THE CALLER OF I/O RESTART   0300 00111000
**       AND ROUTES CONTROL TO THE SPECIFIC SECTION OF CODE        0300 00112000
**       FOR THAT CALLER.                                          0300 00113000
***************************************************************      */ 00114000
*                                                                  0300 00115000
*   IF INDICAT=ACR                  /* IS THIS AN ACR CALL           */ 00116000
*     THEN                                                         0300 00117000
         CLI   INDICAT(REG8),X'00'                                 0300 00118000
         BE    @RT00300                                            0300 00119000
*     GO TO ACRPROC;                /* IF YES GO PROCESS             */ 00120000
*   IF INDICAT=LOSTCHAN             /* IF CHAN LOST          @G552PLM*/ 00121000
*     THEN                                                         0302 00122000
         CLC   INDICAT(1,REG8),LOSTCHAN                            0302 00123000
         BE    @RT00302                                            0302 00124000
*     GO TO LOSTCHCD;               /* GO PROCESS IT         @G552PLM*/ 00125000
*   ELSE                                                           0304 00126000
*     UCBPTR=UCBP;                  /* SET UP PTR TO UCB             */ 00127000
         SLR   UCBPTR,UCBPTR                                       0304 00128000
         ICM   UCBPTR,7,UCBP(REG8)                                 0304 00129000
*   IF INDICAT=CCH                  /* IS THIS A CCH CALL IF SO DO 0305 00130000
*                                      THEN CLAUSE IF NOT DO ELSE  0305 00131000
*                                      CLAUSE                        */ 00132000
*     THEN                                                         0305 00133000
         CLI   INDICAT(REG8),X'40'                                 0305 00134000
         BNE   @RF00305                                            0305 00135000
*     DO;                           /* IF YES PROCESS                */ 00136000
*       GEN(TITLE 'IECVRSTI - CCH CALL CODE');                     0307 00137000
*                                                                  0307 00138000
         TITLE 'IECVRSTI - CCH CALL CODE'                               00139000
*/***************************************************************  0308 00140000
**       THE FOLLOWING CODE HANDLES THE CCH CALL TO I/O RESTART.   0308 00141000
**       AFTER THE CSW HAS BEEN SAVED AN ATTEMPT TO CLEAR THE      0308 00142000
**       DEVICE (VIA THE CLEARDEV SUBROUTINE) IS MADE. BASED ON    0308 00143000
**       THE SUCCESS OF THE CLEARING THE NO RETRY BIT MAY BE SET   0308 00144000
**       FOR ERP NOTIFICATION. THE CSW IS THEN RESTORED AND THE    0308 00145000
**       COMMON MIH-CCH EXIT ROUTINE IS ENTERED.                   0308 00146000
***************************************************************      */ 00147000
*                                                                  0308 00148000
*CCHPROC:                                                          0308 00149000
*       IORCSW(ONE:EIGHT)=CSW;      /* SAVE THE CSW                  */ 00150000
CCHPROC  MVC   IORCSW(8,REG8),CSW                                  0308 00151000
*       ERPIBPTR=REG12;             /* SET ERPIB PTR                 */ 00152000
         ST    REG12,ERPIBPTR(,REG8)                               0309 00153000
*       CALL CLEARDEV;              /* ATTEMPT TO CLEAR DEV          */ 00154000
         BAL   @14,CLEARDEV                                        0310 00155000
*       IF ERPIBPTR^=ZERO           /* IS THERE AN ERPIB             */ 00156000
*         THEN                                                     0311 00157000
         L     @11,ERPIBPTR(,REG8)                                 0311 00158000
         LTR   @11,@11                                             0311 00159000
         BZ    @RF00311                                            0311 00160000
*         DO;                       /* YES - PROCESS IT              */ 00161000
*           RSTIERP(FIVE:EIGHT)=ERPIBSTD;/* SET STANDARD ERPIB       */ 00162000
         MVC   RSTIERP+4(4,@11),ERPIBSTD                           0313 00163000
*           IF NODEVCLR=ON1         /* DID DEV CLEAR                 */ 00164000
*             THEN                                                 0314 00165000
         TM    NODEVCLR(REG8),B'01000000'                          0314 00166000
         BNO   @RF00314                                            0314 00167000
*             RSTINOR=ON1;          /* NO - SET ERPIB NO RTY         */ 00168000
         OI    RSTINOR(@11),B'00000001'                            0315 00169000
*           ELSE                                                   0316 00170000
*             ;                                                    0316 00171000
@RF00314 DS    0H                                                  0317 00172000
*         END;                                                     0317 00173000
*       ELSE                                                       0318 00174000
*         ;                                                        0318 00175000
@RF00311 DS    0H                                                  0319 00176000
*       CALL IGFPSAD0;              /* OBTAIN AUTHORIZATION  @G51BPLC*/ 00177000
         L     @15,@CV03195                                        0319 00178000
         BALR  @14,@15                                             0319 00179000
*       CSW(ONE:EIGHT)=IORCSW(ONE:EIGHT);/* RESTORE CSW              */ 00180000
         MVC   CSW(8),IORCSW(REG8)                                 0320 00181000
*       CHANSTAT=CHANSTAT|CCCICC2;  /* SET CCC ICC IN CSW            */ 00182000
         OI    CHANSTAT,X'06'                                      0321 00183000
*       CALL IGFPSAE0;              /* RELEASE AUTHORIZATION@G51BPLC */ 00184000
         L     @15,@CV03196                                        0322 00185000
         BALR  @14,@15                                             0322 00186000
*     END;                                                         0323 00187000
*   ELSE                                                           0324 00188000
*     DO;                           /* NO - DO MIH PROCESS           */ 00189000
         B     @RC00305                                            0324 00190000
@RF00305 DS    0H                                                  0325 00191000
*       RFY                                                        0325 00192000
*        (REG12) UNRSTD;                                           0325 00193000
*       GEN(TITLE 'IECVRSTI - MIH CALL CODE');                     0326 00194000
*                                                                  0326 00195000
         TITLE 'IECVRSTI - MIH CALL CODE'                               00196000
*/***************************************************************  0327 00197000
**       THIS CODE HANDLES THE MIH CALL TO I/O RESTART.            0327 00198000
**       THE CSW IS RESET TO INDICATE THE RESULT OF THE CLEARDEV   0327 00199000
**       SUBROUTINE. DIFFERENT STATUS IS STORED DEPENDING ON       0327 00200000
**       WHAT KIND OF CHANNEL PROGRAM (SENSE OR NORMAL) WAS        0327 00201000
**       RUNNING WHEN THE INTERRUPT WAS LOST.                      0327 00202000
***************************************************************      */ 00203000
*                                                                  0327 00204000
*MIHPROC:                                                          0327 00205000
*       ;                                                          0327 00206000
MIHPROC  DS    0H                                                  0328 00207000
*       UCBPTR=UCBPTR-UCBPRELN;     /* GET TO PREFIX                 */ 00208000
         SH    UCBPTR,@CH03050                                     0328 00209000
*       UCBP=UCBPTR;                /* RESET PARAMETER               */ 00210000
         STCM  UCBPTR,7,UCBP(REG8)                                 0329 00211000
*       CALL CLEARDEV;              /* ATTEMPT TO CLEAR DEV          */ 00212000
         BAL   @14,CLEARDEV                                        0330 00213000
*       PCCACHUB=UCBPTR+UCBPRELN;   /* SET PCCA ERPIB UCB ADDRESS    */ 00214000
         L     @12,PSAPCCAV                                        0331 00215000
         LA    @11,512                                             0331 00216000
         ALR   @11,UCBPTR                                          0331 00217000
         ST    @11,PCCACHUB(,@12)                                  0331 00218000
*       PCCAWERP(FIVE:EIGHT)=ERPIBSTD;/* SET STANDARD ERPIB          */ 00219000
         MVC   PCCAWERP+4(4,@12),ERPIBSTD                          0332 00220000
*       CALL IGFPSAD0;              /* OBTAIN AUTHORIZATION  @G51BPLC*/ 00221000
         L     @15,@CV03195                                        0333 00222000
         BALR  @14,@15                                             0333 00223000
*       CSW=CSW&&CSW;               /* CLEAR CSW             @G51BPLC*/ 00224000
         XC    CSW(8),CSW                                          0334 00225000
*       IF UCBPSNS=ON1              /* IS A SENSE PENDING            */ 00226000
*         THEN                                                     0335 00227000
         LA    @12,512                                             0335 00228000
         ALR   @12,UCBPTR                                          0335 00229000
         TM    UCBPSNS(@12),B'00010000'                            0335 00230000
         BNO   @RF00335                                            0335 00231000
*         IF UCBASNS=ON1            /* IS A SENSE ACTIVE             */ 00232000
*           THEN                                                   0336 00233000
         TM    UCBASNS(@12),B'01000000'                            0336 00234000
         BNO   @RF00336                                            0336 00235000
*           DO;                     /* YES BOTH SET U.C.             */ 00236000
*             CMDADDR=IOCIOSCP+(UCBCCWOF*8)+EIGHT;/* SET CSW COMMAND    00237000
*                                      ADDR TO POINT 8 PAST THE SENSE   00238000
*                                      CCW                   @ZA16150*/ 00239000
         L     @11,IORSIOCM(,REG8)                                 0338 00240000
         L     @12,UCBEXTPT(,@12)                                  0338 00241000
         LH    @12,UCBCCWOF(,@12)                                  0338 00242000
         SLA   @12,3                                               0338 00243000
         AL    @12,IOCIOSCP(,@11)                                  0338 00244000
         LA    @12,8(,@12)                                         0338 00245000
         STCM  @12,7,CMDADDR                                       0338 00246000
*             STATUS=UCSTAT;        /* SET UNIT CHECK CSW            */ 00247000
         MVC   STATUS(2),@CB03050                                  0339 00248000
*           END;                                                   0340 00249000
*         ELSE                                                     0341 00250000
*           STATUS=DESTAT;          /* SENSE NOT ACTIVE SET DEVICE 0341 00251000
*                                      END.                          */ 00252000
         B     @RC00336                                            0341 00253000
@RF00336 MVC   STATUS(2),@CB03052                                  0341 00254000
*       ELSE                                                       0342 00255000
*         DO;                       /* NO SENSE PENDING              */ 00256000
         B     @RC00335                                            0342 00257000
@RF00335 DS    0H                                                  0343 00258000
*           STATUS=CCCICCST;        /* SET CCC ICC IN CSW            */ 00259000
         MVC   STATUS(2),@CB03054                                  0343 00260000
*           IF NODEVCLR=ON1         /* DID THE DEVICE CLEAR          */ 00261000
*             THEN                                                 0344 00262000
         TM    NODEVCLR(REG8),B'01000000'                          0344 00263000
         BNO   @RF00344                                            0344 00264000
*             PCCACNOR=ON1;         /* NO - SET ERPIB FOR RETRY NOT     00265000
*                                      POSS                          */ 00266000
         L     @12,PSAPCCAV                                        0345 00267000
         OI    PCCACNOR(@12),B'00000001'                           0345 00268000
*           ELSE                                                   0346 00269000
*             ;                                                    0346 00270000
@RF00344 DS    0H                                                  0347 00271000
*         END;                                                     0347 00272000
*       RSTIIOAA(TWO:THREE)=UCBCHAN;/* SET UP INT ADDR       @G51BPLC*/ 00273000
@RC00335 MVC   RSTIIOAA+1(2),UCBCHAN+512(UCBPTR)                   0348 00274000
*       CALL IGFPSAE0;              /* RELEASE AUTHORIZATION@G51BPLC */ 00275000
         L     @15,@CV03196                                        0349 00276000
         BALR  @14,@15                                             0349 00277000
*       RFY                                                        0350 00278000
*        (REG13) RSTD;                                             0350 00279000
*       RFY                                                        0351 00280000
*        (REG11) RSTD;                                             0351 00281000
*       RFY                                                        0352 00282000
*        (REG5) RSTD;                                              0352 00283000
*       GEN(TITLE 'IECVRSTI - I/O RESTART IFACE WITH SLIH');       0353 00284000
*                                                                  0353 00285000
         TITLE 'IECVRSTI - I/O RESTART IFACE WITH SLIH'                 00286000
*/***************************************************************  0354 00287000
**       THIS CODE INTERFACES WITH THE I/O SLIH TO CAUSE A         0354 00288000
**       SIMULATED INTERRUPT ON THE DEVICE FOR WHICH THE MIH       0354 00289000
**       CALL WAS MADE.                                            0354 00290000
***************************************************************      */ 00291000
*                                                                  0354 00292000
*       REG11=RSTIPINT;             /* SET ENVIRONMENT BYTE FOR IOS     00293000
*                                      CALL                          */ 00294000
         LA    REG11,10                                            0354 00295000
*       REG13=WSACRSTI;             /* SAVE AREA ADDRESS             */ 00296000
         L     @06,PSALCCAV                                        0355 00297000
         L     @06,LCCACPUS(,@06)                                  0355 00298000
         L     REG13,WSACRSTI(,@06)                                0355 00299000
*       REG5=ADIECINT;              /* INTERRUPT HANDLER ENTRY POINT */ 00300000
         L     REG5,ADIECINT                                       0356 00301000
*       GEN CODE;                                                  0357 00302000
         STM   @00,@15,0(@13)          SAVE THE REGS ACROSS SLIH        00303000
         BAL   @04,4(@05)               GO TO INT HANDLER               00304000
         LM    @00,@15,0(@13)           RESTORE REGS                    00305000
*       RFY                                                        0358 00306000
*        (REG13) UNRSTD;                                           0358 00307000
*       RFY                                                        0359 00308000
*        (REG11) UNRSTD;                                           0359 00309000
*       RFY                                                        0360 00310000
*        (REG5) UNRSTD;                                            0360 00311000
*     END;                                                         0361 00312000
*   GO TO IORSEND;                  /* RETURN TO MIH AFTER CLEANUP   */ 00313000
         B     IORSEND                                             0362 00314000
*   GEN(TITLE 'IECVRSTI - LOST CHANNEL ROUTINE');/*          @G552PLM*/ 00315000
         TITLE 'IECVRSTI - LOST CHANNEL ROUTINE'                        00316000
*/********************************************************************* 00317000
**       THIS CODE HANDLES I/O RESTART PROCESSING FOR A LOST       0364 00318000
**       CHANNEL.  A LOST CHANNEL IS INDICATED AS SUCH BY THE      0364 00319000
**       CATIORST FLAG BEING ON IN THE CAT ENTRY FOR THE CHANNEL.  0364 00320000
**       THIS CODE WILL SCAN THE CAT FOR A LOST CHANNEL AND CALL   0364 00321000
**       UCBACT PROCEEDURE FOR EACH DEVICE ON THE CHANNEL.         0364 00322000
*************************************************************@G552PLM*/ 00323000
*                                                                  0364 00324000
*LOSTCHCD:                                                         0364 00325000
*   ;                                                              0364 00326000
LOSTCHCD DS    0H                                                  0365 00327000
*   PWAEDSRB=0;                     /* SRB IS RE-USABLE      @G552PLM*/ 00328000
         L     @12,PWAPTR(,REG8)                                   0365 00329000
         MVI   PWAEDSRB(@12),X'00'                                 0365 00330000
*   FRRLCSRB='1'B;                  /* IND SRB HAS BEEN SET RE-USABLE   00331000
*                                                            @G552PLM*/ 00332000
         OI    FRRLCSRB(REG9),B'01000000'                          0366 00333000
*   RFY                                                            0367 00334000
*    (REG1,                                                        0367 00335000
*     REG11,                                                       0367 00336000
*     REG13,                                                       0367 00337000
*     REG15) RSTD;                  /*                       @G552PLM*/ 00338000
*   REG13=WSACRSTI;                 /* STOR MGR SAVE AREA    @G552PLM*/ 00339000
         L     @14,PSALCCAV                                        0368 00340000
         L     @14,LCCACPUS(,@14)                                  0368 00341000
         L     REG13,WSACRSTI(,@14)                                0368 00342000
*   REG11=GET160;                   /* SIZE OF RESERVE TAB   @G552PLM*/ 00343000
         L     REG11,@CF03037                                      0369 00344000
*   REG15=IOCORMGT;                 /* EP OF STOR MGR        @G552PLM*/ 00345000
         L     @12,IORSIOCM(,REG8)                                 0370 00346000
         L     REG15,IOCORMGT(,@12)                                0370 00347000
*   CALL SMGRGB;                    /* GET 160 BYTE BLOCK    @G552PLM*/ 00348000
         BALR  @14,@15                                             0371 00349000
*   BLKCHAIN=REG11;                 /* CHAIN PTR TO TABLE    @G552PLM*/ 00350000
         ST    REG11,BLKCHAIN(,REG8)                               0372 00351000
*   RSVTBLPT=REG11;                 /* BASE PTR FOR TABLE    @G552PLM*/ 00352000
         ST    REG11,RSVTBLPT(,REG8)                               0373 00353000
*   RFY                                                            0374 00354000
*    (REG1,                                                        0374 00355000
*     REG13,                                                       0374 00356000
*     REG15) UNRSTD;                /*                       @G552PLM*/ 00357000
*   REG11->RSVTBL=REG11->RSVTBL&&REG11->RSVTBL;/* CLEAR RESERVE TABLE   00358000
*                                                            @G552PLM*/ 00359000
         XC    RSVTBL(160,REG11),RSVTBL(REG11)                     0375 00360000
*   REG11->ENTCNT=ONE;              /* COUNT OF TAB ENTRYS   @G552PLM*/ 00361000
         MVC   ENTCNT(4,REG11),@CF00039                            0376 00362000
*   RFY                                                            0377 00363000
*     REG11 UNRSTD;                 /*                       @G552PLM*/ 00364000
*   CATPTR=ADDR(PCCACAT);           /* CAT IS IN PCCA        @G552PLM*/ 00365000
         L     @12,PSAPCCAV                                        0378 00366000
         LA    @12,PCCACAT(,@12)                                   0378 00367000
         ST    @12,CATPTR(,REG8)                                   0378 00368000
*CATLOOP:                                                          0379 00369000
*   ;                               /* THIS POINT IS BRANCHED TO   0379 00370000
*                                      AFTER THIS CPU'S CAT HAS BEEN    00371000
*                                      PROCESSED AND IT IS FOUND THAT   00372000
*                                      A CRH CONNECTED CHANNEL HAS 0379 00373000
*                                      BEEN LOST. CATPTR CONTAINS  0379 00374000
*                                      ADDR OF OTHER CAT     @G552PLM*/ 00375000
CATLOOP  DS    0H                                                  0380 00376000
*   DO CATINDEX=1 TO LENGTH(PCCACAT)/LENGTH(CAT);/* SCAN ALL ENTRIES    00377000
*                                                            @G552PLM*/ 00378000
         LA    @12,1                                               0380 00379000
         STC   @12,CATINDEX(,REG8)                                 0380 00380000
@DL00380 DS    0H                                                  0381 00381000
*     IF CATIORST='1'B THEN         /* IF CHANNEL LOST       @G552PLM*/ 00382000
         L     @12,CATPTR(,REG8)                                   0381 00383000
         TM    CATIORST(@12),B'01000000'                           0381 00384000
         BNO   @RF00381                                            0381 00385000
*       DO;                         /* DO I/O RESTART PROC   @G552PLM*/ 00386000
