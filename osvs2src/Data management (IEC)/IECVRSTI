*   RFY                                                            1101 03155000
*     REG14 UNRSTD;                 /*                       @G552PLM*/ 03156000
*   END UCBACT;                     /*                       @G552PLM*/ 03157000
@EL00002 DS    0H                                                  1102 03158000
@EF00002 DS    0H                                                  1102 03159000
@ER00002 BR    @14                                                 1102 03160000
@PB00002 DS    0H                                                  1102 03161000
*   GEN(TITLE ' IECVRSTI - MESSAGES');                             1103 03162000
*                                                                  1103 03163000
         TITLE ' IECVRSTI - MESSAGES'                                   03164000
*/***************************************************************  1104 03165000
**       THE FOLLOWING MESSAGES ARE ISSUED ASYNCHRONOUSLY BY       1104 03166000
**       I/O RESTART TO INFORM THE OPERATOR OF ACTIONS TAKEN       1104 03167000
**       BY I/O RESTART IN ITS RECOVERY EFFORT.                    1104 03168000
***************************************************************      */ 03169000
*                                                                  1104 03170000
*   GEN;                                                           1104 03171000
         DS     0F                                                      03172000
ASMDATE  DC     CL8'&SYSDATE'                                           03173000
IEA003I  WTO   'IEA003I I/O MALFUNCTION ON DEVICE XXX. I/O TERMINATED.'X03174000
               ,ROUTCDE=(1,10),DESC=(4),MF=L                            03175000
IEA004I  WTO   'IEA004I DEVICE 11112222333344445555666677778888 SET FORX03176000
                I/O REQUEST TERMINATION.',ROUTCDE=(1,10),DESC=(4),MF=L  03177000
IEA005I  WTO   'IEA005I DEVICE XXX RESERVED ON OFFLINE PATH Y FROM CPU *03178000
               Z.',ROUTCDE=(1,10),DESC=(4),MF=L                         03179000
IEA969A  WTO   'IEA969A WARNING - SIGP FAILED DURING ACR PROCESSING.',RX03180000
               OUTCDE=(1,10),DESC=(4),MF=L                              03181000
IEA410I  WTO   'IEA410I CHANNEL (X,Y) FORCED OFFLINE',                 X03182000
               ROUTCDE=(1,10),DESC=(4),MF=L                    @G552PLM 03183000
*                                                              @YM04440 03184000
SIGPSCKS DC    X'00000050'         SENSE FOR STOP OR CK STOP   @ZA06051 03185000
IPR      EQU   X'07'          IPRCODE FOR SIGP                 @YM04440 03186000
CPURESET EQU   X'0C'          CPU RESET CODE FOR SIGP          @Y30CQLF 03187000
