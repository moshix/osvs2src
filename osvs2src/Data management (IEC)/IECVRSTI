         BAL   @14,RECORDIT                                        0771 01892000
*   RFY                                                            0772 01893000
*     REG14 RSTD;                   /*                       @YM04440*/ 01894000
*   REG14=RETURNAD;                 /* RESTORE RETURN REG    @YM04440*/ 01895000
         L     REG14,RETURNAD(,REG8)                               0773 01896000
*SIGPOK:                                                           0774 01897000
*   GEN CODE( BR  @14);             /* RETURN                @YM04440*/ 01898000
SIGPOK    BR  @14                                                       01899000
*   RFY                                                            0775 01900000
*    (REG5,                                                        0775 01901000
*     REG6) UNRSTD;                                                0775 01902000
*   GEN(TITLE ' IECVRSTI - WAIT STATE SUBROUTINE');                0776 01903000
*                                                                  0776 01904000
         TITLE ' IECVRSTI - WAIT STATE SUBROUTINE'                      01905000
*/***************************************************************  0777 01906000
**       THIS SECTION OF CODE INTERFACES WITH THE SYSTEM STOP      0777 01907000
**       AND RESTART SUBROUTINE. WHEN CALLED DURING ACR            0777 01908000
**       PROCESSING IT WILL ATTEMPT TO PUT ALL CPU'S IN THE MP     0777 01909000
**       CONFIGURATION INTO A WAIT STATE DEFINED BY THE CALLER     0777 01910000
**       THE ROUTINE FIRST ACQUIRES THE RESTART RESOURCE AND       0777 01911000
**       THEN CALLS THE SUBROUTINE. IF THE SUBROUTINE IS UNABLE    0777 01912000
**       TO STOP ALL THE CPU'S THIS ROUTINE WILL PUT THE CPU       0777 01913000
**       ON WHICH IT IS RUNNING INTO THE DESIRED WAIT STATE.       0777 01914000
***************************************************************      */ 01915000
*                                                                  0777 01916000
*   RFY                                                            0777 01917000
*    (REG0,                                                        0777 01918000
*     REG1,                                                        0777 01919000
*     REG4,                                                        0777 01920000
*     REG12,                                                       0777 01921000
*     REG13,                                                       0777 01922000
*     REG14) RSTD;                                                 0777 01923000
*WAITRTN:                                                          0778 01924000
*   ;                               /* INTERFACE TO THE SYS STOP AND    01925000
*                                      RESTART SUBROUTINE            */ 01926000
WAITRTN  DS    0H                                                  0779 01927000
*   RCPU=PSACPULA;                  /* SET CPU ADDRESS FOR         0779 01928000
*                                      ACQUISITION OF THE RESTART  0779 01929000
*                                      RESOURCE                      */ 01930000
         MVC   RCPU(2,REG8),PSACPULA                               0779 01931000
*   RHLDR=IORSCODE;                 /* INSERT I/O RESTART USER ID    */ 01932000
         MVC   RHLDR(2,REG8),IORSCODE                              0780 01933000
*   REG12=REG14;                    /* SAVE RETURN REG               */ 01934000
         LR    REG12,REG14                                         0781 01935000
*RSTRTRSC:                                                         0782 01936000
*   REG4=RSTRTWRD;                  /* LOAD THE RESTART RESOURCE LOCK*/ 01937000
RSTRTRSC L     REG4,RSTRTWRD(,REG8)                                0782 01938000
*   REG13=ZERO;                     /* LOAD UNLOCKED VALUE           */ 01939000
         SLR   REG13,REG13                                         0783 01940000
*   REG1=ADDR(CVTRSTWD);            /* ADDRESS OF RESTART RESOURCE 0784 01941000
*                                      LOCKWORD                      */ 01942000
         L     REG1,CVTPTR                                         0784 01943000
         LA    REG1,CVTRSTWD(,REG1)                                0784 01944000
*   GEN CODE;                                                      0785 01945000
         CS    @13,@04,0(@01)          ATTEMPT TO LOCK RESOURCE         01946000
*   BC(7,LDWAIT);                   /* COMP FAIL LOOP                */ 01947000
         BC    7,LDWAIT                                            0786 01948000
*RSTRTWR1:                                                         0787 01949000
*   ;                               /* SET UP TO RECORD              */ 01950000
RSTRTWR1 DS    0H                                                  0788 01951000
*   IF(REG0=FORTY3)|(REG0=LTCHANCD) /* IF HARD WAIT          @G552PLM*/ 01952000
*     THEN                                                         0788 01953000
         CH    REG0,@CH03158                                       0788 01954000
         BE    @RT00788                                            0788 01955000
         CH    REG0,@CH03070                                       0788 01956000
         BNE   @RF00788                                            0788 01957000
@RT00788 DS    0H                                                  0789 01958000
*     LOGHDR=DEADWAIT;              /* HARD WAIT HEADER      @G552PLM*/ 01959000
         L     @15,MESSPTR(,REG8)                                  0789 01960000
         MVC   LOGHDR+8(4,@15),DEADWAIT                            0789 01961000
*   ELSE                                                           0790 01962000
*     LOGHDR=RESTWAIT;              /* RESTARTABLE HEADER    @G552PLM*/ 01963000
         B     @RC00788                                            0790 01964000
@RF00788 L     @15,MESSPTR(,REG8)                                  0790 01965000
         MVC   LOGHDR+8(4,@15),RESTWAIT                            0790 01966000
*   LOGPTR=ADDR(LOGDA);             /* BEGIN DATA            @G552PLM*/ 01967000
@RC00788 LA    @15,8                                               0791 01968000
         AL    @15,MESSPTR(,REG8)                                  0791 01969000
         LA    @11,LOGDA(,@15)                                     0791 01970000
         ST    @11,LOGPTR(,@15)                                    0791 01971000
*   LOGDA=EIGHTY8;                  /* LEN OF DATA           @G552PLM*/ 01972000
         MVC   LOGDA(4,@15),@CF03167                               0792 01973000
*   LOGWAIT=REG0;                   /* WAIT STATE CODE       @G552PLM*/ 01974000
         ST    REG0,LOGWAIT(,@15)                                  0793 01975000
*   LOGTIME=LOGTIME&&LOGTIME;       /* ZERO TIME             @G552PLM*/ 01976000
         XC    LOGTIME(8,@15),LOGTIME(@15)                         0794 01977000
*   STM(REG0,REG15,LOGREGS);        /* REGISTERS             @G552PLM*/ 01978000
         STM   REG0,REG15,LOGREGS(@15)                             0795 01979000
*   LOGMSC=LOGMSC&&LOGMSC;          /* NO MSC DATA           @G552PLM*/ 01980000
         XC    LOGMSC(12,@15),LOGMSC(@15)                          0796 01981000
*   REG1=ADDR(RSTILOGB);            /* ADDR OF RCD TO RECD   @G552PLM*/ 01982000
         LR    REG1,@15                                            0797 01983000
*   REG13=WSACRSTI;                 /*                       @YM01377   01984000
*                                      LOAD SAVE AREA ADDRESS        */ 01985000
         L     @15,PSALCCAV                                        0798 01986000
         L     @15,LCCACPUS(,@15)                                  0798 01987000
         L     REG13,WSACRSTI(,@15)                                0798 01988000
*   GEN;                                                           0799 01989000
         RECORD TYPE=LOGREC,RCVRY=SETFRR,HEADER=YES,LENGTH=96,         *01990000
               PARMADR=(REG1)                                           01991000
*   REG0=LOGWAIT;                   /* RESTORE REGISTER FOR STOP   0800 01992000
*                                      START CALL            @YM05421*/ 01993000
         L     @15,MESSPTR(,REG8)                                  0800 01994000
         L     REG0,LOGWAIT+8(,@15)                                0800 01995000
*   REG1=ZERO;                      /* SET NO STAT AREA              */ 01996000
         SLR   REG1,REG1                                           0801 01997000
*   IOCRSVTB=BLKCHAIN;              /* LOAD RESVTBL PTR              */ 01998000
         L     @15,IORSIOCM(,REG8)                                 0802 01999000
         MVC   IOCRSVTB(4,@15),BLKCHAIN(REG8)                      0802 02000000
*   RFY                                                            0803 02001000
*    (REG14) UNRSTD;                                               0803 02002000
*   CALL IEESTPRS;                  /* CALL THE STOP AND RESTART   0804 02003000
*                                      SUBROUTINE                    */ 02004000
         L     @11,CVTPTR                                          0804 02005000
         L     @15,CVTSTPRS(,@11)                                  0804 02006000
         BALR  @14,@15                                             0804 02007000
*   RFY                                                            0805 02008000
*    (REG15) RSTD;                                                 0805 02009000
*   CVTRSTWD=ZERO;                  /* UNLOCK RESOURCE               */ 02010000
         SLR   @14,@14                                             0806 02011000
         L     @11,CVTPTR                                          0806 02012000
         ST    @14,CVTRSTWD(,@11)                                  0806 02013000
*   IF REG15^=ZERO                  /* CHECK WAIT RTRN CODE          */ 02014000
*     THEN                                                         0807 02015000
         CR    REG15,@14                                           0807 02016000
         BE    @RF00807                                            0807 02017000
*     DO;                           /* YES - WAIT THIS CPU           */ 02018000
*/*************                                                    0809 02019000
**       THIS SECTION PUTS THIS CPU INTO A WAIT IF SYSTEM STOP     0809 02020000
**       AND RESTART SUBROUTINE IS UNABLE TO STOP THE ENTIRE       0809 02021000
**       CONFIGURATION.                                            0809 02022000
*************                                                        */ 02023000
*                                                                  0809 02024000
*LDWAIT:                                                           0809 02025000
*       ;                                                          0809 02026000
LDWAIT   DS    0H                                                  0810 02027000
*       RSPSW1=FLCRNPSW;            /* SAVE OLD RESTART PSW          */ 02028000
         MVC   RSPSW1(8,REG8),FLCRNPSW                             0810 02029000
*       RSPSW2(FIVE:EIGHT)=REG0;    /* SET DESIRED CODE              */ 02030000
         ST    REG0,RSPSW2+4(,REG8)                                0811 02031000
*       RSPSW2(ONE:FOUR)=WAITPSW;   /* SET FOR WAIT STATE            */ 02032000
         MVC   RSPSW2(4,REG8),WAITPSW                              0812 02033000
*       CALL IGFPSAD0;              /* OBTAIN AUTHORIZATION  @G51BPLC*/ 02034000
         L     @15,@CV03195                                        0813 02035000
         BALR  @14,@15                                             0813 02036000
*       FLCRNPSW(ONE:FOUR)=IORSPSW; /*                       @G552PLM   02037000
*                                      SET UP FIRST HALF OF PSW TO 0814 02038000
*                                      ROUTE CONTROL TO I/O RESTART     02039000
*                                      WHEN RESTART KEY PRESSED      */ 02040000
         MVC   FLCRNPSW(4),IORSPSW                                 0814 02041000
*       FLCRNPSW(FIVE:EIGHT)=ADDR(IORSTKEY);/* SET ADDRESS PORTION   */ 02042000
         LA    @14,IORSTKEY                                        0815 02043000
         ST    @14,FLCRNPSW+4                                      0815 02044000
*       GEN;                                                       0816 02045000
         LPSW  RSPSW2(8)               LOAD WAIT STATE                  02046000
*IORSTKEY:                                                         0817 02047000
*       ;                           /* RESTART ROUTINE               */ 02048000
IORSTKEY DS    0H                                                  0818 02049000
*       FLCRNPSW=RSPSW1;            /* RESTORE RESTART PSW           */ 02050000
         MVC   FLCRNPSW(8),RSPSW1(REG8)                            0818 02051000
*       CALL IGFPSAE0;              /* RELEASE AUTHORIZATION@G51BPLC */ 02052000
         L     @15,@CV03196                                        0819 02053000
         BALR  @14,@15                                             0819 02054000
*     END;                                                         0820 02055000
*   ELSE                                                           0821 02056000
*     ;                                                            0821 02057000
@RF00807 DS    0H                                                  0822 02058000
*   RFY                                                            0822 02059000
*    (REG14) RSTD;                                                 0822 02060000
*   REG14=REG12;                    /* RESTORE RETURN REGISTER       */ 02061000
         LR    REG14,REG12                                         0823 02062000
*   GEN CODE;                                                      0824 02063000
        BR  @14                        RETURN TO CALLER                 02064000
*   RFY                                                            0825 02065000
*    (REG0,                                                        0825 02066000
*     REG1,                                                        0825 02067000
*     REG4,                                                        0825 02068000
*     REG12,                                                       0825 02069000
*     REG13,                                                       0825 02070000
*     REG14,                                                       0825 02071000
*     REG15) UNRSTD;                                               0825 02072000
*   GEN(TITLE ' IECVRSTI - TERMINATION ROUTINE');                  0826 02073000
*                                                                  0826 02074000
         TITLE ' IECVRSTI - TERMINATION ROUTINE'                        02075000
*/***************************************************************  0827 02076000
**       THIS SECTION OF CODE IS THE TERMINATION SEQUENCE OF I/O   0827 02077000
**       RESTART. IT IS CALLED WHEN CONTROL IS TO BE RETURNED TO   0827 02078000
**       THE CALLER OF I/O RESTART (OR IN THE CASE OF CCH TO IOS)  0827 02079000
**       IT FREES ALL WORK AREAS GOTTEN FROM STORAGE MANAGER,      0827 02080000
**       DELETES THE I/O RESTART FRR AND RESTORES REGISTERS        0827 02081000
**       BEFORE RETURNING.                                         0827 02082000
***************************************************************      */ 02083000
*                                                                  0827 02084000
*   RFY                                                            0827 02085000
*    (REG1,                                                        0827 02086000
*     REG11,                                                       0827 02087000
*     REG13,                                                       0827 02088000
*     REG15) RSTD;                                                 0827 02089000
*IORSEND:                                                          0828 02090000
*   ;                                                              0828 02091000
IORSEND  DS    0H                                                  0829 02092000
*   MBLKCHN=BLKCHAIN;               /* PREPARE TO INSERT THE MESSAGE    02093000
*                                      BUFFER IN THE BLOCK CHAIN     */ 02094000
         L     @14,MESSPTR(,REG8)                                  0829 02095000
         MVC   MBLKCHN(4,@14),BLKCHAIN(REG8)                       0829 02096000
*   BLKCHAIN=MESSPTR;               /* INSERT MESSAGE BUFFER IN CHAIN*/ 02097000
         ST    @14,BLKCHAIN(,REG8)                                 0830 02098000
*   REG13=WSACRSTI;                 /*                       @YM01377   02099000
*                                      SAVE AREA ADDRESS             */ 02100000
         L     @14,PSALCCAV                                        0831 02101000
         L     @14,LCCACPUS(,@14)                                  0831 02102000
         L     REG13,WSACRSTI(,@14)                                0831 02103000
*   REG11=FREE160;                  /* SET TO FREE BLOCKS            */ 02104000
         LA    REG11,32                                            0832 02105000
*   REG15=IOCORMGT;                 /* SET ENTRY POINT               */ 02106000
         L     @12,IORSIOCM(,REG8)                                 0833 02107000
         L     REG15,IOCORMGT(,@12)                                0833 02108000
*   REG1=REG8;                      /* LOAD FIRST BLK IN CHAIN TO  0834 02109000
*                                      FREE                          */ 02110000
         LR    REG1,REG8                                           0834 02111000
*   FRRIORWA=0;                     /* IORWORK FREED         @G552PLM*/ 02112000
         SLR   @14,@14                                             0835 02113000
         ST    @14,FRRIORWA(,REG9)                                 0835 02114000
*   GEN CODE;                                                      0836 02115000
         BAL   @14,4(@15)              GO TO STORAGE MGR FREE           02116000
*                                   /* CALL THE FREE BLOCK SECTION OF   02117000
*                                      IOS STORAGE MANAGER           */ 02118000
*   REG13=FRRSAV;                   /* RESET REG 13 TO SAVE AREA     */ 02119000
         L     REG13,FRRSAV(,REG9)                                 0837 02120000
*   IF FRRIOSUP=ON1                 /* DID I TURN ON IOS SUPER BIT 0838 02121000
*                                                            @YM05487*/ 02122000
*     THEN                                                         0838 02123000
         TM    FRRIOSUP(REG9),B'10000000'                          0838 02124000
         BNO   @RF00838                                            0838 02125000
*     DO;                           /* YES I DID IT          @YM05487*/ 02126000
*       PSAIOSUP=OFF1;              /* TURN IT OFF           @YM05487*/ 02127000
         NI    PSAIOSUP,B'01111111'                                0840 02128000
*       FRRIOSUP=OFF1;              /* INDICATE I TURNED IT OFF    0841 02129000
*                                                            @YM05487*/ 02130000
         NI    FRRIOSUP(REG9),B'01111111'                          0841 02131000
*     END;                          /*                       @YM05487*/ 02132000
*   IF FRRLCSRB='1'B THEN           /* IF LOST CHAN PROC     @G552PLM*/ 02133000
@RF00838 TM    FRRLCSRB(REG9),B'01000000'                          0843 02134000
         BNO   @RF00843                                            0843 02135000
*     DO;                           /*                       @G552PLM*/ 02136000
*       RFY                                                        0845 02137000
*         REG14 RSTD;               /*                       @G552PLM*/ 02138000
*       REG14=CVTSRBRT;             /* LOST CHAN RET ADDR    @G552PLM*/ 02139000
         L     @12,CVTPTR                                          0846 02140000
         L     REG14,CVTSRBRT(,@12)                                0846 02141000
*       REG15=4;                    /* IND LOST CHAN RET     @G552PLM*/ 02142000
         LA    REG15,4                                             0847 02143000
*       GEN(STOSM FRRLCSYS(REG9),X'03');/* ENABLE I/O, EXT   @G552PLM*/ 02144000
         STOSM FRRLCSYS(REG9),X'03'                                     02145000
*     END;                          /*                       @G552PLM*/ 02146000
*   ELSE                                                           0850 02147000
*     REG15=0;                      /* IND NOT LOST CHAN     @G552PLM*/ 02148000
         B     @RC00843                                            0850 02149000
@RF00843 SLR   REG15,REG15                                         0850 02150000
*   GEN CODE;                                                      0851 02151000
@RC00843 DS    0H                                                  0851 02152000
         SETFRR D,WRKREGS=(@11,@03)                                     02153000
         LTR   @15,@15                 DET RETURN              @G552PLM 02154000
         BNZ   NORESTOR                IF LOST CHAN DO NOT     @G552PLM 02155000
*                                       RESTORE REGISTERS      @G552PLM 02156000
         LM    @14,@12,0(@13)          RESTORE REGISTERS       @G552PLM 02157000
NORESTOR EQU   *                       LOST CHAN RET TO SRB    @G552PLM 02158000
*                                      DISPATCHER              @G552PLM 02159000
         BR     @14                    RETURN TO CALLER                 02160000
*   RFY                                                            0852 02161000
*    (REG1,                                                        0852 02162000
*     REG11,                                                       0852 02163000
*     REG13,                                                       0852 02164000
*     REG15,                                                       0852 02165000
*     REG14) UNRSTD;                /*                       @G552PLM*/ 02166000
*   GEN(TITLE ' IECVRSTI - IOSGEN INTERFACE SUBROUTINE');          0853 02167000
*                                                                  0853 02168000
         TITLE ' IECVRSTI - IOSGEN INTERFACE SUBROUTINE'                02169000
*/***************************************************************  0854 02170000
**       THIS SUBROUTINE IS THE I/O RESTART INTERFACE TO IOSGEN'S  0854 02171000
**       PATHMAP FACILITY. IT IS CALLED WITH THE UCB ADDRESS AND   0854 02172000
**       OUTPUTS A PATHMAP TABLE IN THE I/O RESTART WORK AREA      0854 02173000
**       WHICH IS THEN USED TO DETERMINE PATH AVAILABILITY.        0854 02174000
***************************************************************      */ 02175000
*                                                                  0854 02176000
*   RFY                                                            0854 02177000
*    (REG4,                                                        0854 02178000
*     REG5,                                                        0854 02179000
*     REG13,                                                       0854 02180000
*     REG14) RSTD;                                                 0854 02181000
*IOSGIFAC:                                                         0855 02182000
*   REG13=WSACRSTI;                 /* RESTART SAVE AREA             */ 02183000
IOSGIFAC L     @15,PSALCCAV                                        0855 02184000
         L     @15,LCCACPUS(,@15)                                  0855 02185000
         L     REG13,WSACRSTI(,@15)                                0855 02186000
*   REG4=UCBPTR+UCBPRELN;           /* SET UP FOR IOSGEN             */ 02187000
         LA    REG4,512                                            0856 02188000
         ALR   REG4,UCBPTR                                         0856 02189000
*   REG5=ADDR(IOSGTABL);            /* PATH TABLE ADDRESS            */ 02190000
         LA    REG5,IOSGTABL(,REG8)                                0857 02191000
*   GEN CODE;                                                      0858 02192000
         IOSGEN MAP,VAR=ONE                                             02193000
*   RFY                                                            0859 02194000
*    (REG4,                                                        0859 02195000
*     REG5,                                                        0859 02196000
*     REG13,                                                       0859 02197000
*     REG14) UNRSTD;                                               0859 02198000
*   GEN(TITLE ' IECVRSTI - ASYNCHRONOUS MESSAGE INTERFACE');       0860 02199000
*                                                                  0860 02200000
         TITLE ' IECVRSTI - ASYNCHRONOUS MESSAGE INTERFACE'             02201000
*/***************************************************************  0861 02202000
**       THIS SUBROUTINE INTERFACES WITH THE SYSTEM ASYNCHRONOUS   0861 02203000
**       RECORDING FACILITY VIA THE RECORD MACRO. THE MESSAGE      0861 02204000
**       BUFFER IS INPUT BY THE CALLER AND RECORDIT TRIES TO       0861 02205000
**       HAVE THE RECORD FUNCTION TRANSFER THIS INFORMATION TO     0861 02206000
**       ITS BUFFERS FOR EVENTUAL RECORDING. IF THE RECORDING      0861 02207000
**       ROUTINE IS UNABLE TO GET BUFFERS I/O RESTART GOES         0861 02208000
**       TO THE STORAGE MANAGER FOR ANOTHER MESSAGE BUFFER         0861 02209000
**       AND GIVES THE CURRENT ONE TO THE RECORD FUNCTION.         0861 02210000
**            NOTE: THESE BUFFERS ARE LOST TO THE STORAGE          0861 02211000
**                 MANAGER IF THIS IS DONE.                        0861 02212000
***************************************************************      */ 02213000
*                                                                  0861 02214000
*   RFY                                                            0861 02215000
*    (REG1,                                                        0861 02216000
*     REG4,                                                        0861 02217000
*     REG11,                                                       0861 02218000
*     REG12,                                                       0861 02219000
*     REG13,                                                       0861 02220000
*     REG14,                                                       0861 02221000
*     REG15) RSTD;                                                 0861 02222000
*RECORDIT:                                                         0862 02223000
*   ;                                                              0862 02224000
RECORDIT DS    0H                                                  0863 02225000
*   REG4=REG14;                     /* SAVE RETURN ADDRESS           */ 02226000
         LR    REG4,REG14                                          0863 02227000
*   REG1=ADDR(MESSBUF);             /* GET MESS BUF ADDR             */ 02228000
         L     REG1,MESSPTR(,REG8)                                 0864 02229000
         LA    REG1,MESSBUF(,REG1)                                 0864 02230000
*   MSGAD=REG1;                     /* SAVE MSG ADDR IN CASE REISSUE    02231000
*                                      NEEDED                        */ 02232000
         ST    REG1,MSGAD(,REG8)                                   0865 02233000
*   REG13=WSACRSTI;                 /* GET SAVE AREA PTR             */ 02234000
         L     @06,PSALCCAV                                        0866 02235000
         L     @06,LCCACPUS(,@06)                                  0866 02236000
         L     REG13,WSACRSTI(,@06)                                0866 02237000
*RCD1:                                                             0867 02238000
*   ;                               /* ISSUE RECORD                  */ 02239000
RCD1     DS    0H                                                  0868 02240000
*   GEN CODE;                                                      0868 02241000
         LH    @11,0(@01)              GET WTO LENGTH                   02242000
         LA    @11,4(@11)              ADD FOR HEADER                   02243000
         RECORD TYPE=WTO,DATAADR=(@01),BUFFER=YES,RCVRY=SETFRR,        *02244000
               LENGTH=(@11)                                             02245000
*   IF REG15=TWELVE                 /* NOT ENOUGH ROOM IN BUFFER?  0869 02246000
*                                                            @YM05483*/ 02247000
*     THEN                                                         0869 02248000
         CH    REG15,@CH01904                                      0869 02249000
         BNE   @RF00869                                            0869 02250000
*     DO;                                                          0870 02251000
*       REG11=GET160;               /* SET TO GET NEW MSG BUFFER     */ 02252000
         L     REG11,@CF03037                                      0871 02253000
*       REG15=IOCORMGT;             /* IOS STORAGE MGR ADDR          */ 02254000
         L     @06,IORSIOCM(,REG8)                                 0872 02255000
         L     REG15,IOCORMGT(,@06)                                0872 02256000
*       CALL SMGRGB;                /* DO A GET BLOCK                */ 02257000
         BALR  @14,@15                                             0873 02258000
*       MESSPTR=REG11;              /* SET NEW MESSAGE BUFF          */ 02259000
         ST    REG11,MESSPTR(,REG8)                                0874 02260000
*       REG11->MSGENT=ONE;          /* SET MSG COUNT TO 1            */ 02261000
         MVC   MSGENT(2,REG11),@CH00039                            0875 02262000
*RCRDIT2:                                                          0876 02263000
*       REG1=MSGAD;                 /* RELOAD MESSAGE PTR AND ISSUE     02264000
*                                      RECORD WITH BUFFER = NO       */ 02265000
RCRDIT2  L     REG1,MSGAD(,REG8)                                   0876 02266000
*       GEN CODE;                                                  0877 02267000
         RECORD TYPE=WTO,DATAADR=(1),BUFFER=NO,RCVRY=SETFRR             02268000
*     END;                                                         0878 02269000
*   ELSE                                                           0879 02270000
*     ;                             /* GO TO RCRDIT2 REMOVED @YM04212*/ 02271000
@RF00869 DS    0H                                                  0880 02272000
*   REG14=REG4;                     /* RESTORE RETURN REG            */ 02273000
         LR    REG14,REG4                                          0880 02274000
*   GEN CODE;                                                      0881 02275000
        BR   @14                       RETURN TO CALLER                 02276000
*   RFY                                                            0882 02277000
*    (REG1,                                                        0882 02278000
*     REG4,                                                        0882 02279000
*     REG11,                                                       0882 02280000
*     REG12,                                                       0882 02281000
*     REG13,                                                       0882 02282000
*     REG14,                                                       0882 02283000
*     REG15) UNRSTD;                                               0882 02284000
*   GEN(TITLE ' IECVRSTI - FRR');   /*                       @G552PLM*/ 02285000
         TITLE ' IECVRSTI - FRR'                                        02286000
*/***********************************************************@YM05487*/ 02287000
*/*     THE FUNCTIONAL RECOVERY ROUTINE FOR I/O RESTART      @YM05487*/ 02288000
*/*     ATTEMPTS TO FREE OR RESTORE ANY SYSTEM RESOURCES     @YM05487*/ 02289000
*/*     WHICH ARE HELD BY I/O RESTART AT THE TIME THE        @YM05487*/ 02290000
*/*     ERROR OCCURRED. THE I/O RESTART FRR PERCOLATES TO    @YM05487*/ 02291000
*/*     THE CALLERS FRR TO INDICATE THAT THE I/O RESTART     @YM05487*/ 02292000
*/*     FUNCTION FAILED.                                     @YM05487*/ 02293000
*/***********************************************************@YM05487*/ 02294000
*/***********************************************************@YM05487*/ 02295000
*/*     ESTABLISH ADDRESSABILITY AND INITIALIZE SDWA         @YM05487*/ 02296000
*/***********************************************************@YM05487*/ 02297000
*                                                                  0884 02298000
*   RFY                                                            0884 02299000
*    (REG1,                                                        0884 02300000
*     REG14,                                                       0884 02301000
*     REG15,                                                       0884 02302000
*     REG4,                                                        0884 02303000
*     REG5,                                                        0884 02304000
*     REG9) RSTD;                   /*                       @YM05487*/ 02305000
*IORFRR:                            /*                       @YM05487*/ 02306000
*   GEN CODE;                                                      0885 02307000
IORFRR   DS    0H                                                  0885 02308000
*                                                         /* @YM05487*/ 02309000
    LR    REG10,REG15                  GET BASE ADDRESS        @YM05487 02310000
         LA    REG15,IORFRR-IECVRSTI    GET 1ST BASE           @OZ06051 02311000
         SR    REG10,REG15              ADDRESSIBILITY         @OZ06051 02312000
         LA    REG2,4094(,REG10)        GET 2ND BASE           @OZ06051 02313000
*   DO;                             /* SETRP RECORD(YES)RC(0)        */ 02314000
*     RESPECIFY                                                    0887 02315000
*      (GPR00P,                                                    0887 02316000
*       GPR01P,                                                    0887 02317000
*       GPR14P,                                                    0887 02318000
*       GPR15P) RSTD;                                              0887 02319000
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */ 02320000
         MVI   SDWARCDE(GPR01P),X'00'                              0888 02321000
*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */ 02322000
         OI    SDWARCRD(GPR01P),B'10000000'                        0889 02323000
*     RESPECIFY                                                    0890 02324000
*      (GPR00P,                                                    0890 02325000
*       GPR01P,                                                    0890 02326000
*       GPR14P,                                                    0890 02327000
*       GPR15P) UNRSTD;                                            0890 02328000
*   END;                            /* REQUEST NO RETRY,THEREFORE  0891 02329000
*                                      PERCOLATE TO NEXT FRR @YM05487*/ 02330000
*   SDWAMODN=MODNAME;               /* MODULE NAME IN SDWA   @YM05487*/ 02331000
         MVC   SDWAMODN(8,REG1),MODNAME                            0892 02332000
*   SDWACSCT=MODNAME;               /* CSECT NAME IN SDWA    @YM05487*/ 02333000
         MVC   SDWACSCT(8,REG1),MODNAME                            0893 02334000
*   SDWAREXN=FRRNAME;               /* FRR NAME IN SDWA      @YM05487*/ 02335000
         MVC   SDWAREXN(8,REG1),FRRNAME                            0894 02336000
*/***********************************************************@YM05487*/ 02337000
*/*     MAKE SURE I/O RESTART DOES NOT HOLD THE RESTART WORD @YM05487*/ 02338000
*/***********************************************************@YM05487*/ 02339000
*                                                                  0895 02340000
*   REG15=ADDR(CVTRSTWD);           /* GET ADDRESS OF RESTART WORD 0895 02341000
*                                                            @YM05487*/ 02342000
         L     REG15,CVTPTR                                        0895 02343000
         LA    REG15,CVTRSTWD(,REG15)                              0895 02344000
*   REG5=ZERO;                      /* LOAD UNLOCKED VALUE   @YM05487*/ 02345000
         SLR   REG5,REG5                                           0896 02346000
*   REG4=ZERO;                      /* CLEAR REGISTER        @YM05487*/ 02347000
         SLR   REG4,REG4                                           0897 02348000
*   REG4=REG4|PSACPULA;             /* GET LOGICAL CPU ID    @YM05487*/ 02349000
         LH    @12,PSACPULA                                        0898 02350000
         OR    REG4,@12                                            0898 02351000
*   GEN CODE(SLL @04,16);           /* PUT IN HIGH END       @YM05487*/ 02352000
         SLL @04,16                                                     02353000
*   REG4=REG4|IORID;                /* OR IN RESTART ID      @YM05487*/ 02354000
         O     REG4,IORID                                          0900 02355000
*   GEN CODE(CS @04,@05,0(@15) );   /* IF I/O RESTART HAS WORD,THIS     02356000
*                                      WILL FREE IT,IF NOT RESTART 0901 02357000
*                                      WORD IS UNCHANGED     @YM05487*/ 02358000
         CS @04,@05,0(@15)                                              02359000
*/***********************************************************@YM05487*/ 02360000
*/*     GET POINTER TO I/O RESTART WORK AREA FROM FRR AREA   @YM05487*/ 02361000
*/***********************************************************@YM05487*/ 02362000
*                                                                  0902 02363000
*   REG9=SDWAPARM;                  /* GET FRR AREA PTR      @YM05487*/ 02364000
         L     REG9,SDWAPARM(,REG1)                                0902 02365000
*   REG8=FRRIORWA;                  /* GET WORK AREA PTR     @YM05487*/ 02366000
         L     REG8,FRRIORWA(,REG9)                                0903 02367000
*/***********************************************************@YM05487*/ 02368000
*/*     IF WORK AREA POINTER IS ZERO,THERE IS NOTHING TO     @YM05487*/ 02369000
*/*     BACK OUT OR FREE,RETURN TO RTM WITH PERCOLATE.       @YM05487*/ 02370000
*/***********************************************************@YM05487*/ 02371000
*                                                                  0904 02372000
*   IF REG8=ZERO THEN               /* IF NO WORK AREA       @G552PLM*/ 02373000
         LTR   REG8,REG8                                           0904 02374000
         BNZ   @RF00904                                            0904 02375000
*     DO;                           /* RETURN TO RTM         @G552PLM*/ 02376000
*       IF FRRIOSUP=ON1 THEN        /* IF WE SET SUPER BIT   @G552PLM*/ 02377000
         TM    FRRIOSUP(REG9),B'10000000'                          0906 02378000
         BNO   @RF00906                                            0906 02379000
*         DO;                       /*                       @G552PLM*/ 02380000
*           PSAIOSUP=OFF1;          /* TURN IT OFF           @G552PLM*/ 02381000
         NI    PSAIOSUP,B'01111111'                                0908 02382000
*           FRRIOSUP=OFF1;          /* IND WE TURNED OFF     @G552PLM*/ 02383000
