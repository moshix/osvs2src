*     DO;                           /*                       @YM05487*/ 02455000
*       GEN CODE( LCTL @02,@02,CNTRLSAV(@08));/* RESTORE     @YM05487*/ 02456000
          LCTL @02,@02,CNTRLSAV(@08)                                    02457000
*       CTLREG=OFF1;                /* FLAG OFF              @YM05487*/ 02458000
         NI    CTLREG(REG8),B'11110111'                            0935 02459000
*     END;                          /*                       @YM05487*/ 02460000
*/***********************************************************@G552PLM*/ 02461000
*/*     IF LOST CHANNEL FOUND A RESERVED DEVICE, LOAD A      @G552PLM*/ 02462000
*/*     DISABLED WAIT STATE                                  @G552PLM*/ 02463000
*/***********************************************************@G552PLM*/ 02464000
*                                                                  0937 02465000
*   SDWAADDR=ADDR(SDWA);            /* IF LOAD WAIT ADDR OF SDWA IN     02466000
*                                      IORWORK               @G552PLM*/ 02467000
@RF00932 ST    REG1,SDWAADDR(,REG8)                                0937 02468000
*   IF FRRLCRSV='1'B THEN           /* IF RESERVED DEVICE    @G552PLM*/ 02469000
         TM    FRRLCRSV(REG9),B'00100000'                          0938 02470000
         BO    @RT00938                                            0938 02471000
*     GO TO LOSTCHWT;               /* LOAD DISABLED WAIT    @G552PLM*/ 02472000
*   IF INDICAT=LOSTCHAN&            /* IF LOST CHAN AND      @G552PLM*/ 02473000
*       FRRLCSRB='0'B THEN          /* SRB STILL RESERVED    @G552PLM*/ 02474000
         CLC   INDICAT(1,REG8),LOSTCHAN                            0940 02475000
