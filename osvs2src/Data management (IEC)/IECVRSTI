*   GEN CODE(BAL   @14,4(@15));     /* GO TO STORAGE MGR+4   @YM05487*/ 02510000
         BAL   @14,4(@15)                                               02511000
*   RFY                                                            0952 02512000
*    (REG11,                                                       0952 02513000
*     REG13) UNRSTD;                                               0952 02514000
*                                                                  0952 02515000
*/***********************************************************@YM05487*/ 02516000
*/*     TURN OFF IOS SUPER BIT IF I TURNED IT ON             @YM05487*/ 02517000
*/***********************************************************@YM05487*/ 02518000
*                                                                  0953 02519000
*   IF FRRIOSUP=ON1                 /* DID I TURN ON IOS SUPER BIT 0953 02520000
*                                                            @YM05487*/ 02521000
*     THEN                                                         0953 02522000
         TM    FRRIOSUP(REG9),B'10000000'                          0953 02523000
         BNO   @RF00953                                            0953 02524000
*     DO;                           /* YES I DID IT          @YM05487*/ 02525000
*       PSAIOSUP=OFF1;              /* TURN IT OFF           @YM05487*/ 02526000
         NI    PSAIOSUP,B'01111111'                                0955 02527000
*       FRRIOSUP=OFF1;              /* INDICATE I TURNED IT OFF    0956 02528000
*                                                            @YM05487*/ 02529000
         NI    FRRIOSUP(REG9),B'01111111'                          0956 02530000
*     END;                          /*                       @YM05487*/ 02531000
*/***********************************************************@YM05487*/ 02532000
*/*     RETURN TO RTM WITH PERCOLATE REQUEST                 @YM05487*/ 02533000
*/***********************************************************@YM05487*/ 02534000
*                                                                  0958 02535000
*   REG14=REG5;                     /* RESTORE RETURN REG    @YM05487*/ 02536000
@RF00953 LR    REG14,REG5                                          0958 02537000
*   REG1=REG4;                      /* RESTORE SDWA PTR      @YM05487*/ 02538000
         LR    REG1,REG4                                           0959 02539000
*   GEN CODE( BR @14);              /* RETURN TO RTM         @YM05487*/ 02540000
          BR @14                                                        02541000
*   GEN(TITLE ' IECVRSTI - UCB PROCESSOR RTN - UCBACT');/*   @G552PLM*/ 02542000
         TITLE ' IECVRSTI - UCB PROCESSOR RTN - UCBACT'                 02543000
*UCBACT:                                                           0962 02544000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* PROCESS A UCB         @G552PLM*/ 02545000
         B     @PB00002                                            0962 02546000
UCBACT   DS    0H                                                  0963 02547000
*/***************************************************************  0963 02548000
**       THE FOLLOWING CODE  PERFORMS SEVERAL FUNCTIONS.           0963 02549000
**       1) JUSTIFY THE UCB POINTER TO THE PREFIX.                 0963 02550000
**       2) GET THE UCB LOCK.                                      0963 02551000
**       3) DETERMINE (VIA A CALL TO IOSGEN) WHETHER THERE ARE     0963 02552000
**          ANY PATHS LEFT TO THE DEVICE FROM THE GOOD CPU.        0963 02553000
**       4) IF THERE IS A GOOD PATH THAT PATH IS PUT IN THE        0963 02554000
**          RESERVE TABLE FOR TEMP HOLD.                           0963 02555000
**       5) IF THERE ARE NO PATHS THE FIRST PATH TO THE DEVICE     0963 02556000
**          IS PUT IN THE RESERVE TABLE FOR TEMP HOLD. IN          0963 02557000
**          ADDITION THE DEVICE IS MARKED OFFLINE FOR HEIRARCHY    0963 02558000
**          REASONS AND A DESCRIPTIVE MESSAGE IS BUILT.            0963 02559000
***************************************************************      */ 02560000
*                                                                  0963 02561000
*   UCBACTRT=REG14;                 /* SAVE RET ADDR         @G552PLM*/ 02562000
         ST    REG14,UCBACTRT(,REG8)                               0963 02563000
*   PATHAVIL=ON1;                   /* SET PATH AVAILABLE AS INITIAL    02564000
*                                      STATE                         */ 02565000
         OI    PATHAVIL(REG8),B'00010000'                          0964 02566000
*   UCBPTR=UCBPTR-UCBPRELN;         /* JUSTIFY TO UCB PREFIX         */ 02567000
         SH    UCBPTR,@CH03050                                     0965 02568000
*   RFY                                                            0966 02569000
*    (REG11) RSTD;                                                 0966 02570000
*   RFY                                                            0967 02571000
*    (REG14) RSTD;                                                 0967 02572000
*   RFY                                                            0968 02573000
*    (REG13) RSTD;                                                 0968 02574000
*   RFY                                                            0969 02575000
*    (REG12) RSTD;                                                 0969 02576000
*   IF UCBMTPXP=ON1                 /* MULT EXPOSURE DEVICE          */ 02577000
*     THEN                                                         0970 02578000
         LA    @06,512                                             0970 02579000
         ALR   @06,UCBPTR                                          0970 02580000
         TM    UCBMTPXP(@06),B'00001000'                           0970 02581000
         BNO   @RF00970                                            0970 02582000
*     REG11=ADDR(UCBBASE->UCBLOCK); /* YES - LOCK ON BASE            */ 02583000
         L     REG11,UCBBASE(,@06)                                 0971 02584000
         LA    REG11,UCBLOCK(,REG11)                               0971 02585000
*   ELSE                                                           0972 02586000
*     REG11=ADDR(UCBLOCK);          /* NO - LOCK ON UCB              */ 02587000
         B     @RC00970                                            0972 02588000
@RF00970 LA    REG11,UCBLOCK(,UCBPTR)                              0972 02589000
*   LOCKADDR=REG11;                 /* SAVE UCBLOCK ADDR.    @YM05487*/ 02590000
@RC00970 ST    REG11,LOCKADDR(,REG8)                               0973 02591000
*   GEN;                                                           0974 02592000
GLOCK1   SETLOCK   OBTAIN,TYPE=IOSUCB,ADDR=(11),RELATED=(UCB,IECVRSTI(FX02593000
               RLOCK1)),MODE=UNCOND                                     02594000
*   LOCKUCB=ON1;                    /* UCB LOCK HELD         @YM05487*/ 02595000
         OI    LOCKUCB(REG8),B'10000000'                           0975 02596000
*   RFY                                                            0976 02597000
*    (REG11,                                                       0976 02598000
*     REG12) UNRSTD;                                               0976 02599000
*   RFY                                                            0977 02600000
*    (REG4,                                                        0977 02601000
*     REG5) RSTD;                                                  0977 02602000
*   CALL IOSGIFAC;                  /* CALL IOSGEN CODE              */ 02603000
         BAL   @14,IOSGIFAC                                        0978 02604000
*IOSGRTN1:                                                         0979 02605000
*   ;                                                              0979 02606000
*                                                                  0979 02607000
IOSGRTN1 DS    0H                                                  0980 02608000
*/***********************************************************@G552PLM*/ 02609000
*/* DETERMINE IF PATH TO DEVICE IS AVAILABLE                 @G552PLM*/ 02610000
*/*                                                                  */ 02611000
*/* NOTE -                                                           */ 02612000
*/*        THE FLAG UCBIORST HAS TWO MEANINGS.                       */ 02613000
*/*        IT CAN MEAN I/O IS ACTIVE FOR THE DEVICE VIA A CRH        */ 02614000
*/*        CONNECTED CHANNEL ATTACHED TO AN OFFLINE CPU.             */ 02615000
*/*        IT CAN ALSO MEAN THAT A DEVICE IS LOST TO THE SYSTEM (NO  */ 02616000
*/*        PATHS TO THE DEVICE EXIST) AND IOS MUST REJECT ANY        */ 02617000
*/*        REQUESTS FOR THE DEVICE.                                  */ 02618000
*/*                                                                  */ 02619000
*/*        I/O RESTART ACR CALLER - IF UCBIORST IS ON, DEVICE HAS    */ 02620000
*/*        ALREADY BEEN PROCESSED.  IT CANNOT BE ACTIVE.             */ 02621000
*/*                                                                  */ 02622000
*/*        I/O RESTART LOSTCHAN CALLER - DEPENDENT ON CRHCHSW        */ 02623000
*/*                                                                  */ 02624000
*/*          UCBIORST  CRHCHSW                                       */ 02625000
*/*                                                                  */ 02626000
*/*             1         1       I/O RESTART IS PROCESSING A CRH    */ 02627000
*/*                               CONNECTED LOSTCHAN.  DEVICE MAY BE */ 02628000
*/*                               REFERENCED OVER CONNECTION AND     */ 02629000
*/*                               MUST BE PROCESSED.                 */ 02630000
*/*                                                                  */ 02631000
*/*             0         1       I/O RESTART IS PROCESSING A CRH    */ 02632000
*/*                               CONNECTED LOSTCHAN.  DEVICE IS NOT */ 02633000
*/*                               BEING USED OVER CRH CONNECTION AND */ 02634000
*/*                               MUST NOT BE PROCESSED AS IT MAY BE */ 02635000
*/*                               CONNECTED TO THIS CPU.             */ 02636000
*/*                                                                  */ 02637000
*/*             0         0       I/O RESTART IS PROCESSING A LOST   */ 02638000
*/*                               CHANNEL ATTACHED TO THIS CPU.      */ 02639000
*/*                               DEVICE IS NOT ACTIVE OVER A CRH    */ 02640000
*/*                               CONNECTION AND MUST BE PROCESSED.  */ 02641000
*/*                                                                  */ 02642000
*/*             1         0       I/O RESTART IS PROCESSING A LOST   */ 02643000
*/*                               CHANNEL ATTACHED TO THIS CPU.      */ 02644000
*/*                               DEVICE HAS EITHER BEEN PROCESSED   */ 02645000
*/*                               OR IS ACTIVE OVER CRH CONNECTED    */ 02646000
*/*                               CHANNEL.  DEVICE MUST NOT BE       */ 02647000
*/*                               PROCESSED.                         */ 02648000
*/*                                                                  */ 02649000
*/*        IF UCBIORST=CRHCHSW PROCESS DEVICE ELSE BYPASS            */ 02650000
*/*                                                                  */ 02651000
*/***********************************************************@G552PLM*/ 02652000
*                                                                  0980 02653000
*   ENTUCB(ENTCNT)=UCBPTR;          /* UCB ADR IN RSV TAB ENTRY    0980 02654000
*                                                            @G552PLM*/ 02655000
         L     @12,RSVTBLPT(,REG8)                                 0980 02656000
         L     @11,ENTCNT(,@12)                                    0980 02657000
         SLA   @11,3                                               0980 02658000
