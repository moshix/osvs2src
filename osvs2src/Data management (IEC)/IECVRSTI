         BZ    @RF00472                                            0472 00745000
*     DO;                           /* SS1 IN SYSTEM         @Y30LPLC*/ 00746000
*       RFY                                                        0474 00747000
*        (REG1) RSTD;               /*                       @Y30LPLC*/ 00748000
*       RFY                                                        0475 00749000
*         SRBSECT BASED(REG1);      /* BASE SRB ON REG 1     @Y30LPLC*/ 00750000
*       REG1=ICBSRBP;               /* SET UP SRB BASE       @Y30LPLC*/ 00751000
         L     REG1,ICBSRBP(,@12)                                  0476 00752000
*       SRBEP=ICBIORCI;             /* SS1 CODE EP           @Y30LPLC*/ 00753000
         MVC   SRBEP(4,REG1),ICBIORCI(@12)                         0477 00754000
*       SRBASCB=ADDR(IEAMASCB);     /* MASTER SCHEDULER      @Y30LPLC   00755000
*                                      ASCB.                 @Y30LPLC*/ 00756000
         L     @12,@CV03191                                        0478 00757000
         ST    @12,SRBASCB(,REG1)                                  0478 00758000
*       SRBPRIOR=0;                 /* PRIORITY-SYSTEM       @Y30LPLC*/ 00759000
         MVI   SRBPRIOR(REG1),X'00'                                0479 00760000
*       GEN( SCHEDULE SRB=(REG1),SCOPE=GLOBAL);/* SS1 SRB    @Y30LPLC*/ 00761000
          SCHEDULE SRB=(REG1),SCOPE=GLOBAL                              00762000
*     END;                                                         0481 00763000
*                                                                  0481 00764000
*/********************************************************** @G552PLM*/ 00765000
*/*      CHECK ALL DEVICES IN SYSTEM TO DETERMINE WHICH      @G552PLM*/ 00766000
*/*      REQUIRE I/O RESTART PROCESS BECAUSE OF THE LOSS     @G552PLM*/ 00767000
*/*      OF A CPU.                                           @G552PLM*/ 00768000
*/********************************************************** @G552PLM*/ 00769000
*                                                                  0482 00770000
*   UCBPTR=UCBPT;                   /* GET PTR TO 1ST UCB    @G552PLM*/ 00771000
@RF00472 L     @12,TBLPTR(,REG8)                                   0482 00772000
         SLR   UCBPTR,UCBPTR                                       0482 00773000
         ICM   UCBPTR,3,UCBPT(@12)                                 0482 00774000
*   DO WHILE UCBPTR^=TABLEND;       /* INDEX THRU ALL UCB    @G552PLM*/ 00775000
         B     @DE00483                                            0483 00776000
@DL00483 DS    0H                                                  0484 00777000
*     IF UCBPTR^=ZERO THEN          /* IF UCB EXISTS         @G552PLM*/ 00778000
         LTR   UCBPTR,UCBPTR                                       0484 00779000
         BZ    @RF00484                                            0484 00780000
*       DO;                                                        0485 00781000
*         CALL UCBACT;              /* PROCESS DEVICE        @G552PLM*/ 00782000
         BAL   @14,UCBACT                                          0486 00783000
*       END;                                                       0487 00784000
*     ELSE                                                         0488 00785000
*       ;                                                          0488 00786000
@RF00484 DS    0H                                                  0489 00787000
*     TBLPTR=TBLPTR+TWO;            /* UP LOOKUP TAB INDX    @G552PLM*/ 00788000
         LA    @12,2                                               0489 00789000
         AL    @12,TBLPTR(,REG8)                                   0489 00790000
         ST    @12,TBLPTR(,REG8)                                   0489 00791000
*     UCBPTR=UCBPT;                 /* PTR TO NEXT UCB       @G552PLM*/ 00792000
         SLR   UCBPTR,UCBPTR                                       0490 00793000
         ICM   UCBPTR,3,UCBPT(@12)                                 0490 00794000
*   END;                                                           0491 00795000
@DE00483 C     UCBPTR,@CF03012                                     0491 00796000
         BNE   @DL00483                                            0491 00797000
*                                                                  0492 00798000
*/***************************************************************  0492 00799000
**       IF THE END OF THE TABLE HAS BEEN REACHED THE MESSAGE      0492 00800000
**       BUFFER IS PURGED IF NECESSARY, AND A CHECK IS MADE TO     0492 00801000
**       DETERMINE IF THE RESERVE TABLE HAS ANY VALID ENTRIES.     0492 00802000
**       IF NO ENTRIES ARE FOUND AN I/O RESET (SIGP) IS ISSUED     0492 00803000
**       TO THE BAD CPU TO CLEAR ANY CONNECTION WITH ACTIVE        0492 00804000
**       DEVICES AND CONTROL IS RETURNED TO ACR.                   0492 00805000
**       IF THE RESERVE TABLE DOES HAVE VALID ENTRIES THIS CODE    0492 00806000
**       CALLS THE WAIT ROUTINE TO PUT THE SYSTEM INTO A 041       0492 00807000
**       WAIT STATE. AFTER DOING THIS THE I/O RESET (SIGP) IS      0492 00808000
**       ISSUED TO THE BAD CPU.                                    0492 00809000
***************************************************************      */ 00810000
*                                                                  0492 00811000
*   IF MSGENT>ONE                   /* IS THERE SOMETHING IN THE   0492 00812000
*                                      MESSAGE BUFFER                */ 00813000
*     THEN                                                         0492 00814000
         L     @12,MESSPTR(,REG8)                                  0492 00815000
         CLC   MSGENT(2,@12),@CH00039                              0492 00816000
         BNH   @RF00492                                            0492 00817000
*     CALL RECORDIT;                /* YES - OUTPUT IT GO RECORD   0493 00818000
*                                      MESSAGE                       */ 00819000
         BAL   @14,RECORDIT                                        0493 00820000
*   ELSE                                                           0494 00821000
*     ;                                                            0494 00822000
@RF00492 DS    0H                                                  0495 00823000
*   PASSIND=OFF1;                   /* SET PASS INDICATOR TO FIRST 0495 00824000
*                                      PASS                          */ 00825000
         NI    PASSIND(REG8),B'01111111'                           0495 00826000
*   IF(ENTCNT=ONE&RCHAIN=ZERO)      /* IS TABLE EMPTY                */ 00827000
*     THEN                                                         0496 00828000
         L     @12,RSVTBLPT(,REG8)                                 0496 00829000
         CLC   ENTCNT(4,@12),@CF00039                              0496 00830000
         BNE   @RF00496                                            0496 00831000
         ICM   @11,15,RCHAIN(@12)                                  0496 00832000
         BNZ   @RF00496                                            0496 00833000
*     DO;                           /* YES - CLEAR OTHER CPU         */ 00834000
*       CALL SIGPRTN;               /* I/O RESET TO DOWN CPU         */ 00835000
         BAL   @14,SIGPRTN                                         0498 00836000
*       GO TO IORSEND;              /* CLEAN UP AND RETURN           */ 00837000
         B     IORSEND                                             0499 00838000
*     END;                                                         0500 00839000
*   ELSE                                                           0501 00840000
*     ;                                                            0501 00841000
@RF00496 DS    0H                                                  0502 00842000
*   RFY                                                            0502 00843000
*    (REG0) RSTD;                                                  0502 00844000
*   ENT(ENTCNT)=ENT(ENTCNT)&&ENT(ENTCNT);/* ZERO THE RESIDUAL ENT    */ 00845000
         L     @12,RSVTBLPT(,REG8)                                 0503 00846000
         L     @11,ENTCNT(,@12)                                    0503 00847000
         SLA   @11,3                                               0503 00848000
         ALR   @12,@11                                             0503 00849000
         XC    ENT-8(8,@12),ENT-8(@12)                             0503 00850000
*   REG0=FORTY1;                    /* SET WAIT CODE                 */ 00851000
         LA    REG0,65                                             0504 00852000
*   CALL WAITRTN;                   /* GO TO WAIT RNT IFACE          */ 00853000
         BAL   @14,WAITRTN                                         0505 00854000
*   RFY                                                            0506 00855000
*    (REG0) UNRSTD;                                                0506 00856000
*   CALL SIGPRTN;                   /* I/O RESET TO DOWN CPU         */ 00857000
         BAL   @14,SIGPRTN                                         0507 00858000
*                                                                  0508 00859000
*/***************************************************************  0508 00860000
**       THE FOLLOWING CODE IS ENTERED AFTER THE I/O RESET HAS     0508 00861000
**       TAKEN PLACE. IT SETS THE WORK AREA RESERVE TABLE          0508 00862000
**       POINTER TO POINT TO THE FIRST RESERVE TABLE AND SETS      0508 00863000
**       THE TABLE INDEX TO PROCESS THE FIRST ENTRY.               0508 00864000
***************************************************************      */ 00865000
*                                                                  0508 00866000
*RERESV:                                                           0508 00867000
*   ;                                                              0508 00868000
RERESV   DS    0H                                                  0509 00869000
*   RSVTBLPT=BLKCHAIN;              /* SET RESERVE TABLE PTR TO FIRST   00870000
*                                      ENTRY                         */ 00871000
         L     @12,BLKCHAIN(,REG8)                                 0509 00872000
         ST    @12,RSVTBLPT(,REG8)                                 0509 00873000
*   ENTCNT=ONE;                     /* SET FOR FIRST ENT             */ 00874000
         MVC   ENTCNT(4,@12),@CF00039                              0510 00875000
*   RFY                                                            0511 00876000
*    (REG4) RESTRICTED;                                            0511 00877000
*                                                                  0511 00878000
*/***************************************************************  0512 00879000
**       THIS CODE PROCESSES THE RESERVE TABLE.                    0512 00880000
**       FOR ONLINE RESERVE ENTRIES CONTROL IS PASSED TO THE       0512 00881000
**       RESERVE ROUTINE.                                          0512 00882000
**       FOR OFFILNE RESERVE ENTRIES IF THIS IS THE FIRST (ONLINE  0512 00883000
**       RESERVE) PASS THE ENTRY IS IGNORED. IF THIS IS THE        0512 00884000
**       SECOND (OFFLINE RESERVE) PASS THE ENTRY IS PROCESSED      0512 00885000
**       AS A NORMAL RESERVE. AS OFFLINE RESERVES ARE ENCOUNTERED  0512 00886000
**       A FLAG INDICATING OFFLINE RESERVES PRESENT IS SET AND     0512 00887000
**       USED TO DETERMINE IF A SECOND PASS IS NECESSARY AFTER     0512 00888000
**       THE ONLINE RESERVE PASS.(WHETHER A 042 WAIT SHOULD BE     0512 00889000
**       ISSUED.)                                                  0512 00890000
**       IF THE ENTRY IS ZERO THE TABLE IS UPPED TO THE NEXT       0512 00891000
**       ENTRY.                                                    0512 00892000
**       WHEN ALL ENTRIES HAVE BEEN PROCESSED THE TABLE IS         0512 00893000
**       CHECKED TO SEE THAT ALL ENTRIES ARE ZERO. IF SO           0512 00894000
**       NORMAL TERMINATION TAKES PLACE, OTHERWISE A 043 HARD      0512 00895000
**       WAIT IS ENTERED.                                          0512 00896000
***************************************************************      */ 00897000
*                                                                  0512 00898000
*CKOFF:                                                            0512 00899000
*   IF ENT(ENTCNT,ONE:TWO)=ZERO     /* IS THIS A NULL ENTRY          */ 00900000
*     THEN                                                         0512 00901000
CKOFF    L     @12,RSVTBLPT(,REG8)                                 0512 00902000
         L     @11,ENTCNT(,@12)                                    0512 00903000
         SLA   @11,3                                               0512 00904000
         ALR   @11,@12                                             0512 00905000
         SLR   @06,@06                                             0512 00906000
         ICM   @06,3,ENT-8(@11)                                    0512 00907000
         LTR   @06,@06                                             0512 00908000
         BZ    @RT00512                                            0512 00909000
*     GO TO UPNXT1;                 /* YES - GET NEXT ENRTY          */ 00910000
*   ELSE                                                           0514 00911000
*     DO;                           /* NO - PROCESS THIS INT         */ 00912000
*       UCBPTR=ENTUCB(EVAL(ENTCNT));/* SET UP UCB POINTER            */ 00913000
         L     @11,ENTCNT(,@12)                                    0515 00914000
         SLA   @11,3                                               0515 00915000
         L     UCBPTR,ENTUCB-8(@11,@12)                            0515 00916000
*       IF ROFFLINE(EVAL(ENTCNT))=ON1/* IS THIS AN OFFLINE ENTRY     */ 00917000
*         THEN                                                     0516 00918000
         ALR   @12,@11                                             0516 00919000
         TM    ROFFLINE-8(@12),B'10000000'                         0516 00920000
         BNO   @RF00516                                            0516 00921000
*         DO;                       /* YES CHECK FIRST PASS          */ 00922000
*           OFFIND=ON1;             /* SET OFFLINE RESERVE         0518 00923000
*                                      ENCOUNTERED INDICATOR         */ 00924000
         OI    OFFIND(REG8),B'00001000'                            0518 00925000
*           IF PASSIND=OFF1         /* IS THIS FIRST PASS            */ 00926000
*             THEN                                                 0519 00927000
         TM    PASSIND(REG8),B'10000000'                           0519 00928000
         BNZ   @RF00519                                            0519 00929000
*             DO;                   /* YES-UPDATE TABLE INDX         */ 00930000
*UPNXT1:                                                           0521 00931000
*               ENTCNT=ENTCNT+ONE;  /* INCR ENTCNT                   */ 00932000
UPNXT1   L     @12,RSVTBLPT(,REG8)                                 0521 00933000
         LA    @11,1                                               0521 00934000
         AL    @11,ENTCNT(,@12)                                    0521 00935000
         ST    @11,ENTCNT(,@12)                                    0521 00936000
*               IF ENTCNT=ENTMAX+ONE/* LAST ENTRY IN TABLE           */ 00937000
*                 THEN                                             0522 00938000
         CH    @11,@CH01699                                        0522 00939000
         BNE   @RF00522                                            0522 00940000
*                 DO;                                              0523 00941000
*                   IF RCHAIN=ZERO  /* LAST TABLE IN CHAIN           */ 00942000
*                     THEN                                         0524 00943000
         ICM   @11,15,RCHAIN(@12)                                  0524 00944000
         BNZ   @RF00524                                            0524 00945000
*                     DO;                                          0525 00946000
*                       IF PASSIND=OFF1/* IS THIS FIRST PASS         */ 00947000
*                         THEN                                     0526 00948000
         TM    PASSIND(REG8),B'10000000'                           0526 00949000
         BNZ   @RF00526                                            0526 00950000
*                         DO;                                      0527 00951000
*                           IF OFFIND=OFF1/* WAS AN OFFLINE RESV   0528 00952000
*                                      ENCOUNTERED                   */ 00953000
*                             THEN                                 0528 00954000
         TM    OFFIND(REG8),B'00001000'                            0528 00955000
         BZ    @RT00528                                            0528 00956000
*                             GO TO TBLCK0;/* NO - GET OUT           */ 00957000
*                           ELSE                                   0530 00958000
*                             ;     /* YES-SET 042 WAIT              */ 00959000
*                           RFY                                    0531 00960000
*                            (REG0) RSTD;                          0531 00961000
*                           REG0=FORTY2;/* SET WAIT CODE             */ 00962000
         LA    REG0,66                                             0532 00963000
*                           CALL WAITRTN;/* GO TO WAIT RTN IFACE     */ 00964000
         BAL   @14,WAITRTN                                         0533 00965000
*                           RFY                                    0534 00966000
*                            (REG0) UNRSTD;                        0534 00967000
*                           PASSIND=ON1;/* SET SECOND PASS           */ 00968000
         OI    PASSIND(REG8),B'10000000'                           0535 00969000
*                           GO TO RERESV;/* PROCESS OFFLINE RESVS    */ 00970000
         B     RERESV                                              0536 00971000
*                         END;                                     0537 00972000
*                       ELSE                                       0538 00973000
*                         DO;       /* INSURE ALL ENTS 0             */ 00974000
@RF00526 DS    0H                                                  0539 00975000
*TBLCK0:                                                           0539 00976000
*                           RSVTBLPT=BLKCHAIN;/* RESET RESERVE TBL 0539 00977000
*                                      PTR                           */ 00978000
TBLCK0   MVC   RSVTBLPT(4,REG8),BLKCHAIN(REG8)                     0539 00979000
*TBLCK:                                                            0540 00980000
*                           IF((ENTRIES(ONE:ONE)=ZERO)&(ENTRIES(ONE:    00981000
*                               ONE51)=ENTRIES(TWO:ONE52)))/* ARE ALL   00982000
*                                      ENTRIES ZERO SECOND PASS    0540 00983000
*                                      PROCESS                       */ 00984000
*                             THEN                                 0540 00985000
TBLCK    SLR   @12,@12                                             0540 00986000
         L     @11,RSVTBLPT(,REG8)                                 0540 00987000
         SLR   @06,@06                                             0540 00988000
         IC    @06,ENTRIES(,@11)                                   0540 00989000
         CR    @12,@06                                             0540 00990000
         BNE   @RF00540                                            0540 00991000
         CLC   ENTRIES(151,@11),ENTRIES+1(@11)                     0540 00992000
         BNE   @RF00540                                            0540 00993000
*                             DO;   /* YES - TRY NEXT TBL            */ 00994000
*                               RSVTBLPT=RCHAIN;/* SET TO NEXT TABLE */ 00995000
         L     @11,RCHAIN(,@11)                                    0542 00996000
         ST    @11,RSVTBLPT(,REG8)                                 0542 00997000
*                               IF RSVTBLPT=ZERO/* LAST ENTRY        */ 00998000
*                                 THEN                             0543 00999000
         CR    @11,@12                                             0543 01000000
         BE    @RT00543                                            0543 01001000
*                                 GO TO IORSEND;/* FINISHED - RETURN    01002000
*                                      TO ACR                        */ 01003000
*                               ELSE                               0545 01004000
*                                 GO TO TBLCK;/* CONTINUE TABLE SCAN    01005000
*                                      FOR NON-ZERO ENTS             */ 01006000
         B     TBLCK                                               0545 01007000
*                             END;                                 0546 01008000
*                           ELSE                                   0547 01009000
*                             GO TO HARDWAIT;/* PUT SYSTEM INTO A  0547 01010000
*                                      HARD WAIT STATE (043)         */ 01011000
*                         END;                                     0548 01012000
*                     END;                                         0549 01013000
*                   ELSE                                           0550 01014000
*                     DO;                                          0550 01015000
@RF00524 DS    0H                                                  0551 01016000
*                       RSVTBLPT=RCHAIN;/* GET NEXT TABLE            */ 01017000
         L     @12,RSVTBLPT(,REG8)                                 0551 01018000
         L     @12,RCHAIN(,@12)                                    0551 01019000
         ST    @12,RSVTBLPT(,REG8)                                 0551 01020000
*                       ENTCNT=ONE; /* SET FOR FIRST ENT             */ 01021000
         MVC   ENTCNT(4,@12),@CF00039                              0552 01022000
*                       GO TO CKOFF;/* CHECK ENTS IN TABLE           */ 01023000
         B     CKOFF                                               0553 01024000
*                     END;                                         0554 01025000
*                 END;                                             0555 01026000
*               ELSE                                               0556 01027000
*                 GO TO CKOFF;      /* NOT LAST ENT CK NEXT          */ 01028000
*             END;                                                 0557 01029000
*           ELSE                                                   0558 01030000
*             ;                                                    0558 01031000
@RF00519 DS    0H                                                  0559 01032000
*         END;                                                     0559 01033000
*       ELSE                                                       0560 01034000
*         ;                                                        0560 01035000
@RF00516 DS    0H                                                  0561 01036000
*     END;                                                         0561 01037000
*                                                                  0561 01038000
*/***************************************************************  0562 01039000
**       THIS CODE DOES THE ACTUAL RESERVE FOR ENTRIES IN THE      0562 01040000
**       RESERVE TABLE. AFTER ISSUING THE SIO FOR THE RESERVE      0562 01041000
**       ONE OF SEVERAL ROUTINES IS ENTERED TO HANDLE THE          0562 01042000
**       PRESENTED CONDITION CODE.                                 0562 01043000
***************************************************************      */ 01044000
*                                                                  0562 01045000
*RESVIT:                                                           0562 01046000
*   ;                                                              0562 01047000
RESVIT   DS    0H                                                  0563 01048000
*   GEN;                                                           0563 01049000
         LRA   4,RESRVCCW              LOAD REAL ADDR OF CCW            01050000
*   CALL IGFPSAD0;                  /* OBTAIN AUTHORIZATION  @G51BPLC*/ 01051000
         L     @15,@CV03195                                        0564 01052000
         BALR  @14,@15                                             0564 01053000
*   FLCCAW=REG4;                    /* SET CCW ADDR AND KEY OF ZERO     01054000
*                                                            @G51BPLC*/ 01055000
         ST    REG4,FLCCAW                                         0565 01056000
*   CALL IGFPSAE0;                  /* RELEASE AUTHORIZATION @G51BPLC*/ 01057000
         L     @15,@CV03196                                        0566 01058000
         BALR  @14,@15                                             0566 01059000
*   TIOCNTR=ZERO;                   /* INITIALIZE TIO COUNT          */ 01060000
         SLR   @12,@12                                             0567 01061000
         STH   @12,TIOCNTR(,REG8)                                  0567 01062000
*   REG4=ENT(ENTCNT,ONE:TWO)&ORMASK;/* SET DEV ADDR IN REG   @YM04703   01063000
*                                      FOR USE IN RESERVE            */ 01064000
         L     @12,RSVTBLPT(,REG8)                                 0568 01065000
         L     @11,ENTCNT(,@12)                                    0568 01066000
         SLA   @11,3                                               0568 01067000
         LA    REG4,4095                                           0568 01068000
         ALR   @12,@11                                             0568 01069000
         SLR   @11,@11                                             0568 01070000
         ICM   @11,3,ENT-8(@12)                                    0568 01071000
         NR    REG4,@11                                            0568 01072000
*RESVSIO:                                                          0569 01073000
*   ;                                                              0569 01074000
RESVSIO  DS    0H                                                  0570 01075000
*   GEN( SIO   0(4) );              /* ISSUE THE RESERVE             */ 01076000
          SIO   0(4)                                                    01077000
*   BC(4,CKSTAT1);                  /* CC=1 CHECK STATUS             */ 01078000
         BC    4,CKSTAT1                                           0571 01079000
*   BC(2,CHBSY);                    /* CC=2 GO ENABLE CHAN           */ 01080000
         BC    2,CHBSY                                             0572 01081000
*   BC(1,ROFFCK);                   /* CC=3 INOP-HARDWAIT CC=0 BRING    01082000
*                                      IN INT                        */ 01083000
         BC    1,ROFFCK                                            0573 01084000
*/***************************************************************  0574 01085000
**       THIS CODE IS ENTERED BY DEFAULT WHEN A CC0 RESPONSE IS    0574 01086000
**       MADE TO THE SIO FOR THE RESERVE. IT ATTEMPTS TO BRING     0574 01087000
**       IN THE INTERRUPT WITH A TIO LOOP. CC0 OR CC3 RESPONSE     0574 01088000
**       OR THE FAILURE OF THE DEVICE TO STORE STATUS BEFORE THE   0574 01089000
**       LOOP ENDS IS CONSIDERED AN ERROR AND AN 043 WAIT IS       0574 01090000
**       ENTERED. CC1 CAUSES A STATUS CHECKING ROUTINE TO GET      0574 01091000
**       CONTROL.                                                  0574 01092000
***************************************************************      */ 01093000
*                                                                  0574 01094000
*TESTIT:                                                           0574 01095000
*   ;                                                              0574 01096000
TESTIT   DS    0H                                                  0575 01097000
*   GEN( TIO   0(4) );              /* TIO DEVICE                    */ 01098000
          TIO   0(4)                                                    01099000
*   BC(9,HARDWAIT);                 /* CC=3 OR CC=0 HARDWAIT         */ 01100000
         BC    9,HARDWAIT                                          0576 01101000
*   BC(4,CKSTAT);                   /* CC=1 CHECK STATUS             */ 01102000
         BC    4,CKSTAT                                            0577 01103000
*/*************                                                    0578 01104000
**       TIO LOOP COUNT                                            0578 01105000
*************                                                        */ 01106000
*                                                                  0578 01107000
*TMLOOP:                                                           0578 01108000
*   TIOCNTR=TIOCNTR+ONE;            /* INCREMENT TIO CNT             */ 01109000
TMLOOP   LA    @12,1                                               0578 01110000
         AH    @12,TIOCNTR(,REG8)                                  0578 01111000
         STH   @12,TIOCNTR(,REG8)                                  0578 01112000
*   IF TIOCNTR<TIOMAX               /* HAS THE INTERRUPT FOR THE   0579 01113000
*                                      RESERVE TAKEN TOO LONG        */ 01114000
*     THEN                                                         0579 01115000
         CH    @12,@CH03022                                        0579 01116000
         BL    @RT00579                                            0579 01117000
*     GO TO TESTIT;                 /* NO - CONTINUE TO TRY TO BRING    01118000
*                                      IN INTERRUPT                  */ 01119000
*   ELSE                                                           0581 01120000
*     ;                             /* YES - FALL INTO HARD WAIT   0581 01121000
*                                      ROUTINE                       */ 01122000
*/***************************************************************  0582 01123000
**       THIS ROUTINE PUTS THE SYSTEM INTO A HARD 043 WAIT STATE   0582 01124000
**       DUE TO SOME FAILURE IN RESERVE PROCESSING FOR AN ACR      0582 01125000
**       I/O RESTART ENTRY.                                        0582 01126000
***************************************************************      */ 01127000
*                                                                  0582 01128000
*HARDWAIT:                                                         0582 01129000
*   ;                                                              0582 01130000
HARDWAIT DS    0H                                                  0583 01131000
*   RFY                                                            0583 01132000
*    (REG0) RSTD;                                                  0583 01133000
*   REG0=FORTY3;                    /* SET HARDWAIT CODE             */ 01134000
         LA    REG0,67                                             0584 01135000
*   CALL WAITRTN;                   /* GO TO WAIT RTN IFACE          */ 01136000
         BAL   @14,WAITRTN                                         0585 01137000
*   GO TO HARDWAIT;                 /* STAY IN HARDWAIT LOOP         */ 01138000
         B     HARDWAIT                                            0586 01139000
*   RFY                                                            0587 01140000
*    (REG0) UNRSTD;                                                0587 01141000
*                                                                  0588 01142000
*/***************************************************************  0588 01143000
**       THIS SECTION HANDLES STATUS TAKEN AS CC1 TO THE SIO FOR   0588 01144000
**       THE RESERVE CCW. IF CHANNEL ERRORS ARE PRESENT THE        0588 01145000
**       HARDWAIT ROUTINE IS ENTERED OTHERWISE THE RESERVE IS      0588 01146000
**       RETRIED.                                                  0588 01147000
***************************************************************      */ 01148000
*                                                                  0588 01149000
*CKSTAT1:                                                          0588 01150000
*   IF(STATUS&ENDSTAT)=ZERO         /* BAD STATUS - OTHER THAN CE DE    01151000
*                                      WLR UC UE                     */ 01152000
*     THEN                                                         0588 01153000
CKSTAT1  SLR   @12,@12                                             0588 01154000
         ICM   @12,3,ENDSTAT                                       0588 01155000
         SLR   @11,@11                                             0588 01156000
         ICM   @11,3,STATUS                                        0588 01157000
