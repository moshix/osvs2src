*                                                                  0749 01797000
         TITLE ' IECVRSTI - SIGNAL PROCESSOR SUBROUTINE'                01798000
*/***********************************************************@YM04440*/ 01799000
*/*     THIS SUBROUTINE ISSUES A SIGP INSTRUCTION TO THE     @Y30CQLF*/ 01800000
*/*     FAILING CPU ON AN ACR CALL. IF CRH IS BEIN ACTIVATED @Y30CQLF*/ 01801000
*/*     A CPU RESET WILL BE REQUESTED,OTHERWISE AN INITIAL   @Y30CQLF*/ 01802000
*/*     PROGRAM RESET WILL BE REQUESTED. A CC=3 ON THE       @Y30CQLF*/ 01803000
*/*     SIGP WILL CAUSE A WARNING MESSAGE TO BE ISSUED TO    @YM04440*/ 01804000
*/*     THE OPERATOR WARNING HIM NOT TO TOUCH THE DEAD CPU   @YM04440*/ 01805000
*/*     BECAUSE IT HAS NOT BEEN RESET. THE MESSAGE ALSO      @YM04440*/ 01806000
*/*     EXPLAINS THAT SINCE THE RESET FAILED SOME OF THE I/O @YM04440*/ 01807000
*/*     GEAR MAY NO LONGER BE ACCESSIBLE TO THE LIVE CPU.    @YM04440*/ 01808000
*/*     A CC=1,2 WILL RESULT IN RETRYING THE SIGP A FIXED    @YM04440*/ 01809000
*/*     NUMBER OF TIMES AND THEN ISSUEING THE SAME WARNING   @YM04440*/ 01810000
*/*     MESSAGE. A CC=0 INDICATES THAT THE SIGP WAS DECODED  @YM04440*/ 01811000
*/*     AND THE COMMAND RECOGNIZED BY THE FAILING CPU.       @Y30CQLF*/ 01812000
*/*     A CC=0 DOES NOT MEAN THAT THE SIGP HAS COMPLETED     @Y30CQLF*/ 01813000
*/*     SUCCESSFULLY, AND THIS IS THE MAIN REASON THE CC=3   @YM04440*/ 01814000
*/*     RESULTS IN A WARNING MESSAGE RATHER THAN A WAIT      @YM04440*/ 01815000
*/*     STATE, PLUS THE FACT THAT THE SYSTEM MAY BE ABLE TO  @YM04440*/ 01816000
*/*     CONTINUE.                                            @YM04440*/ 01817000
*/***********************************************************@YM04440*/ 01818000
*                                                                  0750 01819000
*   RFY                                                            0750 01820000
*    (REG0,                                                        0750 01821000
*     REG5,                                                        0750 01822000
*     REG6) RSTD;                   /*                       @YM04440*/ 01823000
*SIGPRTN:                           /*                       @YM04440*/ 01824000
*   REG0=SIGPCNT;                   /* LOAD RETRY COUNT      @YM04440*/ 01825000
SIGPRTN  LA    REG0,100                                            0751 01826000
*   DO WHILE REG0>ZERO;             /* SET UP RETRY LOGIC    @YM04440*/ 01827000
         B     @DE00752                                            0752 01828000
@DL00752 DS    0H                                                  0753 01829000
*     REG6=CPUID;                   /* GET CPUID OF FAILING CPU    0753 01830000
*                                                            @YM04440*/ 01831000
         SLR   REG6,REG6                                           0753 01832000
         IC    REG6,CPUID(,REG8)                                   0753 01833000
*     REG5=ZERO;                    /* ZERO STATUS REG       @YM04440*/ 01834000
         SLR   REG5,REG5                                           0754 01835000
*     IF RESETCPU=ON1               /* CPU RESET REQUEST     @Y30CQLF*/ 01836000
*       THEN                        /*                       @OZ06051*/ 01837000
         TM    RESETCPU(REG8),B'00000100'                          0755 01838000
         BNO   @RF00755                                            0755 01839000
*       DO;                         /*                       @OZ06051*/ 01840000
*         GENERATE CODE;                                           0757 01841000
    SIGP  @05,@06,SIGPSEN        DETERMINE STATE OF CPU        @OZ06051 01842000
    BC    2,SIGPST               CC=2 ISSUE STOP IF BUSY       @OZ06051 01843000
    BC    1,SIGPFAIL             CC=3 ISSUE FAIL MSG IF NOT OP @OZ06051 01844000
*                                CC=0 OR 1                              01845000
         N     @05,SIGPSCKS      CHECK FOR STOP OR CHECK STOP  @OZ06051 01846000
         LTR   @05,@05           STATE.                        @OZ06051 01847000
         BNZ   SIGPOK            ONE OR OTHER, YES RETURN      @OZ06051 01848000
SIGPST SIGP @05,@06,SIGPSTOP     OTHERWISE ISSUE STOP          @OZ06051 01849000
*       END;                                                       0758 01850000
*     ELSE                                                         0759 01851000
*       DO;                         /* TRY IPR RESET         @OZ06051*/ 01852000
         B     @RC00755                                            0759 01853000
@RF00755 DS    0H                                                  0760 01854000
*         GEN CODE(SIGP @05,@06,IPR);/* ISSUE IPR            @OZ06051*/ 01855000
         SIGP @05,@06,IPR                                               01856000
*         BC(EIGHT,SIGPOK);         /* CC=0,RETURN           @OZ06051*/ 01857000
         BC    8,SIGPOK                                            0761 01858000
*         BC(ONE,SIGPFAIL);         /* CC=3,SIGP FAILED      @OZ06051*/ 01859000
         BC    1,SIGPFAIL                                          0762 01860000
*       END;                        /*                       @OZ06051*/ 01861000
*     REG0=REG0-ONE;                /* CC=1 OR 2,RETRY SIGPCNT TIMES    01862000
*                                                            @YM04440*/ 01863000
@RC00755 BCTR  REG0,0                                              0764 01864000
*   END;                                                           0765 01865000
@DE00752 LTR   REG0,REG0                                           0765 01866000
         BP    @DL00752                                            0765 01867000
*   GEN(SIGP @05,@06,CPURESET);     /* CAN'T STOP THE CPU    @OZ06051   01868000
