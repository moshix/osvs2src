         BNZ   @RF00940                                            0940 02478000
*     PWAEDSRB=0;                   /* UNALLOCATE SRB        @G552PLM*/ 02479000
         L     @12,PWAPTR(,REG8)                                   0941 02480000
         MVI   PWAEDSRB(@12),X'00'                                 0941 02481000
*/***********************************************************@YM05487*/ 02482000
*/*     RETURN WORK AREAS TO STORAGE MANAGER                 @YM05487*/ 02483000
*/***********************************************************@YM05487*/ 02484000
*                                                                  0942 02485000
*   RFY                                                            0942 02486000
*    (REG11,                                                       0942 02487000
*     REG13) RSTD;                                                 0942 02488000
@RF00940 DS    0H                                                  0943 02489000
*   REG5=REG14;                     /* SAVE RETURN REGISTER  @YM05487*/ 02490000
         LR    REG5,REG14                                          0943 02491000
*   REG4=REG1;                      /* SAVE SDWA POINTER     @YM05487*/ 02492000
