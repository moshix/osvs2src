         GETMAIN ,MF=(E,(1))            GET PAGE FOR LCBS        Y05330 00328000
         MODESET EXTKEY=SUPR            BACK TO KEY 0          @G36XRNB 00329000
         NI    AVTGETMN+8(RAVT),X'FF'-PAGEREQ RESET PARM LIST    Y05330 00330000
         LTR   RJ,RJ                    GOOD RETURN CODE         Y05330 00331000
         BNZ   ABEND7                   NO, EXIT                 Y05330 00332000
         LM    RE,RF,AVTGETMN(RAVT)     LENGTH AND ADDRESS       Y05330 00333000
         L     RF,ZERO(RF)              ADDRESS OF GOTTEN CORE   Y05330 00334000
         MVC   ZERO(FOUR,RF),AVTFZERO(RAVT) SET END OF CHAIN     Y05330 00335000
         LR    RLCB,RE                  SAVE LENGTH              Y05330 00336000
         SR    RLCB,RUCB                FIND CORE LEFT AFTER LCB Y05330 00337000
GETLCB2  EQU   *                                                 Y05330 00338000
         BNZ   GETLCB3                  REQUEST NOT EQUAL AVAIL. Y05330 00339000
         MVC   ZERO(FOUR,RJ),ZERO(RF)   REMOVE PAGE FROM CHAIN   Y05330 00340000
         B     GETLCB4                  CONTINUE                 Y05330 00341000
SIZELCB  EQU   X'C8'                    SIZE OF 3705 LCB       @Y17XANL 00342000
CSA      EQU   241                      CSA SUBPOOL            @G36XRNB 00343000
PAGEREQ  EQU   X'10'                    FLAG FOR PAGE GETMAIN    Y05330 00344000
SYSVS1   EQU   X'20'                    FLAG FOR VS/1 SYSTEM     Y05330 00345000
PAGESZ   EQU   2048                     VS/1 PAGE SIZE           Y05330 00346000
CHAINLCB EQU   60                       OFFSET TO CHAIN OF CORE  Y05330 00347000
*                                        FOR LCBS                Y05330 00348000
ZERO     EQU   0                        NUMERIC ZERO           @Y17XANL 00349000
AD       EQU   7                        ICM/STCM MASK            Y05330 00350000
GETLCB3  EQU   *                                                 Y05330 00351000
         ST    RLCB,FOUR(,RF)           UPDATE AVAILABLE CORE    Y05330 00352000
GETLCB4  EQU   *                                                 Y05330 00353000
