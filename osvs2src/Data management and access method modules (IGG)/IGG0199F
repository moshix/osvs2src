*        RPARC = RPAR;                  /* START AT TOP OF LIST     */  00017000
         LR    RPARC,RPAR                                          0049 00018000
*        WTGTBLEN = 0;                  /* INIT DCB COUNT    @YA02120*/ 00019000
*                                                                  0050 00020000
