*        CISYNRC = '08'X;               /* RESTORE RETURN CODE      */  00211000
         MVI   CISYNRC(RCICB),X'08'                                0134 00212000
*        /***********************************************************/  00213000
*        GEN   (EJECT);                 /* SKIP TO A NEW PAGE       */  00214000
         EJECT                                                          00215000
*                                                                  0136 00216000
*EROPT:  /***********************************************************/  00217000
*        /*    TEST DCBEROPT SPECIFIED                              */  00218000
*        /***********************************************************/  00219000
*                                                                  0136 00220000
