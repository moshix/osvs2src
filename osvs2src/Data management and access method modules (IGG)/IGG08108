*   IF DCBPTR^=CONST000 THEN        /* TEST FOR VALID DCB            */ 00120000
         LTR   DCBPTR,DCBPTR                                       0160 00121000
         BZ    @RF00160                                            0160 00122000
*     DO;                                                          0161 00123000
*       IF DCBDEVT=D3886 THEN       /* TEST FOR 3886 - LOAD FMT RD   */ 00124000
         CLI   DCBDEVT+16(DCBPTR),X'57'                            0162 00125000
         BNE   @RF00162                                            0162 00126000
*         DO;                                                      0163 00127000
*                                                                  0163 00128000
*/********************************************************************/ 00129000
*/*                                                                  */ 00130000
*/*  FUNCTION-THE 3886 SECTION OF SETDEV SEARCHES THE FORMAT         */ 00131000
*/*           RECORD TABLE FOR A DUPLICATE ENTRY OR AN AVAILABLE     */ 00132000
*/*           ENTRY. THE ID IS APPENDED TO 'FRLG' TO LOAD THE FORMAT */ 00133000
*/*           RECORD. THE FORMAT RECORD IS LOADED FROM SYS1.IMAGELIB */ 00134000
*/*           AND PASSED TO THE DEVICE. CONTROL IS THEN RETURNED TO  */ 00135000
*/*           THE USER.                                              */ 00136000
*/*                                                                  */ 00137000
*/*  SEGMENTS-DEVT3886,T3886PMS,FRTABMG,LOADFRID                     */ 00138000
*/*                                                                  */ 00139000
*/*  INPUT-SETDEV PARAMETER LIST,POINTER TO UCB                      */ 00140000
*/*                                                                  */ 00141000
*/*  OUTPUT-3886 CONTROL UNIT LOADED WITH FORMAT RECORD              */ 00142000
*/*         UPDATED FORMAT RECORD TABLE                              */ 00143000
*/*                                                                  */ 00144000
*/*  EXTERNAL REFERENCES-LOAD -INNER PROCEDURE TO THIS MODULE,LOADS  */ 00145000
*/*           THE FORMAT RECORD TO MAIN STORAGE FROM SYS1.IMAGELIB.  */ 00146000
*/*                      IORTN -INNER PROCEDURE TO THIS MODULE,LOADS */ 00147000
*/*           THE FORMAT RECORD TO THE 3886                          */ 00148000
*/*                      CLEANUP -INNER BACK TO CALLER               */ 00149000
*/*                                                                  */ 00150000
*/*  EXITS,NORMAL -RETURN TO CALLER WITH RETURN CODE X'00' IN REG 15 */ 00151000
*/*               -RETURN TO EOF ADDRESS IN CALLERS DCB FOR UNIT EXC */ 00152000
*/*                                                                  */ 00153000
*/*  EXITS,ERROR  - RETURN TO CALLER WITH RETURN CODE IN REG 15      */ 00154000
*/*                 X'04' MEMBER NOT FOUND IN SYS1.IMAGELIB          */ 00155000
*/*                 X'08' UNABLE TO LOAD MEMBER FROM SYS1.IMAGELIB   */ 00156000
*/*                 X'0C' PERMANENT I/O ERROR LOADING DEVICE         */ 00157000
*/*                 X'10' INVALID PARAMETER LIST                     */ 00158000
*/*                                                                  */ 00159000
*/*  TABLES/WORKAREAS-SETDEV PARAMETER LIST                          */ 00160000
*/*                   FORMAT RECORD TABLE                            */ 00161000
*/*                   WORK AREA FOR THIS MODULE                      */ 00162000
*/*                                                                  */ 00163000
*/*  CONTROL BLOCKS-DCB,IOB,ECB,TIOT,DEB,TCB,RB,UCB                  */ 00164000
*/*                                                                  */ 00165000
*/*  NOTES-FOR FURTHER INFORMATION SEE PLM SY21-0028                 */ 00166000
*/*  NOTES- FOR FURTHER INFORMATION SEE PLM SY21-5162                */ 00167000
*/*  NOTES- FOR FURTHER INFORMATION SEE PLM SY21-5162                */ 00168000
*/*                                                                  */ 00169000
*/********************************************************************/ 00170000
*                                                                  0164 00171000
*           /*********************************************************/ 00172000
*           /*                                                       */ 00173000
*           /* ******** TEST FOR VALID 3886 PARAMETER LIST AND DCB   */ 00174000
*           /*                                                       */ 00175000
*           /*********************************************************/ 00176000
*                                                                  0164 00177000
*           RFRT=DCBFRTBA;          /* INIT PTR TO FR TAB    @YM30699*/ 00178000
         L     RFRT,DCBFRTBA+4(,DCBPTR)                            0164 00179000
*           IF FRID=NOTSPCFD THEN   /* TEST IF FRID KEEP IT          */ 00180000
         CLC   FRID(4,RPAR),@CB00189                               0165 00181000
         BNE   @RF00165                                            0165 00182000
*             DO;                                                  0166 00183000
*               RTNCODE=RCIPLT;     /* SET RETURN CODE               */ 00184000
         LA    RTNCODE,16                                          0167 00185000
*               NOMSGFLG=ON;        /* NOT SPECIFIED FOR OPEN        */ 00186000
         MVI   NOMSGFLG(RCORE),X'01'                               0168 00187000
*             END;                                                 0169 00188000
*           ELSE                    /* ELSE STATEMENT                */ 00189000
*             IF FRID=BLANKS THEN   /* FRID NOT ON L OR E FORM       */ 00190000
         B     @RC00165                                            0170 00191000
@RF00165 CLC   FRID(4,RPAR),@CC00195                               0170 00192000
         BNE   @RF00170                                            0170 00193000
*               RTNCODE=RCIPLT;     /* FRID NOT SPECIFIED            */ 00194000
         LA    RTNCODE,16                                          0171 00195000
*             ELSE                  /* FRID IS SPECIFIED             */ 00196000
*               IF FRIDTAB^=CONST000 THEN/* TEST IF OPEN IS NOT    0172 00197000
*                                      CALLER                        */ 00198000
         B     @RC00170                                            0172 00199000
@RF00170 L     @14,FRIDTAB(,RFRT)                                  0172 00200000
         LTR   @14,@14                                             0172 00201000
         BZ    @RF00172                                            0172 00202000
*                 IF DCBOFOPN^='1'B|/* TEST IF DCB OPENED            */ 00203000
*                     DCBIFLGS=PERMERR THEN/* OR DCB HAS PERM ERR IND*/ 00204000
         TM    DCBOFOPN+40(DCBPTR),B'00010000'                     0173 00205000
         BNO   @RT00173                                            0173 00206000
         CLI   DCBIFLGS+40(DCBPTR),B'11000000'                     0173 00207000
         BNE   @RF00173                                            0173 00208000
@RT00173 DS    0H                                                  0174 00209000
*                   RTNCODE=RCIPLT; /* INVALID RC SET                */ 00210000
         LA    RTNCODE,16                                          0174 00211000
*                 ELSE                                             0175 00212000
*                   ;                                              0175 00213000
@RF00173 DS    0H                                                  0176 00214000
*               ELSE                /* OPEN IS THE CALLER            */ 00215000
*                 NOMSGFLG=ON;      /* IF OPEN CALLER SET NO MSG F   */ 00216000
         B     @RC00172                                            0176 00217000
@RF00172 MVI   NOMSGFLG(RCORE),X'01'                               0176 00218000
*           IF RTNCODE=NOERROR THEN /* TEST IF PARM LIST WAS VALID   */ 00219000
@RC00172 DS    0H                                                  0177 00220000
@RC00170 DS    0H                                                  0177 00221000
@RC00165 LTR   RTNCODE,RTNCODE                                     0177 00222000
         BNZ   @RF00177                                            0177 00223000
*             DO;                                                  0178 00224000
*                                                                  0178 00225000
*               /*****************************************************/ 00226000
*               /*                                                   */ 00227000
*               /* ******** FORMAT RECORD TABLE MANAGEMENT ROUTINE   */ 00228000
*               /* (1) ADDRESS'S ARE FILLED IN THE TABLE BY OPEN. (2)*/ 00229000
*               /* FRID ARE FILLED IN BY THIS ROUTINE (3) THERE CAN  */ 00230000
*               /* BE A MAXIMUM OF 8 ENTRIES IN THE TABLE (4) IF THE */ 00231000
*               /* FRID SPECIFIED IN THE SETDEV IS ALREADY IN THE    */ 00232000
*               /* TABLE THE FR IS NOT LOAD FROM SYS1.IMAGELIB. (5)  */ 00233000
*               /* IF THE FRID IS NOT IN THE TABLE THEN THE FR IS    */ 00234000
*               /* LOADED FROM SYS1.IMAGELIB. (6) IF THE FR TABLE IS */ 00235000
*               /* FULL THE TOP FR ENTRY WILL BE REPLACED            */ 00236000
*               /*                                                   */ 00237000
*               /*****************************************************/ 00238000
*                                                                  0179 00239000
*FRTABMG:                           /* LABEL TO IDENTIFY TAB MG      */ 00240000
*               RFRL=CONST000;      /* INIT FMT RCD PTR TO ZERO      */ 00241000
FRTABMG  SLR   RFRL,RFRL                                           0179 00242000
*               DO X=ONE TO EIGHT WHILE RFRL=CONST000;/* LOOP THRU 0180 00243000
*                                      FMT RCD TABLE                 */ 00244000
*                                                                  0180 00245000
         LA    @14,1                                               0180 00246000
         ST    @14,X(,RCORE)                                       0180 00247000
@DL00180 LTR   RFRL,RFRL                                           0180 00248000
         BNZ   @DC00180                                            0180 00249000
*                 /***************************************************/ 00250000
*                 /*                                                 */ 00251000
*                 /* FIND THE NXT AVAILABLE ENTRY OR ENTRY TO BE USED*/ 00252000
*                 /*                                                 */ 00253000
*                 /***************************************************/ 00254000
*                                                                  0181 00255000
*                 IF FRIDTAB=CONST000 THEN/* TST FOR NXT AVAILABLE 0181 00256000
*                                      ENTRY                         */ 00257000
         L     @14,FRIDTAB(,RFRT)                                  0181 00258000
         LTR   @14,@14                                             0181 00259000
         BNZ   @RF00181                                            0181 00260000
*                   RFRL=FRADDR;    /* YES SET PTR TO USE THIS ENTRY */ 00261000
         L     RFRL,FRADDR(,RFRT)                                  0182 00262000
*                 ELSE                                             0183 00263000
*                   IF FRID=FRIDTAB THEN/* TST IF THIS ENTRY TO BE 0183 00264000
*                                      USED                          */ 00265000
         B     @RC00181                                            0183 00266000
@RF00181 CLC   FRID(4,RPAR),FRIDTAB(RFRT)                          0183 00267000
         BNE   @RF00183                                            0183 00268000
*                     RFRL=FRADDR;  /* SET PTR TO ENTRY TO BE USED   */ 00269000
         L     RFRL,FRADDR(,RFRT)                                  0184 00270000
*                   ELSE            /* ENTRY IN TABLE ALREADY TAKEN  */ 00271000
*                     IF X^=EIGHT THEN/* TST IF END OF TABLE REACHED */ 00272000
         B     @RC00183                                            0185 00273000
@RF00183 CLC   X(4,RCORE),@CF00042                                 0185 00274000
         BE    @RF00185                                            0185 00275000
*                       RFRT=RFRT+EIGHT;/* NO,INCR PTR TO NEXT TABLE    00276000
*                                      ENTRY                         */ 00277000
         LA    RFRT,8(,RFRT)                                       0186 00278000
*                     ELSE          /* YES,END OF TABLE REACHED      */ 00279000
*                       DO;                                        0187 00280000
         B     @RC00185                                            0187 00281000
@RF00185 DS    0H                                                  0188 00282000
*                         RWK2=DCBFRTBA->FRADDR;/* SAVE ADDR FOR NXT    00283000
*                                      FORMAT RCD LOADED@YM30699     */ 00284000
         L     @14,DCBFRTBA+4(,DCBPTR)                             0188 00285000
         L     RWK2,FRADDR(,@14)                                   0188 00286000
*                         FRTABLE(1:56)=FRTABSB;/* MOVE LAST 7     0189 00287000
*                                                            @ZA04490*/ 00288000
         L     @14,DCBFRTBA+4(,DCBPTR)                             0189 00289000
         L     @12,DCBFRTBA+4(,DCBPTR)                             0189 00290000
         MVC   FRTABLE(56,@14),FRTABSB+8(@12)                      0189 00291000
*                         FRTABLE(57:64)='';/* ENTRIES IN TBLE     0190 00292000
*                                                            @ZA04490   00293000
*                                      UP TO 1ST 7 POSITIONS @ZA04490*/ 00294000
         L     @14,DCBFRTBA+4(,DCBPTR)                             0190 00295000
         MVI   FRTABLE+56(@14),C' '                                0190 00296000
         MVC   FRTABLE+57(7,@14),FRTABLE+56(@14)                   0190 00297000
*                         FRADDR=RWK2;/* RESTORE ADDR FOR FMT RCD    */ 00298000
         ST    RWK2,FRADDR(,RFRT)                                  0191 00299000
*                         FRIDTAB=CONST000;/* ZERO OUT FRID FIELD    */ 00300000
         SLR   @14,@14                                             0192 00301000
         ST    @14,FRIDTAB(,RFRT)                                  0192 00302000
*                         RFRL=FRADDR;/* SET PTR TO USE THIS ENTRY   */ 00303000
         L     RFRL,FRADDR(,RFRT)                                  0193 00304000
*                       END;                                       0194 00305000
*               END;                                               0195 00306000
@RC00185 DS    0H                                                  0195 00307000
@RC00183 DS    0H                                                  0195 00308000
@RC00181 LA    @14,1                                               0195 00309000
         AL    @14,X(,RCORE)                                       0195 00310000
         ST    @14,X(,RCORE)                                       0195 00311000
         C     @14,@CF00042                                        0195 00312000
         BNH   @DL00180                                            0195 00313000
@DC00180 DS    0H                                                  0196 00314000
*               IF FRIDTAB=CONST000 THEN/* TST IF NEED TO LOAD FR    */ 00315000
         L     @14,FRIDTAB(,RFRT)                                  0196 00316000
         LTR   @14,@14                                             0196 00317000
         BNZ   @RF00196                                            0196 00318000
*                 DO;                                              0197 00319000
*                   PREFIX=FRLG;    /* SET PREFIX FOR FR NAME        */ 00320000
         MVC   PREFIX(4,RCORE),@CC00197                            0198 00321000
*                   IDFR=FRID;      /* COMPLETE FR NAME              */ 00322000
         MVC   IDFR(4,RCORE),FRID(RPAR)                            0199 00323000
*                   NAMEPTR=ADDR(MODNAME);/* SET PTR TO MEMBER TO LD */ 00324000
         LA    NAMEPTR,MODNAME(,RCORE)                             0200 00325000
*                   CALL LOAD;      /* LOAD FR INTO CORE             */ 00326000
         BAL   @14,LOAD                                            0201 00327000
*                   IF RTNCODE=RCSUCC THEN/* IF NO ERRORS CONTINUE   */ 00328000
         LTR   RTNCODE,RTNCODE                                     0202 00329000
         BNZ   @RF00202                                            0202 00330000
*                     DO;                                          0203 00331000
*                       FRIDTAB=FRID;/* MOVE FRID INTO TABLE         */ 00332000
         MVC   FRIDTAB(4,RFRT),FRID(RPAR)                          0204 00333000
*                       RFY                                        0205 00334000
*                         UCBPTR RSTD;/*                     @ZA24702*/ 00335000
*                       UCBPTR=DEBSUCBA;/* GET UCB PTR       @ZA24702*/ 00336000
         L     @14,DEBPTR(,RCORE)                                  0206 00337000
         L     UCBPTR,DEBSUCBA(,@14)                               0206 00338000
*                       DO;         /* MODESET EXTKEY(SUPR)          */ 00339000
*                         SPKA(0);  /* SET PSW KEY                   */ 00340000
         SLR   @14,@14                                             0208 00341000
         SPKA  0(@14)                                              0208 00342000
*                       END;        /* MODESET EXTKEY(SUPR) GET    0209 00343000
*                                      SUPERVISOR PROTECT KEY      0209 00344000
*                                                            @YM02506*/ 00345000
*                       UCBXTADR=FRID;/* PUT FRID IN UCB FOR Y02870  */ 00346000
         L     @14,FRID(,RPAR)                                     0210 00347000
         ST    @14,UCBXTADR(,UCBPTR)                               0210 00348000
*                       DO;         /* MODESET KEYADDR(CALLKEY)    0211 00349000
*                                      WORKREG(REG1)                 */ 00350000
*                         REG1=CALLKEY;/* GET KEY FORM SAVE LOCATION */ 00351000
         SLR   REG1,REG1                                           0212 00352000
         IC    REG1,CALLKEY(,RWTG)                                 0212 00353000
*                         SPKA(REG1);/* SET PSW KEY                  */ 00354000
         SPKA  0(REG1)                                             0213 00355000
*                       END;        /* MODESET KEYADDR(CALLKEY)    0214 00356000
*                                      WORKREG(REG1)         @ZA24698*/ 00357000
*                       RFY                                        0215 00358000
*                         UCBPTR UNRSTD;/*                   @ZA24702*/ 00359000
*                       BYTECNT=MAXRFLTH;/* MAX NUMB BYTES TO MV IN  */ 00360000
         LA    BYTECNT,224                                         0216 00361000
*                       RESPECIFY                                  0217 00362000
*                         NEWFRLTH RSTD;                           0217 00363000
*                       NEWFRLTH=LNTHFR+FRPRLNTH;/* SET LENGTH OF FR    00364000
*                                      LOADED                        */ 00365000
         LA    NEWFRLTH,2                                          0218 00366000
         AH    NEWFRLTH,LNTHFR(,LDMODPTR)                          0218 00367000
*                       GEN(MVCL   8,10      MOVE FR LOADED INTO TABLE) 00368000
*                           ;                                      0219 00369000
*                                                                  0219 00370000
         MVCL   8,10      MOVE FR LOADED INTO TABLE                     00371000
*                       /*********************************************/ 00372000
*                       /*                                           */ 00373000
*                       /* DELETE FORMAT RECORD JUST LOADED          */ 00374000
*                       /*                                           */ 00375000
*                       /*********************************************/ 00376000
*                                                                  0220 00377000
*                       REG1=ADDR(BLDNAME);/* GET ADDR OF MOD NAME   */ 00378000
         LA    REG1,BLDNAME(,RCORE)                                0220 00379000
*                       GEN(DELETE   EPLOC=(1)    DELETE FR LOADED);    00380000
         DELETE   EPLOC=(1)    DELETE FR LOADED                         00381000
*                       RESPECIFY                                  0222 00382000
*                         NEWFRLTH UNRSTD;                         0222 00383000
*                       RFRL=FRADDR;/* REINIT PTR TO FR AFTER MOVE   */ 00384000
         L     RFRL,FRADDR(,RFRT)                                  0223 00385000
*                     END;                                         0224 00386000
*                 END;                                             0225 00387000
@RF00202 DS    0H                                                  0226 00388000
*                                                                  0226 00389000
*               /*****************************************************/ 00390000
*               /*                                                   */ 00391000
*               /* ******** SEQUENCE TO LOAD THE FORMAT RECORD INTO  */ 00392000
*               /* THE 3886 (1) BUILD IOB IN WORKAREA (2) BUILD LOAD */ 00393000
*               /* FORMAT CCW IN WORKAREA (3) ISSUE COMMAND          */ 00394000
*               /*                                                   */ 00395000
*               /*****************************************************/ 00396000
*                                                                  0226 00397000
*LOADFRID:                          /* LABEL FOR LOADFRID     SEGMENT*/ 00398000
*               IF RTNCODE=RCSUCC THEN/* TEST IF OK TO CCNTINUE      */ 00399000
@RF00196 DS    0H                                                  0226 00400000
LOADFRID LTR   RTNCODE,RTNCODE                                     0226 00401000
         BNZ   @RF00226                                            0226 00402000
*                 DO;                                              0227 00403000
*                   IOBSTPTR=ADDR(IOBBODY);                        0228 00404000
