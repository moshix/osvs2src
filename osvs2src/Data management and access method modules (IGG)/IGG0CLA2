*   /* FOLLOWED BY 1 OR MORE 1 BYTE ENTRIES. EACH ENTRY REPRESENTS AN*/ 01623000
*   /* INCREMENT OVER THE TOTAL VALUE OF ALL PREVIOUS ENTRIES. EACH  */ 01624000
*   /* ENTRY WITH A VALUE LESS THAN 201 REPRESENTS A NEW GENERATION  */ 01625000
*   /* LEVEL WHOSE VALUE EQUALS THE TOTAL TO THAT POINT OF ALL       */ 01626000
*   /* INCREMENTS. AN ENTRY WITH A VALUE OVER 200 REPRESENTS AN      */ 01627000
*   /* INCREMENT OF 200 TIMES THE EXCESS OF THE ENTRY VALUE OVER 200.*/ 01628000
*   /* NOTE THAT ENTRIES WITH VALUES OVER 200 DO NOT REPRESENT NEW   */ 01629000
*   /* GENERATION LEVELS. AN ENTRY OF VALUE 255 MARKS THE END OF THE */ 01630000
*   /* ARRAY.                                                        */ 01631000
*   /*                                                               */ 01632000
*   /*****************************************************************/ 01633000
*                                                                  0498 01634000
*   IF CCACD1=RCS&CTGFLNG(1)^=NULL&CTGFLPT(1)^=NULL THEN           0498 01635000
         CLI   CCACD1(R11),0                                       0498 01636000
         BNE   @RF00498                                            0498 01637000
         SLR   @05,@05                                             0498 01638000
         C     @05,CTGFLNG(,@10)                                   0498 01639000
         BE    @RF00498                                            0498 01640000
         L     @10,CTGFLPT(,@10)                                   0498 01641000
         CR    @10,@05                                             0498 01642000
         BE    @RF00498                                            0498 01643000
*     DO;                           /* IF EXTRACT OK ...             */ 01644000
*       GLIMIT=GENLIM;              /* SAVE GDG LIMIT                */ 01645000
*                                                                  0500 01646000
         L     @05,CCAR5REC(,R11)                                  0500 01647000
         SLR   @15,@15                                             0500 01648000
         IC    @15,GENLIM(,@10)                                    0500 01649000
         ST    @15,GLIMIT(,@05)                                    0500 01650000
*       /*************************************************************/ 01651000
*       /*                                                           */ 01652000
*       /* BUILD TABLE OF GENERATION LEVELS FOR THIS GROUP.          */ 01653000
*       /*                                                           */ 01654000
*       /*************************************************************/ 01655000
*                                                                  0501 01656000
*       GLEV=N;                     /* CURRENT LEVEL COUNTER         */ 01657000
         SLR   GLEV,GLEV                                           0501 01658000
*       GX=ONE;                     /* TABLE INDEX                   */ 01659000
         LA    @10,1                                               0502 01660000
         LR    GX,@10                                              0502 01661000
*       GCNT=-ONE;                  /* WILL CONTAIN NO. OF LEVELS    */ 01662000
         L     @05,CCAR3REC(,R11)                                  0503 01663000
         MVC   GCNT(2,@05),@CH03486                                0503 01664000
*       DO GWK=1 BY 1 WHILE GCNT<0; /* STEP THRU EXTRACTED VALUES    */ 01665000
         LR    GWK,@10                                             0504 01666000
         B     @DE00504                                            0504 01667000
@DL00504 DS    0H                                                  0505 01668000
*         GVAL=GENLVL(GWK);         /* GET NEXT VALUE                */ 01669000
         L     @10,CCAR5REC(,R11)                                  0505 01670000
         L     @01,CCAFLPT(,R11)                                   0505 01671000
         L     @01,CTGFLPT(,@01)                                   0505 01672000
         SLR   @00,@00                                             0505 01673000
         IC    @00,GENLVL-1(GWK,@01)                               0505 01674000
         ST    @00,GVAL(,@10)                                      0505 01675000
*         IF GVAL=GLEND THEN        /* IF END OF VALUES              */ 01676000
         CH    @00,@CH03340                                        0506 01677000
         BNE   @RF00506                                            0506 01678000
*           GCNT=GX-ONE;            /* SET COUNT OF NO. OF LEVELS    */ 01679000
         L     @10,CCAR3REC(,R11)                                  0507 01680000
         LR    @15,GX                                              0507 01681000
         BCTR  @15,0                                               0507 01682000
         STH   @15,GCNT(,@10)                                      0507 01683000
*         ELSE                                                     0508 01684000
*           IF GVAL>GLBLOCK THEN    /* DOES TABLE VALUE REPRESENT 1     01685000
*                                      OR MORE LARGE BLOCKS OF     0508 01686000
*                                      NUMBERS                       */ 01687000
         B     @RC00506                                            0508 01688000
@RF00506 LA    @10,200                                             0508 01689000
         L     @01,CCAR5REC(,R11)                                  0508 01690000
         L     @15,GVAL(,@01)                                      0508 01691000
         CR    @15,@10                                             0508 01692000
         BNH   @RF00508                                            0508 01693000
*             GLEV=GLEV+(GVAL-GLBLOCK)*GLBLOCK;/* YES, UPDATE LEVEL  */ 01694000
         SLR   @15,@10                                             0509 01695000
         MR    @14,@10                                             0509 01696000
         ALR   GLEV,@15                                            0509 01697000
*           ELSE                    /* NO, TABLE VALUE SIMPLY        */ 01698000
*             DO;                   /* REPRESENTS INCREMENT          */ 01699000
         B     @RC00508                                            0510 01700000
@RF00508 DS    0H                                                  0511 01701000
*               GLEV=GLEV+GVAL;     /* UPDATE LEVEL                  */ 01702000
         L     @10,CCAR5REC(,R11)                                  0511 01703000
         AL    GLEV,GVAL(,@10)                                     0511 01704000
*               GTAB(GX)=GLEV;      /* ADD ENTRY TO TABLE            */ 01705000
         LR    @10,GX                                              0512 01706000
         ALR   @10,@10                                             0512 01707000
         L     @01,CCAR3REC(,R11)                                  0512 01708000
         STH   GLEV,GTAB-2(@10,@01)                                0512 01709000
*               GX=GX+ONE;          /* INCREMENT TABLE INDEX         */ 01710000
         AH    GX,@CH00037                                         0513 01711000
*             END;                                                 0514 01712000
*       END;                                                       0515 01713000
@RC00508 DS    0H                                                  0515 01714000
@RC00506 AH    GWK,@CH00037                                        0515 01715000
@DE00504 L     @10,CCAR3REC(,R11)                                  0515 01716000
         ICM   @15,3,GCNT(@10)                                     0515 01717000
         BM    @DL00504                                            0515 01718000
*     END;                                                         0516 01719000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 01720000
@RF00498 BAL   @14,EXIT                                            0517 01721000
*   END;                            /* END OF INNER PROCEDURE        */ 01722000
@EL00009 DS    0H                                                  0518 01723000
@EF00009 DS    0H                                                  0518 01724000
@ER00009 BR    @14                                                 0518 01725000
@PB00009 DS    0H                                                  0519 01726000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 01727000
         EJECT                                                          01728000
*                                                                  0520 01729000
*/********************************************************************/ 01730000
*/*                                                                  */ 01731000
*/*         RETURN-TO-CALLER  PROCEDURE-EXITING  SUBFUNCTION         */ 01732000
