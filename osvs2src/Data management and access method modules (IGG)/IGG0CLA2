*   /*                                                               */ 00962000
*   /*****************************************************************/ 00963000
*                                                                  0361 00964000
*   CCAFLPT=CTGFLCHN;               /* SET BASE TO NEXT FPL          */ 00965000
         ST    @05,CCAFLPT(,R11)                                   0361 00966000
*   CTGFLDNO=GLFLDNO;               /* SET NO. OF FIELDS             */ 00967000
         MVI   CTGFLDNO(@05),X'01'                                 0362 00968000
*   CTGFLDNM=ADDR(GGNAME);          /* WILL COMPARE 'GENLEVEL'       */ 00969000
         LA    @10,@CC03396                                        0363 00970000
