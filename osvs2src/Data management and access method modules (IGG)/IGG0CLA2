         OI    CTGNGDSN(@05),B'00010000'                           0337 00842000
*           GV=ZERO;                /* NULL VERSION          @YM06582*/ 00843000
         ST    @10,GV(,@07)                                        0338 00844000
*         END;                      /*                       @YM06582*/ 00845000
*       ELSE                        /* NEGATIVE REL NO       @YM06582*/ 00846000
*         IF GG<GTAB(GX) THEN                                      0340 00847000
