         TM    ACBSCRA(@10),B'00001000'                            0503 01274000
         BNO   @RF00503                                            0503 01275000
*     DO;                           /*                       @Y30SSSB*/ 01276000
*       CCARAEOV=ON;                /* INDICATE CRA EOV      @Y30SSSB*/ 01277000
         OI    CCARAEOV(R11),B'01000000'                           0505 01278000
*       CCARAACB=CCAACB;            /* SAVE CRA ACB ADDR     @Y30SSSB*/ 01279000
         ST    @10,CCARAACB(,R11)                                  0506 01280000
*       CCAACB=CAXACB;              /* CAT ACB ADR FROM CRA CAX      */ 01281000
         L     @10,CAXACB(,R8)                                     0507 01282000
         ST    @10,CCAACB(,R11)                                    0507 01283000
*       R8=ACBUAPTR;                /* PT TO CAT CAXWA       @VS43204*/ 01284000
         L     R8,ACBUAPTR(,@10)                                   0508 01285000
*     END;                          /*                       @Y30SSSB*/ 01286000
*   ELSE                                                           0510 01287000
*     IF ACBUCRA=ON THEN            /* USER CRA OPEN REQUEST         */ 01288000
         B     @RC00503                                            0510 01289000
@RF00503 L     @10,CCAACB(,R11)                                    0510 01290000
         TM    ACBUCRA(@10),B'00000100'                            0510 01291000
         BNO   @RF00510                                            0510 01292000
*       DO;                         /*                       @Y30SSSB*/ 01293000
