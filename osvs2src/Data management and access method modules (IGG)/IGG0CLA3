*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 01937000
         STM 12,14,12(13)                                               01938000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA SEARCH   01939000
*                                      FOR RPL                       */ 01940000
         LA 13,12(13)                                                   01941000
*   RFY                                                            0706 01942000
*    (R8) RSTD;                     /*                       @Z40WSPJ*/ 01943000
*   DO CCARPLX=1 TO CAXRPLCT WHILE(CCARPLK=0);/* SEARCH FOR ONE FREE    01944000
*                                      RPL                           */ 01945000
         LA    @10,1                                               0707 01946000
         B     @DE00707                                            0707 01947000
@DL00707 CLI   CCARPLK(R11),0                                      0707 01948000
         BNE   @DC00707                                            0707 01949000
*     CCARPL1=ADDR(RPLPOOL(CCARPLX));/*                      @Z40WSPJ   01950000
*                                      POINT TO RPL          @Z40WSPJ*/ 01951000
