*   DO WHILE(CCAF1LPS=OFF);         /* LOOK FOR CATLGS PCCB          */ 00178000
         B     @DE00244                                            0244 00179000
@DL00244 DS    0H                                                  0245 00180000
*Z008GPCC:                                                         0245 00181000
*     CALL IGGPGPCC;                /* GET 1ST/NXT PCCB              */ 00182000
Z008GPCC BAL   @14,IGGPGPCC                                        0245 00183000
*     IF CCANPCCB=N|                /* NO MORE PCCBS                 */ 00184000
*         (PCCACTIV=ON&             /* PCCB IS ACTIVE                */ 00185000
*         (SCHQN=PCCDSNAM|          /* CATLG NM MATCH FND            */ 00186000
*         SCHQN=PCCTGCON)) THEN     /* CATLG AL NM MATCH FND         */ 00187000
