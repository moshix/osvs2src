         BNZ   @RF00233                                            0233 00142000
         TM    CCAF2CCT(R11),B'00100000'                           0233 00143000
         BNZ   @RF00233                                            0233 00144000
*       CCAQLEN=GDGSINGL;           /* IND 1 LVL GDG NAME    @YM06556*/ 00145000
         MVI   CCAQLEN(R11),X'23'                                  0234 00146000
*     ELSE                          /* NOT 1 LVL GDG NAME    @YM06556*/ 00147000
*       DO;                         /* YES                           */ 00148000
         B     @RC00233                                            0235 00149000
@RF00233 DS    0H                                                  0236 00150000
*Z007DOG:                                                          0236 00151000
*         CALL IGGPDOG;             /* ORIENT/SRCH MSTR CATLG        */ 00152000
Z007DOG  BAL   @14,IGGPDOG                                         0236 00153000
*         CALL EXIT;                /* RETURN TO CALLER              */ 00154000
         BAL   @14,EXIT                                            0237 00155000
*       END;                                                       0238 00156000
*   ELSE                                                           0239 00157000
*     ;                             /* QUALIFIED ENTRY NAME          */ 00158000
@RF00232 DS    0H                                                  0240 00159000
*   SCHEN=CCASRCH;                  /* SAVE COPY OF ENTRY NAME       */ 00160000
@RC00232 MVC   SCHEN(44,R11),CCASRCH(R11)                          0240 00161000
*   SCHQN(1:CCAQLEN)=SCHEN(1:CCAQLEN);/* ISOLATE QUALIFIER           */ 00162000
