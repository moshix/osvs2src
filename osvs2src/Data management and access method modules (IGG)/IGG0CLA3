*Z046RPLM:                          /* LABEL FOR PROCEDURE TRACKING  */ 01203000
*   RFY                                                            0489 01204000
*    (R5,                                                          0489 01205000
*     R6,                                                          0489 01206000
*     R7,                                                          0489 01207000
*     R8,                                                          0489 01208000
*     R9,                                                          0489 01209000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 01210000
Z046     DS    0H                                                  0489 01211000
Z046RPLM DS    0H                                                  0490 01212000
*   RFY                                                            0490 01213000
*    (R2,                                                          0490 01214000
*     R3,                                                          0490 01215000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01216000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 01217000
         STM 12,14,12(13)                                               01218000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 01219000
         LA 13,12(13)                                                   01220000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 01221000
         BALR 12,0                                                      01222000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 01223000
         LA 15,*-IGGSEGST(0,0)                                          01224000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE RPL 0495 01225000
*                                      MANAGER SF                    */ 01226000
*                                                                  0495 01227000
         SR 12,15                                                       01228000
*   /*****************************************************************/ 01229000
*   /*                                                               */ 01230000
*   /* CATALOG ORIENTATION SUBFUNCTION - THIS ROUTINE ESTABLISHES    */ 01231000
*   /* ORIENTATION TO A SPECIFIC CATALOG.                            */ 01232000
*   /*                                                               */ 01233000
*   /*****************************************************************/ 01234000
*                                                                  0496 01235000
*   CCARPLK=N;                      /* CLEAR RPL COUNT BYTE          */ 01236000
         MVI   CCARPLK(R11),X'00'                                  0496 01237000
*   RFY                                                            0497 01238000
*    (R8) RSTD;                     /*                       @Z40WSPJ*/ 01239000
*   R8=ADDR(IGGCAXWA);              /* LOAD CAXWA BASE REG   @Z40WSPJ*/ 01240000
         L     @10,CCAACB(,R11)                                    0498 01241000
         L     R8,ACBUAPTR(,@10)                                   0498 01242000
*   RFY                                                            0499 01243000
*     IGGCAXWA BASED(R8);           /*                       @Z40WSPJ*/ 01244000
*                                                                  0499 01245000
*   /*****************************************************************/ 01246000
*   /*                                                               */ 01247000
*   /* CHECK FOR A CRA ACCESS AND DETERMINE IF AN RPL NEEDS TO BE    */ 01248000
*   /* ASSIGNED TO THE CRA AND/OR CAT                        @Y30DSSB*/ 01249000
*   /*                                                               */ 01250000
*   /*****************************************************************/ 01251000
*                                                                  0500 01252000
*   IF ACBSCRA=ON&                  /* SYSTEM CRA ACB REQ    @Y30SSSB*/ 01253000
*       (CAXBLD=ON|                 /* CRA BUILD REQ         @Y30SSSB*/ 01254000
*       CAXOPN=ON) THEN             /* SYSTEM CRA OPEN       @Y30SSSB*/ 01255000
         TM    ACBSCRA(@10),B'00001000'                            0500 01256000
         BNO   @RF00500                                            0500 01257000
         TM    CAXBLD(R8),B'11000000'                              0500 01258000
         BZ    @RF00500                                            0500 01259000
*     CALL EXIT;                    /* RETURN TO CALLER      @Y30DSSB*/ 01260000
         BAL   @14,EXIT                                            0501 01261000
*   ELSE                                                           0502 01262000
*     ;                             /*                       @Y30SSSB*/ 01263000
*                                                                  0502 01264000
@RF00500 DS    0H                                                  0503 01265000
*   /*****************************************************************/ 01266000
*   /*                                                               */ 01267000
*   /* GET CATALOG ORIENTATION                               @Y30SSSB*/ 01268000
*   /*                                                               */ 01269000
*   /*****************************************************************/ 01270000
*                                                                  0503 01271000
*   IF ACBSCRA=ON THEN              /* GET THE CATALOG ACB ADDR      */ 01272000
