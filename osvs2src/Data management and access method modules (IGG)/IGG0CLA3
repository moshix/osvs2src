         BZ    @RT00522                                            0522 01341000
@GL00036 L     @10,CCACPL(,R11)                                    0522 01342000
         TM    CTGSUPLT(@10),B'00010000'                           0522 01343000
         BNO   @RF00522                                            0522 01344000
@RT00522 DS    0H                                                  0523 01345000
*     DO;                           /* YES                   @Z40WSPJ*/ 01346000
*       RFY                                                        0524 01347000
*        (R5,                                                      0524 01348000
*         R6) RSTD;                 /*                       @Z40WSPJ*/ 01349000
*                                                                  0524 01350000
*       /*************************************************************/ 01351000
*       /*                                                           */ 01352000
*       /* CHECK IF THE CLEANUP REQUIRED BIT CAXF2CLR        @Z40WSPJ*/ 01353000
*       /* IS ON, AND IF SO, RESET IT AND CHANGE TO          @Z40WSPJ*/ 01354000
*       /* AN EXCLUSIVE ENQ.                                 @Z40WSPJ*/ 01355000
*       /*                                                           */ 01356000
*       /*************************************************************/ 01357000
*                                                                  0525 01358000
*RPLM000E:                                                         0525 01359000
*       ;                           /*                       @Z40WSPJ*/ 01360000
RPLM000E DS    0H                                                  0526 01361000
*       R5=CAXFLGWD|F2CLRON;        /* SET CAXF2CLR IN R5    @Z40WSPJ*/ 01362000
         L     R5,CAXFLGWD(,R8)                                    0526 01363000
