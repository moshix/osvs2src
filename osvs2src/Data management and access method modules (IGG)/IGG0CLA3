         IC    @10,CCARPLX(,R11)                                   0708 01953000
         MH    @10,@CH00142                                        0708 01954000
         L     @09,CAXRPL(,R8)                                     0708 01955000
         AL    @09,@CF04260                                        0708 01956000
         LA    @10,RPLPOOL(@10,@09)                                0708 01957000
         ST    @10,CCARPL1(,R11)                                   0708 01958000
*     R1=ADDR(RXTNAV);              /* POINT TO RPL AVAIL LK BYTE    */ 01959000
         LA    R1,RXTNAV(,@10)                                     0709 01960000
*     GEN(TS  0(R1));               /* IS RPL FREE           @Z40WSPJ*/ 01961000
         TS  0(R1)                                                      01962000
*     GEN(BNZ RPLMTSRT);            /* NO,BRANCH AROUND      @Z40WSPJ*/ 01963000
         BNZ RPLMTSRT                                                   01964000
*     GEN(SR  R1,R1 ) ;             /* YES,INDICATE TO PLS           */ 01965000
         SR  R1,R1                                                      01966000
*RPLMTSRT:                                                         0713 01967000
*     IF(R1=N|                      /* RPL FREE ?             YM03989*/ 01968000
*         CCAF2RHS=ON)&             /* OR RECURSVE REQ        YM03989*/ 01969000
*         RPLACTIV=N THEN           /* AND NOT ACTIVE ?      @Z40WSPJ*/ 01970000
RPLMTSRT LTR   R1,R1                                               0713 01971000
         BZ    @GL00049                                            0713 01972000
         TM    CCAF2RHS(R11),B'00001000'                           0713 01973000
         BNO   @RF00713                                            0713 01974000
@GL00049 L     @10,CCARPL1(,R11)                                   0713 01975000
         CLI   RPLACTIV(@10),0                                     0713 01976000
         BNE   @RF00713                                            0713 01977000
*       DO;                         /* THEN INIT- TO USE IT          */ 01978000
*         RPLUPD=OFF;               /* INSURE NON GET-UPD STATUS     */ 01979000
         NI    RPLUPD(@10),B'11111101'                             0715 01980000
*         CCARPLK=ONE;              /* SET ASS RPL CNT = 1           */ 01981000
         MVI   CCARPLK(R11),X'01'                                  0716 01982000
*         RPLARG=ADDR(CCASRCH);     /* PT TO SCH ARG                 */ 01983000
         LA    @09,CCASRCH(,R11)                                   0717 01984000
         ST    @09,RPLARG(,@10)                                    0717 01985000
*       END;                        /*                       @Z40WSPJ*/ 01986000
*     ELSE                                                         0719 01987000
*       ;                           /* CONTINUE LOOP         @Z40WSPJ*/ 01988000
@RF00713 DS    0H                                                  0720 01989000
*   END;                            /*                       @Z40WSPJ*/ 01990000
         LA    @10,1                                               0720 01991000
         SLR   @09,@09                                             0720 01992000
         IC    @09,CCARPLX(,R11)                                   0720 01993000
         ALR   @10,@09                                             0720 01994000
@DE00707 STC   @10,CCARPLX(,R11)                                   0720 01995000
         CH    @10,CAXRPLCT(,R8)                                   0720 01996000
         BNH   @DL00707                                            0720 01997000
@DC00707 DS    0H                                                  0721 01998000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 01999000
         BAL   @14,EXIT                                            0721 02000000
*   END;                            /* END OF INNER PROCEDURE        */ 02001000
@EL00009 DS    0H                                                  0722 02002000
@EF00009 DS    0H                                                  0722 02003000
@ER00009 BR    @14                                                 0722 02004000
         EJECT                                                          02005000
*                                                                  0723 02006000
