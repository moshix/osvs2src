*                                                                  0529 01371000
         BC    8,RPLM001E                                          0529 01372000
*       /*************************************************************/ 01373000
*       /*                                                           */ 01374000
*       /* DETERMINE IF A BIT OTHER THAN CAXF2CLR            @Z40WSPJ*/ 01375000
*       /* CAUSED THE COMPARE AND SWAP TO FAIL, AND          @Z40WSPJ*/ 01376000
*       /* IF SO, RETRY THE ABOVE SEQUENCE.                  @Z40WSPJ*/ 01377000
*       /*                                                           */ 01378000
*       /*************************************************************/ 01379000
*                                                                  0530 01380000
*       IF R6^=(R5&F2CLROFF) THEN   /* CLEAN-UP REQUIRED?    @Z40WSPJ*/ 01381000
         LR    @10,R5                                              0530 01382000
         N     @10,@CF04057                                        0530 01383000
         CLR   R6,@10                                              0530 01384000
         BNE   @RT00530                                            0530 01385000
*         GOTO RPLM000E;            /* MAYBE, RETRY SEQUENCE @Z40WSPJ*/ 01386000
*       ELSE                        /* NO CLEANUP REQUIRED   @Z40WSPJ*/ 01387000
*         DO;                       /*                       @Z40WSPJ*/ 01388000
*           CCAF2XEQ=OFF;           /* INDICATE SHARED ENQ           */ 01389000
         NI    CCAF2XEQ(R11),B'11101111'                           0533 01390000
*           CCAEDSHR=ON;            /* INDICATE SHARED ENQ           */ 01391000
