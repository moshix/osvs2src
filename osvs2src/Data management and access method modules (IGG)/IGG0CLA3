         TITLE 'IGG0CLA3 - VSAM SEARCH CATALOG PART 2                  *00001000
                        '                                               00002000
IGG0CLA3 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGG0CLA3  78.171'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
JSCBDISP EQU 188                                                        00011000
JSCBAOS1 EQU  72                                                        00012000
JSCBAOS2 EQU 192                                                        00013000
         USING @MAINENT,@15                                             00014000
         USING @DATD-(CCATEMPS-IGGCCA),R11                              00015000
         DS 0F                                                          00016000
         DC CL8'IGGPCSCP'                                               00017000
@EL00001 DS    0H                                                  0196 00018000
@EF00001 DS    0H                                                  0196 00019000
@ER00001 BR    @14                                                 0196 00020000
IGGPCSCP DS    0H                                                  0197 00021000
         ENTRY IGGPCSCP                                                 00022000
         DROP @15                                                       00023000
Z003     DS    0H                                                  0199 00024000
Z003CSCP DS    0H                                                  0200 00025000
         STM 12,14,12(13)                                               00026000
         LA 13,12(13)                                                   00027000
         BALR 12,0                                                      00028000
IGGSEGST DS 0H                                                          00029000
*                                      SRCH-MULTI CATS               */ 00030000
*                                                                  0205 00031000
         USING *,12                                                     00032000
*   /*****************************************************************/ 00033000
*   /*                                                               */ 00034000
*   /* SEARCH CATALOG, CATALOG ID NOT GIVEN SUBFUNCTION - THIS       */ 00035000
*   /* ROUTINE SEARCHES ONE OR MORE CATALOGS WHEN THE CATALOG ID IS  */ 00036000
*   /* NOT GIVEN.                                                    */ 00037000
*   /*                                                               */ 00038000
*   /*****************************************************************/ 00039000
*                                                                  0206 00040000
*   CCAF1LPS=OFF;                   /* PERMIT LOOP                   */ 00041000
         NI    CCAF1LPS(R11),B'01111111'                           0206 00042000
*   DO WHILE(CCAF1LPS=OFF);         /* LOOK FOR CATLGS PCCB          */ 00043000
         B     @DE00207                                            0207 00044000
@DL00207 DS    0H                                                  0208 00045000
*Z004GPCC:                                                         0208 00046000
*     CALL IGGPGPCC;                /* GET 1ST/NXT PCCB              */ 00047000
Z004GPCC BAL   @14,IGGPGPCC                                        0208 00048000
*     IF CCANPCCB=N|                /* NO MORE PCCBS                 */ 00049000
*         (PCCACTIV=ON&             /* PCCB IS ACTIVE                */ 00050000
*         PCCSTEPC=ON) THEN         /* PCCB IS FOR STEPCAT           */ 00051000
         L     @10,CCANPCCB(,R11)                                  0209 00052000
         LTR   @10,@10                                             0209 00053000
         BZ    @RT00209                                            0209 00054000
         L     @10,PCCBPTR(,R11)                                   0209 00055000
         TM    PCCACTIV(@10),B'10100000'                           0209 00056000
         BNO   @RF00209                                            0209 00057000
@RT00209 DS    0H                                                  0210 00058000
*       CCAF1LPS=ON;                /* KILL LOOP                     */ 00059000
         OI    CCAF1LPS(R11),B'10000000'                           0210 00060000
*     IF CCAF1LPS=ON&CCANPCCB^=N THEN/* PCCB FOUND                   */ 00061000
@RF00209 TM    CCAF1LPS(R11),B'10000000'                           0211 00062000
         BNO   @RF00211                                            0211 00063000
         SLR   @10,@10                                             0211 00064000
         C     @10,CCANPCCB(,R11)                                  0211 00065000
         BE    @RF00211                                            0211 00066000
*       DO;                         /* YES                           */ 00067000
*         IF PCCACBP=N THEN         /* UNOPENED PRIVATE CATLG        */ 00068000
         L     @05,PCCBPTR(,R11)                                   0213 00069000
         C     @10,PCCACBP(,@05)                                   0213 00070000
         BNE   @RF00213                                            0213 00071000
*           DO;                     /* YES                           */ 00072000
*                                                                  0214 00073000
*             /*******************************************************/ 00074000
*             /*                                                     */ 00075000
*             /* SET CCAMODID='A3', CCAREASN=RSN2, CCARETRN=RCCATUNA */ 00076000
*             /*                                                     */ 00077000
*             /*******************************************************/ 00078000
*                                                                  0215 00079000
*             CCAPROB=IGGSEGID+(RSN2*K256)+RCCATUNA;/* SET CATALOG 0215 00080000
*                                      NOT AVAILABLE CODE    @VS41827*/ 00081000
         MVC   CCAPROB(4,R11),@CF04212                             0215 00082000
*             IF CTGSUPLT=ON THEN   /* SUPERLOCATE ?                 */ 00083000
         L     @10,CCACPL(,R11)                                    0216 00084000
         TM    CTGSUPLT(@10),B'00010000'                           0216 00085000
         BNO   @RF00216                                            0216 00086000
*               DO;                 /* YES                           */ 00087000
*                 CTGCVDSN=PCCDSNAM;/* STORE CATLG NAME              */ 00088000
         L     @10,CTGCVOL(,@10)                                   0218 00089000
         MVC   CTGCVDSN(44,@10),PCCDSNAM(@05)                      0218 00090000
*                 CTGCVCC=CTGCVCC&&CTGCVCC;/* IND NO QUAL            */ 00091000
         XC    CTGCVCC(44,@10),CTGCVCC(@10)                        0219 00092000
*               END;                                               0220 00093000
*             CALL EXIT;            /* RETURN TO CALLER              */ 00094000
@RF00216 BAL   @14,EXIT                                            0221 00095000
*           END;                                                   0222 00096000
*         ELSE                                                     0223 00097000
*           ;                       /* PVT CATLG OPEN                */ 00098000
@RF00213 DS    0H                                                  0224 00099000
*Z006DOG:                                                          0224 00100000
*         CALL IGGPDOG;             /* ORIENT/GET ENTRY RCD          */ 00101000
Z006DOG  BAL   @14,IGGPDOG                                         0224 00102000
*         IF(CCACD1=RCENT|          /* NOT FND OR            @YM04147*/ 00103000
*             CCACD1=RCCAT)&        /* CATLG DELETED &       @YM04147*/ 00104000
*             CCASC=CSCOD THEN      /* CONCAT SRCH           @YM04147*/ 00105000
         CLI   CCACD1(R11),8                                       0225 00106000
         BE    @GL00003                                            0225 00107000
         CLI   CCACD1(R11),4                                       0225 00108000
         BNE   @RF00225                                            0225 00109000
@GL00003 CLI   CCASC(R11),3                                        0225 00110000
         BNE   @RF00225                                            0225 00111000
*           CCAF1LPS=OFF;           /* THEN CONTINUE SEARCH          */ 00112000
         NI    CCAF1LPS(R11),B'01111111'                           0226 00113000
*         ELSE                                                     0227 00114000
*           CALL EXIT;              /* RETURN TO CALLER              */ 00115000
         B     @RC00225                                            0227 00116000
@RF00225 BAL   @14,EXIT                                            0227 00117000
*       END;                                                       0228 00118000
*     ELSE                                                         0229 00119000
*       ;                           /* PCCB NOT FOUND                */ 00120000
@RF00211 DS    0H                                                  0230 00121000
*   END;                                                           0230 00122000
@RC00211 DS    0H                                                  0230 00123000
@DE00207 TM    CCAF1LPS(R11),B'10000000'                           0230 00124000
         BZ    @DL00207                                            0230 00125000
*   CCASC=CSCOD;                    /* IND NO MORE STEPCATS          */ 00126000
         MVI   CCASC(R11),X'03'                                    0231 00127000
*   IF CCAQLEN=N THEN               /* UNQUALIFIED ENTRY NAME        */ 00128000
         CLI   CCAQLEN(R11),0                                      0232 00129000
         BNE   @RF00232                                            0232 00130000
*     IF CTGFUNC=CTGCMS&            /* CMS REQUEST           @YM06556*/ 00131000
*         CTGOPTNS=CTGDEFIN&        /* DEFINE REQUEST        @YM06556*/ 00132000
*         CCAF2CCT=OFF THEN         /* DEFINE OF GDG         @YM06556*/ 00133000
         L     @10,CCACPL(,R11)                                    0233 00134000
         TM    CTGFUNC(@10),B'10000000'                            0233 00135000
         BNO   @RF00233                                            0233 00136000
         TM    CTGFUNC(@10),B'01100000'                            0233 00137000
         BNZ   @RF00233                                            0233 00138000
         TM    CTGOPTNS(@10),B'00001000'                           0233 00139000
         BNO   @RF00233                                            0233 00140000
