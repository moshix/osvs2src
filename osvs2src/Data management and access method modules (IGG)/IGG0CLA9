*             RSPILL=RSPILLM;       /* RESTORE SPILL BLOCK ADDR      */ 00544000
         LR    RSPILL,RSPILLM                                      0321 00545000
*           END;                    /* PROCESSING FOR POS RMAJOR     */ 00546000
*       END;                        /* OF OUTER SCAN LOOP            */ 00547000
*                                                                  0323 00548000
@RF00305 DS    0H                                                  0323 00549000
@DE00301 LTR   RMAJOR,RMAJOR                                       0323 00550000
         BNZ   @DL00301                                            0323 00551000
*       /*************************************************************/ 00552000
*       /*                                                           */ 00553000
*       /* AFTER ELIMINATING ALL MATCHING PAIRS (I.E. GETMAIN -      */ 00554000
*       /* FREEMAIN PAIRS) SCAN ALL REMAINING ENTRIES, FREEING THE   */ 00555000
*       /* STORAGE REPRESENTED BY EACH UNMATCHED ENTRY               */ 00556000
*       /*                                                           */ 00557000
*       /*************************************************************/ 00558000
*                                                                  0324 00559000
