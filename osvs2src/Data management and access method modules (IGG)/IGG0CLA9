*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00745000
         STM 12,14,12(13)                                               00746000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00747000
*                                                                  0378 00748000
         LA 13,12(13)                                                   00749000
*   /*****************************************************************/ 00750000
*   /*                                                               */ 00751000
*   /* 'GET NEXT TRACKING ENTRY' ROUTINE                             */ 00752000
*   /*                                                               */ 00753000
*   /*****************************************************************/ 00754000
*                                                                  0379 00755000
*   RFY                                                            0379 00756000
*    (RCURR,                                                       0379 00757000
*     REXSW) RSTD;                                                 0379 00758000
*   REXSW=NULL;                     /* INITIALIZE EXIT SWITCH        */ 00759000
         SLR   REXSW,REXSW                                         0380 00760000
*   DO WHILE(REXSW=NULL);           /* LOOP UNTIL VALID OR GONE      */ 00761000
*                                                                  0381 00762000
         B     @DE00381                                            0381 00763000
