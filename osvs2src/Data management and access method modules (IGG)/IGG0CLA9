         BZ    @RF00305                                            0305 00486000
*           DO;                                                    0306 00487000
*                                                                  0306 00488000
*             /*******************************************************/ 00489000
*             /*                                                     */ 00490000
*             /* SCAN ALL PRECEDING ENTRIES, LOOKING FOR A MATCH WITH*/ 00491000
*             /* THE MAJOR ENTRY                                     */ 00492000
*             /*                                                     */ 00493000
*             /*******************************************************/ 00494000
*                                                                  0307 00495000
*             RCURR=NULL;           /* START WITH FIRST ENTRY        */ 00496000
         SLR   RCURR,RCURR                                         0307 00497000
*             REXIT=RCURR;          /* SET EXIT SWITCH               */ 00498000
         LR    REXIT,RCURR                                         0308 00499000
*             DO WHILE REXIT=NULL;  /* SCAN PRECEDING ENTRIES        */ 00500000
         B     @DE00309                                            0309 00501000
