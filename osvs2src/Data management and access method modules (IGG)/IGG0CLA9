*                           R7=ONE; /* NEW POST LOCK VALUE   @Z40WSPJ*/ 00149000
         LA    R7,1                                                0212 00150000
*                           CS(R6,R7,CAXPLOCK);/* TRY TO GET LOCK  0213 00151000
*                                                            @Z40WSPJ*/ 00152000
