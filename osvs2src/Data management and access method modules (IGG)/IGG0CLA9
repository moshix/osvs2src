Z013     DS    0H                                                  0291 00435000
Z013FRES DS    0H                                                  0292 00436000
*   RFY                                                            0292 00437000
*    (R2,                                                          0292 00438000
*     R3,                                                          0292 00439000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00440000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00441000
         STM 12,14,12(13)                                               00442000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00443000
*                                                                  0294 00444000
         LA 13,12(13)                                                   00445000
*   /*****************************************************************/ 00446000
*   /*                                                               */ 00447000
*   /* **** FREE STORAGE ROUTINE ****                                */ 00448000
*   /*                                                               */ 00449000
*   /*****************************************************************/ 00450000
*                                                                  0295 00451000
*   RFY                                                            0295 00452000
*    (RMAJOR,                                                      0295 00453000
*     RCURR) RSTD;                                                 0295 00454000
*   IF RVARFI=OFF THEN              /* IF TRACKING NOT INCOMPLETE    */ 00455000
         TM    RVARFI(R11),B'01000000'                             0296 00456000
         BNZ   @RF00296                                            0296 00457000
*     DO;                           /* CAN FREE STORAGE              */ 00458000
*       RMAJOR=R11;                 /* SET ENTRY POINTER ^= 0        */ 00459000
*                                                                  0298 00460000
         LR    RMAJOR,R11                                          0298 00461000
*       /*************************************************************/ 00462000
*       /*                                                           */ 00463000
*       /* PROCESS EACH SUCCEEDING TRACKING ENTRY BEGINNING WITH THE */ 00464000
*       /* SECOND ENTRY. CALL THIS ENTRY THE 'MAJOR' ENTRY.          */ 00465000
*       /*                                                           */ 00466000
*       /*************************************************************/ 00467000
*                                                                  0299 00468000
*       RCURR=NULL;                 /* NO CURRENT ENTRY              */ 00469000
         SLR   RCURR,RCURR                                         0299 00470000
*Z014GNTE:                                                         0300 00471000
*       CALL IGGPGNTE;              /* GET 1ST ENTRY                 */ 00472000
