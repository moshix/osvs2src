         C     @05,SVEND(,@10)                                     0678 02522000
         BNE   @RF00678                                            0678 02523000
*     SVSTART=SVSTART-LENGTH(CTGVL);/* ALLOW FOR ONE ENTRY           */ 02524000
         SL    @05,@CF00075                                        0679 02525000
         ST    @05,SVSTART(,@10)                                   0679 02526000
*   ELSE                            /* MOVE ENTRIES (THRU INSERT   0680 02527000
*                                      POINT) TO ALLOW INSERTION OF     02528000
*                                      NEW VOLUME                    */ 02529000
*     DO;                                                          0680 02530000
         B     @RC00678                                            0680 02531000
@RF00678 DS    0H                                                  0681 02532000
*       RFY                                                        0681 02533000
*        (RMVLEN2) RSTD;            /* RESTRICT REGISTER 5           */ 02534000
*       RMVFRM=SVSTART;             /* MOVE FROM ADDRESS             */ 02535000
         LA    @10,104                                             0682 02536000
