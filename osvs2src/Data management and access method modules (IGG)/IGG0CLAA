*             CALL EXIT;            /* ERROR, RETURN TO CALLER       */ 01671000
         BAL   @14,EXIT                                            0491 01672000
*           ELSE                                                   0492 01673000
*             ;                     /* CONTINUE, NO ERROR            */ 01674000
*                                                                  0492 01675000
@RF00490 DS    0H                                                  0493 01676000
*           /*********************************************************/ 01677000
*           /*                                                       */ 01678000
*           /* RETRIEVE INDEX RECORD                                 */ 01679000
