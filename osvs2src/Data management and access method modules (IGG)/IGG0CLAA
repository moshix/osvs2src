*               END;                                               0662 02456000
*             ELSE                                                 0663 02457000
*               ;                   /* CURRENT ENTRY NOT NULL        */ 02458000
@RF00658 DS    0H                                                  0664 02459000
*           END;                                                   0664 02460000
         AL    RCND,@CF00040                                       0664 02461000
         CH    RCND,@CH00061                                       0664 02462000
         BNH   @DL00657                                            0664 02463000
*           IF SVOCNDTE=ON THEN     /* INPUT VOLUME A CANDIDATE      */ 02464000
         L     @10,CCAR4REC(,R11)                                  0665 02465000
         L     @10,SVVPT+104(,@10)                                 0665 02466000
         TM    SVOCNDTE(@10),B'01000000'                           0665 02467000
         BNO   @RF00665                                            0665 02468000
*             DO RCND=1 TO DIM(SLCANDTE);/* SCAN CANDIDATE LIST FOR     02469000
*                                      NULL ENTRY                    */ 02470000
         LA    RCND,1                                              0666 02471000
@DL00666 DS    0H                                                  0667 02472000
*               IF SLCANDTE(RCND)=NULL THEN/* IF NULL ENTRY          */ 02473000
         LR    @10,RCND                                            0667 02474000
         MH    @10,@CH00054                                        0667 02475000
         LA    @05,104                                             0667 02476000
         AL    @05,CCAR4REC(,R11)                                  0667 02477000
         ST    @10,@TF00001                                        0667 02478000
         ALR   @10,@05                                             0667 02479000
