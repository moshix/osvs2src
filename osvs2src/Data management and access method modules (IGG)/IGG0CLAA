*                                                                  0448 01461000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 01462000
**   MODULE NAME = IGGPSLY(IGG0CLAA)                                  * 01463000
**                                                                    * 01464000
**   DESCRIPTIVE NAME = SUPER LOCATE UPGRADE PROCESSING               * 01465000
**                                                                    * 01466000
**   FUNCTION =                                                       * 01467000
**      THIS SUB-FUNCTION IS CALLED WHEN THE UPGRADE VOLUME           * 01468000
**      OCCURRENCES ARE TO BE PLACED IN THE VOLUME LIST.  THE         * 01469000
**      PROCESSING IS AS FOLLOWS:                                     * 01470000
**      1. EXTRACT THE GROUP OCCURRENCES FROM THE BASE CLUSTER DATA   * 01471000
**         RECORD.                                                    * 01472000
**      2. EXTRACT THE VOLUME OCCURRENCES FROM THE GROUP OCCURRENCES  * 01473000
**         ASSOCIATED WITH THE UPGRADE SET (CALL IGGPSLEL).           * 01474000
**      3. NON-DUPLICATE VOLUME INFORMATION INSERTED IN THE VOLUME    * 01475000
**         LIST (IGGPSLIV - CALLED BY IGGPSLEL).                      * 01476000
**                                                                    * 01477000
**   INPUT =                                                          * 01478000
**      CPL AND FPLS INITIALIZED TO EXTRACT GROUP OCCURRENCES, CCA    * 01479000
**      INITIALIZED FOR EXTRACT, BASE CLUSTER DATA RECORD IN RAB1.    * 01480000
**                                                                    * 01481000
**   OUTPUT =                                                         * 01482000
**      ALL NON-DUPLICATE VOLUME INFORMATION OF THE VOLUMES           * 01483000
**      ASSOCIATED WITH THE UPGRADE SET. VOLUMES WILL BE LOCATED AT   * 01484000
**      THE HIGH END OF THE VOLUME LIST WHEN EXITING THIS ROUTINE.    * 01485000
**                                                                    * 01486000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01487000
*                                                                  0448 01488000
*   SVSLYLP=ON;                     /* INDICATE UPGRADE PROCESSING TO   01489000
*                                      BE PERFORMED                  */ 01490000
*                                                                  0448 01491000
         L     @10,CCAR4REC(,R11)                                  0448 01492000
         LA    @05,104                                             0448 01493000
         ALR   @05,@10                                             0448 01494000
         OI    SVSLYLP(@05),B'01000000'                            0448 01495000
*   /*****************************************************************/ 01496000
*   /*                                                               */ 01497000
*   /* INITIALIZE FIELDS TO EXTRACT GROUP OCCURRENCES                */ 01498000
*   /*                                                               */ 01499000
*   /*****************************************************************/ 01500000
*                                                                  0449 01501000
*   RFY                                                            0449 01502000
*     CTGPL BASED(CCAXCPL);         /* SUPER LOCATE CTGPL ADDR.      */ 01503000
*   CTGWKA=ADDR(SWKA0);             /* SET ADDRESS OF WORKAREA FOR 0450 01504000
*                                      EXTRACT                       */ 01505000
         L     @01,CCAXCPL(,R11)                                   0450 01506000
         LA    @00,SWKA0(,@05)                                     0450 01507000
         ST    @00,CTGWKA(,@01)                                    0450 01508000
*   WKALEN1=LENGTH(SWKA0);          /* LENGTH OF WORKAREA            */ 01509000
         LR    @01,@00                                             0451 01510000
         MVC   WKALEN1(2,@01),@CH00191                             0451 01511000
*   RFY                                                            0452 01512000
*     CTGPL BASED(CCACPL);          /* CALLER'S CTGPL ADDRESS.       */ 01513000
*   CTGFLDNM=ADDR(ENTUPGD);         /* SET ADDRESS OF FIELD NAMES FOR   01514000
*                                      EXTRACT                       */ 01515000
         LA    @15,SFPLS(,@10)                                     0453 01516000
         LA    @14,@CC02805                                        0453 01517000
         ST    @14,CTGFLDNM(,@15)                                  0453 01518000
*   SLREL=N;                        /* RELATIVE REPETITION TEST VALUE   01519000
*                                      TO ZERO                       */ 01520000
         SLR   @15,@15                                             0454 01521000
         STH   @15,SLREL(,@05)                                     0454 01522000
*   SLCNT=LENGTH(SWKAS0)/LENGTH(SLASS);/* MAXIMUM NUMBER OF        0455 01523000
*                                      OCCURRENCES TO BE RETURNED    */ 01524000
         MVC   SLCNT(2,@05),@CH00258                               0455 01525000
*   SVINMUC=ON;                     /* INDICATE VOLUMES TO BE PLACED    01526000
*                                      WITHIN THE MUC                */ 01527000
         OI    SVINMUC(@05),B'10000000'                            0456 01528000
*   RFY                                                            0457 01529000
*     CTGFL BASED(ADDR(SFPLS(2)));  /* ADDRESSABILITY ON FIRST TEST     01530000
*                                      FPL                           */ 01531000
*   CTGFLDGC=ASSOCGC;               /* SET ASSOCIATION GROUP CODE IN    01532000
*                                      TEST FPL                      */ 01533000
         LA    @10,SFPLS+24(,@10)                                  0458 01534000
         MVI   CTGFLDGC(@10),X'02'                                 0458 01535000
*   RFY                                                            0459 01536000
*     CTGFL BASED(ADDR(SFPLS(1)));  /* ADDRESSABILITY FOR SL FIELD 0459 01537000
*                                      PARAMETER LIST                */ 01538000
*   DO UNTIL(SVSLYLP=OFF);          /* BEGIN UPGRADE PROCESSING      */ 01539000
*                                                                  0460 01540000
@DL00460 DS    0H                                                  0461 01541000
*     /***************************************************************/ 01542000
*     /*                                                             */ 01543000
*     /* EXTRACT UPGRADE GROUP OCCURRENCE ASSOCIATIONS. CALL TO      */ 01544000
*     /* MODULE IGG0CLAZ                                             */ 01545000
*     /*                                                             */ 01546000
*     /***************************************************************/ 01547000
*                                                                  0461 01548000
*Z053EXT:                                                          0461 01549000
*     CALL IGGPEXT;                                                0461 01550000
Z053EXT  L     @15,@CV00036                                        0461 01551000
         BALR  @14,@15                                             0461 01552000
*     IF(CCACD1^=RCS)|(CTGFLNG(1)=N) THEN/* EXIT IF ERROR IN EXTRACT    01553000
*                                      OR IF NO DATA RETURNED        */ 01554000
         CLI   CCACD1(R11),0                                       0462 01555000
         BNE   @RT00462                                            0462 01556000
         L     @10,CCAR4REC(,R11)                                  0462 01557000
         LA    @10,SFPLS(,@10)                                     0462 01558000
         ICM   @05,15,CTGFLNG(@10)                                 0462 01559000
