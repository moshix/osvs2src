*                                                                  0243 00364000
         SR 12,15                                                       00365000
*   /*****************************************************************/ 00366000
*   /*                                                               */ 00367000
*   /* THIS ROUTINE OPENS CRAS FOR CATALOG MGMT USE          @Y30SSRG*/ 00368000
*   /*                                                               */ 00369000
*   /*****************************************************************/ 00370000
*                                                                  0244 00371000
*   RFY                                                            0244 00372000
*     CATRCD BASED(CCARAREC);       /*                       @Y30SSRG*/ 00373000
*   RFY                                                            0245 00374000
*     IFGACB BASED(CCARAACB);       /*                       @Y30SSRG*/ 00375000
*   RFY                                                            0246 00376000
*    (R0,                                                          0246 00377000
*     R1,                                                          0246 00378000
*     R13) RSTD;                    /*                       @Y30SSRG*/ 00379000
*   IF CCARAACB^=N THEN             /* IS CLOSE-REOPEN INDICATED   0247 00380000
*                                                            @Y30SSRG*/ 00381000
         SLR   @10,@10                                             0247 00382000
         L     @05,CCARAACB(,R11)                                  0247 00383000
         CR    @05,@10                                             0247 00384000
         BE    @RF00247                                            0247 00385000
*     DO;                           /* YES                   @Y30SSRG*/ 00386000
*                                                                  0248 00387000
*       /*************************************************************/ 00388000
*       /*                                                           */ 00389000
*       /* UNCHAIN CRA CAXWA                                 @Y30SSRG*/ 00390000
*       /*                                                           */ 00391000
*       /*************************************************************/ 00392000
*                                                                  0249 00393000
*       RFY                                                        0249 00394000
*        (R14,                                                     0249 00395000
*         R15) RSTD;                /*                       @Y30SSRG*/ 00396000
*       R1=CCAACB->ACBUAPTR;        /* PT TO CATLG CAXWA     @Y30SSRG*/ 00397000
         L     @09,CCAACB(,R11)                                    0250 00398000
         L     R1,ACBUAPTR(,@09)                                   0250 00399000
*       R15=R1->CAXCRACB->ACBUAPTR; /* PT TO 1ST CRA CAXWA   @Y30SSRG*/ 00400000
         L     @09,CAXCRACB(,R1)                                   0251 00401000
         L     R15,ACBUAPTR(,@09)                                  0251 00402000
*       IF R1->CAXCRACB=CCARAACB THEN/* IS CRA 1ST ON CHN    @Y30SSRG*/ 00403000
         CR    @09,@05                                             0252 00404000
         BNE   @RF00252                                            0252 00405000
*         DO;                       /* YES,DEQ 1ST CRA       @Y30SSRG*/ 00406000
*           IF R15->CAXCHN=N THEN   /* ONLY 1 CRA ON CHN     @Y30SSRG*/ 00407000
         C     @10,CAXCHN(,R15)                                    0254 00408000
         BNE   @RF00254                                            0254 00409000
*             R1->CAXCRACB=N;       /* YES,REMOVE IT         @Y30SSRG*/ 00410000
         ST    @10,CAXCRACB(,R1)                                   0255 00411000
