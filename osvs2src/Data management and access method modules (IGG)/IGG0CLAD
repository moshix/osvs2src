         L     @05,CVTCBSP(,@05)                                   0221 00286000
         ST    @10,CBSACB(,@05)                                    0221 00287000
*     END;                          /* END CATALOG NOT OPENED        */ 00288000
*   ELSE                            /* SUCCESSFUL CATALOG OPEN       */ 00289000
*     DO;                           /* ACB IS OPEN                   */ 00290000
         B     @RC00207                                            0223 00291000
@RF00207 DS    0H                                                  0224 00292000
*       CBSACB=CCAACB;              /* ACB PTR INTO AMCBS            */ 00293000
