*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */ 01200000
*                                                                  0445 01201000
         SR 12,15                                                       01202000
*   /*****************************************************************/ 01203000
*   /*                                                               */ 01204000
*   /* THIS ROUTINE IS CALLED TO DETERMINE IF ANY PHYSICAL SPACE     */ 01205000
*   /* SHOULD BE RETURNED TO THE BIT MAP. THIS MAY OCCUR WITH        */ 01206000
*   /* MULTIPLE PHYSICAL EXTENTS FOR THE DATA SPACE AND THE CATALOG  */ 01207000
*   /* REQUIRING LESS SPACE THAN THE DATA SPACE. THE BMR ROUTINE IS  */ 01208000
*   /* CALLED TO PERFORM THE BIT MAP UPDATES.                        */ 01209000
*   /*                                                               */ 01210000
*   /*****************************************************************/ 01211000
*                                                                  0446 01212000
*/********************************************************************/ 01213000
*/*                                                                  */ 01214000
*/*   MACRO NAME = IGGMSAWA                                          */ 01215000
*/*                                                                  */ 01216000
*/*   DESCRIPTIVE NAME = SUBALLOCATE INTERFACE                       */ 01217000
*/*                                                                  */ 01218000
*/*   FUNCTION = SUBALLOCATE INTERFACE PARAMETER LIST                */ 01219000
*/*              AND WORKAREA MAPPING MACRO.                         */ 01220000
*/*                                                                  */ 01221000
*/*   INCLUDED MACROS = NONE                                         */ 01222000
*/*                                                                  */ 01223000
*/*   METHOD OF ACCESS = PL/S - NO DECLARES NECESSARY                */ 01224000
*/*                                                                  */ 01225000
*/*   STATUS = VS/2 RELEASE 3   (CHANGE FLAG @Y30SSXX)               */ 01226000
*/*                                                                  */ 01227000
*/*   DATE OF LAST CHANGE = 17 JUL 74                                */ 01228000
*/*                                                                  */ 01229000
*/********************************************************************/ 01230000
*                                                                  0446 01231000
*   DECLARE                                                        0446 01232000
*     1 SALPARL BASED(CCAALPTR),    /* BASE IS IN IGGCCA             */ 01233000
*       3 SALOPTNS PTR(8),          /* ALLOCATION OPTIONS            */ 01234000
*       3 * BIT(8),                 /* RESERVED                      */ 01235000
*       3 SALSOURC PTR(8),          /* RECORD SOURCE INDICATOR       */ 01236000
*       3 SALDSCIN PTR(24) BDY(BYTE),/* OBJECT DATA SET CI NUMBER    */ 01237000
*       3 SALPARMS CHAR(8),         /* ALLOCATION PARAMETERS         */ 01238000
*         5 SALCALN FIXED(15) BDY(BYTE),/* CONTROL AREA LENGTH       */ 01239000
*         5 SALBLKLN FIXED(15) BDY(BYTE),/* RECORD BLOCK LENGTH      */ 01240000
*         5 SALUNITS FIXED(15) BDY(BYTE),/* NUMBER OF UNITS REQUSTED */ 01241000
*         5 SALMXEXT PTR(8),        /* MAX NO. EXTENTS ALLOWED       */ 01242000
