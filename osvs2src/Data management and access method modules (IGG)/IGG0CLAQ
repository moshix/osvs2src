         TITLE 'IGG0CLAQ - CMS DEFINE SPACE IGGPDEFS                   *00001000
                        '                                               00002000
IGG0CLAQ CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGG0CLAQ  75.010'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
         USING @MAINENT,@15                                             00011000
         USING @DATD-(CCATEMPS-IGGCCA),R11                              00012000
         DS 0F                                                          00013000
         DC CL8'IGGPDEFS'                                               00014000
         EJECT                                                          00015000
         B     @PB00002                                            0126 00016000
IGGPDEFS DS    0H                                                  0127 00017000
         ENTRY IGGPDEFS                                                 00018000
         DROP @15                                                       00019000
Z003     DS    0H                                                  0129 00020000
Z003DEFS DS    0H                                                  0130 00021000
         STM 12,14,12(13)                                               00022000
         LA 13,12(13)                                                   00023000
         BALR 12,0                                                      00024000
IGGSEGST DS 0H                                                          00025000
         USING *,12                                                     00026000
*                                                                  0138 00027000
*/********************************************************************/ 00028000
*/*        GET  CORE  FOR  THE  DEFINE  SPACE  WORK  AREA            */ 00029000
*/********************************************************************/ 00030000
*                                                                  0139 00031000
