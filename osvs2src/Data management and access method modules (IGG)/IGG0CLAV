         CR    @10,@15                                             0398 01377000
         BNE   @RF00398                                            0398 01378000
@RT00398 DS    0H                                                  0399 01379000
*               DO;                 /* EXIT                          */ 01380000
*                 CCAEXGR2=ON;      /* EXIT                          */ 01381000
         OI    CCAEXGR2(R11),B'00010000'                           0400 01382000
*               END;                /* END EXIT                      */ 01383000
*             ELSE                  /* CONTINUE SEARCH               */ 01384000
*               CCACPE5=CCACPE5+CCAGOPLN;/* NEXT GOP                 */ 01385000
         B     @RC00398                                            0402 01386000
@RF00398 LH    @10,CCAGOPLN(,R11)                                  0402 01387000
         AL    @10,CCACPE5(,R11)                                   0402 01388000
         ST    @10,CCACPE5(,R11)                                   0402 01389000
