@RC00398 DS    0H                                                  0403 01391000
@RC00393 DS    0H                                                  0403 01392000
@DE00385 L     @10,CCAURAB(,R11)                                   0403 01393000
         CLC   CCACPE5(4,R11),CCACPE3(@10)                         0403 01394000
         BNL   @DC00385                                            0403 01395000
         TM    CCAEXGR2(R11),B'00010000'                           0403 01396000
         BZ    @DL00385                                            0403 01397000
@DC00385 DS    0H                                                  0404 01398000
*         IF EXTGOCI^=0&            /* IF EXTENSION OF BASE AND      */ 01399000
*             CCAURAB=CCARABB&CCANF=ON THEN/* BASE RAB               */ 01400000
         L     @10,CCAURAB(,R11)                                   0404 01401000
         L     @05,CCACPE2(,@10)                                   0404 01402000
