         LA 13,12(13)                                                   00280000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00281000
**   MODULE NAME =  IGGPSFPL(IGG0CLAV)                                * 00282000
**                                                                    * 00283000
**   DESCRIPTIVE NAME =                                               * 00284000
**      SCAN MODIFY FIELD PARAMETER LISTS.                            * 00285000
**                                                                    * 00286000
**   FUNCTION =                                                       * 00287000
**      THIS SUB-FUNCTION SCANS EACH FPL AND DETERMINES WHETHER THE   * 00288000
**      MODIFY OPERATION IS AN ADD GROUP OCCURRENCE, A DELETE GROUP   * 00289000
**      OCCURRENCE, OR AN ALTER OF AN EXISTING GROUP OCCURRENCE.  IN  * 00290000
**      ADDITION, IT INTERFACES WITH TESTS (IGGPTSTS) TO DETERMINE    * 00291000
**      WHETHER ONLY PARTICULAR OCCURRENCES ARE TO BE DELETED OR      * 00292000
**      ALTERED.  THE PROCESSING IS AS FOLLOWS:                       * 00293000
**      1. PERFORM NECESSARY INITIALIZATION: SET THE GOP LENGTH TO 5  * 00294000
**         OR 6 BYTES DEPENDING ON WHETHER THE BASE ENTRY IS A NON-   * 00295000
**         VOLUME OR VOLUME RECORD. INITIALIZE THE USABLE RAB'S AS    * 00296000
**         AVAILABLE. CALL IGGPSMFG TO SET UP                         * 00297000
**         FOR SMF ALTER PROCESSING.                                  * 00298000
**      2. SCAN THE FPL'S. FOR EACH FPL DETERMINE:                    * 00299000
**         A. IF THERE ARE CHAINED TESTS, CALL IGGPTSTS TO DETERMINE  * 00300000
**            THOSE OCCURRENCES TO BE ALTERED OR DELETED.             * 00301000
**         B. IF THERE ARE NO CHAINED TESTS AND FIELD DATA IS PROVIDED* 00302000
**            IN THE FPL, CALL IGGPXDGO TO ADD A GROUP OCCURRENCE.    * 00303000
**         C. CALL IGGPXLT2 (GROUP OCCURRENCE TO BE ALTERED) OR       * 00304000
**            IGGPXEL2 (GROUP OCCURRENCE TO BE DELETED) FOR ALL       * 00305000
**            OCCURRENCES DETERMINED IN 2(A) OR 2(B).                 * 00306000
**      3. WRITE BACK ANY UN-WRITTEN CATALOG RECORDS IN THE USABLE    * 00307000
**         RAB'S.                                                     * 00308000
**                                                                    * 00309000
**   INPUT =  SEE IGGPMOD INPUT ABOVE                                 * 00310000
**                                                                    * 00311000
**   OUTPUT =  SEE IGGPMOD INPUT ABOVE                                * 00312000
**                                                                    * 00313000
**                                                                    * 00314000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00315000
*                                                                  0152 00316000
*   CCAF1UP=ON;                     /* DO GET-FOR-UPDATE             */ 00317000
         OI    CCAF1UP(R11),B'00000010'                            0152 00318000
*   RFY                                                            0153 00319000
*     CCARAB BASED(CCARABB);        /* MAP BASE RAB                  */ 00320000
*   CCACRAVL=RCDCRAVL;              /* SET CRA VOL           @Y30SSRS*/ 00321000
         L     @10,CCARABB(,R11)                                   0154 00322000
         L     @10,CCACPE1(,@10)                                   0154 00323000
         MVC   CCACRAVL(6,R11),RCDCRAVL(@10)                       0154 00324000
*   CCACRADT=RCDCRADT;              /* SET CRA DEV           @Y30SSRS*/ 00325000
         MVC   CCACRADT(4,R11),RCDCRADT(@10)                       0155 00326000
*   IF RCDID=VOLRCDID|RCDID=VOLEXTID/* IF THIS IS VOLUME             */ 00327000
*     THEN                          /* RECORD                        */ 00328000
         CLI   RCDID(@10),C'V'                                     0156 00329000
         BE    @RT00156                                            0156 00330000
         CLI   RCDID(@10),C'W'                                     0156 00331000
         BNE   @RF00156                                            0156 00332000
@RT00156 DS    0H                                                  0157 00333000
*     DO;                           /* SET GOP PARAMETERS            */ 00334000
*       CCAGOPLN=VOLGOPLN;          /* SET GOP LENGTH                */ 00335000
         MVC   CCAGOPLN(2,R11),@CH00087                            0158 00336000
*       CCASL=VOLSNLN;              /* SET SEQUENCE NO. LENGTH       */ 00337000
         MVC   CCASL(2,R11),@CH00065                               0159 00338000
*     END;                          /* END SET                       */ 00339000
*   ELSE                            /* NOT VOLUME RECORD             */ 00340000
*     DO;                           /* SET GOP PARAMETERS            */ 00341000
         B     @RC00156                                            0161 00342000
@RF00156 DS    0H                                                  0162 00343000
*       CCAGOPLN=CATGOPLN;          /* SET GOP LENGTH                */ 00344000
         MVC   CCAGOPLN(2,R11),@CH00089                            0162 00345000
*       CCASL=CATSNLN;              /* SET SEQUENCE NO. LENGTH       */ 00346000
         MVC   CCASL(2,R11),@CH00057                               0163 00347000
*     END;                          /* END SET                       */ 00348000
*   CCACBASE=RCDCINO;               /* SAVE BASE RCD CI NO.          */ 00349000
@RC00156 L     @10,CCARABB(,R11)                                   0165 00350000
         L     @10,CCACPE1(,@10)                                   0165 00351000
         MVC   CCACBASE(3,R11),RCDCINO(@10)                        0165 00352000
*   RFY                                                            0166 00353000
*     CCARAB BASED(CCAURAB);        /* ORGINAL BASE                  */ 00354000
*   CCAURAB=CCARABF;                /* GET FIRST RAB                 */ 00355000
         L     @10,CCARABF(,R11)                                   0167 00356000
         ST    @10,CCAURAB(,R11)                                   0167 00357000
*   DO WHILE CCAURAB<=CCARABL;      /* MAKE ALL BUFFERS AVAIL        */ 00358000
         B     @DE00168                                            0168 00359000
@DL00168 DS    0H                                                  0169 00360000
*     IF CCAURAB^=CCARABB&          /* IF NOT BASE RAB               */ 00361000
*         CCARBUR=OFF THEN          /* AND UNUSED RAB        @ZA04507*/ 00362000
         L     @10,CCAURAB(,R11)                                   0169 00363000
         C     @10,CCARABB(,R11)                                   0169 00364000
         BE    @RF00169                                            0169 00365000
         TM    CCARBUR(@10),B'10000000'                            0169 00366000
         BNZ   @RF00169                                            0169 00367000
*       IF CCAREUSE=OFF|            /* NO REUSE IND          @ZA04507*/ 00368000
*           RCDCINO=CCACBASE THEN   /* OR BASE RCD           @ZA04507*/ 00369000
         TM    CCAREUSE(R11),B'00000100'                           0170 00370000
         BZ    @RT00170                                            0170 00371000
         L     @10,CCACPE1(,@10)                                   0170 00372000
         CLC   RCDCINO(3,@10),CCACBASE(R11)                        0170 00373000
         BNE   @RF00170                                            0170 00374000
@RT00170 DS    0H                                                  0171 00375000
*         CCACPE2=0;                /* NULL RCD AREA         @ZA04507*/ 00376000
         L     @10,CCAURAB(,R11)                                   0171 00377000
         SLR   @05,@05                                             0171 00378000
         ST    @05,CCACPE2(,@10)                                   0171 00379000
*       ELSE                                                       0172 00380000
*         ;                         /* ALLOW REUSE           @ZA04507*/ 00381000
@RF00170 DS    0H                                                  0173 00382000
*     ELSE                                                         0173 00383000
*       ;                           /* RAB MAY BE REUSED             */ 00384000
@RF00169 DS    0H                                                  0174 00385000
*     CCARBU1=OFF;                  /* FREE BUFFER                   */ 00386000
*     CCARBU2=OFF;                  /* FREE BUFFER                   */ 00387000
         L     @10,CCAURAB(,R11)                                   0175 00388000
         NI    CCARBU1(@10),B'10011111'                            0175 00389000
*     IF CCAURAB^=CCARABB THEN      /* IF NOT BASE RAB               */ 00390000
         C     @10,CCARABB(,R11)                                   0176 00391000
         BE    @RF00176                                            0176 00392000
*       CCARBWR=OFF;                /* ALLOW NO WRITE                */ 00393000
         NI    CCARBWR(@10),B'11101111'                            0177 00394000
*     ELSE                                                         0178 00395000
*       ;                           /* ALLOW POSSIBLE WRITE          */ 00396000
@RF00176 DS    0H                                                  0179 00397000
*     CCARBPA=OFF;                  /* INDICATE NO PUT ADD           */ 00398000
         L     @10,CCAURAB(,R11)                                   0179 00399000
         NI    CCARBPA(@10),B'11110111'                            0179 00400000
*     CCAURAB=CCAURAB+LENGTH(CCARAB);/* GET NEXT RAB PTR             */ 00401000
         AL    @10,@CF00145                                        0180 00402000
         ST    @10,CCAURAB(,R11)                                   0180 00403000
*   END;                            /* END FREEING                   */ 00404000
