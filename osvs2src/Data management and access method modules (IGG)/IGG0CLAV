         BNE   @RF00393                                            0393 01351000
         CLC   CCASN(2,R11),CCAGRHI(R11)                           0393 01352000
         BNE   @RF00393                                            0393 01353000
*             DO;                   /* PROCESS FOUND                 */ 01354000
*               CCAEXGR2=ON;        /* EXIT                          */ 01355000
*               CCANF=OFF;          /* FOUND GOP                     */ 01356000
         OI    CCAEXGR2(R11),B'00010000'                           0396 01357000
         NI    CCANF(R11),B'11110111'                              0396 01358000
*             END;                  /* END PROCESS FOUND             */ 01359000
*           ELSE                    /* NOT RIGHT GOP                 */ 01360000
*             IF CCAGRGC<(CATGOGC1&TRUEGC)|/* IF PASSED GOP          */ 01361000
*                 (CCASN>CCAGRHI&CCAGRGC=(CATGOGC1&TRUEGC))/*        */ 01362000
*               THEN                /* POSITION                      */ 01363000
         B     @RC00393                                            0398 01364000
@RF00393 SLR   @10,@10                                             0398 01365000
         IC    @10,CCAGRGC(,R11)                                   0398 01366000
         L     @05,CCACPE5(,R11)                                   0398 01367000
         LA    @15,63                                              0398 01368000
         SLR   @14,@14                                             0398 01369000
