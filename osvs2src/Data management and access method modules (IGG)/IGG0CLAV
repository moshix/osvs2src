         BNO   @RF00404                                            0404 01408000
*           DO;                     /* SET UP FOR GET                */ 01409000
*             CCASRID=EXTGOCI;      /* SEARCH ARG INITLZ             */ 01410000
         MVC   CCASRID(3,R11),EXTGOCI(@05)                         0406 01411000
*             CCAEXGR2=OFF;         /* CONTINUE SEARCH               */ 01412000
         NI    CCAEXGR2(R11),B'11101111'                           0407 01413000
