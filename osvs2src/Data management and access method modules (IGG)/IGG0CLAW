         TM    CCARBWR(@10),B'00010000'                            0177 00346000
         BNO   @RF00177                                            0177 00347000
*Z015PREC:                                                         0178 00348000
*         CALL IGGPPREC;            /* CALL PUT                      */ 00349000
Z015PREC BAL   @14,IGGPPREC                                        0178 00350000
*       ELSE                                                       0179 00351000
*         ;                         /* NO UPDATE NECESSARY           */ 00352000
@RF00177 DS    0H                                                  0180 00353000
*       DO;                         /* INITLZ RCD                    */ 00354000
*         RFY                                                      0181 00355000
*           RCDAR1 BASED(CCACPE1);  /* MAP NEW RECORD                */ 00356000
*         RCDAR1(1:MXM)=RCDAR1(1:MXM)&&RCDAR1(1:MXM);/* ZERO RCD     */ 00357000
         L     @10,CCAURAB(,R11)                                   0182 00358000
         L     @10,CCACPE1(,@10)                                   0182 00359000
