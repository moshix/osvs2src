*         ;                         /* CHECK IF BASE EXTENSION       */ 00305000
*       ELSE                        /* NOT BASE EXTENSION            */ 00306000
*         DO;                       /* CHECK FOR FREE BUFFER         */ 00307000
*           CCAURAB=CCARABF;        /* GET FIRST RCD AREA PTR        */ 00308000
         L     @10,CCARABF(,R11)                                   0167 00309000
         ST    @10,CCAURAB(,R11)                                   0167 00310000
*           CCAEGREC=OFF;           /* START LOOP                    */ 00311000
         NI    CCAEGREC(R11),B'11111110'                           0168 00312000
*           DO WHILE(CCAURAB<=CCARABL&/* FIND FREE BUFFER FOR NEW    */ 00313000
*                 CCAEGREC=OFF);    /* EXTENSION                     */ 00314000
         B     @DE00169                                            0169 00315000
@DL00169 DS    0H                                                  0170 00316000
*             IF CCARBUR=OFF&       /* FIND NON-BASE BUFFER          */ 00317000
*                 CCARBU1=OFF&      /* AVAILABLE FOR USE             */ 00318000
*                 CCARBU2=OFF&CCAURAB^=CCARABB THEN                0170 00319000
         L     @10,CCAURAB(,R11)                                   0170 00320000
         TM    CCARBUR(@10),B'11100000'                            0170 00321000
