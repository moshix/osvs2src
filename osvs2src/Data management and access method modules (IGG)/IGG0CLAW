         C     @10,CCARABB(,R11)                                   0170 00323000
         BE    @RF00170                                            0170 00324000
*               DO;                 /* BUFFER AVAILABLE              */ 00325000
*                 CCAEGREC=ON;      /* INDICATE FOUND                */ 00326000
         OI    CCAEGREC(R11),B'00000001'                           0172 00327000
*               END;                /* END BUFFER                    */ 00328000
*             ELSE                  /* UNAVAILABLE                   */ 00329000
*               CCAURAB=CCAURAB+LENGTH(CCARAB);/* GET NEXT RAB       */ 00330000
         B     @RC00170                                            0174 00331000
@RF00170 LA    @10,20                                              0174 00332000
         AL    @10,CCAURAB(,R11)                                   0174 00333000
         ST    @10,CCAURAB(,R11)                                   0174 00334000
*           END;                    /* END FIND                      */ 00335000
@RC00170 DS    0H                                                  0175 00336000
@DE00169 CLC   CCAURAB(4,R11),CCARABL(R11)                         0175 00337000
