*         NREPCV=0;                 /* NON-REPEATING CNT VAR         */ 00386000
         MVI   NREPCV(@05),X'00'                                   0191 00387000
*         CCACPE2=CCACPE1+NREPLF;   /* REPEATING CNTRL INFO PTR      */ 00388000
         ALR   @05,@15                                             0192 00389000
         ST    @05,CCACPE2(,@10)                                   0192 00390000
*         CCACPE3=CCACPE2+LENGTH(REPCNTRL);/* PTR FIRST POSSIBLE   0193 00391000
*                                      OCCURRENCE                    */ 00392000
         AL    @05,@CF00059                                        0193 00393000
         ST    @05,CCACPE3(,@10)                                   0193 00394000
*         CCACPE4=CCACPE3;          /* END OF RECORD                 */ 00395000
         ST    @05,CCACPE4(,@10)                                   0194 00396000
