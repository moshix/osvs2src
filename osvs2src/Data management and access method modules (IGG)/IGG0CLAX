         AL    @10,CCAILNG(,R11)                                   0144 00211000
         ST    @10,CCAIDPT2(,R11)                                  0144 00212000
*   END;                            /* END PROCESS FIELD LIST        */ 00213000
@RC00142 DS    0H                                                  0145 00214000
@DE00099 TM    CCAELC2(R11),B'00000100'                            0145 00215000
         BNZ   @DC00099                                            0145 00216000
         CLI   CCACD1(R11),0                                       0145 00217000
         BE    @DL00099                                            0145 00218000
@DC00099 DS    0H                                                  0146 00219000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00220000
         BAL   @14,EXIT                                            0146 00221000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00222000
         DS 0F                                                          00223000
*   GEN(DC CL8'IGGPMVAR');          /* NAME OF FOLLOWING PROCEDURE   */ 00224000
         DC CL8'IGGPMVAR'                                               00225000
*   END;                            /* END OF INNER PROCEDURE        */ 00226000
@EL00002 DS    0H                                                  0149 00227000
