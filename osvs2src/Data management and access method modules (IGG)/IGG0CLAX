@DE00118 TM    CCAGVNC(R11),B'00100000'                            0140 00192000
         BNO   @DC00118                                            0140 00193000
         CLI   CCACD1(R11),0                                       0140 00194000
         BE    @DL00118                                            0140 00195000
@DC00118 DS    0H                                                  0141 00196000
*       END;                        /* END PROCESS FIELD             */ 00197000
*     IF CTGFLDNO>1 THEN            /* SEE IF MORE ONE FIELD PTR     */ 00198000
@RC00112 L     @10,CCAFLPT(,R11)                                   0142 00199000
         CLI   CTGFLDNO(@10),1                                     0142 00200000
         BNH   @RF00142                                            0142 00201000
*       CCAIDPT2=CTGFLPT(1+CCACNIX);/* SET DATA PTR                  */ 00202000
         LH    @05,CCACNIX(,R11)                                   0143 00203000
         SLA   @05,3                                               0143 00204000
         L     @10,CTGFLPT(@05,@10)                                0143 00205000
         ST    @10,CCAIDPT2(,R11)                                  0143 00206000
*     ELSE                          /* ONE FIELD                     */ 00207000
*       CCAIDPT2=CCAIDPT2+CCAILNG;  /* SET DATA PTR                  */ 00208000
         B     @RC00142                                            0144 00209000
