*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01224000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 01225000
         STM 12,14,12(13)                                               01226000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA     0383 01227000
*                                      PROCEDURE                     */ 01228000
         LA 13,12(13)                                                   01229000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 01230000
**   MODULE NAME =  IGGPMBGO(IGG0CLAX)                                * 01231000
**                                                                    * 01232000
**   DESCRIPTIVE NAME =  MOVE BASE GROUP OCCURRENCE                   * 01233000
**                                                                    * 01234000
**   FUNCTION =                                                       * 01235000
**      THIS SUB-FUNCTION MOVES AN OCCURRENCE IN THE BASE RECORD INTO * 01236000
**      AN EXTENSION RECORD.  THE PROCESSING IS AS FOLLOWS:           * 01237000
**      1. IF THE AVAILABLE SPACE POINTER DOES NOT HAVE AN EXTENSION  * 01238000
**         CI NUMBER, CALL IGGPAXCI(IGG0CLAG) AND IGGPGNEX(IGG0CLAW)  * 01239000
**         TO OBTAIN THE EXTENSION WITH AVAILABLE SPACE.              * 01240000
**      2. CALL IGGPMGO TO MOVE THE OCCURRENCE TO THE EXTENSION RECORD* 01241000
**      3. IF THERE IS NO SPACE IN THE EXTENSION RECORD FOR THE       * 01242000
**         OCCURRENCE, CALL IGGPAXCI(IGG0CLAG) AND IGGPGNEX(IGG0CLAW) * 01243000
**         TO OBTAIN A NEW EXTENSION AND RE-CALL IGGPMGO TO SUCCESS-  * 01244000
**         FULLY MOVE THE OCCURRENCE.                                 * 01245000
**                                                                    * 01246000
**   INPUT =                                                          * 01247000
**      IGGCCA INPUT:                                                 * 01248000
**         CCACPE5 - ADDRESS OF GOP POINTING TO OCCURRENCE TO BE MOVED* 01249000
**      BASE RECORD ADDRESS                                           * 01250000
**                                                                    * 01251000
**   OUTPUT =                                                         * 01252000
**      GROUP OCCURRENCE MOVED FROM BASE RECORD TO EXTENSION RECORD.  * 01253000
**                                                                    * 01254000
**                                                                    * 01255000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01256000
*                                                                  0384 01257000
*   RFY                                                            0384 01258000
*     CCARAB BASED(CCARABB);        /* MAP BASE RAB                  */ 01259000
*   CCACPE51=CCACPE5;               /* AFFECTED OCCURRENCE           */ 01260000
         L     @10,CCACPE5(,R11)                                   0385 01261000
         ST    @10,CCACPE51(,R11)                                  0385 01262000
*   IF CATSPCI=0 THEN               /* NO AVAIL SPACE RCD CHECK      */ 01263000
         L     @10,CCARABB(,R11)                                   0386 01264000
         L     @10,CCACPE2(,@10)                                   0386 01265000
         ICM   @05,7,CATSPCI+6(@10)                                0386 01266000
         BNZ   @RF00386                                            0386 01267000
*     DO;                           /* GET NEW SPACE                 */ 01268000
*                                                                  0387 01269000
*       /*************************************************************/ 01270000
*       /*                                                           */ 01271000
*       /* CALL TO MODULE IGG0CLAG                                   */ 01272000
*       /*                                                           */ 01273000
*       /*************************************************************/ 01274000
*                                                                  0388 01275000
*Z027AXCI:                                                         0388 01276000
*       CALL IGGPAXCI;              /* ASSIGN CI                     */ 01277000
Z027AXCI L     @15,@CV00040                                        0388 01278000
         BALR  @14,@15                                             0388 01279000
*       CATSPCI=CCAASCI(1);         /* SAVE AVAIL SPACE CI           */ 01280000
*                                                                  0389 01281000
         L     @10,CCARABB(,R11)                                   0389 01282000
         L     @10,CCACPE2(,@10)                                   0389 01283000
         MVC   CATSPCI+6(3,@10),CCAASCI(R11)                       0389 01284000
*       /*************************************************************/ 01285000
*       /*                                                           */ 01286000
*       /* CALL TO MODULE IGG0CLAW                                   */ 01287000
*       /*                                                           */ 01288000
*       /*************************************************************/ 01289000
*                                                                  0390 01290000
*Z028GNEX:                                                         0390 01291000
*       CALL IGGPGNEX;              /* GET NEW EXTENSION             */ 01292000
Z028GNEX L     @15,@CV00036                                        0390 01293000
         BALR  @14,@15                                             0390 01294000
*     END;                          /* END GET NEW SPACE             */ 01295000
*   ELSE                            /* EXISTENT SPACE RCD            */ 01296000
*     DO;                           /* READ RECORD                   */ 01297000
         B     @RC00386                                            0392 01298000
@RF00386 DS    0H                                                  0393 01299000
*       CCASRID=CATSPCI;            /* SET SEARCH ARGUMENT           */ 01300000
*                                                                  0393 01301000
         L     @10,CCARABB(,R11)                                   0393 01302000
         L     @10,CCACPE2(,@10)                                   0393 01303000
         MVC   CCASRID(3,R11),CATSPCI+6(@10)                       0393 01304000
*       /*************************************************************/ 01305000
*       /*                                                           */ 01306000
*       /* CALL TO MODULE IGG0CLBA                                   */ 01307000
*       /*                                                           */ 01308000
*       /*************************************************************/ 01309000
*                                                                  0394 01310000
*Z029GREC:                                                         0394 01311000
*       CALL IGGPGREC;              /* GET RECORD                    */ 01312000
Z029GREC L     @15,@CV00037                                        0394 01313000
         BALR  @14,@15                                             0394 01314000
*     END;                          /* END READ RECORD               */ 01315000
*   CCARABPT=CCARABB;               /* SET BASE RAB ADDR             */ 01316000
@RC00386 L     @10,CCARABB(,R11)                                   0396 01317000
         ST    @10,CCARABPT(,R11)                                  0396 01318000
*   CCAONCE=ON;                     /* ONE MOVE                      */ 01319000
         OI    CCAONCE(R11),B'00001000'                            0397 01320000
*Z030MGO:                                                          0398 01321000
*   CALL IGGPMGO;                   /* MOVE OCCURRENCES              */ 01322000
Z030MGO  BAL   @14,IGGPMGO                                         0398 01323000
*   IF CCAMNOSP=ON THEN             /* IF NO SPACE                   */ 01324000
         TM    CCAMNOSP(R11),B'00100000'                           0399 01325000
         BNO   @RF00399                                            0399 01326000
*     DO;                           /* GET ADDITIONAL SPACE          */ 01327000
*                                                                  0400 01328000
*       /*************************************************************/ 01329000
*       /*                                                           */ 01330000
*       /* CALL TO MODULE IGG0CLAG                                   */ 01331000
*       /*                                                           */ 01332000
*       /*************************************************************/ 01333000
*                                                                  0401 01334000
*Z031AXCI:                                                         0401 01335000
*       CALL IGGPAXCI;              /* ASSIGN CI                     */ 01336000
Z031AXCI L     @15,@CV00040                                        0401 01337000
         BALR  @14,@15                                             0401 01338000
*       CATSPCI=CCAASCI(1);         /* SAVE SPACE CI                 */ 01339000
*                                                                  0402 01340000
         L     @10,CCARABB(,R11)                                   0402 01341000
         L     @10,CCACPE2(,@10)                                   0402 01342000
         MVC   CATSPCI+6(3,@10),CCAASCI(R11)                       0402 01343000
*       /*************************************************************/ 01344000
*       /*                                                           */ 01345000
*       /* CALL TO MODULE IGG0CLAW                                   */ 01346000
*       /*                                                           */ 01347000
*       /*************************************************************/ 01348000
*                                                                  0403 01349000
*Z032GNEX:                                                         0403 01350000
*       CALL IGGPGNEX;              /* FORMAT NEW RECORD             */ 01351000
Z032GNEX L     @15,@CV00036                                        0403 01352000
         BALR  @14,@15                                             0403 01353000
*Z033MGO:                                                          0404 01354000
*       CALL IGGPMGO;               /* MOVE BASE OCCURR              */ 01355000
Z033MGO  BAL   @14,IGGPMGO                                         0404 01356000
*     END;                          /* END GET ADD SPACE             */ 01357000
*   ELSE                                                           0406 01358000
*     ;                             /* MOVE OCCURR COMPLETE          */ 01359000
@RF00399 DS    0H                                                  0407 01360000
*   CCAONCE=OFF;                    /* END ONE MOVE                  */ 01361000
         NI    CCAONCE(R11),B'11110111'                            0407 01362000
*   RFY                                                            0408 01363000
*     CCARAB BASED(CCAURAB);        /* ORGINAL BASED                 */ 01364000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 01365000
         BAL   @14,EXIT                                            0409 01366000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 01367000
         DS 0F                                                          01368000
*   GEN(DC CL8'IGGPMGO');           /* NAME OF FOLLOWING PROCEDURE   */ 01369000
         DC CL8'IGGPMGO'                                                01370000
*   END;                            /* END OF INNER PROCEDURE        */ 01371000
@EL00006 DS    0H                                                  0412 01372000
@EF00006 DS    0H                                                  0412 01373000
@ER00006 BR    @14                                                 0412 01374000
@PB00006 DS    0H                                                  0413 01375000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 01376000
         EJECT                                                          01377000
*                                                                  0414 01378000
*/********************************************************************/ 01379000
*/*                                                                  */ 01380000
*/*        IGGPMGO  -  PROCEDURE  EXTERNALLY  CALLED  NESTED         */ 01381000
*/*                                                                  */ 01382000
*/********************************************************************/ 01383000
*                                                                  0414 01384000
*IGGPMGO:                                                          0414 01385000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 01386000
         B     @PB00007                                            0414 01387000
IGGPMGO  DS    0H                                                  0415 01388000
*   GEN(ENTRY IGGPMGO );            /* IDENTIFY ENTRY-POINT SYMBOL   */ 01389000
         ENTRY IGGPMGO                                                  01390000
*Z034:                              /* LABEL FOR PROCEDURE TRACKING  */ 01391000
*Z034MGO:                           /* LABEL FOR PROCEDURE TRACKING  */ 01392000
*   RFY                                                            0416 01393000
*    (R5,                                                          0416 01394000
*     R6,                                                          0416 01395000
*     R7,                                                          0416 01396000
*     R8,                                                          0416 01397000
*     R9,                                                          0416 01398000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 01399000
Z034     DS    0H                                                  0416 01400000
Z034MGO  DS    0H                                                  0417 01401000
*   RFY                                                            0417 01402000
*    (R2,                                                          0417 01403000
*     R3,                                                          0417 01404000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01405000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 01406000
         STM 12,14,12(13)                                               01407000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 01408000
         LA 13,12(13)                                                   01409000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 01410000
         BALR 12,0                                                      01411000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 01412000
         LA 15,*-IGGSEGST(0,0)                                          01413000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */ 01414000
         SR 12,15                                                       01415000
*/* PROCEDURE                                                        */ 01416000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 01417000
**   MODULE NAME =  IGGPMGO (IGG0CLAX)                                * 01418000
**                                                                    * 01419000
**   DESCRIPTIVE NAME =  MOVE GROUP OCCURRENCES                       * 01420000
**                                                                    * 01421000
**   FUNCTION =                                                       * 01422000
**      THIS SUB-FUNCTION MOVES OCCURRENCES FROM ONE CATALOG RECORD TO* 01423000
**      ANOTHER CATALOG RECORD.  THE PROCESSING IS AS FOLLOWS:        * 01424000
**      1. MOVE ONE OCCURRENCE AT A TIME BY SCANNING THE GOP'S OF THE * 01425000
**         RECORD CONTAINING THE OCCURRENCES TO BE MOVED (FIRST       * 01426000
**         RECORD).                                                   * 01427000
**      2. DO NOT MOVE OCCURRENCES WHICH ARE IN EXTENSION RECORDS,    * 01428000
**         WHICH ARE DELETED, WHICH HAVE AN ASSOCIATION OR AN AMDSB   * 01429000
**         GROUP CODE.                                                * 01430000
**      3. COMPUTE THE LENGTH OF THE OCCURRENCE TO BE MOVED.          * 01431000
**      4. IF THERE IS NO ROOM IN THE SECOND RECORD FOR THIS          * 01432000
**         OCCURRENCE, EXIT INDICATING THIS CONDITION.                * 01433000
**      5. CALL IGGPSGOP(IGG0CLAV) TO FIND A PLACE FOR THE NEW GOP IN * 01434000
**         THE SECOND RECORD. CALL IGGPIGOP(IGG0CLAW) TO INSERT THIS  * 01435000
**         NEW GOP. CALL IGGPCGO TO COPY THE OLD OCCURRENCE IN THE    * 01436000
**         FIRST RECORD INTO THE SECOND RECORD. AND CALL IGGPDGO TO   * 01437000
**         DELETE THE OLD OCCURRENCE IN THE FIRST RECORD.             * 01438000
**      6. IF THE OLD GROUP OCCURRENCE WAS NOT IN A BASE RECORD, CALL * 01439000
**         IGGPDGOP TO DELETE THE OLD GROUP OCCURRENCE POINTER, AND   * 01440000
**         UPDATE THE BASE RECORD CONTAINING THE MOVED OCCURRENCES.   * 01441000
**         OTHERWISE, ONLY UPDATE THE BASE RECORD GOP.                * 01442000
**                                                                    * 01443000
**   INPUT =                                                          * 01444000
**      IGGCCA INPUT:                                                 * 01445000
**         CCARABPT - ADDRESS OF RAB WITH OCCURRENCES TO BE MOVED.    * 01446000
**         CCACPE51 - ADDRESS OF GOP FOR FIRST OCCURRENCE TO BE MOVED.* 01447000
**         CCAURAB - ADDRESS OF RAB WHERE OCCURRENCES ARE TO BE MOVED * 01448000
**                   TO.                                              * 01449000
**         CCAONCE - IF ON, ONLY ONE OCCURRENCE IS MOVED.             * 01450000
**                                                                    * 01451000
**   OUTPUT =                                                         * 01452000
**      IGGCCA OUTPUT:                                                * 01453000
**         CCAMNOSP - IF ON, INDICATE THAT MORE OCCURRENCES MUST BE   * 01454000
**                    MOVED, BUT THERE WAS NO MORE SPACE IN THE RECORD* 01455000
**                    ADDRESSED BY CCARABPT.                          * 01456000
**                                                                    * 01457000
**                                                                    * 01458000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01459000
*                                                                  0423 01460000
*   DCL                                                            0423 01461000
*     VOLRCDID CHAR(1) CONSTANT('V');/* VOL RCD ID                   */ 01462000
*   DCL                                                            0424 01463000
*     CATGOPT1 CHAR(6) BASED(CCACPE5);/* GROUP OCCURR PTR            */ 01464000
*                                                                  0424 01465000
*   /*****************************************************************/ 01466000
*   /*                                                               */ 01467000
*   /* DECLARATION OF FIELD INFO FOR MOVE                            */ 01468000
*   /*                                                               */ 01469000
*   /*****************************************************************/ 01470000
*                                                                  0425 01471000
*   RFY                                                            0425 01472000
*     CATGOPT BASED(CCACPE51);      /* MAP GOP                       */ 01473000
*   DCL                                                            0426 01474000
*     CATASCGC FIXED(31) CONSTANT(2);/* ASSOC GRP CODE               */ 01475000
*   DCL                                                            0427 01476000
*     CATDSBGC FIXED(31) CONSTANT(1);/* DSB GRP CODE                 */ 01477000
*   CCAMNOSP=OFF;                   /* ASSUME SPACE                  */ 01478000
         NI    CCAMNOSP(R11),B'11011111'                           0428 01479000
*   CCAMEX=OFF;                     /* ENTER LOOP                    */ 01480000
         NI    CCAMEX(R11),B'10111111'                             0429 01481000
*   RFY                                                            0430 01482000
*     CCARAB BASED(CCARABPT);       /* MAP RAB TO BE COPIED          */ 01483000
*   DO WHILE(CCACPE51<CCACPE3&CCAMEX=OFF);/* MOVE GROUP OCCURRENCES  */ 01484000
         B     @DE00431                                            0431 01485000
@DL00431 DS    0H                                                  0432 01486000
*     IF CATGOEXT=ON|CATGODEL=ON|   /* IF EXT OR DELETED RO          */ 01487000
*         CATGOGC1=CATASCGC|CATGOGC1=CATDSBGC THEN                 0432 01488000
         L     @10,CCACPE51(,R11)                                  0432 01489000
         TM    CATGOEXT(@10),B'11000000'                           0432 01490000
         BNZ   @RT00432                                            0432 01491000
         CLI   CATGOGC1(@10),2                                     0432 01492000
         BE    @RT00432                                            0432 01493000
         CLI   CATGOGC1(@10),1                                     0432 01494000
         BE    @RT00432                                            0432 01495000
*       ;                           /* ASSOC OR DSB                  */ 01496000
*     ELSE                          /* RIGHT GROUP CODE              */ 01497000
*       DO;                         /* MOVE OCCURRENCE               */ 01498000
*                                                                  0434 01499000
*         /***********************************************************/ 01500000
*         /*                                                         */ 01501000
*         /* GET SEQUENCE NO. OF ONE OR TWO BYTES                    */ 01502000
*         /*                                                         */ 01503000
*         /***********************************************************/ 01504000
*                                                                  0435 01505000
*         CCAGRHI=0;                /* ZERO SEQ. NO.                 */ 01506000
         SLR   @10,@10                                             0435 01507000
         STH   @10,CCAGRHI(,R11)                                   0435 01508000
*         CCAGRHI1(LENGTH(CCASN)+1-CCASL:LENGTH(CCASN))=CATSN(1:CCASL); 01509000
         LA    @10,3                                               0436 01510000
         SH    @10,CCASL(,R11)                                     0436 01511000
         ST    @10,@TF00001                                        0436 01512000
         ALR   @10,R11                                             0436 01513000
         LA    @05,2                                               0436 01514000
         SL    @05,@TF00001                                        0436 01515000
         L     @15,CCACPE51(,R11)                                  0436 01516000
         EX    @05,@SM02902                                        0436 01517000
*         CCAGRGC=(CATGOGC1&TRUEGC);/* SET GROUP CODE                */ 01518000
         LA    @10,63                                              0437 01519000
         SLR   @05,@05                                             0437 01520000
         IC    @05,CATGOGC1(,@15)                                  0437 01521000
         NR    @10,@05                                             0437 01522000
         STC   @10,CCAGRGC(,R11)                                   0437 01523000
*         DO;                       /* MOVE GO IF SPACE              */ 01524000
*           R2=CCACPE3+CATGODSP;    /* POINT TO OCCURRENCE           */ 01525000
         L     @10,CCARABPT(,R11)                                  0439 01526000
         ICM   R2,12,CATGODSP(@15)                                 0439 01527000
         SRA   R2,16                                               0439 01528000
         AL    R2,CCACPE3(,@10)                                    0439 01529000
*           RFY                                                    0440 01530000
*             VARFIELD BASED(R4);   /* MAP VARIABLE FIELD BY REG     */ 01531000
*           RFY                                                    0441 01532000
*             CNTRLINF BASED(R2);   /* MAP OCCURR CONTROL INFO       */ 01533000
*           R4=R2+LENFIX;           /* POINT TO FIRST VAR FIELD      */ 01534000
         LR    R4,R2                                               0442 01535000
         SLR   @10,@10                                             0442 01536000
         IC    @10,LENFIX(,R2)                                     0442 01537000
         ALR   R4,@10                                              0442 01538000
*           DO R3=1 TO CNTVAR;      /* POINT END OCCURR              */ 01539000
         LA    R3,1                                                0443 01540000
         B     @DE00443                                            0443 01541000
@DL00443 DS    0H                                                  0444 01542000
*             R4=R4+VARLEN+LENGTH(VARLEN);/* POINT END VAR FIELD     */ 01543000
         LR    @10,R4                                              0444 01544000
         ICM   @05,12,VARLEN(R4)                                   0444 01545000
         SRA   @05,16                                              0444 01546000
         ALR   @10,@05                                             0444 01547000
         AL    @10,@CF00082                                        0444 01548000
         LR    R4,@10                                              0444 01549000
*           END;                    /* END POINT                     */ 01550000
         AL    R3,@CF00080                                         0445 01551000
@DE00443 SLR   @10,@10                                             0445 01552000
         IC    @10,CNTVAR(,R2)                                     0445 01553000
         CR    R3,@10                                              0445 01554000
         BNH   @DL00443                                            0445 01555000
*           R3=R4-R2;               /* LENGTH OCCURR                 */ 01556000
         LR    R3,R4                                               0446 01557000
         SLR   R3,R2                                               0446 01558000
*           RFY                                                    0447 01559000
*             CCARAB BASED(CCAURAB);/* MAP RAB FOR COPY              */ 01560000
*           IF RCDSIZ+R3+CCAGOPLN>MXRCD/* IF OCCURRENCE AND          */ 01561000
*             THEN                  /* POINTER NOT FIT               */ 01562000
         L     @10,CCAURAB(,R11)                                   0448 01563000
         L     @10,CCACPE1(,@10)                                   0448 01564000
         ICM   @10,12,RCDSIZ(@10)                                  0448 01565000
         SRA   @10,16                                              0448 01566000
         ALR   @10,R3                                              0448 01567000
         LH    @05,CCAGOPLN(,R11)                                  0448 01568000
         ALR   @10,@05                                             0448 01569000
         C     @10,@CF00055                                        0448 01570000
         BNH   @RF00448                                            0448 01571000
*             DO;                   /* SET UP EXIT                   */ 01572000
*               CCAMEX=ON;          /* EXIT INDICATOR                */ 01573000
         OI    CCAMEX(R11),B'01000000'                             0450 01574000
*               CCAMNOSP=ON;        /* NO SPACE                      */ 01575000
         OI    CCAMNOSP(R11),B'00100000'                           0451 01576000
*               CCACPE51=CCACPE51-CCAGOPLN;/* SET UP FOR NEXT CALL   */ 01577000
