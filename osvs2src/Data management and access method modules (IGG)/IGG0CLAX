*   GEN(ENTRY IGGPEXPD );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 00809000
         ENTRY IGGPEXPD                                                 00810000
*Z024:                              /* LABEL FOR PROCEDURE TRACKING  */ 00811000
*Z024EXPD:                          /* LABEL FOR PROCEDURE TRACKING  */ 00812000
*   RFY                                                            0297 00813000
*    (R5,                                                          0297 00814000
*     R6,                                                          0297 00815000
*     R7,                                                          0297 00816000
*     R8,                                                          0297 00817000
*     R9,                                                          0297 00818000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00819000
Z024     DS    0H                                                  0297 00820000
Z024EXPD DS    0H                                                  0298 00821000
*   RFY                                                            0298 00822000
*    (R2,                                                          0298 00823000
*     R3,                                                          0298 00824000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00825000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00826000
         STM 12,14,12(13)                                               00827000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00828000
         LA 13,12(13)                                                   00829000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 00830000
         BALR 12,0                                                      00831000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 00832000
         LA 15,*-IGGSEGST(0,0)                                          00833000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE     0303 00834000
*                                      PROCEDURE                     */ 00835000
         SR 12,15                                                       00836000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00837000
**   MODULE NAME =  IGGPEXPD(IGG0CLAX)                                * 00838000
**                                                                    * 00839000
**   DESCRIPTIVE NAME =  EXPAND CATALOG FIELD                         * 00840000
**                                                                    * 00841000
**   FUNCTION =                                                       * 00842000
**      THIS SUB-FUNCTION EXPANDS A VARIABLE LENGTH CATALOG FIELD BY  * 00843000
**      DELETING A CERTAIN PORTION AND ADDING A LARGER PORTION.  THE  * 00844000
**      PROCESSING IS AS FOLLOWS:                                     * 00845000
**      1. SHIFT THE CATALOG RECORD TO THE RIGHT AT THE POINT IN THE  * 00846000
**         VARIABLE LENGTH FIELD WHERE THE DELETE/INSERT IS TO OCCUR. * 00847000
**      2. UPDATE THE CATALOG POINTERS AND SIZES.                     * 00848000
**      3. INSERT THE USER DATA.                                      * 00849000
**                                                                    * 00850000
**   INPUT =                                                          * 00851000
**      IGGCCA INPUT:                                                 * 00852000
**         CCAURAB - RAB ADDRESS OF CATALOG RECORD CONTAINING FIELD   * 00853000
**                   TO BE EXPANDED.                                  * 00854000
**         CCACPE7 - ADDRESS OF VARIABLE LENGTH FIELD IN THE CATALOG  * 00855000
**                   RECORD.                                          * 00856000
**         CCAIDPT2 - ADDRESS OF USER INSERT INFORMATION              * 00857000
**         CCADISP - DISPLACEMENT INTO VARIABLE LENGTH FIELD WHERE    * 00858000
**                   DELETE/INSERT IS TO OCCUR.                       * 00859000
**         CCADLEN - LENGTH OF VARIABLE INFORMATION TO BE DELETED.    * 00860000
**         CCADIFF - DIFFERENCE BETWEEN INSERT LENGTH AND DELETE      * 00861000
**                   LENGTH.                                          * 00862000
**         CCAILNG2 - USER INSERT DATA LENGTH.                        * 00863000
**                                                                    * 00864000
**   OUTPUT =                                                         * 00865000
**      EXPAND OPERATION COMPLETED                                    * 00866000
**      IGGCCA OUTPUT:                                                * 00867000
**         CCARBWR - SET ON IN RAB ADDRESSED BY CCAURAB.              * 00868000
**                                                                    * 00869000
**                                                                    * 00870000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00871000
*                                                                  0304 00872000
*   RFY                                                            0304 00873000
*     RCDMV1 BASED(R3-R4);          /* DATA TO BE MOVED              */ 00874000
*   RFY                                                            0305 00875000
*     RCDMV2 BASED(R3);             /* MOVE AREA                     */ 00876000
*   R3=CCACPE7+CCADISP+CCADLEN;     /* POINT TO START OF MOVE        */ 00877000
         LH    R3,CCADISP(,R11)                                    0306 00878000
         AL    R3,CCACPE7(,R11)                                    0306 00879000
         AH    R3,CCADLEN(,R11)                                    0306 00880000
*   R4=CCADIFF;                     /* ADDED LENGTH                  */ 00881000
         LH    R4,CCADIFF(,R11)                                    0307 00882000
*   R2=CCACPE4-R3+R4;               /* GET MOVE LENGTH               */ 00883000
         L     @10,CCAURAB(,R11)                                   0308 00884000
         L     R2,CCACPE4(,@10)                                    0308 00885000
         SLR   R2,R3                                               0308 00886000
         ALR   R2,R4                                               0308 00887000
*   DO WHILE(R2>R4&R4^=0);          /* SHIFT RECORD FOR VAR F1D      */ 00888000
         B     @DE00309                                            0309 00889000
@DL00309 DS    0H                                                  0310 00890000
*     RCDMV2(R2-R4+1:R2)=           /* MOVE RECORD                   */ 00891000
*         RCDMV1(R2-R4+1:R2);       /* MOVE RECORD                   */ 00892000
         LR    @10,R2                                              0310 00893000
         SLR   @10,R4                                              0310 00894000
         ST    @10,@TF00001                                        0310 00895000
         ALR   @10,R3                                              0310 00896000
         LR    @05,R2                                              0310 00897000
         SL    @05,@TF00001                                        0310 00898000
         BCTR  @05,0                                               0310 00899000
         LR    @15,R3                                              0310 00900000
         SLR   @15,R4                                              0310 00901000
         AL    @15,@TF00001                                        0310 00902000
         EX    @05,@SM02892                                        0310 00903000
*     R2=R2-R4;                     /* DECR SEGMENT MOVE             */ 00904000
         SLR   R2,R4                                               0311 00905000
*   END;                            /* END SHIFT                     */ 00906000
@DE00309 CR    R2,R4                                               0312 00907000
         BNH   @DC00309                                            0312 00908000
         LTR   R4,R4                                               0312 00909000
         BNZ   @DL00309                                            0312 00910000
@DC00309 DS    0H                                                  0313 00911000
*   R2=CCACPE7-CCACPE3;             /* DISPLACEMENT TO VAR LEN FIELD */ 00912000
         L     @10,CCAURAB(,R11)                                   0313 00913000
         L     R2,CCACPE7(,R11)                                    0313 00914000
         SL    R2,CCACPE3(,@10)                                    0313 00915000
*   RFY                                                            0314 00916000
*     CATGOPT BASED(R3);            /* GOP MAP                       */ 00917000
*   R3=ADDR(REPGOPT);               /* POINT TO FIRST GOP            */ 00918000
         L     R3,CCACPE2(,@10)                                    0315 00919000
         LA    R3,REPGOPT(,R3)                                     0315 00920000
*   DO CCAGVCT=1 TO REPCNT;         /* UPDATE DISPLACEMENTS          */ 00921000
         LA    @10,1                                               0316 00922000
         B     @DE00316                                            0316 00923000
@DL00316 DS    0H                                                  0317 00924000
*     IF CATGOEXT=OFF&CATGODEL=OFF& /* IF THIS IS NON-DELETED,       */ 00925000
*         CATGODSP>R2 THEN          /* NON-EXT GOP FOR UPDATE        */ 00926000
         TM    CATGOEXT(R3),B'11000000'                            0317 00927000
         BNZ   @RF00317                                            0317 00928000
         ICM   @10,12,CATGODSP(R3)                                 0317 00929000
         SRA   @10,16                                              0317 00930000
         CR    @10,R2                                              0317 00931000
         BNH   @RF00317                                            0317 00932000
*       CATGODSP=CATGODSP+R4;       /* RESET DISPLACEMENT            */ 00933000
         ALR   @10,R4                                              0318 00934000
         STCM  @10,3,CATGODSP(R3)                                  0318 00935000
*     ELSE                                                         0319 00936000
*       ;                           /* NO UPDATE GOP                 */ 00937000
@RF00317 DS    0H                                                  0320 00938000
*     R3=R3+CCAGOPLN;               /* GET NEXT GOP                  */ 00939000
         AH    R3,CCAGOPLN(,R11)                                   0320 00940000
*   END;                            /* END UPDATE                    */ 00941000
         LA    @10,1                                               0321 00942000
         AH    @10,CCAGVCT(,R11)                                   0321 00943000
@DE00316 STH   @10,CCAGVCT(,R11)                                   0321 00944000
         L     @05,CCAURAB(,R11)                                   0321 00945000
         L     @05,CCACPE2(,@05)                                   0321 00946000
         SLR   @15,@15                                             0321 00947000
         IC    @15,REPCNT(,@05)                                    0321 00948000
         CR    @10,@15                                             0321 00949000
         BNH   @DL00316                                            0321 00950000
*   RCDSIZ=RCDSIZ+R4;               /* UPDATE RECORD SIZE            */ 00951000
         L     @10,CCAURAB(,R11)                                   0322 00952000
         L     @05,CCACPE1(,@10)                                   0322 00953000
         ICM   @15,12,RCDSIZ(@05)                                  0322 00954000
         SRA   @15,16                                              0322 00955000
         ALR   @15,R4                                              0322 00956000
         STCM  @15,3,RCDSIZ(@05)                                   0322 00957000
*   CCACPE4=CCACPE4+R4;             /* UPDATE END RECORD PTR         */ 00958000
         LR    @05,R4                                              0323 00959000
         AL    @05,CCACPE4(,@10)                                   0323 00960000
         ST    @05,CCACPE4(,@10)                                   0323 00961000
*   VARLEN=VARLEN+CCADIFF;          /* UPDATE LENGTH FIELD           */ 00962000
         L     @05,CCACPE7(,R11)                                   0324 00963000
         ICM   @15,12,VARLEN(@05)                                  0324 00964000
         SRA   @15,16                                              0324 00965000
         AH    @15,CCADIFF(,R11)                                   0324 00966000
         STCM  @15,3,VARLEN(@05)                                   0324 00967000
*   CCARBWR=ON;                     /* MUST WRITE THIS RECORD        */ 00968000
         OI    CCARBWR(@10),B'00010000'                            0325 00969000
*   IF CCAILNG2^=0 THEN             /* IS THERE INSERT DATA          */ 00970000
         L     @10,CCAILNG2(,R11)                                  0326 00971000
         LTR   @10,@10                                             0326 00972000
         BZ    @RF00326                                            0326 00973000
*     VARINFO2(1:CCAILNG2)=VARINFO1(1:CCAILNG2);/* PUT IN INSERT     */ 00974000
         AH    @05,CCADISP(,R11)                                   0327 00975000
         BCTR  @10,0                                               0327 00976000
         L     @01,CCAIDPT2(,R11)                                  0327 00977000
         EX    @10,@SM02894                                        0327 00978000
*   ELSE                                                           0328 00979000
*     ;                             /* NO DATA                       */ 00980000
@RF00326 DS    0H                                                  0329 00981000
*   RFY                                                            0329 00982000
*     CATGOPT BASED(CCACPE5);       /* ORIGINAL BASE                 */ 00983000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00984000
         BAL   @14,EXIT                                            0330 00985000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00986000
         DS 0F                                                          00987000
*   GEN(DC CL8'IGGPSHNK');          /* NAME OF FOLLOWING PROCEDURE   */ 00988000
         DC CL8'IGGPSHNK'                                               00989000
*   END;                            /* END OF INNER PROCEDURE        */ 00990000
@EL00004 DS    0H                                                  0333 00991000
@EF00004 DS    0H                                                  0333 00992000
@ER00004 BR    @14                                                 0333 00993000
@PB00004 DS    0H                                                  0334 00994000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 00995000
         EJECT                                                          00996000
*                                                                  0335 00997000
*/********************************************************************/ 00998000
*/*                                                                  */ 00999000
*/*        IGGPSHNK  -  PROCEDURE  EXTERNALLY  CALLED  NESTED        */ 01000000
*/*                                                                  */ 01001000
*/********************************************************************/ 01002000
*                                                                  0335 01003000
*IGGPSHNK:                                                         0335 01004000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 01005000
         B     @PB00005                                            0335 01006000
IGGPSHNK DS    0H                                                  0336 01007000
*   GEN(ENTRY IGGPSHNK );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 01008000
         ENTRY IGGPSHNK                                                 01009000
