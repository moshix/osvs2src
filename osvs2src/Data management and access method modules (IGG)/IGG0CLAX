*     ELSE                                                         0365 01148000
*       ;                           /* NOT GOP FOR UPDATE            */ 01149000
@RF00363 DS    0H                                                  0366 01150000
*     R3=R3+CCAGOPLN;               /* GET NEXT GOP                  */ 01151000
         AH    R3,CCAGOPLN(,R11)                                   0366 01152000
*   END;                            /* END UPDATE GOP5               */ 01153000
         LA    @10,1                                               0367 01154000
         AH    @10,CCAGVCT(,R11)                                   0367 01155000
@DE00362 STH   @10,CCAGVCT(,R11)                                   0367 01156000
         L     @05,CCAURAB(,R11)                                   0367 01157000
         L     @05,CCACPE2(,@05)                                   0367 01158000
         SLR   @15,@15                                             0367 01159000
         IC    @15,REPCNT(,@05)                                    0367 01160000
         CR    @10,@15                                             0367 01161000
         BNH   @DL00362                                            0367 01162000
*   VARLEN=VARLEN+CCADIFF;          /* UPDATE LEN FIELD              */ 01163000
