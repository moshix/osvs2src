*   CCACPE4=CCACPE4+CCADIFF;        /* UPDATE END POINTER            */ 01122000
         AL    @15,CCACPE4(,@10)                                   0358 01123000
         ST    @15,CCACPE4(,@10)                                   0358 01124000
*   R2=CCACPE7-CCACPE3;             /* SET DISPLACEMENT              */ 01125000
         L     R2,CCACPE7(,R11)                                    0359 01126000
         SL    R2,CCACPE3(,@10)                                    0359 01127000
*   RFY                                                            0360 01128000
*     CATGOPT BASED(R3);            /* MAP GOP                       */ 01129000
*   R3=ADDR(REPGOPT);               /* POINT TO FIRST GOP            */ 01130000
         L     R3,CCACPE2(,@10)                                    0361 01131000
         LA    R3,REPGOPT(,R3)                                     0361 01132000
*   DO CCAGVCT=1 TO REPCNT;         /* UPDATE GOPS                   */ 01133000
         LA    @10,1                                               0362 01134000
         B     @DE00362                                            0362 01135000
@DL00362 DS    0H                                                  0363 01136000
*     IF CATGOEXT=OFF&CATGODEL=OFF& /* IF NON-DELETED, NON-EXT       */ 01137000
*         CATGODSP>R2 THEN          /* GOP FOR UPDATE                */ 01138000
         TM    CATGOEXT(R3),B'11000000'                            0363 01139000
         BNZ   @RF00363                                            0363 01140000
         ICM   @10,12,CATGODSP(R3)                                 0363 01141000
         SRA   @10,16                                              0363 01142000
         CR    @10,R2                                              0363 01143000
         BNH   @RF00363                                            0363 01144000
*       CATGODSP=CATGODSP+CCADIFF;  /* SET NEW DISPLACEMENT          */ 01145000
         AH    @10,CCADIFF(,R11)                                   0364 01146000
