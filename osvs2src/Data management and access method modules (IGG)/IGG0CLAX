*     ;                             /* NO DATA                       */ 01182000
@RF00370 DS    0H                                                  0373 01183000
*   RFY                                                            0373 01184000
*     CATGOPT BASED(CCACPE5);       /* ORIGINAL BASE                 */ 01185000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 01186000
         BAL   @14,EXIT                                            0374 01187000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 01188000
         DS 0F                                                          01189000
*   GEN(DC CL8'IGGPMBGO');          /* NAME OF FOLLOWING PROCEDURE   */ 01190000
         DC CL8'IGGPMBGO'                                               01191000
*   END;                            /* END OF INNER PROCEDURE        */ 01192000
@EL00005 DS    0H                                                  0377 01193000
@EF00005 DS    0H                                                  0377 01194000
@ER00005 BR    @14                                                 0377 01195000
@PB00005 DS    0H                                                  0378 01196000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 01197000
         EJECT                                                          01198000
*                                                                  0379 01199000
*/********************************************************************/ 01200000
*/*                                                                  */ 01201000
*/*        IGGPMBGO  -  PROCEDURE  INTERNALLY  CALLED  NESTED        */ 01202000
*/*                                                                  */ 01203000
*/********************************************************************/ 01204000
*                                                                  0379 01205000
*IGGPMBGO:                                                         0379 01206000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 01207000
         B     @PB00006                                            0379 01208000
IGGPMBGO DS    0H                                                  0380 01209000
*Z026:                              /* LABEL FOR PROCEDURE TRACKING  */ 01210000
*Z026MBGO:                          /* LABEL FOR PROCEDURE TRACKING  */ 01211000
*   RFY                                                            0380 01212000
*    (R5,                                                          0380 01213000
*     R6,                                                          0380 01214000
*     R7,                                                          0380 01215000
*     R8,                                                          0380 01216000
*     R9,                                                          0380 01217000
