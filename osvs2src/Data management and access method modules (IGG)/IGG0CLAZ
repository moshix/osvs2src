*   CCAEXT=ON;                      /* INDICATE EXTRACT FUNCTION     */ 00031000
*   CCAMOD=OFF;                     /* NOT MODIFY FUNCTION           */ 00032000
*                                                                  0117 00033000
         OI    CCAEXT(R11),B'00000010'                             0117 00034000
         NI    CCAMOD(R11),B'11111110'                             0117 00035000
*   /*****************************************************************/ 00036000
*   /*                                                               */ 00037000
*   /* INVOKE SCAN CATALOG PARM LIST SUB-FUNCTION CALL TO MODULE     */ 00038000
*   /* IGG0CLAY                                                      */ 00039000
