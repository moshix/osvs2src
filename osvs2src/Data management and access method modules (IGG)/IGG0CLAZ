*               CCACD2=RCVLSZ;      /* SET ERROR CODE                */ 01401000
         MVI   CCACD2(R11),X'28'                                   0412 01402000
*             END;                  /* END SET UP                    */ 01403000
*           ELSE                                                   0414 01404000
*             ;                     /* NO PARTIAL MOVE               */ 01405000
