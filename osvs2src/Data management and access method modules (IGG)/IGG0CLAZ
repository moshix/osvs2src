*               CTGFLDNO=1;         /* SET NUMBER ARRAY ELT          */ 01479000
         MVI   CTGFLDNO(@10),X'01'                                 0436 01480000
*             END;                  /* END SET FIELD LIST INFO       */ 01481000
*           CCAIDPT2=CCAIDPT2+CCAILNG;/* UPDATE AVAIL SPACE PTR      */ 01482000
@RC00429 L     @10,CCAIDPT2(,R11)                                  0438 01483000
         AL    @10,CCAILNG(,R11)                                   0438 01484000
         ST    @10,CCAIDPT2(,R11)                                  0438 01485000
*         END;                      /* END PUT INFO IN WORK AREA     */ 01486000
*       ELSE                        /* NO ROOM IN WORK AREA          */ 01487000
*         DO;                       /* PROCESS SHORT AREA            */ 01488000
         B     @RC00400                                            0440 01489000
@RF00400 DS    0H                                                  0441 01490000
*           CCACD2=RCVLSZ;          /* SET RETURN CODE               */ 01491000
         MVI   CCACD2(R11),X'28'                                   0441 01492000
*         END;                      /* END PROCESS SHORT AREA        */ 01493000
*     END;                          /* END INSERT                    */ 01494000
*   ELSE                                                           0444 01495000
*     ;                             /* LENGTH ZERO                   */ 01496000
