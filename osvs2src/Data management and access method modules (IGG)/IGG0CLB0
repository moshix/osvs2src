*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */ 00585000
*                                                                  0303 00586000
         SR 12,15                                                       00587000
*   /*****************************************************************/ 00588000
*   /*                                                               */ 00589000
*   /* DETERMINE BEST PHYSICAL BLOCK SIZE INDEX VALUE.       @ZA05929*/ 00590000
*   /* WORKAREA IS IN CCAURAB5. INPUT IS DATA CISIZE         @ZA05929*/ 00591000
*   /* IN CMDCONV. OUTPUT IS INDEX VALUE INTO PHYSICAL       @ZA05929*/ 00592000
*   /* BLKSIZE TABLE AND NUMBER OF BLKS PER TRK TABLE.       @ZA05929*/ 00593000
*   /* OUTPUT NAME IS CMPBI. A TABLE OF TRACK                @ZA05929*/ 00594000
*   /* CAPACITIES IS BUILT BY MULTIPLYING PHYSICAL           @ZA05929*/ 00595000
*   /* BLOCKSIZES BY NUMBER OF BLOCKS PER TRACK FOR          @ZA05929*/ 00596000
*   /* EACH PHYSICAL BLOCKSIZE THAT WILL EVENLY DIVIDE       @ZA05929*/ 00597000
*   /* INTO THE INPUT DATA CISIZE. THEN PICK THE             @ZA05929*/ 00598000
*   /* LARGEST VALUE IN THIS TABLE. IF THERE IS              @ZA05929*/ 00599000
*   /* IDENTICAL VALUES THEN PICK THE ONE WITH THE           @ZA05929*/ 00600000
*   /* LARGEST PHYSICAL BLOCKSIZE.                           @ZA05929*/ 00601000
*   /*                                                               */ 00602000
*   /*****************************************************************/ 00603000
*                                                                  0304 00604000
*   CCAURAB=ADDR(CCARAB5);          /* POINT TO WORK AREA    @ZA05929*/ 00605000
         LA    @10,CCARAB5(,R11)                                   0304 00606000
         ST    @10,CCAURAB(,R11)                                   0304 00607000
*   BLKVOVLY=''B;                   /* ZERO ARRAY            @ZA05929*/ 00608000
         L     @10,CCACPE1(,@10)                                   0305 00609000
         NC    BLKVOVLY(16,@10),@CB03532                           0305 00610000
*   DCWREG=DCWABASE;                /* GET DEV CHAR BASE     @ZA05929*/ 00611000
         ST    DCWABASE,DCWREG(,@10)                               0306 00612000
*   RFY                                                            0307 00613000
*     DSPTBL BASED(DCWREG);         /* REBASE DEV CHAR       @ZA05929*/ 00614000
*   DO CMPBI=ONE TO FOUR;           /* PROCESS 4 BLKSIZES    @ZA05929*/ 00615000
         LA    @10,1                                               0308 00616000
         L     @05,CCACMSWA(,R11)                                  0308 00617000
         ST    @10,CMPBI(,@05)                                     0308 00618000
@DL00308 DS    0H                                                  0309 00619000
*     IF CMDCONV//PHYBLKS(CMPBI)=N&CMDCONV/PHYBLKS(CMPBI)^=N THEN/*     00620000
*                                      VALID BLKSIZE         @ZA05929*/ 00621000
