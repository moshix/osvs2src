*   RFY                                                            0298 00573000
*    (R2,                                                          0298 00574000
*     R3,                                                          0298 00575000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00576000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00577000
         STM 12,14,12(13)                                               00578000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00579000
         LA 13,12(13)                                                   00580000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 00581000
         BALR 12,0                                                      00582000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 00583000
