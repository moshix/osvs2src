         L     @05,CCACMSWA(,R11)                                  0314 00658000
         ST    @10,CMPBI(,@05)                                     0314 00659000
         CH    @10,@CH00062                                        0314 00660000
         BNH   @DL00308                                            0314 00661000
*   /*****************************************************************/ 00662000
*   /*                                                               */ 00663000
*   /* FIND INDEX OF MAXIMUM VALUE IN THIS ARRAY.            @ZA05929*/ 00664000
*   /* IF VALUES ARE THE SAME THEN CHOOSE THE VALUE          @ZA05929*/ 00665000
*   /* WITH THE LARGEST PHYSICAL BLOCK SIZE                  @ZA05929*/ 00666000
*   /* TO REDUCE ACCESS TIME.                                @ZA05929*/ 00667000
*   /*                                                               */ 00668000
*   /*****************************************************************/ 00669000
*                                                                  0315 00670000
*   CMPBI=ONE;                      /* SET SEARCH INDEX      @ZA05929*/ 00671000
         L     @10,CCACMSWA(,R11)                                  0315 00672000
         MVC   CMPBI(4,@10),@CF00041                               0315 00673000
*   CMPBIX=TWO;                     /* SET SEARCH INDEX      @ZA05929*/ 00674000
         MVC   CMPBIX(4,@10),@CF00059                              0316 00675000
*   DO UNTIL CMPBIX>FOUR;           /* FIND MAX OF 4 VALUES  @ZA05929*/ 00676000
@DL00317 DS    0H                                                  0318 00677000
