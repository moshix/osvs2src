         BE    @RF00372                                            0372 01365000
         CLI   RCDID(@10),C'C'                                     0372 01366000
         BE    @RF00372                                            0372 01367000
         CLI   RCDID(@10),C'G'                                     0372 01368000
         BE    @RF00372                                            0372 01369000
*     DO;                           /* IF NOT, SET AN ERROR CODE     */ 01370000
*                                                                  0373 01371000
*       /*************************************************************/ 01372000
*       /*                                                           */ 01373000
*       /* SET CCAMODID='B1', CCAREASN=RSN40, CCARETRN=RCINENT       */ 01374000
*       /*                                                           */ 01375000
*       /*************************************************************/ 01376000
*                                                                  0374 01377000
*       CCAPROB=IGGSEGID+(RSN40*K256)+RCINENT;/* INVALID ENTRY TYPE  */ 01378000
         MVC   CCAPROB(4,R11),@CF03006                             0374 01379000
*       CALL EXIT;                  /* RETURN TO CALLER              */ 01380000
         BAL   @14,EXIT                                            0375 01381000
*     END;                          /* END ERROR TYPE PROCESSING     */ 01382000
*   ELSE                                                           0377 01383000
*     ;                             /* BASE RCD HAS VALID RCD TYPE   */ 01384000
*                                                                  0377 01385000
@RF00372 DS    0H                                                  0378 01386000
*   /*****************************************************************/ 01387000
*   /*                                                               */ 01388000
*   /* INSURE THERE ARE AT LEAST 3 FREE RABS BETWEEN CCARABF AND     */ 01389000
*   /* CCARABL, CCARABB IS FREE, AND LIES BETWEEN CCARABF AND        */ 01390000
*   /* CCARABL.                                                      */ 01391000
*   /*                                                               */ 01392000
*   /*****************************************************************/ 01393000
*                                                                  0378 01394000
*   R2=0;                           /* USE THIS REG TO COUNT FREE  0378 01395000
