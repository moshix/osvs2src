         MVC   CCAPROB(4,R11),@CF03003                             0219 00562000
*       ELSE                                                       0220 00563000
*         ;                         /* BACKOUT ERROR                 */ 00564000
@RF00218 DS    0H                                                  0221 00565000
*     END;                          /* END BACKOUT                   */ 00566000
*   ELSE                                                           0222 00567000
*     ;                             /* MAX NOT REACHED               */ 00568000
@RF00206 DS    0H                                                  0223 00569000
*Z027UEND:                                                         0223 00570000
*   CALL IGGPUEND;                  /* TERMINATE AND EXIT            */ 00571000
Z027UEND BAL   @14,IGGPUEND                                        0223 00572000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00573000
         BAL   @14,EXIT                                            0224 00574000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00575000
         DS 0F                                                          00576000
*   GEN(DC CL8'IGGPUDEL');          /* NAME OF FOLLOWING PROCEDURE   */ 00577000
         DC CL8'IGGPUDEL'                                               00578000
*   END;                            /* END OF INNER PROCEDURE        */ 00579000
@EL00002 DS    0H                                                  0227 00580000
@EF00002 DS    0H                                                  0227 00581000
@ER00002 BR    @14                                                 0227 00582000
@PB00002 DS    0H                                                  0228 00583000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 00584000
         EJECT                                                          00585000
*                                                                  0229 00586000
*/********************************************************************/ 00587000
*/*                                                                  */ 00588000
*/*        IGGPUDEL  -  PROCEDURE  EXTERNALLY  CALLED  NESTED        */ 00589000
*/*                                                                  */ 00590000
*/********************************************************************/ 00591000
*                                                                  0229 00592000
*IGGPUDEL:                                                         0229 00593000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 00594000
         B     @PB00003                                            0229 00595000
IGGPUDEL DS    0H                                                  0230 00596000
*   GEN(ENTRY IGGPUDEL );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 00597000
         ENTRY IGGPUDEL                                                 00598000
*Z028:                              /* LABEL FOR PROCEDURE TRACKING  */ 00599000
*Z028UDEL:                          /* LABEL FOR PROCEDURE TRACKING  */ 00600000
*   RFY                                                            0231 00601000
*    (R5,                                                          0231 00602000
*     R6,                                                          0231 00603000
*     R7,                                                          0231 00604000
*     R8,                                                          0231 00605000
*     R9,                                                          0231 00606000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00607000
Z028     DS    0H                                                  0231 00608000
Z028UDEL DS    0H                                                  0232 00609000
*   RFY                                                            0232 00610000
*    (R2,                                                          0232 00611000
*     R3,                                                          0232 00612000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00613000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00614000
         STM 12,14,12(13)                                               00615000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00616000
         LA 13,12(13)                                                   00617000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 00618000
         BALR 12,0                                                      00619000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 00620000
         LA 15,*-IGGSEGST(0,0)                                          00621000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE     0237 00622000
*                                      PROCEDURE                     */ 00623000
         SR 12,15                                                       00624000
*/********************************************************************  00625000
**                                                                   *  00626000
**   MODULE NAME = IGGPUDEL (IGG0CLB1)                               *  00627000
**                                                                   *  00628000
**   DESCRIPTIVE NAME = DELETE UPGRADE ASSOCIATION                   *  00629000
**                                                                   *  00630000
**   FUNCTION =                                                      *  00631000
**      THIS SUB-FUNCTION IS EXTERNALLY CALLED BY SUCH CMS ROUTINES  *  00632000
**      AS DELETE AND ALTER. IT DELETES UPGRADE ASSOCIATIONS FROM    *  00633000
**      THE UPGRADE ENTRY AND ALSO IMPLICITLY DELETES THE UPGRADE    *  00634000
**      ENTRY IF IT BECOMES EMPTY. THE CALLER MAY ALSO EXPLICITLY    *  00635000
**      REQUEST THAT THE UPGRADE ENTRY BE DELETED EVEN IF IT IS NOT  *  00636000
**      EMPTY (FORCE DELETE). THE PROCESSING IS AS FOLLOWS:          *  00637000
**       - CALL IGGPUCOM TO VALIDITY CHECK THE CALLER'S INPUT        *  00638000
**         PARAMETERS, PERFORM INITIALIZATION, AND READ THE          *  00639000
**         UPGRADE ENTRY INTO CORE,  PROVIDED THIS IS NOT A FORCE    *  00640000
**         DELETE OF THE UPGRADE ENTRY.                              *  00641000
**       - IF THIS IS NOT A FORCE DELETE OF THE UPGRADE ENTRY, CALL  *  00642000
**         MODIFY (IGGPMOD) TO DELETE THE CALLER SPECIFIED UPGRADE   *  00643000
**         ASSOCIATION. IF THIS DELETE FAILS, INDICATE SO (CUMNE).   *  00644000
**         CALL EXTRACT (IGGPEXT) TO DETERMINE WHETHER THE UPGRADE   *  00645000
**         ENTRY IS EMPTY. IF NOT EMPTY, RETURN TO THE CALLER.       *  00646000
**         OTHERWISE, DO THE NEXT STEP.                              *  00647000
**       - IF THIS IS A FORCE DELETE OR IF THE UPGRADE ENTRY IS      *  00648000
**         EMPTY, DO THE FOLLOWING COMMON PROCESSING:   IF THE       *  00649000
**         BASE DATA RECORD IS NOT ALREADY IN-CORE, READ IT INTO     *  00650000
**         CORE. CALL MODIFY TO DELETE THE Y-TYPE ASSOCIATION IN     *  00651000
**         THE BASE DATA RECORD WHICH POINTS TO THE UPGRADE ENTRY.   *  00652000
**         READ THE BASE UPGRADE RECORD INTO CORE, IF NECESSARY.     *  00653000
**         DELETE THE BASE UPGRADE RECORD AND ALL HORIZONTAL         *  00654000
**         EXTENSIONS.                                               *  00655000
**                                                                   *  00656000
**   INPUT =                                                         *  00657000
**      THE FOLLOWING FIELDS ARE IN THE CUMPLWA DECLARATION:         *  00658000
**        CUMDCI - 3-BYTE AIX DATA CI NUMBER.                        *  00659000
**        CUMICI - 3-BYTE AIX INDEX CI NUMBER.                       *  00660000
**        CUMFDEL - IF THIS BIT IS SET ON, THE UPGRADE ENTRY IS      *  00661000
**          DELETED EVEN IF IT IS NON-EMPTY.                         *  00662000
**      COMMON (TO IGGPUADD) INPUT:                                  *  00663000
**        CUMNRG - IF THIS FLAG IS SET ON, NO RE-GET OF THE ORIGINAL *  00664000
**          BASE ENTRY RECORD IS DONE.                               *  00665000
**        THE CALLER MUST PROVIDE CONTIGUOUS CORE FOR THE CATALOG    *  00666000
**        UPGRADE MANAGEMENT PARAMETER LIST AND FOR THE CATALOG      *  00667000
**        UPGRADE MANAGEMENT WORK AREA (MAPPED BY CUMPLWA). CCACUMPL *  00668000
**        MUST CONTAIN THE ADDRESS OF THIS CORE.                     *  00669000
**        THE USER MUST SPECIFY AT LEAST 3 FREE RECORD AREAS WHICH   *  00670000
**        WILL BE USED BY CATALOG UPGRADE MANAGEMENT TO PERFORM      *  00671000
**        ITS I/O OPERATIONS. IN PARTICULAR,                         *  00672000
**        CCARABF - ADDRESS OF THE FIRST RECORD AREA BLOCK WHICH     *  00673000
**          CATALOG UPGRADE MANAGEMENT MAY POSSIBLY USE.             *  00674000
**        CCARABL - ADDRESS OF THE LAST RECORD AREA BLOCK WHICH      *  00675000
**          CATALOG UPGRADE MANAGEMENT MAY POSSIBLY USE.             *  00676000
**        CCARABB - ADDRESS OF THE RECORD AREA BLOCK WHICH POINTS TO *  00677000
**          THE BASE RECORD OF THE ENTRY. THIS BASE RECORD MUST      *  00678000
**          HAVE AN ENTRY TYPE OF G, C, OR D. ALSO, CCARABB MUST     *  00679000
**          LIE BETWEEN CCARABF AND CCARABL.                         *  00680000
**        CCARNUR (WHERE N=1,2,3,4, OR 5) - IF THIS BIT IS SET ON IN *  00681000
**          A RECORD AREA BLOCK, IT INDICATES THE ASSOCIATED RECORD  *  00682000
**          AREA IS NOT FREE FOR CATALOG UPGRADE MGMT'S USE.         *  00683000
**                                                                   *  00684000
**   OUTPUT =                                                        *  00685000
**      THE FOLLOWING FIELDS ARE IN THE CUMPLWA DECLARATION:         *  00686000
**        CUMNE - THIS BIT IS SET ON, IF THE UPGRADE ASSOCIATION TO  *  00687000
**          BE DELETED IS NON-EXISTENT OR THE UPGRADE ENTRY ITSELF   *  00688000
**          IS NON-EXISTENT.                                         *  00689000
**      COMMON (TO IGGPUADD) OUTPUT:                                 *  00690000
**        FLAG CCAREUSE IS SET OFF.                                  *  00691000
**        THE CONTENTS OF THE USER'S CCAURAB IS SAVED AND RESTORED.  *  00692000
**        THE ORIGINAL BASE RECORD REFERENCED BY CCARABB IS          *  00693000
**          RESTORED.                                                *  00694000
**        IN GENERAL CATALOG UPGRADE MGMT WILL NOT CHANGE INPUT      *  00695000
**          FIELDS SO THAT ITS INTERFACE IS COMPLETELY REUSABLE      *  00696000
**          AFTER A PARTICULAR CALL.                                 *  00697000
**        ERROR CODES - THE MAIN ERROR CODE IS RETURNED IN CCACD1.   *  00698000
**          THE POSSIBLE RETURN CODES ARE DESCRIBED BELOW. A         *  00699000
**          SECONDARY ERROR CODE IS RETURNED IN CCACD2. THIS FIELD   *  00700000
**          WILL CONTAIN THE RETURN CODE RECEIVED BACK FROM GET      *  00701000
**          WHEN THE CALLER'S BASE RECORD IS RESTORED. ANY NON-ZERO  *  00702000
**          CODE IN CCACD2 IS PROPAGATED TO CCACD1, PROVIDED OF      *  00703000
**          COURSE, CCACD1 IS NOT ALREADY NON-ZERO.                  *  00704000
**            ANY ERROR CODE RETURNED BY MODIFY OR EXTRACT           *  00705000
**            ANY ERROR CODE RETURNED BY THE I/O ROUTINES            *  00706000
**              (GET, PDE)                                           *  00707000
**            RCINENT - IF THE BASE RECORD ENTRY TYPE IS NOT G, C, OR*  00708000
**              D.                                                   *  00709000
**            RCINCNPM - IF THE RAB REQUIREMENTS ARE NOT MET.        *  00710000
**            RCCATBAL - IF CATALOG UPGRADE MGMT ENCOUNTERS A        *  00711000
**              STRUCTURAL ERROR.                                    *  00712000
**                                                                   *  00713000
******************************************************************** */ 00714000
*                                                                  0238 00715000
*   CUMFUNC=OFF;                    /* INDICATE THIS IS DELETE TO  0238 00716000
*                                      IGGPUCOM                      */ 00717000
*                                                                  0238 00718000
         L     @10,CCACUMPL(,R11)                                  0238 00719000
         NI    CUMFUNC(@10),B'11011111'                            0238 00720000
*   /*****************************************************************/ 00721000
*   /*                                                               */ 00722000
*   /* CALL THE COMMON UPGRADE MANAGEMENT INITIALIZATION ROUTINE     */ 00723000
*   /* (IGGPUCOM) TO PERFORM VALIDITY CHECKING AGAINST THE CALLER'S  */ 00724000
*   /* INPUT PARAMETERS, TO PERFORM INITIALIZATION, AND TO READ THE  */ 00725000
*   /* UPGRADE ENTRY INTO CORE. THE UPGRADE ENTRY IS NOT READ INTO   */ 00726000
*   /* CORE IF THIS IS A FORCE DELETE OF THE UPGRADE ENTRY, SINCE THE*/ 00727000
*   /* BASE DATA RECORD MUST BE PROCESSED FIRST.                     */ 00728000
*   /*                                                               */ 00729000
*   /*****************************************************************/ 00730000
*                                                                  0239 00731000
*Z029UCOM:                                                         0239 00732000
*   CALL IGGPUCOM;                  /* CALL THE COMMON INITIALIZATION   00733000
*                                      ROUTINE                       */ 00734000
Z029UCOM BAL   @14,IGGPUCOM                                        0239 00735000
*   IF CUMNE=ON|CCACD1^=RCS THEN    /* IF THE UPGRADE ENTRY DOES NOT    00736000
*                                      EXIST OR AN ERROR OCCURRED  0240 00737000
*                                      THEN                          */ 00738000
         L     @10,CCACUMPL(,R11)                                  0240 00739000
         TM    CUMNE(@10),B'01000000'                              0240 00740000
         BO    @RT00240                                            0240 00741000
         CLI   CCACD1(R11),0                                       0240 00742000
         BE    @RF00240                                            0240 00743000
@RT00240 DS    0H                                                  0241 00744000
*Z030UEND:                                                         0241 00745000
*     CALL IGGPUEND;                /* PERFORM TERMINATION PROCESSING   00746000
*                                      AND EXIT                      */ 00747000
Z030UEND BAL   @14,IGGPUEND                                        0241 00748000
*   ELSE                                                           0242 00749000
*     ;                             /* THE UPGRADE ENTRY EXISTS AND     00750000
*                                      THERE WERE NO ERRORS          */ 00751000
@RF00240 DS    0H                                                  0243 00752000
*   IF CUMFDEL=OFF THEN             /* IF THIS IS NOT A FORCE DELETE    00753000
*                                      OF THE UPGRADE ENTRY          */ 00754000
         L     @10,CCACUMPL(,R11)                                  0243 00755000
         TM    CUMFDEL(@10),B'10000000'                            0243 00756000
         BNZ   @RF00243                                            0243 00757000
*     DO;                           /* NORMAL DELETE OF UPGRADE    0244 00758000
*                                      ASSOCIATION                   */ 00759000
*                                                                  0244 00760000
*       /*************************************************************/ 00761000
*       /*                                                           */ 00762000
*       /* BUILD THE MODIFY INTERFACE TO DELETE THE UPGRADE          */ 00763000
*       /* ASSOCIATION SPECIFIED BY THE CALLER                       */ 00764000
*       /*                                                           */ 00765000
*       /*************************************************************/ 00766000
*                                                                  0245 00767000
*       RFY                                                        0245 00768000
*         CTGPL BASED(CCAMCPL);     /* MAP CPL AREA                  */ 00769000
*       RFY                                                        0246 00770000
*         CTGFL BASED(CTGFIELD(1)); /* MAP FIRST FPL FOR           0246 00771000
*                                      INITIALIZATION                */ 00772000
*       CTGFLNG(1)=0;               /* NO DATA LENGTH                */ 00773000
         SLR   @05,@05                                             0247 00774000
         L     @01,CCAMCPL(,R11)                                   0247 00775000
         L     @15,CTGFIELD(,@01)                                  0247 00776000
         ST    @05,CTGFLNG(,@15)                                   0247 00777000
*       CTGFLPT(1)=0;               /* NO DATA ADDRESS               */ 00778000
         ST    @05,CTGFLPT(,@15)                                   0248 00779000
