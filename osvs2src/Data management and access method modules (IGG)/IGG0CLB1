         TM    CCARBUR(@10),B'10000000'                            0386 01428000
         BO    @RT00386                                            0386 01429000
         C     @10,CCARABF(,R11)                                   0386 01430000
         BL    @RT00386                                            0386 01431000
         C     @10,CCARABL(,R11)                                   0386 01432000
         BNH   @RF00386                                            0386 01433000
@RT00386 DS    0H                                                  0387 01434000
*     DO;                           /* SET ERROR CODE, EXIT          */ 01435000
*                                                                  0387 01436000
*       /*************************************************************/ 01437000
*       /*                                                           */ 01438000
*       /* SET CCAMODID='B1', CCAREASN=RSN52, CCARETRN=RCINCNPM      */ 01439000
*       /*                                                           */ 01440000
*       /*************************************************************/ 01441000
*                                                                  0388 01442000
