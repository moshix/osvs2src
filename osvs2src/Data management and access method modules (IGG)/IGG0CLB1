         MVC   CUMBCI(3,@10),RCDCINO(@05)                          0366 01332000
*   CUMDUP=OFF;                     /* ASSUME NO DUPLICATE UPGRADE 0367 01333000
*                                      ASSOCIATION                   */ 01334000
*   CUMNE=OFF;                      /* ASSUME UPGRADE ENTRY IS     0368 01335000
*                                      EXISTENT AND UPGRADE        0368 01336000
*                                      ASSOCIATION (FOR DELETION) IS    01337000
*                                      EXISTENT                      */ 01338000
*                                                                  0368 01339000
         NI    CUMDUP(@10),B'00111111'                             0368 01340000
*   /*****************************************************************/ 01341000
*   /*                                                               */ 01342000
*   /* THE FOLLOWING CODE VALIDITY CHECKS THE COMMON INPUT PARAMETERS*/ 01343000
*   /* SUPPLIED TO THE UPGRADE ASSOCIATION ADD SUB-FUNCTION          */ 01344000
*   /* (IGGPUADD) AND THE UPGRADE ASSOCIATION DELETE SUB-FUNCTION    */ 01345000
*   /* (IGGPUDEL).                                                   */ 01346000
*   /*                                                               */ 01347000
*   /*****************************************************************/ 01348000
*                                                                  0369 01349000
*   IF CCACD1^=RCS THEN             /* CHECK WHETHER CCACD1 IS     0369 01350000
*                                      NON-ZERO                      */ 01351000
         CLI   CCACD1(R11),0                                       0369 01352000
         BE    @RF00369                                            0369 01353000
*     CALL EXIT;                    /* IT IS, CALLER ERROR, EXIT     */ 01354000
         BAL   @14,EXIT                                            0370 01355000
*   ELSE                                                           0371 01356000
*     ;                             /* CCACD1 IS ZERO                */ 01357000
@RF00369 DS    0H                                                  0372 01358000
*   IF^(RCDID=DATAID|RCDID=CLUSID|  /* INSURE THAT THE BASE RCD      */ 01359000
*       RCDID=AIXID) THEN           /* HAS THE DATA, CLUSTER, OR AIX    01360000
*                                      RECORD TYPE                   */ 01361000
         L     @10,CCARABB(,R11)                                   0372 01362000
         L     @10,CCACPE1(,@10)                                   0372 01363000
