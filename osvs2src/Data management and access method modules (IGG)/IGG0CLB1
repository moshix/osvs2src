*                                      AND EXIT                      */ 01191000
Z051UEND BAL   @14,IGGPUEND                                        0346 01192000
*       ELSE                                                       0347 01193000
*         ;                         /* NO GET ERROR                  */ 01194000
@RF00345 DS    0H                                                  0348 01195000
*       GOTO A1;                    /* PROCESS EXTENSION DELETE      */ 01196000
         B     A1                                                  0348 01197000
*     END;                          /* END MUST                      */ 01198000
*   ELSE                                                           0350 01199000
*     ;                             /* NO HORIZONTAL EXTENSION       */ 01200000
@RF00336 DS    0H                                                  0351 01201000
*Z052UEND:                                                         0351 01202000
*   CALL IGGPUEND;                  /* TERMINATE AND EXIT            */ 01203000
Z052UEND BAL   @14,IGGPUEND                                        0351 01204000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 01205000
         BAL   @14,EXIT                                            0352 01206000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 01207000
         DS 0F                                                          01208000
