         TITLE 'IGG0CLB3, SMF GET/PUT FOR ALTER                        *00001000
                        '                                               00002000
IGG0CLB3 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGG0CLB3  75.084'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
         USING @MAINENT,@15                                             00011000
         USING @DATD-(CCATEMPS-IGGCCA),R11                              00012000
         DS 0F                                                          00013000
         DC CL8'IGGPSMF'                                                00014000
         EJECT                                                          00015000
         B     @PB00002                                            0076 00016000
IGGPSMF  DS    0H                                                  0077 00017000
         ENTRY IGGPSMF                                                  00018000
         DROP @15                                                       00019000
Z003     DS    0H                                                  0079 00020000
Z003SMF  DS    0H                                                  0080 00021000
         STM 12,14,12(13)                                               00022000
         LA 13,12(13)                                                   00023000
         BALR 12,0                                                      00024000
IGGSEGST DS 0H                                                          00025000
         USING *,12                                                     00026000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00027000
**   MODULE NAME =  IGGPSMF (IGG0CLB3)                                * 00028000
**                                                                    * 00029000
**   DESCRIPTIVE NAME =  SMF LOGICAL PUT OF ALTERED RECORDS           * 00030000
**                                                                    * 00031000
**   COPYRIGHT =  NONE                                                * 00032000
**                                                                    * 00033000
**   STATUS =  RELEASE 1, LEVEL 0                                     * 00034000
**                                                                    * 00035000
**   FUNCTION =                                                       * 00036000
**      THIS SUB-FUNCTION WORKS WITH IGGPSMFG TO SAVE PORTIONS OF THE * 00037000
**      OLD CATALOG ENTRY WHICH ARE TO BE ALTERED (FOR WRITING THE    * 00038000
**      ALTER SMF RECORD 63 BY IGGPSMFA).  IT IS CALLED TO INDICATE   * 00039000
**      THAT A USER RECORD, UPDATED BY AN I/O PUT, HAS BEEN ALTERED   * 00040000
**      AND SO THE OLD SAVED COPY IN THE SMF CHAIN SHOULD BE PRESERVED* 00041000
**                                                                    * 00042000
**   INPUT =                                                          * 00043000
**      IGGCCA INPUT:                                                 * 00044000
**         CCACD1 - NO PROCESSING UNLESS ZERO                         * 00045000
**         CCASMFA - INDICATES WHETHER THIS SMF ROUTINE SHOULD        * 00046000
**                   PROCESS.                                         * 00047000
**         CCAURAB - POINTS TO THE RAB OF THE CATALOG RECORD ALTERED. * 00048000
**                   NO PROCESSING IF VOLUME RECORD ID.               * 00049000
**         CCASMFPT - SMF RECORD CHAIN ADDRESS                        * 00050000
**                                                                    * 00051000
**   OUTPUT =                                                         * 00052000
**      IGGCCA OUTPUT:                                                * 00053000
**         CCASMFCT - INCREMENTED BY ONE, IF THE CATALOG RECORD IS    * 00054000
**                    PROCESSED.                                      * 00055000
**         SMF RECORD CHAIN ALTERED TO INDICATE PRESERVATION OF THE   * 00056000
**         OLD CATALOG RECORD, PROVIDED IT WAS A CANDIDATE FOR        * 00057000
**         PRESERVATION.                                              * 00058000
**                                                                    * 00059000
**                                                                    * 00060000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00061000
*                                                                  0086 00062000
*   IF CCASMFA=OFF|CCACD1^=RCS|     /* IF SMF INDICATOR OFF, OR      */ 00063000
*       RCDID=VOLEXTID|RCDID=VOLRCDID/* ERROR, OR VOLUME ENTRY       */ 00064000
*     THEN                                                         0086 00065000
         TM    CCASMFA(R11),B'00100000'                            0086 00066000
         BZ    @RT00086                                            0086 00067000
         CLI   CCACD1(R11),0                                       0086 00068000
         BNE   @RT00086                                            0086 00069000
         L     @10,CCAURAB(,R11)                                   0086 00070000
         L     @10,CCACPE1(,@10)                                   0086 00071000
         CLI   RCDID(@10),C'W'                                     0086 00072000
         BE    @RT00086                                            0086 00073000
         CLI   RCDID(@10),C'V'                                     0086 00074000
         BE    @RT00086                                            0086 00075000
*     ;                             /* DONT PROCESS SMF              */ 00076000
*   ELSE                            /* PROCESS ALTERED RECORD        */ 00077000
*     DO;                           /* SMF PROCESS                   */ 00078000
*       R2=(CCAURAB-ADDR(CCARAB0))/LENGTH(CCARAB);/* GET RELATIVE RAB   00079000
*                                      SEQUENCE NO.                  */ 00080000
         LA    R2,CCARAB0(,R11)                                    0089 00081000
         LR    @00,R2                                              0089 00082000
         LCR   @00,@00                                             0089 00083000
         AL    @00,CCAURAB(,R11)                                   0089 00084000
         SRDA  @00,32                                              0089 00085000
         D     @00,@CF00137                                        0089 00086000
         LR    R2,@01                                              0089 00087000
*       R3=CCASMFPT;                /* FIRST ELT IN CHAIN            */ 00088000
         L     R3,CCASMFPT(,R11)                                   0090 00089000
*       DO WHILE(R3^=0);            /* FIND RELATED RAB ELT          */ 00090000
         B     @DE00091                                            0091 00091000
@DL00091 DS    0H                                                  0092 00092000
*         IF R2=(SMFLAG&RELBITS) THEN/* TEST IS THIS RELATED RAB   0092 00093000
*                                      ELEMENT                       */ 00094000
         LA    @10,15                                              0092 00095000
         SLR   @05,@05                                             0092 00096000
         IC    @05,SMFLAG(,R3)                                     0092 00097000
         NR    @10,@05                                             0092 00098000
         CR    R2,@10                                              0092 00099000
         BNE   @RF00092                                            0092 00100000
*           IF SMFCAND=ON&          /* SEE IF RELATED RAB HAS        */ 00101000
*               R3->RCDCINO=RCDCINO THEN/* CAND RCD FOR      @Y30SSRS   00102000
*                                      PRESERVATION                  */ 00103000
         TM    SMFCAND(R3),B'01000000'                             0093 00104000
         BNO   @RF00093                                            0093 00105000
         L     @10,CCAURAB(,R11)                                   0093 00106000
         L     @10,CCACPE1(,@10)                                   0093 00107000
         CLC   RCDCINO(3,R3),RCDCINO(@10)                          0093 00108000
         BNE   @RF00093                                            0093 00109000
*             DO;                   /* SAVE RCD                      */ 00110000
*               SMFPRES=ON;         /* INDICATE RCD PRESERVATION     */ 00111000
         OI    SMFPRES(R3),B'10000000'                             0095 00112000
*               CCASMFCT=CCASMFCT+1;/* INCR COUNT PRESERVED RCDS     */ 00113000
         LA    @10,1                                               0096 00114000
         AH    @10,CCASMFCT(,R11)                                  0096 00115000
         STH   @10,CCASMFCT(,R11)                                  0096 00116000
*             END;                  /* END SAVE RECORD               */ 00117000
*           ELSE                                                   0098 00118000
*             ;                     /* NOT CANDIDATE RCD             */ 00119000
@RF00093 DS    0H                                                  0099 00120000
*         ELSE                                                     0099 00121000
*           ;                       /* NOT RELATED RAB ELT           */ 00122000
@RF00092 DS    0H                                                  0100 00123000
*         R3=SMFCHAIN;              /* NEXT ELT OF SMG CHAIN         */ 00124000
         ICM   R3,15,SMFCHAIN(R3)                                  0100 00125000
*       END;                        /* END FIND                      */ 00126000
@DE00091 LTR   R3,R3                                               0101 00127000
         BNZ   @DL00091                                            0101 00128000
*     END;                          /* END SMF PROCESS               */ 00129000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00130000
@RT00086 BAL   @14,EXIT                                            0103 00131000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00132000
         DS 0F                                                          00133000
*   GEN(DC CL8'IGGPSMFG');          /* NAME OF FOLLOWING PROCEDURE   */ 00134000
         DC CL8'IGGPSMFG'                                               00135000
*   END;                            /* END OF INNER PROCEDURE        */ 00136000
@EL00002 DS    0H                                                  0106 00137000
@EF00002 DS    0H                                                  0106 00138000
@ER00002 BR    @14                                                 0106 00139000
@PB00002 DS    0H                                                  0107 00140000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 00141000
         EJECT                                                          00142000
*                                                                  0108 00143000
*/********************************************************************/ 00144000
*/*                                                                  */ 00145000
*/*        IGGPSMFG  -  PROCEDURE  EXTERNALLY  CALLED  NESTED        */ 00146000
*/*                                                                  */ 00147000
*/********************************************************************/ 00148000
*                                                                  0108 00149000
*IGGPSMFG:                                                         0108 00150000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 00151000
         B     @PB00003                                            0108 00152000
IGGPSMFG DS    0H                                                  0109 00153000
*   GEN(ENTRY IGGPSMFG );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 00154000
         ENTRY IGGPSMFG                                                 00155000
*Z004:                              /* LABEL FOR PROCEDURE TRACKING  */ 00156000
*Z004SMFG:                          /* LABEL FOR PROCEDURE TRACKING  */ 00157000
*   RFY                                                            0110 00158000
*    (R5,                                                          0110 00159000
*     R6,                                                          0110 00160000
*     R7,                                                          0110 00161000
*     R8,                                                          0110 00162000
*     R9,                                                          0110 00163000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00164000
Z004     DS    0H                                                  0110 00165000
Z004SMFG DS    0H                                                  0111 00166000
*   RFY                                                            0111 00167000
*    (R2,                                                          0111 00168000
*     R3,                                                          0111 00169000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00170000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00171000
         STM 12,14,12(13)                                               00172000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00173000
         LA 13,12(13)                                                   00174000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 00175000
         BALR 12,0                                                      00176000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 00177000
         LA 15,*-IGGSEGST(0,0)                                          00178000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE     0116 00179000
*                                      PROCEDURE                     */ 00180000
         SR 12,15                                                       00181000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00182000
**   MODULE NAME =  IGGPSMFG (IGG0CLB3)                               * 00183000
**                                                                    * 00184000
**   DESCRIPTIVE NAME =  SMF LOGICAL GET FOR ALTERED RECORDS          * 00185000
**                                                                    * 00186000
**   COPYRIGHT =  NONE                                                * 00187000
**                                                                    * 00188000
**   STATUS =  RELEASE 1, LEVEL 0                                     * 00189000
**                                                                    * 00190000
**   FUNCTION =                                                       * 00191000
**      THIS SUB-FUNCTION WORKS WITH IGGPSMF TO SAVE PORTIONS OF THE  * 00192000
**      OLD CATALOG ENTRY WHICH ARE TO BE ALTERED (FOR WRITING THE    * 00193000
**      ALTER SMF RECORD 63 BY IGGPSMFA).  IT IS CALLED TO SAVE       * 00194000
**      CATALOG ENTRY RECORDS FOR POSSIBLE PRESERVATION (IF FIELDS    * 00195000
**      WITHIN THEM ARE ALTERED).  THE PROCESSING IS AS FOLLOWS:      * 00196000
**      1. CHECK WHETHER SMF PROCESSING SHOULD BE DONE FOR THIS       * 00197000
**         REQUEST.                                                   * 00198000
**      2. FORMAT THE SMF CHAIN, WHICH CONTAINS THE SAVED CATALOG     * 00199000
**         ENTRY RECORDS, IF IT HAS NOT BEEN INITIALIZED.             * 00200000
**      3. SAVE THE INPUT USER CATALOG ENTRY RECORD, GOTTEN BY AN     * 00201000
**         I/O GET, AS A CANDIDATE FOR PRESERVATION, PROVIDED IT IS   * 00202000
**         NOT ALREADY PRESERVED. CALL IGGPSMFZ TO ZERO ANY SECURITY  * 00203000
**         INFORMATION IN THE ENTRY.                                  * 00204000
**      4. CALL IGGPSMFF, IF CORE IS UNAVAILABLE, TO FREE ANY         * 00205000
**         PREVIOUSLY GOTTEN CORE.                                    * 00206000
**                                                                    * 00207000
**   INPUT =                                                          * 00208000
**      IGGCCA INPUT:                                                 * 00209000
**         CCACD1 - NO PROCESSING UNLESS ZERO                         * 00210000
**         CCASMFA - INDICATES WHETHER THIS SMF ROUTINE SHOULD PROCESS* 00211000
**         CCAURAB - POINTS TO THE RAB OF THE CATALOG RECORD TO BE    * 00212000
**                   SAVED. NO PROCESSING IF VOLUME RECORD ID.        * 00213000
**         CCARABF,CCARABL - FIRST, LAST RAB ADDRESSES WITH           * 00214000
**                   USABILITY BIT(CCARBUR) INITIALIZED.              * 00215000
**      SMF RECORD CHAIN                                              * 00216000
**      IEESMCA INPUT:                                                * 00217000
**         SMCADSA,SMCAUSER - NO PROCESSING UNLESS ON                 * 00218000
**                                                                    * 00219000
**   OUTPUT =                                                         * 00220000
**      IGGCCA OUTPUT:                                                * 00221000
**         CCACD1 - USED TO DETECT ERRORS IN THIS ROUTINE. ZERO AT    * 00222000
**                  EXIT.                                             * 00223000
**         CCASMFCD - ERROR RETURN CODE SAVED FROM CCACD1:            * 00224000
**                    RCINSSWA - MAIN CORE STORAGE UNAVAILABLE        * 00225000
**      SMF RECORD CHAIN INITIALIZED, IF PREVIOUSLY NON-EXISTENT      * 00226000
**      SMF RECORD CHAIN INITIALIZED WITH INPUT CATALOG RECORD,       * 00227000
**          IF NOT ALREADY PRESERVED.                                 * 00228000
**                                                                    * 00229000
**                                                                    * 00230000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00231000
*                                                                  0117 00232000
*   IF CCASMFA=OFF|CCACD1^=RCS|     /* IF SMF INDICATOR OFF, OR      */ 00233000
*       RCDID=VOLEXTID|RCDID=VOLRCDID|/* ERROR, OR VOLUME ENTRY      */ 00234000
*       CVTSMCA=0|CCASMFCD^=RCS|    /* OR PREVIOUS ERROR, OR         */ 00235000
*       SMCADSA=OFF|SMCAUSER=OFF THEN                              0117 00236000
         TM    CCASMFA(R11),B'00100000'                            0117 00237000
         BZ    @RT00117                                            0117 00238000
         CLI   CCACD1(R11),0                                       0117 00239000
         BNE   @RT00117                                            0117 00240000
         L     @10,CCAURAB(,R11)                                   0117 00241000
         L     @10,CCACPE1(,@10)                                   0117 00242000
         CLI   RCDID(@10),C'W'                                     0117 00243000
         BE    @RT00117                                            0117 00244000
         CLI   RCDID(@10),C'V'                                     0117 00245000
         BE    @RT00117                                            0117 00246000
         L     @10,CVTPTR                                          0117 00247000
         L     @10,CVTSMCA(,@10)                                   0117 00248000
         LTR   @10,@10                                             0117 00249000
         BZ    @RT00117                                            0117 00250000
         CLI   CCASMFCD(R11),0                                     0117 00251000
         BNE   @RT00117                                            0117 00252000
         TM    SMCADSA(@10),B'00010000'                            0117 00253000
         BZ    @RT00117                                            0117 00254000
         TM    SMCAUSER(@10),B'10000000'                           0117 00255000
         BZ    @RT00117                                            0117 00256000
*     ;                             /* NO SMF IN SYSTEM, DONT PROCESS   00257000
*                                      SMF                           */ 00258000
*   ELSE                            /* PROCESS SMF                   */ 00259000
*     DO;                           /* PROCESS SMF FOR ALTER/ UPDATE */ 00260000
*                                                                  0119 00261000
*       /*************************************************************/ 00262000
*       /*                                                           */ 00263000
*       /* TO SAVE RECORDS THAT MAY BE ALTERED, RELATED RECORD AREAS */ 00264000
*       /* IN AN SMF RECORD CHAIN OFF THE IGGCCA MUST BE FORMATTED   */ 00265000
*       /* FOR RABS SUPPLIED BY THE USER.                            */ 00266000
*       /*                                                           */ 00267000
*       /*************************************************************/ 00268000
*                                                                  0120 00269000
*       IF CCASMFPT=0 THEN          /* IF CHAIN NOT INITIALIZED      */ 00270000
         L     @10,CCASMFPT(,R11)                                  0120 00271000
         LTR   @10,@10                                             0120 00272000
         BNZ   @RF00120                                            0120 00273000
*         DO;                       /* FORMAT SMF CHAIN              */ 00274000
*           RFY                                                    0122 00275000
*             R1 RSTD;              /* GETMAIN REG                   */ 00276000
*           CCAMNLEN=LEN3RAB;       /* LENGTH CORE FOR SMF CHAIN     */ 00277000
         MVC   CCAMNLEN(3,R11),@CF00048+1                          0123 00278000
*           R1=ADDR(CCAMSSPL);      /* POINT TO STRG MGMT P.L.       */ 00279000
         LA    R1,CCAMSSPL(,R11)                                   0124 00280000
*           CCAMNSPL=USUBPOOL;      /* STORE SUBPOOL VALUE           */ 00281000
*                                                                  0125 00282000
         MVI   CCAMNSPL(R11),X'FC'                                 0125 00283000
*           /*********************************************************/ 00284000
*           /*                                                       */ 00285000
*           /* ALLOCATE MAIN STORAGE FROM USER SUBPOOL 252           */ 00286000
*           /*                                                       */ 00287000
*           /*********************************************************/ 00288000
*                                                                  0126 00289000
*           GEN(GETMAIN LC,MF=(E,(1)));/* GET MAIN STORAGE SPACE     */ 00290000
         GETMAIN LC,MF=(E,(1))                                          00291000
*           GEN(LR R1,R15);         /* PICK UP RETURN CODE           */ 00292000
         LR R1,R15                                                      00293000
*           IF R1^=NOGETMSE THEN    /* CHECK FOR ERROR RETURN        */ 00294000
*                                                                  0128 00295000
         LTR   R1,R1                                               0128 00296000
         BZ    @RF00128                                            0128 00297000
*             /*******************************************************/ 00298000
*             /*                                                     */ 00299000
*             /* SET CCAMODID='B3', CCAREASN=RSN2, CCARETRN=RCINSSWA */ 00300000
*             /*                                                     */ 00301000
*             /*******************************************************/ 00302000
*                                                                  0129 00303000
*             CCAPROB=IGGSEGID+(RSN2*K256)+RCINSSWA;/* INSUFFICIENT     00304000
*                                      CORE ERROR                    */ 00305000
         MVC   CCAPROB(4,R11),@CF02826                             0129 00306000
*           ELSE                    /* GETMAIN SUCCESSFUL            */ 00307000
*             DO;                                                  0130 00308000
         B     @RC00128                                            0130 00309000
@RF00128 DS    0H                                                  0131 00310000
*               DO;                                                0131 00311000
*                 IF MNATFULL=ON THEN/* TRACKING TABLE FULL          */ 00312000
*                                                                  0132 00313000
         TM    MNATFULL(R11),B'10000000'                           0132 00314000
         BNO   @RF00132                                            0132 00315000
*                   /*************************************************/ 00316000
*                   /*                                               */ 00317000
*                   /* CALL TO MODULE IGG0CLAT                       */ 00318000
*                   /*                                               */ 00319000
*                   /*************************************************/ 00320000
*                                                                  0133 00321000
*Z008ARFS:                                                         0133 00322000
*                   CALL IGGPARFS;  /* YES,CALL SPILL ROUTINE        */ 00323000
Z008ARFS L     @15,@CV00038                                        0133 00324000
         BALR  @14,@15                                             0133 00325000
*                 GEN REFS(CCAMNAT)(MVC CCAMNAT(248,11),CCAMNAT+8(11)); 00326000
@RF00132 DS    0H                                                  0134 00327000
         MVC CCAMNAT(248,11),CCAMNAT+8(11)                              00328000
*                                   /* PUSH AND TRACK                */ 00329000
*               END;                                               0135 00330000
*             END;                  /* GET MAIN STORAGE              */ 00331000
*           RFY                                                    0137 00332000
*             R1 UNRSTD;            /* FREE REG                      */ 00333000
@RC00128 DS    0H                                                  0138 00334000
*           IF CCACD1^=RCS THEN                                    0138 00335000
         CLI   CCACD1(R11),0                                       0138 00336000
         BNE   @RT00138                                            0138 00337000
*             ;                     /* ERROR CHECK                   */ 00338000
*           ELSE                    /* CORE GOTTEN SUCCESSFULLY      */ 00339000
*             DO;                   /* SET UP RELATED RAB ELTS       */ 00340000
*               CCASMFPT=CCAMNADR;  /* POINT FIRST ELT SMF CHAIN     */ 00341000
         L     @10,CCAMNADR(,R11)                                  0141 00342000
         ST    @10,CCASMFPT(,R11)                                  0141 00343000
*               R3=CCAMNADR;        /* POINT FIRST ELT SMF CHAIN     */ 00344000
         LR    R3,@10                                              0142 00345000
*               R2=CCARABF;         /* FIRST RECORD AREA ADDR        */ 00346000
         L     R2,CCARABF(,R11)                                    0143 00347000
*               DO WHILE(R3<LEN3RAB+CCASMFPT);/* INITLZ 3 RELATED RAB   00348000
*                                      ELTS                          */ 00349000
         B     @DE00144                                            0144 00350000
@DL00144 DS    0H                                                  0145 00351000
*                 SMFLAG=(R2-ADDR(CCARAB0))/LENGTH(CCARAB);/* SET  0145 00352000
*                                      RELATED RAB SEQ. NO.          */ 00353000
         LA    @10,20                                              0145 00354000
         LA    @00,CCARAB0(,R11)                                   0145 00355000
         LCR   @00,@00                                             0145 00356000
         ALR   @00,R2                                              0145 00357000
         SRDA  @00,32                                              0145 00358000
         DR    @00,@10                                             0145 00359000
         STC   @01,SMFLAG(,R3)                                     0145 00360000
*                 SMFELTSZ=0;       /* CORE SIZE INITIALLY ZERO      */ 00361000
         SLR   @05,@05                                             0146 00362000
         STCM  @05,3,SMFELTSZ(R3)                                  0146 00363000
*                 SMFCHAIN=R3+LENGTH(SMFELT);/* POINT NEXT ELEMENT   */ 00364000
         LA    @05,512                                             0147 00365000
         LR    @15,R3                                              0147 00366000
         ALR   @15,@05                                             0147 00367000
         STCM  @15,15,SMFCHAIN(R3)                                 0147 00368000
*                 R3=R3+LENGTH(SMFELT);/* ESTABLISH BASE NEXT ELT    */ 00369000
         ALR   R3,@05                                              0148 00370000
*                 RFY                                              0149 00371000
*                   CCARAB BASED(R2);/* MAP RAB                      */ 00372000
*                 R2=R2+LENGTH(CCARAB);/* NEXT RAB                   */ 00373000
         ALR   R2,@10                                              0150 00374000
*                 DO WHILE(CCARBUR=ON&/* SEARCH FOR USABLE RAB       */ 00375000
*                       R2<=CCARABL);/* WITHIN DEFINED RABS          */ 00376000
         B     @DE00151                                            0151 00377000
@DL00151 DS    0H                                                  0152 00378000
*                   R2=R2+LENGTH(CCARAB);/* NEXT RAB                 */ 00379000
         AL    R2,@CF00137                                         0152 00380000
*                 END;              /* END SEARCH                    */ 00381000
@DE00151 TM    CCARBUR(R2),B'10000000'                             0153 00382000
         BNO   @DC00151                                            0153 00383000
         C     R2,CCARABL(,R11)                                    0153 00384000
         BNH   @DL00151                                            0153 00385000
@DC00151 DS    0H                                                  0154 00386000
*               END;                /* END INITLZ                    */ 00387000
@DE00144 LA    @10,1536                                            0154 00388000
         L     @05,CCASMFPT(,R11)                                  0154 00389000
         ALR   @05,@10                                             0154 00390000
         CR    R3,@05                                              0154 00391000
         BL    @DL00144                                            0154 00392000
*               R3=R3-LENGTH(SMFELT);/* POINT LAST ELT IN SMF        */ 00393000
         SL    R3,@CF00050                                         0155 00394000
*               SMFELTSZ=LEN3RAB;   /* CORE SIZE TO BE FREED         */ 00395000
         STCM  @10,3,SMFELTSZ(R3)                                  0156 00396000
*               SMFCHAIN=0;         /* INDICATE LAST ELEMENT         */ 00397000
         SLR   @10,@10                                             0157 00398000
         STCM  @10,15,SMFCHAIN(R3)                                 0157 00399000
*               CCASMFCT=0;         /* COUNT OF PRESERVED RCDS     0158 00400000
*                                      INITIALLY ZERO                */ 00401000
         STH   @10,CCASMFCT(,R11)                                  0158 00402000
*             END;                  /* END SET UP RELATED            */ 00403000
*         END;                      /* END FORMAT SMF CHAIN          */ 00404000
*       ELSE                                                       0161 00405000
*         ;                         /* CHAIN FOR SMF INITLZD         */ 00406000
