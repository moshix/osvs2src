         BNE   @RT00162                                            0162 00410000
*         ;                         /* ERROR CHECK                   */ 00411000
*       ELSE                        /* NO ERROR                      */ 00412000
*         DO;                       /* CONTINUE SMF                  */ 00413000
*                                                                  0164 00414000
*           /*********************************************************/ 00415000
*           /*                                                       */ 00416000
*           /* DETERMINE WHETHER THE INPUT USER RECORD IS ALREADY    */ 00417000
*           /* PRESERVED, BY SEARCHING THE SMF CHAIN                 */ 00418000
*           /*                                                       */ 00419000
*           /*********************************************************/ 00420000
*                                                                  0165 00421000
*           RFY                                                    0165 00422000
*             CCARAB BASED(CCAURAB);/* ORIGINAL MAP                  */ 00423000
