**                                                                    * 00890000
**   PROCEDURE NAME = IGGPCHIU                                        * 00891000
**                                                                    * 00892000
**   PROCEDURE TYPE = INTERNALLY CALLED, NESTED                       * 00893000
**                                                                    * 00894000
**   DESCRIPTIVE NAME = COMPUTE HIGH-USED RBA OF CRA                  * 00895000
**                                                                    * 00896000
**   FUNCTION =                                                       * 00897000
**      COMPUTE THE HIGH-USED RBA OF THE CRA FOR LATER USE IN         * 00898000
**      THE CONSTRUCTION OF THE CRA SELF-DESCRIBING RECORDS.          * 00899000
**                                                                    * 00900000
**   INPUT =                                                          * 00901000
**      ADDRESS OF CCA IN REGISTER 11.                                * 00902000
**      ADDRESS OF DEFINE CRA WORKAREA IN DCRAWAPR (REGISTER 3).      * 00903000
**      IF CALLER IS IGG0CLAS, THEN CMSCTREC(*) CONTAINS POINTERS     * 00904000
**         TO THE VOLUME BASE AND EXTENSION RECORDS.                  * 00905000
**      IF CALLER IS IGG0CLAQ, THEN CCARAVCA(1) POINTS TO THE         * 00906000
**         FIRST RECORD IN THE CHAIN OF BUFFERED VOLUME RECORDS.      * 00907000
**                                                                    * 00908000
**   OUTPUT =                                                         * 00909000
**      'CCAURAB' POINTS TO RAB 1.                                    * 00910000
**      'DCRACHIU' CONTAINS THE VALUE OF HIGH-USED RBA OF THE CRA.    * 00911000
**                                                                    * 00912000
**********************************************************************/ 00913000
*                                                                  0400 00914000
*   CCAURAB=ADDR(CCARAB1);          /* ADDRESS RECORD THROUGH RAB 1  */ 00915000
         LA    @10,CCARAB1(,R11)                                   0400 00916000
         ST    @10,CCAURAB(,R11)                                   0400 00917000
*   DCRAR1PT=CCACPE1;               /* SAVE POINTER TO RECORD AREA 1 */ 00918000
         MVC   DCRAR1PT(4,DCRAWAPR),CCACPE1(@10)                   0401 00919000
*   DCRACHIU=LEN10CIS;              /* LENGTH OF CI'S 0-9            */ 00920000
         MVC   DCRACHIU(4,DCRAWAPR),@CF04408                       0402 00921000
*   PROCESS=GO;                     /* SET LOOP CONTROL CHARACTERS   */ 00922000
