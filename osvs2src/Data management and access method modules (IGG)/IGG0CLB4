         ST    @05,CCACPE1(,@10)                                   0406 00943000
*       IF RCDID=VOLEXTID THEN      /* COUNT IF EXTENSION RECORD     */ 00944000
         CLI   RCDID(@05),C'W'                                     0407 00945000
         BNE   @RF00407                                            0407 00946000
*         DCRACHIU=DCRACHIU         /* INCREMENT USED-AREA SIZE      */ 00947000
*             +DCRACISZ;                                           0408 00948000
         LA    @10,512                                             0408 00949000
         AL    @10,DCRACHIU(,DCRAWAPR)                             0408 00950000
         ST    @10,DCRACHIU(,DCRAWAPR)                             0408 00951000
*       ELSE                        /* FOR NO MORE EXTENSION RECORDS,*/ 00952000
*         PROCESS=STOP;             /* SET LOOP CONTROL FLAG         */ 00953000
         B     @RC00407                                            0409 00954000
@RF00407 L     @10,CCACMSWA(,R11)                                  0409 00955000
         MVC   PROCESS(2,@10),@CC04440                             0409 00956000
*     END;                          /* END 'DO-WHILE' LOOP           */ 00957000
@RC00407 AH    DCRARCTR,@CH00046                                   0410 00958000
@DE00405 L     @10,CCACMSWA(,R11)                                  0410 00959000
         CLC   PROCESS(2,@10),@CC04442                             0410 00960000
         BE    @DL00405                                            0410 00961000
*/********************************************************************/ 00962000
*/*       COUNT  EXTENSION  RECORDS  FOR  DEFINE  SPACE  CASE        */ 00963000
*/********************************************************************/ 00964000
*                                                                  0411 00965000
*   ELSE                            /* FOR DEFINE SPACE, CANDIDATE   */ 00966000
*     DO;                           /* OR UNIQUE                     */ 00967000
