         TITLE 'IGG0CLB4 - VSAM DEFINE CATALOG RECOVERY AREA           *00001000
                        '                                               00002000
IGG0CLB4 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGG0CLB4  76.057'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
         USING @MAINENT,@15                                             00011000
         USING @DATD-(CCATEMPS-IGGCCA),R11                              00012000
         DS 0F                                                          00013000
         DC CL8'IGGPDCRA'                                               00014000
         EJECT                                                          00015000
         B     @PB00002                                            0201 00016000
IGGPDCRA DS    0H                                                  0202 00017000
         ENTRY IGGPDCRA                                                 00018000
         DROP @15                                                       00019000
Z003     DS    0H                                                  0204 00020000
Z003DCRA DS    0H                                                  0205 00021000
         STM 12,14,12(13)                                               00022000
         LA 13,12(13)                                                   00023000
         BALR 12,0                                                      00024000
IGGSEGST DS 0H                                                          00025000
         USING *,12                                                     00026000
*/********************************************************************* 00027000
**                                                                    * 00028000
**   PROCEDURE NAME = IGGPDCRA                                        * 00029000
**                                                                    * 00030000
**   PROCEDURE TYPE = EXTERNALLY CALLED, NESTED                       * 00031000
**                                                                    * 00032000
**   DESCRIPTIVE NAME = DEFINE CATALOG RECOVERY AREA                  * 00033000
**                                                                    * 00034000
**   FUNCTION =                                                       * 00035000
**      DEFINE A CATALOG RECOVERY AREA (CRA) ON A VOLUME OWNED BY     * 00036000
**      A CATALOG HAVING THE RECOVERABLE ATTRIBUTE:                   * 00037000
**      1.  OBTAIN MAIN STORAGE WORK SPACE AND VOLUME SPACE.          * 00038000
**      2.  PROCESS THE VOLUME'S FORMAT-4 DSCB.                       * 00039000
**      3.  CONSTRUCT THE CRA'S SELF-DESCRIBING RECORDS.              * 00040000
**      4.  OPEN THE CRA ESDS.                                        * 00041000
**      5.  WRITE THE CRA SELF-DESCRIBING RECORDS AND THE VOLUME      * 00042000
**          RECORDS TO THE CRA AND THE CATALOG.                       * 00043000
**      6.  PERFORM CLEANUP PROCESSING AND RETURN TO CALLER.          * 00044000
**                                                                    * 00045000
**   INPUT =                                                          * 00046000
**      ADDRESS OF CCA IN REGISTER 11                                 * 00047000
**      VOLUME UCB ADDRESS IN CMSWA(CMSDFSPC)                         * 00048000
**                                                                    * 00049000
**   OUTPUT =                                                         * 00050000
**      IF ERROR ENCOUNTERED, 'CCAPROB' CONTAINS PROBLEM              * 00051000
**         DETERMINATION INFORMATION.                                 * 00052000
**      FORMAT-4 DSCB CONTAINS TT POINTER TO CRA.                     * 00053000
**      CRA CONTAINS SELF-DESCRIBING RECORDS AND VOLUME RECORDS.      * 00054000
**      CATALOG CONTAINS VOLUME RECORDS.                              * 00055000
**                                                                    * 00056000
**********************************************************************/ 00057000
*/********************************************************************/ 00058000
*/*      OBTAIN  MAIN  STORAGE  WORK  SPACE  AND  VOLUME  SPACE      */ 00059000
*/********************************************************************/ 00060000
*                                                                  0211 00061000
*   CCARAACB=BLANK;                 /* CLEAR CRA ACB ADDR FOR FLAG   */ 00062000
         SLR   @10,@10                                             0211 00063000
         ST    @10,CCARAACB(,R11)                                  0211 00064000
*   CCACRABU=ON;                    /* TURN ON CRA BUILD FLAG        */ 00065000
         OI    CCACRABU(R11),B'00000001'                           0212 00066000
*Z004STRG:                                                         0213 00067000
*   CALL IGGPSTRG;                  /* OBTAIN ALL NECESSARY SPACE    */ 00068000
Z004STRG BAL   @14,IGGPSTRG                                        0213 00069000
*   IF CCACD1^=RCS THEN             /* CHECK IF GET SPACE SUCCESSFUL */ 00070000
         CLI   CCACD1(R11),0                                       0214 00071000
         BE    @RF00214                                            0214 00072000
*Z005DCXT:                                                         0215 00073000
*     CALL IGGPDCXT;                /* IF NOT, CLEAN UP AND EXIT     */ 00074000
Z005DCXT BAL   @14,IGGPDCXT                                        0215 00075000
*   ELSE                                                           0216 00076000
*     ;                             /* ALL REQUIRED STORAGE ACQUIRED */ 00077000
@RF00214 DS    0H                                                  0217 00078000
*/********************************************************************/ 00079000
*/*             PROCESS  THE  VOLUME'S  FORMAT  4  DSCB              */ 00080000
*/********************************************************************/ 00081000
*                                                                  0217 00082000
*Z006FMT4:                                                         0217 00083000
*   CALL IGGPFMT4;                  /* UPDATE THE FORMAT 4 DSCB      */ 00084000
Z006FMT4 BAL   @14,IGGPFMT4                                        0217 00085000
*   IF CCACD1^=RCS THEN             /* CHECK IF F4DSCB UPDATE OKAY   */ 00086000
         CLI   CCACD1(R11),0                                       0218 00087000
         BE    @RF00218                                            0218 00088000
*Z007DCXT:                                                         0219 00089000
*     CALL IGGPDCXT;                /* IF NOT, CLEAN UP AND EXIT     */ 00090000
Z007DCXT BAL   @14,IGGPDCXT                                        0219 00091000
*   ELSE                                                           0220 00092000
*     ;                             /* FORMAT-4 DSCB UPDATE OKAY     */ 00093000
@RF00218 DS    0H                                                  0221 00094000
*/********************************************************************/ 00095000
*/*          CONSTRUCT  THE  CRA'S  SELF-DESCRIBING  RECORDS         */ 00096000
*/********************************************************************/ 00097000
*                                                                  0221 00098000
*   DCRACIPT(1)=ADDR(DCRARACI);     /* SET POINTER TO CI 0           */ 00099000
         LA    @10,DCRARACI(,DCRAWAPR)                             0221 00100000
         ST    @10,DCRACIPT(,DCRAWAPR)                             0221 00101000
*   DO DCRARCTR=2 TO 6;             /* SET ADDR ARRAY SUBSCRIPT RANGE*/ 00102000
         LA    DCRARCTR,2                                          0222 00103000
@DL00222 DS    0H                                                  0223 00104000
*     DCRACIPT(DCRARCTR)=           /* SET POINTER TO RECORD         */ 00105000
*         DCRACIPT(DCRARCTR-INCR1)+DCRACISZ;                       0223 00106000
         LR    @10,DCRARCTR                                        0223 00107000
         SLA   @10,2                                               0223 00108000
         LA    @05,512                                             0223 00109000
         LR    @01,DCRAWAPR                                        0223 00110000
         AL    @01,@CF04659                                        0223 00111000
         AL    @05,DCRACIPT(@10,@01)                               0223 00112000
         AL    @10,@CF04660                                        0223 00113000
         ST    @05,DCRACIPT(@10,DCRAWAPR)                          0223 00114000
*   END;                                                           0224 00115000
         AH    DCRARCTR,@CH00046                                   0224 00116000
         CH    DCRARCTR,@CH00087                                   0224 00117000
         BNH   @DL00222                                            0224 00118000
*Z008CHIU:                                                         0225 00119000
*   CALL IGGPCHIU;                  /* COMPUTE HIGH USED RBA OF CRA  */ 00120000
Z008CHIU BAL   @14,IGGPCHIU                                        0225 00121000
*Z009CCI0:                                                         0226 00122000
*   CALL IGGPCCI0;                  /* CONSTRUCT CRA BASE DATA RECORD*/ 00123000
Z009CCI0 BAL   @14,IGGPCCI0                                        0226 00124000
*Z010CI15:                                                         0227 00125000
*   CALL IGGPCI15;                  /* CONSTRUCT CRA CI'S 1 TO 5     */ 00126000
*                                                                  0227 00127000
Z010CI15 BAL   @14,IGGPCI15                                        0227 00128000
*   /*****************************************************************/ 00129000
*   /*                                                               */ 00130000
*   /* CALL TO MODULE IGG0CLDA                                       */ 00131000
*   /*                                                               */ 00132000
*   /*****************************************************************/ 00133000
*                                                                  0228 00134000
*Z011MODI:                                                         0228 00135000
*   CALL IGGPMODI;                  /* INTERFACE TO MODIFY ROUTINE   */ 00136000
Z011MODI L     @15,@CV04455                                        0228 00137000
         BALR  @14,@15                                             0228 00138000
*   IF CCACD1^=RCS THEN             /* CHECK IF MODIFY SUCCESSFUL    */ 00139000
         CLI   CCACD1(R11),0                                       0229 00140000
         BE    @RF00229                                            0229 00141000
*Z012DCXT:                                                         0230 00142000
*     CALL IGGPDCXT;                /* IF NOT, CLEAN UP AND EXIT     */ 00143000
Z012DCXT BAL   @14,IGGPDCXT                                        0230 00144000
*   ELSE                                                           0231 00145000
*     ;                             /* MODIFY PROCESSING SUCCESSFUL  */ 00146000
@RF00229 DS    0H                                                  0232 00147000
*/********************************************************************/ 00148000
*/*                CRA  'OPEN'-RELATED  PROCESSING                   */ 00149000
*/********************************************************************/ 00150000
*                                                                  0232 00151000
*Z013CACB:                                                         0232 00152000
*   CALL IGGPCACB;                  /* CONSTRUCT CRA ACB IN RA 1     */ 00153000
Z013CACB BAL   @14,IGGPCACB                                        0232 00154000
*Z014CXWA:                                                         0233 00155000
*   CALL IGGPCXWA;                  /* CONSTRUCT CRA CAXWA IN RA 1   */ 00156000
Z014CXWA BAL   @14,IGGPCXWA                                        0233 00157000
*Z015OCRA:                                                         0234 00158000
*   CALL IGGPOCRA;                  /* OPEN THE CRA ESDS             */ 00159000
Z015OCRA BAL   @14,IGGPOCRA                                        0234 00160000
*   IF CCACD1^=RCS THEN             /* IF CRA OPEN FAILED,           */ 00161000
         CLI   CCACD1(R11),0                                       0235 00162000
         BE    @RF00235                                            0235 00163000
*Z016DCXT:                                                         0236 00164000
*     CALL IGGPDCXT;                /* CLEAN UP AND EXIT             */ 00165000
Z016DCXT BAL   @14,IGGPDCXT                                        0236 00166000
*   ELSE                                                           0237 00167000
*     ;                             /* CRA SUCCESSFULLY OPENED       */ 00168000
@RF00235 DS    0H                                                  0238 00169000
*/********************************************************************/ 00170000
*/*      WRITE THE CRA SELF-DESCRIBING RECORDS AND THE VOLUME        */ 00171000
*/*      RECORDS TO THE CRA AND THE CATALOG.                         */ 00172000
*/********************************************************************/ 00173000
*                                                                  0238 00174000
*Z017CRPL:                                                         0238 00175000
*   CALL IGGPCRPL;                  /* CONSTRUCT CRA RPL IN RA 1     */ 00176000
*                                                                  0238 00177000
Z017CRPL BAL   @14,IGGPCRPL                                        0238 00178000
*   /*****************************************************************/ 00179000
*   /*                                                               */ 00180000
*   /* CALL TO MODULE IGG0CLDA                                       */ 00181000
*   /*                                                               */ 00182000
*   /*****************************************************************/ 00183000
*                                                                  0239 00184000
*Z018WCRA:                                                         0239 00185000
*   CALL IGGPWCRA;                  /* WRITE RECORDS TO THE CRA      */ 00186000
Z018WCRA L     @15,@CV04458                                        0239 00187000
         BALR  @14,@15                                             0239 00188000
*   IF CCACD1^=RCS THEN             /* CHECK IF I/O SUCCESSFUL       */ 00189000
         CLI   CCACD1(R11),0                                       0240 00190000
         BE    @RF00240                                            0240 00191000
*Z019DCXT:                                                         0241 00192000
*     CALL IGGPDCXT;                /* IF NOT, CLEAN UP AND EXIT     */ 00193000
Z019DCXT BAL   @14,IGGPDCXT                                        0241 00194000
*   ELSE                                                           0242 00195000
*     ;                             /* CRA RECORDS WRITTEN OKAY      */ 00196000
@RF00240 DS    0H                                                  0243 00197000
*   IF CCAF2CB=OFF THEN             /* CALLER IS DEFINE SPACE        */ 00198000
*                                                                  0243 00199000
         TM    CCAF2CB(R11),B'00000010'                            0243 00200000
         BNZ   @RF00243                                            0243 00201000
*     /***************************************************************/ 00202000
*     /*                                                             */ 00203000
*     /* CALL TO MODULE IGG0CLDA                                     */ 00204000
*     /*                                                             */ 00205000
*     /***************************************************************/ 00206000
*                                                                  0244 00207000
*Z020WCAT:                                                         0244 00208000
*     CALL IGGPWCAT;                /* WRITE VOLUME RECORDS TO CATLG */ 00209000
Z020WCAT L     @15,@CV04457                                        0244 00210000
         BALR  @14,@15                                             0244 00211000
*   ELSE                                                           0245 00212000
*     ;                             /* CALLER IS DEFINE CATALOG      */ 00213000
@RF00243 DS    0H                                                  0246 00214000
*Z021DCXT:                                                         0246 00215000
*   CALL IGGPDCXT;                  /* DO CLEANUP PROCESSING FOR EXIT*/ 00216000
Z021DCXT BAL   @14,IGGPDCXT                                        0246 00217000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00218000
         BAL   @14,EXIT                                            0247 00219000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00220000
         DS 0F                                                          00221000
*   GEN(DC CL8'IGGPSTRG');          /* NAME OF FOLLOWING PROCEDURE   */ 00222000
         DC CL8'IGGPSTRG'                                               00223000
*   END;                            /* END OF INNER PROCEDURE        */ 00224000
@EL00002 DS    0H                                                  0250 00225000
@EF00002 DS    0H                                                  0250 00226000
@ER00002 BR    @14                                                 0250 00227000
@PB00002 DS    0H                                                  0251 00228000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 00229000
         EJECT                                                          00230000
*                                                                  0252 00231000
*/********************************************************************/ 00232000
*/*                                                                  */ 00233000
*/*        IGGPSTRG  -  PROCEDURE  INTERNALLY  CALLED  NESTED        */ 00234000
*/*                                                                  */ 00235000
*/********************************************************************/ 00236000
*                                                                  0252 00237000
*IGGPSTRG:                                                         0252 00238000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 00239000
         B     @PB00003                                            0252 00240000
IGGPSTRG DS    0H                                                  0253 00241000
*Z022:                              /* LABEL FOR PROCEDURE TRACKING  */ 00242000
*Z022STRG:                          /* LABEL FOR PROCEDURE TRACKING  */ 00243000
*   RFY                                                            0253 00244000
*    (R5,                                                          0253 00245000
*     R6,                                                          0253 00246000
*     R7,                                                          0253 00247000
*     R8,                                                          0253 00248000
*     R9,                                                          0253 00249000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00250000
Z022     DS    0H                                                  0253 00251000
Z022STRG DS    0H                                                  0254 00252000
*   RFY                                                            0254 00253000
*    (R2,                                                          0254 00254000
*     R3,                                                          0254 00255000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00256000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00257000
         STM 12,14,12(13)                                               00258000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00259000
         LA 13,12(13)                                                   00260000
*/********************************************************************* 00261000
**                                                                    * 00262000
**   PROCEDURE NAME = IGGPSTRG                                        * 00263000
**                                                                    * 00264000
**   PROCEDURE TYPE = INTERNALLY CALLED, NESTED                       * 00265000
**                                                                    * 00266000
**   DESCRIPTIVE NAME = OBTAIN MAIN STORAGE AND VOLUME SPACE          * 00267000
**                                                                    * 00268000
**   FUNCTION =                                                       * 00269000
**      OBTAIN MAIN STORAGE WORK SPACE FOR THE DEFINE CRA PROCESS     * 00270000
**      AND ALSO TO ALLOCATE SPACE ON THE VOLUME FOR THE CATALOG      * 00271000
**      RECOVERY AREA ITSELF:                                         * 00272000
**      1.  OBTAIN SPACE FOR RECORD CONSTRUCTION AND CONTROL BLOCKS   * 00273000
**      2.  CLEAR NEWLY OBTAINED MAIN STORAGE WORK AREA               * 00274000
**      3.  OBTAIN ONE CYLINDER OF SPACE ON THE VOLUME FOR THE CRA    * 00275000
**                                                                    * 00276000
**   INPUT =                                                          * 00277000
**      ADDRESS OF CCA IN REGISTER 11                                 * 00278000
**                                                                    * 00279000
**   OUTPUT =                                                         * 00280000
**      IF ERROR ENCOUNTERED, 'CCAPROB' CONTAINS PROBLEM              * 00281000
**         DETERMINATION INFORMATION.                                 * 00282000
**      ADDRESS OF DEFINE CRA WORKAREA IN DCRAWAPR (REGISTER 3)       * 00283000
**      ADDRESS OF DEFINE CRA WORKAREA IN CCACRACI                    * 00284000
**                                                                    * 00285000
**********************************************************************/ 00286000
*/********************************************************************/ 00287000
*/*     OBTAIN SPACE FOR RECORD CONSTRUCTION AND CONTROL BLOCKS      */ 00288000
*/********************************************************************/ 00289000
*                                                                  0257 00290000
*   CCAMNLEN=LENGTH(DCRAWKA);       /* SET REQUEST CORE SIZE         */ 00291000
         LA    @10,3472                                            0257 00292000
         STCM  @10,7,CCAMNLEN(R11)                                 0257 00293000
*   IF CCAF2CB=ON THEN              /* CALLER IS DEFINE CATALOG      */ 00294000
         TM    CCAF2CB(R11),B'00000010'                            0258 00295000
         BNO   @RF00258                                            0258 00296000
*     CCAMNLEN=CCAMNLEN             /* GETMAIN RAB 3 SAVE AREA ALSO  */ 00297000
*         +LENGTH(DCRARABS);                                       0259 00298000
         AH    @10,@CH04638                                        0259 00299000
         STCM  @10,7,CCAMNLEN(R11)                                 0259 00300000
*   ELSE                                                           0260 00301000
*     ;                             /* CALLER IS DEFINE SPACE        */ 00302000
@RF00258 DS    0H                                                  0261 00303000
*   RFY                                                            0261 00304000
*     R1 RSTD;                      /* RESTRICT R1 FOR GETMAIN MACRO */ 00305000
*   R1=ADDR(CCAMSSPL);              /* POINT TO STRG MGMT P.L.       */ 00306000
         LA    R1,CCAMSSPL(,R11)                                   0262 00307000
*   CCAMNSPL=USUBPOOL;              /* STORE SUBPOOL VALUE           */ 00308000
*                                                                  0263 00309000
         MVI   CCAMNSPL(R11),X'FC'                                 0263 00310000
*   /*****************************************************************/ 00311000
*   /*                                                               */ 00312000
*   /* ALLOCATE MAIN STORAGE FROM USER SUBPOOL 252                   */ 00313000
*   /*                                                               */ 00314000
*   /*****************************************************************/ 00315000
*                                                                  0264 00316000
*   GEN(GETMAIN LC,MF=(E,(1)));     /* GET MAIN STORAGE SPACE        */ 00317000
         GETMAIN LC,MF=(E,(1))                                          00318000
*   GEN(LR R1,R15);                 /* PICK UP RETURN CODE           */ 00319000
         LR R1,R15                                                      00320000
*   IF R1^=NOGETMSE THEN            /* CHECK FOR ERROR RETURN        */ 00321000
*                                                                  0266 00322000
         LTR   R1,R1                                               0266 00323000
         BZ    @RF00266                                            0266 00324000
*     /***************************************************************/ 00325000
*     /*                                                             */ 00326000
*     /* SET CCAMODID='B4', CCAREASN=RSN2, CCARETRN=RCINSSWA         */ 00327000
*     /*                                                             */ 00328000
*     /***************************************************************/ 00329000
*                                                                  0267 00330000
*     CCAPROB=IGGSEGID+(RSN2*K256)+RCINSSWA;/* INSUFFICIENT CORE   0267 00331000
*                                      ERROR                         */ 00332000
         MVC   CCAPROB(4,R11),@CF04640                             0267 00333000
*   ELSE                            /* GETMAIN SUCCESSFUL            */ 00334000
*     DO;                                                          0268 00335000
         B     @RC00266                                            0268 00336000
@RF00266 DS    0H                                                  0269 00337000
*       DO;                                                        0269 00338000
*         IF MNATFULL=ON THEN       /* TRACKING TABLE FULL           */ 00339000
*                                                                  0270 00340000
         TM    MNATFULL(R11),B'10000000'                           0270 00341000
         BNO   @RF00270                                            0270 00342000
*           /*********************************************************/ 00343000
*           /*                                                       */ 00344000
*           /* CALL TO MODULE IGG0CLAT                               */ 00345000
*           /*                                                       */ 00346000
*           /*********************************************************/ 00347000
*                                                                  0271 00348000
*Z026ARFS:                                                         0271 00349000
*           CALL IGGPARFS;          /* YES,CALL SPILL ROUTINE        */ 00350000
Z026ARFS L     @15,@CV04451                                        0271 00351000
         BALR  @14,@15                                             0271 00352000
*         GEN REFS(CCAMNAT)(MVC CCAMNAT(248,11),CCAMNAT+8(11));/*  0272 00353000
*                                      PUSH AND TRACK                */ 00354000
@RF00270 DS    0H                                                  0272 00355000
         MVC CCAMNAT(248,11),CCAMNAT+8(11)                              00356000
*       END;                                                       0273 00357000
*     END;                          /* GET MAIN STORAGE              */ 00358000
*   RFY                                                            0275 00359000
*     R1 UNRSTD;                    /* UNRESTRICT R1'S USE           */ 00360000
@RC00266 DS    0H                                                  0276 00361000
*   IF CCACD1^=RCS THEN             /* GET CORE SUCCESSFUL?          */ 00362000
         CLI   CCACD1(R11),0                                       0276 00363000
         BE    @RF00276                                            0276 00364000
*     DO;                           /* IF NOT SUCCESSFUL,            */ 00365000
*       DCRAWAPR=BLANK;             /* CLEAR WORK AREA ADDRESS       */ 00366000
         SLR   DCRAWAPR,DCRAWAPR                                   0278 00367000
*       CALL EXIT;                  /* RETURN TO CALLER: IGGPDCRA    */ 00368000
         BAL   @14,EXIT                                            0279 00369000
*     END;                                                         0280 00370000
*   ELSE                                                           0281 00371000
*     ;                             /* GET CORE SUCCESSFUL           */ 00372000
@RF00276 DS    0H                                                  0282 00373000
*/********************************************************************/ 00374000
*/*         CLEAR  NEWLY  OBTAINED  MAIN  STORAGE  AREA              */ 00375000
*/********************************************************************/ 00376000
*                                                                  0282 00377000
*   R2=CCAMNADR;                    /* GET CORE ADDRESS              */ 00378000
         L     R2,CCAMNADR(,R11)                                   0282 00379000
*   R3=CCAMNLEN;                    /* GET CORE LENGTH               */ 00380000
         SLR   R3,R3                                               0283 00381000
         ICM   R3,7,CCAMNLEN(R11)                                  0283 00382000
*   RFY                                                            0284 00383000
*     R5 RSTD;                      /* RESTRICT R5 FOR MVCL INSTRUC. */ 00384000
*   R5=BLANK;                       /* CLEAR R5 FOR MVCL INSTRUCTION */ 00385000
         SLR   R5,R5                                               0285 00386000
*   GEN(MVCL  R2,R4);               /* CLEAR MAIN STORAGE WORKAREA   */ 00387000
         MVCL  R2,R4                                                    00388000
*   RFY                                                            0287 00389000
*     R5 UNRSTD;                    /* UNRESTRICT USE OF REGISTER 5  */ 00390000
*   DCRAWAPR=CCAMNADR;              /* SET CRA WORKAREA BASE REGISTER*/ 00391000
         L     DCRAWAPR,CCAMNADR(,R11)                             0288 00392000
*   CCACRACI=DCRAWAPR;              /* SET PTR TO CRA CI ADDR ARRAY  */ 00393000
         ST    DCRAWAPR,CCACRACI(,R11)                             0289 00394000
*   DCRAMNLN=CCAMNLEN;              /* SAVE TOTAL LENGTH OF WORKAREA */ 00395000
         SLR   @10,@10                                             0290 00396000
         ICM   @10,7,CCAMNLEN(R11)                                 0290 00397000
         ST    @10,DCRAMNLN(,DCRAWAPR)                             0290 00398000
*   DCRADESA=CCADESA;               /* SAVE DADSM EXTENT INFORMATION */ 00399000
         MVC   DCRADESA(44,DCRAWAPR),CCADESA(R11)                  0291 00400000
*/********************************************************************/ 00401000
*/*      OBTAIN ONE CYLINDER OF SPACE ON THE VOLUME FOR THE CRA      */ 00402000
*/********************************************************************/ 00403000
*                                                                  0292 00404000
*   CCACRABU=OFF;                   /* TURN OFF CRA BUILD INDICATOR  */ 00405000
         NI    CCACRABU(R11),B'11111110'                           0292 00406000
*   IF CCAF2CB=OFF THEN             /* CALLER IS DEFINE SPACE        */ 00407000
         TM    CCAF2CB(R11),B'00000010'                            0293 00408000
         BNZ   @RF00293                                            0293 00409000
*     DO;                           /* PERFORM DEFINE SPACE CHORES   */ 00410000
*       DCRATRCY=CTVNOTRK;          /* SAVE TRACKS/CYLINDER VALUE    */ 00411000
         L     @10,CCAURAB(,R11)                                   0295 00412000
         L     @10,CCACPE1(,@10)                                   0295 00413000
         MVC   DCRATRCY(2,DCRAWAPR),CTVNOTRK+48(@10)               0295 00414000
*       DCRABTRK=CCACRABT;          /* SAVE BLOCKS/TRACK VALUE       */ 00415000
         MVC   DCRABTRK(2,DCRAWAPR),CCACRABT(R11)                  0296 00416000
