*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01938000
*/********************************************************************* 01939000
**                                                                    * 01940000
**   PROCEDURE NAME = IGGPDCXT                                        * 01941000
**                                                                    * 01942000
**   PROCEDURE TYPE = INTERNALLY CALLED, UNNESTED                     * 01943000
**                                                                    * 01944000
**   DESCRIPTIVE NAME = DEFINE CRA CLEANUP PROCESSING                 * 01945000
**                                                                    * 01946000
**   FUNCTION =                                                       * 01947000
**      PERFORM CLEANUP PROCESSING IN THE EVENT AN ERROR IS           * 01948000
**      ENCOUNTERED OR AFTER ALL DEFINE CRA PROCESSING IS COMPLETED,  * 01949000
**      FREEING ALL ACQUIRED MAIN STORAGE IF NECESSARY AND CLOSING    * 01950000
**      THE CRA ACB IF IT WAS SUCCESSFULLY OPENED.                    * 01951000
**                                                                    * 01952000
**   INPUT =                                                          * 01953000
**      ADDRESS OF CCA IN REGISTER 11                                 * 01954000
**      ADDRESS OF DEFINE CRA WORKAREA IN DCRAWAPR (REGISTER 3)       * 01955000
**                                                                    * 01956000
**   OUTPUT =                                                         * 01957000
**      IF THE CRA CANNOT BE CLOSED, 'CCAPROB' CONTAINS PROBLEM       * 01958000
**         DETERMINATION INFORMATION.                                 * 01959000
**      SEVERAL FIELDS AND FLAGS IN THE CCA RELATED TO DEFINE CRA     * 01960000
**         PROCESSING ARE CLEARED.                                    * 01961000
**                                                                    * 01962000
**********************************************************************/ 01963000
*                                                                  0641 01964000
*   CCACRABU=OFF;                   /* TURN OFF CRA BUILD BIT IN CCA */ 01965000
         NI    CCACRABU(R11),B'11111110'                           0641 01966000
*   CCADSRCL=OFF;                   /* TURN OFF RECURSIVE CALL BIT   */ 01967000
*   CCAVBUFI=OFF;                   /* TURN OFF VOLUME BUFFER FLAG   */ 01968000
         NI    CCADSRCL(R11),B'00111111'                           0643 01969000
*   IF DCRAWAPR^=BLANK THEN         /* CHECK IF WORK SPACE ACQUIRED  */ 01970000
         SLR   @10,@10                                             0644 01971000
         CR    DCRAWAPR,@10                                        0644 01972000
         BE    @RF00644                                            0644 01973000
*     DO;                           /* YES, MAIN STG WORKAREA EXISTS */ 01974000
*       CCADESA=DCRADESA;           /* RESTORE DADSM EXTENT INFO     */ 01975000
         MVC   CCADESA(44,R11),DCRADESA(DCRAWAPR)                  0646 01976000
*       IF DCRAR1PT^=BLANK THEN     /* RA 1 POINTER WAS SAVED        */ 01977000
         L     @05,DCRAR1PT(,DCRAWAPR)                             0647 01978000
         CR    @05,@10                                             0647 01979000
         BE    @RF00647                                            0647 01980000
*         CCAR1REC=DCRAR1PT;        /* RESTORE RA 1 PTR IN RAB 1     */ 01981000
         ST    @05,CCAR1REC(,R11)                                  0648 01982000
*       ELSE                                                       0649 01983000
*         ;                         /* RECORD AREA 1 PTR NOT SAVED   */ 01984000
@RF00647 DS    0H                                                  0650 01985000
*       CCAMNLEN=DCRAMNLN;          /* SET CORE LENGTH FOR FREEMAIN  */ 01986000
         MVC   CCAMNLEN(3,R11),DCRAMNLN+1(DCRAWAPR)                0650 01987000
*       CCAMNADR=DCRAWAPR;          /* SET CORE ADDRESS FOR FREEMAIN */ 01988000
         ST    DCRAWAPR,CCAMNADR(,R11)                             0651 01989000
*       RFY                                                        0652 01990000
*         R1 RSTD;                  /* RESTRICT R1 FOR FREEMAIN MACRO*/ 01991000
*       R1=ADDR(CCAMSSPL);          /* POINT TO STRG MGMT P.L.       */ 01992000
         LA    R1,CCAMSSPL(,R11)                                   0653 01993000
*       CCAMNSPL=USUBPOOL;          /* STORE SUBPOOL VALUE           */ 01994000
         MVI   CCAMNSPL(R11),X'FC'                                 0654 01995000
*       DO;                                                        0655 01996000
*         IF MNATFULL=ON THEN       /* TRACKING TABLE FULL           */ 01997000
*                                                                  0656 01998000
         TM    MNATFULL(R11),B'10000000'                           0656 01999000
         BNO   @RF00656                                            0656 02000000
*           /*********************************************************/ 02001000
*           /*                                                       */ 02002000
*           /* CALL TO MODULE IGG0CLAT                               */ 02003000
*           /*                                                       */ 02004000
*           /*********************************************************/ 02005000
*                                                                  0657 02006000
*Z047ARFS:                                                         0657 02007000
*           CALL IGGPARFS;          /* YES,CALL SPILL ROUTINE        */ 02008000
Z047ARFS L     @15,@CV04451                                        0657 02009000
         BALR  @14,@15                                             0657 02010000
*         GEN REFS(CCAMNAT)(MVC CCAMNAT(248,11),CCAMNAT+8(11));/*  0658 02011000
*                                      PUSH AND TRACK                */ 02012000
@RF00656 DS    0H                                                  0658 02013000
         MVC CCAMNAT(248,11),CCAMNAT+8(11)                              02014000
*       END;                                                       0659 02015000
*                                                                  0659 02016000
*       /*************************************************************/ 02017000
*       /*                                                           */ 02018000
*       /* RELEASE MAIN STORAGE FROM USER SUBPOOL 252                */ 02019000
*       /*                                                           */ 02020000
*       /*************************************************************/ 02021000
*                                                                  0660 02022000
*       GEN(FREEMAIN L,MF=(E,(1))) ;/* FREE CRA WORK SPACE           */ 02023000
         FREEMAIN L,MF=(E,(1))                                          02024000
*       RFY                                                        0661 02025000
*         R1 UNRSTD;                /* UNRESTRICT R1'S USE           */ 02026000
*     END;                          /* END MAIN STORAGE PROCESSING   */ 02027000
*   ELSE                                                           0663 02028000
*     ;                             /* NO WORKAREA WAS ACQUIRED      */ 02029000
@RF00644 DS    0H                                                  0664 02030000
*   IF(CCARAACB^=BLANK)&            /* IF CRA ACB WAS CONSTRUCTED    */ 02031000
*       (ACBOPEN=ON) THEN           /* AND THE ACB IS OPEN           */ 02032000
         L     @10,CCARAACB(,R11)                                  0664 02033000
         LTR   @10,@10                                             0664 02034000
         BZ    @RF00664                                            0664 02035000
         TM    ACBOPEN(@10),B'00010000'                            0664 02036000
         BNO   @RF00664                                            0664 02037000
*     DO;                           /* CLOSE CRA ACB PROCESSING      */ 02038000
*       RFY                                                        0666 02039000
*         R1 RSTD;                  /* RESTRICT REG 1 FOR CLOSE      */ 02040000
*       R1=ADDR(CAXOPLST);          /* GET ADDR OF CLOSE PARM LIST   */ 02041000
         L     R1,ACBUAPTR(,@10)                                   0667 02042000
         LA    R1,CAXOPLST(,R1)                                    0667 02043000
*       GEN(CLOSE  ,MF=(E,(1)));    /* CLOSE THE CRA                 */ 02044000
         CLOSE  ,MF=(E,(1))                                             02045000
*       RFY                                                        0669 02046000
*         R1 UNRSTD;                /* UNRESTRICT USE OF REG 1       */ 02047000
*       IF(ACBOPEN=ON)|             /* IF CRA STILL OPEN OR          */ 02048000
*           (ACBERFLG^=RCS) THEN    /* UNSUCCESSFULLY CLOSED         */ 02049000
*                                                                  0670 02050000
         L     @10,CCARAACB(,R11)                                  0670 02051000
         TM    ACBOPEN(@10),B'00010000'                            0670 02052000
         BO    @RT00670                                            0670 02053000
         CLI   ACBERFLG(@10),0                                     0670 02054000
         BE    @RF00670                                            0670 02055000
@RT00670 DS    0H                                                  0671 02056000
*         /***********************************************************/ 02057000
*         /*                                                         */ 02058000
*         /* SET CCAMODID='B4', CCAREASN=RSN14, CCARETRN=RCCAT       */ 02059000
*         /*                                                         */ 02060000
*         /***********************************************************/ 02061000
*                                                                  0671 02062000
*         CCAPROB=IGGSEGID+(RSN14*K256)+RCCAT;/* ECD: CRA OPEN FAULT.   02063000
*                                      RCD: CANNOT CLOSE CRA AFTER 0671 02064000
*                                      BUILD.                        */ 02065000
         MVC   CCAPROB(4,R11),@CF04653                             0671 02066000
*       ELSE                                                       0672 02067000
*         ;                         /* CRA SUCCESSFULLY CLOSED       */ 02068000
@RF00670 DS    0H                                                  0673 02069000
*     END;                          /* END 'DO' FOR CRA ACB CLOSE    */ 02070000
*   ELSE                                                           0674 02071000
*     ;                             /* CRA ACB IMPROPERLY PROCESSED  */ 02072000
@RF00664 DS    0H                                                  0675 02073000
*   CCARAACB=BLANK;                 /* CLEAR CRA ACB ADDRESS         */ 02074000
         SLR   @10,@10                                             0675 02075000
         ST    @10,CCARAACB(,R11)                                  0675 02076000
*   CCARARPL=BLANK;                 /* CLEAR CRA RPL ADDRESS         */ 02077000
         ST    @10,CCARARPL(,R11)                                  0676 02078000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 02079000
         BAL   @14,EXIT                                            0677 02080000
*   END;                            /* END OF INNER PROCEDURE        */ 02081000
