         BNE   @RF00196                                            0196 00120000
         CLI   RCDID(RCATRCD),C'F'                                 0196 00121000
         BNE   @RF00196                                            0196 00122000
*           DO;                     /* YES-FREE RECORD WAS GOTTEN    */ 00123000
*             DELCODE=ON;           /* INDICATE CLUSTER GET ERROR    */ 00124000
*                                                                  0198 00125000
         OI    DELCODE(RDELWKA),B'10000000'                        0198 00126000
*             /*******************************************************/ 00127000
*             /*                                                     */ 00128000
*             /* CLEAR CCAMODID, CCAREASN, AND CCARETRN              */ 00129000
*             /*                                                     */ 00130000
*             /*******************************************************/ 00131000
*                                                                  0199 00132000
