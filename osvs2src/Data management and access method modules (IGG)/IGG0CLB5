@RF00205 NI    DELCODE(RDELWKA),B'01111111'                        0212 00181000
*   CCAURAB=ADDR(CCARAB1);          /* RE-POINT TO AIX BEING DELETED */ 00182000
@RC00205 LA    @10,CCARAB1(,R11)                                   0213 00183000
         ST    @10,CCAURAB(,R11)                                   0213 00184000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00185000
         BAL   @14,EXIT                                            0214 00186000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00187000
         DS 0F                                                          00188000
