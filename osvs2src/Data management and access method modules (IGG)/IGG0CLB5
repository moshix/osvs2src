         LA 15,*-IGGSEGST(0,0)                                          00233000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */ 00234000
*                                                                  0227 00235000
         SR 12,15                                                       00236000
*   /*****************************************************************/ 00237000
*   /*                                                               */ 00238000
*   /* EXTERNAL PROCEDURE TO IMPLICITLY DELETE ALL AIXS.             */ 00239000
*   /*                                                               */ 00240000
*   /*****************************************************************/ 00241000
*                                                                  0228 00242000
*   RDELWKA=CCACMSWA;               /* INITIALIZE DELWKA POINTER     */ 00243000
*                                                                  0228 00244000
         L     RDELWKA,CCACMSWA(,R11)                              0228 00245000
*   /*****************************************************************/ 00246000
*   /*                                                               */ 00247000
*   /* LOOP THROUGH THIS CODE ONCE FOR EACH AIX IMPLICITLY DELETED   */ 00248000
*   /*                                                               */ 00249000
*   /*****************************************************************/ 00250000
*                                                                  0229 00251000
*   DO WHILE DELNGOBJ^=NOAIXS;      /* DO FOR EACH AIX               */ 00252000
*                                                                  0229 00253000
         B     @DE00229                                            0229 00254000
@DL00229 DS    0H                                                  0230 00255000
*     /***************************************************************/ 00256000
*     /*                                                             */ 00257000
