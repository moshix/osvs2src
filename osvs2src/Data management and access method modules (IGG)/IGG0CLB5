@RF00196 DS    0H                                                  0201 00148000
Z008DLXT L     @15,@CV04105                                        0201 00149000
         BALR  @14,@15                                             0201 00150000
*       ELSE                        /* NO-GET WAS ERROR FREE         */ 00151000
*         DELCODE=OFF;              /* RESET INDICATER               */ 00152000
         B     @RC00195                                            0202 00153000
@RF00195 NI    DELCODE(RDELWKA),B'01111111'                        0202 00154000
*     END;                                                         0203 00155000
@RC00195 DS    0H                                                  0204 00156000
*Z009DIPT:                                                         0204 00157000
*   CALL IGGPDIPT;                  /* IF ANY PATHS DELETE THEM      */ 00158000
@RC00179 DS    0H                                                  0204 00159000
Z009DIPT BAL   @14,IGGPDIPT                                        0204 00160000
*   IF DELCODE=OFF THEN             /* WAS CLUSTER GOTTEN?           */ 00161000
         TM    DELCODE(RDELWKA),B'10000000'                        0205 00162000
         BNZ   @RF00205                                            0205 00163000
*     DO;                           /* YES-CLUSTER GOTTEN            */ 00164000
*       CCAURAB=ADDR(CCARAB2);      /* POINT TO BASE CLUSTER         */ 00165000
         LA    @10,CCARAB2(,R11)                                   0207 00166000
         ST    @10,CCAURAB(,R11)                                   0207 00167000
*Z010DUPG:                                                         0208 00168000
*       CALL IGGPDUPG;              /* DELETE UPGRADE ASSOC IN     0208 00169000
*                                      CLUSTER                       */ 00170000
Z010DUPG BAL   @14,IGGPDUPG                                        0208 00171000
*       RCATRCD=CCAR1REC;           /* INITIALIZE CATRCD POINTER     */ 00172000
         L     RCATRCD,CCAR1REC(,R11)                              0209 00173000
*Z011DBMD:                                                         0210 00174000
*       CALL IGGPDBMD;              /* DELETE AIX ASSOC IN CLUSTER   */ 00175000
Z011DBMD BAL   @14,IGGPDBMD                                        0210 00176000
*     END;                                                         0211 00177000
*   ELSE                            /* NO-CLUSTER NOT GOTTEN         */ 00178000
*     DELCODE=OFF;                  /* RESET INDICATER               */ 00179000
