         TITLE 'IGG0CLB5 - CMS DELETE LOAD THREE                       *00001000
                        '                                               00002000
IGG0CLB5 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGG0CLB5  76.055'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
         USING @MAINENT,@15                                             00011000
         USING @DATD-(CCATEMPS-IGGCCA),R11                              00012000
         DS 0F                                                          00013000
         DC CL8'IGGPDEAX'                                               00014000
         EJECT                                                          00015000
         B     @PB00002                                            0166 00016000
IGGPDEAX DS    0H                                                  0167 00017000
         ENTRY IGGPDEAX                                                 00018000
         DROP @15                                                       00019000
Z003     DS    0H                                                  0169 00020000
Z003DEAX DS    0H                                                  0170 00021000
         STM 12,14,12(13)                                               00022000
         LA 13,12(13)                                                   00023000
         BALR 12,0                                                      00024000
IGGSEGST DS 0H                                                          00025000
*                                                                  0175 00026000
         USING *,12                                                     00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* EXTERNAL PROCEDURE TO EXPLICITLY DELETE A SPECIFIED AIX.      */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                                  0176 00033000
*   RDELWKA=CCACMSWA;               /* INITIALIZE DELWKA POINTER     */ 00034000
         L     RDELWKA,CCACMSWA(,R11)                              0176 00035000
*   DELXTYPE(1)=CTGTCL;             /* SET TO CLUSTER                */ 00036000
*                                                                  0177 00037000
         MVI   DELXTYPE(RDELWKA),C'C'                              0177 00038000
*   /*****************************************************************/ 00039000
*   /*                                                               */ 00040000
*   /* EXTRACT CLUSTER ASSOCIATION FROM THE ALTERNATE INDEX          */ 00041000
*   /*                                                               */ 00042000
*   /*****************************************************************/ 00043000
*                                                                  0178 00044000
*Z004DEXB:                                                         0178 00045000
*   CALL IGGPDEXB;                  /* EXTRACT CLUSTER ASSOC         */ 00046000
Z004DEXB BAL   @14,IGGPDEXB                                        0178 00047000
*   IF ADDR(DELFPLT)->CTGFLDTS=ON THEN/* EXTRACT TEST FAIL?          */ 00048000
         TM    CTGFLDTS+116(RDELWKA),B'00000001'                   0179 00049000
         BNO   @RF00179                                            0179 00050000
*     DELCODE=ON;                   /* YES-NO CLUSTER ASSOCIATION    */ 00051000
         OI    DELCODE(RDELWKA),B'10000000'                        0180 00052000
*   ELSE                            /* NO-TEST DID NOT FAIL          */ 00053000
*     DO;                                                          0181 00054000
         B     @RC00179                                            0181 00055000
@RF00179 DS    0H                                                  0182 00056000
*       DELNROBJ=DELNROBJ-INCR;     /* ACCOUNT FOR CLUSTER ASSOC.    */ 00057000
*                                                                  0182 00058000
         SLR   @10,@10                                             0182 00059000
         IC    @10,DELNROBJ(,RDELWKA)                              0182 00060000
         BCTR  @10,0                                               0182 00061000
         STC   @10,DELNROBJ(,RDELWKA)                              0182 00062000
*       /*************************************************************/ 00063000
*       /*                                                           */ 00064000
*       /* GET AIXS BASE CLUSTER IN RECORD AREA 2                    */ 00065000
*       /*                                                           */ 00066000
*       /*************************************************************/ 00067000
*                                                                  0183 00068000
*       RCATRCD=CCAR2REC;           /* INITIALIZE CATRCD POINTER     */ 00069000
         L     RCATRCD,CCAR2REC(,R11)                              0183 00070000
*       CATRCD=CATRCD&&CATRCD;      /* CLEAR KEY INFO IN RECORD AREA */ 00071000
         XC    CATRCD(49,RCATRCD),CATRCD(RCATRCD)                  0184 00072000
*       CCASRCIN=DELXCI(1);         /* CI OF CLUSTER                 */ 00073000
         MVC   CCASRCIN(3,R11),DELXCI(RDELWKA)                     0185 00074000
*       DO;                                                        0186 00075000
*         CCAF1KEY=OFF;             /* GET BY CI NO.                 */ 00076000
*         CCAF1KGE=OFF;             /* NOT GET NEXT                  */ 00077000
*         CCAF1UP=ON;               /* GET FOR UPD                   */ 00078000
         OI    CCAF1UP(R11),B'00000010'                            0189 00079000
         NI    CCAF1KEY(R11),B'11100111'                           0189 00080000
*         CCAURAB=ADDR(CCARAB2);    /* SET RAB PTR                   */ 00081000
*                                                                  0190 00082000
         LA    @10,CCARAB2(,R11)                                   0190 00083000
         ST    @10,CCAURAB(,R11)                                   0190 00084000
*         /***********************************************************/ 00085000
*         /*                                                         */ 00086000
*         /* CALL TO MODULE IGG0CLBI                                 */ 00087000
*         /*                                                         */ 00088000
*         /***********************************************************/ 00089000
*                                                                  0191 00090000
*Z006GET:                                                          0191 00091000
*         CALL IGGPGET;             /* GET RECORD                    */ 00092000
Z006GET  L     @15,@CV04113                                        0191 00093000
         BALR  @14,@15                                             0191 00094000
*       END;                        /* GET CLUSTER IN RECORD AREA 2  */ 00095000
*       RDELWKA=CCACMSWA;           /* INITIALIZE DELWKA POINTER     */ 00096000
         L     RDELWKA,CCACMSWA(,R11)                              0193 00097000
*       RCATRCD=CCAR2REC;           /* INITIALIZE CATRCD POINTER     */ 00098000
*                                                                  0194 00099000
         L     RCATRCD,CCAR2REC(,R11)                              0194 00100000
*       /*************************************************************/ 00101000
*       /*                                                           */ 00102000
*       /* CHECK IF ERROR OCCURED ON GET CLUSTER.                    */ 00103000
*       /*                                                           */ 00104000
*       /*************************************************************/ 00105000
*                                                                  0195 00106000
*       IF CCACD1^=RCS THEN         /* GET ERROR OCCUR?              */ 00107000
*                                                                  0195 00108000
         CLI   CCACD1(R11),0                                       0195 00109000
         BE    @RF00195                                            0195 00110000
*         /***********************************************************/ 00111000
*         /*                                                         */ 00112000
*         /* GET ERROR OCCURED - CHECK IF A FREE RECORD WAS GOTTEN   */ 00113000
*         /*                                                         */ 00114000
*         /***********************************************************/ 00115000
*                                                                  0196 00116000
*         IF CCACD1=RCENT&          /* YES-INVALID CI NUMBER AND     */ 00117000
*             RCDID=CTGTFREE THEN   /* FREE RECORD GOTTEN?           */ 00118000
