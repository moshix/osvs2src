*   GEN(ENTRY IGGPCKCC );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 01463000
         ENTRY IGGPCKCC                                                 01464000
*Z022:                              /* LABEL FOR PROCEDURE TRACKING  */ 01465000
*Z022CKCC:                          /* LABEL FOR PROCEDURE TRACKING  */ 01466000
*   RFY                                                            0433 01467000
*    (R5,                                                          0433 01468000
*     R6,                                                          0433 01469000
*     R7,                                                          0433 01470000
*     R8,                                                          0433 01471000
*     R9,                                                          0433 01472000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 01473000
Z022     DS    0H                                                  0433 01474000
Z022CKCC DS    0H                                                  0434 01475000
*   RFY                                                            0434 01476000
*    (R2,                                                          0434 01477000
*     R3,                                                          0434 01478000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01479000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 01480000
         STM 12,14,12(13)                                               01481000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 01482000
