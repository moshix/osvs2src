*   GEN(ENTRY IGGPINMD );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 01101000
         ENTRY IGGPINMD                                                 01102000
*Z017:                              /* LABEL FOR PROCEDURE TRACKING  */ 01103000
*Z017INMD:                          /* LABEL FOR PROCEDURE TRACKING  */ 01104000
*   RFY                                                            0351 01105000
*    (R5,                                                          0351 01106000
*     R6,                                                          0351 01107000
*     R7,                                                          0351 01108000
*     R8,                                                          0351 01109000
*     R9,                                                          0351 01110000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 01111000
Z017     DS    0H                                                  0351 01112000
Z017INMD DS    0H                                                  0352 01113000
*   RFY                                                            0352 01114000
*    (R2,                                                          0352 01115000
*     R3,                                                          0352 01116000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01117000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 01118000
         STM 12,14,12(13)                                               01119000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 01120000
         LA 13,12(13)                                                   01121000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 01122000
         BALR 12,0                                                      01123000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 01124000
         LA 15,*-IGGSEGST(0,0)                                          01125000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */ 01126000
         SR 12,15                                                       01127000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 01128000
**   MODULE NAME =  IGGPINMD                                          * 01129000
**                                                                    * 01130000
**   DESCRIPTIVE NAME =  USVR INTERFACE BUILDER AND PROCESSOR         * 01131000
**                                                                    * 01132000
**   COPYRIGHT =  NONE                                                * 01133000
**                                                                    * 01134000
**   STATUS =  RELEASE 1, LEVEL 0                                     * 01135000
**                                                                    * 01136000
**   FUNCTION =                                                       * 01137000
**      THIS EXTERNAL PROCEDURE INTERFACES WITH THE USVR.  IF A NON-  * 01138000
**      MASTER LEVEL OF PASSWORD WAS VERIFIED AND A USVR EXISTS, THIS * 01139000
**      PROCEDURE RECEIVES CONTROL TO INTERFACE WITH THE USVR.  THE   * 01140000
**      FOLLOWING STEPS OUTLINE THE GENERAL PROCEDURE:                * 01141000
**      1. GET STORAGE TO CONTAIN THE INFORMATION TO BE PASSED TO THE * 01142000
**         USVR.                                                      * 01143000
**      2. CONSTRUCT THE USVR INPUT PARAMETER LIST                    * 01144000
**      3. CLEAR THE IN CORE RECORDS.                                 * 01145000
**      4. LOAD THE USER SPECIFIED VERIFICATION ROUTINE FROM          * 01146000
**         SYS1.LINKLIB.                                              * 01147000
**      5. SYNCH TO THE LOADED MODULE.                                * 01148000
**      6. EXAMINE THE RETURN CODE                                    * 01149000
**      7. FREE THE INPUT PARAMETER LIST STORAGE                      * 01150000
**      8. RETURN TO THE INVOKER OF THIS PROCEDURE.                   * 01151000
**                                                                    * 01152000
**   INPUT =                                                          * 01153000
**      CCA                                                           * 01154000
**        CCAR5REC                                                    * 01155000
**          CKAUWKA                                                   * 01156000
**            CKAUINFO                                                * 01157000
**            CKAUPRMT                                                * 01158000
**        CCAR1REC                                                    * 01159000
**          RCDCINO                                                   * 01160000
**          CNAME                                                     * 01161000
**          COWNID                                                    * 01162000
**                                                                    * 01163000
**   OUTPUT =                                                         * 01164000
**      CCA                                                           * 01165000
**        CCAR5REC                                                    * 01166000
**          CKAUWKA                                                   * 01167000
**            CKUSVRPT                                                * 01168000
**            CKBASECI                                                * 01169000
**            CKAUINFO                                                * 01170000
**        CCASRCIN                                                    * 01171000
**      USVRPWKA                                                      * 01172000
**                                                                    * 01173000
**     MESSAGES =  NONE                                               * 01174000
**                                                                    * 01175000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01176000
*                                                                  0358 01177000
*   /*****************************************************************/ 01178000
*   /*                                                               */ 01179000
*   /* IGGPINMD - THIS IS AN EXTERNAL ROUTINE THAT BUILDS THE LIST OF*/ 01180000
*   /* INFORMATION TO PASS TO THE USVR MODULE, LOADS THE USVR MODULE */ 01181000
*   /* FROM SYS1.LINKLIB AND TRANSFERS CONTROL TO THE USVR VIA A     */ 01182000
*   /* SYNCH MACRO. WHEN CONTROL IS RETURNED FROM THE USVR, A RETURN */ 01183000
*   /* CODE IS SET IN REGISTER 15 INDICATING THE CONTINUATION STATUS.*/ 01184000
*   /*                                                               */ 01185000
*   /*****************************************************************/ 01186000
*                                                                  0358 01187000
*   CCAMNLEN=LENGTH(USVRPWKA)+LENUSAR+SAVEASIZ;/* CORE SIZE FOR USVR    01188000
*                                      PARM L                        */ 01189000
         MVC   CCAMNLEN(3,R11),@CF03830+1                          0358 01190000
*   RFY                                                            0359 01191000
*    (R1) RSTD;                     /* AND USVR REG SAVE AREA        */ 01192000
*                                                                  0359 01193000
*   /*****************************************************************/ 01194000
*   /*                                                               */ 01195000
*   /* GET CORE FOR PARM LIST                                        */ 01196000
*   /*                                                               */ 01197000
*   /*****************************************************************/ 01198000
*                                                                  0360 01199000
*   R1=ADDR(CCAMSSPL);              /* POINT TO MSS PL               */ 01200000
         LA    R1,CCAMSSPL(,R11)                                   0360 01201000
*   CCAMNSPL=USERSTG;               /* USER STG SUBPOOL NO.          */ 01202000
