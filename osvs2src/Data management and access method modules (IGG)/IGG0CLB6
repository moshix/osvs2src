*       CTGMAST=OFF THEN            /* VERIFY MASTER OK      @Y30LSSB*/ 00682000
         L     @10,CCACPL(,R11)                                    0276 00683000
         TM    CTGFUNC(@10),B'00100000'                            0276 00684000
         BNO   @RF00276                                            0276 00685000
         TM    CTGFUNC(@10),B'11000000'                            0276 00686000
         BNZ   @RF00276                                            0276 00687000
         L     @05,CCAURAB(,R11)                                   0276 00688000
         L     @05,CCACPE1(,@05)                                   0276 00689000
         CLI   RCDID(@05),C'V'                                     0276 00690000
         BNE   @RF00276                                            0276 00691000
         TM    CTGMAST(@10),B'01000000'                            0276 00692000
         BNZ   @RF00276                                            0276 00693000
*     CALL EXIT;                    /* NO SECURITY EXIT              */ 00694000
         BAL   @14,EXIT                                            0277 00695000
*   ELSE                                                           0278 00696000
*     ;                                                            0278 00697000
@RF00276 DS    0H                                                  0279 00698000
*   IF CCAF11B6=ON&                 /* IF NO UPDATE PSWD     @ZA30975*/ 00699000
*       CAXRAC=OFF&                 /* & NOT RAC DEF'D       @Z40RSRC*/ 00700000
*       CTGFUNC=CTGCMS&CTGOPTNS=CTGDEFIN THEN/* & DEF        @Z30TSBM*/ 00701000
         TM    CCAF11B6(R11),B'00000100'                           0279 00702000
         BNO   @RF00279                                            0279 00703000
         L     @10,CCAACB(,R11)                                    0279 00704000
         L     @10,ACBUAPTR(,@10)                                  0279 00705000
         TM    CAXRAC(@10),B'00010000'                             0279 00706000
         BNZ   @RF00279                                            0279 00707000
         L     @10,CCACPL(,R11)                                    0279 00708000
         TM    CTGFUNC(@10),B'10000000'                            0279 00709000
         BNO   @RF00279                                            0279 00710000
         TM    CTGFUNC(@10),B'01100000'                            0279 00711000
         BNZ   @RF00279                                            0279 00712000
         TM    CTGOPTNS(@10),B'00001000'                           0279 00713000
         BNO   @RF00279                                            0279 00714000
         TM    CTGOPTNS(@10),B'11110000'                           0279 00715000
         BNZ   @RF00279                                            0279 00716000
*     CALL EXIT;                    /* NO SECURITY EXIT      @Z30TSBM*/ 00717000
*                                                                  0280 00718000
         BAL   @14,EXIT                                            0280 00719000
*   /*****************************************************************/ 00720000
*   /*                                                               */ 00721000
*   /* CHECK IF THE CALLER OF THIS CALLER WAS PRIVILEGED             */ 00722000
*   /*                                                               */ 00723000
*   /*****************************************************************/ 00724000
*                                                                  0281 00725000
*   RFY                                                            0281 00726000
*    (R1) RSTD;                                                    0281 00727000
@RF00279 DS    0H                                                  0282 00728000
*   R1=CCARB->XRBLNK->XRBLNK;       /* GET ADDRESS OF SVRB           */ 00729000
         L     @10,CCARB(,R11)                                     0282 00730000
         L     @10,XRBLNK(,@10)                                    0282 00731000
         L     R1,XRBLNK(,@10)                                     0282 00732000
*   IF(R1^=CCATCB&((CTGFUNC=CTGLOC)|(CTGOPTNS=CTGDELET&CTGENT->ENTRYNM2 00733000
*       =STGINDX)|(CTGOPTNS=CTGDEFIN&CTGFVT->CTGFVENT->ENTRYNM2=STGINDX 00734000
*       )))&CTGBYPMT=OFF            /*                       @YM43286*/ 00735000
*     THEN                          /* CHECK FOR NOT TCB ADDR      0283 00736000
*                                                            @ZA06709   00737000
*                                      AND A LOCATE REQ      @ZA06709   00738000
*                                      OR DELETE OF SYS1.STGINDEX OR    00739000
*                                      DEFINE OF SYS1.STGINDEX     0283 00740000
*                                                            @ZA06709   00741000
*                                      AND BYPASS PROMPTING OFF    0283 00742000
*                                                            @YM43286*/ 00743000
         C     R1,CCATCB(,R11)                                     0283 00744000
         BE    @RF00283                                            0283 00745000
         L     @10,CCACPL(,R11)                                    0283 00746000
         TM    CTGFUNC(@10),B'00100000'                            0283 00747000
         BNO   @GL00019                                            0283 00748000
         TM    CTGFUNC(@10),B'11000000'                            0283 00749000
         BZ    @GL00016                                            0283 00750000
@GL00019 L     @10,CCACPL(,R11)                                    0283 00751000
         TM    CTGOPTNS(@10),B'00011000'                           0283 00752000
         BNO   @GL00018                                            0283 00753000
         TM    CTGOPTNS(@10),B'11100000'                           0283 00754000
         BNZ   @GL00018                                            0283 00755000
         L     @10,CTGENT(,@10)                                    0283 00756000
         CLC   ENTRYNM2(13,@10),@CC03622                           0283 00757000
         BE    @GL00016                                            0283 00758000
@GL00018 L     @10,CCACPL(,R11)                                    0283 00759000
         TM    CTGOPTNS(@10),B'00001000'                           0283 00760000
         BNO   @RF00283                                            0283 00761000
         TM    CTGOPTNS(@10),B'11110000'                           0283 00762000
         BNZ   @RF00283                                            0283 00763000
         L     @10,CTGFVT(,@10)                                    0283 00764000
         L     @10,CTGFVENT(,@10)                                  0283 00765000
         CLC   ENTRYNM2(13,@10),@CC03622                           0283 00766000
         BNE   @RF00283                                            0283 00767000
@GL00016 L     @10,CCACPL(,R11)                                    0283 00768000
         TM    CTGBYPMT(@10),B'00001000'                           0283 00769000
         BNZ   @RF00283                                            0283 00770000
*     IF RBRPSWPK='0000'B|          /* CHECK FOR KEY ZERO USER       */ 00771000
*         RBRPSWPS=OFF THEN         /* CHECK FOR PRIVILEGED STAT     */ 00772000
         LA    @10,16                                              0284 00773000
         ALR   @10,R1                                              0284 00774000
         TM    RBRPSWPK(@10),B'11110000'                           0284 00775000
         BZ    @RT00284                                            0284 00776000
         TM    RBRPSWPS(@10),B'00000001'                           0284 00777000
         BNZ   @RF00284                                            0284 00778000
@RT00284 DS    0H                                                  0285 00779000
*       DO;                         /* BYPASS VERIFY         @Y30SSSB*/ 00780000
*         CCASUPFD=ON;              /* SET BYPASS INDIC      @Y30SSSB*/ 00781000
         OI    CCASUPFD(R11),B'00001000'                           0286 00782000
*         CALL EXIT;                /* RETURN                @Y30SSSB*/ 00783000
         BAL   @14,EXIT                                            0287 00784000
*       END;                        /* NO BYPASS VERIFY      @Y30SSSB*/ 00785000
*     ELSE                                                         0289 00786000
*       ;                           /* NOT PRIVILEGED OR KEY 0       */ 00787000
@RF00284 DS    0H                                                  0290 00788000
*   ELSE                                                           0290 00789000
*     ;                             /* NOT LOCATE OR PROPER TASK     */ 00790000
@RF00283 DS    0H                                                  0291 00791000
*   RFY                                                            0291 00792000
*    (R1) UNRSTD;                                                  0291 00793000
*                                                                  0291 00794000
*   /*****************************************************************/ 00795000
*   /*                                                               */ 00796000
*   /* CHECK FOR CATALOG-ONLY VERIFICATION SITUATIONS.       @Z40RSRC*/ 00797000
*   /*                                                               */ 00798000
*   /*****************************************************************/ 00799000
*                                                                  0292 00800000
*   CCACD2=OKAY;                    /* CLEAR THE SIGNAL BYTE         */ 00801000
         MVI   CCACD2(R11),X'00'                                   0292 00802000
*   IF CTGFUNC=CTGCMS|              /* CMS TYPE FUNCTION     @Z40RSRC*/ 00803000
*       (CTGFUNC=CTGLOC&            /* LOCATE OF VOL REC     @Z40RSRC*/ 00804000
*       RCDID=CTGTVOL) THEN         /* WITH MPWD REQUEST     @Z40RSRC*/ 00805000
         L     @01,CCACPL(,R11)                                    0293 00806000
         TM    CTGFUNC(@01),B'10000000'                            0293 00807000
         BNO   @GL00023                                            0293 00808000
         TM    CTGFUNC(@01),B'01100000'                            0293 00809000
         BZ    @RT00293                                            0293 00810000
