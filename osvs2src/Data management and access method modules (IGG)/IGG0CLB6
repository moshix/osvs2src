         B     @PB00004                                            0254 00510000
IGGPSPSC DS    0H                                                  0255 00511000
*   GEN(ENTRY IGGPSPSC );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 00512000
         ENTRY IGGPSPSC                                                 00513000
*Z012:                              /* LABEL FOR PROCEDURE TRACKING  */ 00514000
*Z012SPSC:                          /* LABEL FOR PROCEDURE TRACKING  */ 00515000
*   RFY                                                            0256 00516000
*    (R5,                                                          0256 00517000
*     R6,                                                          0256 00518000
*     R7,                                                          0256 00519000
*     R8,                                                          0256 00520000
*     R9,                                                          0256 00521000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00522000
Z012     DS    0H                                                  0256 00523000
Z012SPSC DS    0H                                                  0257 00524000
*   RFY                                                            0257 00525000
*    (R2,                                                          0257 00526000
*     R3,                                                          0257 00527000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00528000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00529000
         STM 12,14,12(13)                                               00530000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00531000
         LA 13,12(13)                                                   00532000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 00533000
         BALR 12,0                                                      00534000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 00535000
         LA 15,*-IGGSEGST(0,0)                                          00536000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */ 00537000
         SR 12,15                                                       00538000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00539000
**   MODULE NAME =  IGGPSPSC                                          * 00540000
**                                                                    * 00541000
**   DESCRIPTIVE NAME =  SPECIAL SITUATION CHECK                      * 00542000
**                                                                    * 00543000
**   COPYRIGHT =  NONE                                                * 00544000
**                                                                    * 00545000
**   STATUS =  RELEASE 1, LEVEL 0                                     * 00546000
**                                                                    * 00547000
**   FUNCTION =                                                       * 00548000
**      THIS EXTERNAL PROCEDURE EXAMINES THE CALLER PROVIDED CPL AND  * 00549000
**      DETERMINES IF THIS IS A REQUEST IN WHICH VERIFICATION IS NOT  * 00550000
**      REQUIRED OR IF THE CATALOG RECORD IS NEEDED FOR VERIFICATION  * 00551000
**      (DEFINE). FOLLOWING ITEMS ARE THE BASE ASSUMPTIONS FOR THIS   * 00552000
**      SPECIAL CHECKING AND HANDLING:                                * 00553000
**      A. NO VERIFICATION IS REQUIRED FOR THE FOLLWING:              * 00554000
**         1. LOCATE ON ALIEN OBJECTS.                                * 00555000
**         2. NON-VSAM LOCATE ON ALIEN OBJECTS                        * 00556000
**         3. LOCATE ON VOLUME ENTRY RECORDS                          * 00557000
**         4. DEFINE OF MASTER CATALOGS                               * 00558000
**      B. SPECIAL PROCESSING FOR CMS FUNCTIONS:                      * 00559000
**         1. DELETE OF ALIEN OR VOLUME RECORD ENTRIES REQUIRES THAT  * 00560000
**            CATALOG SECURITY BE VERIFIED.                           * 00561000
**         2. DEFINE OF ANYTHING OTHER THAN MASTER CATALOGS REQUIRES  * 00562000
**            THAT CATALOG SECURITY BE VERIFIED.                      * 00563000
**         3. THE ABOVE SITUATIONS ARE HANDLED BY SAVING THE CI OF    * 00564000
**            THE ORIGINAL RECORD, IF ONE EXISTS, AND READING THE     * 00565000
**            CATALOG RECORD THAT MAY CONTAIN SECURITY INFORMATION    * 00566000
**            IF IT EXISTS.                                           * 00567000
**      C. IF SECURITY VERIFICATION IS NOT REQUIRED A NON-ZERO VALUE  * 00568000
**         WILL BE RETURNED IN CCACD2.                                * 00569000
**                                                                    * 00570000
**   INPUT =                                                          * 00571000
**      CCA                                                           * 00572000
**        CCACPL - USER PROVIDED CATALOG PARAMETER LIST               * 00573000
**          CTGFUNC - FUNCTION TYPE                                   * 00574000
**          CTGAM0  - VSAM REQUEST BIT                                * 00575000
**          CTGOPTNS - CMS FUNCTION TYPE                              * 00576000
**          CTGTYPE  - ENTRY TYPE                                     * 00577000
**      CCAURAB                                                       * 00578000
**        CCACPE1                                                     * 00579000
**          RCDID - RECORD TYPE INDENTIFICATION                       * 00580000
**          RCDCINO - RECORD CI NUMBER                                * 00581000
**                                                                    * 00582000
**   OUTPUT =                                                         * 00583000
**      CCA                                                           * 00584000
**        CCAR5REC                                                    * 00585000
**          CKAUWKA                                                   * 00586000
**            REGETCI - RE-GET RECORD CI VALUE                        * 00587000
**        CCACD2 - NO VERIFICATION REQUIRED INDICATOR                 * 00588000
**        CCASRCIN                                                    * 00589000
**                                                                    * 00590000
**     MESSAGES =  NONE                                               * 00591000
**                                                                    * 00592000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00593000
*                                                                  0263 00594000
*   /*****************************************************************/ 00595000
*   /*                                                               */ 00596000
*   /* IGGPSPSC - THIS IS AN EXTERNAL PROCEDURE THAT CHECKS FOR      */ 00597000
*   /* SPECIAL SITUATIONS AND DETERMINES WHAT LEVELS OF SECURITY NEED*/ 00598000
*   /* TO BE VERIFIED AND ON WHICH ENTITY SECURITY VERIFICATION IS TO*/ 00599000
*   /* TAKE PLACE. ALSO, IT CHECKS FOR CASES IN WHICH VERIFICATION IS*/ 00600000
*   /* NOT REQUIRED                                                  */ 00601000
*   /*                                                               */ 00602000
*   /*****************************************************************/ 00603000
*                                                                  0263 00604000
*   CKAUCATC=OFF;                   /* ASSUME NO CAT CHK     @Z40RSRC*/ 00605000
*                                                                  0263 00606000
         L     @10,CCAR5REC(,R11)                                  0263 00607000
         NI    CKAUCATC(@10),B'11110111'                           0263 00608000
*   /*****************************************************************/ 00609000
*   /*                                                               */ 00610000
*   /* DETERMINE IF CATALOG IS RAC DEFINED                   @Z40RSRC*/ 00611000
*   /*                                                               */ 00612000
*   /*****************************************************************/ 00613000
*                                                                  0264 00614000
*   IF CAXRACP=NONE THEN            /* IF NOT YET CHK'D      @Z40RSRC*/ 00615000
         L     @10,CCAACB(,R11)                                    0264 00616000
         L     @10,ACBUAPTR(,@10)                                  0264 00617000
         ICM   @05,15,CAXRACP(@10)                                 0264 00618000
         BNZ   @RF00264                                            0264 00619000
*     DO;                           /*                       @Z40RSRC*/ 00620000
*                                                                  0265 00621000
*       /*************************************************************/ 00622000
*       /*                                                           */ 00623000
*       /* CALL TO MODULE IGG0CLBM                                   */ 00624000
*       /*                                                           */ 00625000
*       /*************************************************************/ 00626000
*                                                                  0266 00627000
*Z013CKCC:                                                         0266 00628000
*       CALL IGGPCKCC;              /* CHK IF RAC DEF'D      @Z40RSRC*/ 00629000
Z013CKCC BAL   @14,IGGPCKCC                                        0266 00630000
*       IF CAXRAC=OFF THEN          /* IF NOT RAC DEF'D      @Z40RSRC*/ 00631000
         L     @10,CCAACB(,R11)                                    0267 00632000
         L     @10,ACBUAPTR(,@10)                                  0267 00633000
         TM    CAXRAC(@10),B'00010000'                             0267 00634000
         BNZ   @RF00267                                            0267 00635000
*         CAXRACP=INCR1;            /* INDICATED CHECKED     @Z40RSRC*/ 00636000
         MVC   CAXRACP(4,@10),@CF00038                             0268 00637000
*       ELSE                                                       0269 00638000
*         ;                         /* CAXRACP WILL BE FILLED IN   0269 00639000
*                                      AFTER 1ST RACHECK     @Z40RSRC*/ 00640000
@RF00267 DS    0H                                                  0270 00641000
*     END;                          /*                       @Z40RSRC*/ 00642000
*   ELSE                                                           0271 00643000
*     ;                             /* RAC ALREADY CHK'D     @Z40RSRC*/ 00644000
@RF00264 DS    0H                                                  0272 00645000
*   CCACD2=INCR1;                   /* SET EARLY RETURN CODE         */ 00646000
*                                                                  0272 00647000
         MVI   CCACD2(R11),X'01'                                   0272 00648000
*   /*****************************************************************/ 00649000
*   /*                                                               */ 00650000
*   /* CHECK FOR SPECIAL LOCATE SITUATIONS                   @Z40RSRC*/ 00651000
*   /*                                                               */ 00652000
*   /*****************************************************************/ 00653000
*                                                                  0273 00654000
*   IF CTGFUNC=CTGLOC&              /* LOCATE FUNCTION       @Y30DSSB*/ 00655000
*       CTGMAST=OFF&CTGCI=OFF&      /* NOT REQUEST TO        @Y30SSSB*/ 00656000
*       CTGUPD=OFF&CTGREAD=OFF&     /* VERIFY SECURITY       @Z40RSRC*/ 00657000
*       CCAGC4=OFF THEN             /* NOT LOC SECURE DATA   @Y30SSSB*/ 00658000
         L     @10,CCACPL(,R11)                                    0273 00659000
         TM    CTGFUNC(@10),B'00100000'                            0273 00660000
         BNO   @RF00273                                            0273 00661000
         TM    CTGFUNC(@10),B'11000000'                            0273 00662000
         BNZ   @RF00273                                            0273 00663000
         TM    CTGMAST(@10),B'01111000'                            0273 00664000
         BNZ   @RF00273                                            0273 00665000
         TM    CCAGC4(R11),B'01000000'                             0273 00666000
         BNZ   @RF00273                                            0273 00667000
*     CALL EXIT;                    /* RETURN                @Y30SSSB*/ 00668000
         BAL   @14,EXIT                                            0274 00669000
*   ELSE                                                           0275 00670000
*     ;                             /* NON-LOCATE FUNCTION   @Y30SSSB*/ 00671000
*                                                                  0275 00672000
