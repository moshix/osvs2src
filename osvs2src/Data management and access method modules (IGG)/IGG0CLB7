*     R3,                                                          0513 01506000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01507000
*/********************************************************************* 01508000
**                                                                    * 01509000
**   MODULE NAME = IGGPFRWK                                           * 01510000
**                                                                    * 01511000
**   FUNCTION =                                                       * 01512000
**     THIS SUB-FUNCTION IS CALLED EITHER BECAUSE A NORMAL RESET OF   * 01513000
**     THE DATA SET HAS OCCURRED OR BECAUSE AN ERROR CONDITION HAS    * 01514000
**     BEEN ENCOUNTERED IN IGGPRUS. IT PERFORMS THE FOLLOWING         * 01515000
**     FUNCTIONS:                                                     * 01516000
**                                                                    * 01517000
**       1.FREES THE REUSE WORK AREA                                  * 01518000
**       2.TURNS OFF RESET FLAG IN THE USERS CPL                      * 01519000
**       3.IF A KSDS HAS BEEN RESET THEN THE D ENTRY IS RELOADED INTO * 01520000
**         RECORD AREA 1. IF NOT A KSDS THE D ENTRY WILL ALREADY BE   * 01521000
**         IN RA1                                                     * 01522000
**       4.CALL LOCATE TO PROCESS USERS REQUESTS                      * 01523000
**       5.RETURN TO CALLER OF IGGPRUS                                * 01524000
**                                                                    * 01525000
**       NOTE: IF AN ERROR SITUATION EXISTS ONLY THE WORK AREA IS     * 01526000
**            FREED BEFORE EXIT                                       * 01527000
**                                                                    * 01528000
**   INPUT =  ADDRESS AND LENGTH OF REUSE WORK AREA                   * 01529000
**                                                                    * 01530000
**            FLAG TO INDICATE KSDS PROCESSING                        * 01531000
**                                                                    * 01532000
**            D ENTRY CI NUMBER                                       * 01533000
**                                                                    * 01534000
**            ERROR CODES                                             * 01535000
**                                                                    * 01536000
**   OUTPUT = REUSE WORK AREA FREED                                   * 01537000
**                                                                    * 01538000
**            RESET FLAG TURNED OFF IN USERS CPL(CTGREL)              * 01539000
**                                                                    * 01540000
**            USERS DATA FROM CALL TO LOCATE                          * 01541000
**                                                                    * 01542000
**            APPROPRIATE ERROR CODES SET                             * 01543000
**                                                                    * 01544000
**********************************************************************/ 01545000
*                                                                  0514 01546000
*   /*****************************************************************/ 01547000
*   /*                                                               */ 01548000
*   /* IF KSDS THEN SAVE D ENTRY CI NO IN CCASRCH. FREE REUSE WORK   */ 01549000
*   /* AREA. IF NO ERRORS TURN OFF CPL REUSE FLAG AND RELOAD D ENTRY */ 01550000
*   /* INTO RA1,IF KSDS PROCESSING.(D ENTRY REQD BY LOCATE           */ 01551000
*   /*                                                               */ 01552000
*   /*****************************************************************/ 01553000
*                                                                  0514 01554000
*   IF RUDCINO^=RU0 THEN            /* KSDS PROCESSING               */ 01555000
         L     @10,CCAREWKA(,R11)                                  0514 01556000
         L     @10,RUDCINO-1(,@10)                                 0514 01557000
         LA    @10,0(,@10)                                         0514 01558000
         LTR   @10,@10                                             0514 01559000
         BZ    @RF00514                                            0514 01560000
*     DO;                           /* YES                           */ 01561000
*       CCASRID=RUDCINO;            /* D ENTRY CI NO                 */ 01562000
         STCM  @10,7,CCASRID(R11)                                  0516 01563000
*     END;                                                         0517 01564000
*   ELSE                            /* NOT KSDS                      */ 01565000
*     CCASRCH=CCASRCH&&             /* CLEAR SRCH AREA               */ 01566000
*         CCASRCH;                                                 0518 01567000
         B     @RC00514                                            0518 01568000
@RF00514 XC    CCASRCH(44,R11),CCASRCH(R11)                        0518 01569000
*   RFY                                                            0519 01570000
*     R1 RSTD;                                                     0519 01571000
@RC00514 DS    0H                                                  0520 01572000
*   CCAMNLEN=REUWALNG;              /* LENGTH OF AREA TO FREE        */ 01573000
         L     @10,CCAREWKA(,R11)                                  0520 01574000
         MVC   CCAMNLEN(3,R11),REUWALNG+1(@10)                     0520 01575000
*   CCAMNADR=CCAREWKA;              /* ADDR OF AREA TO FREE          */ 01576000
         ST    @10,CCAMNADR(,R11)                                  0521 01577000
*   R1=ADDR(CCAMSSPL);              /* POINT TO STRG MGMT P.L.       */ 01578000
         LA    R1,CCAMSSPL(,R11)                                   0522 01579000
*   CCAMNSPL=USUBPOOL;              /* STORE SUBPOOL VALUE           */ 01580000
         MVI   CCAMNSPL(R11),X'FC'                                 0523 01581000
*   DO;                                                            0524 01582000
