*                                                            @ZA04701*/ 00492000
*       CCATEX1=ON;                 /* ASSUME NO MORE TESTS  @ZA04701*/ 00493000
         OI    CCATEX1(R11),B'00000010'                            0224 00494000
         NI    CCATEX2(R11),B'11110110'                            0224 00495000
*       CCATLNG=0;                  /* SET INITIAL LENGTH TESTED     */ 00496000
         SLR   @10,@10                                             0225 00497000
         STH   @10,CCATLNG(,R11)                                   0225 00498000
*       CCAILNG=CCAILNG-LENGTH(VARLEN);/* GET FIELD LENGTH           */ 00499000
         LA    @10,2                                               0226 00500000
         L     @15,CCAILNG(,R11)                                   0226 00501000
         SLR   @15,@10                                             0226 00502000
         ST    @15,CCAILNG(,R11)                                   0226 00503000
*       CCACPE7=CCACPE7+LENGTH(VARLEN);/* GET FIELD ADDRESS          */ 00504000
         AL    @10,CCACPE7(,R11)                                   0227 00505000
         ST    @10,CCACPE7(,R11)                                   0227 00506000
*       DO WHILE(CCATEX2=OFF);      /* RBA COMPARE LOOP              */ 00507000
         B     @DE00228                                            0228 00508000
@DL00228 DS    0H                                                  0229 00509000
*         R2=MIN(EXTLEN,CCAILNG-CCATLNG);/* GET MINIMUM LENGTH       */ 00510000
