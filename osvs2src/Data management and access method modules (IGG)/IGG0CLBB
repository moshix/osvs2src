*   R2=R3+LENGTH(RELINFO);          /* POINT NEXT FPL AREA           */ 02143000
         LA    R2,4                                                0588 02144000
         ALR   R2,R3                                               0588 02145000
*   CTGFIELD(2)=R2+LENGTH(FIELDNM); /* POINT TO FPL AREA             */ 02146000
         LR    @05,R2                                              0589 02147000
         ALR   @05,@09                                             0589 02148000
         ST    @05,CTGFIELD+4(,@14)                                0589 02149000
*   RFY                                                            0590 02150000
*     CTGFL BASED(CTGFIELD(2));     /* MAP VOLUME FPL                */ 02151000
*   CTGFLDNO=NODAT;                 /* INDICATE DATA                 */ 02152000
         MVI   CTGFLDNO(@05),X'01'                                 0591 02153000
*   CTGFLDNM=R2;                    /* FIELD NAME ADDRESS            */ 02154000
         ST    R2,CTGFLDNM(,@05)                                   0592 02155000
*   R4=ADDR(UPVOLVAR);              /* LENGTH FIXED INFORMATION      */ 02156000
         LA    R4,UPVOLVAR(,@15)                                   0593 02157000
*   RFY                                                            0594 02158000
*     VARFIELD BASED(R4);           /* MAP LOW KEY                   */ 02159000
*   R4=R4+LENGTH(VARLEN)+VARLEN;    /* ADD LOW KEY TO TOTAL LEN      */ 02160000
         LR    @14,R4                                              0595 02161000
         ALR   @14,@08                                             0595 02162000
         ICM   @07,12,VARLEN(R4)                                   0595 02163000
         SRA   @07,16                                              0595 02164000
         ALR   @14,@07                                             0595 02165000
         LR    R4,@14                                              0595 02166000
*   R4=R4+LENGTH(VARLEN)+VARLEN;    /* ADD HI KEY TO TOTAL LEN       */ 02167000
         LR    @14,R4                                              0596 02168000
         ALR   @14,@08                                             0596 02169000
         ICM   @07,12,VARLEN(R4)                                   0596 02170000
         SRA   @07,16                                              0596 02171000
         ALR   @14,@07                                             0596 02172000
         LR    R4,@14                                              0596 02173000
*   R4=R4-ADDR(UPVOLFIX);           /* SET ADDR OF FIXED INFO        */ 02174000
         SLR   R4,@15                                              0597 02175000
*   R3=R4+LENGTH(VARLEN);           /* LENGTH TO FIRST EXTENT        */ 02176000
         LR    R3,R4                                               0598 02177000
         ALR   R3,@08                                              0598 02178000
*   CTGFLPT(1)=ADDR(UPENTVOL);      /* POINT DATA INFO FOR VOL       */ 02179000
         ST    @15,CTGFLPT(,@05)                                   0599 02180000
*   IF UPRLRP^=0 THEN               /* IF EXISTENT OCCURRENCE        */ 02181000
         LH    @14,UPRLRP(,@15)                                    0600 02182000
         LTR   @14,@14                                             0600 02183000
         BZ    @RF00600                                            0600 02184000
*     DO;                           /* SET UP FOR UPDATE             */ 02185000
*       FIELDNM=UPDVOL;             /* INSERT FIELD NAME             */ 02186000
