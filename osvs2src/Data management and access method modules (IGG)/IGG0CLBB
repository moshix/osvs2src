         NI    CCAR3UR(R11),B'01111111'                            0222 00370000
*           IF CUNIQ=ON THEN        /* IF UNIQUE                     */ 00371000
         L     @10,CCARABB(,R11)                                   0223 00372000
         L     @10,CCACPE1(,@10)                                   0223 00373000
         TM    CUNIQ(@10),B'01000000'                              0223 00374000
         BNO   @RF00223                                            0223 00375000
