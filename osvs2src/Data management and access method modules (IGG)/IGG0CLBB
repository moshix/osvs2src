         ALR   @10,R2                                              0616 02224000
         L     @05,CTGFLDNM(,@10)                                  0616 02225000
         MVC   FIELDNM(8,@05),@CC00083                             0616 02226000
*       CTGFLNG(1)=R3;              /* LENGTH ENTVOL                 */ 02227000
         ST    R3,CTGFLNG(,@10)                                    0617 02228000
*     END;                          /* END ADD                       */ 02229000
*                                                                  0618 02230000
*   /*****************************************************************/ 02231000
*   /*                                                               */ 02232000
*   /* CALL TO MODULE IGG0CLAV                                       */ 02233000
*   /*                                                               */ 02234000
*   /*****************************************************************/ 02235000
*                                                                  0619 02236000
*Z053MOD:                                                          0619 02237000
*   CALL IGGPMOD;                   /* MODIFY AMDSB AND VOLUME     0619 02238000
*                                      OCCURRENCE                    */ 02239000
@RC00600 DS    0H                                                  0619 02240000
