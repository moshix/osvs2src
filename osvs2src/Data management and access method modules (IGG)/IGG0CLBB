         MVC   UPVOLSER(6,@10),UPEVOL(@01)                         0206 00318000
*       IF CTGNOFLD=NOKEYFPL THEN   /* IF KEY RANGE REQUEST          */ 00319000
         CLI   CTGNOFLD(@05),3                                     0207 00320000
         BNE   @RF00207                                            0207 00321000
*         DO;                       /* GET PARM INFO                 */ 00322000
*           RFY                                                    0209 00323000
*             CTGFL BASED(CTGFIELD(2));/* MAP LOW KEY FPL            */ 00324000
*           UPLKEYPT=CTGFLPT(1);    /* POINT TO LOW KEY              */ 00325000
         L     @15,CTGFIELD+4(,@05)                                0210 00326000
