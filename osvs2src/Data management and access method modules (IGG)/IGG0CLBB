*       +NOEXTFPL*LENGTH(CTGFIELD);                                0332 00813000
         LA    R2,32                                               0332 00814000
         ALR   R2,@14                                              0332 00815000
*   CTGFIELD(1)=R2+LENGTH(FIELDNM); /* EXTENT FPL                    */ 00816000
         LA    @09,8                                               0333 00817000
         LR    @08,R2                                              0333 00818000
         ALR   @08,@09                                             0333 00819000
         ST    @08,CTGFIELD(,@14)                                  0333 00820000
*   RFY                                                            0334 00821000
*     CTGFL BASED(CTGFIELD(1));     /* MAP EXTENT FPL                */ 00822000
*   CTGFLDNM=R2;                    /* POINT FIELD NAME AREA         */ 00823000
         ST    R2,CTGFLDNM(,@08)                                   0335 00824000
*   FIELDNM=EXTENT;                 /* INSERT EXTENT NAME            */ 00825000
         L     @01,CCAMCPL(,R11)                                   0336 00826000
         L     @01,CTGFIELD(,@01)                                  0336 00827000
         L     @14,CTGFLDNM(,@01)                                  0336 00828000
         MVC   FIELDNM(8,@14),@CC00073                             0336 00829000
*   CTGFLDNO=NODAT;                 /* DATA                          */ 00830000
         MVI   CTGFLDNO(@08),X'01'                                 0337 00831000
*   CTGFLPT(1)=ADDR(UPEND)+LENGTH(IDAAMDSB);/* EXTENT INFO ADDR      */ 00832000
         AL    @15,@CF00479                                        0338 00833000
         ST    @15,CTGFLPT(,@08)                                   0338 00834000
*   CTGFLNG(1)=0;                   /* DELETE LENGTH OF ZERO         */ 00835000
         SLR   @15,@15                                             0339 00836000
         ST    @15,CTGFLNG(,@08)                                   0339 00837000
*   CTGVLDSP=UPEXTLN+LENGTH(UPEXTLN1);/* SET DISPL                   */ 00838000
         LA    @15,2                                               0340 00839000
         LR    @14,@08                                             0340 00840000
         ALR   @14,@15                                             0340 00841000
         LH    @10,UPEXTLN(,@10)                                   0340 00842000
         ALR   @10,@15                                             0340 00843000
         STH   @10,CTGVLDSP(,@14)                                  0340 00844000
*   R2=ADDR(CTGFL)+LENGTH(CTGFL)+   /* POINT TEST FPL AREA           */ 00845000
*       NODAT*LENGTH(CTGFLDAT);                                    0341 00846000
         LA    R2,24                                               0341 00847000
         ALR   R2,@08                                              0341 00848000
*   CTGFLCHN=R2+LENGTH(FIELDNM);    /* CHAIN TEST FPL                */ 00849000
         ALR   @09,R2                                              0342 00850000
         ST    @09,CTGFLCHN(,@08)                                  0342 00851000
*   RFY                                                            0343 00852000
*     CTGFL BASED(R2+LENGTH(FIELDNM));/* MAP TEST FPL                */ 00853000
*   CTGFLDNM=R2;                    /* FIELD NAME ADDR               */ 00854000
         ST    R2,CTGFLDNM(,@09)                                   0344 00855000
*   FIELDNM=RELREPNO;               /* INSERT NAME RELREPNO          */ 00856000
         L     @10,CTGFLDNM+8(,R2)                                 0345 00857000
         MVC   FIELDNM(8,@10),@CC00081                             0345 00858000
*   CTGFLDNO=NODAT;                 /* DATA                          */ 00859000
         MVI   CTGFLDNO(@09),X'01'                                 0346 00860000
*   CTGFLDCD=CTGFLDEQ;              /* EQUAL RELREPNO TEST           */ 00861000
         MVI   CTGFLDCD(@09),X'80'                                 0347 00862000
*   CTGFLDGC=CATVOLGC;              /* GROUP CODE                    */ 00863000
         MVI   CTGFLDGC(@09),X'03'                                 0348 00864000
*   CTGFLPT(1)=ADDR(UPRLRP);        /* POINT TO TEST DATA            */ 00865000
         ST    @05,CTGFLPT(,@09)                                   0349 00866000
*   CTGFLNG(1)=LENGTH(UPRLRP);      /* LENGTH TEST DATA              */ 00867000
*                                                                  0350 00868000
         ST    @15,CTGFLNG(,@09)                                   0350 00869000
*   /*****************************************************************/ 00870000
*   /*                                                               */ 00871000
*   /* CALL TO MODULE IGG0CLAV                                       */ 00872000
*   /*                                                               */ 00873000
*   /*****************************************************************/ 00874000
*                                                                  0351 00875000
*Z023MOD:                                                          0351 00876000
*   CALL IGGPMOD;                   /* MODIFY EXTENTS                */ 00877000
Z023MOD  L     @15,@CV00039                                        0351 00878000
         BALR  @14,@15                                             0351 00879000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00880000
         BAL   @14,EXIT                                            0352 00881000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00882000
         DS 0F                                                          00883000
*   GEN(DC CL8'IGGPUALL');          /* NAME OF FOLLOWING PROCEDURE   */ 00884000
         DC CL8'IGGPUALL'                                               00885000
*   END;                            /* END OF INNER PROCEDURE        */ 00886000
@EL00003 DS    0H                                                  0355 00887000
@EF00003 DS    0H                                                  0355 00888000
@ER00003 BR    @14                                                 0355 00889000
@PB00003 DS    0H                                                  0356 00890000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 00891000
         EJECT                                                          00892000
*                                                                  0357 00893000
*/********************************************************************/ 00894000
*/*                                                                  */ 00895000
*/*        IGGPUALL  -  PROCEDURE  INTERNALLY  CALLED  NESTED        */ 00896000
*/*                                                                  */ 00897000
*/********************************************************************/ 00898000
*                                                                  0357 00899000
*IGGPUALL:                                                         0357 00900000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 00901000
         B     @PB00004                                            0357 00902000
IGGPUALL DS    0H                                                  0358 00903000
*Z024:                              /* LABEL FOR PROCEDURE TRACKING  */ 00904000
*Z024UALL:                          /* LABEL FOR PROCEDURE TRACKING  */ 00905000
*   RFY                                                            0358 00906000
*    (R5,                                                          0358 00907000
*     R6,                                                          0358 00908000
*     R7,                                                          0358 00909000
*     R8,                                                          0358 00910000
*     R9,                                                          0358 00911000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00912000
Z024     DS    0H                                                  0358 00913000
Z024UALL DS    0H                                                  0359 00914000
*   RFY                                                            0359 00915000
*    (R2,                                                          0359 00916000
*     R3,                                                          0359 00917000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00918000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00919000
         STM 12,14,12(13)                                               00920000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA     0361 00921000
*                                      PROCEDURE                     */ 00922000
         LA 13,12(13)                                                   00923000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00924000
**   MODULE NAME =  IGGPUALL (IGG0CLBB)                               * 00925000
**                                                                    * 00926000
**   DESCRIPTIVE NAME =  UNIQUE DATA SET ALLOCATION                   * 00927000
**                                                                    * 00928000
**   FUNCTION =                                                       * 00929000
**      THIS SUB-FUNCTION INTERFACES WITH IGGPSPAC (IGG0CLBP) TO      * 00930000
**      OBTAIN MORE EXTENTS FROM DADSM FOR A NEW UNIQUE DATA SPACE OR * 00931000
**      AN EXISTING UNIQUE DATA SPACE.  THE PROCESSING IS AS FOLLOWS: * 00932000
**      1. BUILD IGGPSPAC PARAMETER LIST FOR A UNIQUE DATA SET.       * 00933000
**      2. READ ASSOCIATED VOLUME ENTRY INTO RECORD AREA 3.           * 00934000
**      3. CALL IGGPSPAC.                                             * 00935000
**      IGGPSPAC WILL RETURN THE NEWLY ACQUIRED EXTENTS IN THE DADSM  * 00936000
**      EXTENTS SAVE AREA OF THE CCA (THE SAME AS VSAM SUB-ALLOCATE   * 00937000
**      (IGG0CLAR) DOES).                                             * 00938000
**                                                                    * 00939000
**   INPUT =                                                          * 00940000
**      RECORD AREAS 3,4, AND 5 AVAILABLE FOR USE (CCARBUR OFF).      * 00941000
**      BASE CATALOG RECORD OF THE DATA SET BEING EXTENDED IN RAB 1.  * 00942000
**         CSORT - ALLOCATION UNIT.                                   * 00943000
**         CNAME - DATA SET NAME.                                     * 00944000
**         CREUSE - REUSE FLAG.                               @Y30SSRS* 00945000
**         CPRIM - PRIMARY ALLOCATION QUANTITY.                       * 00946000
**         CSEC - SECONDARY ALLOCATION QUANTITY.                      * 00947000
**      UPREPARM PARAMETERS:                                          * 00948000
**         UPEXTLN - LENGTH OF OLD VOLUME OCCURRENCE EXTENTS.         * 00949000
**         UPNIEXT - NO. OF INDEX EXTENTS OR ZERO IF NOT SEQ. SET     * 00950000
**                   WITH DATA.                               @XA02392* 00951000
**      UPENTVOL PARAMETERS:                                          * 00952000
**         UPVOL - VOLUME SERIAL.                                     * 00953000
**         UPPHYB - PHYSICAL BLOCK SIZE.                              * 00954000
**         UPSSDS - DATA SET DIRECTORY SEQUENCE NO.                   * 00955000
**                                                                    * 00956000
**   OUTPUT =                                                         * 00957000
**      VOLUME ENTRY IN RECORD AREA 3.                                * 00958000
**      CCA EXTRACT/MODIFY INTERFACE FLAGS AND ADDRESSES CHANGED.     * 00959000
**      CCADESA - DADSM EXTENTS SAVE AREA.                            * 00960000
**         CCANDEXT - NUMBER OF NEW EXTENTS OBTAINED FOR UNIQUE DATA  * 00961000
**                    SET.                                            * 00962000
**         CCAEXTDE(*) - ARRAY OF EXTENT DESCRIPTORS INITIALIZED.     * 00963000
**            CCAEXTSS - DESCRIPTOR SPACE SEQUENCE NO.                * 00964000
**            CCAEXTAD - EXTENT STARTING CCHH.                        * 00965000
**            CCAEXTTH - NUMBER OF TRACKS IN EXTENT.                  * 00966000
**      UPREPARM PARAMETERS:                                          * 00967000
**         UPSMFL - SET ON, TO INDICATE LSPACE SMF RECORD 69 SHOULD   * 00968000
**                  BE WRITTEN.                                       * 00969000
**      CCACD1 - RCS, NO ERROR.                                       * 00970000
**             - RCNOSP, NO SPACE USER VOLUME.                        * 00971000
**             - RCDSEXT, DATA SET HAS MAXIMUM NO. OF EXTENTS.        * 00972000
**             - ANY ERROR RETURNED FROM IGGPSPAC.                    * 00973000
**      NEW DATA SPACE CREATED OR OLD DATA SPACE EXTENDED.            * 00974000
**                                                                    * 00975000
**                                                                    * 00976000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00977000
*                                                                  0362 00978000
*   DCL                                                            0362 00979000
*     MXVOLEXT FIXED(31) CONSTANT(16);/* MAX UNIQUE EXTENTS/VOL      */ 00980000
*   UPSPARM=UPSPARM&&UPSPARM;       /* ZERO SPACE WORK AREA          */ 00981000
         L     @10,CCAR3REC(,R11)                                  0363 00982000
         SL    @10,@CF00544                                        0363 00983000
         XC    UPSPARM(220,@10),UPSPARM(@10)                       0363 00984000
*   UPSOPT=CSORT;                   /* SPACE OPTIONS                 */ 00985000
         L     @05,CCARABB(,R11)                                   0364 00986000
         L     @05,CCACPE1(,@05)                                   0364 00987000
         MVC   UPSOPT(1,@10),CSORT(@05)                            0364 00988000
*   UPSUNIQ=ON;                     /* UNIQUE DATA SET               */ 00989000
         OI    UPSUNIQ(@10),B'10000000'                            0365 00990000
*   UPUDSNPT=ADDR(CNAME);           /* UNIQUE DATA SET NAME          */ 00991000
         LA    @15,CNAME(,@05)                                     0366 00992000
         ST    @15,UPUDSNPT(,@10)                                  0366 00993000
*   UPMXBLK=UPPHYB;                 /* BLOCKSIZE                     */ 00994000
         L     @15,CCAR2REC(,R11)                                  0367 00995000
         LA    @14,18                                              0367 00996000
         ALR   @14,@15                                             0367 00997000
         MVC   UPMXBLK(2,@10),UPPHYB+2(@14)                        0367 00998000
*   UPSECQTY=CSEC;                  /* SECONDARY ALLOC       @ZA07574*/ 00999000
         MVC   UPSECQTY(3,@10),CSEC(@05)                           0368 01000000
*   UPDSDIR=UPSSDS;                 /* DATA SET DIRECTORY SEQ. NO.   */ 01001000
         ICM   @14,12,UPSSDS(@14)                                  0369 01002000
         SRA   @14,16                                              0369 01003000
         STCM  @14,7,UPDSDIR(@10)                                  0369 01004000
*   IF UPEXTLN=0 THEN               /* IF NEW VOLUME                 */ 01005000
         LH    @15,UPEXTLN(,@15)                                   0370 01006000
         LTR   @15,@15                                             0370 01007000
         BNZ   @RF00370                                            0370 01008000
*     UPSPQTY=CPRIM;                /* USE PRIMARY ALLOCATION        */ 01009000
         MVC   UPSPQTY(3,@10),CPRIM(@05)                           0371 01010000
*   ELSE                            /* OLD VOLUME WITH EXTENTS       */ 01011000
*     IF CSEC=0 THEN                /* IF NO SECONDARY QUANTITY      */ 01012000
*                                                                  0372 01013000
         B     @RC00370                                            0372 01014000
@RF00370 L     @10,CCARABB(,R11)                                   0372 01015000
         L     @10,CCACPE1(,@10)                                   0372 01016000
         L     @10,CSEC-1(,@10)                                    0372 01017000
         LA    @10,0(,@10)                                         0372 01018000
         LTR   @10,@10                                             0372 01019000
         BNZ   @RF00372                                            0372 01020000
*       /*************************************************************/ 01021000
*       /*                                                           */ 01022000
*       /* SET CCAMODID='BB', CCAREASN=RSN4, CCARETRN=RCNOSP         */ 01023000
*       /*                                                           */ 01024000
*       /*************************************************************/ 01025000
*                                                                  0373 01026000
*       CCAPROB=IGGSEGID+(RSN4*K256)+RCNOSP;/* INDICATE NO SPACE   0373 01027000
*                                                            @Y30SSRS*/ 01028000
         MVC   CCAPROB(4,R11),@CF03389                             0373 01029000
*     ELSE                          /* USE SECONDARY QUANTITY        */ 01030000
*       UPSPQTY=CSEC;               /* SET QUANTITY                  */ 01031000
         B     @RC00372                                            0374 01032000
@RF00372 L     @10,CCAR3REC(,R11)                                  0374 01033000
         L     @05,CCARABB(,R11)                                   0374 01034000
         L     @05,CCACPE1(,@05)                                   0374 01035000
         AL    @10,@CF03422                                        0374 01036000
         MVC   UPSPQTY-1(3,@10),CSEC(@05)                          0374 01037000
*   IF UPNOEXT>=MXVOLEXT THEN       /* IF MAX EXTENTS ON VOLUME      */ 01038000
*                                                                  0375 01039000
