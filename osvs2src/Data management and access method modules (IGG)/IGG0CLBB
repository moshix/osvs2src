*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 02027000
**   MODULE NAME =  IGGPMVOL (IGG0CLBB)                               * 02028000
**                                                                    * 02029000
**   DESCRIPTIVE NAME =  MODIFY VOLUME OCCURRENCE                     * 02030000
**                                                                    * 02031000
**   FUNCTION =                                                       * 02032000
**      THIS SUB-FUNCTION CALLS MODIFY (IGGPMOD) TO UPDATE THE AMDSB  * 02033000
**      CONTROL BLOCK OCCURRENCE AND TO UPDATE OR ADD A NEW VOLUME    * 02034000
**      OCCURRENCE FOR THE PARTICULAR VSAM DATA SET BEING EXTENDED.   * 02035000
**      THE PROCESSING IS AS FOLLOWS:                                 * 02036000
**      1. BUILD THE IGGPMOD (IGG0CLAV) PARAMETER LISTS REQUIRED TO   * 02037000
**         UPDATE THE AMDSB. IF THE VOLUME OCCURRENCE SEQUENCE NUMBER * 02038000
**         IS NON-ZERO, THE OCCURRENCE ALREADY EXISTS AND MUST BE     * 02039000
**         UPDATED. OTHERWISE, IF THE SEQUENCE NUMBER IS ZERO, THE    * 02040000
**         VOLUME OCCURRENCE MUST BE ADDED.  (THE VOLUME OCCURRENCE   * 02041000
**         SEQUENCE NUMBER HAS BEEN ESTABLISHED BY IGGPSVOL           * 02042000
**         (IGG0CLBC).)  BUILD THE REST OF THE REQUIRED IGGPMOD       * 02043000
**         PARAMETER LISTS.                                           * 02044000
**      2. CALL IGGPMOD.  MODIFY WILL RETURN THE SEQUENCE NUMBER OF   * 02045000
**         THE VOLUME OCCURRENCE, IF THE OCCURRENCE WAS ADDED. OTHER- * 02046000
**         WISE, THE SEQUENCE NUMBER WILL ALREADY BE SET.             * 02047000
**                                                                    * 02048000
**   INPUT =                                                          * 02049000
**      CCA MODIFY INTERFACE FLAGS/ADDRESSES SET.                     * 02050000
**      IDAAMDSB - UPDATED IN-CORE COPY OF THE AMDSB CONTROL BLOCK.   * 02051000
**      UPENTVOL - UPDATED IN-CORE COPY OF AN EXISTING VOLUME         * 02052000
**                 OCCURRENCE OR A NEW VOLUME OCCURRENCE.             * 02053000
**                                                                    * 02054000
**   OUTPUT =                                                         * 02055000
**      IDAAMDSB - CATALOG COPY OF THE AMDSB CONTROL BLOCK FOR THE    * 02056000
**                 PARTICULAR DATA SET UPDATED.                       * 02057000
**      UPENTVOL - NEW CATALOG VOLUME OCCURRENCE ADDED OR UPDATED.    * 02058000
**         UPRLRP - SEQUENCE NUMBER OF VOLUME OCCURRENCE (RETURN BY   * 02059000
**                  IGGPMOD, IF IT WAS AN ADD SITUATION).             * 02060000
**      CCACD1 - RCS, NO ERROR.                                       * 02061000
**             - ANY ERROR CODE RETURNED FROM IGGPMOD.                * 02062000
**                                                                    * 02063000
**                                                                    * 02064000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 02065000
*                                                                  0563 02066000
*   RFY                                                            0563 02067000
*     RELINFO BASED(R3);            /* MAP RELREPNO FIELD            */ 02068000
*   CCAMCPL=ADDR(UPEND)+LENGTH(IDAAMDSB);/* POINT AREA FOR MODIFY CPL*/ 02069000
         LA    @10,96                                              0564 02070000
         L     @05,CCAR2REC(,R11)                                  0564 02071000
         LA    @15,18                                              0564 02072000
         ALR   @15,@05                                             0564 02073000
         LA    @14,UPEND(,@15)                                     0564 02074000
         ALR   @14,@10                                             0564 02075000
         ST    @14,CCAMCPL(,R11)                                   0564 02076000
*   RFY                                                            0565 02077000
*     CLEAR BASED(CCAMCPL);         /* MAP WORK AREA                 */ 02078000
*   CLEAR=CLEAR&&CLEAR;             /* CLEAR WORK AREA               */ 02079000
         XC    CLEAR(256,@14),CLEAR(@14)                           0566 02080000
*   CTGNOFLD=NOAVFPL;               /* NO. FPL'S FOR AMDSB AND VOLUME   02081000
*                                      OCCURRENCE                    */ 02082000
         MVI   CTGNOFLD(@14),X'02'                                 0567 02083000
*   R2=LENGTH(CTGPL)+CCAMCPL        /* POINT TO AMDSB FPL AREA       */ 02084000
*       +NOAVFPL*LENGTH(CTGFIELD);                                 0568 02085000
         LA    R2,36                                               0568 02086000
         ALR   R2,@14                                              0568 02087000
*   CTGFIELD(1)=R2+LENGTH(FIELDNM); /* AMDSB FPL                     */ 02088000
         LA    @09,8                                               0569 02089000
         LR    @08,R2                                              0569 02090000
         ALR   @08,@09                                             0569 02091000
         ST    @08,CTGFIELD(,@14)                                  0569 02092000
*   RFY                                                            0570 02093000
*     CTGFL BASED(CTGFIELD(1));     /* MAP CURRENT FPL               */ 02094000
*   CTGFLDNM=R2;                    /* POINT FIELD NAME AREA         */ 02095000
         ST    R2,CTGFLDNM(,@08)                                   0571 02096000
*   FIELDNM=AMDSBCAT;               /* INSERT AMDSB NAME             */ 02097000
         L     @01,CCAMCPL(,R11)                                   0572 02098000
         L     @01,CTGFIELD(,@01)                                  0572 02099000
         L     @07,CTGFLDNM(,@01)                                  0572 02100000
         MVC   FIELDNM(8,@07),@CC00085                             0572 02101000
*   CTGFLDNO=NODAT;                 /* INDICATE DATA                 */ 02102000
         MVI   CTGFLDNO(@08),X'01'                                 0573 02103000
*   CTGFLPT(1)=ADDR(IDAAMDSB);      /* ADDRESS OF AMDSB              */ 02104000
         LA    @05,UPREND(,@05)                                    0574 02105000
         AL    @05,@CF00522                                        0574 02106000
         ST    @05,CTGFLPT(,@08)                                   0574 02107000
*   CTGFLNG(1)=LENGTH(IDAAMDSB);    /* LENGTH OF AMDSB               */ 02108000
         ST    @10,CTGFLNG(,@08)                                   0575 02109000
*   R2=ADDR(CTGFL)+LENGTH(CTGFL)+   /* POINT TO TEST FPL AREA        */ 02110000
*       NODAT*LENGTH(CTGFLDAT);                                    0576 02111000
         LA    @10,24                                              0576 02112000
         LR    R2,@08                                              0576 02113000
         ALR   R2,@10                                              0576 02114000
*   CTGFLCHN=R2+LENGTH(FIELDNM);    /* CHAIN TEST FPL                */ 02115000
         LR    @05,R2                                              0577 02116000
         ALR   @05,@09                                             0577 02117000
         ST    @05,CTGFLCHN(,@08)                                  0577 02118000
*   RFY                                                            0578 02119000
*     CTGFL BASED(R2+LENGTH(FIELDNM));/* MAP TEST FPL                */ 02120000
*   CTGFLDNM=R2;                    /* FIELD NAME ADDRESS            */ 02121000
         ST    R2,CTGFLDNM(,@05)                                   0579 02122000
*   FIELDNM=RELREPNO;               /* INSERT NAME RELREPNO          */ 02123000
         L     @08,CTGFLDNM+8(,R2)                                 0580 02124000
         MVC   FIELDNM(8,@08),@CC00081                             0580 02125000
*   CTGFLDNO=NODAT;                 /* DATA                          */ 02126000
         MVI   CTGFLDNO(@05),X'01'                                 0581 02127000
*   CTGFLDCD=CTGFLDEQ;              /* EQUAL RELREPNO TEST           */ 02128000
         MVI   CTGFLDCD(@05),X'80'                                 0582 02129000
*   CTGFLDGC=CATDSBGC;              /* GROUP CODE                    */ 02130000
         MVI   CTGFLDGC(@05),X'01'                                 0583 02131000
*   R3=ADDR(CTGFL)+LENGTH(CTGFL)+   /* POINT TO NEXT FPL AREA        */ 02132000
*       NODAT*LENGTH(CTGFLDAT);                                    0584 02133000
         LA    R3,32                                               0584 02134000
         ALR   R3,R2                                               0584 02135000
*   CTGFLPT(1)=R3;                  /* SET RELREPNO DATA ADDR        */ 02136000
         ST    R3,CTGFLPT(,@05)                                    0585 02137000
