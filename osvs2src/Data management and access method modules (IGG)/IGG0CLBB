@RC00370 L     @10,CCAR2REC(,R11)                                  0375 01041000
         CLI   UPNOEXT+18(@10),16                                  0375 01042000
         BL    @RF00375                                            0375 01043000
*     /***************************************************************/ 01044000
*     /*                                                             */ 01045000
*     /* SET CCAMODID='BB', CCAREASN=RSN8, CCARETRN=RCNOSP           */ 01046000
*     /*                                                             */ 01047000
*     /***************************************************************/ 01048000
*                                                                  0376 01049000
*     CCAPROB=IGGSEGID+(RSN8*K256)+RCNOSP;/* INDICATE NO SPACE     0376 01050000
*                                                            @Y30SSRS*/ 01051000
         MVC   CCAPROB(4,R11),@CF03392                             0376 01052000
