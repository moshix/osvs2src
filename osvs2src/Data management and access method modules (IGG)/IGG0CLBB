         LH    @05,UPRLRP+18(,@05)                                 0680 02527000
         STH   @05,WKARELSS(,@10)                                  0680 02528000
*               IF CCACD1^=RCS THEN                                0681 02529000
         CLI   CCACD1(R11),0                                       0681 02530000
         BNE   @RT00681                                            0681 02531000
*                 ;                 /* ERROR, END PROCESS            */ 02532000
*               ELSE                /* NO ERROR                      */ 02533000
*                 DO;               /* UPDATE EXTENTS                */ 02534000
*Z060CEXT:                                                         0684 02535000
*                   CALL IGGPCEXT;  /* COMPUTE EXTENTS               */ 02536000
Z060CEXT BAL   @14,IGGPCEXT                                        0684 02537000
*Z061MEXT:                                                         0685 02538000
*                   CALL IGGPMEXT;  /* MODIFY EXTENT INFO            */ 02539000
Z061MEXT BAL   @14,IGGPMEXT                                        0685 02540000
