         L     @01,CTGFIELD+4(,@01)                                0602 02188000
         L     @14,CTGFLDNM(,@01)                                  0602 02189000
         MVC   FIELDNM(8,@14),@CC00056                             0602 02190000
*       CTGFLNG(1)=R4;              /* SET LENGTH                    */ 02191000
         ST    R4,CTGFLNG(,@05)                                    0603 02192000
*       R2=ADDR(CTGFL)+LENGTH(CTGFL)/* POINT TO TEST FPL             */ 02193000
*           +NODAT*LENGTH(CTGFLDAT);                               0604 02194000
         ALR   @10,@05                                             0604 02195000
         LR    R2,@10                                              0604 02196000
*       CTGFLCHN=R2+LENGTH(FIELDNM);/* CHAIN TEST FPL                */ 02197000
         ALR   @09,R2                                              0605 02198000
         ST    @09,CTGFLCHN(,@05)                                  0605 02199000
*       RFY                                                        0606 02200000
*         CTGFL BASED(R2+LENGTH(FIELDNM));/* MAP TEST FPL            */ 02201000
*       CTGFLDNO=NODAT;             /* INDICATE DATA                 */ 02202000
         MVI   CTGFLDNO(@09),X'01'                                 0607 02203000
*       CTGFLDGC=CATVOLGC;          /* VOLUME GROUP CODE             */ 02204000
         MVI   CTGFLDGC(@09),X'03'                                 0608 02205000
