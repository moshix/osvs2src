         MVC   FIELDNM(8,@10),@CC00081                             0611 02212000
*       CTGFLPT(1)=ADDR(UPRLRP);    /* ADDR TEST RELREPNO            */ 02213000
         ST    @15,CTGFLPT(,@09)                                   0612 02214000
*       CTGFLNG(1)=LENGTH(UPRLRP);  /* LENGTH RELREPNO               */ 02215000
         ST    @08,CTGFLNG(,@09)                                   0613 02216000
