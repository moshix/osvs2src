         SL    R13,@CF00119                                        0367 00774000
*   RFY                                                            0368 00775000
*    (R0,                                                          0368 00776000
*     R1) UNRSTD;                   /*                       @Y30SSSM*/ 00777000
*   IF R15^=OKAY THEN               /* CHECK FOR VOL MOUNT ERROR     */ 00778000
         SLR   @14,@14                                             0369 00779000
         CR    R15,@14                                             0369 00780000
         BE    @RF00369                                            0369 00781000
*     DO;                           /* ERROR                 @Y30SSSM*/ 00782000
*       DO;                                                        0371 00783000
*         CCAMODID='BL';            /* SET MODULE ID                 */ 00784000
         MVC   CCAMODID(2,R11),@CC04152                            0372 00785000
*         CCAREASN=VVOLLPDC;        /* SET REASON CODE               */ 00786000
         L     @10,CCACMSWA(,R11)                                  0373 00787000
         IC    @05,VVOLLPDC+454(,@10)                              0373 00788000
         STC   @05,CCAREASN(,R11)                                  0373 00789000
*         CCARETRN=RCNMNTD;         /* SET RETURN CODE               */ 00790000
         MVI   CCARETRN(R11),X'48'                                 0374 00791000
*       END;                        /* SET PROPER ERROR CODE @Y30SSSM*/ 00792000
*       IF CTGWKA^=NONE THEN        /* CHECK FOR USER WORK AREA    0376 00793000
*                                                            @Y30SSSM*/ 00794000
