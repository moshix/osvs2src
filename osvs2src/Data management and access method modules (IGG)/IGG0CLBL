*                                                                  0507 01484000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 01485000
**   MODULE NAME =  IGGPDLET                                          * 01486000
**                                                                    * 01487000
**   DESCRIPTIVE NAME =  DELETE VOLUME ENTRY RECORD AND UPDATE VTOC   * 01488000
**                                                                    * 01489000
**   COPYRIGHT =  NONE                                                * 01490000
**                                                                    * 01491000
**   STATUS =  RELEASE 1, LEVEL 0                                     * 01492000
**                                                                    * 01493000
**   FUNCTION =                                                       * 01494000
**      THIS SUBFUNCTION DELETES THE VOLUME ENTRY RECORD AND          * 01495000
**      ALL OF ITS EXTENSION RECORDS. ALSO, THE FORMAT 4 DSCB         * 01496000
**      MAINTENANCE ROUTINES OF VSAM CATALOG MANAGEMENT ARE CALLED    * 01497000
**      TO UPDATE THE VTOC ON THE VOLUME TO REMOVE VSAM USAGE         * 01498000
**      INDICATORS.                                                   * 01499000
**      1. DELETE THE BASE RECORD AND ALL EXTENSIONS.                 * 01500000
**      2. CALL THE FORMAT 4 DSCB READ ROUTINE.                       * 01501000
**      3. UPDATE THE FORMAT 4 DSCB.                                  * 01502000
**      4. CALL THE FORMAT 4 DSCB WRITE ROUTINE.                      * 01503000
**      5. PASS THE DELETED ENTRY NAME TO THE USER.                   * 01504000
**      6. RETURN TO THE MAIN ROUTINE.                                * 01505000
**                                                                    * 01506000
**   INPUT =                                                          * 01507000
**      IGGCCA                                                        * 01508000
**        CCARABB                                                     * 01509000
**        CCACMSWA                                                    * 01510000
**        CCAR0REC                                                    * 01511000
**        CCAURAB                                                     * 01512000
**          CCAR1REC                                                  * 01513000
**        CCACPL                                                      * 01514000
**      VOLUME ENTRY RECORD                                           * 01515000
**        EXTGOCI                                                     * 01516000
**        NREPLF                                                      * 01517000
**      IDAVVOLL                                                      * 01518000
**        VVOLLUCB                                                    * 01519000
**      IEZCTGPL                                                      * 01520000
**        CTGWKA                                                      * 01521000
**                                                                    * 01522000
**   OUTPUT =                                                         * 01523000
**      IECSDSL1                                                      * 01524000
**        IECSDSF4                                                    * 01525000
**          DS4AMCAT                                                  * 01526000
**          DS4AMTIM                                                  * 01527000
**      DELETED OBJECT NAME                                           * 01528000
**        IGGMF4WA                                                    * 01529000
**          F4WDUCB                                                   * 01530000
**                                                                    * 01531000
**     MESSAGES =  NONE                                               * 01532000
**                                                                    * 01533000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01534000
*                                                                  0507 01535000
*   /*****************************************************************/ 01536000
*   /*                                                               */ 01537000
*   /* IF CATALOG IS RECOVERABLE, CLOSE CRA IF OPEN          @Y30SSSM*/ 01538000
*   /*                                                               */ 01539000
*   /*****************************************************************/ 01540000
*                                                                  0507 01541000
*   IF CAXF2REC=ON THEN             /* RECOVERABLE CATALOG   @Y30SSSM*/ 01542000
         L     @14,CCAACB(,R11)                                    0507 01543000
         L     @14,ACBUAPTR(,@14)                                  0507 01544000
         TM    CAXF2REC(@14),B'00000010'                           0507 01545000
         BNO   @RF00507                                            0507 01546000
*     DO;                           /* YES, ORIENT TO CRA ACB      0508 01547000
*                                                            @Y30SSSM*/ 01548000
*       RFY                                                        0509 01549000
*         R1 RSTD;                  /*                       @Y30SSSM*/ 01550000
*       CCARAACB=NONE;              /* CLEAR CRA ACB PTR     @Y30SSSM*/ 01551000
         SLR   @10,@10                                             0510 01552000
         ST    @10,CCARAACB(,R11)                                  0510 01553000
*       R1=CCAACB->ACBUAPTR->CAXCRACB;/* GET FIRST CRA ACB POINTER 0511 01554000
*                                                            @Y30SSSM*/ 01555000
         L     R1,CAXCRACB(,@14)                                   0511 01556000
*       IF R1^=NONE THEN            /* CRA CAX CHAIN EXIST   @Y30SSSM*/ 01557000
         CR    R1,@10                                              0512 01558000
         BE    @RF00512                                            0512 01559000
*         DO;                       /* YES, CHAIN EXISTS     @Y30SSSM*/ 01560000
*           R1=R1->ACBUAPTR;        /* POINT TO 1ST CRA CAX  @Y30SSSM*/ 01561000
         L     R1,ACBUAPTR(,R1)                                    0514 01562000
*           DO WHILE(CCARAACB=NONE&R1^=NONE);/* SEARCH CRA CAX CHAIN    01563000
*                                                            @Y30SSSM*/ 01564000
         B     @DE00515                                            0515 01565000
@DL00515 DS    0H                                                  0516 01566000
*             RFY                                                  0516 01567000
*               IGGCAXWA BASED(R1); /*                       @Y30SSSM*/ 01568000
*             IF RCDCRAVL=CAXVOLID THEN/* DO RECORD AND CRA VOLSERS     01569000
*                                      MATCH                 @Y30SSSM*/ 01570000
         L     @14,CCAURAB(,R11)                                   0517 01571000
         L     @14,CCACPE1(,@14)                                   0517 01572000
         CLC   RCDCRAVL(6,@14),CAXVOLID(R1)                        0517 01573000
         BNE   @RF00517                                            0517 01574000
*               CCARAACB=CAXCRACB;  /* YES, CRA ACB HAS BEEN FOUND 0518 01575000
*                                                            @Y30SSSM*/ 01576000
         L     @14,CAXCRACB(,R1)                                   0518 01577000
         ST    @14,CCARAACB(,R11)                                  0518 01578000
*             ELSE                  /* CRA ACB NOT YET FOUND @Y30SSSM*/ 01579000
*               R1=CAXCHN;          /* PT TO NXT CRA CAX     @Y30SSSM*/ 01580000
         B     @RC00517                                            0519 01581000
@RF00517 L     R1,CAXCHN(,R1)                                      0519 01582000
*             RFY                                                  0520 01583000
*               IGGCAXWA BASED(ACBUAPTR);/*                  @Y30SSSM*/ 01584000
@RC00517 DS    0H                                                  0521 01585000
*           END;                    /*                       @Y30SSSM*/ 01586000
@DE00515 SLR   @14,@14                                             0521 01587000
         C     @14,CCARAACB(,R11)                                  0521 01588000
         BNE   @DC00515                                            0521 01589000
         CR    R1,@14                                              0521 01590000
         BNE   @DL00515                                            0521 01591000
@DC00515 DS    0H                                                  0522 01592000
*         END;                      /*                       @Y30SSSM*/ 01593000
*       ELSE                                                       0523 01594000
*         ;                         /* NO CAX CHAIN          @Y30SSSM*/ 01595000
@RF00512 DS    0H                                                  0524 01596000
*       RFY                                                        0524 01597000
*         R1 UNRSTD;                /*                       @Y30SSSM*/ 01598000
*       CCASCRA=ON;                 /* SUPPRESS CRA OUTPUT   @Y30SSSM*/ 01599000
         OI    CCASCRA(R11),B'00100000'                            0525 01600000
*       IF CCARAACB^=NONE THEN      /* CRA ACB PTR FOUND     @Y30SSSM*/ 01601000
         L     @14,CCARAACB(,R11)                                  0526 01602000
         LTR   @14,@14                                             0526 01603000
         BZ    @RF00526                                            0526 01604000
*         DO;                       /* YES, MUST CLOSE CRA   @Y30SSSM*/ 01605000
*           CCACD1=RCEE;            /* PREVENT REOPEN        @Y30SSSM*/ 01606000
*                                                                  0528 01607000
         MVI   CCACD1(R11),X'FC'                                   0528 01608000
*           /*********************************************************/ 01609000
*           /*                                                       */ 01610000
*           /* CALL TO MODULE IGG0CLAD                               */ 01611000
*           /*                                                       */ 01612000
*           /*********************************************************/ 01613000
*                                                                  0529 01614000
