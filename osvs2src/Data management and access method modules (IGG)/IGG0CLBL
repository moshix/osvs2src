*   CTGFL=CTGFL&&CTGFL;             /* CLEAR MAIN FPL AREA           */ 01197000
         XC    CTGFL(16,@14),CTGFL(@14)                            0449 01198000
*   CTGNOFLD=INCR1;                 /* NO. OF MAIN FPLS PRESENT      */ 01199000
