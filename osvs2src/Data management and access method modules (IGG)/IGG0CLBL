*   RFY                                                            0363 00762000
*    (R0,                                                          0363 00763000
*     R1,                                                          0363 00764000
*     R15) RSTD;                                                   0363 00765000
*   R15=R0;                         /* GET ROUTINE ENTRY ADDRESS     */ 00766000
         LR    R15,R0                                              0364 00767000
*   R1=ADDR(IDAVVOLL);              /* SET PARAMETER LIST ADDR       */ 00768000
         LA    R1,454                                              0365 00769000
