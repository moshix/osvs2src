         LA    @05,@CC04127                                        0451 01209000
         ST    @05,CTGFLDNM(,@14)                                  0451 01210000
*   CTGFLDNO=INCR1;                 /* SET NO. LENGTH/DATA PTRS      */ 01211000
         MVI   CTGFLDNO(@14),X'01'                                 0452 01212000
*   CTGFLCHN=DELTSFPL;              /* SET THE TEST FPL REF          */ 01213000
