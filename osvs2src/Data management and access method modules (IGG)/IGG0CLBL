*   CTGFIELD(1)=ADDR(DELFPLDT);     /* DATA FPL ADDRESS-IN CPL       */ 00972000
         LA    @14,DELFPLDT(,@10)                                  0402 00973000
         ST    @14,CTGFIELD(,@15)                                  0402 00974000
*   CTGFL=CTGFL&&CTGFL;             /* CLEAR THE FPL STORAGE         */ 00975000
