         BNO   @RF00355                                            0355 00737000
*     VVOLLACB=NONE;                /* NO TIMESTAMP CHECK    @Y30SSSM*/ 00738000
         SLR   @10,@10                                             0356 00739000
         ST    @10,VVOLLACB(,@05)                                  0356 00740000
*   ELSE                            /* NOT FORCE DELETE      @Y30SSSM*/ 00741000
*     VVOLLACB=CCAACB;              /* SET CATALOG ACB ADDRESS     0357 00742000
