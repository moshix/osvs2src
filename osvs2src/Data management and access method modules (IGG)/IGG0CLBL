*                                                                  0397 00956000
*   RFY                                                            0397 00957000
*     CTGPL BASED(ADDR(DELCPLST));                                 0397 00958000
*   RFY                                                            0398 00959000
*     CTGFL BASED(CTGFIELD(1));                                    0398 00960000
*   CCAXCPL=ADDR(DELCPLST);         /* EXTRACT CPL ADDRESS           */ 00961000
         L     @10,CCACMSWA(,R11)                                  0399 00962000
         LA    @05,DELCPLST(,@10)                                  0399 00963000
         ST    @05,CCAXCPL(,R11)                                   0399 00964000
*   DELTSFPL=ADDR(DELFPLT1);        /* SET TEST FPL ADDRESS          */ 00965000
         LA    @05,DELFPLT1(,@10)                                  0400 00966000
         ST    @05,DELTSFPL(,@10)                                  0400 00967000
*   CTGPL=CTGPL&&CTGPL;             /* CLEAR THE CPL STORAGE         */ 00968000
         LA    @15,52                                              0401 00969000
