           TR    DELSF1NA+21(14,R2),DELSF1TR-240 TRANSLATE TO PRINT     01396000
*   DELSF1NA(20:26)=DELSF1NA(22:28);/* SET NAME IN POSITION          */ 01397000
         L     @10,CCACMSWA(,R11)                                  0484 01398000
         MVC   @TS00001(7),DELSF1NA+21(@10)                        0484 01399000
         MVC   DELSF1NA+19(7,@10),@TS00001                         0484 01400000
*   DELSF1NA(27:28)=DELIM;          /* SET DELIMITER CHARACTERS      */ 01401000
         MVC   DELSF1NA+26(2,@10),@CC03881                         0485 01402000
*   DELSF1NA(36:37)=RESET;          /* RESET THE PERIOD IN NAME      */ 01403000
*                                                                  0486 01404000
         MVC   DELSF1NA+35(2,@10),@CC04136                         0486 01405000
*   /*****************************************************************/ 01406000
*   /*                                                               */ 01407000
