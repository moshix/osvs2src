         B     @PB00008                                            0515 02023000
IGGPCLGT DS    0H                                                  0516 02024000
*Z040:                              /* LABEL FOR PROCEDURE TRACKING  */ 02025000
*Z040CLGT:                          /* LABEL FOR PROCEDURE TRACKING  */ 02026000
*   RFY                                                            0516 02027000
*    (R5,                                                          0516 02028000
*     R6,                                                          0516 02029000
*     R7,                                                          0516 02030000
*     R8,                                                          0516 02031000
*     R9,                                                          0516 02032000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 02033000
Z040     DS    0H                                                  0516 02034000
Z040CLGT DS    0H                                                  0517 02035000
*   RFY                                                            0517 02036000
*    (R2,                                                          0517 02037000
*     R3,                                                          0517 02038000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 02039000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 02040000
         STM 12,14,12(13)                                               02041000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 02042000
*                                                                  0519 02043000
         LA 13,12(13)                                                   02044000
*   /*****************************************************************/ 02045000
*   /*                                                               */ 02046000
*   /* IGGPCLGT - THIS INTERNAL ROUTINE RECEIVES CONTROL TO GET AN   */ 02047000
*   /* ASSOCIATED CLUSTER FOR A DATA OR INDEX ENTRY THAT IS BEING    */ 02048000
*   /* PROCESSED FOR LOCATE, ALTER OR LIST CAT REQUESTS. IF THE      */ 02049000
*   /* CLUSTER IS SECURITY PROTECTED, A CHECK IS MADE TO SEE IF THE  */ 02050000
*   /* USER HAS MASTER LEVEL AUTH TO THE CLUSTER, WHICH IS SUFFICIENT*/ 02051000
*   /* FOR REQUESTS OF THIS TYPE. ALSO, IF A DATA OR INDEX OBJECT IS */ 02052000
*   /* THE ORIGINAL RECORD TO BE CHECKED AND PATH RECORDS EXIST OVER */ 02053000
*   /* THE BASE CLUSTER OR AIX, THEN MASTER AUTHORIZATION TO THE PATH*/ 02054000
*   /* IS ACCEPTABLE IF IT IS PROVIDED. IN ADDITION, IF THE BASE     */ 02055000
*   /* CLUSTER OR AIX IS NOT PROTECTED, BUT THE DATA OR INDEX ARE,   */ 02056000
*   /* AND IT IS AN OPEN(LOCATE) REQUEST, NO SECURITY IS REQUIRED.   */ 02057000
*   /*                                                       @Y30SSSB*/ 02058000
*   /*                                                               */ 02059000
*   /*****************************************************************/ 02060000
*                                                                  0520 02061000
*   RFY                                                            0520 02062000
*     CTGPL BASED(CCACPL);          /*                       @Y30SSSB*/ 02063000
*                                                                  0520 02064000
*   /*****************************************************************/ 02065000
*   /*                                                               */ 02066000
*   /* DECLARATION OF THE CLUSTER ASSOCIATION OCCURRENCE WITHIN THE  */ 02067000
*   /* BASE DATA OR INDEX RECORD                                     */ 02068000
*   /*                                                               */ 02069000
*   /*****************************************************************/ 02070000
*                                                                  0521 02071000
*   DCL                                                            0521 02072000
*     1 ASOCDATA BASED(R4),                                        0521 02073000
*      2 ASOCCNVR PTR(8),           /* COUNT OF VARIABLE FIELDS      */ 02074000
*      2 ASOCLNFX PTR(8),           /* LENGTH OF FIXED DATA          */ 02075000
*      2 ASOCKIND CHAR(1),          /* ASSOCIATION TYPE              */ 02076000
*      2 ASOCCINO PTR(24) BDY(BYTE);/* ASSOCIATION CI VALUE          */ 02077000
*   REGETCI=RCDCINO;                /* SAVE CURRENT RECORD CI        */ 02078000
         L     @10,CCAR5REC(,R11)                                  0522 02079000
         L     @05,CCAURAB(,R11)                                   0522 02080000
         L     @15,CCACPE1(,@05)                                   0522 02081000
         MVC   REGETCI(3,@10),RCDCINO(@15)                         0522 02082000
*   CKAUBASE=ON;                    /* SET RE-GET BASE RECORD        */ 02083000
         OI    CKAUBASE(@10),B'00010000'                           0523 02084000
*   CKAUNOSV=OFF;                   /* INDICATE REC SAVED      Y02020*/ 02085000
         NI    CKAUNOSV(@10),B'10111111'                           0524 02086000
*   CCAR4REC->CATRECRD(1)=CATRECRD(1);                             0525 02087000
         L     @14,CCAR4REC(,R11)                                  0525 02088000
         MVC   CATRECRD(256,@14),CATRECRD(@15)                     0525 02089000
*   CCAR4REC->CATRECRD(2)=CATRECRD(2);                             0526 02090000
         MVC   CATRECRD+256(256,@14),CATRECRD+256(@15)             0526 02091000
*   CCARAB4=CCARAB;                 /* SAVE THE CPE POINTERS         */ 02092000
         MVC   CCARAB4(20,R11),CCARAB(@05)                         0527 02093000
*   CKAUNOSV=OFF;                   /* INDICATE RECORD SAVED         */ 02094000
*                                                                  0528 02095000
         NI    CKAUNOSV(@10),B'10111111'                           0528 02096000
*   /*****************************************************************/ 02097000
*   /*                                                               */ 02098000
*   /* LOCATE THE ASSOCIATION TO THE CLUSTER AND READ THE CLUSTER    */ 02099000
*   /* RECORD                                                        */ 02100000
*   /*                                                               */ 02101000
*   /*****************************************************************/ 02102000
*                                                                  0529 02103000
*   R4=CCACPE3+INCR2+LENGTH(IDAAMDSB);/* GRP OCCUR ADDR              */ 02104000
         LA    R4,98                                               0529 02105000
         AL    R4,CCACPE3(,@05)                                    0529 02106000
*   CKAUTCIN=ASOCCINO;              /* SET AIX, CLUS CI      @Y30DSSB*/ 02107000
