         BO    @RT00311                                            0311 01047000
@GL00034 L     @10,CCACPL(,R11)                                    0311 01048000
         CLI   CTGTYPE(@10),C'X'                                   0311 01049000
         BE    @RT00311                                            0311 01050000
         CLI   CTGTYPE(@10),C'B'                                   0311 01051000
         BNE   @RF00311                                            0311 01052000
@RT00311 DS    0H                                                  0312 01053000
*           CTGUPD=ON;              /* UPDATE FOR VOLUME DELETE      */ 01054000
         L     @10,CCACPL(,R11)                                    0312 01055000
         OI    CTGUPD(@10),B'00010000'                             0312 01056000
*         ELSE                      /* ALTER OR OTHER TYPE DELET     */ 01057000
*           IF CTGOPTNS=CTGALTER&   /* OPTION IS ALTER       @ZA24944*/ 01058000
*               CCASAVID=CTGTALIN|  /* NON-VSAM RECORD       @ZA24944*/ 01059000
*               CCASAVID=CTGTGBS    /* OR GDG RECD THEN      @ZA24944*/ 01060000
*             THEN                                                 0313 01061000
         B     @RC00311                                            0313 01062000
@RF00311 L     @10,CCACPL(,R11)                                    0313 01063000
         TM    CTGOPTNS(@10),B'00010000'                           0313 01064000
         BNO   @GL00039                                            0313 01065000
         TM    CTGOPTNS(@10),B'11101000'                           0313 01066000
         BNZ   @GL00039                                            0313 01067000
