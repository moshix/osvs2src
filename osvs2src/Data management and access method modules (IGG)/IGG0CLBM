         BAL   @14,EXIT                                            0246 00641000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00642000
         DS 0F                                                          00643000
*   GEN(DC CL8'IGGPCKEX');          /* NAME OF FOLLOWING PROCEDURE   */ 00644000
         DC CL8'IGGPCKEX'                                               00645000
*   END;                            /* END OF INNER PROCEDURE        */ 00646000
