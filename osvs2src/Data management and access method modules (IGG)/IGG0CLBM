*             CTGMAST=ON;           /* MASTER IS REQUIRED      Y02020*/ 01077000
         B     @RC00313                                            0315 01078000
@RF00313 L     @10,CCACPL(,R11)                                    0315 01079000
         OI    CTGMAST(@10),B'01000000'                            0315 01080000
*   ELSE                            /* NON-CMS FUNCTION CLASS        */ 01081000
*     IF CTGFUNC=CTGLOC THEN        /* LOCATE REQUEST                */ 01082000
         B     @RC00306                                            0316 01083000
@RF00306 L     @10,CCACPL(,R11)                                    0316 01084000
         TM    CTGFUNC(@10),B'00100000'                            0316 01085000
         BNO   @RF00316                                            0316 01086000
         TM    CTGFUNC(@10),B'11000000'                            0316 01087000
         BNZ   @RF00316                                            0316 01088000
*       CTGREAD=ON;                 /* AT LEAST VERIFY READ LVL      */ 01089000
         OI    CTGREAD(@10),B'00001000'                            0317 01090000
*     ELSE                          /* NOT LOCATE                    */ 01091000
*       CTGMAST=ON;                 /* MASTER FOR ALL OTHERS         */ 01092000
         B     @RC00316                                            0318 01093000
@RF00316 L     @10,CCACPL(,R11)                                    0318 01094000
         OI    CTGMAST(@10),B'01000000'                            0318 01095000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 01096000
@RC00316 DS    0H                                                  0319 01097000
@RC00306 BAL   @14,EXIT                                            0319 01098000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 01099000
         DS 0F                                                          01100000
*   GEN(DC CL8'IGGPPWGT');          /* NAME OF FOLLOWING PROCEDURE   */ 01101000
         DC CL8'IGGPPWGT'                                               01102000
*   END;                            /* END OF INNER PROCEDURE        */ 01103000
@EL00004 DS    0H                                                  0322 01104000
@EF00004 DS    0H                                                  0322 01105000
@ER00004 BR    @14                                                 0322 01106000
