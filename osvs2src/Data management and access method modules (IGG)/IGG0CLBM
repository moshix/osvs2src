*   CCAURAB=CCARABB;                /* RE-SET BASE RECORD RAB        */ 00828000
         L     @10,CCARABB(,R11)                                   0271 00829000
         ST    @10,CCAURAB(,R11)                                   0271 00830000
*   IF CCACD1=OKAY THEN             /* NO MAJOR ERRORS               */ 00831000
         CLI   CCACD1(R11),0                                       0272 00832000
         BNE   @RF00272                                            0272 00833000
*     DO;                           /*                       @Z30TSBM*/ 00834000
*       IF(CTGFLNG(1)=NONE&         /* NO SECURITY DATA              */ 00835000
*           CTGFLPT(1)=NONE)|(CKAUMSTR=NULLPRMT) THEN/* NO MASTER  0274 00836000
*                                      PASSWORD                      */ 00837000
         SLR   @10,@10                                             0274 00838000
