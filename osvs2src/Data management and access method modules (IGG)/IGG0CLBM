         EJECT                                                          00652000
*                                                                  0251 00653000
*/********************************************************************/ 00654000
*/*                                                                  */ 00655000
*/*        IGGPCKEX  -  PROCEDURE  EXTERNALLY  CALLED  NESTED        */ 00656000
*/*                                                                  */ 00657000
*/********************************************************************/ 00658000
*                                                                  0251 00659000
*IGGPCKEX:                                                         0251 00660000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 00661000
         B     @PB00003                                            0251 00662000
IGGPCKEX DS    0H                                                  0252 00663000
*   GEN(ENTRY IGGPCKEX );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 00664000
         ENTRY IGGPCKEX                                                 00665000
*Z018:                              /* LABEL FOR PROCEDURE TRACKING  */ 00666000
*Z018CKEX:                          /* LABEL FOR PROCEDURE TRACKING  */ 00667000
*   RFY                                                            0253 00668000
*    (R5,                                                          0253 00669000
*     R6,                                                          0253 00670000
*     R7,                                                          0253 00671000
*     R8,                                                          0253 00672000
*     R9,                                                          0253 00673000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00674000
Z018     DS    0H                                                  0253 00675000
Z018CKEX DS    0H                                                  0254 00676000
*   RFY                                                            0254 00677000
*    (R2,                                                          0254 00678000
*     R3,                                                          0254 00679000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00680000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00681000
         STM 12,14,12(13)                                               00682000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00683000
         LA 13,12(13)                                                   00684000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 00685000
         BALR 12,0                                                      00686000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 00687000
         LA 15,*-IGGSEGST(0,0)                                          00688000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */ 00689000
*                                                                  0259 00690000
         SR 12,15                                                       00691000
*   /*****************************************************************/ 00692000
*   /*                                                               */ 00693000
*   /*                                                       @Z40RSRC*/ 00694000
*   /*                                                               */ 00695000
*   /*****************************************************************/ 00696000
*                                                                  0260 00697000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00698000
**   MODULE NAME =  IGGPCKEX                                          * 00699000
**                                                                    * 00700000
**   DESCRIPTIVE NAME =  EXTRACT ROUTINE INTERFACE FOR SECURITY DATA  * 00701000
**                                                                    * 00702000
**   COPYRIGHT =  NONE                                                * 00703000
**                                                                    * 00704000
**   STATUS =  RELEASE 1, LEVEL 0                                     * 00705000
**                                                                    * 00706000
**   FUNCTION =                                                       * 00707000
**      THIS INTERNAL PROCEDURE CALLS THE VSAM CATALOG MANAGEMENT     * 00708000
**      EXTRACT FUNCTION TO DETERMINE IF SECURITY DATA EXISTS IN THE  * 00709000
**      ENTRY.  IF SECURITY DATA EXISTS, IT IS SAVED IN THE CHECK     * 00710000
**      AUTHORIZATION WORK AREA FOR VERIFICATION.  IF SECURITY DOES   * 00711000
**      NOT EXIST, A VALUE OF 2 IS RETURNED IN CCACD2 FOR ALTER AND   * 00712000
**      DELETE REQUESTS, OR A VALUE OF 1 IS RETURNED FOR ALL OTHER    * 00713000
**      REQUESTS.                                                     * 00714000
**      1. CONSTRUCT AN INTERNAL CPL FOR EXTRACT USAGE.               * 00715000
**      2. CONSTRUCT AN INTERNAL FPL TO EXTRACT THE 'PASSWALL'        * 00716000
**         COMBINATION NAME DATA.                                     * 00717000
**      3. INITIALIZE THE CCA FOR THE CALL TO EXTRACT.                * 00718000
**      4. CALL EXTRACT AND IF NO ERRORS AND NO SECURITY THEN.        * 00719000
**         A. PASS A RETURN CODE OF 2 IN CCACD2 FOR ALTER OR DELETE   * 00720000
**            REQUESTS.                                               * 00721000
**         B. PASS A RETURN CODE OF 1 IN CCACD2 FOR OTHER REQUEST     * 00722000
**            TYPES.                                                  * 00723000
**      5. RETURN TO THE PROCEDURE INVOKER.                           * 00724000
**                                                                    * 00725000
**   INPUT =                                                          * 00726000
**      CCA                                                           * 00727000
**        CCAR5REC                                                    * 00728000
**          CKAUWKA                                                   * 00729000
**            CKAUCPL(LOCATION)                                       * 00730000
**            CKAUFPL(LOCATION)                                       * 00731000
**            CKAUFLNM(LOCATION)                                      * 00732000
**          CCARAB1                                                   * 00733000
**          CCARAB4                                                   * 00734000
**          CCAURAB                                                   * 00735000
**                                                                    * 00736000
**   OUTPUT =                                                         * 00737000
**      CCA                                                           * 00738000
**        CCAR5REC                                                    * 00739000
**          CKAUWKA                                                   * 00740000
**            CKAUCPL - INTERNAL CPL                                  * 00741000
**            CKAUFLPT - INTERNAL CPL                                 * 00742000
**            CKUAFPL - INTERNAL FPL                                  * 00743000
**            CKAULNPT - INTERNAL FPL                                 * 00744000
**            CKAUFLNM - INTERNAL FPL                                 * 00745000
**            CKAUDTLN - WORK AREA HEADER FOR DATA                    * 00746000
**              CKAUDATA - SECURITY DATA                              * 00747000
**                CKAUINFO                                            * 00748000
**         CCACPL                                                     * 00749000
**           CPL (USER)                                               * 00750000
**             CTGUPD                                                 * 00751000
**         CCAEXT                                                     * 00752000
**           CCAF2NVC                                                 * 00753000
**           CCAREUSE                                                 * 00754000
**           CCARAB1                                                  * 00755000
**           CCACD2                                                   * 00756000
**           CCAXCPL                                                  * 00757000
**           CCARABB                                                  * 00758000
**           CCARABF                                                  * 00759000
**                                                                    * 00760000
**     MESSAGES =  NONE                                               * 00761000
**                                                                    * 00762000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00763000
*                                                                  0260 00764000
*   /*****************************************************************/ 00765000
*   /*                                                               */ 00766000
*   /* IGGPCKEX - THIS INTERNAL ROUTINE CALLS THE EXTRACT ROUTINE TO */ 00767000
*   /* DETERMINE IF PASSWORDS EXIST IN AN ENTRY. IF PASSWORDS EXIST, */ 00768000
*   /* THEY ARE RETAINED IN THE CHECK AUTHORIZATION WORK AREA. IF    */ 00769000
*   /* THEY DO NOT EXIST, A NON ZERO VALUE IS RETURNED IN CCACD2.    */ 00770000
*   /* CALL TO MODULE IGG0CLDC                                       */ 00771000
*   /*                                                               */ 00772000
*   /*****************************************************************/ 00773000
*                                                                  0260 00774000
*Z019RACS:                                                         0260 00775000
*   CALL IGGPRACS;                  /* SETUP FOR EXTRACT     @Z40RSRC*/ 00776000
Z019RACS L     @15,@CV00050                                        0260 00777000
         BALR  @14,@15                                             0260 00778000
*   RFY                                                            0261 00779000
*     CTGPL BASED(CCAXCPL);                                        0261 00780000
*                                                                  0261 00781000
*   /*****************************************************************/ 00782000
*   /*                                                               */ 00783000
*   /* INITIALIZE THE CPL FOR THE EXTRACT REQUEST                    */ 00784000
*   /*                                                               */ 00785000
*   /*****************************************************************/ 00786000
*                                                                  0262 00787000
*   CTGWKA=ADDR(CKAUDTLN);          /* WORK AREA LOCATION            */ 00788000
         L     @10,CCAXCPL(,R11)                                   0262 00789000
         L     @05,CCAR5REC(,R11)                                  0262 00790000
         LA    @15,CKAUDTLN(,@05)                                  0262 00791000
         ST    @15,CTGWKA(,@10)                                    0262 00792000
*   WKALEN1=LENGTH(USERWKA)+LENGTH(CKAUINFO)+MAXUSAR;              0263 00793000
*                                                                  0263 00794000
         MVC   WKALEN1(2,@15),@CH03498                             0263 00795000
*   /*****************************************************************/ 00796000
*   /*                                                               */ 00797000
*   /* INITIALIZE THE FPL FOR EXTRACTING THE SECURITY INFO           */ 00798000
*   /*                                                               */ 00799000
*   /*****************************************************************/ 00800000
*                                                                  0264 00801000
*   RFY                                                            0264 00802000
*     CTGFL BASED(CTGFIELD(1));                                    0264 00803000
*   CTGFLDNM=ADDR(CKAUFLNM);        /* FIELD NAME LOCATION           */ 00804000
         L     @10,CTGFIELD(,@10)                                  0265 00805000
         LA    @15,CKAUFLNM(,@05)                                  0265 00806000
         ST    @15,CTGFLDNM(,@10)                                  0265 00807000
*   CTGFLDNM->FIELDNME='PASSWALL';  /* SET FIELD NAME                */ 00808000
         MVC   FIELDNME(8,@15),@CC03385                            0266 00809000
*   CKUSVRPT=INDXPWRG;              /* SAVE INDEX REGISTER           */ 00810000
*                                                                  0267 00811000
         ST    INDXPWRG,CKUSVRPT(,@05)                             0267 00812000
*   /*****************************************************************/ 00813000
*   /*                                                               */ 00814000
*   /* CALL TO MODULE IGG0CLAZ                                       */ 00815000
*   /*                                                               */ 00816000
*   /*****************************************************************/ 00817000
*                                                                  0268 00818000
*Z020EXT:                                                          0268 00819000
*   CALL IGGPEXT;                   /* CALL EXTRACT FOR SECURITY     */ 00820000
Z020EXT  L     @15,@CV00042                                        0268 00821000
         BALR  @14,@15                                             0268 00822000
*   CCACD2=OKAY;                    /* CLEAR WORK BYTE       @Y30SSSB*/ 00823000
