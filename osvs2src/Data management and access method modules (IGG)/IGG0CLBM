         B     @PB00007                                            0479 01869000
IGGPBSGT DS    0H                                                  0480 01870000
*   GEN(ENTRY IGGPBSGT );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 01871000
         ENTRY IGGPBSGT                                                 01872000
*Z037:                              /* LABEL FOR PROCEDURE TRACKING  */ 01873000
*Z037BSGT:                          /* LABEL FOR PROCEDURE TRACKING  */ 01874000
*   RFY                                                            0481 01875000
*    (R5,                                                          0481 01876000
*     R6,                                                          0481 01877000
*     R7,                                                          0481 01878000
*     R8,                                                          0481 01879000
*     R9,                                                          0481 01880000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 01881000
Z037     DS    0H                                                  0481 01882000
Z037BSGT DS    0H                                                  0482 01883000
*   RFY                                                            0482 01884000
*    (R2,                                                          0482 01885000
*     R3,                                                          0482 01886000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01887000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 01888000
         STM 12,14,12(13)                                               01889000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 01890000
         LA 13,12(13)                                                   01891000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 01892000
         BALR 12,0                                                      01893000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 01894000
         LA 15,*-IGGSEGST(0,0)                                          01895000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */ 01896000
*                                                                  0487 01897000
         SR 12,15                                                       01898000
*   /*****************************************************************/ 01899000
*   /*                                                               */ 01900000
*   /*                                                       @Z40RSRC*/ 01901000
*   /*                                                               */ 01902000
*   /*****************************************************************/ 01903000
*                                                                  0488 01904000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 01905000
**   MODULE NAME =  IGGPBSGT                                          * 01906000
**                                                                    * 01907000
**   DESCRIPTIVE NAME =  RE-READ ORIGINAL BASE RECORD                 * 01908000
**                                                                    * 01909000
**   COPYRIGHT =  NONE                                                * 01910000
**                                                                    * 01911000
**   STATUS =  RELEASE 1, LEVEL 0                                     * 01912000
**                                                                    * 01913000
**   FUNCTION =                                                       * 01914000
**      THIS INTERNAL PROCEDURE GETS THE ORIGINAL BASE RECORD, THAT   * 01915000
**      WAS IN THE BUFFER ON ENTRY TO THE SECURITY FUNCTION, BACK     * 01916000
**      INTO THE BUFFER.                                              * 01917000
**                                                                    * 01918000
**   INPUT =                                                          * 01919000
**      REGISTER 1 REFERENCES THE CCA WHICH CONTAINS:                 * 01920000
**        CCAURAB REFERENCING THE RAB INTO WHICH THE RECORD SHOULD    * 01921000
**        BE READ.                                                    * 01922000
**        CCARSREC POINTS TO THE CHECK AUTHORIZATION WORK AREA WHICH  * 01923000
**        CONTAINS: (CKAUWKA)                                         * 01924000
**          REGETCI - CONTROL INTERNAL VALUE OF BASE RECORD           * 01925000
**                                                                    * 01926000
**   OUTPUT =                                                         * 01927000
**      BASE RECORD IN RAB1                                           * 01928000
**      CKAUBASE - NO NEED TO GET BASE RECORD                         * 01929000
**                                                                    * 01930000
**     MESSAGES =  NONE                                               * 01931000
**                                                                    * 01932000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01933000
*                                                                  0488 01934000
*   /*****************************************************************/ 01935000
*   /*                                                               */ 01936000
*   /* IGGPBSGT - THIS IS AN INTERNAL ROUTINE TO RE-READ THE ORIGINAL*/ 01937000
*   /* BASE RECORD BACK INTO THE BUFFER FOR PROCESSING.              */ 01938000
*   /*                                                               */ 01939000
*   /*****************************************************************/ 01940000
*                                                                  0488 01941000
*   IF CKAUNOSV=ON THEN             /* NEED TO GET THE RECORD        */ 01942000
         L     @10,CCAR5REC(,R11)                                  0488 01943000
         TM    CKAUNOSV(@10),B'01000000'                           0488 01944000
         BNO   @RF00488                                            0488 01945000
*     DO;                                                          0489 01946000
*       CKAUNOSV=OFF;               /* CLEAR RECORD SAVED FLAG       */ 01947000
         NI    CKAUNOSV(@10),B'10111111'                           0490 01948000
*       CCASRCIN=REGETCI;           /* SET THE CI NO. OF THE RCD     */ 01949000
         MVC   CCASRCIN(3,R11),REGETCI(@10)                        0491 01950000
*       DO;                                                        0492 01951000
*         CCAF1KEY=OFF;             /* GET BY CI NO.                 */ 01952000
*         CCAF1KGE=OFF;             /* NOT GET NEXT                  */ 01953000
*         CCAF1UP=ON;               /* GET FOR UPD                   */ 01954000
*                                                                  0495 01955000
         OI    CCAF1UP(R11),B'00000010'                            0495 01956000
         NI    CCAF1KEY(R11),B'11100111'                           0495 01957000
*         /***********************************************************/ 01958000
*         /*                                                         */ 01959000
*         /* CALL TO MODULE IGG0CLBI                                 */ 01960000
*         /*                                                         */ 01961000
*         /***********************************************************/ 01962000
*                                                                  0496 01963000
*Z039GET:                                                          0496 01964000
*         CALL IGGPGET;             /* GET RECORD                    */ 01965000
Z039GET  L     @15,@CV00052                                        0496 01966000
         BALR  @14,@15                                             0496 01967000
*       END;                                                       0497 01968000
*       REGETCI=NONE;               /* CLEAR RE-GET CI VALUE         */ 01969000
         L     @10,CCAR5REC(,R11)                                  0498 01970000
         SLR   @05,@05                                             0498 01971000
         STCM  @05,7,REGETCI(@10)                                  0498 01972000
*     END;                                                         0499 01973000
*   ELSE                            /* RECORD IS IN STORAGE          */ 01974000
*     DO;                           /* MOVE THE RECORD AROUND        */ 01975000
         B     @RC00488                                            0500 01976000
@RF00488 DS    0H                                                  0501 01977000
*       R2=CCACPE1;                 /* GET PROPER BUFFER ADDRESS     */ 01978000
         L     @10,CCAURAB(,R11)                                   0501 01979000
         L     R2,CCACPE1(,@10)                                    0501 01980000
*       CCARAB=CCARAB4;             /* RESTORE THE RAB POINTERS      */ 01981000
         MVC   CCARAB(20,@10),CCARAB4(R11)                         0502 01982000
*       CCAR4REC=CCAR3REC+CTRECSIZ; /* RE-SET THE CPE1 PTR           */ 01983000
         LA    @10,512                                             0503 01984000
         AL    @10,CCAR3REC(,R11)                                  0503 01985000
         ST    @10,CCAR4REC(,R11)                                  0503 01986000
*       R2->CATRECRD(1)=CCAR4REC->CATRECRD(1);                     0504 01987000
