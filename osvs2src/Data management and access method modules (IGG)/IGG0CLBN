*   CTGFLDNO=INCR1;                 /* NO. OF LENGTH/DATA PTRS       */ 00403000
         MVI   CTGFLDNO(@05),X'01'                                 0232 00404000
*   CTGFLDNM=CMDTAFPL+LENGTH(CTGFL)+LENGTH(CTGFLDAT);              0233 00405000
