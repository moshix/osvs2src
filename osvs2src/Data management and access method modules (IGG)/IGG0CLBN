         MVC   WORKPROC(2,@10),@CC00074                            0157 00038000
*   CMCNVNO=CMADDVOL;               /* SAVE ADD VOLUME COUNT         */ 00039000
         LH    @05,CMADDVOL(,@10)                                  0158 00040000
         STH   @05,CMCNVNO(,@10)                                   0158 00041000
*   DO WHILE WORKPROC=GO;           /* COUNTINUE AS NECESSARY      0159 00042000
*                                                            @Y30SSPJ*/ 00043000
