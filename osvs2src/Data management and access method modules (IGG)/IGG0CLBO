         TITLE 'IGG0CLBO - CATALOG RECOVERY AREA I/O SUBFUNCTIONS      *00001000
                        '                                               00002000
IGG0CLBO CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGG0CLBO  77.260'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
         USING @MAINENT,@15                                             00011000
         USING @DATD-(CCATEMPS-IGGCCA),R11                              00012000
         DS 0F                                                          00013000
         DC CL8'IGGPRAPA'                                               00014000
         EJECT                                                          00015000
         B     @PB00002                                            0239 00016000
IGGPRAPA DS    0H                                                  0240 00017000
         ENTRY IGGPRAPA                                                 00018000
         DROP @15                                                       00019000
Z003     DS    0H                                                  0242 00020000
Z003RAPA DS    0H                                                  0243 00021000
         STM 12,14,12(13)                                               00022000
         LA 13,12(13)                                                   00023000
         BALR 12,0                                                      00024000
IGGSEGST DS 0H                                                          00025000
*                                                                  0248 00026000
         USING *,12                                                     00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* THIS ROUTINE CONTROLS THE PROCESS OF ADDING NEW RECORDS TO    */ 00030000
*   /* CRAS.                                                         */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                                  0249 00034000
*   CCARAREC=CCACPE1;               /* PT TO INPUT RECORD            */ 00035000
         L     @10,CCAURAB(,R11)                                   0249 00036000
         L     @10,CCACPE1(,@10)                                   0249 00037000
         ST    @10,CCARAREC(,R11)                                  0249 00038000
*   IF HOBCRAVL=N|                  /* IF CRA VOL ^ YET KNOWN OR     */ 00039000
*       CCAVBUFI=ON THEN            /* FORCE OF BUFFRING IS IND      */ 00040000
         CLI   HOBCRAVL(@10),0                                     0250 00041000
         BE    @RT00250                                            0250 00042000
         TM    CCAVBUFI(R11),B'01000000'                           0250 00043000
         BNO   @RF00250                                            0250 00044000
@RT00250 DS    0H                                                  0251 00045000
*     DO;                                                          0251 00046000
*Z004RAPC:                                                         0252 00047000
*       CALL IGGPRAPC;              /* ADD RCD TO BUFF CHN           */ 00048000
Z004RAPC BAL   @14,IGGPRAPC                                        0252 00049000
*Z005RAX:                                                          0253 00050000
*       CALL IGGPRAX;               /* EXIT                          */ 00051000
Z005RAX  BAL   @14,IGGPRAX                                         0253 00052000
*     END;                                                         0254 00053000
*Z006RAOR:                                                         0255 00054000
*   CALL IGGPRAOR;                  /* ORIENT TO CRA                 */ 00055000
@RF00250 DS    0H                                                  0255 00056000
Z006RAOR BAL   @14,IGGPRAOR                                        0255 00057000
*   IF CCACD1^=RCS THEN             /* IF UNSUCCESSFUL               */ 00058000
         CLI   CCACD1(R11),0                                       0256 00059000
         BE    @RF00256                                            0256 00060000
*Z007RAX:                                                          0257 00061000
*     CALL IGGPRAX;                 /* EXIT                          */ 00062000
Z007RAX  BAL   @14,IGGPRAX                                         0257 00063000
*   IF RCDID=TYPV THEN              /* INPUT RCD TYPE=VOL RCD        */ 00064000
@RF00256 L     @10,CCARAREC(,R11)                                  0258 00065000
         CLI   RCDID(@10),C'V'                                     0258 00066000
         BNE   @RF00258                                            0258 00067000
*     CAXF2VTU=ON;                  /* IND VOL TIMESTMP UPD COMP     */ 00068000
         L     @10,CCARAACB(,R11)                                  0259 00069000
         L     @10,ACBUAPTR(,@10)                                  0259 00070000
         OI    CAXF2VTU(@10),B'00000001'                           0259 00071000
*   ELSE                            /* OTHERWISE                     */ 00072000
*     DO;                                                          0260 00073000
         B     @RC00258                                            0260 00074000
@RF00258 DS    0H                                                  0261 00075000
*       IF RCDID=TYPW&              /* EXTENSION TO VOL ENT          */ 00076000
*           CAXF2VTU=OFF THEN       /* & TMESTMP UPDATE REQD         */ 00077000
         L     @10,CCARAREC(,R11)                                  0261 00078000
         CLI   RCDID(@10),C'W'                                     0261 00079000
         BNE   @RF00261                                            0261 00080000
         L     @10,CCARAACB(,R11)                                  0261 00081000
         L     @10,ACBUAPTR(,@10)                                  0261 00082000
         TM    CAXF2VTU(@10),B'00000001'                           0261 00083000
         BNZ   @RF00261                                            0261 00084000
*         DO;                                                      0262 00085000
*                                                                  0262 00086000
*           /*********************************************************/ 00087000
*           /*                                                       */ 00088000
*           /* CALL TO MODULE IGG0CLDA                               */ 00089000
*           /*                                                       */ 00090000
*           /*********************************************************/ 00091000
*                                                                  0263 00092000
*Z008RAPV:                                                         0263 00093000
*           CALL IGGPRAPV;          /* UPDATE VOL ENT & F4 TMESTMP   */ 00094000
Z008RAPV L     @15,@CV03786                                        0263 00095000
         BALR  @14,@15                                             0263 00096000
*           IF CCACD1^=RCS THEN     /* IF UNSUCCESSFUL               */ 00097000
         CLI   CCACD1(R11),0                                       0264 00098000
         BE    @RF00264                                            0264 00099000
*Z009RAX:                                                          0265 00100000
*             CALL IGGPRAX;         /* EXIT                          */ 00101000
Z009RAX  BAL   @14,IGGPRAX                                         0265 00102000
*         END;                                                     0266 00103000
@RF00264 DS    0H                                                  0267 00104000
*     END;                                                         0267 00105000
@RF00261 DS    0H                                                  0268 00106000
*   IF RCDID^=TYPV&                 /* ^ VOL ENT                     */ 00107000
*       RCDID^=TYPW&                /* ^ VOL ENT EXTENTION           */ 00108000
*       RBCBEG^=N THEN              /* BUFF CHN EXISTS               */ 00109000
@RC00258 L     @10,CCARAREC(,R11)                                  0268 00110000
         CLI   RCDID(@10),C'V'                                     0268 00111000
         BE    @RF00268                                            0268 00112000
         CLI   RCDID(@10),C'W'                                     0268 00113000
         BE    @RF00268                                            0268 00114000
         L     @10,RBCBEG+1452(,R11)                               0268 00115000
         LTR   @10,@10                                             0268 00116000
         BZ    @RF00268                                            0268 00117000
*     DO;                           /* FLUSH BUFFER CHAIN            */ 00118000
*       CCARALSA=CCACPE1;           /* SAVE INPUT RCD PTR            */ 00119000
         L     @05,CCAURAB(,R11)                                   0270 00120000
         MVC   CCARALSA(4,R11),CCACPE1(@05)                        0270 00121000
*       CCARAREC=RBCBEG;            /* PT TO 1ST RCD ON BUF CHN      */ 00122000
         ST    @10,CCARAREC(,R11)                                  0271 00123000
*       CCASCRA=ON;                 /* SET TO STOP PAD RECURSION     */ 00124000
         OI    CCASCRA(R11),B'00100000'                            0272 00125000
*       DO WHILE(CCARAREC^=N);      /* PROCESS BUFF CHN              */ 00126000
         B     @DE00273                                            0273 00127000
@DL00273 DS    0H                                                  0274 00128000
*         IF RABUFCHN^=N&           /* NOT LAST BUFF                 */ 00129000
*             RABUFPPN^=N THEN      /* & PROCESS TO BE DEFERRED      */ 00130000
         SLR   @10,@10                                             0274 00131000
         L     @05,CCARAREC(,R11)                                  0274 00132000
         L     @15,RABUFCHN(,@05)                                  0274 00133000
         CR    @15,@10                                             0274 00134000
         BE    @RF00274                                            0274 00135000
         CLI   RABUFPPN(@05),0                                     0274 00136000
         BE    @RF00274                                            0274 00137000
*           DO;                     /* DEFER PROCESSING              */ 00138000
*             RABUFPPN=RABUFPPN-ONE;/* DECR PROCESS PASS CNT         */ 00139000
         SLR   @14,@14                                             0276 00140000
         IC    @14,RABUFPPN(,@05)                                  0276 00141000
         BCTR  @14,0                                               0276 00142000
         STC   @14,RABUFPPN(,@05)                                  0276 00143000
*             RBCEND->RABUFCHN=CCARAREC;/* MV THIS BUFF TO END OF CHN*/ 00144000
         LA    @14,1452                                            0277 00145000
         ALR   @14,R11                                             0277 00146000
         L     @09,RBCEND(,@14)                                    0277 00147000
         ST    @05,RABUFCHN(,@09)                                  0277 00148000
*             RBCEND=CCARAREC;      /* ADJ LAST BUFF PTR             */ 00149000
         ST    @05,RBCEND(,@14)                                    0278 00150000
*             RBCBEG=RABUFCHN;      /* ADJ CHN START PTR             */ 00151000
         ST    @15,RBCBEG(,@14)                                    0279 00152000
*             RABUFCHN=N;           /* CURR BUF NOW LAST BUFF        */ 00153000
         ST    @10,RABUFCHN(,@05)                                  0280 00154000
*             CCARAREC=RBCBEG;      /* PREP TO PROC NXT BUFF         */ 00155000
         ST    @15,CCARAREC(,R11)                                  0281 00156000
*           END;                                                   0282 00157000
*         ELSE                      /* THIS BUFF TO BE PROCESS NOW   */ 00158000
*           DO;                                                    0283 00159000
         B     @RC00274                                            0283 00160000
@RF00274 DS    0H                                                  0284 00161000
*             RCDCRAVL=CCARALSA->RCDCRAVL;/* INPUT VOL SER TO CHAIN     00162000
*                                      RCD                           */ 00163000
         L     @10,CCARAREC(,R11)                                  0284 00164000
         L     @05,CCARALSA(,R11)                                  0284 00165000
         MVC   RCDCRAVL(6,@10),RCDCRAVL(@05)                       0284 00166000
*             RCDCRADT=CCARALSA->RCDCRADT;/* MV DEV TYPE TO CHAINED     00167000
*                                      RCD                           */ 00168000
         MVC   RCDCRADT(4,@10),RCDCRADT(@05)                       0285 00169000
*             RBCBEG=RABUFCHN;      /* HOLD PTR TO NXT BUFF          */ 00170000
         MVC   RBCBEG+1452(4,R11),RABUFCHN(@10)                    0286 00171000
*Z010RARA:                                                         0287 00172000
*             CALL IGGPRARA;        /* ADD BUFFERED RCD TO CRA       */ 00173000
Z010RARA BAL   @14,IGGPRARA                                        0287 00174000
*             IF CCACD1=RCS THEN    /* CONTINUE IF OK                */ 00175000
         CLI   CCACD1(R11),0                                       0288 00176000
         BNE   @RF00288                                            0288 00177000
*               DO;                                                0289 00178000
*                 CCACPE1=CCARAREC; /* ST RCD PTR FOR IGGPPAD        */ 00179000
         L     @10,CCAURAB(,R11)                                   0290 00180000
         MVC   CCACPE1(4,@10),CCARAREC(R11)                        0290 00181000
*                 DO;                                              0291 00182000
*                                                                  0291 00183000
*                   /*************************************************/ 00184000
*                   /*                                               */ 00185000
*                   /* CALL TO MODULE IGG0CLAG                       */ 00186000
*                   /*                                               */ 00187000
*                   /*************************************************/ 00188000
*                                                                  0292 00189000
*Z012PAD:                                                          0292 00190000
*                   CALL IGGPPAD;   /* PUT-ADD RECORD                */ 00191000
Z012PAD  L     @15,@CV03787                                        0292 00192000
         BALR  @14,@15                                             0292 00193000
*                 END;              /* ADD BUFFERED RCD TO CATLG     */ 00194000
*               END;                                               0294 00195000
*             IF CCACD1=RCS THEN    /* CONTINUE IF STILL OK          */ 00196000
@RF00288 CLI   CCACD1(R11),0                                       0295 00197000
         BNE   @RF00295                                            0295 00198000
*               DO;                                                0296 00199000
*                 CCAMNADR=CCARAREC;/* ADDR OF BUFFER                */ 00200000
         MVC   CCAMNADR(4,R11),CCARAREC(R11)                       0297 00201000
*                 CCAMNLEN=LENGTH(RABUFFER);/* SIZE OF BUFFER        */ 00202000
         MVC   CCAMNLEN(3,R11),@CF01050+1                          0298 00203000
*                 R1=ADDR(CCAMSSPL);/* POINT TO STRG MGMT P.L.       */ 00204000
         LA    R1,CCAMSSPL(,R11)                                   0299 00205000
*                 CCAMNSPL=USUBPOOL;/* STORE SUBPOOL VALUE           */ 00206000
         MVI   CCAMNSPL(R11),X'FC'                                 0300 00207000
*                 DO;                                              0301 00208000
*                   IF MNATFULL=ON THEN/* TRACKING TABLE FULL        */ 00209000
*                                                                  0302 00210000
         TM    MNATFULL(R11),B'10000000'                           0302 00211000
         BNO   @RF00302                                            0302 00212000
*                     /***********************************************/ 00213000
*                     /*                                             */ 00214000
*                     /* CALL TO MODULE IGG0CLAT                     */ 00215000
*                     /*                                             */ 00216000
*                     /***********************************************/ 00217000
*                                                                  0303 00218000
*Z015ARFS:                                                         0303 00219000
*                     CALL IGGPARFS;/* YES,CALL SPILL ROUTINE        */ 00220000
Z015ARFS L     @15,@CV03820                                        0303 00221000
         BALR  @14,@15                                             0303 00222000
*                   GEN REFS(CCAMNAT)                              0304 00223000
*                       (MVC CCAMNAT(248,11),CCAMNAT+8(11));/* PUSH     00224000
*                                      AND TRACK                     */ 00225000
@RF00302 DS    0H                                                  0304 00226000
         MVC CCAMNAT(248,11),CCAMNAT+8(11)                              00227000
*                 END;                                             0305 00228000
*                                                                  0305 00229000
*                 /***************************************************/ 00230000
*                 /*                                                 */ 00231000
*                 /* RELEASE MAIN STORAGE FROM USER SUBPOOL 252      */ 00232000
*                 /*                                                 */ 00233000
*                 /***************************************************/ 00234000
*                                                                  0306 00235000
*                 GEN(FREEMAIN L,MF=(E,(1))) ;/* FREE BUFFER         */ 00236000
         FREEMAIN L,MF=(E,(1))                                          00237000
*                 CCARAREC=RBCBEG;  /* PT TO NXT BUFFER              */ 00238000
         LA    @10,1452                                            0307 00239000
         ALR   @10,R11                                             0307 00240000
         L     @05,RBCBEG(,@10)                                    0307 00241000
         ST    @05,CCARAREC(,R11)                                  0307 00242000
*                 IF CCARAREC=N THEN/* NO MORE BUFFERS               */ 00243000
         SLR   @15,@15                                             0308 00244000
         CR    @05,@15                                             0308 00245000
         BNE   @RF00308                                            0308 00246000
*                   RBCEND=N;       /* CLR BUFF CHAIN END PTR        */ 00247000
         ST    @15,RBCEND(,@10)                                    0309 00248000
*               END;                                               0310 00249000
*             ELSE                  /* ERROR HAS OCCURED             */ 00250000
*               DO;                                                0311 00251000
         B     @RC00295                                            0311 00252000
@RF00295 DS    0H                                                  0312 00253000
*                 RABUFCHN=RBCBEG;  /* RESTORE BUFF CHAIN PTR        */ 00254000
         L     @10,CCARAREC(,R11)                                  0312 00255000
         LA    @05,1452                                            0312 00256000
         ALR   @05,R11                                             0312 00257000
         MVC   RABUFCHN(4,@10),RBCBEG(@05)                         0312 00258000
*                 RABUFPPN=N;       /* CLEAR PROCESS PASS CNTR       */ 00259000
         MVI   RABUFPPN(@10),X'00'                                 0313 00260000
*                 RBCBEG=CCARAREC;  /* PUT BUFF BACK INTO CHAIN      */ 00261000
         ST    @10,RBCBEG(,@05)                                    0314 00262000
*                 CCARAREC=N;       /* KILL LOOP                     */ 00263000
         SLR   @10,@10                                             0315 00264000
         ST    @10,CCARAREC(,R11)                                  0315 00265000
*               END;                                               0316 00266000
*           END;                                                   0317 00267000
@RC00295 DS    0H                                                  0318 00268000
*       END;                                                       0318 00269000
@RC00274 DS    0H                                                  0318 00270000
@DE00273 ICM   @10,15,CCARAREC(R11)                                0318 00271000
         BNZ   @DL00273                                            0318 00272000
*       CCASCRA=OFF;                /* RESET TO NORMAL               */ 00273000
         NI    CCASCRA(R11),B'11011111'                            0319 00274000
*       CCACPE1=CCARALSA;           /* RESTORE INPUT RCD PTR         */ 00275000
         L     @10,CCAURAB(,R11)                                   0320 00276000
         MVC   CCACPE1(4,@10),CCARALSA(R11)                        0320 00277000
*       IF CCACD1^=RCS THEN         /* HAS ANY ERROR OCCURED         */ 00278000
         CLI   CCACD1(R11),0                                       0321 00279000
         BE    @RF00321                                            0321 00280000
*Z016RAX:                                                          0322 00281000
*         CALL IGGPRAX;             /* YES,EXIT                      */ 00282000
Z016RAX  BAL   @14,IGGPRAX                                         0322 00283000
*       CCARAREC=CCARALSA;          /* RESTORE INPUT RCD PTR         */ 00284000
@RF00321 MVC   CCARAREC(4,R11),CCARALSA(R11)                       0323 00285000
*     END;                                                         0324 00286000
*Z017RARA:                                                         0325 00287000
*   CALL IGGPRARA;                  /* ADD INPUT RCD TO CRA          */ 00288000
@RF00268 DS    0H                                                  0325 00289000
Z017RARA BAL   @14,IGGPRARA                                        0325 00290000
*Z018RAX:                                                          0326 00291000
*   CALL IGGPRAX;                   /* EXIT                          */ 00292000
Z018RAX  BAL   @14,IGGPRAX                                         0326 00293000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00294000
         BAL   @14,EXIT                                            0327 00295000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00296000
         DS 0F                                                          00297000
*   GEN(DC CL8'IGGPRAPU');          /* NAME OF FOLLOWING PROCEDURE   */ 00298000
         DC CL8'IGGPRAPU'                                               00299000
*   END;                            /* END OF INNER PROCEDURE        */ 00300000
@EL00002 DS    0H                                                  0330 00301000
@EF00002 DS    0H                                                  0330 00302000
@ER00002 BR    @14                                                 0330 00303000
@PB00002 DS    0H                                                  0331 00304000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 00305000
         EJECT                                                          00306000
*                                                                  0332 00307000
*/********************************************************************/ 00308000
*/*                                                                  */ 00309000
*/*        IGGPRAPU  -  PROCEDURE  EXTERNALLY  CALLED  NESTED        */ 00310000
*/*                                                                  */ 00311000
*/********************************************************************/ 00312000
*                                                                  0332 00313000
*IGGPRAPU:                                                         0332 00314000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 00315000
         B     @PB00003                                            0332 00316000
IGGPRAPU DS    0H                                                  0333 00317000
*   GEN(ENTRY IGGPRAPU );           /* IDENTIFY ENTRY-POINT SYMBOL   */ 00318000
         ENTRY IGGPRAPU                                                 00319000
*Z019:                              /* LABEL FOR PROCEDURE TRACKING  */ 00320000
*Z019RAPU:                          /* LABEL FOR PROCEDURE TRACKING  */ 00321000
*   RFY                                                            0334 00322000
*    (R5,                                                          0334 00323000
*     R6,                                                          0334 00324000
*     R7,                                                          0334 00325000
*     R8,                                                          0334 00326000
*     R9,                                                          0334 00327000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00328000
Z019     DS    0H                                                  0334 00329000
Z019RAPU DS    0H                                                  0335 00330000
*   RFY                                                            0335 00331000
*    (R2,                                                          0335 00332000
*     R3,                                                          0335 00333000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00334000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00335000
         STM 12,14,12(13)                                               00336000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 00337000
         LA 13,12(13)                                                   00338000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 00339000
         BALR 12,0                                                      00340000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 00341000
         LA 15,*-IGGSEGST(0,0)                                          00342000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE       */ 00343000
*                                                                  0340 00344000
         SR 12,15                                                       00345000
*   /*****************************************************************/ 00346000
*   /*                                                               */ 00347000
*   /* THIS ROUTINE UPDATES EXISTING CRA RECORDS.                    */ 00348000
*   /*                                                               */ 00349000
*   /*****************************************************************/ 00350000
*                                                                  0341 00351000
*   CCARAREC=CCACPE1;               /* ST PTR TO INPUT RCD           */ 00352000
         L     @10,CCAURAB(,R11)                                   0341 00353000
         L     @10,CCACPE1(,@10)                                   0341 00354000
         ST    @10,CCARAREC(,R11)                                  0341 00355000
*   IF RCDCINO^<HICATRCD THEN       /* CRA OR CAT SELF DESC RCD      */ 00356000
         CLC   RCDCINO(3,@10),@CF02676+1                           0342 00357000
         BL    @RF00342                                            0342 00358000
*     DO;                           /* NO,CONTINUE                   */ 00359000
*       IF HOBCRAVL=N|              /* CRA VOL NOT YET DETERMND      */ 00360000
*           CCAVBUFI=ON THEN        /* OR FORCE OF BUFFERING IND     */ 00361000
         CLI   HOBCRAVL(@10),0                                     0344 00362000
         BE    @RT00344                                            0344 00363000
         TM    CCAVBUFI(R11),B'01000000'                           0344 00364000
         BNO   @RF00344                                            0344 00365000
@RT00344 DS    0H                                                  0345 00366000
*         DO;                       /* YES                           */ 00367000
*           IF RCDID=TYPW|          /* VOL ENT EXTN RCD              */ 00368000
*               RCDID=TYPV THEN     /* OR VOL ENT RCD                */ 00369000
         L     @10,CCARAREC(,R11)                                  0346 00370000
         CLI   RCDID(@10),C'W'                                     0346 00371000
         BE    @RT00346                                            0346 00372000
         CLI   RCDID(@10),C'V'                                     0346 00373000
         BNE   @RF00346                                            0346 00374000
@RT00346 DS    0H                                                  0347 00375000
*             R1=CCARAVCA(ONE);     /* YES,PT TO VOL RCD BUFF CHN    */ 00376000
         L     R1,CCARAVCA(,R11)                                   0347 00377000
*           ELSE                    /* NOT TYPE "V" OR "W" RCD       */ 00378000
*             R1=CCARANCA(ONE);     /* PT TO NORMAL RCD BUFF CHN     */ 00379000
         B     @RC00346                                            0348 00380000
@RF00346 L     R1,CCARANCA(,R11)                                   0348 00381000
*           DO WHILE(R1->RCDCINO^=RCDCINO);/* SEARCH FOR THIS RCD"S     00382000
*                                      BUFF                          */ 00383000
@RC00346 B     @DE00349                                            0349 00384000
@DL00349 DS    0H                                                  0350 00385000
*             R1=R1->RABUFCHN;      /* PT TO NXT BUFF                */ 00386000
         L     R1,RABUFCHN(,R1)                                    0350 00387000
*           END;                                                   0351 00388000
@DE00349 L     @10,CCARAREC(,R11)                                  0351 00389000
         CLC   RCDCINO(3,R1),RCDCINO(@10)                          0351 00390000
         BNE   @DL00349                                            0351 00391000
*           R1->RABUFSG1=RABUFSG1;  /* MV RCD TO BUFF                */ 00392000
         MVC   RABUFSG1(256,R1),RABUFSG1(@10)                      0352 00393000
*           R1->RABUFSG2=RABUFSG2;  /* MV RCD TO BUFF                */ 00394000
         MVC   RABUFSG2(249,R1),RABUFSG2(@10)                      0353 00395000
*           CCARABYC=ON;            /* PROHIBIT CATALOG I/O          */ 00396000
         OI    CCARABYC(R11),B'10000000'                           0354 00397000
*Z020RAX:                                                          0355 00398000
