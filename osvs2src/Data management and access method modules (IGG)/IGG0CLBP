*           IF E15=DEXTIO THEN      /* IF I/O ERROR                  */ 01913000
         B     @RC00717                                            0723 01914000
@RF00717 CH    E15,@CH05318                                        0723 01915000
         BNE   @RF00723                                            0723 01916000
*             DO;                                                  0724 01917000
*               CCAMODID='BP';      /* SET MODULE ID                 */ 01918000
         MVC   CCAMODID(2,R11),@CC05296                            0725 01919000
*               CCAREASN=R15;       /* SET REASON CODE               */ 01920000
         STC   R15,CCAREASN(,R11)                                  0726 01921000
*               CCARETRN=RCIOU;     /* SET RETURN CODE               */ 01922000
         MVI   CCARETRN(R11),X'34'                                 0727 01923000
*             END;                  /* USER VOL I/O ERR      @Y30SSRS*/ 01924000
*           ELSE                    /* SECONDARY ALLOC ERROR         */ 01925000
*                                                                  0729 01926000
*             /*******************************************************/ 01927000
*             /*                                                     */ 01928000
*             /* SET CCAMODID='BP', CCAREASN=RSN2, CCARETRN=RCNOSP   */ 01929000
*             /*                                                     */ 01930000
*             /*******************************************************/ 01931000
*                                                                  0729 01932000
*             CCAPROB=IGGSEGID+(RSN2*K256)+RCNOSP;/* SET NO SPACE ERR   01933000
*                                                            @Y30SSRS*/ 01934000
         B     @RC00723                                            0729 01935000
@RF00723 MVC   CCAPROB(4,R11),@CF05486                             0729 01936000
*       ELSE                                                       0730 01937000
