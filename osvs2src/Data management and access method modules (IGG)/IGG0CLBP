*   DCL                                                            0748 02024000
*     PERIODT CHAR(2) CONSTANT('.T');/* PERIOD/T DELIMITER           */ 02025000
*   DCL                                                            0749 02026000
*     BLANKS CHAR(9) CONSTANT('         ');/* BLANKS                 */ 02027000
*   DCL                                                            0750 02028000
*     TSFIX CHAR(17)                /* FIXED PORTION OF DATA         */ 02029000
*         CONSTANT('Z9999992.VSAMDSPC');/* SPACE NAME                */ 02030000
*   DCL                                                            0751 02031000
*     TRTAB CHAR(16) INIT('0123456789ABCDEF');/* TRANSLATE TABLE     */ 02032000
*                                                                  0751 02033000
*   /*****************************************************************/ 02034000
*   /*                                                               */ 02035000
*   /* MAPPING FOR TIMESTAMP WORK AREA                       @Y30SSRS*/ 02036000
*   /*                                                               */ 02037000
*   /*****************************************************************/ 02038000
*                                                                  0752 02039000
*   DCL                                                            0752 02040000
*     1 TIMESTMP CHAR(8) BASED(R3), /* MAP TIMESTAMP                 */ 02041000
*      2 * CHAR(7),                 /* TIME STAMP INFO               */ 02042000
*      2 TSLCHAR CHAR(1);           /* LAST BYTE                     */ 02043000
*                                                                  0752 02044000
*   /*****************************************************************/ 02045000
*   /*                                                               */ 02046000
*   /* MAPPING FOR DATA SPACE NAME                           @Y30SSRS*/ 02047000
*   /*                                                               */ 02048000
*   /*****************************************************************/ 02049000
*                                                                  0753 02050000
*   DCL                                                            0753 02051000
*     1 DSPNAM CHAR(44) BASED(R5),  /* DATA SPACE NAME               */ 02052000
*      2 DSPFIX CHAR(17),           /* FIXED PORTION                 */ 02053000
*       3 * CHAR(7),                /* FIXED DATA SPACE NAME         */ 02054000
*       3 DSPTYPE CHAR(1),          /* CATALOG INDICATOR             */ 02055000
*       3 * CHAR(9),                /* FIXED DATA SPACE NAME         */ 02056000
*      2 DSPTIME,                   /* TIME STAMP FIELDS             */ 02057000
*       3 DSPDEL0 CHAR(2),          /* DELIMITERS                    */ 02058000
*       3 DSPUNPK1 CHAR(2),         /* UNPACKED INFO                 */ 02059000
*       3 DSPUNPK2,                 /* UNPACKED INFO                 */ 02060000
*        4 * CHAR(5),               /* UNPACKED INFO                 */ 02061000
*        4 DSPDEL1 CHAR(2),         /* DELIMITERS                    */ 02062000
*        4 * CHAR(7),               /* UNPACKED INFO                 */ 02063000
*       3 DSPBLK CHAR(9);           /* BLANKS                        */ 02064000
*   DCL                                                            0754 02065000
*     SHIFT1 CHAR(7) BASED(ADDR(DSPUNPK1));/* PROPER POSITION FOR  0754 02066000
*                                      TIME STAMP                    */ 02067000
*   DCL                                                            0755 02068000
*     SHIFT2 CHAR(7) BASED(ADDR(DSPUNPK2));/* TIMESTAMP INFO FOR   0755 02069000
*                                      SHIFT                         */ 02070000
*   RFY                                                            0756 02071000
*     R5 RSTD;                      /* DATA SET NAME ADDR            */ 02072000
*   RFY                                                            0757 02073000
*     CTVSPHDR BASED(ADDR(UPSPHDR));/* MAP SPACE HEADER              */ 02074000
*   TSLCHAR=SIGN;                   /* INITLZ TIMESTAMP FOR UNPACKING*/ 02075000
         MVI   TSLCHAR(R3),X'0F'                                   0758 02076000
*   GEN;                                                           0759 02077000
*                                       /* UNPACK TIMESTAMP         */  02078000
UNPKLEN  EQU   15                       UNPACKED LENGTH                 02079000
TSLEN    EQU   8                        TIME STAMP LENGTH               02080000
Z        EQU   0                        DISPL INTO TIMESTAMP FIELD      02081000
         UNPK  DSPUNPK2(UNPKLEN,R5),Z(TSLEN,R3) UNPACK TIMESTAMP        02082000
*   TSLCHAR=ZERO;                   /* RE-INITLZ BACK TO OLD VALUE   */ 02083000
         MVI   TSLCHAR(R3),X'00'                                   0760 02084000
*   R3=ADDR(TRTAB)-TRTABST;         /* SET TRANSLATE TABLE ADDR      */ 02085000
         LA    R3,TRTAB                                            0761 02086000
         SH    R3,@CH00615                                         0761 02087000
*   GEN;                                                           0762 02088000
*                                       /* MAKE TIMESTAMP PRINTABLE */  02089000
         TR    DSPUNPK2(UNPKLEN,R5),Z(R3)  MAKE TIMESTAMP PRINTABLE     02090000
*   SHIFT1=SHIFT2;                  /* GET PROPER POSITION FOR FIRST    02091000
*                                      TIMESTAMP FIELD               */ 02092000
         MVC   SHIFT1+19(7,R5),SHIFT2+21(R5)                       0763 02093000
*   DSPFIX=TSFIX;                   /* FIXED PORTION OF DATA SPACE 0764 02094000
*                                      NAME                          */ 02095000
         MVC   DSPFIX(17,R5),@CC05383                              0764 02096000
*   DSPDEL0=PERIODT;                /* DELIMITER                     */ 02097000
         MVC   DSPDEL0(2,R5),@CC05378                              0765 02098000
*   DSPDEL1=PERIODT;                /* DELIMITER                     */ 02099000
         MVC   DSPDEL1(2,R5),@CC05378                              0766 02100000
*   DSPBLK=BLANKS;                  /* BLANKS                        */ 02101000
         MVC   DSPBLK(9,R5),@CC05380                               0767 02102000
*   IF CTVSPCDF=ON THEN             /* IF DATA SPACE CREATED BY    0768 02103000
*                                      CATALOG DEFINE                */ 02104000
         L     @10,CCAR3REC(,R11)                                  0768 02105000
         AL    @10,@CF05519                                        0768 02106000
         TM    CTVSPCDF-15(@10),B'00001000'                        0768 02107000
         BNO   @RF00768                                            0768 02108000
*     IF CAXMCT=ON THEN             /* IF MASTER CATALOG IN DATA   0769 02109000
*                                      SPACE                         */ 02110000
         L     @10,CCAACB(,R11)                                    0769 02111000
         L     @10,ACBUAPTR(,@10)                                  0769 02112000
         TM    CAXMCT(@10),B'00000100'                             0769 02113000
         BNO   @RF00769                                            0769 02114000
*       DSPTYPE=FF6;                /* SET MASTER CAT SUFFIX         */ 02115000
         MVI   DSPTYPE(R5),C'6'                                    0770 02116000
*     ELSE                          /* USER CATALOG IN DATA SPACE    */ 02117000
*       DSPTYPE=FF4;                /* SET USER CAT SUFFIX           */ 02118000
         B     @RC00769                                            0771 02119000
@RF00769 MVI   DSPTYPE(R5),C'4'                                    0771 02120000
*   ELSE                                                           0772 02121000
*     ;                             /* DATA SPACE NOT CREATED BY   0772 02122000
*                                      CATALOG DEFINE                */ 02123000
@RF00768 DS    0H                                                  0773 02124000
*   RFY                                                            0773 02125000
*     R5 UNRSTD;                    /* FREE REG                      */ 02126000
@RC00768 DS    0H                                                  0774 02127000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 02128000
         BAL   @14,EXIT                                            0774 02129000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 02130000
         DS 0F                                                          02131000
*   GEN(DC CL8'IGGPOBTN');          /* NAME OF FOLLOWING PROCEDURE   */ 02132000
         DC CL8'IGGPOBTN'                                               02133000
*   END;                            /* END OF INNER PROCEDURE        */ 02134000
@EL00008 DS    0H                                                  0777 02135000
@EF00008 DS    0H                                                  0777 02136000
@ER00008 BR    @14                                                 0777 02137000
