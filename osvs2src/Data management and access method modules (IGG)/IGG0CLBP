@RC00716 DS    0H                                                  0732 01945000
*     END;                          /* END CALL DADSM EXTEND         */ 01946000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 01947000
@RC00695 BAL   @14,EXIT                                            0733 01948000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 01949000
