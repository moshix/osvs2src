*   EXTWA(513:520)=EXTWA(513:520)&&EXTWA(513:520);/* AREA            */ 01685000
         XC    EXTWA+512(8,R4),EXTWA+512(R4)                       0651 01686000
*   RFY                                                            0652 01687000
*     CTVSPHDR BASED(ADDR(UPSPHDR));/* MAP SPACE HEADER              */ 01688000
*   IF UPSUNIQ=ON THEN              /* IF UNIQUE DATA SET            */ 01689000
         L     @10,CCAR3REC(,R11)                                  0653 01690000
         SH    @10,@CH00597                                        0653 01691000
         TM    UPSUNIQ(@10),B'10000000'                            0653 01692000
         BNO   @RF00653                                            0653 01693000
*     JFCBDSNM=UDSN;                /* SET DATA SET NAME             */ 01694000
         L     @10,UPUDSNPT(,@10)                                  0654 01695000
         MVC   JFCBDSNM+100(44,R4),UDSN(@10)                       0654 01696000
*   ELSE                            /* DATA SPACE                    */ 01697000
*     DO;                           /* CREATE DATA SPACE NAME        */ 01698000
         B     @RC00653                                            0655 01699000
@RF00653 DS    0H                                                  0656 01700000
*       RFY                                                        0656 01701000
*         R5 RSTD;                  /* GET NAME PARM REG             */ 01702000
*       UPUDSNPT=ADDR(JFCBDSNM);    /* ADDR OF DSN FOR OBTAIN        */ 01703000
         L     @10,CCAR3REC(,R11)                                  0657 01704000
         LA    @15,100                                             0657 01705000
