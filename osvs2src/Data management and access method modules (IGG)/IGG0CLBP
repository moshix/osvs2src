*   CCAR5REC->RCDCINO=0;            /* CLEAR REC CI          @YM05977*/ 02662000
         L     @05,CCAR5REC(,R11)                                  0897 02663000
         STCM  @10,7,RCDCINO(@05)                                  0897 02664000
*   CCACPE34=0;                     /* ZERO OCCURRENCE       @ZA07558*/ 02665000
         ST    @10,CCACPE34(,R11)                                  0898 02666000
*   CCACPE35=0;                     /* ZERO OCCURRENCE       @ZA07558*/ 02667000
*                                                                  0899 02668000
         ST    @10,CCACPE35(,R11)                                  0899 02669000
*   /*****************************************************************/ 02670000
*   /*                                                               */ 02671000
*   /* CALL TO MODULE IGG0CLAV                                       */ 02672000
*   /*                                                               */ 02673000
*   /*****************************************************************/ 02674000
*                                                                  0900 02675000
*Z048MOD:                                                          0900 02676000
*   CALL IGGPMOD;                   /* MODIFY SPACE HEADER           */ 02677000
Z048MOD  L     @15,@CV00040                                        0900 02678000
         BALR  @14,@15                                             0900 02679000
*   IF CCACD1^=RCS THEN                                            0901 02680000
         CLI   CCACD1(R11),0                                       0901 02681000
         BNE   @RT00901                                            0901 02682000
*     ;                             /* IF ERROR, END                 */ 02683000
*   ELSE                            /* NO ERROR                      */ 02684000
*     DO;                           /* ADD SPACE DESCRIPTORS         */ 02685000
*       RFY                                                        0904 02686000
*         CTVDESGP BASED(ADDR(UPSPD));/* MAP WORK AREA SPACE       0904 02687000
*                                      DESCRIPTOR FIELDS             */ 02688000
*       CTGFLDNM=ADDR(DSPDSCRP);    /* POINT TO SPACE DESCRIPTOR   0905 02689000
*                                      FIELD NAME                    */ 02690000
         L     @10,CCAMCPL(,R11)                                   0905 02691000
         L     @10,CTGFIELD(,@10)                                  0905 02692000
         LA    @05,DSPDSCRP                                        0905 02693000
