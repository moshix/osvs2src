         TITLE 'IGG0CLBP, UPDATE EXTEND INTERFACE TO OS/VS DADSM       *00001000
                        '                                               00002000
IGG0CLBP CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGG0CLBP  78.124'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
         USING @MAINENT,@15                                             00011000
         USING @DATD-(CCATEMPS-IGGCCA),R11                              00012000
         DS 0F                                                          00013000
         DC CL8'IGGPSPAC'                                               00014000
         EJECT                                                          00015000
         B     @PB00002                                            0254 00016000
IGGPSPAC DS    0H                                                  0255 00017000
         ENTRY IGGPSPAC                                                 00018000
         DROP @15                                                       00019000
Z003     DS    0H                                                  0257 00020000
Z003SPAC DS    0H                                                  0258 00021000
