*         IF UPSPACE<=R2&           /* IF SPACE HEADER HAS           */ 01055000
*             (UPSOPT&TRUESPO)<=(CTVSPSPO&TRUESPO)&/* ADEQUATE SEC   */ 01056000
*             CTVSPUNQ=OFF&         /* ALLOCATION AND NOT UNIQUE     */ 01057000
*             CTVSPNEX<=MAXNOEXT THEN/* AND ALLOWABLE NO.EXTENTS     */ 01058000
