         BH    @RF00511                                            0511 01076000
         TM    CTVSPUNQ(@14),B'10000000'                           0511 01077000
         BNZ   @RF00511                                            0511 01078000
         CLI   CTVSPNEX(@14),11                                    0511 01079000
         BH    @RF00511                                            0511 01080000
*           DO;                     /* SAVE SPACE HEADER             */ 01081000
*             UPSPHDR=FLDATA(1:LENGTH(UPSPHDR));/* SAVE HEADER INFO  */ 01082000
         MVC   UPSPHDR(23,@10),FLDATA(@14)                         0513 01083000
*             UPSEXIT=ON;           /* EXIT INDICATOR                */ 01084000
         OI    UPSEXIT(@10),B'01000000'                            0514 01085000
*           END;                    /* END SAVE                      */ 01086000
*         ELSE                                                     0516 01087000
*           ;                       /* NOT PROPER SPACE HEADER       */ 01088000
@RF00511 DS    0H                                                  0517 01089000
*         UPREL=FLDATA(1:LENGTH(UPREL));/* SET RELREPNO FOR NEXT   0517 01090000
*                                      EXTRACT                       */ 01091000
         L     @10,CCAR3REC(,R11)                                  0517 01092000
         L     @05,CCAXCPL(,R11)                                   0517 01093000
         L     @05,CTGFIELD(,@05)                                  0517 01094000
         L     @05,CTGFLPT(,@05)                                   0517 01095000
         AL    @10,@CF05511                                        0517 01096000
         MVC   UPREL-16(2,@10),FLDATA(@05)                         0517 01097000
*       END;                        /* END TEST                      */ 01098000
*   END;                            /* END SEARCH                    */ 01099000
@RC00500 DS    0H                                                  0519 01100000
@DE00497 L     @10,CCAR3REC(,R11)                                  0519 01101000
