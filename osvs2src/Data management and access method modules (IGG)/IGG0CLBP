*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 02172000
**   MODULE NAME = IGGPOBTN(IGG0CLBP)                                 * 02173000
**                                                                    * 02174000
**   DESCRIPTIVE NAME = OBTAIN NEW EXTENTS                            * 02175000
**                                                                    * 02176000
**   FUNCTION =                                                       * 02177000
**      THIS SUB-FUNCTION OBTAINS THE NEW DATA SPACE EXTENTS          * 02178000
**      PREVIOUSLY ACQUIRED BY CALLING DADSM EXTEND OR DADSM ALLOCATE.* 02179000
**      THE NEW EXTENTS ARE PLACED INTO THE CCA DADSM EXTENTS SAVE    * 02180000
**      AREA BY RECURSIVELY CALLING DADSM OBTAIN TO READ FORMAT 1     * 02181000
**      AND FORMAT 3 DSCB'S                                           * 02182000
**                                                                    * 02183000
**   INPUT =                                                          * 02184000
**      R2 = ADDRESS OF OBTAIN WORK AREA                              * 02185000
**      OBTPL - DADSM OBTAIN PARAMETER LIST                           * 02186000
**      OBTARG - CCHHR DSCB ADDRESS OR 44-BYTE DATA SPACE NAME        * 02187000
**               ADDRESS                                              * 02188000
**      OBTCODE - TYPE SEARCH ARGUMENT                                * 02189000
**      OBTVOL - ADDRESS OF USER VOLUME SERIAL                        * 02190000
**      OBTNOEXT - NUMBER OF OLD EXTENTS IN VSAM DATA SPACE           * 02191000
**                                                                    * 02192000
**   OUTPUT =                                                         * 02193000
**      IGGCCA                                                        * 02194000
**      CCADESA - DADSM EXTENTS SAVE AREA INITIALIZED WITH NEW        * 02195000
**               EXTENTS ACQUIRED VIA DADSM EXTEND OR DADSM ALLOCATE  * 02196000
**      CCACD1 - RCNMNTD, VOLUME NOT MOUNTED                          * 02197000
**               RCIOU, I/O ERROR USER VOLUME                         * 02198000
**               RCVTCBAL, USER VOLUME VTOC OUT OF BALANCE            * 02199000
**                                                                    * 02200000
**                                                                    * 02201000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 02202000
*                                                                  0784 02203000
*   DCL                                                            0784 02204000
*     FZERO CHAR(5) INIT('0000000000'X);/* SEEK OF ZERO              */ 02205000
*   DCL                                                            0785 02206000
*     OBTS FIXED(31) CONSTANT(0);   /* OBTAIN SUCCESSFUL             */ 02207000
*   DCL                                                            0786 02208000
*     OBTRCVNM FIXED(31) CONSTANT(4);/* DADSM ERR CODE VOL NOT     0786 02209000
*                                      MOUNTED                       */ 02210000
*   DCL                                                            0787 02211000
*     OBTRCIO FIXED(31) CONSTANT(12);/* DADSM ERR CODE I/O ERR       */ 02212000
*   DCL                                                            0788 02213000
*     F3CONTEN FIXED(31) CONSTANT(4);/* FIRST NUMBER OF              */ 02214000
*   DCL                                                            0789 02215000
*     F1EXTNO FIXED(31) CONSTANT(3);/* MAX NO. OF EXTENTS IN FORMAT 1   02216000
*                                      DSCB                          */ 02217000
*   DCL                                                            0790 02218000
*     F3EXTNO FIXED(31) CONSTANT(13);/* MAX NO. OF EXTENTS IN FORMAT    02219000
*                                      3 DSCB                        */ 02220000
*   DCL                                                            0791 02221000
*     FMT3KID CHAR(4) INIT('03030303'X);/* FORMAT 3 DSCB KEY ID      */ 02222000
*   DCL                                                            0792 02223000
*     FMT1PTR CHAR(5) BASED(ADDR(DS1END));/* END OF FORMAT 1 DSCB HAS   02224000
*                                      FORMAT 1 CCHHR IF SEARCH    0792 02225000
*                                      OBTAIN                        */ 02226000
*                                                                  0792 02227000
*   /*****************************************************************/ 02228000
*   /*                                                               */ 02229000
*   /* MAPPING FOR EXTENT DESCRIPTOR                         @Y30SSRS*/ 02230000
*   /*                                                               */ 02231000
*   /*****************************************************************/ 02232000
*                                                                  0793 02233000
*   DCL                                                            0793 02234000
*     1 EXTENT1 BASED(R3),          /* EXTENT DESCRIPTOR             */ 02235000
*      2 EXTTYPE CHAR(1),           /* EXTENT TYPE INDICATOR         */ 02236000
*      2 EXTSQNO PTR(8),            /* EXTENT SEQ. NO.               */ 02237000
*      2 EXTLCCHH CHAR(4),          /* LOW CCHH                      */ 02238000
*       3 EXTLCC CHAR(2),           /* LOW CC - CYLINDER             */ 02239000
*       3 EXTLHH CHAR(2),           /* LOW HH - HEAD                 */ 02240000
*      2 EXTHCCHH CHAR(4),          /* HIGH CCHH                     */ 02241000
*       3 EXTHCC CHAR(2),           /* HIGH CC - CYLINDER            */ 02242000
*       3 EXTHHH CHAR(2);           /* HIGH HH - HEAD                */ 02243000
*   OBTWKA=ADDR(OBTDSCB);           /* OBTAIN WORK AREA              */ 02244000
         LA    @10,OBTDSCB(,R2)                                    0794 02245000
         ST    @10,OBTWKA(,R2)                                     0794 02246000
*   CCANDEXT=0;                     /* SET NO. OF OBTAINED EXTENTS TO   02247000
*                                      ZER.                          */ 02248000
         MVI   CCANDEXT(R11),X'00'                                 0795 02249000
*   OBTEXTN=0;                      /* SET NUMBER OF PROCESSED     0796 02250000
*                                      EXTENTS TO ZERO               */ 02251000
         SLR   @10,@10                                             0796 02252000
         STH   @10,OBTEXTN(,R2)                                    0796 02253000
*   OBTEXIT=OFF;                    /* ENTER OBTAIN EXTENTS LOOP     */ 02254000
         NI    OBTEXIT(R2),B'01111111'                             0797 02255000
*   DO WHILE(OBTEXIT=OFF&CCACD1=RCS);/* GET NEW EXTENTS GOTTEN FROM     02256000
*                                      DADSM                         */ 02257000
         B     @DE00798                                            0798 02258000
@DL00798 DS    0H                                                  0799 02259000
*     RFY                                                          0799 02260000
*      (R1,                                                        0799 02261000
*       R15) RSTD;                  /* USE THESE REGS                */ 02262000
*     R1=R2;                        /* POINT TO OBTAIN PARM LIST     */ 02263000
         LR    R1,R2                                               0800 02264000
*     GEN(SVC 27);                  /* CALL DADSM OBTAIN     @Y30SSRS*/ 02265000
         SVC 27                                                         02266000
*     IF R15^=OBTS THEN             /* IF ERROR                      */ 02267000
         LTR   R15,R15                                             0802 02268000
         BZ    @RF00802                                            0802 02269000
*       IF R15=OBTRCVNM THEN        /* IF VOLUME NOT MOUNTED ERR     */ 02270000
*                                                                  0803 02271000
         CH    R15,@CH00099                                        0803 02272000
         BNE   @RF00803                                            0803 02273000
*         /***********************************************************/ 02274000
*         /*                                                         */ 02275000
*         /* SET CCAMODID='BP', CCAREASN=RSN2, CCARETRN=RCNUNIT      */ 02276000
*         /*                                                         */ 02277000
*         /***********************************************************/ 02278000
*                                                                  0804 02279000
*         CCAPROB=IGGSEGID+(RSN2*K256)+RCNUNIT;/* SET CAT CODE WITH     02280000
*                                                            @Y30SSRS   02281000
*                                      SAME MEANING                  */ 02282000
         MVC   CCAPROB(4,R11),@CF05485                             0804 02283000
*       ELSE                        /* NOT MOUNT ERROR               */ 02284000
*         IF R15=OBTRCIO THEN       /* IF I/O ERROR                  */ 02285000
         B     @RC00803                                            0805 02286000
@RF00803 CH    R15,@CH00424                                        0805 02287000
         BNE   @RF00805                                            0805 02288000
*           DO;                                                    0806 02289000
*             CCAMODID='BP';        /* SET MODULE ID                 */ 02290000
         MVC   CCAMODID(2,R11),@CC05296                            0807 02291000
*             CCAREASN=R15;         /* SET REASON CODE               */ 02292000
         STC   R15,CCAREASN(,R11)                                  0808 02293000
*             CCARETRN=RCIOU;       /* SET RETURN CODE               */ 02294000
         MVI   CCARETRN(R11),X'34'                                 0809 02295000
*           END;                    /* USER I/O ERROR        @Y30SSRS*/ 02296000
*         ELSE                      /* NOT I/O ERROR                 */ 02297000
*                                                                  0811 02298000
*           /*********************************************************/ 02299000
*           /*                                                       */ 02300000
*           /* SET CCAMODID='BP', CCAREASN=RSN4, CCARETRN=RCVTCBAL   */ 02301000
*           /*                                                       */ 02302000
*           /*********************************************************/ 02303000
*                                                                  0811 02304000
*           CCAPROB=IGGSEGID+(RSN4*K256)+RCVTCBAL;/* FOR OTHER OBTAIN   02305000
*                                                            @Y30SSRS   02306000
*                                      ERRORS SET VTOC OUT OF BALANCE*/ 02307000
         B     @RC00805                                            0811 02308000
@RF00805 MVC   CCAPROB(4,R11),@CF05489                             0811 02309000
*     ELSE                          /* SUCCESSFUL OBTAIN             */ 02310000
*       DO;                         /* SAVE NEW EXTENTS              */ 02311000
         B     @RC00802                                            0812 02312000
@RF00802 DS    0H                                                  0813 02313000
*         RFY                                                      0813 02314000
*          (R1,                                                    0813 02315000
*           R15) UNRSTD;            /* FREE REGS                     */ 02316000
*         OBTFMT1=ON;               /* ASSUME FORMAT 1 RETURNED      */ 02317000
         OI    OBTFMT1(R2),B'00100000'                             0814 02318000
*         IF OBTCODE=SRCH THEN      /* IF 44-BYTE NAME GIVEN         */ 02319000
