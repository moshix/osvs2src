*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00712000
**   MODULE NAME =  IGGPSRH1(IGG0CLBP)                                * 00713000
**                                                                    * 00714000
**   DESCRIPTIVE NAME =                                               * 00715000
**      SEARCH FOR DATA SET DIRECTORY AND SPACE HEADER OF UNIQUE DATA * 00716000
**      SET.                                                          * 00717000
**                                                                    * 00718000
**   FUNCTION =                                                       * 00719000
**      THIS SUB-FUNCTION IS CALLED BY IGGPSPAC TO EXTRACT THE DATA   * 00720000
**      SET DIRECTORY OF A UNIQUE DATA SET.  IF A SPACE HEADER        * 00721000
**      ALREADY EXISTS FOR THIS DATA SET ON THIS VOLUME, IT ALSO IS   * 00722000
**      OBTAINED.                                                     * 00723000
**                                                                    * 00724000
**   INPUT =                                                          * 00725000
**      UPSPARM - UPDATE EXTEND SPACE PARAMETER WORK AREA.            * 00726000
**         UPSPWA - CONTAINS THE EXTRACT INTERFACE INITIALIZED BY     * 00727000
**                  IGGPINEX.                                         * 00728000
**         UPDSDIR - SEQUENCE NO. OF DATA SET DIRECTORY.              * 00729000
**      IGGCCA:                                                       * 00730000
**         CCA RECORD AREA 3 CONTAINS VOLUME ENTRY. RECORD AREAS 4    * 00731000
**         AND 5 ARE REUSABLE.                                        * 00732000
**                                                                    * 00733000
**   OUTPUT =                                                         * 00734000
**      UPSPARM:                                                      * 00735000
**         UPSPWA - EXTRACT INTERFACE INITIALIZATION COMPLETED        * 00736000
**         UPDIRSP - CONTAINS DATA SET DIRECTORY OF UNIQUE DATA SET   * 00737000
**         UPSPHDR - CONTAINS SPACE HEADER OF UNIQUE DATA SET (IF ONE * 00738000
**                   EXISTS).                                         * 00739000
**      IGGCCA:                                                       * 00740000
**         CCACD1 - ANY RETURN CODE PASSED BACK BY EXTRACT.           * 00741000
**                - RCCATBAL, CATALOG OUT OF BALANCE.                 * 00742000
**                - RCDSNF, DATA SET NOT FOUND                        * 00743000
**                - RCDSNFND, DATA SPACE NOT FOUND                    * 00744000
**                                                                    * 00745000
**                                                                    * 00746000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00747000
*                                                                  0450 00748000
*   RFY                                                            0450 00749000
*     CTGPL BASED(CCAXCPL);         /* MAP EXTRACT CPL               */ 00750000
*   RFY                                                            0451 00751000
*     CTGFL BASED(CTGFIELD(1));     /* MAP FIELD PARM LIST           */ 00752000
*   UPREL=UPDSDIR;                  /* SEQUENCE NO. OF DATA SET    0452 00753000
*                                      DIRECTORY                     */ 00754000
         L     @10,CCAR3REC(,R11)                                  0452 00755000
         SH    @10,@CH00597                                        0452 00756000
         MVC   UPREL(2,@10),UPDSDIR+1(@10)                         0452 00757000
*   CTGFLDNM=ADDR(DSDIRECT);        /* POINT TO DATA SET DIRECTORY 0453 00758000
*                                      FIELD NAME                    */ 00759000
         L     @10,CCAXCPL(,R11)                                   0453 00760000
         L     @10,CTGFIELD(,@10)                                  0453 00761000
         LA    @05,DSDIRECT                                        0453 00762000
         ST    @05,CTGFLDNM(,@10)                                  0453 00763000
*   CTGFLCHN->CTGFLDGC=DSDIRGC;     /* SET PROPER GROUP CODE         */ 00764000
         L     @10,CTGFLCHN(,@10)                                  0454 00765000
         MVI   CTGFLDGC(@10),X'08'                                 0454 00766000
*   CTGFLCHN->CTGFLDCD=CTGFLDEQ;    /* TEST FOR EQUAL SEQ. NO.       */ 00767000
*                                                                  0455 00768000
         MVI   CTGFLDCD(@10),X'80'                                 0455 00769000
*   /*****************************************************************/ 00770000
*   /*                                                               */ 00771000
*   /* CALL TO MODULE IGG0CLAZ                                       */ 00772000
*   /*                                                               */ 00773000
*   /*****************************************************************/ 00774000
*                                                                  0456 00775000
*Z020EXT:                                                          0456 00776000
*   CALL IGGPEXT;                   /* EXTRACT DATA SET DIRECTORY FOR   00777000
*                                      UNIQUE                        */ 00778000
Z020EXT  L     @15,@CV00036                                        0456 00779000
         BALR  @14,@15                                             0456 00780000
*   CCAREUSE=ON;                    /* REUSE RCD AREAS HERE ON       */ 00781000
         OI    CCAREUSE(R11),B'00000100'                           0457 00782000
*   IF CCACD1^=RCS THEN                                            0458 00783000
         CLI   CCACD1(R11),0                                       0458 00784000
         BNE   @RT00458                                            0458 00785000
*     ;                             /* CHECK FOR ERROR               */ 00786000
*   ELSE                            /* NO ERROR                      */ 00787000
*     IF CTGFLPT(1)=0 THEN          /* IF NO DATA RETURNED           */ 00788000
*                                                                  0460 00789000
