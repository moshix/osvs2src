*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 02539000
**   MODULE NAME = IGGPWRIT(IGG0CLBP)                                 * 02540000
**                                                                    * 02541000
**   DESCRIPTIVE NAME = WRITE BACK SPACE OCCURRENCES                  * 02542000
**                                                                    * 02543000
**   FUNCTION =                                                       * 02544000
**      THIS SUB-FUNCTION UPDATES THE VOLUME ENTRY TO REFLECT THE     * 02545000
**      NEWLY EXTENDED OR ALLOCATED DATA SPACE.  THE PROCESSING IS    * 02546000
**      AS FOLLOWS:                                                   * 02547000
**      1.  IF AN EXISTING SPACE HEADER WAS EXTENDED, UPDATE IT TO    * 02548000
**          REFLECT THE NEW EXTENTS. OTHERWISE, ADD A NEW SPACE       * 02549000
**          HEADER FOR THE NEWLY ALLOCATED DATA SPACE.                * 02550000
**      2.  IF THE DATA SPACE WAS UNIQUE, UPDATE ITS DATA SET         * 02551000
**          DIRECTORY.                                                * 02552000
**      3.  ADD A NEW DATA SPACE DESCRIPTOR FOR EACH NEW EXTENT       * 02553000
**          DESCRIBED IN THE DADSM EXTENTS SAVE AREA.                 * 02554000
**                                                                    * 02555000
**   INPUT =                                                          * 02556000
**      UPSPARM - UPDATE EXTEND SPACE PARAMETER WORK AREA             * 02557000
**      UPSPHDR - SPACE HEADER                                        * 02558000
**      UPDIRSP - DATA SET DIRECTORY (IF UNIQUE)                      * 02559000
**      IGGCCA                                                        * 02560000
**      CCADESA - DADSM EXTENTS SAVE AREA                             * 02561000
**                                                                    * 02562000
**   OUTPUT =                                                         * 02563000
**      IGGCCA                                                        * 02564000
**        CCADESA - DADSM EXTENTS SAVE AREA                           * 02565000
**        CCAEXTSS - DATA SPACE DESCRIPTOR SEQUENCE NO.               * 02566000
**        CCACD1 - ANY RETURN CODE PASSED BACK BY MODIFY              * 02567000
**                                                                    * 02568000
**                                                                    * 02569000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 02570000
*                                                                  0876 02571000
*   DCL                                                            0876 02572000
*     RUNCODE FIXED(31) CONSTANT(253);/* RUN CODE                    */ 02573000
*                                                                  0876 02574000
*   /*****************************************************************/ 02575000
*   /*                                                               */ 02576000
*   /* MAPPING FOR SPACE MAP                                 @Y30SSRS*/ 02577000
*   /*                                                               */ 02578000
*   /*****************************************************************/ 02579000
*                                                                  0877 02580000
*   DCL                                                            0877 02581000
*     1 SPACEMAP BASED(ADDR(CTVMAP)),/* SPACE MAP DECLARE            */ 02582000
*      2 SPCODE PTR(8),             /* SPACE CODE                    */ 02583000
*      2 SPRUNC PTR(8),             /* RUN LENGTH CODE               */ 02584000
*      2 SPTRKS FIXED(15);          /* NUMBER OF TRACKS              */ 02585000
*                                                                  0877 02586000
*   /*****************************************************************/ 02587000
*   /*                                                               */ 02588000
*   /* CONVERT EXTRACT PARAMETER LISTS BUILT BY SUB-FUNCTION IGGPINEX*/ 02589000
*   /* INTO MODIFY PARAMETER LISTS TO UPDATE AND ADD REQUIRED VOLUME */ 02590000
*   /* ENTRY OCCURRENCES                                             */ 02591000
*   /*                                                               */ 02592000
*   /*****************************************************************/ 02593000
*                                                                  0878 02594000
*   RFY                                                            0878 02595000
*     CTGPL BASED(CCAMCPL);         /* MAP MODIFY CPL                */ 02596000
*   RFY                                                            0879 02597000
*     CTGFL BASED(CTGFIELD(1));     /* MAP FIELD PARM LIST           */ 02598000
*   RFY                                                            0880 02599000
*     CTVSPHDR BASED(ADDR(UPSPHDR));/* MAP SPACE HEADER              */ 02600000
*                                                                  0880 02601000
*   /*****************************************************************/ 02602000
*   /*                                                               */ 02603000
*   /* UPDATE/ADD SPACE HEADER GROUP                                 */ 02604000
*   /*                                                               */ 02605000
*   /*****************************************************************/ 02606000
*                                                                  0881 02607000
*   CTGFLDNM=ADDR(SPACEHDR);        /* POINT TO SPACE HEADER FIELD 0881 02608000
*                                      NAME                          */ 02609000
         L     @10,CCAMCPL(,R11)                                   0881 02610000
         L     @10,CTGFIELD(,@10)                                  0881 02611000
         LA    @05,SPACEHDR                                        0881 02612000
         ST    @05,CTGFLDNM(,@10)                                  0881 02613000
*   CTGFLNG(1)=LENGTH(UPSPHDR);     /* LENGTH OF SPACE HEADER        */ 02614000
         MVC   CTGFLNG(4,@10),@CF05482                             0882 02615000
*   CTGFLPT(1)=ADDR(UPSPHDR);       /* ADDR OF SPACE HEADER          */ 02616000
         L     @05,CCAR3REC(,R11)                                  0883 02617000
         LR    @15,@05                                             0883 02618000
         SH    @15,@CH00597                                        0883 02619000
         LA    @14,UPSPHDR(,@15)                                   0883 02620000
         ST    @14,CTGFLPT(,@10)                                   0883 02621000
*   CTVSPNEX=CTVSPNEX+CCANDEXT;     /* UPDATE NO. OF EXTENTS         */ 02622000
         AL    @05,@CF05474                                        0884 02623000
         SLR   @14,@14                                             0884 02624000
         IC    @14,CTVSPNEX(,@05)                                  0884 02625000
         SLR   @09,@09                                             0884 02626000
         IC    @09,CCANDEXT(,R11)                                  0884 02627000
         ALR   @14,@09                                             0884 02628000
         STC   @14,CTVSPNEX(,@05)                                  0884 02629000
*   CTGFLCHN->CTGFLCHN=0;           /* AT MOST ONE TEST              */ 02630000
         SLR   @05,@05                                             0885 02631000
         L     @10,CTGFLCHN(,@10)                                  0885 02632000
         ST    @05,CTGFLCHN(,@10)                                  0885 02633000
*   IF UPSHREL^=0 THEN              /* IF EXISTENT SPACE HEADER      */ 02634000
         LH    @14,UPSHREL(,@15)                                   0886 02635000
         CR    @14,@05                                             0886 02636000
         BE    @RF00886                                            0886 02637000
*     DO;                           /* ALTER IT                      */ 02638000
*       CTGFLCHN->CTGFLDCD=CTGFLDEQ;/* ALTER JUST ONE OCCURRENCE     */ 02639000
         MVI   CTGFLDCD(@10),X'80'                                 0888 02640000
*       CTGFLCHN->CTGFLDGC=SPHDRGC; /* SET PROPER GROUP CODE         */ 02641000
         MVI   CTGFLDGC(@10),X'06'                                 0889 02642000
*       UPREL=UPSHREL;              /* SEQUENCE NO. OF SPACE HEADER  */ 02643000
         STH   @14,UPREL(,@15)                                     0890 02644000
*     END;                          /* END ALTER                     */ 02645000
*   ELSE                            /* SPACE HEADER MUST BE ADDED    */ 02646000
*     DO;                           /* PROCESS NEW SPACE HEADER      */ 02647000
         B     @RC00886                                            0892 02648000
@RF00886 DS    0H                                                  0893 02649000
*       CTGFLCHN=0;                 /* INDICATE ADD TO MODIFY        */ 02650000
         L     @10,CCAMCPL(,R11)                                   0893 02651000
         L     @10,CTGFIELD(,@10)                                  0893 02652000
         SLR   @05,@05                                             0893 02653000
         ST    @05,CTGFLCHN(,@10)                                  0893 02654000
*     END;                          /* END PROCESS                   */ 02655000
*   CCAREUSE=ON;                    /* REUSE RECORD AREAS    @YM05977*/ 02656000
@RC00886 OI    CCAREUSE(R11),B'00000100'                           0895 02657000
*   CCAR4REC->RCDCINO=0;            /* CLEAR REC CI          @YM05977*/ 02658000
         SLR   @10,@10                                             0896 02659000
         L     @05,CCAR4REC(,R11)                                  0896 02660000
