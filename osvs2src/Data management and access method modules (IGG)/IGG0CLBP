*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00934000
**   MODULE NAME =  IGGPSRH2(IGG0CLBP)                                * 00935000
**                                                                    * 00936000
**   DESCRIPTIVE NAME =                                               * 00937000
**      SEARCH FOR SPACE HEADER TO SUBALLOCATE SPACE.                 * 00938000
**                                                                    * 00939000
**   FUNCTION =                                                       * 00940000
**      THIS SUB-FUNCTION SEQUENTIALLY SCANS EACH SPACE HEADER FOR A  * 00941000
**      DATA SPACE THAT CAN BE EXTENDED TO MEET THE SUB-ALLOCATION    * 00942000
**      REQUIREMENTS OF THE DATA SET REQUESTING MORE SPACE.  THE      * 00943000
**      CRITERIA FOR USING THE DATA SPACE IS DETERMINED BY ITS TYPE   * 00944000
**      AND QUANTITY OF SECONDARY ALLOCATION AND THE NUMBER OF EXIST- * 00945000
**      ING EXTENTS.                                                  * 00946000
**                                                                    * 00947000
**   INPUT =                                                          * 00948000
**      IGGCCA:                                                       * 00949000
**         CCA RECORD AREA 3 CONTAINS VOLUME ENTRY. RECORD AREAS 4    * 00950000
**         AND 5 ARE REUSABLE.                                        * 00951000
**      UPSPARM - UPDATE EXTEND SPACE PARAMETER WORK AREA.            * 00952000
**         UPSPWA - CONTAINS THE EXTRACT INTERFACE INITIALIZED BY     * 00953000
**                  IGGPINEX.                                         * 00954000
**         UPSPACE - MINIMUM NUMBER OF TRACKS REQUIRED.               * 00955000
**         UPSOPT - SPACE OPTIONS (CYLINDERS OR TRACKS)               * 00956000
**                                                                    * 00957000
**   OUTPUT =                                                         * 00958000
**      IGGCCA:                                                       * 00959000
**         CCACD1 - ANY RETURN CODE PASSED BACK BY EXTRACT.           * 00960000
**      UPSPARM:                                                      * 00961000
**         UPSPWA - EXTRACT INTERFACE INTIALIZATION COMPLETED.        * 00962000
**         UPSPHDR - CONTAINS SPACE HEADER, IF SPACE AND EXTENT       * 00963000
**                   CONDITIONS ARE MET.                              * 00964000
**                                                                    * 00965000
**                                                                    * 00966000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00967000
*                                                                  0490 00968000
*   RFY                                                            0490 00969000
*     CTGPL BASED(CCAXCPL);         /* MAP EXTRACT CPL               */ 00970000
*   RFY                                                            0491 00971000
*     CTGFL BASED(CTGFIELD(1));     /* MAP FIELD PARM LIST           */ 00972000
*   UPREL=0;                        /* STARTING RELREPNO FOR SPACE 0492 00973000
*                                      HEADER SEARCH                 */ 00974000
         L     @10,CCAR3REC(,R11)                                  0492 00975000
         SH    @10,@CH00597                                        0492 00976000
         SLR   @05,@05                                             0492 00977000
         STH   @05,UPREL(,@10)                                     0492 00978000
*   CTGFLDNM=ADDR(SPACEHDR);        /* ADDR OF SPACE HEADER FIELD  0493 00979000
*                                      NAME                          */ 00980000
         L     @05,CCAXCPL(,R11)                                   0493 00981000
         L     @05,CTGFIELD(,@05)                                  0493 00982000
         LA    @15,SPACEHDR                                        0493 00983000
         ST    @15,CTGFLDNM(,@05)                                  0493 00984000
*   CTGFLCHN->CTGFLDGC=SPHDRGC;     /* SPACE HEADER GROUP CODE       */ 00985000
         L     @05,CTGFLCHN(,@05)                                  0494 00986000
         MVI   CTGFLDGC(@05),X'06'                                 0494 00987000
*   CTGFLCHN->CTGFLDCD=CTGFLDGT;    /* TEST TO RETURN NEXT HIGHER  0495 00988000
*                                      GROUP CODE                    */ 00989000
         MVI   CTGFLDCD(@05),X'20'                                 0495 00990000
*   UPSEXIT=OFF;                    /* ENTER SEARCH LOOP FOR SPACE 0496 00991000
*                                      HEADER TO DO EXTEND           */ 00992000
         NI    UPSEXIT(@10),B'10111111'                            0496 00993000
*   DO WHILE(UPSEXIT=OFF&CCACD1=RCS);/* SEARCH FOR SPACE HEADER      */ 00994000
*                                                                  0497 00995000
         B     @DE00497                                            0497 00996000
@DL00497 DS    0H                                                  0498 00997000
*     /***************************************************************/ 00998000
*     /*                                                             */ 00999000
*     /* CALL TO MODULE IGG0CLAZ                                     */ 01000000
*     /*                                                             */ 01001000
*     /***************************************************************/ 01002000
*                                                                  0498 01003000
*Z025EXT:                                                          0498 01004000
*     CALL IGGPEXT;                 /* CALL EXTRACT FOR SPACE HEADER */ 01005000
Z025EXT  L     @15,@CV00036                                        0498 01006000
         BALR  @14,@15                                             0498 01007000
*     CCAREUSE=ON;                  /* REUSE RCD AREAS HERE ON       */ 01008000
         OI    CCAREUSE(R11),B'00000100'                           0499 01009000
*     IF CCACD1^=RCS|               /* IF ERROR ON EXTRACT OR        */ 01010000
*         CTGFLPT(1)=0 THEN         /* NO DATA RETURNED              */ 01011000
         CLI   CCACD1(R11),0                                       0500 01012000
         BNE   @RT00500                                            0500 01013000
