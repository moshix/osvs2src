*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00537000
**   MODULE NAME =  IGGPINEX(IGG0CLBP)                                * 00538000
**                                                                    * 00539000
**   DESCRIPTIVE NAME =                                               * 00540000
**      INITIALIZE EXTRACT PARM LIST FOR UPDATE EXTEND.               * 00541000
**                                                                    * 00542000
**   FUNCTION =                                                       * 00543000
**      THIS SUB-FUNCTION IS CALLED BY IGGPSPAC TO INITIALIZE THE     * 00544000
**      EXTRACT PARAMETER LIST SO THE  DATA SPACE HEADER AND DATA SET * 00545000
**      DIRECTORY INFORMATION CAN BE OBTAINED FROM THE VOLUME ENTRY   * 00546000
**      BEING EXTENDED.  IT ALSO PERFORMS INITIALIZATION SO THAT      * 00547000
**      SUBSEQUENT MODIFIES CAN BE DONE.                              * 00548000
**                                                                    * 00549000
**   INPUT =                                                          * 00550000
**      UPSPARM - UPDATE EXTEND SPACE PARAMETER WORK AREA.            * 00551000
**         UPSPWA - EXTRACT/MODIFY CPL/FPL AREA (ZEROED BY IGGPSPAC   * 00552000
**                  CALLER)                                           * 00553000
**                                                                    * 00554000
**   OUTPUT =                                                         * 00555000
**      IGGCCA OUTPUT:                                                * 00556000
**         CCAXCPL - EXTRACT CPL ADDRESS POINTS TO UPSPWA.            * 00557000
**         CCAREUSE - REUSE OF EXTRACT RECORD AREAS IS REQUESTED.     * 00558000
**         CCAF2NVC - NO VALIDITY CHECK IS REQUESTED.                 * 00559000
**         CCARABB,CCARABF,CCARABL - BASE,FIRST,AND LAST RECORD AREA  * 00560000
**                                   ADDRESSES INITIALIZED USING 3,4, * 00561000
**                                   AND 5 RESPECTIVELY.              * 00562000
**         CCAEXT,CCAMOD - EXTRACT AND MODIFY INDICATED.              * 00563000
**         CCAMODPA - NO PUT ADD INDICATED FOR MODIFY.                * 00564000
**                                                                    * 00565000
**      UPSPARM:                                                      * 00566000
**         UPSPWA - INITIALIZED WITH AN EXTRACT CPL WHICH IS TO BE    * 00567000
**                  USED TO OBTAIN AT MOST ONE OCCURRENCE WITH A      * 00568000
**                  PARTICULAR SEQUENCE NUMBER.  THE EXTRACT RETURN   * 00569000
**                  AREA FOLLOWS THE EXTRACT CPL/FPL'S BUILT BY THIS  * 00570000
**                  ROUTINE. THE FOLLOWING ADDITIONAL FIELDS NEED TO  * 00571000
**                  BE INITIALIZED:                                   * 00572000
**                  CTGFLDNM - THE NAME OF THE OCCURRENCE TO BE       * 00573000
**                             EXTRACTED (FIRST FPL).                 * 00574000
**                  UPREL - THE SEQUENCE NUMBER OF THE OCCURRENCE TO  * 00575000
**                          BE RETRIEVED.                             * 00576000
**                  CTGFLDCD - THE TYPE OF TEST TO BE USED AGAINST    * 00577000
**                             UPCNT FOR RETRIEVING THE OCCURRENCE    * 00578000
**                             (2ND FPL)                              * 00579000
**                  CTGFLDGC - THE GROUP CODE OF THE OCCURRENCE (2ND  * 00580000
**                             FPL).                                  * 00581000
**                                                                    * 00582000
**                                                                    * 00583000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00584000
*                                                                  0410 00585000
*   RFY                                                            0410 00586000
*     CTGPL BASED(CCAXCPL);         /* MAP EXTRACT CPL               */ 00587000
*   CCAXCPL=ADDR(UPSPWA);           /* EXTRACT CPL AREA              */ 00588000
         L     @10,CCAR3REC(,R11)                                  0411 00589000
         SH    @10,@CH00597                                        0411 00590000
         LA    @05,UPSPWA(,@10)                                    0411 00591000
         ST    @05,CCAXCPL(,R11)                                   0411 00592000
*   CCAREUSE=OFF;                   /* DONT REUSE RECORD AREAS       */ 00593000
         NI    CCAREUSE(R11),B'11111011'                           0412 00594000
*   CCAF2NVC=ON;                    /* NO ADDR VALIDITY CHECK        */ 00595000
         OI    CCAF2NVC(R11),B'01000000'                           0413 00596000
*   CCARABB=ADDR(CCARAB3);          /* BASE RECORD AREA              */ 00597000
         LA    @15,CCARAB3(,R11)                                   0414 00598000
         ST    @15,CCARABB(,R11)                                   0414 00599000
*   CCARABF=ADDR(CCARAB3);          /* FIRST RECORD AREA             */ 00600000
         ST    @15,CCARABF(,R11)                                   0415 00601000
*   CCARABL=ADDR(CCARAB5);          /* LAST RECORD AREA              */ 00602000
         LA    @15,CCARAB5(,R11)                                   0416 00603000
         ST    @15,CCARABL(,R11)                                   0416 00604000
*   CCAEXT=ON;                      /* EXTRACT INDICATOR             */ 00605000
*   CCAMOD=ON;                      /* MODIFY INDICATOR              */ 00606000
         OI    CCAEXT(R11),B'00000011'                             0418 00607000
*   CCAMODPA=OFF;                   /* NO PUT ADD                    */ 00608000
         NI    CCAMODPA(R11),B'11101111'                           0419 00609000
*   CTGNOFLD=FPLNUM;                /* NUMBER FPLS                   */ 00610000
         MVI   CTGNOFLD(@05),X'02'                                 0420 00611000
*   CTGFIELD(1)=CCAXCPL+LENGTH(CTGPL)+/* POINT TO THE FIRST FIELD    */ 00612000
*       FPLNUM*LENGTH(CTGFIELD);    /* PARM LIST AREA                */ 00613000
         LA    @15,36                                              0421 00614000
         ALR   @15,@05                                             0421 00615000
         ST    @15,CTGFIELD(,@05)                                  0421 00616000
*   RFY                                                            0422 00617000
*     CTGFL BASED(CTGFIELD(1));     /* MAP CURRENT FPL               */ 00618000
*   CTGFLCHN=CTGFIELD(1)+LENGTH(CTGFL)/* POINT TO FOLLOWING          */ 00619000
*       +NODAT*LENGTH(CTGFLDAT);    /* TEST FPL                      */ 00620000
         LA    @14,24                                              0423 00621000
         LR    @09,@15                                             0423 00622000
         ALR   @09,@14                                             0423 00623000
         ST    @09,CTGFLCHN(,@15)                                  0423 00624000
*   R2=CTGFLCHN;                    /* BASE FOR NEXT TEST FPL        */ 00625000
         LR    R2,@09                                              0424 00626000
*   RFY                                                            0425 00627000
*     CTGFL BASED(R2);              /* MAP TEST FPL                  */ 00628000
*   CTGFLDNM=ADDR(RELREPNO);        /* POINT TO FIELD NAME           */ 00629000
         LA    @15,@CC00143                                        0426 00630000
         ST    @15,CTGFLDNM(,R2)                                   0426 00631000
*   CTGFLDNO=NODAT;                 /* INDICATE TEST DATA            */ 00632000
         MVI   CTGFLDNO(R2),X'01'                                  0427 00633000
*   CTGFLPT(1)=ADDR(UPREL);         /* POINT TO TEST DATA            */ 00634000
         LA    @15,UPREL(,@10)                                     0428 00635000
         ST    @15,CTGFLPT(,R2)                                    0428 00636000
*   CTGFLNG(1)=LENGTH(UPREL);       /* LENGTH TEST DATA              */ 00637000
         LA    @15,2                                               0429 00638000
         ST    @15,CTGFLNG(,R2)                                    0429 00639000
*   CTGFLCHN=ADDR(CTGFL)+LENGTH(CTGFL)/* POINT TO NEXT TEST FPL      */ 00640000
*       +NODAT*LENGTH(CTGFLDAT);    /* AREA                          */ 00641000
         LR    @09,R2                                              0430 00642000
         ALR   @09,@14                                             0430 00643000
         ST    @09,CTGFLCHN(,R2)                                   0430 00644000
*   R2=CTGFLCHN;                    /* BASE FOR TEST FPL             */ 00645000
         LR    R2,@09                                              0431 00646000
*   CTGFLDNM=ADDR(CNTREPNO);        /* POINT TO FIELD NAME           */ 00647000
         LA    @09,@CC00141                                        0432 00648000
         ST    @09,CTGFLDNM(,R2)                                   0432 00649000
*   CTGFLDNO=NODAT;                 /* SET NO. DATA/LNG PAIR         */ 00650000
         MVI   CTGFLDNO(R2),X'01'                                  0433 00651000
*   CTGFLDCD=CTGFLDLE;              /* TEST CONDITION                */ 00652000
         MVI   CTGFLDCD(R2),X'C0'                                  0434 00653000
*   CTGFLPT(1)=ADDR(UPCNT);         /* POINT TO TEST DATA            */ 00654000
         LA    @09,UPCNT(,@10)                                     0435 00655000
         ST    @09,CTGFLPT(,R2)                                    0435 00656000
*   UPCNT=1;                        /* LIMIT NO. OF OCCURRENCES    0436 00657000
*                                      RETURNED TO ONE               */ 00658000
         MVC   UPCNT(2,@10),@CH00088                               0436 00659000
*   CTGFLNG(1)=LENGTH(UPCNT);       /* LENGTH TEST DATA              */ 00660000
         ST    @15,CTGFLNG(,R2)                                    0437 00661000
*   CTGWKA=ADDR(CTGFL)+LENGTH(CTGFL)/* SET ADDR OF WORK AREA         */ 00662000
*       +NODAT*LENGTH(CTGFLDAT);    /* AFTER LAST FPL                */ 00663000
         ALR   @14,R2                                              0438 00664000
         ST    @14,CTGWKA(,@05)                                    0438 00665000
*   WKALEN1=EXTWALEN;               /* EXTRACT WORK AREA LENGTH      */ 00666000
         MVC   WKALEN1(2,@14),@CH00061                             0439 00667000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00668000
         BAL   @14,EXIT                                            0440 00669000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00670000
         DS 0F                                                          00671000
*   GEN(DC CL8'IGGPSRH1');          /* NAME OF FOLLOWING PROCEDURE   */ 00672000
         DC CL8'IGGPSRH1'                                               00673000
*   END;                            /* END OF INNER PROCEDURE        */ 00674000
@EL00003 DS    0H                                                  0443 00675000
@EF00003 DS    0H                                                  0443 00676000
@ER00003 BR    @14                                                 0443 00677000
@PB00003 DS    0H                                                  0444 00678000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 00679000
         EJECT                                                          00680000
*                                                                  0445 00681000
