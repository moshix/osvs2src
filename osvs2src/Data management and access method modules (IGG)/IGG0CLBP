         BNZ   @RF00500                                            0500 01017000
@RT00500 DS    0H                                                  0501 01018000
*       DO;                         /* EXIT                          */ 01019000
*         UPSEXIT=ON;               /* EXIT INDICATOR                */ 01020000
         L     @10,CCAR3REC(,R11)                                  0502 01021000
         SH    @10,@CH00597                                        0502 01022000
         OI    UPSEXIT(@10),B'01000000'                            0502 01023000
*         UPSHREL=0;                /* NO SPACE HEADER FOUND         */ 01024000
         SLR   @05,@05                                             0503 01025000
         STH   @05,UPSHREL(,@10)                                   0503 01026000
*       END;                        /* END EXIT                      */ 01027000
*     ELSE                          /* DATA RETURNED                 */ 01028000
*       DO;                         /* TEST SPACE HEADER             */ 01029000
