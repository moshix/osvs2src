*       /*************************************************************/ 01002000
*       /*                                                           */ 01003000
*       /* PROCESS A CHECK OR CONDITIONAL CHECK REQUEST              */ 01004000
*       /*                                                           */ 01005000
*       /*************************************************************/ 01006000
*                                                                  0351 01007000
*       RFY                                                        0351 01008000
*         R9 RSTD;                  /* GET REGISTER FOR EXEC.        */ 01009000
*       IF CCABMOST=ON              /* IF ON, BRANCH ZERO IF         */ 01010000
*         THEN                                                     0352 01011000
         TM    CCABMOST(R11),B'10000000'                           0352 01012000
         BNO   @RF00352                                            0352 01013000
*         R9=BZMASK;                /* MISMATCH. IF OFF, BR          */ 01014000
         LA    R9,128                                              0353 01015000
