*         R6=CCABMPTR;              /* POINT TO BYTE TO TEST         */ 00237000
@RC00159 L     R6,CCABMPTR(,R11)                                   0162 00238000
*         GEN(EX    R7,TEST);       /* SEE IF BIT IS ON OR OFF       */ 00239000
         EX    R7,TEST                                                  00240000
*         BC(8,NOTON);              /* BRANCH IF OFF                 */ 00241000
