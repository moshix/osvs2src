         CR    R7,@15                                              0181 00315000
         BNH   *+6                                                      00316000
         LR    R7,@15                                              0181 00317000
*       CCABMBYT=CCABMBYT-R7;       /* RESET FULL BYTES LEFT         */ 00318000
         SLR   @15,R7                                              0182 00319000
         STH   @15,CCABMBYT(,R11)                                  0182 00320000
*       IF R7^=ZERO                 /* PROCESS ANY FULL BYTES        */ 00321000
*         THEN                      /* IN THE CURRENT BIT MASK       */ 00322000
         LTR   R7,R7                                               0183 00323000
         BZ    @RF00183                                            0183 00324000
*         DO;                                                      0184 00325000
*                                                                  0184 00326000
*           /*********************************************************/ 00327000
*           /*                                                       */ 00328000
*           /* CHECK OR SET REQUIRED BYTES IN CURRENT BIT MASK       */ 00329000
*           /*                                                       */ 00330000
*           /*********************************************************/ 00331000
*                                                                  0185 00332000
*           RFY                                                    0185 00333000
*            (R8,                                                  0185 00334000
*             R9,                                                  0185 00335000
*             R10) RSTD;            /* GET WORK REGISTERS            */ 00336000
*           R10=R7;                 /* SAVE PROCESS COUNT            */ 00337000
         LR    R10,R7                                              0186 00338000
*           R6=CCABMPTR;            /* POINT TO BIT MASK             */ 00339000
         L     @15,CCABMPTR(,R11)                                  0187 00340000
         LR    R6,@15                                              0187 00341000
*           R8=ADDR(CCABMPAD);      /* POINT TO STATE BYTE           */ 00342000
         LA    R8,CCABMPAD(,R11)                                   0188 00343000
*           R9=ONE;                 /* SET LENGTH OF STATE           */ 00344000
         LA    R9,1                                                0189 00345000
*           GEN(ICM   R9,8,0(R8));  /* PUT PAD CHAR IN HI BYTE       */ 00346000
         ICM   R9,8,0(R8)                                               00347000
*           IF CCABMSET=ON          /* IF A SET REQUEST, PUT         */ 00348000
*             THEN                                                 0191 00349000
         TM    CCABMSET(R11),B'00100000'                           0191 00350000
         BNO   @RF00191                                            0191 00351000
*             GEN(MVCL  R6,R8);     /* PAD CHARS IN. ELSE,           */ 00352000
         MVCL  R6,R8                                                    00353000
*           ELSE                                                   0193 00354000
*             GEN(CLCL  R6,R8);     /* COUNT EQUAL BYTES.            */ 00355000
*                                                                  0193 00356000
         B     @RC00191                                            0193 00357000
@RF00191 DS    0H                                                  0193 00358000
         CLCL  R6,R8                                                    00359000
*           /*********************************************************/ 00360000
*           /*                                                       */ 00361000
*           /* DETERMINE NUMBER OF BYTES ACTUALLY PROCESSED.         */ 00362000
*           /* (MEANINGFUL ONLY FOR CHECK REQUESTS.)                 */ 00363000
*           /*                                                       */ 00364000
*           /*********************************************************/ 00365000
*                                                                  0194 00366000
*           R10=R10-R7;             /* BYTES PROCESSED               */ 00367000
@RC00191 SLR   R10,R7                                              0194 00368000
*           CCABMONN=R10*EIGHT+CCABMONN;/* BITS PROCESSED            */ 00369000
*                                                                  0195 00370000
         LR    @15,R10                                             0195 00371000
         SLA   @15,3                                               0195 00372000
         AH    @15,CCABMONN(,R11)                                  0195 00373000
         STH   @15,CCABMONN(,R11)                                  0195 00374000
*           /*********************************************************/ 00375000
*           /*                                                       */ 00376000
*           /* IF COMPARE STOPPED EARLY, CAUSE NO MORE CHECKING OF   */ 00377000
*           /* FULL BYTES. CAUSE THE FIRST SEVEN BITS OF THE BYTE AT */ 00378000
*           /* WHICH THE COMPARE FAILED TO BE CHECKED INSTEAD.       */ 00379000
*           /*                                                       */ 00380000
*           /*********************************************************/ 00381000
*                                                                  0196 00382000
*           IF R7^=ZERO             /* SEE IF COMPARE FAILED         */ 00383000
*             THEN                  /* IN THE MIDDLE.                */ 00384000
         SLR   @15,@15                                             0196 00385000
         CR    R7,@15                                              0196 00386000
         BE    @RF00196                                            0196 00387000
*             DO;                   /* YES, RESET BIT/BYTE CNT       */ 00388000
*               CCABMBYT=ZERO;      /* NO MORE FULL BYTES            */ 00389000
         STH   @15,CCABMBYT(,R11)                                  0198 00390000
*               CCABMBTL=EIGHT-ONE; /* SET LAST BIT CNT TO 7         */ 00391000
