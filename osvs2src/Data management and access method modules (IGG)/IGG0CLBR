*         ELSE                                                     0345 00983000
*           R6=R6|R8;               /* WISE, OR REGISTERS.           */ 00984000
         B     @RC00343                                            0345 00985000
@RF00343 OR    R6,R8                                               0345 00986000
*         R8=R8/TWO;                /* SHIFT MASK OVER               */ 00987000
@RC00343 SRL   R8,1                                                0346 00988000
*       END;                        /* END OF BIT RESETTING          */ 00989000
         BCTR  R7,0                                                0347 00990000
@DE00342 LTR   R7,R7                                               0347 00991000
         BP    @DL00342                                            0347 00992000
*       PBYTE=R6;                   /* STORE UPDATED BYTE            */ 00993000
         L     @10,CCABMPTR(,R11)                                  0348 00994000
