@RT00177 DS    0H                                                  0178 00297000
*           CCABMPTR=CCABMPTR+ONE;  /* POINT TO NEXT BYTE            */ 00298000
         LA    @10,1                                               0178 00299000
         AL    @10,CCABMPTR(,R11)                                  0178 00300000
         ST    @10,CCABMPTR(,R11)                                  0178 00301000
*       END;                        /* END OF 1ST BYTE PROCESS       */ 00302000
@RF00177 DS    0H                                                  0180 00303000
*/*  PROCESS ANY FULL BYTES THAT EXIST WITHIN THE SPECIFIED RANGE    */ 00304000
*                                                                  0180 00305000
*     DO WHILE CCABMBYT^=ZERO;      /* LOOP UNTIL ALL PROCESS        */ 00306000
@RF00172 B     @DE00180                                            0180 00307000
@DL00180 DS    0H                                                  0181 00308000
*       R7=MIN(CCABMEND-CCABMPTR,CCABMBYT);/* DETERMINE PROCESS COUNT   00309000
*                                      (LIMITING TO BLOCK END)       */ 00310000
         L     @10,CCABMPTR(,R11)                                  0181 00311000
         L     R7,CCABMEND(,R11)                                   0181 00312000
         SLR   R7,@10                                              0181 00313000
