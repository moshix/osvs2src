*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA     0331 00911000
*                                      PROCESS PARTIAL               */ 00912000
         LA 13,12(13)                                                   00913000
*/*                      IGGPPART--PROCESS PARTIAL BYTE            0332 00914000
* 01 FUNCTION = FOR A CHECK REQUEST, DETERMINE THE NUMBER OF BITS THAT  00915000
*               MATCH CCABMOST.  FOR A SET REQUEST, SET THE GIVEN  0332 00916000
*               NUMBER OF BITS TO CCABMOST.                        0332 00917000
* 01 INPUT = R6 CONTAINS THE NUMBER OF BITS TO SKIP OVER, R7 CONTAINS   00918000
*            A LIMITING COUNT, CCABMPTR CONTAINS THE ADDRESS OF THE     00919000
*            BYTE TO PROCESS, AND CCABMOST CONTAINS THE STATE TO SET    00920000
*            OR BE CHECKED.                                        0332 00921000
* 01 OUTPUT = FOR A CHECK REQUEST, CCABMONN REFLECTS THE NUMBER OF 0332 00922000
*             BITS MATCHING CCABMOST AND IN ADDITION, CCABMBT1,    0332 00923000
*             CCABMBTL AND CCABMBYT ARE RESET. FOR A SET REQUEST   0332 00924000
*             THE REQUIRED NUMBER OF BITS ARE RESET.                 */ 00925000
*                                                                  0332 00926000
*   RFY                                                            0332 00927000
*    (R6,                                                          0332 00928000
*     R7,                                                          0332 00929000
*     R8) RSTD;                     /* GET REGISTERS                 */ 00930000
*/*  DETERMINE TYPE OF REQUEST--SET, CHECK, OR CONDITIONAL CHECK.    */ 00931000
*                                                                  0333 00932000
*   IF CCABMSET=ON                  /* IF THIS IS A SET REQ.,        */ 00933000
*     THEN                          /* COME HERE.                    */ 00934000
         TM    CCABMSET(R11),B'00100000'                           0333 00935000
         BNO   @RF00333                                            0333 00936000
*     DO;                                                          0334 00937000
*                                                                  0334 00938000
*       /*************************************************************/ 00939000
*       /*                                                           */ 00940000
*       /* PROCESS A SET BIT MASK REQUEST. PICK UP A MASK WITH WHICH */ 00941000
*       /* TO AND OR OR BITS.                                        */ 00942000
*       /*                                                           */ 00943000
*       /*************************************************************/ 00944000
*                                                                  0335 00945000
*       IF CCABMOST=ON              /* IF BIT ON, GET MASK OF        */ 00946000
*         THEN                                                     0335 00947000
         TM    CCABMOST(R11),B'10000000'                           0335 00948000
         BNO   @RF00335                                            0335 00949000
*         R8=TSTMASKS(R6+ONE);      /* SINGLE BIT ON. ELSE,          */ 00950000
         SLR   R8,R8                                               0336 00951000
         IC    R8,TSTMASKS(R6)                                     0336 00952000
*       ELSE                        /* MASK OF SINGLE BIT OFF        */ 00953000
*         DO;                                                      0337 00954000
         B     @RC00335                                            0337 00955000
@RF00335 DS    0H                                                  0338 00956000
*           R8=OFFMASK(R6+ONE);     /* GET PROPER MASK               */ 00957000
         SLR   R8,R8                                               0338 00958000
         IC    R8,OFFMASK(R6)                                      0338 00959000
*           GEN(ICM R8,2,ALLON);    /* PROTECT OTHER BITS            */ 00960000
         ICM R8,2,ALLON                                                 00961000
*         END;                      /* END OF OFF CODE               */ 00962000
*       R6=PBYTE;                   /* PICK UP PROCESS BYTE          */ 00963000
*                                                                  0341 00964000
@RC00335 L     @10,CCABMPTR(,R11)                                  0341 00965000
         SLR   R6,R6                                               0341 00966000
         IC    R6,PBYTE(,@10)                                      0341 00967000
*       /*************************************************************/ 00968000
*       /*                                                           */ 00969000
*       /* SET BITS IN PROCESS BYTE REGISTER                         */ 00970000
*       /*                                                           */ 00971000
*       /*************************************************************/ 00972000
*                                                                  0342 00973000
*       DO R7=R7 TO ONE BY-ONE;     /* LOOP UNTIL ALL SET            */ 00974000
         B     @DE00342                                            0342 00975000
@DL00342 DS    0H                                                  0343 00976000
*         IF CCABMOST=OFF           /* IF BIT IS TO BE OFF,          */ 00977000
*           THEN                                                   0343 00978000
         TM    CCABMOST(R11),B'10000000'                           0343 00979000
