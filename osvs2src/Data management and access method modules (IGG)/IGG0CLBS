         BNE   @RF00397                                            0397 01045000
*     DO;                           /* CONTINUE PROCESS      @Y30SSRS*/ 01046000
*                                                                  0398 01047000
*       /*************************************************************/ 01048000
*       /*                                                           */ 01049000
*       /* CLEAR CCAMODID, CCAREASN, AND CCARETRN                    */ 01050000
*       /*                                                           */ 01051000
*       /*************************************************************/ 01052000
*                                                                  0399 01053000
*       CCAPROB=IGNOREIT;           /* CLEAR ERR FIELD       @Y30SSRS   01054000
*                                      FOR PARTIAL ENTRY     @Y30SSRS*/ 01055000
         SLR   @10,@10                                             0399 01056000
         ST    @10,CCAPROB(,R11)                                   0399 01057000
*     END;                          /* END CONTINUE          @Y30SSRS*/ 01058000
*   ELSE                                                           0401 01059000
*     ;                             /* NOT FREE RCD ERR      @Y30SSRS*/ 01060000
@RF00397 DS    0H                                                  0402 01061000
*   CCALREL=1;                      /* SET FIRST SEQUENCE NO.        */ 01062000
         MVC   CCALREL(2,R11),@CH00041                             0402 01063000
