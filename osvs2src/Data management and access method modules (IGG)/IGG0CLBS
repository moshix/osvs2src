*       DSDEXIT=ON;                 /* EXIT INDICATOR                */ 01114000
         LA    @10,CCACHAIN(,R11)                                  0414 01115000
         OI    DSDEXIT+6(@10),B'10000000'                          0414 01116000
*     ELSE                          /* GOT EXTENT DATA               */ 01117000
*       IF DSDVOL=VOLVOL&           /* IF CORRECT VOLUME             */ 01118000
*           VOLSSWD=OFF THEN        /* OCCURRENCE                    */ 01119000
         B     @RC00413                                            0415 01120000
