         BNE   @RF00407                                            0407 01088000
*       DO;                         /* CONTINUE PROCESS      @Y30SSRS*/ 01089000
*                                                                  0408 01090000
*         /***********************************************************/ 01091000
*         /*                                                         */ 01092000
*         /* CLEAR CCAMODID, CCAREASN, AND CCARETRN                  */ 01093000
*         /*                                                         */ 01094000
*         /***********************************************************/ 01095000
*                                                                  0409 01096000
*         CCAPROB=IGNOREIT;         /* CLEAR ERR FIELD       @Y30SSRS   01097000
*                                      FOR PARTIAL ENTRY     @Y30SSRS*/ 01098000
         SLR   @10,@10                                             0409 01099000
         ST    @10,CCAPROB(,R11)                                   0409 01100000
*         CCAGVNF=ON;               /* DATA NOT FOUND        @Y30SSRS*/ 01101000
         OI    CCAGVNF(R11),B'00010000'                            0410 01102000
*       END;                        /* END CONTINUE          @Y30SSRS*/ 01103000
*     ELSE                                                         0412 01104000
*       ;                           /* NOT FREE RCD ERR      @Y30SSRS*/ 01105000
@RF00407 DS    0H                                                  0413 01106000
*     IF CCAGVNBS=ON|CCAGVNF=ON|    /* IF UNABLE TO GET EXTENT       */ 01107000
