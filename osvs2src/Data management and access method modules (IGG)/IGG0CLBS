         B     @DE00403                                            0403 01065000
@DL00403 DS    0H                                                  0404 01066000
*     CCADICT='C8140303'X;          /* SET DICT INFO TO GET EXTENT 0404 01067000
*                                      INFO                          */ 01068000
         MVC   CCADICT(4,R11),@CB02921                             0404 01069000
*     CCAGVEXT=0;                   /* FIRST CALL                    */ 01070000
*                                                                  0405 01071000
         SLR   @10,@10                                             0405 01072000
         STCM  @10,7,CCAGVEXT(R11)                                 0405 01073000
*     /***************************************************************/ 01074000
*     /*                                                             */ 01075000
*     /* CALL TO MODULE IGG0CLBA                                     */ 01076000
*     /*                                                             */ 01077000
*     /***************************************************************/ 01078000
*                                                                  0406 01079000
*Z022GVAL:                                                         0406 01080000
*     CALL IGGPGVAL;                /* GET FIELD VALUE               */ 01081000
Z022GVAL L     @15,@CV00038                                        0406 01082000
         BALR  @14,@15                                             0406 01083000
*     IF CCACD1=RCENT&CCAREASN=RSN4 THEN/* IF FREE RCD ERR   @Y30SSRS*/ 01084000
