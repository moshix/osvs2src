         STH   @15,DSDEXCT(,@05)                                   0417 01133000
*           DSDRESID=ELN;           /* INITIALLY NO RESIDUE          */ 01134000
         MVC   DSDRESID(2,@05),@CH00114                            0418 01135000
*           DSDEXIT1=OFF;           /* START EXTENT SCAN             */ 01136000
         NI    DSDEXIT1(@05),B'10111111'                           0419 01137000
*           DO WHILE(DSDEXIT1=OFF&CCACD1=RCS);/* EXTENT SCAN FOR DATA   01138000
