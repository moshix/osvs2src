         ST    @15,DSDPTR(,@10)                                    0371 00914000
*         END;                      /* END PUT                       */ 00915000
*       ELSE                        /* NO ROOM IN RETURN FIELD       */ 00916000
*         DO;                       /* SET UP FOR RETURN             */ 00917000
         B     @RC00366                                            0373 00918000
@RF00366 DS    0H                                                  0374 00919000
*           CCAGVEXT=DSDSEQ;        /* SET SEQ. NO.                  */ 00920000
