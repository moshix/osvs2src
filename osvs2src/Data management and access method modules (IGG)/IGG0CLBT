*     SPGFLG BIT(8) CONSTANT('C8'X);/* ZERO RESERVED BITS IN DATA  0207 00518000
*                                      SPACE GROUP                   */ 00519000
*   DCL                                                            0208 00520000
*     DIMDSAR FIXED(31) CONSTANT(16);/* DIM OF DATA          @Y30SSRS   00521000
*                                      SPACE ARRAY           @Y30SSRS*/ 00522000
*                                                                  0208 00523000
*   /*****************************************************************/ 00524000
*   /*                                                               */ 00525000
*   /* MAPPING FOR DELETE/INSERT CCA FIELDS                  @Y30SSRS*/ 00526000
*   /*                                                               */ 00527000
*   /*****************************************************************/ 00528000
*                                                                  0209 00529000
*   DCL                                                            0209 00530000
*     1 NEWGROUP BASED(ADDR(CCACHAIN)),/* USE DEIN CCA FIELDS        */ 00531000
*      2 CCAFLNG1 PTR(31),          /* SAVE FPL LENGTH               */ 00532000
*      2 CCAFLPT1 PTR(31),          /* SAVE FPL DATA ADDR            */ 00533000
*      2 CCAFLDWA CHAR(4),          /* SAVE FPL DICT INFO            */ 00534000
*      2 NEWSPGRP CHAR(85),         /* NEW DATA SPACE GROUP          */ 00535000
*       3 NEWSPG1 CHAR(16),         /* DATA SPACE DATA               */ 00536000
*       3 * CHAR(1),                /* DATA SPACE DATA               */ 00537000
*       3 NEWSPG2 CHAR(4),          /* DATA SPACE DATA               */ 00538000
*       3 * CHAR(64),               /* DATA SPACE DATA               */ 00539000
