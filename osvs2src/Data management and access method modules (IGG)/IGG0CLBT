         BE    @RF00139                                            0139 00210000
@RT00139 DS    0H                                                  0140 00211000
*     DO;                           /* NEGATE ALTER                  */ 00212000
*       CCANF=ON;                   /* END PROCESSING OF GROUP       */ 00213000
         OI    CCANF(R11),B'00001000'                              0141 00214000
