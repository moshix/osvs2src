*       FIELDNM='NODSDSP '|         /* DESCRIPTOR ALTER OR E         */ 00183000
*       FIELDNM='NODSPEXT'|         /* DIRECTORY ALTER               */ 00184000
*       FIELDNM='NODSPACE'|FIELDNM='NODSET  '|DICTGC=CTVDSPDC|(DICTGC=  00185000
*       CTVDSDGC&^(FIELDNM='CRADIRCT'|FIELDNM='DSCRETS '|FIELDNM=  0139 00186000
*       'DSSUMTT ')) THEN                                          0139 00187000
