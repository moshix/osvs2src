         BE    @RT00139                                            0139 00191000
         CLC   FIELDNM(8,@05),@CC02979                             0139 00192000
         BE    @RT00139                                            0139 00193000
         CLC   FIELDNM(8,@05),@CC02980                             0139 00194000
         BE    @RT00139                                            0139 00195000
         CLC   FIELDNM(8,@05),@CC02981                             0139 00196000
         BE    @RT00139                                            0139 00197000
