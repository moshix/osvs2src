*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 02542000
**   MODULE NAME =  IGGPSMFC (IGG0CLBV)                               * 02543000
**                                                                    * 02544000
**   DESCRIPTIVE NAME =  BUILD AND WRITE COMMON SMF RECORD            * 02545000
**                                                                    * 02546000
**   FUNCTION =                                                       * 02547000
**      THIS SUB-FUNCTION FORMATS THE COMMON PORTIONS OF SMF RECORDS  * 02548000
**      63,67,68, AND 69. INVOKES THE SMF SVC TO WRITE THE SMF        * 02549000
**      RECORD. AND FREES ANY SMF RECORD CORE OBTAINED IN THIS        * 02550000
**      MODULE.                                                       * 02551000
**                                                                    * 02552000
**   INPUT =                                                          * 02553000
**      IGGCCA INPUT:                                                 * 02554000
**         CCASMFRD - ADDRESS OF SMF RECORD TO BE WRITTEN             * 02555000
**         CCASMFLN - LENGTH OF SMF RECORD CORE TO BE FREED. MAY BE   * 02556000
**                    ZERO.                                           * 02557000
**                                                                    * 02558000
**   OUTPUT =                                                         * 02559000
**      SMF RECORD WRITTEN                                            * 02560000
**      SMF RECORD CORE FREED                                         * 02561000
**                                                                    * 02562000
**                                                                    * 02563000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 02564000
*                                                                  0830 02565000
*   DCL                                                            0830 02566000
*     JM1JOB CHAR(8) BASED(ADDR(JMRJOB));/* MAP JOB NAME             */ 02567000
*   DCL                                                            0831 02568000
*     JM1USEID CHAR(8) BASED(ADDR(JMRUSEID));/* MAP USER ID          */ 02569000
*   DO;                             /* CREATE COMMON FIELDS          */ 02570000
*     RFY                                                          0833 02571000
*       SMFRCD63 BASED(CCASMFRD);   /* USE THIS DECLARE TO MAP COMMON   02572000
*                                      PORTION OF SMF RCD            */ 02573000
*     SMF63SEG=0;                   /* ZERO RCD SPAN INDICATOR       */ 02574000
         SLR   @10,@10                                             0834 02575000
         L     @05,CCASMFRD(,R11)                                  0834 02576000
         STH   @10,SMF63SEG(,@05)                                  0834 02577000
*     SMF63SYS=0;                   /* CLEAR SYSTEM IND      @VS06444*/ 02578000
         MVI   SMF63SYS(@05),X'00'                                 0835 02579000
*     IF TCBTCT^=0&TCTJMR^=0 THEN   /* IF JOB MGMT RCD EXISTS        */ 02580000
         L     @01,CVTPTR                                          0836 02581000
         L     @01,CVTTCBP(,@01)                                   0836 02582000
         L     @01,TCBPTR(,@01)                                    0836 02583000
         L     @15,TCBTCT(,@01)                                    0836 02584000
         CR    @15,@10                                             0836 02585000
         BE    @RF00836                                            0836 02586000
         L     @15,TCTJMR(,@15)                                    0836 02587000
         CR    @15,@10                                             0836 02588000
         BE    @RF00836                                            0836 02589000
*       DO;                         /* SET INFO FROM JMR             */ 02590000
*         SMF63JNM=JM1JOB;          /* JOB NAME                      */ 02591000
         MVC   SMF63JNM(8,@05),JM1JOB(@15)                         0838 02592000
*         SMF63RST=JMRENTRY;        /* READER START TIME             */ 02593000
         LR    @10,@15                                             0839 02594000
         SL    @10,@CF00061                                        0839 02595000
         MVC   SMF63RST(4,@05),JMRENTRY(@10)                       0839 02596000
*         SMF63RDT=JMREDATE;        /* READER START DATE             */ 02597000
         MVC   SMF63RDT(4,@05),JMREDATE(@10)                       0840 02598000
*         SMF63UID=JM1USEID;        /* USER ID                       */ 02599000
         MVC   SMF63UID(8,@05),JM1USEID+20(@15)                    0841 02600000
*       END;                        /* END SET                       */ 02601000
*     ELSE                          /* JOB MGMT RCD NOT EXIST        */ 02602000
*       DO;                         /* DEFAULT VALUES SET            */ 02603000
         B     @RC00836                                            0843 02604000
@RF00836 DS    0H                                                  0844 02605000
*         SMF63JNM=BLANKS;          /* JOB NAME                      */ 02606000
         L     @10,CCASMFRD(,R11)                                  0844 02607000
         MVC   SMF63JNM(8,@10),@CC00062                            0844 02608000
*         SMF63RST=0;               /* READER START TIME             */ 02609000
         SLR   @05,@05                                             0845 02610000
         STCM  @05,15,SMF63RST(@10)                                0845 02611000
*         SMF63RDT=0;               /* READER START DATE             */ 02612000
         STCM  @05,15,SMF63RDT(@10)                                0846 02613000
*         SMF63UID=BLANKS;          /* USER ID                       */ 02614000
         MVC   SMF63UID(8,@10),@CC00062                            0847 02615000
*       END;                        /* END DEFAULT                   */ 02616000
*     R2=CCASMFRD;                  /* SET UP TO WRITE SMF RECORD    */ 02617000
