@RC00542 DS    0H                                                  0549 01357000
@RC00540 DS    0H                                                  0549 01358000
@RC00538 DS    0H                                                  0549 01359000
@RC00536 DS    0H                                                  0549 01360000
@RC00534 L     @10,CCACPE1+108(,R11)                               0549 01361000
         CLI   RCDID(@10),C'B'                                     0549 01362000
         BNE   @RF00549                                            0549 01363000
*     SMF63GDB=ON;                  /* TURN BIT ON             Y02020*/ 01364000
