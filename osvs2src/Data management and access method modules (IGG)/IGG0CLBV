Z038     DS    0H                                                  0473 01062000
Z038SMFM DS    0H                                                  0474 01063000
*   RFY                                                            0474 01064000
*    (R2,                                                          0474 01065000
*     R3,                                                          0474 01066000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01067000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 01068000
         STM 12,14,12(13)                                               01069000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA     0476 01070000
*                                      PROCEDURE                     */ 01071000
         LA 13,12(13)                                                   01072000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 01073000
**   MODULE NAME =  IGGPSMFM (IGG0CLBV)                               * 01074000
**                                                                    * 01075000
**   DESCRIPTIVE NAME =  CREATE SMF NEW CATALOG ENTRY FIELD           * 01076000
**                                                                    * 01077000
**   FUNCTION =                                                       * 01078000
**      THIS SUB-FUNCTION IS CALLED BY IGGPSMFE TO CREATE THE NEW     * 01079000
**      CATALOG ENTRY FIELD IN SMF RECORDS 63 AND 67.  IT READS IN    * 01080000
**      ALL RECORDS ASSOCIATED WITH THE CATALOG ENTRY AND USES        * 01081000
**      IGGPSMFP TO PLACE THEM INTO THE SMF RECORD.  (IF THE APPROX-  * 01082000
**      IMATION OF THE SIZE OF THE RECORDS IN THE ENTIRE CATALOG      * 01083000
**      ENTRY IS LESS THAN THE ACTUAL SIZE,   THIS SUB-FUNCTION       * 01084000
**      EFFECTIVELY ONLY RETURNS THE CORRECT COUNT FOR A RE-TRY)      * 01085000
**      IT ALSO INTIALIZES THE CATALOG DATA SET ENTRY TYPE FIELD.     * 01086000
**      BECAUSE THIS SUB-FUNCTION MAY BE CALLED AGAIN FOR A RE-TRY,   * 01087000
**      THE BUFFERS ARE MANIPULATED SO THAT THE BASE ENTRY RECORD IS  * 01088000
**      NOT OVERLAID.                                                 * 01089000
**                                                                    * 01090000
**   INPUT =                                                          * 01091000
**      IGGCCA INPUT:                                                 * 01092000
**         CCASMFRD - POINTS TO SMF RECORD 63 OR 67.                  * 01093000
**         CCASMFLN - DISPLACEMENT TO NEW CATALOG ENTRY FIELD.        * 01094000
**         CCA RECORD AREA 1 HAS CATALOG ENTRY BASE RECORD.           * 01095000
**      SMF RECORD FIELDS:                                            * 01096000
**         SMF63NSZ - APPROXIMATION OF THE TOTAL NUMBER OF RECORDS    * 01097000
**                    IN THE CATALOG ENTRY.                           * 01098000
**                                                                    * 01099000
**   OUTPUT =                                                         * 01100000
**      IGGCCA OUTPUT:                                                * 01101000
**         CCAURAB,CCARABB - USED TO KEEP TRACK OF CATALOG ENTRY      * 01102000
**                           RECORDS.                                 * 01103000
**         CCA RECORD AREAS 1,2,AND 3 VOLATILE                        * 01104000
**         CCACD1 - RCS, NO ERROR.                                    * 01105000
**                  ANY ERROR CODE RETURNED BY GET CI.                * 01106000
**      SMF RECORD FIELDS:                                            * 01107000
**         SMF63SEG - SIZE  OF RECORDS IN ENTIRE CATALOG ENTRY.       * 01108000
**         SMF63DTY - DATA SET TYPE FIELD INITIALIZED NEW CATALOG     * 01109000
**                    ENTRY FIELD INITIALIZED.                        * 01110000
**                                                                    * 01111000
**                                                                    * 01112000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01113000
*                                                                  0477 01114000
*   RFY                                                            0477 01115000
*     SMFRCD63 BASED(CCASMFRD);     /* MAP SMF RECORD AREA           */ 01116000
*   CCASMFEX=OFF;                   /* ENTER SEARCH LOOP             */ 01117000
         NI    CCASMFEX(R11),B'10111111'                           0478 01118000
*   SMF63SEG=0;                     /* SET THE INITIAL SIZE OF THE 0479 01119000
*                                      ENTIRE ENTRY          @Y30SSRS*/ 01120000
         L     @10,CCASMFRD(,R11)                                  0479 01121000
         SLR   @05,@05                                             0479 01122000
         STH   @05,SMF63SEG(,@10)                                  0479 01123000
*   CCARABB=ADDR(CCARAB1);          /* INITLZ BASE RAB ADDR          */ 01124000
         LA    @10,CCARAB1(,R11)                                   0480 01125000
         ST    @10,CCARABB(,R11)                                   0480 01126000
*   CCAURAB=ADDR(CCARAB1);          /* MAP RECORD AREA FOR BASE      */ 01127000
         ST    @10,CCAURAB(,R11)                                   0481 01128000
*Z039SMFP:                                                         0482 01129000
*   CALL IGGPSMFP;                  /* PUT BASE RCD INTO SMF AREA    */ 01130000
Z039SMFP BAL   @14,IGGPSMFP                                        0482 01131000
*   DO WHILE(CCASMFEX=OFF&CCACD1=RCS);/* PROCESS EXTENSION RECORDS   */ 01132000
         B     @DE00483                                            0483 01133000
@DL00483 DS    0H                                                  0484 01134000
*     RFY                                                          0484 01135000
*       CCARAB BASED(CCARABB);      /* MAP BASE RCD AREA             */ 01136000
*     CCACPE5=ADDR(REPGOPT);        /* POINT FIRST OCCURR PTR        */ 01137000
         L     @10,CCARABB(,R11)                                   0485 01138000
         L     @10,CCACPE2(,@10)                                   0485 01139000
         LA    @10,REPGOPT(,@10)                                   0485 01140000
         ST    @10,CCACPE5(,R11)                                   0485 01141000
*     DO WHILE(CCACPE5<CCACPE3&CCACD1=RCS);/* SCAN OCCURR PTRS       */ 01142000
         B     @DE00486                                            0486 01143000
@DL00486 DS    0H                                                  0487 01144000
*       IF CATGOEXT=ON&CATGODEL=OFF THEN/* IF EXTENSION POINTER      */ 01145000
         L     @10,CCACPE5(,R11)                                   0487 01146000
         TM    CATGOEXT(@10),B'10000000'                           0487 01147000
         BNO   @RF00487                                            0487 01148000
         TM    CATGODEL(@10),B'01000000'                           0487 01149000
         BNZ   @RF00487                                            0487 01150000
*         DO;                       /* RETRIEVE RECORD               */ 01151000
*           CCASRID=CATGOCI;        /* SET SEARCH ARGUMENT           */ 01152000
         MVC   CCASRID(3,R11),CATGOCI(@10)                         0489 01153000
*           CCAURAB=ADDR(CCARAB5);  /* SET RAB POINTER               */ 01154000
         LA    @10,CCARAB5(,R11)                                   0490 01155000
         ST    @10,CCAURAB(,R11)                                   0490 01156000
*           DO;                                                    0491 01157000
*             CCAF1KEY=OFF;         /* GET BY CI NO.                 */ 01158000
*             CCAF1KGE=OFF;         /* NOT GET NEXT                  */ 01159000
*             CCAF1UP=OFF;          /* NOT GET UPD                   */ 01160000
*                                                                  0494 01161000
         NI    CCAF1KEY(R11),B'11100101'                           0494 01162000
*             /*******************************************************/ 01163000
*             /*                                                     */ 01164000
*             /* CALL TO MODULE IGG0CLBI                             */ 01165000
*             /*                                                     */ 01166000
*             /*******************************************************/ 01167000
*                                                                  0495 01168000
*Z041GET:                                                          0495 01169000
*             CALL IGGPGET;         /* GET RECORD                    */ 01170000
Z041GET  L     @15,@CV00036                                        0495 01171000
         BALR  @14,@15                                             0495 01172000
*           END;                    /* READ RECORD                   */ 01173000
*           IF CCACD1^=RCS THEN                                    0497 01174000
         CLI   CCACD1(R11),0                                       0497 01175000
         BNE   @RT00497                                            0497 01176000
*             ;                     /* CHECK ERROR                   */ 01177000
*           ELSE                    /* NO ERROR                      */ 01178000
*             DO;                   /* PROCESS RECORD                */ 01179000
*Z042SMFP:                                                         0500 01180000
*               CALL IGGPSMFP;      /* PUT RECORD INTO SMF AREA      */ 01181000
Z042SMFP BAL   @14,IGGPSMFP                                        0500 01182000
*               RFY                                                0501 01183000
*                 CCARAB BASED(CCAURAB);/* ORIGINAL BASE             */ 01184000
*               DO WHILE(EXTGOCI^=0&CCACD1=RCS);/* PROCESS EXTENSIONS   01185000
*                                      OF EXTENSIONS                 */ 01186000
