         AL    @05,@CF04588                                        0657 01869000
         STH   @05,SMF67LEN(,@10)                                  0657 01870000
*           SMF67RTY=SMFSCR;        /* SET RECORD TYPE               */ 01871000
         MVI   SMF67RTY(@10),X'43'                                 0658 01872000
*           SMF67IND=CCASMFG1|SMF67IND;/* INDICATORS         @Y30SSRS*/ 01873000
