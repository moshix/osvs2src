*     GEN(SMFWTM (R2));             /* WRITE SMF                     */ 02619000
         SMFWTM (R2)                                                    02620000
*     IF CCASMFLN^=0 THEN           /* CHECK IF CORE GOTTEN          */ 02621000
         LH    @10,CCASMFLN(,R11)                                  0851 02622000
         LTR   @10,@10                                             0851 02623000
         BZ    @RF00851                                            0851 02624000
*       DO;                         /* FREE IT                       */ 02625000
*         CCAMNLEN=CCASMFLN;        /* CORE LENGTH TO BE FREED       */ 02626000
         STCM  @10,7,CCAMNLEN(R11)                                 0853 02627000
*         CCAMNADR=CCASMFRD;        /* CORE ADDRESS                  */ 02628000
         L     @10,CCASMFRD(,R11)                                  0854 02629000
