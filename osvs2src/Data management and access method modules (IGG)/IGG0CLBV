         CLC   RCDCINO(3,@10),@CF00059+1                           0554 01381000
         BH    @RF00554                                            0554 01382000
*     SMF63CAT=ON;                  /* INDICATE CATALOG              */ 01383000
         L     @10,CCASMFRD(,R11)                                  0555 01384000
         OI    SMF63CAT(@10),B'00010000'                           0555 01385000
*   ELSE                                                           0556 01386000
*     ;                             /* NOT CATALOG                   */ 01387000
@RF00554 DS    0H                                                  0557 01388000
*   RFY                                                            0557 01389000
*     CCARAB BASED(CCAURAB);        /* ORIGINAL BASE                 */ 01390000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 01391000
         BAL   @14,EXIT                                            0558 01392000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 01393000
         DS 0F                                                          01394000
*   GEN(DC CL8'IGGPSMFP');          /* NAME OF FOLLOWING PROCEDURE   */ 01395000
         DC CL8'IGGPSMFP'                                               01396000
*   END;                            /* END OF INNER PROCEDURE        */ 01397000
@EL00005 DS    0H                                                  0561 01398000
@EF00005 DS    0H                                                  0561 01399000
@ER00005 BR    @14                                                 0561 01400000
@PB00005 DS    0H                                                  0562 01401000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 01402000
         EJECT                                                          01403000
*                                                                  0563 01404000
*/********************************************************************/ 01405000
*/*                                                                  */ 01406000
*/*        IGGPSMFP  -  PROCEDURE  INTERNALLY  CALLED  NESTED        */ 01407000
*/*                                                                  */ 01408000
*/********************************************************************/ 01409000
*                                                                  0563 01410000
*IGGPSMFP:                                                         0563 01411000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 01412000
         B     @PB00006                                            0563 01413000
IGGPSMFP DS    0H                                                  0564 01414000
*Z049:                              /* LABEL FOR PROCEDURE TRACKING  */ 01415000
*Z049SMFP:                          /* LABEL FOR PROCEDURE TRACKING  */ 01416000
*   RFY                                                            0564 01417000
*    (R5,                                                          0564 01418000
*     R6,                                                          0564 01419000
*     R7,                                                          0564 01420000
*     R8,                                                          0564 01421000
*     R9,                                                          0564 01422000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 01423000
Z049     DS    0H                                                  0564 01424000
Z049SMFP DS    0H                                                  0565 01425000
*   RFY                                                            0565 01426000
*    (R2,                                                          0565 01427000
*     R3,                                                          0565 01428000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 01429000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 01430000
         STM 12,14,12(13)                                               01431000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA     0567 01432000
*                                      PROCEDURE                     */ 01433000
         LA 13,12(13)                                                   01434000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 01435000
**   MODULE NAME =  IGGPSMFP (IGG0CLBV)                               * 01436000
**                                                                    * 01437000
**   DESCRIPTIVE NAME =  PLACE CATALOG ENTRY RECORD INTO SMF RECORD.  * 01438000
**                                                                    * 01439000
**   FUNCTION =                                                       * 01440000
**      THIS SUB-FUNCTION IS PART OF THE ROUTINE IGGPSMFM, WHICH      * 01441000
**      PARTIALLY INITIALIZES SMF RECORDS 63 AND 67.  IT IS CALLED TO * 01442000
**      COPY RECORDS FROM A RECORD AREA BUFFER INTO THE               * 01443000
**      ACTUAL SMF RECORD NEW CATALOG ENTRY FIELD, AS LONG AS THERE   * 01444000
**      IS AVAILABLE SPACE. IT CALLS IGGPSMFZ TO ZERO ANY SECURITY    * 01445000
**      INFORMATION IN THE RECORD.                                    * 01446000
**                                                                    * 01447000
**   INPUT =                                                          * 01448000
**      IGGCCA INPUT:                                                 * 01449000
**         CCASMFRD - ADDRESS OF SMF RECORD 63 OR 67.                 * 01450000
**         CCASMFLN - DISPLACEMENT TO NEW CATALOG ENTRY FIELD.        * 01451000
**         CCAURAB  - RECORD AREA BLOCK ADDRESS OF CATALOG ENTRY      * 01452000
**                    RECORD.                                         * 01453000
**      SMF RECORD FIELDS:                                            * 01454000
**         SMF63SEG - CURRENT SIZE  OF RECORDS IN ENTIRE CATALOG ENTRY* 01455000
**         SMF63NSZ - APPROXIMATION OF SIZE  OF RECORDS IN ENTIRE     * 01456000
**                    CATALOG ENTRY.                                  * 01457000
**                                                                    * 01458000
**   OUTPUT =                                                         * 01459000
**      SMF RECORD FIELDS:                                            * 01460000
**         SMF63SEG - CURRENT SIZE  OF RECORDS IN ENTIRE CATALOG      * 01461000
**                    ENTRY INCREMENTED.                              * 01462000
**         NEW CATALOG ENTRY FIELD INITIALIZED WITH CATALOG ENTRY     * 01463000
**         RECORD (NOT DONE IF SMF63SEG EXCEEDS SMF63NSZ).            * 01464000
**                                                                    * 01465000
**                                                                    * 01466000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01467000
*                                                                  0568 01468000
*   RFY                                                            0568 01469000
*     SMFRCD63 BASED(CCASMFRD);     /* MAP SMF RECORD AREA           */ 01470000
*   RFY                                                            0569 01471000
*     RCDAREA BASED(CCACPE1);       /* MAP CATALOG RCD AREA          */ 01472000
*   RFY                                                            0570 01473000
*     SMFRCDAR BASED(CCASMFRD+CCASMFLN/* MAP SMF RECORD AREA         */ 01474000
*         +SMF63SEG);                                              0570 01475000
*                                                                  0570 01476000
*   /*****************************************************************/ 01477000
*   /*                                                               */ 01478000
*   /* COPY CATALOG ENTRY RECORD INTO SMF RECORD ONLY IF IT HAS NOT  */ 01479000
*   /* BEEN PREVIOUSLY COPIED                                        */ 01480000
*   /*                                                               */ 01481000
*   /*****************************************************************/ 01482000
*                                                                  0571 01483000
*   R2=CCASMFRD+CCASMFLN;           /* POINT TO FIRST RCD            */ 01484000
         LH    R2,CCASMFLN(,R11)                                   0571 01485000
         AL    R2,CCASMFRD(,R11)                                   0571 01486000
*   DO WHILE(R2^=0&R2<CCASMFRD+CCASMFLN+SMF63SEG&SMF63SEG<=SMF63NSZ);   01487000
         B     @DE00572                                            0572 01488000
@DL00572 DS    0H                                                  0573 01489000
*                                   /* SEARCH FOR DUPLICATE RCD IN 0573 01490000
*                                      SMF RCD                       */ 01491000
*     IF R2->RCDCINO=RCDCINO THEN   /* IF EQUAL CI NUMBERS           */ 01492000
         L     @10,CCAURAB(,R11)                                   0573 01493000
         L     @10,CCACPE1(,@10)                                   0573 01494000
         CLC   RCDCINO(3,R2),RCDCINO(@10)                          0573 01495000
         BNE   @RF00573                                            0573 01496000
*       R2=0;                       /* INDICATE DUPLICATE RCD        */ 01497000
         SLR   R2,R2                                               0574 01498000
*     ELSE                          /* NOT DUPLICATE RCD             */ 01499000
*       R2=R2+(R2->RCDSIZ);         /* INCR TO NEXT RECORD           */ 01500000
         B     @RC00573                                            0575 01501000
@RF00573 ICM   @10,12,RCDSIZ(R2)                                   0575 01502000
         SRA   @10,16                                              0575 01503000
         ALR   R2,@10                                              0575 01504000
*   END;                            /* END SEARCH                    */ 01505000
@RC00573 DS    0H                                                  0576 01506000
@DE00572 LTR   R2,R2                                               0576 01507000
         BZ    @DC00572                                            0576 01508000
         L     @10,CCASMFRD(,R11)                                  0576 01509000
         LH    @05,CCASMFLN(,R11)                                  0576 01510000
         ALR   @05,@10                                             0576 01511000
         LH    @15,SMF63SEG(,@10)                                  0576 01512000
         ALR   @05,@15                                             0576 01513000
         CR    R2,@05                                              0576 01514000
         BNL   @DC00572                                            0576 01515000
         CH    @15,SMF63NSZ(,@10)                                  0576 01516000
         BNH   @DL00572                                            0576 01517000
@DC00572 DS    0H                                                  0577 01518000
*   IF R2^=0 THEN                   /* IF RCD NOT DUPLICATE          */ 01519000
         LTR   R2,R2                                               0577 01520000
         BZ    @RF00577                                            0577 01521000
*     DO;                           /* PLACE RCD INTO SMF RCD        */ 01522000
*       IF SMF63SEG+RCDSIZ<=SMF63NSZ THEN/* IF RECORD FITS           */ 01523000
         L     @10,CCASMFRD(,R11)                                  0579 01524000
         LH    @05,SMF63SEG(,@10)                                  0579 01525000
         L     @01,CCAURAB(,R11)                                   0579 01526000
         L     @15,CCACPE1(,@01)                                   0579 01527000
         ICM   @14,12,RCDSIZ(@15)                                  0579 01528000
         SRA   @14,16                                              0579 01529000
         LR    @09,@05                                             0579 01530000
         ALR   @09,@14                                             0579 01531000
         CH    @09,SMF63NSZ(,@10)                                  0579 01532000
         BH    @RF00579                                            0579 01533000
*         DO;                       /* PUT RECORD INTO SMF AREA      */ 01534000
*           IF RCDSIZ>MXM THEN      /* IF TWO MOVES REQUIRED         */ 01535000
         C     @14,@CF00078                                        0581 01536000
         BNH   @RF00581                                            0581 01537000
*             DO;                   /* MOVE TWICE                    */ 01538000
*               SMFRCDAR(1:MXM)=RCDAREA(1:MXM);/* FIRST HALF RCD COPY*/ 01539000
         AH    @10,CCASMFLN(,R11)                                  0583 01540000
         ALR   @10,@05                                             0583 01541000
         MVC   SMFRCDAR(256,@10),RCDAREA(@15)                      0583 01542000
*               SMFRCDAR(MXM+1:RCDSIZ)=RCDAREA(MXM+1:RCDSIZ);/*    0584 01543000
*                                      SECOND HALF RCD COPY          */ 01544000
         SL    @14,@CF04362                                        0584 01545000
         EX    @14,@SM04608                                        0584 01546000
*             END;                  /* END MOVE TWICE                */ 01547000
*           ELSE                    /* ONE MOVE                      */ 01548000
*             SMFRCDAR(1:RCDSIZ)=RCDAREA(1:RCDSIZ);/* MOVE ONCE      */ 01549000
         B     @RC00581                                            0586 01550000
@RF00581 L     @10,CCASMFRD(,R11)                                  0586 01551000
         LH    @05,CCASMFLN(,R11)                                  0586 01552000
         ALR   @05,@10                                             0586 01553000
         AH    @05,SMF63SEG(,@10)                                  0586 01554000
         L     @10,CCAURAB(,R11)                                   0586 01555000
         L     @10,CCACPE1(,@10)                                   0586 01556000
         ICM   @15,12,RCDSIZ(@10)                                  0586 01557000
