*       CCASMFRD=CCAMNADR;          /* POINT TO SMF RECORD           */ 00561000
         ST    @10,CCASMFRD(,R11)                                  0346 00562000
*     END;                          /* END FORMAT                    */ 00563000
*   RFY                                                            0348 00564000
*     CATRCD BASED(CCACPE1);        /* ORIGINAL BASE                 */ 00565000
@RT00302 DS    0H                                                  0349 00566000
*   RFY                                                            0349 00567000
*     REPCNTRL BASED(CCACPE2);      /* ORIGINAL BASE                 */ 00568000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00569000
         BAL   @14,EXIT                                            0350 00570000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00571000
         DS 0F                                                          00572000
*   GEN(DC CL8'IGGPSMFE');          /* NAME OF FOLLOWING PROCEDURE   */ 00573000
         DC CL8'IGGPSMFE'                                               00574000
*   END;                            /* END OF INNER PROCEDURE        */ 00575000
@EL00003 DS    0H                                                  0353 00576000
@EF00003 DS    0H                                                  0353 00577000
@ER00003 BR    @14                                                 0353 00578000
@PB00003 DS    0H                                                  0354 00579000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 00580000
         EJECT                                                          00581000
*                                                                  0355 00582000
*/********************************************************************/ 00583000
*/*                                                                  */ 00584000
*/*        IGGPSMFE  -  PROCEDURE  INTERNALLY  CALLED  NESTED        */ 00585000
*/*                                                                  */ 00586000
*/********************************************************************/ 00587000
*                                                                  0355 00588000
*IGGPSMFE:                                                         0355 00589000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 00590000
         B     @PB00004                                            0355 00591000
IGGPSMFE DS    0H                                                  0356 00592000
*Z021:                              /* LABEL FOR PROCEDURE TRACKING  */ 00593000
*Z021SMFE:                          /* LABEL FOR PROCEDURE TRACKING  */ 00594000
*   RFY                                                            0356 00595000
*    (R5,                                                          0356 00596000
*     R6,                                                          0356 00597000
*     R7,                                                          0356 00598000
*     R8,                                                          0356 00599000
*     R9,                                                          0356 00600000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00601000
Z021     DS    0H                                                  0356 00602000
Z021SMFE DS    0H                                                  0357 00603000
*   RFY                                                            0357 00604000
*    (R2,                                                          0357 00605000
*     R3,                                                          0357 00606000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00607000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00608000
         STM 12,14,12(13)                                               00609000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA     0359 00610000
*                                      PROCEDURE                     */ 00611000
         LA 13,12(13)                                                   00612000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00613000
**   MODULE NAME =  IGGPSMFE (IGG0CLBV)                               * 00614000
**                                                                    * 00615000
**   DESCRIPTIVE NAME =                                               * 00616000
**      OBTAIN AND PARTIALLY FORMAT SMF RECORD 63 AND 67              * 00617000
**                                                                    * 00618000
**   FUNCTION =                                                       * 00619000
**      THIS SUB-FUNCTION IS CALLED BY IGGPSMFS AND IGGPSMFA.  IT     * 00620000
**      OBTAINS THE CORE REQUIRED FOR SMF RECORDS 63 AND 67 (USING    * 00621000
**      CORE APPOXIMATIONS) AND FILLS IN THE (NEW OR OLD) CATALOG     * 00622000
**      ENTRY FIELD AND THE DATA SET TYPE FIELD IN THESE TWO RECORDS. * 00623000
**      THE PROCESSING IS AS FOLLOWS:                                 * 00624000
**      1. APPROXIMATE SMF RECORD SIZE BY SCANNING BASE RECORD FOR    * 00625000
**         EXTENSION RECORDS.                                         * 00626000
**      2. OBTAIN THE APPROXIMATE AMOUNT OF CORE REQUIRED AND CALL    * 00627000
**         IGGPSMFM TO INITIALIZE THE NEW CATALOG ENTRY FIELD AND THE * 00628000
**         DATA SET TYPE FIELD.                                       * 00629000
**      3. IF THE FIRST APPROXIMATION OF THE SIZE  OF RECORDS IN THE  * 00630000
**         ENTIRE ENTRY (USED TO GET SMF RECORD CORE) IS BELOW THE    * 00631000
**         ACTUAL SIZE, FREE THE OLD SMF RECORD CORE. OBTAIN THE      * 00632000
**         CORRECT AMOUNT AND CALL IGGPSMFM AGAIN.                    * 00633000
**                                                                    * 00634000
**   INPUT =                                                          * 00635000
**      IGGCCA INPUT:                                                 * 00636000
**         CCASMFCT - COUNT OF PRESERVED RECORDS WHICH WERE ALTERED.  * 00637000
**                    MUST BE INCLUDED IN CORE APPROXIMATION.         * 00638000
**         CCA RECORD AREA 1 HAS CATALOG ENTRY BASE RECORD.           * 00639000
**         CCASMFLN - DISPALCEMENT TO NEW CATALOG ENTRY FIELD IN SMF  * 00640000
**                    RECORD 63 OR 67.                                * 00641000
**                                                                    * 00642000
**   OUTPUT =                                                         * 00643000
**      IGGCCA OUTPUT:                                                * 00644000
**         CCASMFRD - ADDRESS OF CORE FOR SMF RECORD 63 OR 67.        * 00645000
**         CCASMFLN - LENGTH OF CORE GOTTEN FOR SMF RECORD.           * 00646000
**         CCACD1 - RCS, NO ERROR                                     * 00647000
**                - ANY ERROR CODE RETURNED BY GET CI.                * 00648000
**                - RCINSSWA, MAIN STORAGE CORE UNAVAILABLE.          * 00649000
**         CCA RECORD AREAS 1,2,AND 3 VOLATILE.                       * 00650000
**      SMF RECORD FIELDS:                                            * 00651000
**         SMF NEW CATALOG ENTRY FIELD INITIALIZED.                   * 00652000
**         SMF63DTY - DATA SET TYPE FIELD INITIALIZED.                * 00653000
**      R4 - SIZE  OF RECORDS IN ENTIRE CATALOG ENTRY.                * 00654000
**                                                                    * 00655000
**                                                                    * 00656000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00657000
*                                                                  0360 00658000
*   /*****************************************************************/ 00659000
*   /*                                                               */ 00660000
*   /* APPROXIMATE AMOUNT OF CORE REQUIRED FOR SMF RECORD            */ 00661000
*   /*                                                               */ 00662000
*   /*****************************************************************/ 00663000
*                                                                  0360 00664000
*   RFY                                                            0360 00665000
*     CCARAB BASED(ADDR(CCARAB1));  /* MAP BASE RECORD AREA          */ 00666000
*   R4=-1;                          /* COUNT OF EXTENSION RCDS     0361 00667000
*                                      DISCOUNT SPACE POINTER        */ 00668000
         SLR   R4,R4                                               0361 00669000
         BCTR  R4,0                                                0361 00670000
*   R2=ADDR(REPGOPT);               /* POINT TO FIRST GOP            */ 00671000
         L     R2,CCACPE2+108(,R11)                                0362 00672000
         LA    R2,REPGOPT(,R2)                                     0362 00673000
*   DO WHILE(R2<CCACPE3);           /* SCAN OCCURRENCE POINTERS      */ 00674000
         B     @DE00363                                            0363 00675000
@DL00363 DS    0H                                                  0364 00676000
*     RFY                                                          0364 00677000
*       CATGOPT BASED(R2);          /* REGISTER BASE                 */ 00678000
*     IF CATGOEXT=ON THEN           /* IF EXTENSION POINTER          */ 00679000
         TM    CATGOEXT(R2),B'10000000'                            0365 00680000
         BNO   @RF00365                                            0365 00681000
*       R4=R4+1;                    /* INCR COUNT OF SUCH PTRS       */ 00682000
         AL    R4,@CF00040                                         0366 00683000
*     ELSE                                                         0367 00684000
*       ;                           /* NOT EXTENSION POINTER         */ 00685000
@RF00365 DS    0H                                                  0368 00686000
*     R2=R2+LENGTH(CATGOPT);        /* NEXT GROUP OCCURR PTR         */ 00687000
         AL    R2,@CF00256                                         0368 00688000
*     RFY                                                          0369 00689000
*       CATGOPT BASED(CCACPE5);     /* OLD BASE                      */ 00690000
*   END;                            /* END SCAN                      */ 00691000
@DE00363 C     R2,CCACPE3+108(,R11)                                0370 00692000
         BL    @DL00363                                            0370 00693000
*   IF R4^=0 THEN                   /* IF EXTENSION POINTERS         */ 00694000
         LTR   R4,R4                                               0371 00695000
         BZ    @RF00371                                            0371 00696000
*     R4=R4/COREFC3+COREFC1;        /* ADD HEDGE FACTOR      @Y30SSRS*/ 00697000
         LR    @10,R4                                              0372 00698000
         SRL   @10,2                                               0372 00699000
         AL    @10,@CF00040                                        0372 00700000
         LR    R4,@10                                              0372 00701000
*   ELSE                                                           0373 00702000
*     ;                             /* NO EXTENSION POINTERS         */ 00703000
@RF00371 DS    0H                                                  0374 00704000
*   R4=R4+1;                        /* ACCOUNT FOR BASE RECORD       */ 00705000
         AL    R4,@CF00040                                         0374 00706000
*   IF EXTGOCI^=0 THEN              /* IF EXTENSION OF BASE          */ 00707000
         L     @10,CCACPE2+108(,R11)                               0375 00708000
         ICM   @05,7,EXTGOCI(@10)                                  0375 00709000
         BZ    @RF00375                                            0375 00710000
*     R4=R4+COREFC2;                /* ADD APPROXIMATION FACTOR      */ 00711000
         AL    R4,@CF00059                                         0376 00712000
*   ELSE                                                           0377 00713000
*     ;                             /* NO EXTENSION OF BASE          */ 00714000
@RF00375 DS    0H                                                  0378 00715000
*   R3=R4+CCASMFCT;                 /* ACCOUNT FOR ANY ALTERED     0378 00716000
*                                      RECORDS                       */ 00717000
         LR    R3,R4                                               0378 00718000
         AH    R3,CCASMFCT(,R11)                                   0378 00719000
*   CCAMNLEN=R3*CATRCDLN+CCASMFLN;  /* GET TOTAL SMF RECORD CORE   0379 00720000
*                                      REQUIRED                      */ 00721000
         LR    @10,R3                                              0379 00722000
         SLA   @10,9                                               0379 00723000
         AH    @10,CCASMFLN(,R11)                                  0379 00724000
         STCM  @10,7,CCAMNLEN(R11)                                 0379 00725000
*   CCASMFRD=0;                     /* INDICATE NO CORE TO BE FREED  */ 00726000
         SLR   @10,@10                                             0380 00727000
         ST    @10,CCASMFRD(,R11)                                  0380 00728000
*   RFY                                                            0381 00729000
*     R1 RSTD;                      /* USED IN GETMAIN               */ 00730000
*   R1=ADDR(CCAMSSPL);              /* POINT TO STRG MGMT P.L.       */ 00731000
         LA    R1,CCAMSSPL(,R11)                                   0382 00732000
*   CCAMNSPL=USUBPOOL;              /* STORE SUBPOOL VALUE           */ 00733000
*                                                                  0383 00734000
         MVI   CCAMNSPL(R11),X'FC'                                 0383 00735000
*   /*****************************************************************/ 00736000
*   /*                                                               */ 00737000
*   /* ALLOCATE MAIN STORAGE FROM USER SUBPOOL 252                   */ 00738000
*   /*                                                               */ 00739000
*   /*****************************************************************/ 00740000
*                                                                  0384 00741000
*   GEN(GETMAIN LC,MF=(E,(1)));     /* GET MAIN STORAGE SPACE        */ 00742000
         GETMAIN LC,MF=(E,(1))                                          00743000
*   GEN(LR R1,R15);                 /* PICK UP RETURN CODE           */ 00744000
         LR R1,R15                                                      00745000
*   IF R1^=NOGETMSE THEN            /* CHECK FOR ERROR RETURN        */ 00746000
*                                                                  0386 00747000
         LTR   R1,R1                                               0386 00748000
         BZ    @RF00386                                            0386 00749000
*     /***************************************************************/ 00750000
*     /*                                                             */ 00751000
*     /* SET CCAMODID='BV', CCAREASN=RSN2, CCARETRN=RCINSSWA         */ 00752000
*     /*                                                             */ 00753000
*     /***************************************************************/ 00754000
*                                                                  0387 00755000
*     CCAPROB=IGGSEGID+(RSN2*K256)+RCINSSWA;/* INSUFFICIENT CORE   0387 00756000
*                                      ERROR                         */ 00757000
         MVC   CCAPROB(4,R11),@CF04587                             0387 00758000
*   ELSE                            /* GETMAIN SUCCESSFUL            */ 00759000
*     DO;                                                          0388 00760000
         B     @RC00386                                            0388 00761000
@RF00386 DS    0H                                                  0389 00762000
*       DO;                                                        0389 00763000
*         IF MNATFULL=ON THEN       /* TRACKING TABLE FULL           */ 00764000
*                                                                  0390 00765000
         TM    MNATFULL(R11),B'10000000'                           0390 00766000
         BNO   @RF00390                                            0390 00767000
*           /*********************************************************/ 00768000
*           /*                                                       */ 00769000
*           /* CALL TO MODULE IGG0CLAT                               */ 00770000
*           /*                                                       */ 00771000
*           /*********************************************************/ 00772000
*                                                                  0391 00773000
*Z025ARFS:                                                         0391 00774000
*           CALL IGGPARFS;          /* YES,CALL SPILL ROUTINE        */ 00775000
Z025ARFS L     @15,@CV00037                                        0391 00776000
         BALR  @14,@15                                             0391 00777000
*         GEN REFS(CCAMNAT)(MVC CCAMNAT(248,11),CCAMNAT+8(11));/*  0392 00778000
*                                      PUSH AND TRACK                */ 00779000
@RF00390 DS    0H                                                  0392 00780000
         MVC CCAMNAT(248,11),CCAMNAT+8(11)                              00781000
*       END;                                                       0393 00782000
*     END;                          /* GET MAIN CORE STORAGE         */ 00783000
*   RFY                                                            0395 00784000
*     R1 UNRSTD;                    /* FREE REG                      */ 00785000
@RC00386 DS    0H                                                  0396 00786000
*   IF CCACD1^=RCS THEN                                            0396 00787000
         CLI   CCACD1(R11),0                                       0396 00788000
         BNE   @RT00396                                            0396 00789000
*     ;                             /* CHECK ERROR                   */ 00790000
*   ELSE                            /* NO ERROR                      */ 00791000
*     DO;                           /* FORMAT RECORD                 */ 00792000
*       CCASMFRD=CCAMNADR;          /* SAVE SMF RECORD ADDR          */ 00793000
         L     @10,CCAMNADR(,R11)                                  0399 00794000
         ST    @10,CCASMFRD(,R11)                                  0399 00795000
