         AL    @10,CCACPE5(,R11)                                   0514 01227000
         ST    @10,CCACPE5(,R11)                                   0514 01228000
*     END;                          /* END SCAN OCCURR PTRS          */ 01229000
@DE00486 L     @10,CCARABB(,R11)                                   0515 01230000
