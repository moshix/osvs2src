         TITLE 'IGG0CLBV, WRITES CATALOG SMF RECORDS                   *00001000
                        '                                               00002000
IGG0CLBV CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGG0CLBV  76.012'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
         USING @MAINENT,@15                                             00011000
         USING @DATD-(CCATEMPS-IGGCCA),R11                              00012000
         DS 0F                                                          00013000
         DC CL8'IGGPSMFA'                                               00014000
         EJECT                                                          00015000
         B     @PB00002                                            0200 00016000
IGGPSMFA DS    0H                                                  0201 00017000
         ENTRY IGGPSMFA                                                 00018000
         DROP @15                                                       00019000
Z003     DS    0H                                                  0203 00020000
Z003SMFA DS    0H                                                  0204 00021000
         STM 12,14,12(13)                                               00022000
         LA 13,12(13)                                                   00023000
         BALR 12,0                                                      00024000
IGGSEGST DS 0H                                                          00025000
         USING *,12                                                     00026000
*   IF CVTSMCA=0|SMCADSA=OFF|       /* IF NO SMF OR NO DATA SET      */ 00027000
*       SMCAUSER=OFF THEN                                          0211 00028000
         L     @10,CVTPTR                                          0211 00029000
         L     @10,CVTSMCA(,@10)                                   0211 00030000
         LTR   @10,@10                                             0211 00031000
         BZ    @RT00211                                            0211 00032000
         TM    SMCADSA(@10),B'00010000'                            0211 00033000
         BZ    @RT00211                                            0211 00034000
         TM    SMCAUSER(@10),B'10000000'                           0211 00035000
         BZ    @RT00211                                            0211 00036000
*     ;                             /* ACCOUNTING OR NO SMF DATA DONT   00037000
*                                      PROCESS SMF                   */ 00038000
*   ELSE                            /* WRITE SMF RECORD 63           */ 00039000
*     IF CCACD1^=RCS|CCASMFCD^=RCS THEN/* IF IN ERROR MODE           */ 00040000
         CLI   CCACD1(R11),0                                       0213 00041000
         BNE   @RT00213                                            0213 00042000
         CLI   CCASMFCD(R11),0                                     0213 00043000
         BE    @RF00213                                            0213 00044000
@RT00213 DS    0H                                                  0214 00045000
*Z004SMFF:                                                         0214 00046000
*       CALL IGGPSMFF;              /* FREE CORE                     */ 00047000
Z004SMFF BAL   @14,IGGPSMFF                                        0214 00048000
*     ELSE                          /* NOT IN ERROR MODE             */ 00049000
*       DO;                         /* SMF WRITE                     */ 00050000
         B     @RC00213                                            0215 00051000
@RF00213 DS    0H                                                  0216 00052000
*         CCASMFLN=LENGTH(SMFRCD63);/* LENGTH OF FIXED INFO IN THIS     00053000
*                                      RECORD                        */ 00054000
         MVC   CCASMFLN(2,R11),@CH00369                            0216 00055000
*         IF CCASMFDF=ON THEN       /* IF DEFINE CALLER              */ 00056000
         TM    CCASMFDF(R11),B'10000000'                           0217 00057000
         BNO   @RF00217                                            0217 00058000
*           DO;                     /* SET UP FOR DEFINE             */ 00059000
*             CCASMFPT=0;           /* NO SMF CHAIN                  */ 00060000
         SLR   @10,@10                                             0219 00061000
         ST    @10,CCASMFPT(,R11)                                  0219 00062000
*             CCASMFCT=0;           /* NO SMF CHAIN COUNT            */ 00063000
         STH   @10,CCASMFCT(,R11)                                  0220 00064000
*           END;                    /* END SET UP                    */ 00065000
*         ELSE                                                     0222 00066000
*           ;                       /* ALTER CALLER                  */ 00067000
@RF00217 DS    0H                                                  0223 00068000
*         IF CCASMFRD^=0&CCASMFDF=ON THEN/* IF THIS IS DEFINE      0223 00069000
*                                      CATALOG, SPECIAL PROCESS      */ 00070000
         L     @10,CCASMFRD(,R11)                                  0223 00071000
         LTR   @10,@10                                             0223 00072000
         BZ    @RF00223                                            0223 00073000
         TM    CCASMFDF(R11),B'10000000'                           0223 00074000
         BNO   @RF00223                                            0223 00075000
*Z005SMFD:                                                         0224 00076000
*           CALL IGGPSMFD;          /* GET CORE AND SET CATALOG ENTRY   00077000
*                                      FIELD                         */ 00078000
Z005SMFD BAL   @14,IGGPSMFD                                        0224 00079000
*         ELSE                      /* NOT DEFINE CATALOG            */ 00080000
*           IF CCASMFBR=ON THEN     /* IF BASE NOT IN CORE           */ 00081000
         B     @RC00223                                            0225 00082000
@RF00223 TM    CCASMFBR(R11),B'00010000'                           0225 00083000
         BNO   @RF00225                                            0225 00084000
*             DO;                   /* READ BASE RECORD              */ 00085000
*               CCAURAB=ADDR(CCARAB1);/* SET UP RAB                  */ 00086000
         LA    @10,CCARAB1(,R11)                                   0227 00087000
         ST    @10,CCAURAB(,R11)                                   0227 00088000
*               DO;                                                0228 00089000
*                 CCAF1KEY=OFF;     /* GET BY CI NO.                 */ 00090000
*                 CCAF1KGE=OFF;     /* NOT GET NEXT                  */ 00091000
*                 CCAF1UP=OFF;      /* NOT GET UPD                   */ 00092000
*                                                                  0231 00093000
         NI    CCAF1KEY(R11),B'11100101'                           0231 00094000
*                 /***************************************************/ 00095000
*                 /*                                                 */ 00096000
*                 /* CALL TO MODULE IGG0CLBI                         */ 00097000
*                 /*                                                 */ 00098000
*                 /***************************************************/ 00099000
*                                                                  0232 00100000
*Z007GET:                                                          0232 00101000
*                 CALL IGGPGET;     /* GET RECORD                    */ 00102000
Z007GET  L     @15,@CV00036                                        0232 00103000
         BALR  @14,@15                                             0232 00104000
*               END;                /* READ RECORD                   */ 00105000
*               IF CCACD1^=RCS THEN                                0234 00106000
         CLI   CCACD1(R11),0                                       0234 00107000
         BNE   @RT00234                                            0234 00108000
*                 ;                 /* CHECK FOR ERROR               */ 00109000
*               ELSE                /* NO ERROR                      */ 00110000
*Z008SMFE:                                                         0236 00111000
*                 CALL IGGPSMFE;    /* GET CORE FOR RECORD AND INSERT   00112000
*                                      NEW ENTRY                     */ 00113000
Z008SMFE BAL   @14,IGGPSMFE                                        0236 00114000
*             END;                  /* END READ                      */ 00115000
*           ELSE                    /* BASE IN-CORE                  */ 00116000
*Z009SMFE:                                                         0238 00117000
*             CALL IGGPSMFE;        /* GET CORE AND ENTRY RCD        */ 00118000
         B     @RC00225                                            0238 00119000
@RF00225 DS    0H                                                  0238 00120000
Z009SMFE BAL   @14,IGGPSMFE                                        0238 00121000
*         IF CCACD1^=RCS THEN       /* CHECK ERROR                   */ 00122000
@RC00225 DS    0H                                                  0239 00123000
@RC00223 CLI   CCACD1(R11),0                                       0239 00124000
         BE    @RF00239                                            0239 00125000
*Z010SMFF:                                                         0240 00126000
*           CALL IGGPSMFF;          /* FREE SMF CHAIN CORE           */ 00127000
Z010SMFF BAL   @14,IGGPSMFF                                        0240 00128000
*         ELSE                      /* NO ERROR                      */ 00129000
*           DO;                     /* FORMAT ENTRY                  */ 00130000
         B     @RC00239                                            0241 00131000
@RF00239 DS    0H                                                  0242 00132000
*             SMF63RTY=SMFALT;      /* DEFINE/ALTER RECORD TYPE      */ 00133000
         L     @10,CCASMFRD(,R11)                                  0242 00134000
         MVI   SMF63RTY(@10),X'3F'                                 0242 00135000
*             SMF63IND=CCASMFG1|SMF63IND;/* RECORDING IND    @Y30SSRS*/ 00136000
         OC    SMF63IND(1,@10),CCASMFG1(R11)                       0243 00137000
*             SMF63NSZ=R4;          /* NEW CATALOG ENTRY SIZE        */ 00138000
         STH   R4,SMF63NSZ(,@10)                                   0244 00139000
*             SMF63OSZ=0;           /* OLD CATALOG ENTRY SIZE        */ 00140000
         SLR   @05,@05                                             0245 00141000
         STH   @05,SMF63OSZ(,@10)                                  0245 00142000
*             SMF63CNM=CAXCNAM;     /* CATALOG NAME                  */ 00143000
         L     @05,CCAACB(,R11)                                    0246 00144000
         L     @05,ACBUAPTR(,@05)                                  0246 00145000
         MVC   SMF63CNM(44,@10),CAXCNAM(@05)                       0246 00146000
*             RFY                                                  0247 00147000
*               CATRCD BASED(ADDR(SMF63NCE));/* MAP ENTRY            */ 00148000
*             SMF63DNM=DSNAME;      /* SET DATA SET NAME             */ 00149000
         MVC   SMF63DNM(44,@10),DSNAME+185(@10)                    0248 00150000
*             RFY                                                  0249 00151000
*               CATRCD BASED(CCACPE1);/* OLD BASE                    */ 00152000
*             IF CCASMFAL=ON THEN   /* IF ALTER CALLER               */ 00153000
         TM    CCASMFAL(R11),B'01000000'                           0250 00154000
         BNO   @RF00250                                            0250 00155000
*               DO;                 /* SET OLD ALTERED RCDS INTO SMF    00156000
*                                      RECORD                        */ 00157000
*                 RFY                                              0252 00158000
*                   RCDAREA BASED(R3);/* MAP CHAIN ELEMENT           */ 00159000
*                 RFY                                              0253 00160000
*                   SMFRCDAR BASED(CCASMFRD+LENGTH(SMFRCD63)/* MAP   */ 00161000
*                       +SMF63NSZ+SMF63OSZ);/* SMF RECORD            */ 00162000
*                 R3=CCASMFPT;      /* FIRST ELEMENT                 */ 00163000
         L     R3,CCASMFPT(,R11)                                   0254 00164000
*                 DO WHILE(R3^=0);  /* SEARCH CHAIN FOR OLD          */ 00165000
*                                                                  0255 00166000
         B     @DE00255                                            0255 00167000
@DL00255 DS    0H                                                  0256 00168000
*                   /*************************************************/ 00169000
*                   /*                                               */ 00170000
*                   /* ALTERED RECORDS                               */ 00171000
*                   /*                                               */ 00172000
*                   /*************************************************/ 00173000
*                                                                  0256 00174000
*                   IF SMFPRES=ON THEN/* IF RECORD TO BE SAVED       */ 00175000
         TM    SMFPRES(R3),B'10000000'                             0256 00176000
         BNO   @RF00256                                            0256 00177000
*                     DO;           /* SAVE IT                       */ 00178000
*                       SMFRCDAR(1:256)=RCDAREA(1:256);/* SAVE RECORD*/ 00179000
         L     @10,CCASMFRD(,R11)                                  0258 00180000
         LA    @05,136                                             0258 00181000
         ALR   @05,@10                                             0258 00182000
         AH    @05,SMF63NSZ(,@10)                                  0258 00183000
         LH    @15,SMF63OSZ(,@10)                                  0258 00184000
         ALR   @05,@15                                             0258 00185000
         MVC   SMFRCDAR(256,@05),RCDAREA(R3)                       0258 00186000
*                       SMFRCDAR(257:512)=RCDAREA(257:512);/* SAVE 0259 00187000
*                                      RECORD                        */ 00188000
         MVC   SMFRCDAR+256(256,@05),RCDAREA+256(R3)               0259 00189000
*                       SMF63OSZ=SMF63OSZ+(R3->RCDSIZ);/* INCR OLD 0260 00190000
*                                      SIZE                          */ 00191000
         ICM   @05,12,RCDSIZ(R3)                                   0260 00192000
         SRA   @05,16                                              0260 00193000
         ALR   @15,@05                                             0260 00194000
         STH   @15,SMF63OSZ(,@10)                                  0260 00195000
*                     END;          /* END SAVE IT                   */ 00196000
*                   ELSE                                           0262 00197000
*                     ;             /* NOT RECORD TO BE SAVED        */ 00198000
@RF00256 DS    0H                                                  0263 00199000
*                   R3=SMFCHAIN;    /* NEXT ELEMENT                  */ 00200000
         ICM   R3,15,SMFCHAIN(R3)                                  0263 00201000
*                 END;              /* END SEARCH                    */ 00202000
@DE00255 LTR   R3,R3                                               0264 00203000
         BNZ   @DL00255                                            0264 00204000
*               END;                /* END SET                       */ 00205000
*             ELSE                                                 0266 00206000
*               ;                   /* DEFINE CALLER                 */ 00207000
@RF00250 DS    0H                                                  0267 00208000
*             SMF63LEN=LENGTH(SMFRCD63)+/* SET TOTAL SMF RECORD      */ 00209000
*                 SMF63OSZ+SMF63NSZ;/* LENGTH                        */ 00210000
         L     @10,CCASMFRD(,R11)                                  0267 00211000
         LA    @05,136                                             0267 00212000
         AH    @05,SMF63OSZ(,@10)                                  0267 00213000
         AH    @05,SMF63NSZ(,@10)                                  0267 00214000
         STH   @05,SMF63LEN(,@10)                                  0267 00215000
*Z011SMFC:                                                         0268 00216000
*             CALL IGGPSMFC;        /* CREATE COMMON SMF RECORD AND     00217000
*                                      WRITE IT                      */ 00218000
Z011SMFC BAL   @14,IGGPSMFC                                        0268 00219000
*Z012SMFF:                                                         0269 00220000
*             CALL IGGPSMFF;        /* FREE CORE                     */ 00221000
Z012SMFF BAL   @14,IGGPSMFF                                        0269 00222000
*           END;                    /* END FORMAT ENTRY              */ 00223000
*         IF CCACD1^=RCS THEN       /* IF ERROR              @Y30SSRS*/ 00224000
@RC00239 CLI   CCACD1(R11),0                                       0271 00225000
         BE    @RF00271                                            0271 00226000
*           CCASMFP=IGGSEGID+CCACD1*K256+RCSMFER;/* SET ERR  @Y30SSRS*/ 00227000
         SLR   @10,@10                                             0272 00228000
         IC    @10,CCACD1(,R11)                                    0272 00229000
         SLA   @10,8                                               0272 00230000
         AL    @10,@CF04584                                        0272 00231000
         ST    @10,CCASMFP(,R11)                                   0272 00232000
*         ELSE                      /* NO ERROR              @Y30SSRS*/ 00233000
*           CCASMFP=0;              /* ZERO ERR FIELD        @Y30SSRS*/ 00234000
*                                                                  0273 00235000
         B     @RC00271                                            0273 00236000
@RF00271 SLR   @10,@10                                             0273 00237000
         ST    @10,CCASMFP(,R11)                                   0273 00238000
*         /***********************************************************/ 00239000
*         /*                                                         */ 00240000
*         /* CLEAR CCAMODID, CCAREASN, AND CCARETRN                  */ 00241000
*         /*                                                         */ 00242000
*         /***********************************************************/ 00243000
*                                                                  0274 00244000
*         CCAPROB=IGNOREIT;         /* ZERO RETURN CODE      @Y30SSRS*/ 00245000
@RC00271 SLR   @10,@10                                             0274 00246000
         ST    @10,CCAPROB(,R11)                                   0274 00247000
*       END;                        /* END SMF WRITE                 */ 00248000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 00249000
@RC00213 DS    0H                                                  0276 00250000
@RT00211 BAL   @14,EXIT                                            0276 00251000
*   GEN(DS 0F);                     /* FULLWORD BOUNDARY ALIGNMENT   */ 00252000
         DS 0F                                                          00253000
*   GEN(DC CL8'IGGPSMFD');          /* NAME OF FOLLOWING PROCEDURE   */ 00254000
         DC CL8'IGGPSMFD'                                               00255000
*   END;                            /* END OF INNER PROCEDURE        */ 00256000
@EL00002 DS    0H                                                  0279 00257000
@EF00002 DS    0H                                                  0279 00258000
@ER00002 BR    @14                                                 0279 00259000
@PB00002 DS    0H                                                  0280 00260000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 00261000
         EJECT                                                          00262000
*                                                                  0281 00263000
*/********************************************************************/ 00264000
*/*                                                                  */ 00265000
*/*        IGGPSMFD  -  PROCEDURE  INTERNALLY  CALLED  NESTED        */ 00266000
*/*                                                                  */ 00267000
*/********************************************************************/ 00268000
*                                                                  0281 00269000
*IGGPSMFD:                                                         0281 00270000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/* SPECIFY PROCEDURE OPTIONS     */ 00271000
         B     @PB00003                                            0281 00272000
IGGPSMFD DS    0H                                                  0282 00273000
*Z014:                              /* LABEL FOR PROCEDURE TRACKING  */ 00274000
*Z014SMFD:                          /* LABEL FOR PROCEDURE TRACKING  */ 00275000
*   RFY                                                            0282 00276000
*    (R5,                                                          0282 00277000
*     R6,                                                          0282 00278000
*     R7,                                                          0282 00279000
*     R8,                                                          0282 00280000
*     R9,                                                          0282 00281000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 00282000
Z014     DS    0H                                                  0282 00283000
Z014SMFD DS    0H                                                  0283 00284000
*   RFY                                                            0283 00285000
*    (R2,                                                          0283 00286000
*     R3,                                                          0283 00287000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 00288000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 00289000
         STM 12,14,12(13)                                               00290000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA     0285 00291000
*                                      PROCEDURE                     */ 00292000
         LA 13,12(13)                                                   00293000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 00294000
**   MODULE NAME =  IGGPSMFD (IGG0CLBV)                               * 00295000
**                                                                    * 00296000
**   DESCRIPTIVE NAME =                                               * 00297000
**      OBTAIN AND PARTIALLY FORMAT SMF RECORD 63 FOR DEFINE CATALOG  * 00298000
**                                                                    * 00299000
**   FUNCTION =                                                       * 00300000
**      THIS SUB-FUNCTION IS CALLED BY IGGPSMFA.  IT OBTAINS THE CORE * 00301000
**      REQUIRED FOR SMF RECORD 63 IN THE CASE OF A DEFINE CATALOG    * 00302000
**      AND INITIALIZES THE NEW CATALOG ENTRY FIELD AND DATA SET TYPE * 00303000
**      FIELD IN THIS RECORD.  THE PROCESSING IS AS FOLLOWS:          * 00304000
**      1. MAP THE BASE CATALOG ENTRY RECORD POINTED TO BY CCASMFIX   * 00305000
**         AND SCAN THE GROUP OCCURRENCE POINTERS FOR EXTENSION       * 00306000
**         RECORDS. RETRIEVE THE EXTENSION RECORDS OF THE ENTRY BY    * 00307000
**         USING THE GROUP OCCURRENCE CI VALUE + 1 TO INDEX INTO THE  * 00308000
**         CATALOG RECORD LIST. CALL IGGPSMFZ TO ZERO SECURITY INFO.  * 00309000
**      2. SET THE DATA SET TYPE FIELD BY THE BASE ENTRY RECORD TYPE. * 00310000
**                                                                    * 00311000
**   INPUT =                                                          * 00312000
**      IGGCCA INPUT:                                                 * 00313000
**         CCASMFRD - ADDRESS OF CATALOG RECORD LIST, CONTAINING A    * 00314000
**                    POINTER TO EACH OF THE FIRST 10 CATALOG RECORDS * 00315000
**                    (BEFORE CLOSE FOR CREATE).                      * 00316000
**         CCASMFIX - INDEX INTO CATALOG RECORD LIST FOR BASE ENTRY   * 00317000
**                    RECORD (IE. DATA, INDEX, OR CLUSTER).           * 00318000
**                                                                    * 00319000
**   OUTPUT =                                                         * 00320000
**      IGGCCA OUTPUT:                                                * 00321000
**         CCASMFRD - ADDRESS OF SMF RECORD 63 FOR DEFINE CATALOG.    * 00322000
**         CCASMFLN - LENGTH OF CORE GOTTEN FOR SMF RECORD.           * 00323000
**         CCACD1 - RCS, NO ERROR.  RCINSSWA, MAINSTORAGE CORE        * 00324000
**                  UNAVAILABLE.                                      * 00325000
**      SMF RECORD FIELDS:                                            * 00326000
**         SMF63NCE - NEW CATALOG ENTRY FIELD INITIALIZED.            * 00327000
**         SMF63DTY - DATA SET TYPE FIELD INITIALIZED.                * 00328000
**      R4 - SIZE  OF RECORDS IN ENTIRE CATALOG ENTRY.                * 00329000
**                                                                    * 00330000
**                                                                    * 00331000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00332000
*                                                                  0286 00333000
*   CCAMNLEN=MXCTLN+LENGTH(SMFRCD63);/* CORE LENGTH REQUIRED         */ 00334000
         MVC   CCAMNLEN(3,R11),@CF04585+1                          0286 00335000
*   RFY                                                            0287 00336000
*     R1 RSTD;                      /* USED IN GETMAIN               */ 00337000
*   R1=ADDR(CCAMSSPL);              /* POINT TO STRG MGMT P.L.       */ 00338000
         LA    R1,CCAMSSPL(,R11)                                   0288 00339000
*   CCAMNSPL=USUBPOOL;              /* STORE SUBPOOL VALUE           */ 00340000
*                                                                  0289 00341000
         MVI   CCAMNSPL(R11),X'FC'                                 0289 00342000
*   /*****************************************************************/ 00343000
*   /*                                                               */ 00344000
*   /* ALLOCATE MAIN STORAGE FROM USER SUBPOOL 252                   */ 00345000
*   /*                                                               */ 00346000
*   /*****************************************************************/ 00347000
*                                                                  0290 00348000
*   GEN(GETMAIN LC,MF=(E,(1)));     /* GET MAIN STORAGE SPACE        */ 00349000
         GETMAIN LC,MF=(E,(1))                                          00350000
*   GEN(LR R1,R15);                 /* PICK UP RETURN CODE           */ 00351000
         LR R1,R15                                                      00352000
*   IF R1^=NOGETMSE THEN            /* CHECK FOR ERROR RETURN        */ 00353000
*                                                                  0292 00354000
         LTR   R1,R1                                               0292 00355000
         BZ    @RF00292                                            0292 00356000
*     /***************************************************************/ 00357000
*     /*                                                             */ 00358000
*     /* SET CCAMODID='BV', CCAREASN=RSN2, CCARETRN=RCINSSWA         */ 00359000
*     /*                                                             */ 00360000
*     /***************************************************************/ 00361000
*                                                                  0293 00362000
*     CCAPROB=IGGSEGID+(RSN2*K256)+RCINSSWA;/* INSUFFICIENT CORE   0293 00363000
*                                      ERROR                         */ 00364000
         MVC   CCAPROB(4,R11),@CF04587                             0293 00365000
*   ELSE                            /* GETMAIN SUCCESSFUL            */ 00366000
*     DO;                                                          0294 00367000
         B     @RC00292                                            0294 00368000
@RF00292 DS    0H                                                  0295 00369000
*       DO;                                                        0295 00370000
*         IF MNATFULL=ON THEN       /* TRACKING TABLE FULL           */ 00371000
*                                                                  0296 00372000
         TM    MNATFULL(R11),B'10000000'                           0296 00373000
         BNO   @RF00296                                            0296 00374000
*           /*********************************************************/ 00375000
*           /*                                                       */ 00376000
*           /* CALL TO MODULE IGG0CLAT                               */ 00377000
*           /*                                                       */ 00378000
*           /*********************************************************/ 00379000
*                                                                  0297 00380000
*Z018ARFS:                                                         0297 00381000
*           CALL IGGPARFS;          /* YES,CALL SPILL ROUTINE        */ 00382000
Z018ARFS L     @15,@CV00037                                        0297 00383000
         BALR  @14,@15                                             0297 00384000
*         GEN REFS(CCAMNAT)(MVC CCAMNAT(248,11),CCAMNAT+8(11));/*  0298 00385000
*                                      PUSH AND TRACK                */ 00386000
@RF00296 DS    0H                                                  0298 00387000
         MVC CCAMNAT(248,11),CCAMNAT+8(11)                              00388000
*       END;                                                       0299 00389000
*     END;                          /* GET MAIN STORAGE              */ 00390000
*   RFY                                                            0301 00391000
*     R1 UNRSTD;                    /* FREE REG                      */ 00392000
@RC00292 DS    0H                                                  0302 00393000
*   IF CCACD1^=RCS THEN                                            0302 00394000
         CLI   CCACD1(R11),0                                       0302 00395000
         BNE   @RT00302                                            0302 00396000
*     ;                             /* SEE IF ERROR                  */ 00397000
*   ELSE                            /* CORE GOTTEN                   */ 00398000
*     DO;                           /* FORMAT RECORD                 */ 00399000
*       RFY                                                        0305 00400000
*         SMFRCD63 BASED(CCAMNADR); /* MAP SMF RECORD                */ 00401000
*       CCASMFLN=CCAMNLEN;          /* SAVE FOR FREE CORE            */ 00402000
         L     @10,CCAMNLEN-1(,R11)                                0306 00403000
         LA    @10,0(,@10)                                         0306 00404000
         STH   @10,CCASMFLN(,R11)                                  0306 00405000
*       RFY                                                        0307 00406000
*         CATRCD BASED(DCATRCD(CCASMFIX));/* MAP BASE ENTRY          */ 00407000
*       R2=ADDR(CATRCD)+NREPLF;     /* POINT TO REPEATING CNTRL    0308 00408000
*                                      INFORMATION                   */ 00409000
         LH    @10,CCASMFIX(,R11)                                  0308 00410000
