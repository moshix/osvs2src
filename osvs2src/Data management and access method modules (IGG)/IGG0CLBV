*     R8,                                                          0876 02708000
*     R9,                                                          0876 02709000
*     R10) UNRSTD;                  /* PL/S COMPILER WORK REGISTERS  */ 02710000
Z072     DS    0H                                                  0876 02711000
Z072SMFZ DS    0H                                                  0877 02712000
*   RFY                                                            0877 02713000
*    (R2,                                                          0877 02714000
*     R3,                                                          0877 02715000
*     R4) RSTD;                     /* RESTRICT LOCAL WORK REGISTERS */ 02716000
*   GEN(STM 12,14,12(13));          /* SAVE REGISTERS OF CALLER      */ 02717000
         STM 12,14,12(13)                                               02718000
*   GEN(LA 13,12(13));              /* POINT TO NEXT SAVE AREA       */ 02719000
         LA 13,12(13)                                                   02720000
*   GEN(BALR 12,0);                 /* ESTABLISH ADDRESSABILITY      */ 02721000
         BALR 12,0                                                      02722000
*   GEN(LA 15,*-IGGSEGST(0,0));     /* COMPUTE MODULE BASE           */ 02723000
         LA 15,*-IGGSEGST(0,0)                                          02724000
*   GEN(SR 12,15);                  /* SET BASE TO MODULE BASE     0882 02725000
*                                      PROCEDURE                     */ 02726000
         SR 12,15                                                       02727000
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 02728000
**   MODULE NAME =  IGGPSMFZ (IGG0CLBV)                               * 02729000
**                                                                    * 02730000
**   DESCRIPTIVE NAME =  ZERO SECURITY INFORMATION                    * 02731000
**                                                                    * 02732000
**   FUNCTION =                                                       * 02733000
**      THIS SUB-FUNCTION SCANS THE GOP'S IN THE CATALOG RECORD       * 02734000
**      SPECIFIED BY REGISTER 1 FOR A SECURITY GROUP OCCURRENCE. IF   * 02735000
**      ONE IS FOUND THE SECURITY INFORMATION IS ZEROED.              * 02736000
**                                                                    * 02737000
**   INPUT =                                                          * 02738000
**      R1 = ADDRESS OF THE CATALOG RECORD TO BE EXAMINED.            * 02739000
**   OUTPUT =                                                         * 02740000
**      SECURITY INFORMATION CLEARED.                                 * 02741000
**                                                                    * 02742000
**                                                                    * 02743000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 02744000
*                                                                  0883 02745000
*   RFY                                                            0883 02746000
*    (R1,                                                          0883 02747000
*     R6,                                                          0883 02748000
*     R7) RSTD;                     /* WORK REGS                     */ 02749000
*   RFY                                                            0884 02750000
*     CATRCD BASED(R1);             /* MAP CAT RCD                   */ 02751000
*   RFY                                                            0885 02752000
*     REPCNTRL BASED(R6);           /* MAP REPEATING CNTRL INFO      */ 02753000
*   RFY                                                            0886 02754000
*     CATGOPT BASED(R6);            /* MAP GOP                       */ 02755000
*   RFY                                                            0887 02756000
*     CNTRLINF BASED(R6);           /* MAP GROUP OCCURR INFO         */ 02757000
*   RFY                                                            0888 02758000
*     VARFIELD BASED(R7);           /* MAP VARIABLE LENGTH FIELD     */ 02759000
*   R6=R1+NREPLF;                   /* POINT TO REPEATING CONTROL  0889 02760000
*                                      INFORMATION                   */ 02761000
         LR    R6,R1                                               0889 02762000
         SLR   @10,@10                                             0889 02763000
         IC    @10,NREPLF(,R1)                                     0889 02764000
         ALR   R6,@10                                              0889 02765000
*   R7=REPCNT*LENGTH(CATGOPT)+LENGTH(REPCNTRL)+R6;/* POINT TO FIRST     02766000
*                                      GROUP OCCUR                   */ 02767000
         LA    @10,6                                               0890 02768000
         SLR   R7,R7                                               0890 02769000
         IC    R7,REPCNT(,R6)                                      0890 02770000
         MH    R7,@CH00256                                         0890 02771000
         ALR   R7,@10                                              0890 02772000
         ALR   R7,R6                                               0890 02773000
*   R6=R6+LENGTH(REPCNTRL);         /* POINT TO FIRST GOP            */ 02774000
         ALR   R6,@10                                              0891 02775000
*   DO WHILE(R6<R7&R7^=0);          /* SCAN GOP'S FOR GROUP 4        */ 02776000
         B     @DE00892                                            0892 02777000
@DL00892 DS    0H                                                  0893 02778000
*     IF CATGOGC1=PSWDGC THEN       /* IF GROUP CODE 4               */ 02779000
         CLI   CATGOGC1(R6),4                                      0893 02780000
         BNE   @RF00893                                            0893 02781000
*       DO;                         /* SET UP FOR CLEAR              */ 02782000
*         R6=R7+CATGODSP;           /* POINT TO PSWD OCCUR           */ 02783000
         LR    @10,R7                                              0895 02784000
         ICM   @05,12,CATGODSP(R6)                                 0895 02785000
         SRA   @05,16                                              0895 02786000
         ALR   @10,@05                                             0895 02787000
         LR    R6,@10                                              0895 02788000
*         R7=0;                     /* INDICATE EXIT                 */ 02789000
         SLR   R7,R7                                               0896 02790000
*       END;                        /* END SET UP                    */ 02791000
*     ELSE                          /* NOT GROUP CODE 4              */ 02792000
*       R6=R6+LENGTH(CATGOPT);      /* CHECK NEXT GOP                */ 02793000
         B     @RC00893                                            0898 02794000
@RF00893 AL    R6,@CF00256                                         0898 02795000
*   END;                            /* END SCAN                      */ 02796000
@RC00893 DS    0H                                                  0899 02797000
@DE00892 CR    R6,R7                                               0899 02798000
         BNL   @DC00892                                            0899 02799000
         LTR   R7,R7                                               0899 02800000
         BNZ   @DL00892                                            0899 02801000
@DC00892 DS    0H                                                  0900 02802000
*   IF R7^=0 THEN                                                  0900 02803000
         LTR   R7,R7                                               0900 02804000
         BNZ   @RT00900                                            0900 02805000
*     ;                             /* IF GROUP 4 NOT FOUND          */ 02806000
*   ELSE                            /* GROUP 4 FOUND                 */ 02807000
*     DO;                           /* ZERO GROUP 4                  */ 02808000
*       R7=R6+LENFIX;               /* POINT TO VAR FIELD            */ 02809000
         LR    R7,R6                                               0903 02810000
         SLR   @10,@10                                             0903 02811000
         IC    @10,LENFIX(,R6)                                     0903 02812000
         ALR   R7,@10                                              0903 02813000
*       IF CNTVAR^=0 THEN           /* IF VARIABLE FIELDS            */ 02814000
         CLI   CNTVAR(R6),0                                        0904 02815000
         BE    @RF00904                                            0904 02816000
*         R7=R7+VARLEN+LENGTH(VARLEN);/* POINT TO END OF OCCURR      */ 02817000
         LR    @10,R7                                              0905 02818000
         ICM   @05,12,VARLEN(R7)                                   0905 02819000
         SRA   @05,16                                              0905 02820000
         ALR   @10,@05                                             0905 02821000
         AL    @10,@CF00059                                        0905 02822000
         LR    R7,@10                                              0905 02823000
*       ELSE                                                       0906 02824000
*         ;                         /* NO VARIABLE FIELDS            */ 02825000
@RF00904 DS    0H                                                  0907 02826000
*       R6=R6+LENGTH(CNTRLINF);     /* POINT TO DATA                 */ 02827000
         AL    R6,@CF00059                                         0907 02828000
*       R7=R7-R6;                   /* GROUP 4 LENGTH                */ 02829000
         SLR   R7,R6                                               0908 02830000
*       RFY                                                        0909 02831000
*        (R8,                                                      0909 02832000
*         R9) RSTD;                 /* USE THESE REGS TO ZERO        */ 02833000
*       R9=0;                       /* PAD CHARACTER                 */ 02834000
         SLR   R9,R9                                               0910 02835000
*       GEN(MVCL R6,R8);            /* ZERO SECURITY INFO            */ 02836000
         MVCL R6,R8                                                     02837000
*       RFY                                                        0912 02838000
*        (R8,                                                      0912 02839000
*         R9) UNRSTD;               /* FREE REGS                     */ 02840000
*     END;                          /* END ZERO GROUP 4              */ 02841000
*   RFY                                                            0914 02842000
*     CATRCD BASED(CCACPE1);        /* ORGINAL BASE                  */ 02843000
@RT00900 DS    0H                                                  0915 02844000
*   RFY                                                            0915 02845000
*     REPCNTRL BASED(CCACPE2);      /* ORGINAL BASE                  */ 02846000
*   RFY                                                            0916 02847000
*     CATGOPT BASED(CCACPE5);       /* ORGINAL BASE                  */ 02848000
*   RFY                                                            0917 02849000
*     CNTRLINF BASED(CCACPE6);      /* ORGINAL BASE                  */ 02850000
*   RFY                                                            0918 02851000
*     VARFIELD BASED(CCACPE7);      /* ORGINAL BASE                  */ 02852000
*   RFY                                                            0919 02853000
*    (R1,                                                          0919 02854000
*     R6,                                                          0919 02855000
*     R7) UNRSTD;                   /* FREE REGS                     */ 02856000
*   CALL EXIT;                      /* RETURN TO CALLER              */ 02857000
         BAL   @14,EXIT                                            0920 02858000
*   END;                            /* END OF INNER PROCEDURE        */ 02859000
@EL00012 DS    0H                                                  0921 02860000
@EF00012 DS    0H                                                  0921 02861000
@ER00012 BR    @14                                                 0921 02862000
@PB00012 DS    0H                                                  0922 02863000
*   GEN(EJECT);                     /* GO TO A NEW PRINT PAGE        */ 02864000
         EJECT                                                          02865000
*                                                                  0923 02866000
