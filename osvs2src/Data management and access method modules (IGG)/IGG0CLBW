*             END;                  /* END CREATE                    */ 00923000
*         END;                      /* END RETRIEVE                  */ 00924000
*       ELSE                        /* NO CONTINUATION OF FIELD      */ 00925000
*         DO;                       /* FORMAT NEW RECORD             */ 00926000
*                                                                  0293 00927000
         B     @RC00282                                            0293 00928000
@RF00282 DS    0H                                                  0294 00929000
*           /*********************************************************/ 00930000
*           /*                                                       */ 00931000
*           /* CALL TO MODULE IGG0CLAW                               */ 00932000
*           /*                                                       */ 00933000
*           /*********************************************************/ 00934000
*                                                                  0294 00935000
*Z022GNEX:                                                         0294 00936000
*           CALL IGGPGNEX;          /* FORMAT RECORD                 */ 00937000
Z022GNEX L     @15,@CV00037                                        0294 00938000
         BALR  @14,@15                                             0294 00939000
*           CCACPE5=ADDR(REPGOPT);  /* POINT TO GOP                  */ 00940000
         L     @10,CCAURAB(,R11)                                   0295 00941000
         L     @05,CCACPE2(,@10)                                   0295 00942000
