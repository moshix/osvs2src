*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 01546000
**   MODULE NAME =  IGGPUPUP(IGG0CLBW)                                * 01547000
**                                                                    * 01548000
**   DESCRIPTIVE NAME =  SHIFT FIELD DATA UP                          * 01549000
**                                                                    * 01550000
**   FUNCTION =                                                       * 01551000
**      THIS SUB-FUNCTION SHIFTS FIELD DATA UPWARD IN ORDER TO        * 01552000
**      ACCOMODATE AN INSERTION ON THE RIGHT.  BASICALLY PROCESSING   * 01553000
**      CONSISTS OF SAVING ANY OVERFLOW DATA CREATED, SHIFTING THE    * 01554000
**      REMAINING FIELD DATA TO THE LEFT, INSERTING THE OVERFLOW FIELD* 01555000
**      DATA FROM THE PREVIOUS RECORD, AND ADJUSTING THE VARIOUS      * 01556000
**      CATALOG POINTERS AND SIZES TO REFLECT THIS NEW INSERTION.     * 01557000
**                                                                    * 01558000
**   INPUT =                                                          * 01559000
**      IGGCCA INPUT:                                                 * 01560000
**         CCARBASE - RELATIVE BASE RAB ADDRESS OF RECORD TO RECEIVE  * 01561000
**                    INSERTION OF FIELD DATA.                        * 01562000
**         CCACHAIN - CI NUMBER OF PREVIOUS RECORD.                   * 01563000
**         CCAVARLN - INSERTION DATA LENGTH                           * 01564000
**         CCAVARPT - ADDRESS OF INSERTION DATA.                      * 01565000
**         CCAVAR - INSERTION DATA.                                   * 01566000
**                                                                    * 01567000
**   OUTPUT =                                                         * 01568000
**      IGGCCA OUTPUT:                                                * 01569000
**         CCAVARLN - INSERTION DATA LENGTH FOR NEXT RECORD.          * 01570000
**         CCAVARPT - INSERTION DATA ADDRESS                          * 01571000
**         CCAVAR - INSERTION DATA.                                   * 01572000
**                                                                    * 01573000
**                                                                    * 01574000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 01575000
*                                                                  0439 01576000
*   RFY                                                            0439 01577000
*     CCARAB BASED(CCARBASE);       /* MAP RAB OF RELATIVE BASE      */ 01578000
*   IF CCACHAIN^=0 THEN             /* IF ANOTHER RCD FOLLOWS        */ 01579000
         ICM   @10,7,CCACHAIN(R11)                                 0440 01580000
         BZ    @RF00440                                            0440 01581000
*     DO;                           /* SET CHAIN PTR                 */ 01582000
*       EXTGOCI=CCACHAIN;           /* CI NUMBER                     */ 01583000
         L     @10,CCARBASE(,R11)                                  0442 01584000
         L     @10,CCACPE2(,@10)                                   0442 01585000
         MVC   EXTGOCI(3,@10),CCACHAIN(R11)                        0442 01586000
*     END;                          /* END SET                       */ 01587000
*   ELSE                                                           0444 01588000
*     ;                             /* NO RCD FOLLOWS                */ 01589000
@RF00440 DS    0H                                                  0445 01590000
*   R2=RCDSIZ+CCAVARLN-MXRCD;       /* GET LENGTH OF OVERFLOW        */ 01591000
         L     @10,CCARBASE(,R11)                                  0445 01592000
         L     @10,CCACPE1(,@10)                                   0445 01593000
         ICM   R2,12,RCDSIZ(@10)                                   0445 01594000
         SRA   R2,16                                               0445 01595000
         AH    R2,CCAVARLN(,R11)                                   0445 01596000
         SL    R2,@CF00049                                         0445 01597000
*   IF R2>0 THEN                    /* IF OVERFLOW EXISTS            */ 01598000
         SLR   @10,@10                                             0446 01599000
         CR    R2,@10                                              0446 01600000
         BNH   @RF00446                                            0446 01601000
*     DO;                           /* SAVE IT                       */ 01602000
*       IF DIC2GC=DSDPGC THEN       /* IF SPACE DESCRIPTOR           */ 01603000
         CLI   DIC2GC+372(R11),7                                   0448 01604000
         BNE   @RF00448                                            0448 01605000
*         DO;                       /* ADJUST OVERFLOW TO RUN LENGTH    01606000
*                                      CODE BOUNDARY                 */ 01607000
*           RFY                                                    0450 01608000
*             RUNLNCDE BASED(ADDR(VARINFO)+R3);/* OVERLAY SPACE MAP  */ 01609000
*           R3=0;                   /* DISPLACEMENT INTO MAP       0451 01610000
*                                      INITIALLY ZERO                */ 01611000
         LR    R3,@10                                              0451 01612000
*           DO WHILE(R3<=R2);       /* SCAN FOR RUN CODE JUST AFTER     01613000
