**   MODULE NAME =  IGGPDOWN(IGG0CLBW)                                * 00809000
**                                                                    * 00810000
**   DESCRIPTIVE NAME =  SHIFT FIELD DATA DOWN                        * 00811000
**                                                                    * 00812000
**   FUNCTION =                                                       * 00813000
**      THIS SUB-FUNCTION SHIFTS FIELD INFORMATION DOWNWARD IN A      * 00814000
**      VARIABLE LENGTH CATALOG FIELD IN ORDER TO ACCOMODATE AN INSERT* 00815000
**      OF USER FIELD DATA.  TO PRESERVE INTEGRITY, THE CATALOG RECORD* 00816000
**      TO RECEIVE THE NEW FIELD DATA IS DUPLICATED WITH A DIFFERENT  * 00817000
**      CONTROL INTERVAL NUMBER AND THE ACTUAL INSERT IS DONE INTO    * 00818000
**      THIS DUPLICATED RECORD.  BECAUSE THE VARIABLE LENGTH FIELD    * 00819000
**      MAY BE STORED IN A CHAIN OF RECORDS, THIS SUB-FUNCTION MAY    * 00820000
**      HAVE TO USE A CI NUMBER PREVIOUSLY OBTAINED, IF THE INSERT    * 00821000
**      CAUSES A SERIES OF OVERFLOWS IN THE CHAIN OF RECORDS.  AN     * 00822000
**      OVERFLOW MAY OCCUR IF THE CURRENT INSERT LENGTH (MINUS ANY    * 00823000
**      DELETE LENGTH) EXCEEDS THE CAPACITY OF THE CURRENT RECORD.    * 00824000
**      IN THIS CASE THE EXCESS IS SAVED FOR THE NEXT RECORD BEFORE   * 00825000
**      THE ACTUAL EXPANSION INSERT IS PERFORMED, AND A NEW CI NUMBER * 00826000
**      IS OBTAINED FOR THE NEXT RECORD TO RECEIVE THE OVERFLOW.  THE * 00827000
**      CI NUMBER MUST BE OBTAINED AT THIS TIME BECAUSE THE DUPLICATED* 00828000
**      RECORD IS ADDED IN THIS SUB-FUNCTION AND MUST BE CHAINED TO   * 00829000
**      THE NEXT DUPLICATED RECORD TO RECEIVE THE OVERFLOW.           * 00830000
**                                                                    * 00831000
**   INPUT =                                                          * 00832000
**      IGGCCA INPUT:                                                 * 00833000
**         CCARBASE - RELATIVE BASE RAB ADDRESS OF CATALOG RECORD     * 00834000
**                    CAUSING OVERFLOW.                               * 00835000
**         CCACHAIN - PREVIOUSLY OBTAINED CI NUMBER.                  * 00836000
**         CCAREPCT - COUNT OF VARIABLE FIELDS FOR THE GROUP.         * 00837000
**         CCAVARLN - LENGTH OF USER DATA FOR INSERT.                 * 00838000
**         CCADELN - LENGTH OF CATALOG FIELD DATA TO BE DELETED.      * 00839000
**         CCAVARPT - ADDRESS OF USER DATA FOR INSERT.                * 00840000
**                                                                    * 00841000
**   OUTPUT =                                                         * 00842000
**      IGGCCA OUTPUT:                                                * 00843000
**         CCACHAIN - CI NUMBER FOR NEXT RECORD TO RECEIVE OVERFLOW.  * 00844000
**         CCAVARLN - LENGTH OF OVERFLOW FIELD DATA.                  * 00845000
**         CCADELN - ALWAYS ZERO.                                     * 00846000
**         CCAVARPT - ADDRESS OF OVERFLOW FIELD DATA.                 * 00847000
**         CCAURAB - ADDRESS OF RAB CONTAINING DUPLICATED RECORD WITH * 00848000
**                   INSERTED DATA.                                   * 00849000
**      DUPLICATED RECORD (WITH INSERTED FIELD DATA) ADDED TO VSAM    * 00850000
**      CATALOG.                                                      * 00851000
**                                                                    * 00852000
**                                                                    * 00853000
** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */ 00854000
*                                                                  0271 00855000
*   RFY                                                            0271 00856000
*     CCARAB BASED(CCARBASE);       /* MAP RAB OF RELATIVE BASE      */ 00857000
*   IF CCACHAIN^=0 THEN             /* CHECK WHETHER ASSIGN HAS    0272 00858000
*                                      ALREADY BEEN DONE             */ 00859000
         SLR   @10,@10                                             0272 00860000
         ICM   @10,7,CCACHAIN(R11)                                 0272 00861000
         LTR   @10,@10                                             0272 00862000
         BZ    @RF00272                                            0272 00863000
*     DO;                           /* USE THIS CI                   */ 00864000
*       CCAASCI(1)=CCACHAIN;        /* SET NEW RECORD CI             */ 00865000
         STCM  @10,7,CCAASCI(R11)                                  0274 00866000
*     END;                          /* END USE                       */ 00867000
*   ELSE                            /* NO PREVIOUS ASSIGN            */ 00868000
*     DO;                           /* CALL ASSIGN                   */ 00869000
*                                                                  0276 00870000
         B     @RC00272                                            0276 00871000
@RF00272 DS    0H                                                  0277 00872000
*       /*************************************************************/ 00873000
*       /*                                                           */ 00874000
*       /* CALL TO MODULE IGG0CLAG                                   */ 00875000
*       /*                                                           */ 00876000
*       /*************************************************************/ 00877000
*                                                                  0277 00878000
*Z020AXCI:                                                         0277 00879000
*       CALL IGGPAXCI;              /* ASSIGN CI                     */ 00880000
Z020AXCI L     @15,@CV00041                                        0277 00881000
         BALR  @14,@15                                             0277 00882000
*     END;                          /* END CALL                      */ 00883000
*   IF CCACD1^=RCS THEN                                            0279 00884000
@RC00272 CLI   CCACD1(R11),0                                       0279 00885000
         BNE   @RT00279                                            0279 00886000
*     ;                             /* CHECK FOR ERROR               */ 00887000
*   ELSE                            /* NO ERROR                      */ 00888000
*     DO;                           /* CONTINUE                      */ 00889000
*       IF EXTGOCI^=0 THEN          /* CHECK WHETHER RELATIVE BASE 0282 00890000
*                                      POINTS TO A CONTINUATION OF 0282 00891000
*                                      FIELD                         */ 00892000
         L     @10,CCARBASE(,R11)                                  0282 00893000
         L     @10,CCACPE2(,@10)                                   0282 00894000
         ICM   @05,7,EXTGOCI(@10)                                  0282 00895000
         BZ    @RF00282                                            0282 00896000
*         DO;                       /* RETRIEVE RECORD               */ 00897000
*           CCASRID=EXTGOCI;        /* SET CI NUMBER                 */ 00898000
*                                                                  0284 00899000
         MVC   CCASRID(3,R11),EXTGOCI(@10)                         0284 00900000
*           /*********************************************************/ 00901000
*           /*                                                       */ 00902000
*           /* CALL TO MODULE IGG0CLBA                               */ 00903000
*           /*                                                       */ 00904000
*           /*********************************************************/ 00905000
*                                                                  0285 00906000
*Z021GVAL:                                                         0285 00907000
*           CALL IGGPGVAL;          /* GET OLD RECORD                */ 00908000
Z021GVAL L     @15,@CV00044                                        0285 00909000
         BALR  @14,@15                                             0285 00910000
*           IF CCACD1^=RCS THEN                                    0286 00911000
         CLI   CCACD1(R11),0                                       0286 00912000
         BNE   @RT00286                                            0286 00913000
*             ;                     /* IF ERROR, NO PROCESS          */ 00914000
*           ELSE                    /* NO ERROR                      */ 00915000
*             DO;                   /* CREATE NEW RECORD             */ 00916000
*               RFY                                                0289 00917000
*                 CCARAB BASED(CCAURAB);/* MAP NEW RECORD            */ 00918000
*               RCDCINO=CCAASCI(1); /* NEW RECORD CI                 */ 00919000
         L     @10,CCAURAB(,R11)                                   0290 00920000
         L     @10,CCACPE1(,@10)                                   0290 00921000
