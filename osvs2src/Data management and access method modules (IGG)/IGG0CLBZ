         TITLE 'IGG0CLBZ - CATALOG AMS CONVERTV SUPPORT                *00001000
                        '                                               00002000
IGG0CLBZ CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGG0CLBZ  78.174'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
         USING @MAINENT,@15                                             00011000
         USING @DATD-(CCATEMPS-IGGCCA),R11                              00012000
         DS 0F                                                          00013000
         DC CL8'IGGPCONV'                                               00014000
@EL00001 DS    0H                                                  0163 00015000
@EF00001 DS    0H                                                  0163 00016000
@ER00001 BR    @14                                                 0163 00017000
IGGPCONV DS    0H                                                  0164 00018000
         ENTRY IGGPCONV                                                 00019000
         DROP @15                                                       00020000
Z003     DS    0H                                                  0166 00021000
Z003CONV DS    0H                                                  0167 00022000
         STM 12,14,12(13)                                               00023000
         LA 13,12(13)                                                   00024000
         BALR 12,0                                                      00025000
IGGSEGST DS 0H                                                          00026000
         USING *,12                                                     00027000
         TESTAUTH  FCTN=1                                               00028000
*        IF R15 ^= ZERO THEN             /* NON ZERO AUTHORIZATION  */  00029000
         LTR   R15,R15                                             0174 00030000
         BZ    @RF00174                                            0174 00031000
*          DO;                           /* INADEQUATE AUTHORIZATION*/  00032000
*                                                                  0175 00033000
*             /* SET CCAMODID='BZ', CCAREASN=RSN4, CCARETRN=RCTSAUTH */ 00034000
*             CCAPROB=IGGSEGID+(RSN4*K256)+RCTSAUTH;               0176 00035000
         MVC   CCAPROB(4,R11),@CF04038                             0176 00036000
*                                          /* INDICATE AUTHORIZATION    00037000
*                                          ERROR           @Y30LSJG*/   00038000
*            CALL EXIT;                 /*  RETURN WITH ERROR       */  00039000
         BAL   @14,EXIT                                            0177 00040000
*          END;                          /* END APF CHECKING        */  00041000
*                                                                  0178 00042000
*                                                                  0178 00043000
*   /* FIND THE TIOT ENTRY ADDRESS FOR THE DD CARD ASSOCIATED        */ 00044000
*   /* WITH THIS REQUEST                                             */ 00045000
*   /*                                                               */ 00046000
*        RESPECIFY TIOENTRY BASED(TIOTENTY);                       0179 00047000
@RF00174 DS    0H                                                  0180 00048000
*        TIOTENTY=CCATCB->TCBTIO;            /*SET PTR TO FIRST DD   */ 00049000
         L     @10,CCATCB(,R11)                                    0180 00050000
         L     TIOTENTY,TCBTIO(,@10)                               0180 00051000
*        TIOTENTY=TIOTENTY+24;               /*ENTRY IN THE TIOT     */ 00052000
         AL    TIOTENTY,@CF00130                                   0181 00053000
*        DO WHILE(TIOEDDNM^=DDNAME & TIOELNGH^=0);/*SRCH FOR DD ENTY */ 00054000
         B     @DE00182                                            0182 00055000
@DL00182 DS    0H                                                  0183 00056000
*                                                            /* LOOP */ 00057000
*          TIOTENTY=TIOTENTY+TIOELNGH;       /*SET ADDR NEXT DD ENTRY*/ 00058000
         SLR   @10,@10                                             0183 00059000
         IC    @10,TIOELNGH(,TIOTENTY)                             0183 00060000
         ALR   TIOTENTY,@10                                        0183 00061000
*        END;                                /*END SEARCH LOOP       */ 00062000
@DE00182 L     @10,CCACPL(,R11)                                    0184 00063000
         L     @10,CTGDDNM(,@10)                                   0184 00064000
         CLC   TIOEDDNM(8,TIOTENTY),DDNAME(@10)                    0184 00065000
         BE    @DC00182                                            0184 00066000
         CLI   TIOELNGH(TIOTENTY),0                                0184 00067000
         BNE   @DL00182                                            0184 00068000
@DC00182 DS    0H                                                  0185 00069000
*        IF TIOEDDNM ^= DDNAME THEN      /* CHECK IF A DDNAME WAS FND*/ 00070000
         L     @10,CCACPL(,R11)                                    0185 00071000
         L     @10,CTGDDNM(,@10)                                   0185 00072000
         CLC   TIOEDDNM(8,TIOTENTY),DDNAME(@10)                    0185 00073000
         BE    @RF00185                                            0185 00074000
*          DO;                           /* EXIT WITH ERR IF NO DD  */  00075000
*                                                                  0186 00076000
*             /* SET CCAMODID='BZ', CCAREASN=RSN8, CCARETRN=RCNMNTD  */ 00077000
*             CCAPROB=IGGSEGID+(RSN8*K256)+RCNMNTD;                0187 00078000
         MVC   CCAPROB(4,R11),@CF04041                             0187 00079000
*                                         /* INDICATE UNIT NOT FOND*/   00080000
*            CALL EXIT;                  /* QUIT WITH ERROR         */  00081000
         BAL   @14,EXIT                                            0188 00082000
*          END;                          /* END IF NO DD ENTRY FOUND */ 00083000
*        CONVUCB = TIOEFSRT;             /* GET UCB POINTER FROM TIOT*/ 00084000
*                                                                  0190 00085000
@RF00185 L     @10,CCACMSWA(,R11)                                  0190 00086000
         L     @15,TIOEFSRT-1(,TIOTENTY)                           0190 00087000
         LA    @15,0(,@15)                                         0190 00088000
         ST    @15,CONVUCB(,@10)                                   0190 00089000
*   /* THE VOLID IN THE UCB FOR THE DEVICE MUST BE VERIFIED AGAINST */  00090000
*   /* THE VOLID SUPPLIED AS PART IN THE INPUT FPL                  */  00091000
*                                                                  0191 00092000
*        CCAFLPT = CTGFIELD(1);         /* POSITION TO FIRST FPL    */  00093000
         L     @10,CCACPL(,R11)                                    0191 00094000
         L     @10,CTGFIELD(,@10)                                  0191 00095000
         ST    @10,CCAFLPT(,R11)                                   0191 00096000
*        DO WHILE FIELDNM ^= 'VOLSER  ' & /* SEARCH FOR VOLSER FPL  */  00097000
*                 CCAFLPT ^= ZERO;      /* STOP IF END OF CHAIN     */  00098000
         B     @DE00192                                            0192 00099000
@DL00192 DS    0H                                                  0193 00100000
*          CCAFLPT = CTGFLCHN;          /* GET NEXT FPL             */  00101000
         L     @10,CCAFLPT(,R11)                                   0193 00102000
         L     @10,CTGFLCHN(,@10)                                  0193 00103000
         ST    @10,CCAFLPT(,R11)                                   0193 00104000
*        END;                           /* END OF SEARCH FOR VOLSER */  00105000
@DE00192 L     @10,CCAFLPT(,R11)                                   0194 00106000
         L     @05,CTGFLDNM(,@10)                                  0194 00107000
         CLC   FIELDNM(8,@05),@CC03976                             0194 00108000
         BE    @DC00192                                            0194 00109000
         LTR   @10,@10                                             0194 00110000
         BNZ   @DL00192                                            0194 00111000
@DC00192 DS    0H                                                  0195 00112000
*        CONVOLSR = CONVOLID;           /* SAVE CONVERTED VOLSER    */  00113000
*                                                                  0195 00114000
         L     @10,CCACMSWA(,R11)                                  0195 00115000
         L     @05,CCAFLPT(,R11)                                   0195 00116000
         L     @05,CTGFLPT(,@05)                                   0195 00117000
         MVC   CONVOLSR(6,@10),CONVOLID(@05)                       0195 00118000
*        IF CONVOLID ^= UCBVOLI |       /* ERROR IF INCORRECT VOLID */  00119000
*           UCBMOUNT = ON THEN          /* ERROR IF MOUNT PENDING   */  00120000
         L     @10,CONVUCB(,@10)                                   0196 00121000
         CLC   CONVOLID(6,@05),UCBVOLI(@10)                        0196 00122000
         BNE   @RT00196                                            0196 00123000
         TM    UCBMOUNT(@10),B'10000000'                           0196 00124000
         BNO   @RF00196                                            0196 00125000
@RT00196 DS    0H                                                  0197 00126000
*          DO;                          /* QUIT IF ERROR FOUND      */  00127000
*                                                                  0197 00128000
*             /* SET CCAMODID='BZ', CCAREASN=RSN2, CCARETRN=RCNMNTD  */ 00129000
*             CCAPROB=IGGSEGID+(RSN2*K256)+RCNMNTD;                0198 00130000
         MVC   CCAPROB(4,R11),@CF04043                             0198 00131000
*                                         /* VOLUME NOT MOUNTED ERR*/   00132000
*            CALL EXIT;                 /* QUIT WITH ERROR          */  00133000
         BAL   @14,EXIT                                            0199 00134000
*          END;                         /* END IF VOLUME NOT MOUNTED*/  00135000
*                                                                  0200 00136000
*   /* THE UCB FOR THE VOLUME BEING CONVERTED HAS NOW BEEN          */  00137000
*   /* FOUND. THE FORMAT 4 DSCB CAN NOW BE READ AND MODIFIED        */  00138000
*                                                                  0201 00139000
*   /* A BIT IN THE FORMAT 4 DSCB (DS4VSBAD) INDICATING THIS        */  00140000
*   /* VOLUME IS NOT USABLE MUST BE SET TO TAKE CARE OF THE         */  00141000
*   /* CASE WHERE THE CONVERTV OPERATION FAILS IN THE MIDDLE,       */  00142000
*   /* AND ANOTHER PROGRAM TRIES TO ACCESS THE CONVERTED VOLUME     */  00143000
*   /* BEFORE THE CATALOG HAS BEEN PROPERLY UPDATED TO REFLECT      */  00144000
*   /* THE NEW DEVICE TYPE                                          */  00145000
*   /* THE FORMAT 4 READING AND UPDATING ROUTINES ARE NOW CALLED    */  00146000
*   /* TO READ THE FORMAT 4 DSCB, SET THE DS4VSBAD BIT ON, AND      */  00147000
*   /* REWRITE THE FORMAT 4. WHEN THIS HAS BEEN ACCOMPLISHED,       */  00148000
*   /* THE CATALOG CAN BE CONVERTED TO POINT TO THE VOLUME          */  00149000
*   /* BEING CONVERTED                                              */  00150000
*                                                                  0201 00151000
*        RFY IECSDSF4 BASED(ADDR(F4WBUF));                         0201 00152000
@RF00196 DS    0H                                                  0202 00153000
*        RFY IGGMF4WA BASED(CCAURAB);                              0202 00154000
*        CONVURAB = CCAURAB;             /* SAVE RAB WITH VOLUME ETY */ 00155000
         L     @10,CCACMSWA(,R11)                                  0203 00156000
         L     @05,CCAURAB(,R11)                                   0203 00157000
         ST    @05,CONVURAB(,@10)                                  0203 00158000
*        CCAURAB=CCAR0REC;              /* FORMAT 4 ROUTINE WRK AREA*/  00159000
         L     @05,CCAR0REC(,R11)                                  0204 00160000
         ST    @05,CCAURAB(,R11)                                   0204 00161000
*        F4WDUCB=CONVUCB;               /* UCB ADDRESS              */  00162000
*                                                                  0205 00163000
         MVC   F4WDUCB(3,@05),CONVUCB+1(@10)                       0205 00164000
*        /* CALL TO MODULE IGG0CLBU                                  */ 00165000
*Z007F4RD:                                                         0206 00166000
*        CALL IGGPF4RD;                                            0206 00167000
Z007F4RD L     @15,@CV00040                                        0206 00168000
         BALR  @14,@15                                             0206 00169000
*                                       /* READ FORMAT 4 DSCB       */  00170000
*        IF CCACD1^=0 THEN                  /*CHECK IF ANY ERROR    */  00171000
         CLI   CCACD1(R11),0                                       0207 00172000
         BE    @RF00207                                            0207 00173000
*          DO;                                                     0208 00174000
*            IF CCACD1^='08'X |              /*CHECK SPECIAL CASE   */  00175000
*              DS4IDFMT^='F4'X THEN               /* ON FMT4 READ   */  00176000
*                                                                  0209 00177000
         CLI   CCACD1(R11),X'08'                                   0209 00178000
         BNE   @RT00209                                            0209 00179000
         L     @10,CCAURAB(,R11)                                   0209 00180000
         CLI   DS4IDFMT+120(@10),X'F4'                             0209 00181000
         BE    @RF00209                                            0209 00182000
@RT00209 DS    0H                                                  0210 00183000
*               /* SET CCAMODID='BZ', CCAREASN=RSN0, CCARETRN=RCNTFMT4  00184000
*                                                                    */ 00185000
*               CCAPROB=IGGSEGID+(RSN0*K256)+RCNTFMT4;             0210 00186000
         MVC   CCAPROB(4,R11),@CF04044                             0210 00187000
*                                            /* SET I/O ERROR CODE */   00188000
*            CALL EXIT;                  /* EXIT WITH THE READ ERROR*/  00189000
@RF00209 BAL   @14,EXIT                                            0211 00190000
*          END;                          /* END ERROR ON READ       */  00191000
*        DS4VSBAD = ON;                  /* INDICATE BAD VOLUME     */  00192000
@RF00207 L     @10,CCAURAB(,R11)                                   0213 00193000
         OI    DS4VSBAD+120(@10),B'01000000'                       0213 00194000
*   /* THE BAD VOLUME BIT WILL BE RESET AFTER THE CONVERT OP        */  00195000
*   /* HAS BEEN SUCCESSFULLY COMPLETED                              */  00196000
*                                                                  0214 00197000
*        /* CALL TO MODULE IGG0CLBU                                  */ 00198000
*Z009F4WR:                                                         0214 00199000
*        CALL IGGPF4WR;                                            0214 00200000
Z009F4WR L     @15,@CV00041                                        0214 00201000
         BALR  @14,@15                                             0214 00202000
*                                                  /* WRITE F4 DSCB   * 00203000
*        CCAURAB=CONVURAB;                   /* RESET RAB ADDR */  0215 00204000
*                                                                  0215 00205000
         L     @10,CCACMSWA(,R11)                                  0215 00206000
         L     @10,CONVURAB(,@10)                                  0215 00207000
         ST    @10,CCAURAB(,R11)                                   0215 00208000
*   /* THE CONTROL INTERVAL NUMBER OF THE VOLUME RECORD IS          */  00209000
*   /* CHECKED TO SEE IF IT IS THE VOLUME RECORD FOR THE CATLOG     */  00210000
*   /* IF A CATALOG IS ON THE VOLUME, SPECIAL PROCESSING MUST       */  00211000
*   /* BE PERFORMED .                                               */  00212000
*                                                                  0216 00213000
*        IF RCDCINO = 9 THEN             /* CHECK IF CATALOG VOLUME */  00214000
         L     @10,CCACPE1(,@10)                                   0216 00215000
         CLC   RCDCINO(3,@10),@CF03521+1                           0216 00216000
         BNE   @RF00216                                            0216 00217000
*          CCAAUCAT = ON;                /* INDICATE THIS VOL IS CAT*/  00218000
*                                                                  0217 00219000
         OI    CCAAUCAT(R11),B'00000001'                           0217 00220000
*        CCAFLPT = CTGFIELD(1);          /* POINT TO FIRST FPL      */  00221000
@RF00216 L     @10,CCACPL(,R11)                                    0218 00222000
         L     @10,CTGFIELD(,@10)                                  0218 00223000
         ST    @10,CCAFLPT(,R11)                                   0218 00224000
*   /* THE DEVICE OPTION BITS IN THE PASSED DEVICE TYPES  THAT ARE  */  00225000
*   /* NOT RETAINED IN THE CATALOG MUST BE TURNED OFF               */  00226000
*                                                                  0219 00227000
*        DO WHILE CCAFLPT ^= ZERO;      /* CHECK ALL FPLS           */  00228000
         B     @DE00219                                            0219 00229000
@DL00219 DS    0H                                                  0220 00230000
*          IF FIELDNM = 'DEVTYP  ' THEN /* IS THIS A DEVTYPE FPL    */  00231000
         L     @10,CCAFLPT(,R11)                                   0220 00232000
         L     @05,CTGFLDNM(,@10)                                  0220 00233000
         CLC   FIELDNM(8,@05),@CC03985                             0220 00234000
         BNE   @RF00220                                            0220 00235000
*            DO;                        /* TURN OFF NON CATLOGED BIT*/  00236000
*              DEVSHR = OFF;            /* TURN OFF SHARE BIT       */  00237000
*              DEVOPT7 = OFF;           /* TURN OFF OPTION 7        */  00238000
*              DEVOPT8 = OFF;           /* TURN OFF OPTION 8        */  00239000
         L     @05,CTGFLPT(,@10)                                   0224 00240000
         NI    DEVSHR(@05),B'11011100'                             0224 00241000
*              IF CTGFLDCD = CTGFLDEQ THEN /* CHECK IF TEST FPL     */  00242000
         CLI   CTGFLDCD(@10),X'80'                                 0225 00243000
         BNE   @RF00225                                            0225 00244000
*                CONVDEV1 = DEVDATA;    /* SAVE OLD DEVTYPE         */  00245000
         L     @10,CCACMSWA(,R11)                                  0226 00246000
         MVC   CONVDEV1(4,@10),DEVDATA(@05)                        0226 00247000
*            END;                       /* END TURN OFF OPTIONS     */  00248000
@RF00225 DS    0H                                                  0228 00249000
*          CCAFLPT = CTGFLCHN;          /*GET NEXT FPL OR ZERO      */  00250000
@RF00220 L     @10,CCAFLPT(,R11)                                   0228 00251000
         L     @10,CTGFLCHN(,@10)                                  0228 00252000
         ST    @10,CCAFLPT(,R11)                                   0228 00253000
*        END;                           /* END RESET NON CATLGED OPT*/  00254000
*                                                                  0229 00255000
@DE00219 L     @10,CCAFLPT(,R11)                                   0229 00256000
         LTR   @10,@10                                             0229 00257000
         BNZ   @DL00219                                            0229 00258000
*        CCAFLPT = CTGFIELD(1);         /* POINT TO FIRST FPL AGAIN */  00259000
*                                                                  0230 00260000
         L     @10,CCACPL(,R11)                                    0230 00261000
         L     @10,CTGFIELD(,@10)                                  0230 00262000
         ST    @10,CCAFLPT(,R11)                                   0230 00263000
*        CONVDEV2 = DEVDATA;            /* SAVE NEW DEVTYPE         */  00264000
         L     @05,CCACMSWA(,R11)                                  0231 00265000
         L     @10,CTGFLPT(,@10)                                   0231 00266000
         MVC   CONVDEV2(4,@05),DEVDATA(@10)                        0231 00267000
*        CTVDEVTY = DEVDATA;             /* REPLACE THE DEVICE TYPE */  00268000
         L     @01,CCAURAB(,R11)                                   0232 00269000
         L     @15,CCACPE1(,@01)                                   0232 00270000
         MVC   CTVDEVTY+48(4,@15),DEVDATA(@10)                     0232 00271000
*                                        /* DATA IN THE VOLUME RECRD*/  00272000
*                                        /* WITH THE DEVICE TYPE DAT*/  00273000
*                                        /* FROM THE LAST FPL       */  00274000
*                                                                  0233 00275000
*        IF RCDCRAVL = CONVOLSR &        /* CK IF CRA VOL   @Y30LSPS*/  00276000
*           RCDCRADT = CONVDEV1 THEN     /* DEVTYPE MATCH   @Y30LSPS*/  00277000
         CLC   RCDCRAVL(6,@15),CONVOLSR(@05)                       0233 00278000
         BNE   @RF00233                                            0233 00279000
         CLC   RCDCRADT(4,@15),CONVDEV1(@05)                       0233 00280000
         BNE   @RF00233                                            0233 00281000
*          DO;                           /* CRA VOLUME PROCS @Y30LSPS*/ 00282000
*            RCDCRADT = CONVDEV2;        /* CHANGE DEVTYPE   @Y30LSPS*/ 00283000
         MVC   RCDCRADT(4,@15),CONVDEV2(@05)                       0235 00284000
*            CONVCRA = ON;               /* INDICATE CRA VOL @Y30LSPS*/ 00285000
         OI    CONVCRA(@05),B'01000000'                            0236 00286000
*            CONVOLCI = RCDCINO;         /* SAVE VOL CI NO   @Y30LSPS*/ 00287000
         MVC   CONVOLCI(3,@05),RCDCINO(@15)                        0237 00288000
*          END;                          /* END IF CRA VOL   @Y30LSPS*/ 00289000
*                                                                  0238 00290000
*   /* THE UPDATE VOLUME RECORD MUST NOW BE WRITTEN OUT             */  00291000
*                                                                  0239 00292000
*         DO;                                                      0239 00293000
*                                                                  0239 00294000
@RF00233 DS    0H                                                  0240 00295000
*         /* CALL TO MODULE IGG0CLAG                                 */ 00296000
*Z011PUPC:CALL IGGPPUPC;                                           0240 00297000
Z011PUPC L     @15,@CV00039                                        0240 00298000
         BALR  @14,@15                                             0240 00299000
*                                       /* PUT-UPDATE RECORD      */    00300000
*         END;                        /* CALL PUT UPDATE         */     00301000
*                                                                  0241 00302000
*                                                                  0241 00303000
*   /* THE CCR RECORD MUST BE READ IN AND THE CAXNFCI UPDATED       */  00304000
*                                                                  0242 00305000
*         /* CALL TO MODULE IGG0CLAG                                 */ 00306000
*Z012RCCR:CALL IGGPRCCR;                                           0242 00307000
Z012RCCR L     @15,@CV00042                                        0242 00308000
         BALR  @14,@15                                             0242 00309000
*                                        /* READ CCR AND UPDATE CAX  */ 00310000
*                                                                  0243 00311000
*   /* THE CALLER CPL AND ASSOCIATED TEST AND MODIFY FPLS           */  00312000
*   /* ARE SETUP TO CHANGE THE DEVICE TYPE INFORMATION IN ALL       */  00313000
*   /*  RECORDS FOR ALL VOLUME ENTRIES FOR THE VOLUME               */  00314000
*   /* BEING CONVERTED                                              */  00315000
*                                                                  0243 00316000
*        RFY CTGPL BASED(CCAMCPL);                                 0243 00317000
*        CCAMCPL = CCACPL;              /* SET CPL LOCATION         */  00318000
         L     @10,CCACPL(,R11)                                    0244 00319000
         ST    @10,CCAMCPL(,R11)                                   0244 00320000
*        CCAREUSE = ON;                 /* INDICATE RABS REUSEABLE  */  00321000
         OI    CCAREUSE(R11),B'00000100'                           0245 00322000
*        CCAF2NVC = ON;                 /* SET NO VALIDITY CHECK FLG*/  00323000
         OI    CCAF2NVC(R11),B'01000000'                           0246 00324000
*        CCARABB = CCAURAB;             /* SET BASE RECORD RAB ADDR */  00325000
         L     @10,CCAURAB(,R11)                                   0247 00326000
         ST    @10,CCARABB(,R11)                                   0247 00327000
*        CCARABF = ADDR(CCARAB1);       /* SET FIRST RAB TO USE     */  00328000
         LA    @10,CCARAB1(,R11)                                   0248 00329000
         ST    @10,CCARABF(,R11)                                   0248 00330000
*        CCARABL = ADDR(CCARAB3);       /* SET LAST RAB TO USE      */  00331000
         LA    @10,CCARAB3(,R11)                                   0249 00332000
         ST    @10,CCARABL(,R11)                                   0249 00333000
*        CCASCRA = ON;                  /* INHIBIT CRA UPDATE       */  00334000
*                                                                  0250 00335000
*        CCASCRA = OFF;                  /* ALLOW CRA UPDATE @Y30LSPS*/ 00336000
         NI    CCASCRA(R11),B'11011111'                            0251 00337000
*        CCASRCIN = ZERO;                /* START WITH FIRST RECORD */  00338000
         SLR   @10,@10                                             0252 00339000
         STCM  @10,7,CCASRCIN(R11)                                 0252 00340000
*        DO WHILE CCACD1 = RCS;          /* CONTINUE UNTIL EOF      */  00341000
*                                                                  0253 00342000
         B     @DE00253                                            0253 00343000
@DL00253 DS    0H                                                  0254 00344000
*Z013GALO:   CALL IGGPGALO;                                        0254 00345000
Z013GALO BAL   @14,IGGPGALO                                        0254 00346000
*                                        /* CALL GET ALL LOW KEY RNG*/  00347000
*          IF CCACD1 = RCS THEN          /* UPDATE ONLY IF GET OK   */  00348000
         CLI   CCACD1(R11),0                                       0255 00349000
         BNE   @RF00255                                            0255 00350000
*            DO;                         /* MODIFY DEVICE TYPE FIELDS*/ 00351000
*              CONVSRCI = CCASRCIN;      /* SAVE CI NUMBER ACROS MOD */ 00352000
         L     @10,CCACMSWA(,R11)                                  0257 00353000
         SLR   @05,@05                                             0257 00354000
         ICM   @05,7,CCASRCIN(R11)                                 0257 00355000
         ST    @05,CONVSRCI(,@10)                                  0257 00356000
*              IF RCDCRAVL = CONVOLSR & /* CHECK IF CONVERTV CRA VOL*/  00357000
*                 RCDCRADT = CONVDEV1 THEN /* CK SAME DEVTYPE       */  00358000
         L     @05,CCAURAB(,R11)                                   0258 00359000
         L     @15,CCACPE1(,@05)                                   0258 00360000
         CLC   RCDCRAVL(6,@15),CONVOLSR(@10)                       0258 00361000
         BNE   @RF00258                                            0258 00362000
         CLC   RCDCRADT(4,@15),CONVDEV1(@10)                       0258 00363000
         BNE   @RF00258                                            0258 00364000
*                DO;                    /* MUST UPDATE CAR DEVICETYP*/  00365000
*                  RCDCRADT = CONVDEV2; /* CHANGE CRA DEVTYPE       */  00366000
         MVC   RCDCRADT(4,@15),CONVDEV2(@10)                       0260 00367000
*                  CCARBWR = ON;        /* FORCE MODIFY TO WRITE REC*/  00368000
         OI    CCARBWR(@05),B'00010000'                            0261 00369000
*                END;                   /* END IF CHANGE CRA DEVTYPE*/  00370000
*              IF RCDID ^= 'E' &         /* DO NOT CALL MODIFY FOR E*/  00371000
