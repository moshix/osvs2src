*IGGPGALO: PROC OPTIONS(NOSAVEAREA,NOSAVE);                        0364 00765000
IGGPGALO DS    0H                                                  0365 00766000
*                                   /* SPECIFY PROCEDURE OPTIONS */     00767000
*          GEN(ENTRY IGGPGALO );    /* IDENTIFY ENTRY-POINT SYMBOL */   00768000
         ENTRY IGGPGALO                                                 00769000
*Z031:                              /* LABEL FOR PROCEDURE TRACKING  */ 00770000
*Z031GALO:                          /* LABEL FOR PROCEDURE TRACKING  */ 00771000
*          RFY (R5,R6,R7,R8,R9,R10) UNRSTD;                        0366 00772000
Z031     DS    0H                                                  0366 00773000
Z031GALO DS    0H                                                  0367 00774000
*                                   /* PL/S COMPILER WORK REGISTERS */  00775000
*          RFY (R2,R3,R4) RSTD;     /* RESTRICT LOCAL WORK REGISTERS */ 00776000
*          GEN(STM 12,14,12(13));       /* SAVE REGISTERS OF CALLER */  00777000
         STM 12,14,12(13)                                               00778000
*          GEN(LA 13,12(13));           /* POINT TO NEXT SAVE AREA  */  00779000
         LA 13,12(13)                                                   00780000
*          GEN(BALR 12,0);              /* ESTABLISH ADDRESSABILITY */  00781000
         BALR 12,0                                                      00782000
*          GEN(LA 15,*-IGGSEGST(0,0));  /* COMPUTE MODULE BASE      */  00783000
         LA 15,*-IGGSEGST(0,0)                                          00784000
*          GEN(SR 12,15);               /* SET BASE TO MODULE BASE  */  00785000
*                                                                  0372 00786000
         SR 12,15                                                       00787000
*   /* THIS ROUTINE READS ALL LOW KEY RANGE CI RECORDS. IT WILL     */  00788000
*   /* INCREMENT TO THE NEXT EXTENT WHEN ZERO VALUE RECORDS OR NON  */  00789000
*   /* VALID LOW KEY RANGE RECORDS ARE ENCOUNTERED. THIS WILL       */  00790000
*   /* CONTINUE UNTIL THE CI REQUESTED IS THE NEXT NEVER ASSIGNED CI */ 00791000
*   /* VALUE. THIS IS ONE MORE THAN THE HIGHEST VALID LOW KEY RANGE */  00792000
*   /* CI.                                                          */  00793000
*                                                                  0373 00794000
*        IF CCASRCIN ^< CAXNFCI THEN     /* CHECK IS NEXT NEVER ASSGN*/ 00795000
*                                                                  0373 00796000
         L     @10,CCAACB(,R11)                                    0373 00797000
         L     @10,ACBUAPTR(,@10)                                  0373 00798000
         CLC   CCASRCIN(3,R11),CAXNFCI(@10)                        0373 00799000
         BL    @RF00373                                            0373 00800000
*           /* SET CCAMODID='BZ', CCAREASN=RSN4, CCARETRN=RCLEOD     */ 00801000
*           CCAPROB=IGGSEGID+(RSN4*K256)+RCLEOD;                   0374 00802000
         MVC   CCAPROB(4,R11),@CF04046                             0374 00803000
*                                       /* INDICATE EOF IF REACHED 0375 00804000
*                                                          @Y30LSJG*/   00805000
*        ELSE                            /* KEEP GONG. THE END IS NOT*/ 00806000
*        DO;                             /* GET THE NEXT LOW KEY CI */  00807000
         B     @RC00373                                            0375 00808000
@RF00373 DS    0H                                                  0376 00809000
*          CCAF1KEY = OFF;               /* TURN OFF GET BY KEY     */  00810000
         NI    CCAF1KEY(R11),B'11101111'                           0376 00811000
*          CCABZSAV = CCASRCIN;          /* SAVE CI NUMBR ACROS I/O */  00812000
*                                                                  0377 00813000
         SLR   @10,@10                                             0377 00814000
         ICM   @10,7,CCASRCIN(R11)                                 0377 00815000
         ST    @10,CCABZSAV(,R11)                                  0377 00816000
*           /* CALL TO MODULE IGG0CLBI                               */ 00817000
*Z033GET:   CALL IGGPGET;                                          0378 00818000
Z033GET  L     @15,@CV00038                                        0378 00819000
         BALR  @14,@15                                             0378 00820000
*                                        /* GET CI RECORD CCASRCIN  */  00821000
*          CCASRCIN = CCABZSAV;          /* RESTORE CI NUMBER       */  00822000
         MVC   CCASRCIN(3,R11),CCABZSAV+1(R11)                     0379 00823000
*          IF CCACD1 ^= RCS THEN         /* SOME ERRORS ARE VALID   */  00824000
         CLI   CCACD1(R11),0                                       0380 00825000
         BE    @RF00380                                            0380 00826000
*            DO;                         /* CHANGE VALID ERRORS TO OK*/ 00827000
*              IF RCDID = 'L' |          /* CAN GET INDEX EXTENTIONS */ 00828000
*                 RCDID = 'F' THEN       /* CAN GET FREE RECORDS    */  00829000
         L     @10,CCAURAB(,R11)                                   0382 00830000
         L     @10,CCACPE1(,@10)                                   0382 00831000
         CLI   RCDID(@10),C'L'                                     0382 00832000
         BE    @RT00382                                            0382 00833000
         CLI   RCDID(@10),C'F'                                     0382 00834000
         BNE   @RF00382                                            0382 00835000
@RT00382 DS    0H                                                  0383 00836000
*                   /* CLEAR CCAMODID, CCAREASN, AND CCARETRN*/    0383 00837000
*                 CCAPROB=IGNOREIT;       /* CHANNGE ERROR TO VALID*/   00838000
         SLR   @10,@10                                             0383 00839000
         ST    @10,CCAPROB(,R11)                                   0383 00840000
*            END;                        /* END IF NOT ZERO RET CODE*/  00841000
@RF00382 DS    0H                                                  0385 00842000
*          DO WHILE CCACD1 = RCHIGH;     /* RCHIGH IS RETURNED BY GET*/ 00843000
@RF00380 B     @DE00385                                            0385 00844000
@DL00385 DS    0H                                                  0386 00845000
*                                        /* IF THE RCDKEYTY FIELD IS */ 00846000
*                                        /* NOT EQUAL TO ZERO       */  00847000
*            RESPECIFY IDAEDBHD BASED(EDBPTR);                     0386 00848000
*            RESPECIFY IDAEDB BASED(EDBENTRY);                     0387 00849000
*            EDBPTR = ACBAMBL->AMBLDTA->AMBEDB; /* GET DATA EDB     */  00850000
         L     @10,CCAACB(,R11)                                    0388 00851000
         L     @10,ACBAMBL(,@10)                                   0388 00852000
         L     @10,AMBLDTA(,@10)                                   0388 00853000
         L     EDBPTR,AMBEDB(,@10)                                 0388 00854000
*            EDBENTRY = EDBPTR + LENGTH(IDAEDBHD); /* POSITION  HDR */  00855000
         LA    EDBENTRY,8                                          0389 00856000
         ALR   EDBENTRY,EDBPTR                                     0389 00857000
*            DO WHILE CCASRCIN * CILEN > EDBLORBA; /*SEARCH EDB EXTS*/  00858000
         B     @DE00390                                            0390 00859000
@DL00390 DS    0H                                                  0391 00860000
*                                        /* AN RBA HIGHER THAN CURRET*/ 00861000
*                                        /* IS DISCOVERED           */  00862000
*              EDBENTRY = EDBENTRY + EDBLEN; /* GET NEXT EDB EXTENT */  00863000
         AH    EDBENTRY,EDBLEN(,EDBPTR)                            0391 00864000
*            END;                        /* END SEARCH EDB EXTENTS  */  00865000
*                                                                  0392 00866000
@DE00390 SLR   @10,@10                                             0392 00867000
         ICM   @10,7,CCASRCIN(R11)                                 0392 00868000
         SLA   @10,9                                               0392 00869000
