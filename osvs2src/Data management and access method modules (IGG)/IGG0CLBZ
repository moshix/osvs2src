         BNO   @RF00357                                            0357 00733000
*                 /* SET CCAMODID='BZ', CCAREASN=RSN0, CCARETRN=RCNOUCE 00734000
*                                                                    */ 00735000
*                 CCAPROB=IGGSEGID+(RSN0*K256)+RCNOUCEN;           0358 00736000
         MVC   CCAPROB(4,R11),@CF04045                             0358 00737000
*                                              /* NO UCAT ERROR    */   00738000
*            END;                                                  0359 00739000
*                                                                  0359 00740000
@RF00357 DS    0H                                                  0360 00741000
*        /* ANY CORE GOTTEN DURING THE OPERATION OF THIS ROUTINE    */  00742000
*        /* MUST NOW BE FREED                                       */  00743000
*                                                                  0360 00744000
*        /* HOPEFULLY EVERYTHING HAS NOW BEEN CONVERTED AND CONTROL */  00745000
*        /* CAN NOW RETURN TO THE CALLER                            */  00746000
*                                                                  0360 00747000
*                                                                  0360 00748000
*          CALL EXIT;                   /* RETURN TO CALLER         */  00749000
@RF00329 BAL   @14,EXIT                                            0360 00750000
*          GEN(DS 0F);              /* FULLWORD BOUNDARY ALIGNMENT */   00751000
         DS 0F                                                          00752000
*          GEN(DC CL8'IGGPGALO');   /* NAME OF FOLLOWING PROCEDURE */   00753000
         DC CL8'IGGPGALO'                                               00754000
*          END;                         /* END OF INNER PROCEDURE   */  00755000
