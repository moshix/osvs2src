         BR    R14                RETURN                                49800002
ROUT3    ST    R3,XSAVE           SAVE REG 3                            49860002
         TM    0(R4),HEX80        TEST SHORT FORM RECORD SWITCH         49920002
         BO    ROUT3A             BRANCH IF SHORT FORM                  49980002
         SR    R15,R15            DON'T PRINT LAST LINE                 50040002
ROUT3A   L     R3,XSAVE           RESTORE REG 3                         50100002
         BR    R14                RETURN                                50160002
RETURN   LM    R5,R8,XSAVE         RESTORE WORK REGISTERS               50220002
         BR    R14                 RETURN                               50280002
