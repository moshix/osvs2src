         MACRO                                                          12750000
&NAME    DSGEN                                                          12780000
.*                            THIS MACRO GENERATES ONE DSECT WORD       12810000
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 12840000
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  12870000
.*                                 FIRST IS THE NAME OF THE LOG ITEM    12900000
.*                                 WHILE THE SECOND IS THE # OF BITS TO 12930000
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   12960000
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 12990000
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      13020000
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  13050000
.*                                 THE LOG ITEMS MAY THEN BE USED IN    13080000
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   13110000
         GBLC &ITEM(3000)                                               13140000
         GBLA &ITEMNBR,&BITS(3000),&SHIFT(3000)                         13170000
         LCLB  &ONLY1,&CTIND                                            13200000
         LCLA  &START1,&START2,&END1,&END2                              13230000
         LCLC  &SYMBOL,&EQUSYM                                          13260000
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 13290000
         ACTR  100000                                                   13320000
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            13350000
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     13380000
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   13410000
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        13440000
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            13470000
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             13500000
         AGO   .SET2A                   BRANCH AROUND                   13530000
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              13560000
         MEXIT                                                          13590000
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X13620000
               ).SET2                   TEST FOR NO SECOND              13650000
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   13680000
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        13710000
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            13740000
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              13770000
         AGO   .LOOP                                                    13800000
.SET2    ANOP                                                           13830000
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 13860000
         AGO   .LOOP                                                    13890000
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             13920000
         MEXIT                                                          13950000
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               13980000
         MEXIT                                                          14010000
.LOOP    ANOP                                                           14040000
&OP      SETA  &OP+1                                                    14070000
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           14100000
&ITEMNBR SETA  &ITEMNBR+1                                               14130000
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            14160000
         MNOTE '**NULL BIT COUNT FIELD**'                               14190000
         MEXIT                                                          14220000
.OK1     ANOP                                                           14250000
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        14280000
         AIF   ('&SYMBOL' NE '').OK                                     14310000
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         14340000
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         14370000
         AGO   .START                   GET NEXT                        14400000
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        14430000
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          14460000
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        14490000
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         14520000
         AGO   .NO3                     BRANCH AROUND                   14550000
.OK2     ANOP                                                           14580000
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              14610000
.NO3     ANOP                                                           14640000
&SYMBOL  EQU   *+&BYTE                                                  14670000
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             14700000
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    14730000
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    14760000
.CNT     ANOP                                                           14790000
&CTIND   SETB  1                        SET COUNT INDICATOR             14820000
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   14850000
&BYTE    SETA  &COUNT/8                                                 14880000
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        14910000
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        14940000
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             14970000
&END1    SETA  &END1+1                  INCREMENT COUNT                 15000000
&OP      SETA  &OP-1                    DECREMENT INCREMENT             15030000
         AGO   .LOOP                    GENERATE NEXT                   15060000
.TSTST   ANOP                                                           15090000
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               15120000
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        15150000
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      15180000
.NSET1   ANOP                                                           15210000
&OP      SETA  &OP-1                    DECREMENT INDEX                 15240000
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           15270000
         AGO   .LOOP                    GO DO NEXT                      15300000
.LPDONE  ANOP                                                           15330000
&ONLY1   SETB  0                        CLEAR INDICATOR                 15360000
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      15390000
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 15420000
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               15450000
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             15480000
&NAME    DS    &COUNT.C                                                 15510000
.END     MEXIT                                                          15540000
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          15570000
         MEXIT                                                          15600000
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   15630000
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   15660000
         MEXIT                                                          15690000
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         15720000
*****               THE FOLLOWING STATEMENT                *****        15750000
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        15780000
.EQU2    ANOP                                                           15810000
&EQUCNT  SETA  0                        CLEAR COUNTER                   15840000
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 15870000
.EQULOP  ANOP                                                           15900000
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 15930000
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     15960000
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                15990000
         AGO   .EQULOP                  NO-GET NEXT                     16020000
.EQUOUT  ANOP                                                           16050000
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  16080000
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              16110000
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 16140000
