         TITLE '****  2880C EDIT  ****'                                 00060020
         MACRO                                                          00090020
         PARITY &ENTRY,&REPEAT,&SPACES,&PBITS=,&DISP=,&OFFBYTE=         00120020
         GBLC  &ITEM(1500),&DSECT                                       00150020
         GBLA  &SHIFT(1500),&ITEMNBR                                    00180020
         LCLA  &IBITS,&NDX,&ISHIFT                                      00210020
         DC    AL4(&ENTRY)                                              00240020
         DC    AL1(&REPEAT)                                             00270020
         DC    AL1(&SPACES)                                             00300020
         AIF   ('&PBITS' EQ '').HARDCD                                  00330020
.LOOP    ANOP                                                           00360020
&NDX     SETA  &NDX+1                                                   00390020
         AIF   ('&ITEM(&NDX)' EQ '&PBITS').FOUND                        00420020
         AIF   (&NDX LT &ITEMNBR).LOOP                                  00450020
         MNOTE *,'PARITY GROUP NOT FOUND'                               00480020
         MEXIT                                                          00510020
.FOUND   ANOP                                                           00540020
&ISHIFT  SETA  &SHIFT(&NDX)                                             00570020
         DC    AL.12(&PBITS-&DSECT),AL.4(&ISHIFT)                       00600020
         AGO   .OUT                                                     00630020
.HARDCD  ANOP                                                           00660020
&ISHIFT  SETA  &OFFBYTE                                                 00690020
         DC    AL.12(&DISP),AL.4(&ISHIFT)                               00720020
.OUT     ANOP                                                           00750020
         MEND                                                           00780020
         SPACE 2                                                        00810020
         EJECT                                                 @YM2954P 00840002
         MACRO                                                          00860000
         SYSRELN  &LOGITEM,&SKIP$OR,&BYTES                              00880000
.*                            THIS MACRO FACILITATES GENERATION OF THE  00900000
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    00920000
.*                                 CONVERSIONS.                         00940000
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        00960000
.*                                 CONVERTED                            00980000
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    01000000
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   01020000
.*                                 BACK UP FROM COLUMN FOR START OF     01040000
.*                                 PRINTING. THE LETTER B PRECEEDS THE  01060000
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  01080000
.*                                 DESIRED. OMISSION OF THIS OPERAND    01100000
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  01120000
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  01140000
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 01160000
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    01180000
.*                                 IMPLICITLY B).                       01200000
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   01220000
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     01240000
.*                                 WITH AN L,R, OR B TO STIPULATE       01260000
.*                                 PRINTING OF THE LEFT HALF, RIGHT     01280000
.*                                 HALF, OR BOTH HALVES OF THE BYTE     01300000
.*                                 RESPECTIVELY. OMISSION OF THIS       01320000
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  01340000
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 01360000
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 01380000
.*                                 SARY. THIS MACRO MUST BE THE FIRST   01400000
.*                                 MACRO INSTRUCTION ISSUED FOR THE     01420000
.*                                 SUBJECT LINE(S).                     01440000
         GBLC &ITEM(1500),&DSECT                                        01460000
         GBLA &ITEMNBR,&BITS(1500),&SHIFT(1500),&NRENTS                 01480000
         LCLB  &B                                                       01500000
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  01520000
         LCLC  &S,&LETTER                                               01540000
.LOOP    ANOP                                                           01560000
&NDX     SETA  &NDX+1                                                   01580000
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      01600000
         AIF   (&NDX LT &ITEMNBR).LOOP                                  01620000
         MNOTE *,'LOG ITEM NOT DEFINED'                                 01640000
         MEXIT                                                          01660000
.FOUND   ANOP                                                           01680000
&ISHIFT  SETA  &SHIFT(&NDX)                                             01700000
&IBITS   SETA  &BITS(&NDX)                                              01720000
&CODE    SETA  1                                                        01740000
&K       SETA  K'&BYTES                                                 01760000
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            01780000
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   01800000
         MEXIT                                                          01820000
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 01840000
&LETTER  SETC  '&BYTES'(&K,1)                                           01860000
&S       SETC  '&BYTES'(1,&K-1)                                         01880000
&BYTESP  SETA  &S                                                       01900000
         AIF   ('&LETTER' EQ 'B').BOTH                                  01920000
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       01940000
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            01960000
         MEXIT                                                          01980000
.USEDEFN ANOP                                                           02000000
&BYTESP  SETA  &IBITS/8                                                 02020000
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       02040000
&CODE    SETA  3                                                        02060000
&T       SETA  &BYTESP*2-1                                              02080000
&S       SETC  '&T'                                                     02100000
.OK2     AIF   (&ISHIFT EQ 0).L                                         02120000
&CODE    SETA  2                                                        02140000
.L       ANOP                                                           02160000
&B       SETB  1                                                        02180000
         AIF   ('&SKIP$OR' EQ '').BACKUP                                02200000
&K       SETA  K'&SKIP$OR                                               02220000
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 02240000
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   02260000
.BACKUP  ANOP                                                           02280000
&NRENTS  SETA  &NRENTS+1                                                02300000
         DC    AL.12(&LOGITEM-&DSECT),XL.4'7'                           02320000
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              02340000
.OUT     ANOP                                                           02360000
         MEND                                                           02380000
         SPACE 2                                                        13020020
*  DSGEN MACRO     DSGEN MACRO     DSGEN MACRO     DSGEN MACRO        * 13050000
