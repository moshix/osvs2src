         MACRO                                                          13050200
&NAME    DSGEN &X1,&X2,&X3,&X4,&X5,&X6,&X7,&X8,&X9,&X10,&X11,&X12,&X13,X13050300
               &X14,&X15,&X16,&X17,&X18,&X19,&X20,&X21,&X22,&X23,&X24, X13050400
               &X25,&X26,&X27,&X28,&X29,&X30,&X31,&X32                  13050500
.*                            THIS MACRO GENERATES ONE DSECT WORD       13050600
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 13050700
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  13050800
.*                                 FIRST IS THE NAME OF THE LOG ITEM    13050900
.*                                 WHILE THE SECOND IS THE # OF BITS TO 13051000
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   13051100
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 13051200
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      13051300
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  13051400
.*                                 THE LOG ITEMS MAY THEN BE USED IN    13051500
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   13051600
         GBLC &ITEM(1500)                                               13051700
         GBLA &ITEMNBR,&BITS(1500),&SHIFT(1500)                         13051800
         LCLC  &SYMBOL                                                  13051900
         LCLA  &OP,&COUNT,&BYTE                                         13052000
.LOOP    ANOP                                                           13052100
&OP      SETA  &OP+1                                                    13052200
&ITEMNBR SETA  &ITEMNBR+1                                               13052300
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        13052400
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            13052500
         MNOTE *,'NULL BIT COUNT FIELD'                                 13052600
         MEXIT                                                          13052700
.OK1     ANOP                                                           13052800
         AIF   ('&SYMBOL' NE '').OK                                     13052900
         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           13053000
         AGO   .CNT                                                     13053100
.OK      ANOP                                                           13053200
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       13053300
&SYMBOL  EQU   *+&BYTE                                                  13053400
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 13053500
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    13053600
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    13053700
.CNT     ANOP                                                           13053800
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   13053900
&BYTE    SETA  &COUNT/8                                                 13054000
         AIF (&OP LT N'&SYSLIST).LOOP                                   13054100
         AIF   (&COUNT NE 32).NOT32                                     13054200
&NAME    DS    4C                                                       13054300
         MEXIT                                                          13054400
.NOT32   AIF   (&COUNT NE 64).ERR                                       13054500
&NAME    DS    8C                                                       13054600
         MEXIT                                                          13054700
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             13054800
         MEND                                                           13054900
         EJECT                                                          13055000
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 13055100
         SPACE 2                                                        13055200
         MACRO                                                          13055300
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      13055400
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     13055500
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    13055600
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 13055700
.*                                 ENTRIES. LASTLY IT DETERMINES THE    13055800
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    13055900
.*                                 THE PREVIOUS LINE AND STORES THAT #  13056000
.*                                 INTO THE APPROPRIATE BYTE.           13056100
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    13056200
.*                                 DATA.                                13056300
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 13056400
.*                                 DATA COLUMN STARTING CHARACTERS.     13056500
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   13056600
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 13056700
.*                                 STARTING CHARACTERS.                 13056800
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   13056900
.*                                 TO BEGIN NEXT PRINTED LINE.          13057000
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       13057100
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   13057200
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 13057300
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   13057400
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 13057500
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    13057600
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   13057700
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   13057800
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  13057900
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   13058000
.*                                 THE SUBJECT LINE                     13058100
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  13058200
         GBLA  &LASTCC,&INITIAL                                         13058300
         GBLC  &NRENTSX                                                 13058400
         GBLC  &CARRCON                                                 13058500
         GBLB  &TEST                                                    13058600
         LCLA  &S                                                       13058700
&NAME    DS    0C                                                       13058800
         AIF   (&INITIAL EQ 0).FIRSTLN                                  13058900
         LINEND                                                         13059000
         AGO   .NTFIRST                                                 13059100
.FIRSTLN ANOP                                                           13059200
&NRENTSX SETC  'N&SYSNDX'                                               13059300
&INITIAL SETA  1                                                        13059400
N&SYSNDX EQU   *                                                        13059500
.NTFIRST ANOP                                                           13059600
         DC    AL1(0)                                                   13059700
         AIF   ('&DATA(1)' EQ '').CKDTABS                               13059800
         AIF   (&DATA(1) EQ &DB).CKDTABS                                13059900
&DB      SETA  &DATA(1)                                                 13060000
         AGO   .AA                                                      13060100
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                13060200
         AIF   (&DATA(2) EQ &DT).NCDATA                                 13060300
         AGO   .CC                                                      13060400
.AA      AIF   ('&DATA(2)' EQ '').BB                                    13060500
         AIF   (&DATA(2) EQ &DT).BB                                     13060600
.CC      ANOP                                                           13060700
&DT      SETA  &DATA(2)                                                 13060800
.BB      DC    XL2'FFFE'                                                13060900
         DC    FL1'&DB'                                                 13061000
         DC    FL1'&DT'                                                 13061100
&NRENTS  SETA  &NRENTS+1                                                13061200
.NCDATA  ANOP                                                           13061300
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              13061400
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               13061500
&LB      SETA  &LABEL(1)                                                13061600
         AGO   .AAA                                                     13061700
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              13061800
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               13061900
         AGO   .CCC                                                     13062000
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  13062100
         AIF   (&LABEL(2) EQ &LT).BBB                                   13062200
.CCC     ANOP                                                           13062300
&LT      SETA  &LABEL(2)                                                13062400
.BBB     DC    XL2'FFFF'                                                13062500
         DC    FL1'&LB'                                                 13062600
         DC    FL1'&LT'                                                 13062700
&NRENTS  SETA  &NRENTS+1                                                13062800
.NCLABEL ANOP                                                           13062900
         AIF   ('&SKIP' NE '').CONT                                     13063000
         AIF   (&TEST EQ 0).NOCHANG                                     13063100
&S       SETA  &LASTCC                                                  13063200
         AGO   .DEFAULT                                                 13063300
.CONT    ANOP                                                           13063400
         AIF   (&TEST EQ 0).AB                                          13063500
&S       SETA  &SKIP                                                    13063600
.DEFAULT ANOP                                                           13063700
         ORG   &CARRCON+3                                               13063800
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 13063900
         ORG                                                            13064000
&LASTCC  SETA  &S                                                       13064100
&TEST    SETB  0                                                        13064200
         AGO   .DD                                                      13064300
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               13064400
A&SYSNDX DC    XL2'FFFB'                                                13064500
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              13064600
         DC    FL1'9'                                                   13064700
&LASTCC  SETA  &SKIP                                                    13064800
&TEST    SETB  1                                                        13064900
