         MACRO                                                          13081400
&NAME    LABEL &LABEL1,&LABEL2,&LABEL3,&LABEL4,&LABEL5,&LABEL6,        C13081500
               &LABEL7,&LABEL8                                          13081600
.*                            THIS MACRO ALLOWS UP TO ACHT LABELS TO    13081700
.*                                 BE SPECIFIED FOR A GIVEN LINE. THE   13081800
.*                                 LABELS SHOULD BE ENCLOSED IN QUOTA-  13081900
.*                                 TION MARKS. THE COLUMN AND LINE END  13082000
.*                                 CONTROLS WILL BE INSERTED BY THE     13082100
.*                                 MACRO. OMITTED OPERANDS MEAN NO      13082200
.*                                 LABEL REQUIRED FOR RESPECTIVE        13082300
.*                                 COLUMN. THIS MACRO WILL ALSO ALERT   13082400
.*                                 THE PROGRAMMER TO EXACTLY WHERE THE  13082500
.*                                 TABLE MUST BE BROKEN IF NECESSARY    13082600
.*                                 TO AVOID EXCEEDING THE MAXIMUM LIMIT 13082700
         GBLC &LABELS(2000)                                             13082800
         GBLC  &LBL(100)                                                13082900
         GBLA  &LBLCTR(100)                                             13083000
         GBLA  &LABELNR                                                 13083100
         GBLA  &NRENTS,&LPTR                                            13083200
         LCLC  &MARGIN,&TACK                                            13083300
         LCLC  &TEXT                                                    13083400
         LCLA  &OP,&K,&START,&NRCHAR,&REMAIN                            13083500
&MARGIN  SETC  '*'                                                      13083600
         AIF   ('&NAME' EQ '').LOOPOUT                                  13083700
&LPTR    SETA  &LPTR+1                                                  13083800
&LBL(&LPTR) SETC '&NAME'                                                13083900
&LBLCTR(&LPTR) SETA &LABELNR+1                                          13084000
.LOOPOUT ANOP                                                           13084100
&OP      SETA  &OP+1                                                    13084200
&K       SETA  K'&SYSLIST(&OP)                                          13084300
         AIF (&K GE 2).NORM                                             13084400
&K       SETA  2                                                        13084500
.NORM    ANOP                                                           13084600
&START   SETA  2                                                        13084700
         AIF (&OP LT N'&SYSLIST).NOTLAST                                13084800
&MARGIN  SETC  '$'                                                      13084900
.NOTLAST ANOP                                                           13085000
&NRCHAR  SETA  8                                                        13085100
&TACK    SETC  ''                                                       13085200
.LOOPIN  ANOP                                                           13085300
&REMAIN  SETA  &K-&START                                                13085400
         AIF (&REMAIN GE 8).MORE                                        13085500
&TACK    SETC  '&MARGIN'                                                13085600
&NRCHAR  SETA  &REMAIN                                                  13085700
.MORE    ANOP                                                           13085800
&TEXT    SETC  ''                                                       13085900
         AIF (&REMAIN EQ 0).ENDTEXT                                     13086000
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          13086100
.ENDTEXT ANOP                                                           13086200
&LABELNR SETA  &LABELNR+1                                               13086300
&LABELS(&LABELNR) SETC '&TEXT'.'&TACK'                                  13086400
&START   SETA  &START+8                                                 13086500
         AIF ('&TACK' EQ '').LOOPIN                                     13086600
         AIF ('&TACK' EQ '*').LOOPOUT                                   13086700
         MEND                                                           13086800
         SPACE 2                                                        13086900
         MACRO                                                          13087000
         LSTART                                                         13087100
.*                            THIS MACRO IS ISSUED WHEN THE LABEL AREA  13087200
.*                                 OF THE TABLE IS TO BEGIN.            13087300
         GBLC &LABELS(2000)                                             13087400
         GBLA  &LABELNR                                                 13087500
         GBLC  &LBL(100)                                                13087600
         GBLA  &LBLCTR(100)                                             13087700
         LCLA  &NDX,&CTR                                                13087800
         LINEND                                                         13087900
&CTR     SETA  1                                                        13088000
.LOOP    ANOP                                                           13088100
&NDX     SETA  &NDX+1                                                   13088200
         AIF   (&LBLCTR(&CTR) NE &NDX).NOLBL                            13088300
&LBL(&CTR) DC  C'&LABELS(&NDX)'                                         13088400
&CTR     SETA  &CTR+1                                                   13088500
         AGO   .LOOPCTL                                                 13088600
.NOLBL   ANOP                                                           13088700
         DC    C'&LABELS(&NDX)'                                         13088800
.LOOPCTL AIF   (&NDX LT &LABELNR).LOOP                                  13088900
         MEND                                                           13089000
         SPACE 2                                                        13089100
         MACRO                                                          13089200
         LINEND                                                         13089300
.*                            THIS MACRO IS USED ONLY BY THE LINE       13089400
.*                                 MACRO TO SET THE NUMBER OF ENTRIES   13089500
.*                                 ASSOCIATED WITH EACH LINE.           13089600
         GBLA  &NRENTS                                                  13089700
         GBLC &NRENTSX                                                  13089800
         ORG   &NRENTSX                                                 13089900
         DC    FL1'&NRENTS'                                             13090000
         ORG                                                            13090100
&NRENTS  SETA  0                                                        13090200
B&SYSNDX EQU   *                                                        13090300
&NRENTSX SETC  'B&SYSNDX'                                               13090400
         MEND                                                           13090500
         SPACE 2                                                        13090600
