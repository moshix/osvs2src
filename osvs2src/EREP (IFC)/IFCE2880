IFCASR01 DS    0H                                                       38610020
         USING *,R15                                                    38640020
         STM   R14,R12,12(R13)          SAVE REGISTERS                  38670020
         CLI   XKEY,HEX21                                      @YL02021 38700002
         BNE   CONTIN                                          @YL02021 38707002
         OI    VS2FLG,X01                                      @YL02021 38714002
CONTIN   ST    R13,SVR13               SAVE REG 13             @YL02021 38721002
         L     R11,NXENTRY              LOAD SUBROUTINE RETURN ADR      38730020
         LTR   R11,R11                  TEST FOR INITIAL ENTRY          38760020
         BZ    IFCASR02                 BRANCH IF 0-INITIAL ENTRY       38790020
         LM    R12,R10,MYSAVE           RELOAD SUBROUTINE REGS          38820020
         BR    R11                      CONTINUE PROCESSING             38850020
IFCASR02 DS    0H                                                       38880020
         SH    R3,TWOX                  SUBTRACT 2 FROM PRNT BFR ADR    38910020
         MVC   L0(L1,R3),L1(R3)         MOVE PRINTER CNTRL CHAR         38940020
         LA    R3,L1(R3)                INCREMENT PRNT BFR ADR          38970020
         USING XDADBUF,R4                                               39000020
         USING XPRTBUF,R3                                               39030020
         USING XKEYBUF,R2                                               39060020
NEWMOVE  CLI   XKEYBUF+1,REL19          RELEASE 19                19E03 39090020
         BE    NEWMOVE1                 BRANCH--YES               19E03 39120020
         MVC   CSWORD(8),XDADBUF+54     MOVE CSW                  19E03 39150020
         MVC   ERPIBFLD(4),XDADBUF+62   MOVE ERPIB                19E03 39180020
         MVC   MULTISYS(4),XDADBUF+74   MOVE CHANSTAT INFO        19E03 39210020
         B     CONTINUE                 CONTINUE                  19E03 39240020
NEWMOVE1 MVC   CSWORD(8),XDADBUF+46     MOVE CSW                  19E03 39270020
         MVC   ERPIBFLD(4),XDADBUF+54   MOVE ERPIB                19E03 39300020
         MVC   MULTISYS(4),XDADBUF+66   MOVE CHANSTAT INFO        19E03 39330020
CONTINUE LA    R9,ERPIBFLD              DSECT ADDRESSIBILITY      19E03 39360020
         USING ERPIB,R9                                                 39390020
         MVI   SETUPBFR+1,C'*'                                     MP   39420020
         MVC   SETUPBFR+2(78),SETUPBFR+1                           MP   39450020
         BAL   BRANPRT,XWRTPRT    MOVE BORDER TO BUFFER AND WRITE  MP   39480020
         L     R6,VS2                                          @YL02021 39487002
         CLI   0(R6),X'01'                                     @YL02021 39494002
         BE    DUMMYTST                                        @YL02021 39501002
         MVI   SETUPBFR,C'-'           SKIP TWO SPACES             MP   39510020
         TM    MPINFOFD,MSBIT      IS MULTI-SYSTEM BIT SET        19032 39540020
         BZ    DUMMYTST      IFNOT,CKECK FOR ENTRANCE FROM ASR01   MP   39570020
         LA    SETUPADR,SETUPBFR       ADDRESS OF PRINT BUFFER     MP   39600020
         MVC   24(24,SETUPADR),MSHEAD   MOVE MULTI-SYSTEM HDR TO BUFMP  39630020
         BAL   BRANPRT,XWRTPRT     WRITE HEADER                    MP   39660020
         MVI   SETUPBFR+1,C'*'                                     MP   39690020
         MVI   VDSTAR,C'*'                                         MP   39720020
         BAL   BRANPRT,XWRTPRT     WRITE LINE DIVIDER              MP   39750020
*                                                                  MP   39780020
*                                                                  MP   39810020
*              THIS ROUTINE FORMATS AND PRINTS A MESSAGE           MP   39840020
*              CONTAINING THE ID OF THE FAILING CPU AND            MP   39870020
*              THE STATUS OF THE MULTI-PROCESSING SYSTEM           MP   39900020
*                                                                  MP   39930020
*                                                                  MP   39960020
         MVC   10(12,SETUPADR),MSCPUID  MOVE CPUID MESSAGE TO BUFFERMP  39990020
         MVC   30(15,SETUPADR),MSCPUST  MOVE CPU STATUS MESSAGE    MP   40020020
         MVI   0(SETUPADR),C'-'    SKIP TWO SPACES AFTER LINE      MP   40050020
         TM    MPINFOFD,ID         TEST FOR WHICH CPU             19032 40080020
         BZ    STATEST       SKIP CPU ID                           MP   40110020
