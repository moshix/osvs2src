*********************************************************************** 19530020
* PARITY CHECKING ROUTINE                                               19560020
*     R0-NOT USED                                                       19590020
*     R1-NOT USED                                                       19620020
*     R2-ADDR OF TOP OF LOGREC,LOOP COUNTER                             19650020
*     R3-POINTS TO CURRENT MARGIN IN PRINT BUFFER                       19680020
*     R4-ADDRESS OF DATA(=ARG1 OF ROUTINE MACRO)                        19710020
*     R5-WORK REG                                                       19740020
*     R6-USED TO COMPUTE PARITY                                         19770020
*     R7-CONTAINS PARITY BITS                                           19800020
*     R8-CONTAINS PRINT SPACING                                         19830020
*     R9-POINTS TO CURRENT EDIT TABLE ENTRY(=LABEL OF ROUTINE MACRO)    19860020
*    R10-WORK REG                                                       19890020
*    R11-WORK REG                                                       19920020
*    R12-CONTAINS NO. OF BYTES TO PROCESS                               19950020
*    R13-BASE REG                                                       19980020
*    R14-LINK REGS                                                      20010020
*    R15-LINK REGS                                                      20040020
@PARITY  DS    0F                                                       20070020
         USING *,15                                                     20100020
         STM   14,12,12(13)                                             20130020
         ST    13,SAVE13                                                20160020
* FIND ARG LIST *                                                       20190020
         L     9,R9ADR                                                  20220020
         L     9,0(9)              PICK UP ADR OF EDIT ENTRY            20250020
         LA    5,ARGTBL-8          INITIALIZE R5                        20280020
LOOP1    LA    5,8(5)              BUMP R5 TO NEXT ARG LIST             20310020
         L     6,0(5)              LOAD ADDR OF EDIT ENTRY-             20340020
         CR    9,6                 =TO ADDR OF ROUTINE MACRO            20370020
         BC    7,LOOP1             NO,LOOK AT NEXT ARG LIST             20400020
*                                  YES,EXTRACT DATA FROM ARG LIST       20430020
         XR    12,12               CLEAR                                20460020
         IC    12,4(5)             REPEAT FACTOR(NO. BYTES TO PROCESS)  20490020
         XR    8,8                 CLEAR                                20520020
         IC    8,5(5)              PRINT SPACING BETWEEN BYTES          20550020
         LH    10,6(5)             PARITY DISP. AND BIT OFFSET          20580020
         SRDL  10,4                DISP R.J. IN R10,OFFSET L.J. IN R11  20610020
         SRL   11,28               R.J. OFFSET                          20640020
         IC    7,0(2,10)           C(R7)=PARITY BITS                    20670020
         SLL   7,8                 MOVE OVER                            20700020
         IC    7,1(2,10)           MORE PARITY BITS(MAYBE)              20730020
         SLL   7,16(11)            SLL 16 PLUS BIT OFFSET               20760020
         XR    2,2                 CLEAR R2                             20790020
         XR    10,10               CLEAR                                20820020
         DROP  15                                                       20850020
PARCOM   EQU   *                                                        20880020
         BALR  13,0                SET UP NEW BASE REG                  20910020
         USING *,13                                                     20940020
* CONV AND STORE DATA *                                                 20970020
LOOP2    IC    10,0(4,2)           LOAD BYTE OF DATA                    21000020
         SRDL  10,4                SPLIT HEX DIGITS                     21030020
         IC    10,CONVTBL(10)      CONV LEFT DIGIT TO EBCDIC            21060020
         STC   10,2(3)             STORE IN PRINT BUFFER                21090020
         XR    10,10               CLEAR                                21120020
         SLDL  10,4                RIGHT DIGIT TO R10                   21150020
         IC    10,CONVTBL(10)      CONVERT                              21180020
         STC   10,3(3)             STORE                                21210020
         XR    6,6                 CLEAR                                21240020
         SLDL  6,1                 PAR BIT TO R6                        21270020
         IC    10,CONVTBL(6)       CONVERT                              21300020
         STC   10,0(3)             STORE                                21330020
* CHECK PARITY *                                                        21360020
         SLL   6,8                 MOVE OVER PAR BIT                    21390020
         IC    6,0(4,2)            INSERT DATA                          21420020
         SLL   6,23                LEFT JUSTIFY                         21450020
         XR    10,10               CLEAR                                21480020
         LA    R5,9                     LOAD LOOP COUNTER               21510020
ALR      ALR   6,6                 SET C.C.                             21540020
         BC    12,LC               BRANCH ON NO CARRY                   21570020
         LA    10,1(10)            COUNT CARRIES IN R10                 21600020
LC       BCT   5,ALR               LOOP                                 21630020
         SRDL  10,1                LOW ORDER BIT TO R11                 21660020
         ALR   11,11               WAS COUNT ODD OR EVEN (BIT 0=0 OR 1) 21690020
         BC    3,GOODPAR           ODD,PARITY GOOD                      21720020
         MVI   1(3),C'*'           EVEN,STORE FLAG                      21750020
GOODPAR  LA    2,1(2)              BUMP DATA XR                         21780020
         AR    3,8                 BUMP PRINT LINE POINTER              21810020
         BCT   12,LOOP2            LOOP BACK                            21840020
         L     13,SAVE13           FINISHED                             21870020
         LM    14,12,12(13)                                             21900020
         BR    14                                                       21930020
         DROP  13                                                       21960020
R9ADR    DC    A(REG9)                                                  21990020
SAVE13   DS    1F                                                       22020020
CONVTBL  DC    C'0123456789ABCDEF' HEX TO EBCDIC CONVERSION TABLE       22050020
ARGTBL   DS    0D                                                       22080020
         SPACE 2                                                        22110020
