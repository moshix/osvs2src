*   ROUTINE TO PRINT CHANNEL OPERATION                                * 35460020
START    DS    0H                                                       35490020
         USING *,R15                                                    35520020
         MVC   AD0(L40,R3),STMSG0       MOVE CHN OP MESSAGE BASE        35550020
         TM    AD12(R4),X20             TEST FOR SIO MAJOR OP           35580020
         BO    STSIO                    BRANCH IF TRIGGER SET           35610020
         TM    AD12(R4),X10             TEST FOR TIO MAJOR OP           35640020
         BO    STTIO                    BRANCH IF TRIGGER SET           35670020
         TM    AD12(R4),X08             TEST FOR HIO MAJOR OP           35700020
         BO    STHIO                    BRANCH IF TRIGGER SET           35730020
         TM    AD13(R4),X20             TEST FOR INTERRUPT RESPONSE     35760020
         BO    STINTRSP                 BRANCH IF TRIGGER SET           35790020
         TM    AD12(R4),X01             TEST FOR CDA MAJOR OP           35820020
         BO    STCDA                    BRANCH IF TRIGGER SET           35850020
         TM    AD12(R4),X04             TEST FOR DATA TXFR MAJOR OP     35880020
         BO    STDTXFR                  BRANCH IF TRIGGER SET           35910020
         TM    AD12(R4),X02             TEST FOR COMND CHAIN RESELCT    35940020
         BO    STCCRSLT                 BRANCH IF TRIGGER SET           35970020
         TM    AD12(R4),X80             TEST FOR SCAN MAJOR OP          36000020
         BO    STSCAN                   BRANCH IF TRIGGER SET           36030020
         TM    AD12(R4),X40             TEST FOR CU REQ MAJOR OP        36060020
         BO    STCUREQ                  BRANCH IF TRIGGER SET           36090020
         MVC   AD0(L40,R3),STMSG1       MOVE NO CHN OP MESSAGE          36120020
         BR    R14                     RETURN                           36150020
STSIO    DS    0C                                                       36180020
         MVC   AD41(L3,R3),STMSG7      MOVE SIO BASE MESSAGE            36210020
         TM    AD16(R4),X30            TEST FOR CTRL FTH OR INSP SEQ    36240020
         BC    CC7,STSIO0              BRANCH IF ANY TRIGGER SET        36270020
         TM    AD20(R4),X30            TEST CTRL FTH OR INSP END SEQ    36300020
         BCR   CC8,R14                 RETURN IF BOTH TRIGGERS OFF      36330020
STSIO0   DS    0C                                                       36360020
         MVC   AD45(L9,R3),STMSG8       MOVE CCW FETCH MESSAGE          36390020
         TM    AD14(R4),X80             TEST FOR CCW FETCH              36420020
         BCR   CC1,R14                  RETURN IF TRIGGER SET           36450020
         MVC   AD45(L3,R3),STMSG9       MOVE CAW TO MESSAGE             36480020
         BR    R14                      RETURN                          36510020
STTIO    DS    0C                                                       36540020
         MVC   AD41(L3,R3),STMSGA       MOVE TIO TO MESSAGE             36570020
         TM    AD16(R4),X08             TEST FOR INITIAL SELECTION      36600020
         BO    STTIO0                   BRANCH IF TRIGGER SET           36630020
         TM    AD15(R4),X80             TEST FOR COMMAND OUT SENT       36660020
         BCR   CC8,R14                  RETURN IF TRIGGER RESET         36690020
STTIO0   DS    0C                                                       36720020
         MVC   AD45(L22,R3),STMSGB      MOVE REMAINDER OF MESSAGE       36750020
         BR    R14                      RETURN                          36780020
STHIO    DS    0C                                                       36810020
         MVC   AD41(L8,R3),STMSGH       MOVE HIO TO MESSAGE             36840020
         TM    AD108(R4),X02            TEST BUSY TRIGGER               36870020
         BCR   CC1,R14                  RETURN IF TRIGGER SET           36900020
         MVC   AD45(L8,R3),STMSGJ       MOVE NOT BUSY TO MESSAGE        36930020
         BR    R14                      RETURN                          36960020
STINTRSP DS    0C                                                       36990020
         MVC   AD41(L31,R3),STMSGC      MOVE BASE PART OF MESSAGE       37020020
         TM    AD108(R4),X02            TEST BUSY TRIGGER               37050020
         BCR   CC1,R14                  RETURN IF TRIGGER SET           37080020
         MVC   AD63(L12,R3),STMSGD      MOVE STORE STATUS TO MSG        37110020
         TM    AD16(R4),X08             TEST FOR INITIAL SELECTION      37140020
         BO    STIRPT0                  BRANCH IF TRIGGER SET           37170020
         TM    AD15(R4),X80             TEST FOR COMMAND OUT SENT       37200020
         BCR   CC8,R14                  RETURN IF TRIGGER RESET         37230020
STIRPT0  DS    0C                                                       37260020
         MVC   AD69(L16,R3),STMSGE      MOVE SECONDARY STATUS TO MSG    37290020
         BR    R14                      RETURN                          37320020
STCDA    DS    0C                                                       37350020
         MVC   AD41(L15,R3),STMSGK      MOVE READ CHAIN COMMAND         37380020
         TM    AD37(R4),X10             TEST READ TRIGGER               37410020
