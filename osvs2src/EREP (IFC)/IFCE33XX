@RT00240 DS    0H                                                  0241 00412000
*           R15=ADDR(F3330)-4;      /* DEVICE IS 3330 ANY MOD      0241 00413000
*                                                            @D35UGGB*/ 00414000
         LA    R15,F3330                                           0241 00415000
         SL    R15,@CF00062                                        0241 00416000
*         ELSE                                                     0242 00417000
*           IF OBRDEV=DEVWOOD THEN                                 0242 00418000
         B     @RC00240                                            0242 00419000
@RF00240 CLC   OBRDEV(2,R7),DEVWOOD                                0242 00420000
         BNE   @RF00242                                            0242 00421000
*             R15=ADDR(WOODY)-4;    /* DEVICE IS 88--                */ 00422000
         LA    R15,WOODY                                           0243 00423000
         SL    R15,@CF00062                                        0243 00424000
*           ELSE                                                   0244 00425000
*             IF OBRDEV=DEV3370 THEN                               0244 00426000
         B     @RC00242                                            0244 00427000
@RF00242 CLC   OBRDEV(2,R7),DEV3370                                0244 00428000
         BNE   @RF00244                                            0244 00429000
*               R15=ADDR(D3370)-4;  /*                       @D3506GB*/ 00430000
         LA    R15,D3370                                           0245 00431000
         SL    R15,@CF00062                                        0245 00432000
