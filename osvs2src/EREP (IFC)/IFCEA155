         MACRO                                                          16960002
&NAME    DSGEN                                                          17000002
.*                            THIS MACRO GENERATES ONE DSECT WORD       17040002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 17080002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  17120002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    17160002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 17200002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   17240002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 17280002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      17320002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  17360002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    17400002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   17440002
         GBLC &ITEM(3000)                                               17480002
         GBLA &ITEMNBR,&BITS(3000),&SHIFT(3000)                         17520002
         LCLB  &ONLY1,&CTIND                                            17560002
         LCLA  &START1,&START2,&END1,&END2                              17600002
         LCLC  &SYMBOL,&EQUSYM                                          17640002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 17680002
         ACTR  100000                                                   17720002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            17760002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     17800002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   17840002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        17880002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            17920002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             17960002
         AGO   .SET2A                   BRANCH AROUND                   18000002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              18040002
         MEXIT                                                          18080002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X18120002
               ).SET2                   TEST FOR NO SECOND              18160002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   18200002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        18240002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            18280002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              18320002
         AGO   .LOOP                                                    18360002
.SET2    ANOP                                                           18400002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 18440002
         AGO   .LOOP                                                    18480002
