         MEXIT                                                          18560002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               18600002
         MEXIT                                                          18640002
.LOOP    ANOP                                                           18680002
&OP      SETA  &OP+1                                                    18720002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           18760002
&ITEMNBR SETA  &ITEMNBR+1                                               18800002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            18840002
         MNOTE '**NULL BIT COUNT FIELD**'                               18880002
         MEXIT                                                          18920002
.OK1     ANOP                                                           18960002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        19000002
         AIF   ('&SYMBOL' NE '').OK                                     19040002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         19080002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         19120002
         AGO   .START                   GET NEXT                        19160002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        19200002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          19240002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        19280002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         19320002
         AGO   .NO3                     BRANCH AROUND                   19360002
.OK2     ANOP                                                           19400002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              19440002
.NO3     ANOP                                                           19480002
&SYMBOL  EQU   *+&BYTE                                                  19520002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             19560002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    19600002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    19640002
.CNT     ANOP                                                           19680002
&CTIND   SETB  1                        SET COUNT INDICATOR             19720002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   19760002
&BYTE    SETA  &COUNT/8                                                 19800002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        19840002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        19880002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             19920002
&END1    SETA  &END1+1                  INCREMENT COUNT                 19960002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             20000002
         AGO   .LOOP                    GENERATE NEXT                   20040002
.TSTST   ANOP                                                           20080002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               20120002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        20160002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      20200002
.NSET1   ANOP                                                           20240002
&OP      SETA  &OP-1                    DECREMENT INDEX                 20280002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           20320002
         AGO   .LOOP                    GO DO NEXT                      20360002
.LPDONE  ANOP                                                           20400002
&ONLY1   SETB  0                        CLEAR INDICATOR                 20440002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      20480002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 20520002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               20560002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             20600002
&NAME    DS    &COUNT.C                                                 20640002
.END     MEXIT                                                          20680002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          20720002
         MEXIT                                                          20760002
