.OUT     ANOP                                                           03120002
&NDX     SETA  0                                                        03160002
         AIF   (&SUB EQ 0).OUTA                                         03200002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 03240002
.OUTA    ANOP                                                           03280002
