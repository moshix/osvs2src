*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     01280002
*        LINE                                                           01320002
         LINE                                                           01360002
.NOMSG   ANOP                                                           01400002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  01440002
&SUB     SETB  1                                                        01480002
.LOOP    ANOP                                                           01520002
&OP      SETA  &OP+1                                                    01560002
.LOOP1   ANOP                                                           01600002
&NDX     SETA  &NDX+1                                                   01640002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND               01680002
         AIF   (&NDX LT &ITEMNBR).LOOP1                                 01720002
         MNOTE '**LOG ITEM NOT DEFINED**'                               01760002
         MEXIT                                                          01800002
.FOUND   ANOP                                                           01840002
&ISHIFT  SETA  &SHIFT(&NDX)                                             01880002
&IBITS   SETA  &BITS(&NDX)                                              01920002
&T       SETA  &IBITS-1                                                 01960002
&B       SETB  0                                                        02000002
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      02040002
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  02080002
         MEXIT                                                          02120002
.SUB0    AIF   (&SUB EQ 1).SUBA                                         02160002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             02200002
&K       SETA  K'&SYSLIST(2)                                            02240002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              02280002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                02320002
         AGO   .SUBB                                                    02360002
.SUBA    ANOP                                                           02400002
