         MACRO                                                          79110002
         SUM   &LOGITEM,&MASK                                           79200002
.*                            THIS MACRO GENERATES A NORMAL EDIT ENTRY  79290002
.*                                 FOR PROCESSING BY THE SUMMARY MODULE 79380002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE SUMMED 79470002
.*                            &MASK - BYTE MASK FOR BITS TO BE TESTED.  79560002
.*                                 THIS OPERAND SHOULD BE OMITTED FOR   79650002
.*                                 SINGLE BIT ITEMS IN WHICH CASE THE   79740002
.*                                 MASK WILL BE SET ACCORDING TO THE    79830002
.*                                 OFF BYTE BIT DISPLACEMENT OF THE     79920002
.*                                 ITEM AS DEFINED BY THE MACRO 'DSGEN' 80010002
         GBLA  &COUNTR,&ITEMNBR,&BITS(3000),&SHIFT(3000)                80100002
         GBLC  &ITEM(3000),&DSECT                                       80190002
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           80280002
.LOOP    ANOP                                                           80370002
&NDX     SETA  &NDX+1                                                   80460002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      80550002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  80640002
         MNOTE *,'LOGITEM NOT FOUND'                                    80730002
         MEXIT                                                          80820002
.FOUND   ANOP                                                           80910002
&ISHIFT  SETA  &SHIFT(&NDX)                                             81000002
&IBITS   SETA  &BITS(&NDX)                                              81090002
         AIF   (&IBITS EQ 1).FORM1      TEST FOR OLD FORM               81180002
         AIF   ('&MASK' NE '').FORM1    TEST FOR MASK                   81270002
         AIF   (&IBITS+&ISHIFT GT 8).MSG     TEST FOR MORE THAN EIGHT   81360002
         DC    AL.1(0),AL.3(&ISHIFT),AL.4(&IBITS)                       81450002
         DC    AL1(0)                                                   81540002
         AGO   .ITEM                                                    81630002
.MSG     MNOTE '**BITS GREATER THAN 8**'                                81720002
         MEXIT                                                          81810002
.FORM1   ANOP                                                           81900002
         DC    AL1(0)                                                   81990002
         AIF   ('&MASK' EQ '').FORM                                     82080002
         DC    B'&MASK'                                                 82170002
         AGO   .ITEM                                                    82260002
.FORM    ANOP                                                           82350002
&BITNBR  SETA  &ISHIFT-7                                                82440002
&BITNBR  SETA  0-&BITNBR                                                82530002
&M       SETA  1                                                        82620002
.LOOP2   ANOP                                                           82710002
         AIF   (&BITNBR EQ 0).GOTIT                                     82800002
