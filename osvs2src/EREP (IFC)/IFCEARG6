         TITLE 'I F C E A R G 6 --- ARGONAUT FORMAT 6 SUPPORT'          00090002
         MACRO                                                          00180002
         BIN   ,                                                        00270002
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   00360002
.*                                      RECORD DSECT                    00450002
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      00540002
.*                                      TO SKIP ON MULTIPLE             00630002
.*                                      PRINT OR NBR OF SPACES          00720002
.*                                      TO BACK UP FROM COLUMN FOR      00810002
.*                                      START OF PRINTING. THE LETTER   00900002
.*                                      B PRECEEDS THE NUMBER (B5) IF   00990002
.*                                      THE LATTER IS DESIRED. OMISSION 01080002
.*                                      OF THIS OPERAND FORCES OPTION 2 01170002
.*                                      WITH BACKUP SPACING EQUAL TO    01260002
.*                                 THE NUMBER OF BITS IN THE ITEM       01350002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      01440002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     01530002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       01620002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             01710002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  01800002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  01890002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              01980002
         GBLA  &NRENTS,&SHIFT(3000)                                     02070002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           02160002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       02250002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         02340002
         LCLB  &B,&SUB                                                  02430002
         LCLA  &IBITS,&ISHIFT,&K                                        02520002
         LCLA  &TEMP,&T,&OP                                             02610002
         LCLC  &S,&ARG                                                  02700002
          ACTR  500000                                                  02790002
         AIF   (&DIAGTST EQ 0).NOMSG                                    02880002
         AIF   (&INITIAL EQ 0).NOMSG                                    02970002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    03060002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  03150002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     03240002
*        LINE                                                           03330002
         LINE                                                           03420002
.NOMSG   ANOP                                                           03510002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  03600002
&SUB     SETB  1                                                        03690002
.LOOP    ANOP                                                           03780002
&OP      SETA  &OP+1                                                    03870002
&ARG     SETC  '&SYSLIST(&OP,1)'                                        03960002
         ITEMFIND &ARG                                                  04050002
         AIF   (&ITEMXPT EQ 0).MEXIT                                    04140002
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         04230002
&IBITS   SETA  &BITS(&ITEMXPT)                                          04320002
         AIF   (&SUB EQ 1).SUB00                                        04410002
         AIF   ('&SYSLIST(3)' EQ '').SETT                               04500002
&IBITS   SETA  &SYSLIST(3)                                              04590002
         AGO   .SETT                                                    04680002
.SUB00   AIF   ('&SYSLIST(&OP,3)' EQ '').SETT                           04770002
&IBITS   SETA  &SYSLIST(&OP,3)                                          04860002
.SETT    ANOP                                                           04950002
&T       SETA  &IBITS-1                                                 05040002
&B       SETB  0                                                        05130002
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      05220002
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  05310002
.MEXIT   MEXIT                                                          05400002
.SUB0    AIF   (&SUB EQ 1).SUBA                                         05490002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             05580002
&K       SETA  K'&SYSLIST(2)                                            05670002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              05760002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                05850002
         AGO   .SUBB                                                    05940002
.SUBA    ANOP                                                           06030002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         06120002
&K       SETA  K'&SYSLIST(&OP,2)                                        06210002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          06300002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            06390002
.SUBB    ANOP                                                           06480002
&T       SETA  &S                                                       06570002
.BACKUP  ANOP                                                           06660002
&NRENTS  SETA  &NRENTS+1                                                06750002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            06840002
&TEMP    SETA  &ISHIFT                                                  06930002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                07020002
         AGO   .OUT                                                     07110002
         AIF   (&SUB EQ 1).BIN2                                         07200002
         AIF   ('&SYSLIST(2)' NE '').BIN3                               07290002
         AGO   .BIN2A                                                   07380002
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           07470002
.BIN2A   ANOP                                                           07560002
&B       SETB  1                                                        07650002
.BIN3    ANOP                                                           07740002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    07830002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             07920002
.OUT     ANOP                                                           08010002
&ITEMXPT     SETA  0                                                    08100002
         AIF   (&SUB EQ 0).OUTA                                         08190002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 08280002
.OUTA    ANOP                                                           08370002
         MEND                                                           08460002
         MACRO                                                          08550002
&NAME    DSGEN ,                                                        08640002
.*                            THIS MACRO GENERATES ONE DSECT WORD       08730002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 08820002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  08910002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    09000002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 09090002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   09180002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 09270002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      09360002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  09450002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    09540002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   09630002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             09720002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  09810002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  09900002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              09990002
         GBLA  &NRENTS,&SHIFT(3000)                                     10080002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           10170002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       10260002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         10350002
         LCLB  &ONLY1,&CTIND                                            10440002
         LCLA  &START1,&START2,&END1,&END2                              10530002
         LCLC  &SYMBOL,&EQUSYM                                          10620002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 10710002
         ACTR  100000                                                   10800002
         AIF   (&ITEMITR EQ 0).START                                    10890002
 MNOTE   20,'DSGEN MACRO OCCURRS AFTER ITEMSORT MACRO HAS BEEN ISSUED.' 10980002
         MEXIT                                                          11070002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            11160002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     11250002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   11340002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        11430002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            11520002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             11610002
         AGO   .SET2A                   BRANCH AROUND                   11700002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        11790002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              11880002
         MEXIT                                                          11970002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X12060002
               ).SET2                   TEST FOR NO SECOND              12150002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   12240002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            12330002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              12420002
         AGO   .LOOP                                                    12510002
.SET2    ANOP                                                           12600002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 12690002
         AGO   .LOOP                                                    12780002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             12870002
         MEXIT                                                          12960002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               13050002
         MEXIT                                                          13140002
.LOOP    ANOP                                                           13230002
&OP      SETA  &OP+1                                                    13320002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           13410002
&ITEMNBR SETA  &ITEMNBR+1                                               13500002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            13590002
         MNOTE '**NULL BIT COUNT FIELD**'                               13680002
         MEXIT                                                          13770002
.OK1     ANOP                                                           13860002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        13950002
         AIF   ('&SYMBOL' NE '').OK                                     14040002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         14130002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         14220002
         AGO   .START                   GET NEXT                        14310002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        14400002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          14490002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        14580002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         14670002
         AGO   .NO3                     BRANCH AROUND                   14760002
.OK2     ANOP                                                           14850002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              14940002
.NO3     ANOP                                                           15030002
&SYMBOL  EQU   *+&BYTE                                                  15120002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             15210002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    15300002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    15390002
.CNT     ANOP                                                           15480002
&CTIND   SETB  1                        SET COUNT INDICATOR             15570002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   15660002
&BYTE    SETA  &COUNT/8                                                 15750002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        15840002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        15930002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             16020002
&END1    SETA  &END1+1                  INCREMENT COUNT                 16110002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             16200002
         AGO   .LOOP                    GENERATE NEXT                   16290002
.TSTST   ANOP                                                           16380002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               16470002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        16560002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      16650002
.NSET1   ANOP                                                           16740002
&OP      SETA  &OP-1                    DECREMENT INDEX                 16830002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           16920002
         AGO   .LOOP                    GO DO NEXT                      17010002
.LPDONE  ANOP                                                           17100002
&ONLY1   SETB  0                        CLEAR INDICATOR                 17190002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      17280002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 17370002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               17460002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             17550002
&NAME    DS    &COUNT.C                                                 17640002
.END     MEXIT                                                          17730002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          17820002
         MEXIT                                                          17910002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   18000002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   18090002
         MEXIT                                                          18180002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         18270002
*****               THE FOLLOWING STATEMENT                *****        18360002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        18450002
.EQU2    ANOP                                                           18540002
&EQUCNT  SETA  0                        CLEAR COUNTER                   18630002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 18720002
.EQULOP  ANOP                                                           18810002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 18900002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     18990002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                19080002
         AGO   .EQULOP                  NO-GET NEXT                     19170002
.EQUOUT  ANOP                                                           19260002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  19350002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              19440002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 19530002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             19620002
         AGO   .START                   RETURN FOR NEXT                 19710002
         MEND                                                           19800002
         MACRO                                                          19890002
         CONVT ,                                                        19980002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             20070002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  20160002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  20250002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              20340002
         GBLA  &NRENTS,&SHIFT(3000)                                     20430002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           20520002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       20610002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         20700002
         LCLB  &SUB                                                     20790002
         LCLA  &OP                                                      20880002
         LCLA  &OFFSET,&OUT,&IN,&IND,&IBITS,&ISHIFT                     20970002
.*                                                                      21060002
.*       CONVERT BINARY TO DECIMAL                                      21150002
.*             &NAME IS THE REFERENCE INTO THE DSECT RECORD             21240002
.*             &OUT    IS THE NUMBER OF CHARS TO BE PUT ONTO PRINT LINE 21330002
.*                                      DEFAULT IS FOUR MAXIMUM IS 8    21420002
.*             &IN       IS THE NUMBER OF BYTES AS INPUT                21510002
.*                                      ERROR DEFAULT IS 2 MAXIMUM IS 4 21600002
.*                                                                      21690002
         AIF   (&DIAGTST EQ 0).NOMSG                                    21780002
         AIF   (&INITIAL EQ 0).NOMSG                                    21870002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    21960002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  22050002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     22140002
*        LINE                                                           22230002
         LINE                                                           22320002
.NOMSG   ANOP                                                           22410002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LLOP                  22500002
&SUB     SETB  1                                                        22590002
.LLOP    ANOP                                                           22680002
&OP      SETA  &OP+1                                                    22770002
&OUT     SETA  4                        .BR NOT NULL ELSE SET TO 4      22860002
         AIF   (&SUB EQ 1).SUBA                                         22950002
         AIF   ('&SYSLIST(3)' EQ '').B  TEST FOR NULL INPUT             23040002
&OUT     SETA  &SYSLIST(3)             SET &OUT FROM INPUT              23130002
         AGO   .A                                                       23220002
.SUBA    AIF   ('&SYSLIST(&OP,3)' EQ '').B                              23310002
&OUT     SETA  &SYSLIST(&OP,3)          SET &OUT FROM INPUT             23400002
.A       AIF   (&OUT GT 0 AND &OUT LT 9).B   TEST RANGE                 23490002
.AERR    MNOTE '*** BYTES OUT INCORRECTLY SPECIFIED DEFAULT USED ***'   23580002
&OUT     SETA  4                                                        23670002
.B       AIF   (&SUB EQ 1).SUBB                                         23760002
         AIF   ('&SYSLIST(2)' EQ '').LOOP    BRANCH IF NULL             23850002
&IN      SETA  &SYSLIST(2)              .SET &IN FROM INPUT             23940002
         AGO   .BB                                                      24030002
.SUBB    AIF   ('&SYSLIST(&OP,2)' EQ '').LOOP                           24120002
&IN      SETA  &SYSLIST(&OP,2)          .SET &IN FROM INPUT             24210002
.BB      AIF   (&IN GT 0 AND &IN LT 5).D     TEST RANGE                 24300002
.BERR    MNOTE '*** BYTES IN INCORRECTLY SPECIFIED DEFAULT USED ***'    24390002
&IN      SETA  2                                                        24480002
         AGO   .D                                                       24570002
.LOOP    ANOP                           .LOOP TO FIND &NAME IN DSECT    24660002
&IND     SETA  &IND+1                   .SET UP LOOP COUNTER            24750002
         AIF   ('&ITEM(&IND)' EQ '&SYSLIST(&OP,1)').FOUND               24840002
         AIF   (&IND LT &ITEMNBR).LOOP  .TEST FOR END OF DSECT          24930002
         MNOTE '*** LOG ITEM NOT DEFINED ***'                           25020002
         MEXIT                                                          25110002
.FOUND   ANOP                                                           25200002
&IBITS   SETA  &BITS(&IND)              .SET UP NUMBER OF BITS SPEC     25290002
         AIF   (&IBITS-((&IBITS/8)*8) EQ 0).C  .TEST FOR MULTIPLE OF 8  25380002
         MNOTE '*** DATA NOT DEFINED A MULTIPLE OF 8 BITS ***'          25470002
         MEXIT                                                          25560002
.C       ANOP                                                           25650002
&IN      SETA  &IBITS/8                 .COMPUTE NUMBER OF BYTES        25740002
         AIF   (&IN LT 1 OR &IN GT 4).BERR  .TEST RANGE B IF IN ERROR   25830002
&ISHIFT  SETA  &SHIFT(&IND)             .SET UP FOR TEST OF BYTE BNDR   25920002
         AIF   (&ISHIFT EQ 0).D         .TEST FOR BYTE BNDRY            26010002
         MNOTE '*** DATA NOT ALIGNED ON A BYTE BOUNDARY ***'            26100002
         MEXIT                                                          26190002
.*                                                                      26280002
.D       ANOP                                                           26370002
&NRENTS  SETA  &NRENTS+1                                                26460002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),AL.4(9)                    26550002
         DC    X'&IN',X'&OUT'                                           26640002
&IND     SETA  0                                                        26730002
         AIF   (&SUB EQ 0).OUTA                                         26820002
         AIF   (&OP LT N'&SYSLIST).LLOP                                 26910002
.OUTA    ANOP                                                           27000002
         MEND                                                           27090002
         MACRO                                                          27180002
         ITEMFIND &ARG,&ITFDIAG=                                        27270002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             27360002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  27450002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  27540002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              27630002
         GBLA  &NRENTS,&SHIFT(3000)                                     27720002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           27810002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       27900002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         27990002
         LCLA  &A,&B,&X,&I                                              28080002
         LCLC  &C1,&C2,&S                                               28170002
         ACTR  100000                                                   28260002
&ITEMXPT SETA  0                  INDICATE ITEM NOT FOUND.              28350002
         AIF   ('&ARG' NE '').NORM                                      28440002
         AIF   ('&ITFDIAG' EQ 'OFF'  OR  '&ITFDIAG' EQ 'ON').DIAGSET    28530002
  MNOTE 0,'ITEMFIND - OPERAND (&ITFDIAG) INVALID FOR ITFDIAG KEYWORD.'  28620002
         MEXIT                                                          28710002
.DIAGSET ANOP                                                           28800002
&DIAGITF SETB  ('&ITFDIAG' EQ 'ON')                                     28890002
         MEXIT                                                          28980002
.NORM    AIF   (&ITEMITR GT 0).SORTED                                   29070002
         ITEMSORT                                                       29160002
.SORTED  ANOP                                                           29250002
&I       SETA  &ITEMITR           COMPUTED ITTERATION COUNT.            29340002
&X       SETA  &ITEMMDX           COMPUTED MAXIMUM INDEX.               29430002
&C1      SETC  '&ARG'             ISOLATE SEARCH ARGUMENT.              29520002
&C1      SETC  '&C1'.'       '(1,8-K'&C1) PAD TO FULL 8 CHARACTERS.     29610002
.SRCHUP  ANOP                                                           29700002
&A       SETA  &A+&X              CURRENT TABLE SUBSCRIPT + INDEX.      29790002
&S       SETC  '+'                                                      29880002
         AIF   (&A  GT &ITEMNBR  AND  NOT &DIAGITF).SRCHDN1             29970002
         AIF   (&A  LE &ITEMNBR).SRCH00                                 30060002
&C2      SETC  '*-OVER-*'                                               30150002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         30240002
&X       SETA  &X/2               HALVE INDEX VALUE.                    30330002
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X'                     30420002
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      30510002
.NOFIND  MNOTE  20,' ITEMFIND FAILED FOR NAME  &C1  '                   30600002
         MEXIT                                                          30690002
.SRCH00  ANOP                                                           30780002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         30870002
         AIF   (&I LT 0).NOFIND   IF COUNT HAS GONE TO ZERO.            30960002
&X       SETA  &X/2               HALVE INDEX VALUE.                    31050002
&B       SETA  &ITEMNDX(&A)       GET SUBSCRIPT FROM INDEX TABLE.       31140002
&C2      SETC  '&ITEM(&B)'.'       '(1,8-K'&ITEM(&B))  PAD COMPARAND.   31230002
         AIF   (NOT &DIAGITF).COMPARE                                   31320002
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X   POS = &B'          31410002
.COMPARE AIF   ('&C1' LT '&C2').SRCHDWN   ARGUMENT LOWER, MOVE DOWN.    31500002
         AIF   ('&C1' GT '&C2').SRCHUP    ARGUMENT HIGHER, MOVE UP.     31590002
&ITEMXPT SETA  &B                 FOUND. SET INDEX POINTER FOR RETURN.  31680002
&ITEMFST(&I+1)  SETA  &ITEMFST(&I+1)+1                                  31770002
         MEXIT                                                          31860002
.SRCHDWN ANOP                                                           31950002
&A       SETA  &A-&X              CURRENT POINTER MINUS INDEX VALUE.    32040002
&S       SETC  '-'                                                      32130002
         AIF   (&A LE &ITEMNBR).SRCH00    IF WITHIN BOUNDS OF TABLE.    32220002
.SRCHDN1 ANOP                                                           32310002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         32400002
&X       SETA  &X/2               HALVE INDEX VALUE.                    32490002
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      32580002
         MNOTE  20,' ITEMFIND FAILED FOR NAME  &C1  '                   32670002
         MEND                                                           32760002
         MACRO                                                          32850002
         ITEMSORT  &DUMMY                                               32940002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             33030002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  33120002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  33210002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              33300002
         GBLA  &NRENTS,&SHIFT(3000)                                     33390002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           33480002
