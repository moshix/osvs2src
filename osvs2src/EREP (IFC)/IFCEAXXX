SETDEVT  STM   R5,R8,XSAVE             SAVE REGS                        62340002
         CLC   L0(L2,R4),UNIT90    IS DEVICE A 3790            @ZA26849 62400000
         BNE   SET3270             NO,CHECK FOR 3270           @ZA26849 62420000
         MVC   L0(L10,R3),LOC3791  YES,MOVE 3791 LOCAL TO IMAGE@ZA26849 62440000
         B     RETURN              AND RETURN                  @ZA26849 62460000
SET3270  CLC   L0(L2,R4),UNIT70    IS DEVICE A 3270            @ZA26849 62480000
         BNE   SET3705             NO,CHECK FOR 3705           @ZA26849 62500000
         MVC   L0(L4,R3),LOC3270   YES,MOVE 3270 TO IMAGE      @ZA26849 62520000
         B     RETURN              AND RETURN                  @ZA26849 62540000
SET3705  MVC   L0(L4,R3),LOC3705   MOVE 3705 TO IMAGE          @ZA26849 62560000
         LA    R5,VALTYPES         GET LIST OF KNOWN CODES     @ZA26849 62580000
         LA    R6,L5               COUNT OF TABLE LESS ONE     @ZA26849 62600000
         CLI   L0(R4),H40          IS THIS A COMM TYPE DEVICE  @ZA26849 62620000
         BNE   SETUNKWN            NO,UNKNOWN OR UNSUPPORTED   @ZA26849 62640000
SETLP    CLC   L1(L1,R4),L0(R5)    YES,CHECK FOR 3705 ID       @ZA26849 62660000
         BE    RETURN              FOUND IT,RETURN             @ZA26849 62680000
         LA    R5,L1(R5)           GET NEXT AND TRY AGAIN      @ZA26849 62700000
         BCT   R6,SETLP                                        @ZA26849 62720000
SETUNKWN MVC   L0(L4,R3),UNKWN     NOT IN TABLE MUST BE UNKNOWN@ZA26849 62740000
         B     RETURN                                                   62760000
*                                                              @D3504VS 62780000
*        BYPASS THE DOS VTAM SUPPORT                           @D3504VS 62800000
*                                                              @D3504VS 62820000
CHECKSYS CLC   CLASRC(L2),DOSYSREL  IS IT DOS?                 @D3504VS 62840000
         BNER  R14                 NO, EXIT                    @D3504VS 62860000
         CLC   DEVTYP1(2),XDOS3705  IS IT 3705?                @D3504VS 62880000
         BE    CHECKSY1            YES, GO TO BYPASS OUTPUT    @D3504VS 62900000
         CLC   DEVTYP1(2),X2703BI  IS IT 2703 BISYNCH?         @D3504VS 62920000
         BNER  R14                 NO, EXIT                    @D3504VS 62940000
CHECKSY1 MVC   XR9PTR,FINADDR      CHANGE POINTER TO ONLY DUMP @D3504VS 62960000
         BR    R14                 EXIT                        @D3504VS 62980000
DOSYSREL DC    X'3629'                                         @D3504VS 63000000
XDOS3705 DC    X'DC10'                                         @D3504VS 63020000
X2703BI  DC    X'D200'                                         @D3504VS 63040000
FINADDR  DC    A(FINISH-4)                                     @D3504VS 63060000
