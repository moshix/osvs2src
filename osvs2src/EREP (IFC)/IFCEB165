         LCLC  &TEXT                                                    16800002
         LCLA  &OP,&K,&START,&NRCHAR,&REMAIN                            16850002
&MARGIN  SETC  '*'                                                      16900002
         AIF   ('&NAME' EQ '').LOOPOUT                                  16950002
&LPTR    SETA  &LPTR+1                                                  17000002
&LBL(&LPTR) SETC '&NAME'                                                17050002
&LBLCTR(&LPTR) SETA &LABELNR+1                                          17100002
.LOOPOUT ANOP                                                           17150002
&OP      SETA  &OP+1                                                    17200002
&K       SETA  K'&SYSLIST(&OP)                                          17250002
         AIF (&K GE 2).NORM                                             17300002
&K       SETA  2                                                        17350002
.NORM    ANOP                                                           17400002
&START   SETA  2                                                        17450002
         AIF (&OP LT N'&SYSLIST).NOTLAST                                17500002
&MARGIN  SETC  '$'                                                      17550002
.NOTLAST ANOP                                                           17600002
&NRCHAR  SETA  8                                                        17650002
&TACK    SETC  ''                                                       17700002
.LOOPIN  ANOP                                                           17750002
&REMAIN  SETA  &K-&START                                                17800002
         AIF (&REMAIN GE 8).MORE                                        17850002
&TACK    SETC  '&MARGIN'                                                17900002
&NRCHAR  SETA  &REMAIN                                                  17950002
.MORE    ANOP                                                           18000002
&TEXT    SETC  ''                                                       18050002
         AIF (&REMAIN EQ 0).ENDTEXT                                     18100002
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          18150002
.ENDTEXT ANOP                                                           18200002
&LABELNR SETA  &LABELNR+1                                               18250002
&LABELS(&LABELNR) SETC '&TEXT'.'&TACK'                                  18300002
&START   SETA  &START+8                                                 18350002
         AIF ('&TACK' EQ '').LOOPIN                                     18400002
         AIF ('&TACK' EQ '*').LOOPOUT                                   18450002
         MEND                                                           18500002
         SPACE 2                                                        18550002
         MACRO                                                          18600002
         LSTART                                                         18650002
.*                            THIS MACRO IS ISSUED WHEN THE LABEL AREA  18700002
.*                                 OF THE TABLE IS TO BEGIN.            18750002
