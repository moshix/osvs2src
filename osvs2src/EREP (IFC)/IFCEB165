       TITLE   'IFCEB165 MODEL 165 CPU EDIT TABLE 3'             S21021 00050002
*  DSGEN MACRO     DSGEN MACRO     DSGEN MACRO     DSGEN MACRO        * 00100002
         SPACE 2                                                        00150002
         MACRO                                                          00200002
&NAME    DSGEN &X1,&X2,&X3,&X4,&X5,&X6,&X7,&X8,&X9,&X10,&X11,&X12,&X13,X00250002
               &X14,&X15,&X16,&X17,&X18,&X19,&X20,&X21,&X22,&X23,&X24, X00300002
               &X25,&X26,&X27,&X28,&X29,&X30,&X31,&X32                  00350002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00400002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00450002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00500002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00550002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00600002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00650002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00700002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00750002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00800002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00850002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   00900002
         GBLC &ITEM(1500)                                               00950002
         GBLA &ITEMNBR,&BITS(1500),&SHIFT(1500)                         01000002
         LCLC  &SYMBOL                                                  01050002
         LCLA  &OP,&COUNT,&BYTE                                         01100002
.LOOP    ANOP                                                           01150002
&OP      SETA  &OP+1                                                    01200002
&ITEMNBR SETA  &ITEMNBR+1                                               01250002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        01300002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            01350002
         MNOTE *,'NULL BIT COUNT FIELD'                                 01400002
         MEXIT                                                          01450002
.OK1     ANOP                                                           01500002
         AIF   ('&SYMBOL' NE '').OK                                     01550002
         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           01600002
         AGO   .CNT                                                     01650002
.OK      ANOP                                                           01700002
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       01750002
&SYMBOL  EQU   *+&BYTE                                                  01800002
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 01850002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    01900002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    01950002
.CNT     ANOP                                                           02000002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   02050002
&BYTE    SETA  &COUNT/8                                                 02100002
         AIF (&OP LT N'&SYSLIST).LOOP                                   02150002
         AIF   (&COUNT NE 32).NOT32                                     02200002
&NAME    DS    4C                                                       02250002
         MEXIT                                                          02300002
.NOT32   AIF   (&COUNT NE 64).ERR                                       02350002
&NAME    DS    8C                                                       02400002
         MEXIT                                                          02450002
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             02500002
         MEND                                                           02550002
         EJECT                                                          02600002
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 02650002
         SPACE 2                                                        02700002
         MACRO                                                          02750002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      02800002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     02850002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    02900002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 02950002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    03000002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    03050002
.*                                 THE PREVIOUS LINE AND STORES THAT #  03100002
.*                                 INTO THE APPROPRIATE BYTE.           03150002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    03200002
.*                                 DATA.                                03250002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 03300002
.*                                 DATA COLUMN STARTING CHARACTERS.     03350002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   03400002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 03450002
.*                                 STARTING CHARACTERS.                 03500002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   03550002
.*                                 TO BEGIN NEXT PRINTED LINE.          03600002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       03650002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   03700002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 03750002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   03800002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 03850002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    03900002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   03950002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   04000002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  04050002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   04100002
.*                                 THE SUBJECT LINE                     04150002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  04200002
         GBLA  &LASTCC,&INITIAL                                         04250002
         GBLC  &NRENTSX                                                 04300002
         GBLC  &CARRCON                                                 04350002
         GBLB  &TEST                                                    04400002
         LCLA  &S                                                       04450002
&NAME    DS    0C                                                       04500002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  04550002
         LINEND                                                         04600002
         AGO   .NTFIRST                                                 04650002
.FIRSTLN ANOP                                                           04700002
&NRENTSX SETC  'N&SYSNDX'                                               04750002
&INITIAL SETA  1                                                        04800002
N&SYSNDX EQU   *                                                        04850002
.NTFIRST ANOP                                                           04900002
         DC    AL1(0)                                                   04950002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               05000002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                05050002
&DB      SETA  &DATA(1)                                                 05100002
         AGO   .AA                                                      05150002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                05200002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 05250002
         AGO   .CC                                                      05300002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    05350002
         AIF   (&DATA(2) EQ &DT).BB                                     05400002
.CC      ANOP                                                           05450002
&DT      SETA  &DATA(2)                                                 05500002
.BB      DC    XL2'FFFE'                                                05550002
         DC    FL1'&DB'                                                 05600002
         DC    FL1'&DT'                                                 05650002
&NRENTS  SETA  &NRENTS+1                                                05700002
.NCDATA  ANOP                                                           05750002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              05800002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               05850002
&LB      SETA  &LABEL(1)                                                05900002
         AGO   .AAA                                                     05950002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              06000002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               06050002
         AGO   .CCC                                                     06100002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  06150002
         AIF   (&LABEL(2) EQ &LT).BBB                                   06200002
.CCC     ANOP                                                           06250002
&LT      SETA  &LABEL(2)                                                06300002
.BBB     DC    XL2'FFFF'                                                06350002
         DC    FL1'&LB'                                                 06400002
         DC    FL1'&LT'                                                 06450002
&NRENTS  SETA  &NRENTS+1                                                06500002
.NCLABEL ANOP                                                           06550002
         AIF   ('&SKIP' NE '').CONT                                     06600002
         AIF   (&TEST EQ 0).NOCHANG                                     06650002
&S       SETA  &LASTCC                                                  06700002
         AGO   .DEFAULT                                                 06750002
.CONT    ANOP                                                           06800002
         AIF   (&TEST EQ 0).AB                                          06850002
&S       SETA  &SKIP                                                    06900002
.DEFAULT ANOP                                                           06950002
         ORG   &CARRCON+3                                               07000002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 07050002
         ORG                                                            07100002
&LASTCC  SETA  &S                                                       07150002
&TEST    SETB  0                                                        07200002
         AGO   .DD                                                      07250002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               07300002
A&SYSNDX DC    XL2'FFFB'                                                07350002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              07400002
         DC    FL1'9'                                                   07450002
&LASTCC  SETA  &SKIP                                                    07500002
&TEST    SETB  1                                                        07550002
&CARRCON SETC  'A&SYSNDX'                                               07600002
&NRENTS  SETA  &NRENTS+1                                                07650002
.DD      ANOP                                                           07700002
.NOCHANG ANOP                                                           07750002
         MEND                                                           07800002
         EJECT                                                          07850002
*  BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO     BIN MACRO  * 07900002
         SPACE 2                                                        07950002
         MACRO                                                          08000002
         BIN   &LOGITEM,&SKIP$OR                                        08050002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       08100002
.*                                      RECORD DSECT                    08150002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  08200002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      08250002
.*                                      TO BACK UP FROM COLUMN FOR      08300002
.*                                      START OF PRINTING. THE LETTER   08350002
.*                                      B PRECEEDS THE NUMBER (B5) IF   08400002
.*                                      THE LATTER IS DESIRED. OMISSION 08450002
.*                                      OF THIS OPERAND FORCES OPTION 2 08500002
.*                                      WITH BACKUP SPACING EQUAL TO    08550002
.*                                 THE NUMBER OF BITS IN THE ITEM       08600002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      08650002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     08700002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       08750002
         GBLC &ITEM(1500)                                               08800002
         GBLA &ITEMNBR,&BITS(1500),&SHIFT(1500),&NRENTS                 08850002
         GBLC  &DSECT                                                   08900002
         LCLB  &B                                                       08950002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   09000002
         LCLA  &TEMP,&T                                                 09050002
         LCLC  &S                                                       09100002
.LOOP    ANOP                                                           09150002
&NDX     SETA  &NDX+1                                                   09200002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      09250002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  09300002
         MNOTE *,'YOU BLEW IT'                                          09350002
         ROUTINE DUM,SKIPPER                                            09400002
         MEXIT                                                          09450002
.FOUND   ANOP                                                           09500002
&ISHIFT  SETA  &SHIFT(&NDX)                                             09550002
&IBITS   SETA  &BITS(&NDX)                                              09600002
&T       SETA  &IBITS-1                                                 09650002
&B       SETB  0                                                        09700002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                09750002
&K       SETA  K'&SKIP$OR                                               09800002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 09850002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   09900002
&T       SETA  &S                                                       09950002
.BACKUP  ANOP                                                           10000002
&NRENTS  SETA  &NRENTS+1                                                10050002
&TEMP    SETA  &ISHIFT                                                  10100002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       10150002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              10200002
         ORG   A&SYSNDX                                                 10250002
&B       SETB  1                                                        10300002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           10350002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             10400002
.OUT     ANOP                                                           10450002
         MEND                                                           10500002
         EJECT                                                          10550002
*  HEX MACRO     HEX MACRO     HEX MACRO    HEXN MACRO     HEX MACRO  * 10600002
         SPACE 2                                                        10650002
         MACRO                                                          10700002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 10750002
.*                            THIS MACRO FACILITATES GENERATION OF THE  10800002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    10850002
.*                                 CONVERSIONS.                         10900002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        10950002
.*                                 CONVERTED                            11000002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    11050002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   11100002
.*                                 BACK UP FROM COLUMN FOR START OF     11150002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  11200002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  11250002
.*                                 DESIRED. OMISSION OF THIS OPERAND    11300002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  11350002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  11400002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 11450002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    11500002
.*                                 IMPLICITLY B).                       11550002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   11600002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     11650002
.*                                 WITH AN L,R, OR B TO STIPULATE       11700002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     11750002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     11800002
.*                                 RESPECTIVELY. OMISSION OF THIS       11850002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  11900002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 11950002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 12000002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   12050002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     12100002
.*                                 SUBJECT LINE(S).                     12150002
         GBLC &ITEM(1500),&DSECT                                        12200002
         GBLA &ITEMNBR,&BITS(1500),&SHIFT(1500),&NRENTS                 12250002
         LCLB  &B                                                       12300002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  12350002
         LCLC  &S,&LETTER                                               12400002
.LOOP    ANOP                                                           12450002
&NDX     SETA  &NDX+1                                                   12500002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      12550002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  12600002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 12650002
         MEXIT                                                          12700002
.FOUND   ANOP                                                           12750002
&ISHIFT  SETA  &SHIFT(&NDX)                                             12800002
&IBITS   SETA  &BITS(&NDX)                                              12850002
&CODE    SETA  1                                                        12900002
&K       SETA  K'&BYTES                                                 12950002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            13000002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   13050002
         MEXIT                                                          13100002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 13150002
&LETTER  SETC  '&BYTES'(&K,1)                                           13200002
&S       SETC  '&BYTES'(1,&K-1)                                         13250002
&BYTESP  SETA  &S                                                       13300002
         AIF   ('&LETTER' EQ 'B').BOTH                                  13350002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       13400002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            13450002
         MEXIT                                                          13500002
.USEDEFN ANOP                                                           13550002
&BYTESP  SETA  &IBITS/8                                                 13600002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       13650002
&CODE    SETA  3                                                        13700002
&T       SETA  &BYTESP*2-1                                              13750002
&S       SETC  '&T'                                                     13800002
.OK2     AIF   (&ISHIFT EQ 0).L                                         13850002
&CODE    SETA  2                                                        13900002
.L       ANOP                                                           13950002
&B       SETB  1                                                        14000002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                14050002
&K       SETA  K'&SKIP$OR                                               14100002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 14150002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   14200002
.BACKUP  ANOP                                                           14250002
&NRENTS  SETA  &NRENTS+1                                                14300002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           14350002
         DC    BL.1'&B',FL.7'&S',AL.4(&BYTESP),FL.4'&CODE'              14400002
.OUT     ANOP                                                           14450002
         MEND                                                           14500002
         SPACE 2                                                        14550002
