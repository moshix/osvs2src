         AIF   (&LBLCTR(&CTR) NE &NDX).NOLBL                            19250002
&LBL(&CTR) DC  C'&LABELS(&NDX)'                                         19300002
&CTR     SETA  &CTR+1                                                   19350002
         AGO   .LOOPCTL                                                 19400002
.NOLBL   ANOP                                                           19450002
         DC    C'&LABELS(&NDX)'                                         19500002
.LOOPCTL AIF   (&NDX LT &LABELNR).LOOP                                  19550002
         MEND                                                           19600002
         SPACE 2                                                        19650002
         MACRO                                                          19700002
         LINEND                                                         19750002
