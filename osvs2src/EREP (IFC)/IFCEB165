ROUT0    DS    0C                                                       30350002
         STM   R3,R4,SAVER34            SAVE PRINT BFR PTR- LOG START   30400002
         BR    R14                      RETURN                          30450002
ROUT1    DS    0C                                                       30500002
         STM   R3,R8,XSAVE              SAVE ROUTINE REGISTERS          30550002
         L     R3,SAVER3                LOAD ADR OF PRINT BFR           30600002
         L     R7,L0(R4)                LOAD LOG DATA                   30650002
         LA    R3,L3(R3)                INCREMENT PRINT BFR POINTER     30700002
         LA    R8,L4                    LOAD BYTE COUNT                 30750002
ROUT1A   DS    0C                                                       30800002
         LA    R5,L6                    LOAD BIT COUNT                  30850002
ROUT1B   DS    0C                                                       30900002
         SR    R6,R6                    CLEAR REGISTER R6               30950002
         MVI   L0(R3),CB                ASSUME 0 - SET BFR TO BLANK     31000002
         SLDL  R6,L1                    SHIFT DATA BIT INTO R6          31050002
         LTR   R6,R6                    TEST VALUE IN R6                31100002
         BZ    ROUT1C                   BRANCH IF R6 ZERO               31150002
         MVI   L0(R3),CS                MOVE SPLAT TO PRINT BFR         31200002
ROUT1C   DS    0C                                                       31250002
         LA    R3,L3(R3)                INCREMENT PRINT BFR POINTER     31300002
         BCT   R5,ROUT1B                BRANCH ON BIT COUNT             31350002
         SLL   R7,L2                    BYPASS BITS 6 AND 7             31400002
         BCT   R8,ROUT1A                BRANCH ON BYTE COUNT            31450002
         LM    R3,R8,XSAVE              RESTORE REGISTERS               31500002
         BR    R14                      RETURN                          31550002
************************************************************** @XA21819 31552001
* THIS ROUTINE WILL SET THE PROPER DUMP SIZE TO DUMP THE WHOLE @XA21819 31554001
* RECORD BASED ON THE SIZE WHICH PRECEDES THE START OF THE     @XA21819 31556001
* MCH RECORD.                                                  @XA21819 31558001
************************************************************** @XA21819 31560001
DUMPSET  STM   R4,R8,XSAVE             SAVE WORK REGS          @XA21819 31562001
         S     R4,TWO                  BACK UP PTR TO REC LEN  @XA21819 31564001
         MVC   WORKSIZE(2),0(R4)       GET RECORD LENGTH       @XA21819 31566001
         SR    R5,R5                   ZERO REG                @XA21819 31568001
         LH    R5,WORKSIZE             R5 = RECORD LENGTH      @XA21819 31570001
         SH    R5,HTWO                 GET ACTUAL RECD LENGTH  @XA21819 31572001
         BC    3,DUMPSET1              NO, LENGTH > ZERO       @XA21819 31574001
         MVC   DUMPSIZE(2),DUMPAMT     YES, USE DEFAULT SIZE   @XA21819 31576001
         B     FIN                     EXIT                    @XA21819 31578001
DUMPSET1 STH   R5,WORKSIZE             STORE FOR MOVE TO AVOID @XA21819 31580001
         MVC   DUMPSIZE(2),WORKSIZE    BOUNDARY PROBLEMS       @XA21819 31582001
FIN      SR    R15,R15                 DO NOT PRINT            @XA21819 31584001
         LM    R4,R8,XSAVE             RESTORE WORK REGS       @XA21819 31586001
         BR    R14                     EXIT                    @XA21819 31588001
TWO      DC    F'00000002'                                     @XA21819 31590001
HTWO     DC    H'0002'                                         @XA21819 31592001
WORKSIZE DC    H'0000'                 WORK AREA               @XA21819 31594001
DUMPAMT  DC    H'1754'                 DUMPSIZE IF LEN IS BAD  @XA21819 31596001
************************************************************** @XA21819 31598001
