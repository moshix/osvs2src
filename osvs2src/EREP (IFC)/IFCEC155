*                                                                       00070020
*                                                                       00140020
*  DSGEN MACRO     DSGEN MACRO     DSGEN MACRO     DSGEN MACRO        * 00210020
         SPACE 2                                                        00280020
         MACRO                                                          00350020
&NAME    DSGEN &X1,&X2,&X3,&X4,&X5,&X6,&X7,&X8,&X9,&X10,&X11,&X12,&X13,X00420020
               &X14,&X15,&X16,&X17,&X18,&X19,&X20,&X21,&X22,&X23,&X24, X00490020
               &X25,&X26,&X27,&X28,&X29,&X30,&X31,&X32                  00560020
.*                            THIS MACRO GENERATES ONE DSECT WORD       00630020
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00700020
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00770020
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00840020
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00910020
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00980020
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 01050020
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      01120020
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  01190020
.*                                 THE LOG ITEMS MAY THEN BE USED IN    01260020
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   01330020
         GBLC  &ITEM(2000)                                              01400020
         GBLA  &ITEMNBR,&BITS(2000),&SHIFT(2000)                        01470020
         LCLC  &SYMBOL                                                  01540020
         LCLA  &OP,&COUNT,&BYTE                                         01610020
.LOOP    ANOP                                                           01680020
&OP      SETA  &OP+1                                                    01750020
&ITEMNBR SETA  &ITEMNBR+1                                               01820020
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        01890020
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            01960020
         MNOTE 'NULL BIT COUNT FIELD'                                   02030020
         MEXIT                                                          02100020
.OK1     ANOP                                                           02170020
         AIF   ('&SYMBOL' NE '').OK                                     02240020
         MNOTE 'LOG ITEM SYMBOL NOT PROVIDED'                           02310020
         AGO   .CNT                                                     02380020
.OK      ANOP                                                           02450020
         AIF   ('&SYMBOL'(1,1) EQ '''').CNT                *TEMP*       02520020
&SYMBOL  EQU   *+&BYTE                                                  02590020
&ITEM(&ITEMNBR)  SETC '&SYSLIST(&OP,1)'                                 02660020
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    02730020
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    02800020
.CNT     ANOP                                                           02870020
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   02940020
&BYTE    SETA  &COUNT/8                                                 03010020
         AIF (&OP LT N'&SYSLIST).LOOP                                   03080020
         AIF   (&COUNT NE 32).NOT32                                     03150020
&NAME    DS    4C                                                       03220020
         MEXIT                                                          03290020
.NOT32   AIF   (&COUNT NE 64).ERR                                       03360020
&NAME    DS    8C                                                       03430020
         MEXIT                                                          03500020
.ERR     MNOTE '**BIT COUNT FIELDS DO NOT TOTAL 32 OR 64**'             03570020
         MEND                                                           03640020
*                                                                       03710020
*                                                                       03780020
         MACRO                                                          03850020
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      03920020
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     03990020
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    04060020
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 04130020
.*                                 ENTRIES. LASTLY IT DETERMINES THE    04200020
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    04270020
.*                                 THE PREVIOUS LINE AND STORES THAT #  04340020
.*                                 INTO THE APPROPRIATE BYTE.           04410020
.*                                                                      04480020
.*                                 &DATA(1) - CHARACTER POSITION TO     04550020
.*                                 BEGIN DATA.                          04620020
.*                                                                      04690020
.*                                 &DATA(2) - # OF SPACES BETWEEN       04760020
.*                                 SUCCESSIVE DATA COLUMN STARTING      04830020
.*                                 CHARACTERS.                          04900020
.*                                                                      04970020
.*                                 &LABEL(1) - CHARACTER POSITION       05040020
.*                                 TO BEGIN LABELS.                     05110020
.*                                                                      05180020
.*                                 &LABEL(2) - # OF SPACES BETWEEN      05250020
.*                                 LINE'S LABEL STARTING CHARACTERS.    05320020
.*                                                                      05390020
.*                                 &SKIP= - # OF LINES TO ADVANCE       05460020
.*                                 CARRIAGE TO BEGIN NEXT PRINTED LINE. 05530020
.*                                                                      05600020
.*                                 NEW ENTRIES ARE GENERATED ONLY WHEN  05670020
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   05740020
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 05810020
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   05880020
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 05950020
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    06020020
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   06090020
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   06160020
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  06230020
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   06300020
.*                                 THE SUBJECT LINE.                    06370020
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  06440020
         GBLA  &LASTCC,&INITIAL                                         06510020
         GBLC  &NRENTSX                                                 06580020
         GBLC  &CARRCON                                                 06650020
         GBLB  &TEST                                                    06720020
         LCLA  &S                                                       06790020
&NAME    DS    0C                                                       06860020
         AIF   (&INITIAL EQ 0).FIRSTLN                                  06930020
         LINEND                                                         07000020
         AGO   .NTFIRST                                                 07070020
.FIRSTLN ANOP                                                           07140020
&NRENTSX SETC  'N&SYSNDX'                                               07210020
&INITIAL SETA  1                                                        07280020
N&SYSNDX EQU   *                                                        07350020
.NTFIRST ANOP                                                           07420020
         DC    AL1(0)                                                   07490020
         AIF   ('&DATA(1)' EQ '').CKDTABS                               07560020
         AIF   (&DATA(1) EQ &DB).CKDTABS                                07630020
&DB      SETA  &DATA(1)                                                 07700020
         AGO   .AA                                                      07770020
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                07840020
         AIF   (&DATA(2) EQ &DT).NCDATA                                 07910020
         AGO   .CC                                                      07980020
.AA      AIF   ('&DATA(2)' EQ '').BB                                    08050020
         AIF   (&DATA(2) EQ &DT).BB                                     08120020
.CC      ANOP                                                           08190020
&DT      SETA  &DATA(2)                                                 08260020
.BB      DC    XL2'FFFE'                                                08330020
         DC    FL1'&DB'                                                 08400020
         DC    FL1'&DT'                                                 08470020
&NRENTS  SETA  &NRENTS+1                                                08540020
.NCDATA  ANOP                                                           08610020
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              08680020
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               08750020
&LB      SETA  &LABEL(1)                                                08820020
         AGO   .AAA                                                     08890020
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              08960020
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               09030020
         AGO   .CCC                                                     09100020
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  09170020
         AIF   (&LABEL(2) EQ &LT).BBB                                   09240020
.CCC     ANOP                                                           09310020
&LT      SETA  &LABEL(2)                                                09380020
.BBB     DC    XL2'FFFF'                                                09450020
         DC    FL1'&LB'                                                 09520020
         DC    FL1'&LT'                                                 09590020
&NRENTS  SETA  &NRENTS+1                                                09660020
.NCLABEL ANOP                                                           09730020
         AIF   ('&SKIP' NE '').CONT                                     09800020
         AIF   (&TEST EQ 0).NOCHANG                                     09870020
&S       SETA  &LASTCC                                                  09940020
         AGO   .DEFAULT                                                 10010020
.CONT    ANOP                                                           10080020
         AIF   (&TEST EQ 0).AB                                          10150020
&S       SETA  &SKIP                                                    10220020
.DEFAULT ANOP                                                           10290020
         ORG   &CARRCON+3                                               10360020
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 10430020
         ORG                                                            10500020
&LASTCC  SETA  &S                                                       10570020
&TEST    SETB  0                                                        10640020
         AGO   .DD                                                      10710020
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               10780020
A&SYSNDX DC    XL2'FFFB'                                                10850020
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              10920020
         DC    FL1'9'                                                   10990020
&LASTCC  SETA  &SKIP                                                    11060020
&TEST    SETB  1                                                        11130020
&CARRCON SETC  'A&SYSNDX'                                               11200020
&NRENTS  SETA  &NRENTS+1                                                11270020
.DD      ANOP                                                           11340020
.NOCHANG ANOP                                                           11410020
         MEND                                                           11480020
