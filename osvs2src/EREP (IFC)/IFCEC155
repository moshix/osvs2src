*********************************************************************** 11690020
*                                                                       11760020
*                                                                       11830020
         MACRO                                                          11900020
         BIN   &LOGITEM,&SKIP$OR                                        11970020
.*                                 &LOGITEM - NAME OF ITEM IN LOG DSECT 12040020
.*                                                                      12110020
.*                                 &SKIP$OR - NUMBER OF SPACES TO SKIP  12180020
.*                                 ON MULTIPLE PRINT OR NUMBER OF       12250020
.*                                 SPACES TO BACK UP FROM COLUMN FOR    12320020
.*                                 START OF PRINTING. THE LETTER B      12390020
.*                                 PRECEEDS THE NUMBER (B5) IF THE      12460020
.*                                 LATTER IS DESIRED. OMISSION OF THIS  12530020
.*                                 OPERAND FORCES OPTION 2 WITH BACKUP  12600020
.*                                 SPACING EQUAL TO THE NUMBER OF BITS  12670020
.*                                 IN THE ITEM DEFINITION (VIA MACRO    12740020
.*                                 'DSGEN'). A LIMIT OF 15 BITS IS      12810020
.*                                 IMPOSED FOR THE TOTAL NUMBER OF BITS 12880020
.*                                 TO PRINT.                            12950020
         GBLC  &ITEM(2000)                                              13020020
         GBLA  &ITEMNBR,&BITS(2000),&SHIFT(2000),&NRENTS                13090020
         GBLC  &DSECT                                                   13160020
         LCLB  &B                                                       13230020
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   13300020
         LCLA  &TEMP,&T                                                 13370020
         LCLC  &S                                                       13440020
.LOOP    ANOP                                                           13510020
&NDX     SETA  &NDX+1                                                   13580020
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      13650020
         AIF   (&NDX LT &ITEMNBR).LOOP                                  13720020
         MNOTE '**LOG ITEM NOT FOUND**'                                 13790020
         ROUTINE  DUM,SKIPPER                                           13860020
         MEXIT                                                          13930020
.FOUND   ANOP                                                           14000020
&ISHIFT  SETA  &SHIFT(&NDX)                                             14070020
&IBITS   SETA  &BITS(&NDX)                                              14140020
&T       SETA  &IBITS-1                                                 14210020
&B       SETB  0                                                        14280020
         AIF   ('&SKIP$OR' EQ '').BACKUP                                14350020
&K       SETA  K'&SKIP$OR                                               14420020
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 14490020
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   14560020
&T       SETA  &S                                                       14630020
.BACKUP  ANOP                                                           14700020
&NRENTS  SETA  &NRENTS+1                                                14770020
&TEMP    SETA  &ISHIFT                                                  14840020
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       14910020
         AIF   (&IBITS EQ 1 AND &B EQ 0).OUT                            14980020
         ORG   A&SYSNDX                                                 15050020
&B       SETB  1                                                        15120020
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           15190020
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             15260020
.OUT     ANOP                                                           15330020
         MEND                                                           15400020
*                                                                       15470020
*                                                                       15540020
*********************************************************************** 15610020
*                                                                       15680020
*                                                                       15750020
         MACRO                                                          15820020
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 15890020
.*                                                                      15960020
.*                                 THIS MACRO FACILITATES GENERATION    16030020
.*                                 OF THE TABLE ENTRIES SPECIFYING HEX  16100020
.*                                 TYPE CONVERSIONS.                    16170020
.*                                                                      16240020
.*                                 &LOGITEM - NAME OF DATA ITEM TO BE   16310020
.*                                 CONVERTED.                           16380020
.*                                                                      16450020
.*                                 &SKIP$OR - NUMBER OF SPACES TO SKIP  16520020
.*                                 ON MULTIPLE PRINT OF NUMBER OF       16590020
.*                                 SPACES TO BACK UP FROM COLUMN FOR    16660020
.*                                 START OF PRINTING. THE LETTER B      16730020
.*                                 PRECEEDS THE NUMBER (B8) IF THE      16800020
.*                                 LATTER OPTION IS DESIRED. OMISSION   16870020
.*                                 OF THIS OPERAND FORCES OPTION 2 WITH 16940020
.*                                 BACKUP SPACING EQUAL TO THE NUMBER   17010020
.*                                 OF BYTES SPECIFIED BY THE NEXT       17080020
.*                                 OPERAND (TWICE THIS NUMBER IF SUFFIX 17150020
.*                                 IS EXPLICITLY OR IMPLICITLY B).      17220020
.*                                                                      17290020
.*                                 &BYTES - NUMBER OF TOTAL INPUT BYTES 17360020
.*                                 TO PROCESS. THIS NUMBER IS SUFFIXED  17430020
.*                                 WITH A L,R, OR B TO STIPULATE        17500020
.*                                 PRINTING OF THE LEFT HALF, RIGHT     17570020
.*                                 HALF, OR BOTH HALVES OF THE BYTE     17640020
.*                                 RESPECTIVELY. OMISSION OF THIS       17710020
.*                                 OPERAND DEFAULTS TO B WITH THE       17780020
.*                                 NUMBER OF BYTES EQUAL TO THE ITEM    17850020
.*                                 LENGTH.                              17920020
.*                                                                      17990020
.*                                 A NEW ENTRY IS GENERATED ONLY WHEN   18060020
.*                                 NECESSARY. THIS MACRO MUST BE THE    18130020
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   18200020
.*                                 THE SUBJECT LINE(S)                  18270020
         GBLC  &ITEM(2000),&DSECT                                       18340020
         GBLA  &ITEMNBR,&BITS(2000),&SHIFT(2000),&NRENTS                18410020
         LCLB  &B                                                       18480020
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  18550020
         LCLC  &S,&LETTER                                               18620020
.LOOP    ANOP                                                           18690020
&NDX     SETA  &NDX+1                                                   18760020
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      18830020
         AIF   (&NDX LT &ITEMNBR).LOOP                                  18900020
         MNOTE '**LOG ITEM NOT FOUND**'                                 18970020
         MEXIT                                                          19040020
.FOUND   ANOP                                                           19110020
&ISHIFT  SETA  &SHIFT(&NDX)                                             19180020
&IBITS   SETA  &BITS(&NDX)                                              19250020
&CODE    SETA  1                                                        19320020
&K       SETA  K'&BYTES                                                 19390020
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            19460020
         MNOTE 'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'     19530020
         MEXIT                                                          19600020
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 19670020
&LETTER  SETC  '&BYTES'(&K,1)                                           19740020
&S       SETC  '&BYTES'(1,&K-1)                                         19810020
&BYTESP  SETA  &S                                                       19880020
         AIF   ('&LETTER' EQ 'B').BOTH                                  19950020
         AIF   ('&LETTER' EQ 'L').LEFT                                  20020020
&CODE    SETA  2                                                        20090020
         AGO   .L                                                       20160020
.LEFT    ANOP                                                           20230020
&CODE    SETA  1                                                        20300020
         AGO   .L                                                       20370020
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       20440020
.ERR     MNOTE 'DATA NOT ALIGNED ON HEX OR BYTE BOUNDARY'               20510020
         MEXIT                                                          20580020
.USEDEFN ANOP                                                           20650020
&BYTESP  SETA  &IBITS/8                                                 20720020
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       20790020
&CODE    SETA  3                                                        20860020
&T       SETA  &BYTESP*2-1                                              20930020
&S       SETC  '&T'                                                     21000020
.OK2     AIF   (&ISHIFT EQ 0).L                                         21070020
&CODE    SETA  2                                                        21140020
.L       ANOP                                                           21210020
&B       SETB  1                                                        21280020
         AIF   ('&SKIP$OR' EQ '').BACKUP                                21350020
&K       SETA  K'&SKIP$OR                                               21420020
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 21490020
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   21560020
.BACKUP  ANOP                                                           21630020
&NRENTS  SETA  &NRENTS+1                                                21700020
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           21770020
         DC    BL.1'&B',FL.7'&S',AL.4(&BYTESP),FL.4'&CODE'              21840020
.OUT     ANOP                                                           21910020
         MEND                                                           21980020
