         LCLC  &MARGIN,&TACK                                            25410020
         LCLC  &TEXT                                                    25480020
         LCLA  &OP,&K,&START,&NRCHAR,&REMAIN                            25550020
&MARGIN  SETC  '*'                                                      25620020
         AIF   ('&NAME' EQ '').LOOPOUT                                  25690020
&LPTR    SETA  &LPTR+1                                                  25760020
&LBL(&LPTR) SETC '&NAME'                                                25830020
&LBLCTR(&LPTR) SETA &LABELNR+1                                          25900020
.LOOPOUT ANOP                                                           25970020
&OP      SETA  &OP+1                                                    26040020
&K       SETA  K'&SYSLIST(&OP)                                          26110020
         AIF (&K GE 2).NORM                                             26180020
&K       SETA  2                                                        26250020
.NORM    ANOP                                                           26320020
&START   SETA  2                                                        26390020
         AIF (&OP LT N'&SYSLIST).NOTLAST                                26460020
&MARGIN  SETC  '$'                                                      26530020
.NOTLAST ANOP                                                           26600020
&NRCHAR  SETA  8                                                        26670020
&TACK    SETC  ''                                                       26740020
.LOOPIN  ANOP                                                           26810020
&REMAIN  SETA  &K-&START                                                26880020
         AIF (&REMAIN GE 8).MORE                                        26950020
&TACK    SETC  '&MARGIN'                                                27020020
&NRCHAR  SETA  &REMAIN                                                  27090020
.MORE    ANOP                                                           27160020
&TEXT    SETC  ''                                                       27230020
         AIF (&REMAIN EQ 0).ENDTEXT                                     27300020
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          27370020
.ENDTEXT ANOP                                                           27440020
&LABELNR SETA  &LABELNR+1                                               27510020
&LABELS(&LABELNR) SETC '&TEXT'.'&TACK'                                  27580020
&START   SETA  &START+8                                                 27650020
         AIF ('&TACK' EQ '').LOOPIN                                     27720020
         AIF ('&TACK' EQ '*').LOOPOUT                                   27790020
         MEND                                                           27860020
