       TITLE   'IFCEC165 MODEL 165 VERSION 2 CPU EDIT TABLE 1'          00030002
*  DSGEN MACRO     DSGEN MACRO     DSGEN MACRO     DSGEN MACRO        * 00060002
         SPACE 2                                                        00090002
         MACRO                                                          00120002
&NAME    DSGEN                                                          00150002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00180002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00210002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00240002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00270002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00300002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00330002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00360002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00390002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00420002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00450002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   00480002
         GBLC &ITEM(2000)                                               00510002
         GBLA &ITEMNBR,&BITS(2000),&SHIFT(2000)                         00540002
         LCLB  &ONLY1,&CTIND                                            00570002
         LCLA  &START1,&START2,&END1,&END2                              00600002
         LCLC  &SYMBOL,&EQUSYM                                          00630002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 00660002
         ACTR  100000                                                   00690002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            00720002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     00750002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   00780002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        00810002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            00840002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             00870002
         AGO   .SET2A                   BRANCH AROUND                   00900002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              00930002
         MEXIT                                                          00960002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X00990002
               ).SET2                   TEST FOR NO SECOND              01020002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   01050002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        01080002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            01110002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              01140002
         AGO   .LOOP                                                    01170002
.SET2    ANOP                                                           01200002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 01230002
         AGO   .LOOP                                                    01260002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             01290002
         MEXIT                                                          01320002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               01350002
         MEXIT                                                          01380002
.LOOP    ANOP                                                           01410002
&OP      SETA  &OP+1                                                    01440002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           01470002
&ITEMNBR SETA  &ITEMNBR+1                                               01500002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            01530002
         MNOTE '**NULL BIT COUNT FIELD**'                               01560002
         MEXIT                                                          01590002
.OK1     ANOP                                                           01620002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        01650002
         AIF   ('&SYMBOL' NE '').OK                                     01680002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         01710002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         01740002
         AGO   .START                   GET NEXT                        01770002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        01800002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          01830002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        01860002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         01890002
         AGO   .NO3                     BRANCH AROUND                   01920002
.OK2     ANOP                                                           01950002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              01980002
.NO3     ANOP                                                           02010002
&SYMBOL  EQU   *+&BYTE                                                  02040002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             02070002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    02100002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    02130002
.CNT     ANOP                                                           02160002
&CTIND   SETB  1                        SET COUNT INDICATOR             02190002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   02220002
&BYTE    SETA  &COUNT/8                                                 02250002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        02280002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        02310002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             02340002
&END1    SETA  &END1+1                  INCREMENT COUNT                 02370002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             02400002
         AGO   .LOOP                    GENERATE NEXT                   02430002
.TSTST   ANOP                                                           02460002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               02490002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        02520002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      02550002
.NSET1   ANOP                                                           02580002
&OP      SETA  &OP-1                    DECREMENT INDEX                 02610002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           02640002
         AGO   .LOOP                    GO DO NEXT                      02670002
.LPDONE  ANOP                                                           02700002
&ONLY1   SETB  0                        CLEAR INDICATOR                 02730002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      02760002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 02790002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               02820002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             02850002
&NAME    DS    &COUNT.C                                                 02880002
.END     MEXIT                                                          02910002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          02940002
         MEXIT                                                          02970002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   03000002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   03030002
         MEXIT                                                          03060002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         03090002
*****               THE FOLLOWING STATEMENT                *****        03120002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        03150002
.EQU2    ANOP                                                           03180002
&EQUCNT  SETA  0                        CLEAR COUNTER                   03210002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 03240002
.EQULOP  ANOP                                                           03270002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 03300002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     03330002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                03360002
         AGO   .EQULOP                  NO-GET NEXT                     03390002
.EQUOUT  ANOP                                                           03420002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  03450002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              03480002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 03510002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             03540002
         AGO   .START                   RETURN FOR NEXT                 03570002
         MEND                                                           03600002
         MACRO                                                          03630002
         LINEND                                                         03660002
.*                            THIS MACRO IS USED ONLY BY THE LINE       03690002
.*                                 MACRO TO SET THE NUMBER OF ENTRIES   03720002
.*                                 ASSOCIATED WITH EACH LINE.           03750002
         GBLA  &NRENTS                                                  03780002
         GBLC &NRENTSX                                                  03810002
         AIF   (&NRENTS EQ 0).NORENTS   TEST FOR NO COUNT               03840002
         ORG   &NRENTSX                                                 03870002
         DC    FL1'&NRENTS'                                             03900002
         ORG                                                            03930002
&NRENTS  SETA  0                                                        03960002
.NORENTS ANOP                                                           03990002
B&SYSNDX EQU   *                                                        04020002
&NRENTSX SETC  'B&SYSNDX'                                               04050002
         MEND                                                           04080002
*                                                                       04110002
         MACRO                                                          04140002
         BIN                                                            04170002
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   04200002
.*                                      RECORD DSECT                    04230002
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      04260002
.*                                      TO SKIP ON MULTIPLE             04290002
.*                                      PRINT OR NBR OF SPACES          04320002
.*                                      TO BACK UP FROM COLUMN FOR      04350002
.*                                      START OF PRINTING. THE LETTER   04380002
.*                                      B PRECEEDS THE NUMBER (B5) IF   04410002
.*                                      THE LATTER IS DESIRED. OMISSION 04440002
.*                                      OF THIS OPERAND FORCES OPTION 2 04470002
.*                                      WITH BACKUP SPACING EQUAL TO    04500002
.*                                 THE NUMBER OF BITS IN THE ITEM       04530002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      04560002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     04590002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       04620002
         GBLA  &INITIAL                                                 04650002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               04680002
         GBLC &ITEM(2000)                                               04710002
         GBLA &ITEMNBR,&BITS(2000),&SHIFT(2000),&NRENTS                 04740002
         GBLC  &DSECT                                                   04770002
         LCLB  &B,&SUB                                                  04800002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   04830002
         LCLA  &TEMP,&T,&OP                                             04860002
         LCLC  &S                                                       04890002
         AIF   (&DIAGTST EQ 0).NOMSG                                    04920002
         AIF   (&INITIAL EQ 0).NOMSG                                    04950002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    04980002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  05010002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     05040002
*        LINE                                                           05070002
         LINE                                                           05100002
.NOMSG   ANOP                                                           05130002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  05160002
&SUB     SETB  1                                                        05190002
.LOOP    ANOP                                                           05220002
&OP      SETA  &OP+1                                                    05250002
.LOOP1   ANOP                                                           05280002
&NDX     SETA  &NDX+1                                                   05310002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND               05340002
         AIF   (&NDX LT &ITEMNBR).LOOP1                                 05370002
         MNOTE '**LOG ITEM NOT DEFINED**'                               05400002
         MEXIT                                                          05430002
.FOUND   ANOP                                                           05460002
&ISHIFT  SETA  &SHIFT(&NDX)                                             05490002
&IBITS   SETA  &BITS(&NDX)                                              05520002
&T       SETA  &IBITS-1                                                 05550002
&B       SETB  0                                                        05580002
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      05610002
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  05640002
         MEXIT                                                          05670002
.SUB0    AIF   (&SUB EQ 1).SUBA                                         05700002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             05730002
&K       SETA  K'&SYSLIST(2)                                            05760002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              05790002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                05820002
         AGO   .SUBB                                                    05850002
.SUBA    ANOP                                                           05880002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         05910002
&K       SETA  K'&SYSLIST(&OP,2)                                        05940002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          05970002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            06000002
.SUBB    ANOP                                                           06030002
&T       SETA  &S                                                       06060002
.BACKUP  ANOP                                                           06090002
&NRENTS  SETA  &NRENTS+1                                                06120002
&TEMP    SETA  &ISHIFT                                                  06150002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            06180002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                06210002
         AGO   .OUT                                                     06240002
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           06270002
&B       SETB  1                                                        06300002
.BIN3    ANOP                                                           06330002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    06360002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             06390002
.OUT     ANOP                                                           06420002
&NDX     SETA  0                                                        06450002
         AIF   (&SUB EQ 0).OUTA                                         06480002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 06510002
.OUTA    ANOP                                                           06540002
         MEND                                                           06570002
         MACRO                                                          06600002
         HEX                                                            06630002
.*                            THIS MACRO FACILITATES GENERATION OF THE  06660002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    06690002
.*                                 CONVERSIONS.                         06720002
.*                            &SYSLIST(&OP,1) - NAME OF DATA ITEM TO BE 06750002
.*                                 CONVERTED                            06780002
.*                            &SYSLIST(&OP,2) - # OF SPACES TO SKIP ON  06810002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   06840002
.*                                 BACK UP FROM COLUMN FOR START OF     06870002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  06900002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  06930002
.*                                 DESIRED. OMISSION OF THIS OPERAND    06960002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  06990002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  07020002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 07050002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    07080002
.*                                 IMPLICITLY B).                       07110002
.*                            &SYSLIST(&OP,3) - NUMBER OF BYTES TO      07140002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     07170002
.*                                 WITH AN L,R, OR B TO STIPULATE       07200002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     07230002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     07260002
.*                                 RESPECTIVELY. OMISSION OF THIS       07290002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  07320002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 07350002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 07380002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   07410002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     07440002
.*                                 SUBJECT LINE(S).                     07470002
         GBLC &ITEM(2000),&DSECT                                        07500002
         GBLA &ITEMNBR,&BITS(2000),&SHIFT(2000),&NRENTS                 07530002
         GBLA  &INITIAL                                                 07560002
         GBLB  &DIAGTST,&DIAGSET,&DIAGLIN                               07590002
         LCLB  &B,&SUB                                                  07620002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP,&OP              07650002
         LCLC  &S,&LETTER                                               07680002
         AIF   (&DIAGTST EQ 0).NOMSG                                    07710002
         AIF   (&INITIAL EQ 0).NOMSG                                    07740002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    07770002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  07800002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     07830002
*        LINE                                                           07860002
         LINE                                                           07890002
.NOMSG   ANOP                                                           07920002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  07950002
&SUB     SETB  1                                                        07980002
.LOOP    ANOP                                                           08010002
&OP      SETA  &OP+1                                                    08040002
.LOOP1   ANOP                                                           08070002
&NDX     SETA  &NDX+1                                                   08100002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND               08130002
         AIF   (&NDX LT &ITEMNBR).LOOP1                                 08160002
         MNOTE '**LOG ITEM NOT DEFINED**'                               08190002
         MEXIT                                                          08220002
.FOUND   ANOP                                                           08250002
&ISHIFT  SETA  &SHIFT(&NDX)                                             08280002
&IBITS   SETA  &BITS(&NDX)                                              08310002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).ALIGN                     08340002
.ERR     MNOTE '**DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY**'          08370002
         MEXIT                                                          08400002
.ALIGN   ANOP                                                           08430002
&CODE    SETA  1                                                        08460002
         AIF   (&SUB EQ 1).SUBA                                         08490002
&K       SETA  K'&SYSLIST(3)                                            08520002
         AGO   .SUBB                                                    08550002
.SUBA    ANOP                                                           08580002
&K       SETA  K'&SYSLIST(&OP,3)                                        08610002
.SUBB    ANOP                                                           08640002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            08670002
         MNOTE '**BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL**' 08700002
         MEXIT                                                          08730002
.OK      AIF   (&SUB EQ 1).SUBC                                         08760002
         AIF   ('&SYSLIST(3)' EQ '').USEDEFN                            08790002
&LETTER  SETC  '&SYSLIST(3)'(&K,1)                                      08820002
&S       SETC  '&SYSLIST(3)'(1,&K-1)                                    08850002
         AGO   .SUBD                                                    08880002
.SUBC    ANOP                                                           08910002
         AIF   ('&SYSLIST(&OP,3)' EQ '').USEDEFN                        08940002
&LETTER  SETC  '&SYSLIST(&OP,3)'(&K,1)                                  08970002
&S       SETC  '&SYSLIST(&OP,3)'(1,&K-1)                                09000002
.SUBD    ANOP                                                           09030002
&BYTESP  SETA  &S                                                       09060002
         AIF   ('&LETTER' EQ 'B').BOTH                                  09090002
         AIF   ('&LETTER' EQ 'L').LEFT                                  09120002
&CODE    SETA  2                                                        09150002
         AGO   .L                                                       09180002
.LEFT    ANOP                                                           09210002
&CODE    SETA  1                                                        09240002
         AGO   .L                                                       09270002
.USEDEFN ANOP                                                           09300002
&BYTESP  SETA  &IBITS/8                                                 09330002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       09360002
&CODE    SETA  3                                                        09390002
&T       SETA  &BYTESP*2-1                                              09420002
&S       SETC  '&T'                                                     09450002
.OK2     AIF   (&ISHIFT EQ 0).L                                         09480002
&CODE    SETA  2                                                        09510002
.L       ANOP                                                           09540002
&B       SETB  1                                                        09570002
         AIF   (&SUB EQ 1).SUBE                                         09600002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             09630002
&K       SETA  K'&SYSLIST(2)                                            09660002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              09690002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                09720002
         AGO   .SUBF                                                    09750002
.SUBE    ANOP                                                           09780002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         09810002
&K       SETA  K'&SYSLIST(&OP,2)                                        09840002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          09870002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            09900002
.SUBF    ANOP                                                           09930002
.BACKUP  ANOP                                                           09960002
&NRENTS  SETA  &NRENTS+1                                                09990002
         AIF   (&BYTESP GT 0 AND &BYTESP LT 16).SUB0                    10020002
         MNOTE '**NO. OF BYTES OUTSIDE RANGE OF 1-15**'                 10050002
         MEXIT                                                          10080002
.SUB0    AIF   (&SUB EQ 1).SUBG                                         10110002
         DC    AL.12(&SYSLIST(1)-&DSECT),XL.4'A'                        10140002
         AGO   .SUBH                                                    10170002
.SUBG    ANOP                                                           10200002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'A'                    10230002
.SUBH    ANOP                                                           10260002
         DC    BL.1'&B',FL.7'&S',AL.4(&BYTESP),FL.4'&CODE'              10290002
.OUT     ANOP                                                           10320002
&NDX     SETA  0                                                        10350002
         AIF   (&SUB EQ 0).OUTA                                         10380002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 10410002
.OUTA    ANOP                                                           10440002
         MEND                                                           10470002
