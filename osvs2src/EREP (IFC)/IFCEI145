************************************************************** @ZA30677 34075500
* PRINT THE CURRENT LINE IF THIS IS A 148 CONSOLE (3148) CCH RECORD   * 34076000
************************************************************** @ZA30677 34076500
PRT3148  TM    21(R2),X'48'             IS THIS A 148 CPU      @ZA30677 34077000
         BO    CK3148                   YES                    @ZA30677 34077500
         B     NOPRT                    NO, DO NOT PRINT LINE  @ZA30677 34078000
CK3148   CLC   XDEVT2(L2),DEV3148       3148 CONSOLE CCH ?     @ZA30677 34078500
         BCR   8,R14                    YES, PRINT THIS LINE   @ZA30677 34079000
         B     NOPRT                    NO, DO NOT PRINT LINE  @ZA30677 34079500
************************************************************** @ZA30677 34080000
* PRINT THE CURRENT LINE IF THIS IS A 148 CCH RECORD           @ZA30677 34080500
************************************************************** @ZA30677 34081000
PRT148   TM    21(R2),X'48'             IS THIS A 148 CPU      @ZA30677 34081500
         BOR   R14                      YES                    @ZA30677 34082000
         B     NOPRT                    NO, DO NOT PRINT LINE  @ZA30677 34082500
************************************************************** @ZA30677 34083000
* PRINT THE CURRENT LINE IF THIS IS NOT A 148 CONSOLE (3148) CCH RECD * 34083500
************************************************************** @ZA30677 34084000
NPRT3148 TM    21(R2),X'48'             IS THIS A 148 CPU      @ZA30677 34084500
         BCR   14,R14                   NO, SO PRINT THIS LINE @ZA30677 34085000
         CLC   XDEVT2(L2),DEV3148       3148 CONSOLE CCH ?     @ZA30677 34085500
         BCR   7,R14                    NO, SO PRINT THIS LINE @ZA30677 34086000
         B     NOPRT                    YES, DO NOT PRINT LINE @ZA30677 34086500
DEV3148  DC    X'100F'                                         @ZA30677 34087000
************************************************************** @ZA30677 34087500
* PRINT THE CURRENT LINE IF THIS IS NOT A 148 CCH RECD         @ZA30677 34088000
************************************************************** @ZA30677 34088500
NPRT148  TM    21(R2),X'48'             IS THIS A 148 CPU      @ZA30677 34089000
         BNOR  R14                      NO, SO PRINT THIS LINE @ZA30677 34089500
         B     NOPRT                    YES, DO NOT PRINT LINE @ZA30677 34090000
************************************************************** @ZA30677 34090500
* SKIP OVER FORMATTING 145 TA/TT BYTES IF CURR. RECD IS 148    @ZA30677 34091000
************************************************************** @ZA30677 34091500
TEST148  TM    21(R2),X'48'             IS THIS A 148 CPU      @ZA30677 34092000
         BCR   14,R14                   NO, EXIT               @ZA30677 34092500
         CLC   XDEVT2(L2),DEV3148       3148 CONSOLE CCH ?     @ZA30677 34093000
         BCR   7,R14                    NO, EXIT               @ZA30677 34093500
         MVC   REG9(8),SKIP148          YES, UPDATE L/L PTRS   @ZA30677 34094000
         B     NOPRT                    DO NOT PRINT THIS LINE @ZA30677 34094500
SKIP148  DC    A(LINE148-4)                                    @ZA30677 34095000
         DC    A(LAB148)                                       @ZA30677 34095500
************************************************************** @ZA30677 34096000
* SKIP OVER FORMATTING 148 TA/TT BYTES BECAUSE CURRENT RECD IS 145    * 34096500
************************************************************** @ZA30677 34097000
JUMPN148 MVC   REG9(8),SKIPN148         UPDATE LINE/LABEL PTRS @ZA30677 34097500
         SR    R15,R15                  DO NOT PRINT THIS LINE @ZA30677 34098000
         BR    R14                      EXIT                   @ZA30677 34098500
SKIPN148 DC    A(VS2LINE-4)                                    @ZA30677 34099000
         DC    A(LABN148)                                      @ZA30677 34099500
************************************************************** @ZA30677 34100000
* DETERMINE THE PROPER LABEL FOR BITS 0,1 OF THE 148 TT BYTE   @ZA30677 34100500
* BIT 0 1  REQUIRED LABEL                                      @ZA30677 34101000
*     0 0  DISPLAY                                             @ZA30677 34101500
*     0 1  3286                                                @ZA30677 34102000
*     1 0  3203 NO. 1                                          @ZA30677 34102500
*     1 1  3203 NO. 2                                          @ZA30677 34103000
************************************************************** @ZA30677 34103500
EXTPTR   STM   R5,R8,XSAVE              SAVE THE WORK REGS     @ZA30677 34104000
         LR    R5,R3                    GET PTR TO POS IN BUFR @ZA30677 34104500
         S     R5,TWOFIVE               BACK UP BUFR PTR       @ZA30677 34105000
         TM    CONSWD08,X'C0'           CHK EXTN PTR BITS      @ZA30677 34105500
         BZ    DISP00                   DISPLAY INDICATED      @ZA30677 34106000
         BO    N320311                  3203 NO. 2 INDICATED   @ZA30677 34106500
         TM    CONSWD08,X'80'           CHECK BIT              @ZA30677 34107000
         BO    N320310                  3203 NO. 1 INDICATED   @ZA30677 34107500
         MVC   L0(L26,R5),LAB3286       MOVE IN 3286 LABEL     @ZA30677 34108000
         B     RETURN                   EXIT                   @ZA30677 34108500
DISP00   MVC   L0(L26,R5),LABDISP       MOVE IN DISPLAY LABEL  @ZA30677 34109000
         B     RETURN                   EXIT                   @ZA30677 34109500
N320311  MVC   L0(L26,R5),LAB32032      MOVE IN 3203 NO. 2 LAB @ZA30677 34110000
         B     RETURN                   EXIT                   @ZA30677 34110500
N320310  MVC   L0(L26,R5),LAB32031      MOVE IN 3203 NO. 1 LAB @ZA30677 34111000
         B     RETURN                   EXIT                   @ZA30677 34111500
TWOFIVE  DC    F'25'                                           @ZA30677 34112000
LAB32031 DC    C'EXT PTR (3203 NO. 1)    10'                   @ZA30677 34112500
LAB32032 DC    C'EXT PTR (3203 NO. 2)    11'                   @ZA30677 34113000
LABDISP  DC    C'EXT PTR (DISPLAY)       00'                   @ZA30677 34113500
LAB3286  DC    C'EXT PTR (3286)          01'                   @ZA30677 34114000
*                                                                     * 34122002
*                                                                     * 34169002
* IN CASE OF AN INTERFACE CONTROL CHECK ONLY NINE WORDS OF LOGOUT ARE * 34216002
* VALID AND THE REST HAS TO BE SKIPPED OVER.                            34263002
*                                                                     * 34310002
WHERETO  TM    L0(R4),X'02'       IS THIS INTERFACE CONTROL CHECK       34357002
         BCR   8,R14              NO RETURN                             34404002
         ST    R7,XSAVE           SAVE REGISTER                         34451002
         L     R7,NEWENT          GET NEW ENTRY POINTER                 34498002
         ST    R7,REG9                                                  34545002
         L     R7,NEWLAB          GET NEW LABEL POINTER                 34592002
         ST    R7,LP                                                    34639002
         L     R7,XSAVE           RELOAD THE REGISTER                   34686002
         B     NOPRT              GO TO SET REG FOR NO PRINT            34733002
SETSOURC STM   R5,R8,XSAVE              SAVE REGS                       34780002
         XC    NOPRO(4),NOPRO           CLEAR                  @YM2954P 34800002
         MVC   L0(L3,R3),CCCH           MOVE CHANNEL RECORD             34827002
         CLI   L0(R4),HEX20             TEST REC SOURCE                 34874002
         BE    RETURN                                                   34921002
         CLI   L0(R4),H21               TEST SOURCE            @YM2954P 34931002
         BE    RETURN                   CCH VS2(MVM)           @YM2954P 34941002
         MVC   L0(L3,R3),MCHH           MOVE MCH RECORD                 34968002
         B     RETURN                   RETURN                          35015002
SETPGMN  MVC   L0(L8,R3),L0(R4)         MOVE 8 CHAR NAME TO BUFF        35156002
         BR    R14                                                      35203002
