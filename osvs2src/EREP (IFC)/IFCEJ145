         TITLE 'IFCEJ145 MODEL 145 CHANNEL EDIT LOAD 2'                 00115002
         MACRO                                                          00230002
&NAME    DSGEN                                                          00345002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00460002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00575002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00690002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00805002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00920002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   01035002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 01150002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      01265002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  01380002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    01495002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   01610002
         GBLC &ITEM(1000)                                               01725002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         01840002
         LCLB  &ONLY1,&CTIND                                            01955002
         LCLA  &START1,&START2,&END1,&END2                              02070002
         LCLC  &SYMBOL,&EQUSYM                                          02185002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 02300002
         ACTR  100000                                                   02415002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            02530002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     02645002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' AND '&SYSLIST(&OP+1,4)' EQ '').ERR1  02760002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   02875002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        02990002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            03105002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             03220002
         AGO   .SET2A                   BRANCH AROUND                   03335002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              03450002
         MEXIT                                                          03565002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X03680002
               ).SET2                   TEST FOR NO SECOND              03795002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   03910002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        04025002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            04140002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              04255002
         AGO   .LOOP                                                    04370002
.SET2    ANOP                                                           04485002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 04600002
         AGO   .LOOP                                                    04715002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             04830002
         MEXIT                                                          04945002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               05060002
         MEXIT                                                          05175002
.LOOP    ANOP                                                           05290002
&OP      SETA  &OP+1                                                    05405002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           05520002
&ITEMNBR SETA  &ITEMNBR+1                                               05635002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            05750002
         MNOTE '**NULL BIT COUNT FIELD**'                               05865002
         MEXIT                                                          05980002
.OK1     ANOP                                                           06095002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        06210002
         AIF   ('&SYMBOL' NE '').OK                                     06325002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         06440002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         06555002
         AGO   .START                   GET NEXT                        06670002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        06785002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          06900002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        07015002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         07130002
         AGO   .NO3                     BRANCH AROUND                   07245002
.OK2     ANOP                                                           07360002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              07475002
.NO3     ANOP                                                           07590002
&SYMBOL  EQU   *+&BYTE                                                  07705002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             07820002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    07935002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    08050002
.CNT     ANOP                                                           08165002
&CTIND   SETB  1                        SET COUNT INDICATOR             08280002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   08395002
&BYTE    SETA  &COUNT/8                                                 08510002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        08625002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        08740002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             08855002
&END1    SETA  &END1+1                  INCREMENT COUNT                 08970002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             09085002
         AGO   .LOOP                    GENERATE NEXT                   09200002
.TSTST   ANOP                                                           09315002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               09430002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        09545002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      09660002
.NSET1   ANOP                                                           09775002
&OP      SETA  &OP-1                    DECREMENT INDEX                 09890002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           10005002
         AGO   .LOOP                    GO DO NEXT                      10120002
.LPDONE  ANOP                                                           10235002
&ONLY1   SETB  0                        CLEAR INDICATOR                 10350002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      10465002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 10580002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               10695002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             10810002
&NAME    DS    &COUNT.C                                                 10925002
.END     MEXIT                                                          11040002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          11155002
         MEXIT                                                          11270002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   11385002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   11500002
         MEXIT                                                          11615002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         11730002
*****               THE FOLLOWING STATEMENT                *****        11845002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        11960002
.EQU2    ANOP                                                           12075002
&EQUCNT  SETA  0                        CLEAR COUNTER                   12190002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 12305002
.EQULOP  ANOP                                                           12420002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 12535002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     12650002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                12765002
         AGO   .EQULOP                  NO-GET NEXT                     12880002
.EQUOUT  ANOP                                                           12995002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  13110002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              13225002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 13340002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             13455002
         AGO   .START                   RETURN FOR NEXT                 13570002
         MEND                                                           13685002
         EJECT                                                          13800002
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 13915002
         SPACE 2                                                        14030002
         MACRO                                                          14145002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      14260002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     14375002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    14490002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 14605002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    14720002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    14835002
.*                                 THE PREVIOUS LINE AND STORES THAT #  14950002
.*                                 INTO THE APPROPRIATE BYTE.           15065002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    15180002
.*                                 DATA.                                15295002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 15410002
.*                                 DATA COLUMN STARTING CHARACTERS.     15525002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   15640002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 15755002
.*                                 STARTING CHARACTERS.                 15870002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   15985002
.*                                 TO BEGIN NEXT PRINTED LINE.          16100002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       16215002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   16330002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 16445002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   16560002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 16675002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    16790002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   16905002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   17020002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  17135002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   17250002
.*                                 THE SUBJECT LINE                     17365002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  17480002
         GBLA  &LASTCC,&INITIAL                                         17595002
         GBLC  &NRENTSX                                                 17710002
         GBLC  &CARRCON                                                 17825002
         GBLB  &TEST                                                    17940002
         LCLA  &S                                                       18055002
&NAME    DS    0C                                                       18170002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  18285002
         LINEND                                                         18400002
         AGO   .NTFIRST                                                 18515002
.FIRSTLN ANOP                                                           18630002
&NRENTSX SETC  'N&SYSNDX'                                               18745002
&INITIAL SETA  1                                                        18860002
N&SYSNDX EQU   *                                                        18975002
.NTFIRST ANOP                                                           19090002
         DC    AL1(0)                                                   19205002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               19320002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                19435002
&DB      SETA  &DATA(1)                                                 19550002
         AGO   .AA                                                      19665002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                19780002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 19895002
         AGO   .CC                                                      20010002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    20125002
         AIF   (&DATA(2) EQ &DT).BB                                     20240002
.CC      ANOP                                                           20355002
&DT      SETA  &DATA(2)                                                 20470002
.BB      DC    XL2'FFFE'                                                20585002
         DC    FL1'&DB'                                                 20700002
         DC    FL1'&DT'                                                 20815002
&NRENTS  SETA  &NRENTS+1                                                20930002
.NCDATA  ANOP                                                           21045002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              21160002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               21275002
&LB      SETA  &LABEL(1)                                                21390002
         AGO   .AAA                                                     21505002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              21620002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               21735002
         AGO   .CCC                                                     21850002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  21965002
         AIF   (&LABEL(2) EQ &LT).BBB                                   22080002
.CCC     ANOP                                                           22195002
&LT      SETA  &LABEL(2)                                                22310002
.BBB     DC    XL2'FFFF'                                                22425002
         DC    FL1'&LB'                                                 22540002
         DC    FL1'&LT'                                                 22655002
&NRENTS  SETA  &NRENTS+1                                                22770002
.NCLABEL ANOP                                                           22885002
         AIF   ('&SKIP' NE '').CONT                                     23000002
         AIF   (&TEST EQ 0).NOCHANG                                     23115002
&S       SETA  &LASTCC                                                  23230002
         AGO   .DEFAULT                                                 23345002
.CONT    ANOP                                                           23460002
         AIF   (&TEST EQ 0).AB                                          23575002
&S       SETA  &SKIP                                                    23690002
.DEFAULT ANOP                                                           23805002
         ORG   &CARRCON+3                                               23920002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 24035002
         ORG                                                            24150002
&LASTCC  SETA  &S                                                       24265002
&TEST    SETB  0                                                        24380002
         AGO   .DD                                                      24495002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               24610002
A&SYSNDX DC    XL2'FFFB'                                                24725002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              24840002
         DC    FL1'9'                                                   24955002
&LASTCC  SETA  &SKIP                                                    25070002
&TEST    SETB  1                                                        25185002
&CARRCON SETC  'A&SYSNDX'                                               25300002
&NRENTS  SETA  &NRENTS+1                                                25415002
.DD      ANOP                                                           25530002
.NOCHANG ANOP                                                           25645002
         MEND                                                           25760002
         EJECT                                                          25875002
         MACRO                                                          25990002
         BIN                                                            26105002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       26220002
.*                                      RECORD DSECT                    26335002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  26450002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      26565002
.*                                      TO BACK UP FROM COLUMN FOR      26680002
.*                                      START OF PRINTING. THE LETTER   26795002
.*                                      B PRECEEDS THE NUMBER (B5) IF   26910002
.*                                      THE LATTER IS DESIRED. OMISSION 27025002
.*                                      OF THIS OPERAND FORCES OPTION 2 27140002
.*                                      WITH BACKUP SPACING EQUAL TO    27255002
.*                                 THE NUMBER OF BITS IN THE ITEM       27370002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      27485002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     27600002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       27715002
         GBLC &ITEM(1000)                                               27830002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 27945002
         GBLC  &DSECT                                                   28060002
         LCLB  &B                                                       28175002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   28290002
         LCLA  &TEMP,&T,&OP                                             28405002
         LCLC  &S                                                       28520002
.LOOP    ANOP                                                           28635002
&OP      SETA  &OP+1                                                    28750002
.LOOP1   ANOP                                                           28865002
&NDX     SETA  &NDX+1                                                   28980002
         AIF   ('&ITEM(&NDX)' EQ '&SYSLIST(&OP,1)').FOUND               29095002
         AIF   (&NDX LT &ITEMNBR).LOOP1                                 29210002
         MNOTE *,'YOU BLEW IT'                                          29325002
         ROUTINE DUM,SKIPPER                                            29440002
         MEXIT                                                          29555002
.FOUND   ANOP                                                           29670002
&ISHIFT  SETA  &SHIFT(&NDX)                                             29785002
&IBITS   SETA  &BITS(&NDX)                                              29900002
&T       SETA  &IBITS-1                                                 30015002
&B       SETB  0                                                        30130002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         30245002
&K       SETA  K'&SYSLIST(&OP,2)                                        30360002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          30475002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            30590002
&T       SETA  &S                                                       30705002
.BACKUP  ANOP                                                           30820002
&NRENTS  SETA  &NRENTS+1                                                30935002
&TEMP    SETA  &ISHIFT                                                  31050002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            31165002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                31280002
         AGO   .OUT                                                     31395002
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           31510002
&B       SETB  1                                                        31625002
.BIN3    ANOP                                                           31740002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    31855002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             31970002
.OUT     ANOP                                                           32085002
&NDX     SETA  0                                                        32200002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 32315002
         MEND                                                           32430002
         EJECT                                                          32545002
