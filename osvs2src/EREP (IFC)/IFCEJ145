.SUBA    ANOP                                                  @YL02021 36910002
&K       SETA  K'&SYSLIST(&OP,3)                               @YL02021 36970002
.SUBB    ANOP                                                  @YL02021 37030002
         AIF   (&K EQ 1).ERR3                                  @YL02021 37090002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                   @YL02021 37150002
