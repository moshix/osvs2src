         LCLC  &S,&LETTER                                      @YL02021 35050002
         ACTR  500000                                          @YL02021 35110002
         AIF   (&DIAGTST EQ 0).NOMSG                           @YL02021 35170002
         AIF   (&INITIAL EQ 0).NOMSG                           @YL02021 35230002
         AIF   (&DIAGLIN EQ 1).NOMSG                           @YL02021 35290002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'         @YL02021 35350002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****            @YL02021 35410002
*        LINE                                                  @YL02021 35470002
         LINE                                                  @YL02021 35530002
.NOMSG   ANOP                                                  @YL02021 35590002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP         @YL02021 35650002
&SUB     SETB  1                                               @YL02021 35710002
.LOOP    ANOP                                                  @YL02021 35770002
&OP      SETA  &OP+1                                           @YL02021 35830002
.LOOP1   ANOP                                                  @YL02021 35890002
