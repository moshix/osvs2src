         MACRO                                                          32600002
&NAME    DSGEN ,                                                        32630002
.*                            THIS MACRO GENERATES ONE DSECT WORD       32660002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 32690002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  32720002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    32750002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 32780002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   32810002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 32840002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      32870002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  32900002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    32930002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   32960002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             32990002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  33020002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  33050002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              33080002
         GBLA  &NRENTS,&SHIFT(3000)                                     33110002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           33140002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       33170002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         33200002
         LCLB  &ONLY1,&CTIND                                            33230002
         LCLA  &START1,&START2,&END1,&END2                              33260002
         LCLC  &SYMBOL,&EQUSYM                                          33290002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 33320002
         ACTR  100000                                                   33350002
         AIF   (&ITEMITR EQ 0).START                                    33380002
 MNOTE   20,'DSGEN MACRO OCCURRS AFTER ITEMSORT MACRO HAS BEEN ISSUED.' 33410002
         MEXIT                                                          33440002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            33470002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     33500002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   33530002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        33560002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            33590002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             33620002
         AGO   .SET2A                   BRANCH AROUND                   33650002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        33680002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              33710002
         MEXIT                                                          33740002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X33770002
               ).SET2                   TEST FOR NO SECOND              33800002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   33830002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            33860002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              33890002
         AGO   .LOOP                                                    33920002
.SET2    ANOP                                                           33950002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 33980002
         AGO   .LOOP                                                    34010002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             34040002
         MEXIT                                                          34070002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               34100002
         MEXIT                                                          34130002
.LOOP    ANOP                                                           34160002
&OP      SETA  &OP+1                                                    34190002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           34220002
&ITEMNBR SETA  &ITEMNBR+1                                               34250002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            34280002
         MNOTE '**NULL BIT COUNT FIELD**'                               34310002
         MEXIT                                                          34340002
.OK1     ANOP                                                           34370002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        34400002
         AIF   ('&SYMBOL' NE '').OK                                     34430002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         34460002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         34490002
         AGO   .START                   GET NEXT                        34520002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        34550002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          34580002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        34610002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         34640002
         AGO   .NO3                     BRANCH AROUND                   34670002
.OK2     ANOP                                                           34700002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              34730002
.NO3     ANOP                                                           34760002
&SYMBOL  EQU   *+&BYTE                                                  34790002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             34820002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    34850002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    34880002
.CNT     ANOP                                                           34910002
&CTIND   SETB  1                        SET COUNT INDICATOR             34940002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   34970002
&BYTE    SETA  &COUNT/8                                                 35000002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        35030002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        35060002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             35090002
&END1    SETA  &END1+1                  INCREMENT COUNT                 35120002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             35150002
         AGO   .LOOP                    GENERATE NEXT                   35180002
.TSTST   ANOP                                                           35210002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               35240002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        35270002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      35300002
.NSET1   ANOP                                                           35330002
&OP      SETA  &OP-1                    DECREMENT INDEX                 35360002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           35390002
         AGO   .LOOP                    GO DO NEXT                      35420002
.LPDONE  ANOP                                                           35450002
&ONLY1   SETB  0                        CLEAR INDICATOR                 35480002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      35510002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 35540002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               35570002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             35600002
&NAME    DS    &COUNT.C                                                 35630002
.END     MEXIT                                                          35660002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          35690002
         MEXIT                                                          35720002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   35750002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   35780002
         MEXIT                                                          35810002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         35840002
*****               THE FOLLOWING STATEMENT                *****        35870002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        35900002
.EQU2    ANOP                                                           35930002
&EQUCNT  SETA  0                        CLEAR COUNTER                   35960002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 35990002
.EQULOP  ANOP                                                           36020002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 36050002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     36080002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                36110002
         AGO   .EQULOP                  NO-GET NEXT                     36140002
.EQUOUT  ANOP                                                           36170002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  36200002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              36230002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 36260002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             36290002
         AGO   .START                   RETURN FOR NEXT                 36320002
         MEND                                                           36350002
         MACRO                                                          36380002
         HEX   ,                                                        36410002
.*                            THIS MACRO FACILITATES GENERATION OF THE  36440002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    36470002
.*                                 CONVERSIONS.                         36500002
.*                            &SYSLIST(&OP,1) - NAME OF DATA ITEM TO BE 36530002
.*                                 CONVERTED                            36560002
.*                            &SYSLIST(&OP,2) - # OF SPACES TO SKIP ON  36590002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   36620002
.*                                 BACK UP FROM COLUMN FOR START OF     36650002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  36680002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  36710002
.*                                 DESIRED. OMISSION OF THIS OPERAND    36740002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  36770002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  36800002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 36830002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    36860002
.*                                 IMPLICITLY B).                       36890002
.*                            &SYSLIST(&OP,3) - NUMBER OF BYTES TO      36920002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     36950002
.*                                 WITH AN L,R, OR B TO STIPULATE       36980002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     37010002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     37040002
.*                                 RESPECTIVELY. OMISSION OF THIS       37070002
