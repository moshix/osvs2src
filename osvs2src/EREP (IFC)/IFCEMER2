         DC    CL8'IFCEMER2'                                            52400002
XR9PTR   DC    F'0'                                                     52500002
XLABELP  DC    F'0'                                                     52600002
*                                                                       52700002
*        SET HEX -PRINTS 1 CHAR HEX                                     52800002
*                                                                       52900002
SETHEX   STM   R5,R8,XSAVE             SAVE REGS                        53000002
         LA    R8,CHARS                CHARACTER TABLE POINTER          53100002
         IC    R7,L0(R4)               PICK UP CHARACTER                53200002
         SLL   R7,L24                  MOVE TO HI R7                    53300002
         SR    R6,R6                   CLEAR REGS                       53400002
         SLDL  R6,L4                   MOVE FOUR BITS                   53500002
         AR    R6,R8                   ADD OFFSET AND POINTER           53600002
         MVC   L0(L1,R3),L0(R6)        MOVE CHARACTER TO LINE           53700002
         SR    R6,R6                   CLEAR AGAIN                      53800002
         SLDL  R6,L4                   MOVE LAST 4 BITS                 53900002
         AR    R6,R8                   COMPUTE OFFSET                   54000002
         MVC   L1(L1,R3),L0(R6)        MOVE LAST CHARACTER              54100002
         B     RETURN                                                   54200002
*                                                                       54300002
*        TEST TO SEE IF RECORD IS IN AR-149 FORMAT                      54400002
*                                                                       54500002
SETCONV  TM    L0(R4),HEX02            HAS RECORD BEEN CONVERTED KEY32  54600002
         BCR   ON,R14                  YES RETURN                       54700002
         SR    R15,R15                 NO CLEAR R15 FOR NO PRINT OF     54800002
         BR    R14                     LINE                             54900002
ON       EQU   1                                                        55000002
HEX02    EQU   X'02'                                                    55100002
*                                                                       55200002
*        NO OP RETURN                                                   55300002
*                                                                       55400002
SKIPPER  BR    R14                                                      55500002
*                                                                       55600002
*        TO ADD SPACES ON LINE                                          55700002
*                                                                       55800002
SPACER2  LA    R3,L4(R3)                                                55900002
         BR    14                                                       56000002
*                                                                       56100002
*        RESTORE REGISTERS USED BY ROUTINES IN THIS MODULE              56200002
*                                                                       56300002
RETURN   LM    R5,R8,XSAVE             RESTORE REGS                     56400002
         BR    R14                     RETURN                           56500002
*                                                                       56600002
*        SET THE DATE READY FOR PRINT                                   56700002
*                                                                       56800002
SETDATE  UNPK  XSAVE(L8),L1(L3,R4)     UNPACK THE DATE                  56900002
         MVC   L0(L3,R3),XSAVE+L5      MOVE DAY                         57000002
         MVC   L6(L2,R3),XSAVE+L3      MOVE YEAR                        57100002
         BR    R14                                                      57200002
*                                                                       57300002
*        SET THE DEVICE TYPE READY FOR PRINT                            57400002
*                                                                       57500002
SETDEVT  STM   R5,R8,XSAVE             SAVE REGS                        57600002
         LA    R5,L1                    LOAD NO. OF POSSIBLE DEVICE TPS 57700002
         LA    R6,DEVTABLE             POIT TO DEVICE TYPES             57800002
SETDEVLP CLC   L0(L2,R4),L0(R6)        COMPARE TYPES                    57900002
         BE    SDEVFND                 FOUND ONE SO BRANCH              58000002
         B     RETURN                                                   58100002
SDEVFND  MVC   L0(L4,R3),L2(R6)        MOVE DEV NAME TO PRINT LINE      58200002
         B     RETURN                                                   58300002
         LA    R6,L6(R6)               MOVE POINTER TO LOOK AT NEXT T   58400002
         BCT   R5,SETDEVLP             BRANCH IF THERE ARE MORE         58500002
         MVC   L0(L4,R3),NONE          NO DEV TYPE MOVE IN NONE         58600002
*                                                                       58700002
*        CHECK FOR CPU AND SERIAL NUMBER                                58800002
*                                                                       58900002
SETCPUNO STM   R5,R8,XSAVE             SAVE REGS                        59000002
         SR    R5,R5                   CLEAR R5                         59100002
         IC    R5,L0(R4)               PICK UP MODEL NO                 59200002
         SLL   R5,L8                   IN TO                            59300002
         IC    R5,L1(R4)               R5                               59400002
         LTR   R5,R5                   TEST FOR ZERO                    59500002
         BNZ   RETURN                  NONE ZERO BR                     59600002
         LR    R5,R3                   PUT LINE ADDRESS IN R5           59700002
         MVC   L0(L9,R5),UNIVERS       PUT IN UNIVERSAL                 59800002
         MVC   L11(L6,R5),NONE         PUT IN NONE                      59900002
         B     RETURN                                                   60000002
*                                                                       60100002
*        SET PROGRAM NAME TO PRINT LINE                                 60200002
*                                                                       60300002
SETPGMN  MVC   L0(L8,R3),L0(R4)        MOVE 8 CHARS TO LINE             60400002
         BR    R14                     RETURN                           60500002
         SPACE 2                                               @Y30LPDS 60505002
*                                                              @Y30LPDS 60510002
*      THE FOLLOWING CHECKS THE RECORD FOR EXTRA SENSE(28 BYTES).       60515002
*        IF THE RECORD DOES CONTAIN SS1(3850) SENSE,THIS ROUTINE        60520002
*        WILL SET PTRS TO PASS CONTROL TO IFCEMER0.            @Y30LPDS 60525002
*                                                              @Y30LPDS 60530002
CKIFSS1  DS    0H                                              @Y30LPDS 60535002
         CLI   SBCNT+1,SS1CNT          WERE EXTRA SNS BYTS REC'D        60540002
         BE    PTMER0                  YES - BR                @Y30LPDS 60545002
         SR    R15,R15                 NO - DONT PRINT LINE    @Y30LPDS 60550002
         BR    R14                     RETURN                  @Y30LPDS 60555002
         SPACE 1                                               @Y30LPDS 60560002
PTMER0   DS    0H                                              @Y30LPDS 60565002
         STM   R5,R8,XSAVE             SAVE REGS               @Y30LPDS 60570002
         LA    R5,FEMER0-L4            LOAD PARM & MOD NAME ADDR        60575002
         ST    R5,XR9PTR               STORE FOR EREP0         @Y30LPDS 60580002
         SR    R15,R15                 DONT PRINT THIS LINE    @Y30LPDS 60585002
         MVI   CLASRC,HFF              SET INDICATOR FOR MER0  @Y30LPDS 60590002
         B     RETURN                  BR TO RETURN            @Y30LPDS 60595002
*                                                                       60600002
SETBIN1  STM   R5,R8,XSAVE             SAVE REGS                        60700002
         IC    R7,L0(R4)                                                60800002
         SLL   R7,L24                                                   60900002
         LR    R8,R3                                                    61000002
         SR    R6,R6                                                    61100002
         SLDL  R6,L1                                                    61200002
         STC   R6,L11(R8)                                               61300002
         OC    L11(L1,R8),CHAR0                                         61400002
         B     RETURN                                                   61500002
*        SET BINARY WITH R4 POINTING TO THE CHAR                        61600002
*                                                                       61700002
SETBIN   STM   R5,R8,XSAVE             SAVE REGS                        61800002
         IC    R7,L0(R4)               PICK UP CHAR TO PRINT            61900002
         SLL   R7,L24                  MOVE IT TO HI OF R7              62000002
         LR    R8,R3                   PRINT POS POINTER TO REG8        62100002
         LA    R5,L8                   LOOP COUNT                       62200002
SETBINA  SR    R6,R6                   CLEAR REG 6                      62300002
         SLDL  R6,L1                   MOVE BIT TO R6                   62400002
         STC   R6,L0(R8)               PUT IT INTO PRINT LINE           62500002
         MVZ   L0(L1,R8),CHAR0                                          62600002
         LA    R8,L1(R8)               MOVE POINTER TO NEXT POS         62700002
         BCT   R5,SETBINA              LOOP TILL 8 PRINTED              62800002
         B     RETURN                                                   62900002
*        CONVERT COUNTERS FROM HEX TO DECIMAL READY FOR PRINT           63000002
*                                                                       63100002
CONV1    STM   R5,R8,XSAVE             SAVE REGISTERS                   63200002
         XC    READYP(L8),READYP       CLEAR UNPACK BUFFER              63300002
         SR    R6,R6                   ZERO REG                         63400002
         LH    R6,L0(R4)               LOAD DATA TO BE CONVERTED        63500002
         CVD   R6,READYP               CONVERT ERROR COUNTER TO DECI    63600002
         UNPK  READYP(L5),READYP+L5(L3)  UNPACK DECI COUNT              63700002
         OI    READYP+L4,CLRSGN        CLEAR SIGN                       63800002
         MVC   L0(L4,R3),READYP+L1     MOVE COUNTER READY TO PRINT      63900002
         B     RETURN                                                   64000002
         DS    0F                                                       64100002
