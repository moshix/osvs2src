         AIF   (&LABEL(2) EQ &LT).BBB                                   14760002
.CCC     ANOP                                                           14880002
&LT      SETA  &LABEL(2)                                                15000002
.BBB     DC    XL2'FFFF'                                                15120002
         DC    FL1'&LB'                                                 15240002
         DC    FL1'&LT'                                                 15360002
&NRENTS  SETA  &NRENTS+1                                                15480002
.NCLABEL ANOP                                                           15600002
         AIF   ('&SKIP' NE '').CONT                                     15720002
         AIF   (&TEST EQ 0).NOCHANG                                     15840002
&S       SETA  &LASTCC                                                  15960002
         AGO   .DEFAULT                                                 16080002
.CONT    ANOP                                                           16200002
         AIF   (&TEST EQ 0).AB                                          16320002
&S       SETA  &SKIP                                                    16440002
.DEFAULT ANOP                                                           16560002
         ORG   &CARRCON+3                                               16680002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 16800002
         ORG                                                            16920002
&LASTCC  SETA  &S                                                       17040002
&TEST    SETB  0                                                        17160002
         AGO   .DD                                                      17280002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               17400002
A&SYSNDX DC    XL2'FFFB'                                                17520002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              17640002
         DC    FL1'9'                                                   17760002
&LASTCC  SETA  &SKIP                                                    17880002
&TEST    SETB  1                                                        18000002
&CARRCON SETC  'A&SYSNDX'                                               18120002
