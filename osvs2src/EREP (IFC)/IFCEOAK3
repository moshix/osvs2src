*** THIS MODULE SUPPORTS 3850 DEVICE --- SUPPORT CODE=Y30LPDG ***       00050002
         MACRO                                                          00100002
         BIN   ,                                                        00150002
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   00200002
.*                                      RECORD DSECT                    00250002
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      00300002
.*                                      TO SKIP ON MULTIPLE             00350002
.*                                      PRINT OR NBR OF SPACES          00400002
.*                                      TO BACK UP FROM COLUMN FOR      00450002
.*                                      START OF PRINTING. THE LETTER   00500002
.*                                      B PRECEEDS THE NUMBER (B5) IF   00550002
.*                                      THE LATTER IS DESIRED. OMISSION 00600002
.*                                      OF THIS OPERAND FORCES OPTION 2 00650002
.*                                      WITH BACKUP SPACING EQUAL TO    00700002
.*                                 THE NUMBER OF BITS IN THE ITEM       00750002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      00800002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     00850002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       00900002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             00950002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  01000002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  01050002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              01100002
         GBLA  &NRENTS,&SHIFT(3000)                                     01150002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           01200002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       01250002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         01300002
         LCLB  &B,&SUB                                                  01350002
         LCLA  &IBITS,&ISHIFT,&K                                        01400002
         LCLA  &TEMP,&T,&OP                                             01450002
         LCLC  &S,&ARG                                                  01500002
          ACTR  500000                                                  01550002
         AIF   (&DIAGTST EQ 0).NOMSG                                    01600002
         AIF   (&INITIAL EQ 0).NOMSG                                    01650002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    01700002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  01750002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     01800002
*        LINE                                                           01850002
         LINE                                                           01900002
.NOMSG   ANOP                                                           01950002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  02000002
&SUB     SETB  1                                                        02050002
.LOOP    ANOP                                                           02100002
&OP      SETA  &OP+1                                                    02150002
&ARG     SETC  '&SYSLIST(&OP,1)'                                        02200002
         ITEMFIND &ARG                                                  02250002
         AIF   (&ITEMXPT EQ 0).MEXIT                                    02300002
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         02350002
&IBITS   SETA  &BITS(&ITEMXPT)                                          02400002
         AIF   (&SUB EQ 1).SUB00                                        02450002
         AIF   ('&SYSLIST(3)' EQ '').SETT                               02500002
&IBITS   SETA  &SYSLIST(3)                                              02550002
         AGO   .SETT                                                    02600002
.SUB00   AIF   ('&SYSLIST(&OP,3)' EQ '').SETT                           02650002
&IBITS   SETA  &SYSLIST(&OP,3)                                          02700002
.SETT    ANOP                                                           02750002
&T       SETA  &IBITS-1                                                 02800002
&B       SETB  0                                                        02850002
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      02900002
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  02950002
.MEXIT   MEXIT                                                          03000002
.SUB0    AIF   (&SUB EQ 1).SUBA                                         03050002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             03100002
&K       SETA  K'&SYSLIST(2)                                            03150002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              03200002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                03250002
         AGO   .SUBB                                                    03300002
.SUBA    ANOP                                                           03350002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         03400002
&K       SETA  K'&SYSLIST(&OP,2)                                        03450002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          03500002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            03550002
.SUBB    ANOP                                                           03600002
&T       SETA  &S                                                       03650002
.BACKUP  ANOP                                                           03700002
&NRENTS  SETA  &NRENTS+1                                                03750002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            03800002
&TEMP    SETA  &ISHIFT                                                  03850002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                03900002
         AGO   .OUT                                                     03950002
         AIF   (&SUB EQ 1).BIN2                                         04000002
         AIF   ('&SYSLIST(2)' NE '').BIN3                               04050002
         AGO   .BIN2A                                                   04100002
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           04150002
.BIN2A   ANOP                                                           04200002
&B       SETB  1                                                        04250002
.BIN3    ANOP                                                           04300002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    04350002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             04400002
.OUT     ANOP                                                           04450002
&ITEMXPT     SETA  0                                                    04500002
         AIF   (&SUB EQ 0).OUTA                                         04550002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 04600002
.OUTA    ANOP                                                           04650002
         MEND                                                           04700002
         MACRO                                                          04750002
&NAME    DSGEN ,                                                        04800002
.*                            THIS MACRO GENERATES ONE DSECT WORD       04850002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 04900002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  04950002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    05000002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 05050002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   05100002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 05150002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      05200002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  05250002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    05300002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   05350002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             05400002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  05450002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  05500002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              05550002
         GBLA  &NRENTS,&SHIFT(3000)                                     05600002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           05650002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       05700002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         05750002
         LCLB  &ONLY1,&CTIND                                            05800002
         LCLA  &START1,&START2,&END1,&END2                              05850002
         LCLC  &SYMBOL,&EQUSYM                                          05900002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 05950002
         ACTR  100000                                                   06000002
         AIF   (&ITEMITR EQ 0).START                                    06050002
 MNOTE   20,'DSGEN MACRO OCCURRS AFTER ITEMSORT MACRO HAS BEEN ISSUED.' 06100002
         MEXIT                                                          06150002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            06200002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     06250002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   06300002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        06350002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            06400002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             06450002
         AGO   .SET2A                   BRANCH AROUND                   06500002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        06550002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              06600002
         MEXIT                                                          06650002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X06700002
               ).SET2                   TEST FOR NO SECOND              06750002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   06800002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            06850002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              06900002
         AGO   .LOOP                                                    06950002
.SET2    ANOP                                                           07000002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 07050002
         AGO   .LOOP                                                    07100002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             07150002
         MEXIT                                                          07200002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               07250002
         MEXIT                                                          07300002
.LOOP    ANOP                                                           07350002
&OP      SETA  &OP+1                                                    07400002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           07450002
&ITEMNBR SETA  &ITEMNBR+1                                               07500002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            07550002
         MNOTE '**NULL BIT COUNT FIELD**'                               07600002
         MEXIT                                                          07650002
.OK1     ANOP                                                           07700002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        07750002
         AIF   ('&SYMBOL' NE '').OK                                     07800002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         07850002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         07900002
         AGO   .START                   GET NEXT                        07950002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        08000002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          08050002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        08100002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         08150002
         AGO   .NO3                     BRANCH AROUND                   08200002
.OK2     ANOP                                                           08250002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              08300002
.NO3     ANOP                                                           08350002
&SYMBOL  EQU   *+&BYTE                                                  08400002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             08450002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    08500002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    08550002
.CNT     ANOP                                                           08600002
&CTIND   SETB  1                        SET COUNT INDICATOR             08650002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   08700002
&BYTE    SETA  &COUNT/8                                                 08750002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        08800002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        08850002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             08900002
&END1    SETA  &END1+1                  INCREMENT COUNT                 08950002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             09000002
         AGO   .LOOP                    GENERATE NEXT                   09050002
.TSTST   ANOP                                                           09100002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               09150002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        09200002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      09250002
.NSET1   ANOP                                                           09300002
&OP      SETA  &OP-1                    DECREMENT INDEX                 09350002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           09400002
         AGO   .LOOP                    GO DO NEXT                      09450002
.LPDONE  ANOP                                                           09500002
&ONLY1   SETB  0                        CLEAR INDICATOR                 09550002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      09600002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 09650002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               09700002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             09750002
&NAME    DS    &COUNT.C                                                 09800002
.END     MEXIT                                                          09850002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          09900002
         MEXIT                                                          09950002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   10000002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   10050002
         MEXIT                                                          10100002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         10150002
*****               THE FOLLOWING STATEMENT                *****        10200002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        10250002
.EQU2    ANOP                                                           10300002
&EQUCNT  SETA  0                        CLEAR COUNTER                   10350002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 10400002
.EQULOP  ANOP                                                           10450002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 10500002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     10550002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                10600002
         AGO   .EQULOP                  NO-GET NEXT                     10650002
.EQUOUT  ANOP                                                           10700002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  10750002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              10800002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 10850002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             10900002
         AGO   .START                   RETURN FOR NEXT                 10950002
         MEND                                                           11000002
         MACRO                                                          11050002
         CONVT ,                                                        11100002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             11150002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  11200002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  11250002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              11300002
         GBLA  &NRENTS,&SHIFT(3000)                                     11350002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           11400002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       11450002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         11500002
         LCLB  &SUB                                                     11550002
         LCLA  &OP                                                      11600002
         LCLA  &OFFSET,&OUT,&IN,&IND,&IBITS,&ISHIFT                     11650002
.*                                                                      11700002
.*       CONVERT BINARY TO DECIMAL                                      11750002
.*             &NAME IS THE REFERENCE INTO THE DSECT RECORD             11800002
.*             &OUT    IS THE NUMBER OF CHARS TO BE PUT ONTO PRINT LINE 11850002
.*                                      DEFAULT IS FOUR MAXIMUM IS 8    11900002
.*             &IN       IS THE NUMBER OF BYTES AS INPUT                11950002
.*                                      ERROR DEFAULT IS 2 MAXIMUM IS 4 12000002
.*                                                                      12050002
         AIF   (&DIAGTST EQ 0).NOMSG                                    12100002
         AIF   (&INITIAL EQ 0).NOMSG                                    12150002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    12200002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  12250002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     12300002
*        LINE                                                           12350002
         LINE                                                           12400002
.NOMSG   ANOP                                                           12450002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LLOP                  12500002
&SUB     SETB  1                                                        12550002
.LLOP    ANOP                                                           12600002
&OP      SETA  &OP+1                                                    12650002
&OUT     SETA  4                        .BR NOT NULL ELSE SET TO 4      12700002
         AIF   (&SUB EQ 1).SUBA                                         12750002
         AIF   ('&SYSLIST(3)' EQ '').B  TEST FOR NULL INPUT             12800002
&OUT     SETA  &SYSLIST(3)             SET &OUT FROM INPUT              12850002
         AGO   .A                                                       12900002
.SUBA    AIF   ('&SYSLIST(&OP,3)' EQ '').B                              12950002
&OUT     SETA  &SYSLIST(&OP,3)          SET &OUT FROM INPUT             13000002
.A       AIF   (&OUT GT 0 AND &OUT LT 9).B   TEST RANGE                 13050002
.AERR    MNOTE '*** BYTES OUT INCORRECTLY SPECIFIED DEFAULT USED ***'   13100002
&OUT     SETA  4                                                        13150002
.B       AIF   (&SUB EQ 1).SUBB                                         13200002
         AIF   ('&SYSLIST(2)' EQ '').LOOP    BRANCH IF NULL             13250002
&IN      SETA  &SYSLIST(2)              .SET &IN FROM INPUT             13300002
         AGO   .BB                                                      13350002
.SUBB    AIF   ('&SYSLIST(&OP,2)' EQ '').LOOP                           13400002
&IN      SETA  &SYSLIST(&OP,2)          .SET &IN FROM INPUT             13450002
.BB      AIF   (&IN GT 0 AND &IN LT 5).D     TEST RANGE                 13500002
.BERR    MNOTE '*** BYTES IN INCORRECTLY SPECIFIED DEFAULT USED ***'    13550002
&IN      SETA  2                                                        13600002
         AGO   .D                                                       13650002
.LOOP    ANOP                           .LOOP TO FIND &NAME IN DSECT    13700002
&IND     SETA  &IND+1                   .SET UP LOOP COUNTER            13750002
         AIF   ('&ITEM(&IND)' EQ '&SYSLIST(&OP,1)').FOUND               13800002
         AIF   (&IND LT &ITEMNBR).LOOP  .TEST FOR END OF DSECT          13850002
         MNOTE '*** LOG ITEM NOT DEFINED ***'                           13900002
         MEXIT                                                          13950002
.FOUND   ANOP                                                           14000002
&IBITS   SETA  &BITS(&IND)              .SET UP NUMBER OF BITS SPEC     14050002
         AIF   (&IBITS-((&IBITS/8)*8) EQ 0).C  .TEST FOR MULTIPLE OF 8  14100002
         MNOTE '*** DATA NOT DEFINED A MULTIPLE OF 8 BITS ***'          14150002
         MEXIT                                                          14200002
.C       ANOP                                                           14250002
&IN      SETA  &IBITS/8                 .COMPUTE NUMBER OF BYTES        14300002
         AIF   (&IN LT 1 OR &IN GT 4).BERR  .TEST RANGE B IF IN ERROR   14350002
&ISHIFT  SETA  &SHIFT(&IND)             .SET UP FOR TEST OF BYTE BNDR   14400002
         AIF   (&ISHIFT EQ 0).D         .TEST FOR BYTE BNDRY            14450002
         MNOTE '*** DATA NOT ALIGNED ON A BYTE BOUNDARY ***'            14500002
         MEXIT                                                          14550002
.*                                                                      14600002
.D       ANOP                                                           14650002
&NRENTS  SETA  &NRENTS+1                                                14700002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),AL.4(9)                    14750002
         DC    X'&IN',X'&OUT'                                           14800002
&IND     SETA  0                                                        14850002
         AIF   (&SUB EQ 0).OUTA                                         14900002
         AIF   (&OP LT N'&SYSLIST).LLOP                                 14950002
.OUTA    ANOP                                                           15000002
         MEND                                                           15050002
         MACRO                                                          15100002
         ITEMFIND &ARG,&ITFDIAG=                                        15150002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             15200002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  15250002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  15300002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              15350002
         GBLA  &NRENTS,&SHIFT(3000)                                     15400002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           15450002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       15500002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         15550002
         LCLA  &A,&B,&X,&I                                              15600002
         LCLC  &C1,&C2,&S                                               15650002
         ACTR  100000                                                   15700002
&ITEMXPT SETA  0                  INDICATE ITEM NOT FOUND.              15750002
         AIF   ('&ARG' NE '').NORM                                      15800002
         AIF   ('&ITFDIAG' EQ 'OFF'  OR  '&ITFDIAG' EQ 'ON').DIAGSET    15850002
  MNOTE 0,'ITEMFIND - OPERAND (&ITFDIAG) INVALID FOR ITFDIAG KEYWORD.'  15900002
         MEXIT                                                          15950002
.DIAGSET ANOP                                                           16000002
&DIAGITF SETB  ('&ITFDIAG' EQ 'ON')                                     16050002
         MEXIT                                                          16100002
.NORM    AIF   (&ITEMITR GT 0).SORTED                                   16150002
         ITEMSORT                                                       16200002
.SORTED  ANOP                                                           16250002
&I       SETA  &ITEMITR           COMPUTED ITTERATION COUNT.            16300002
&X       SETA  &ITEMMDX           COMPUTED MAXIMUM INDEX.               16350002
&C1      SETC  '&ARG'             ISOLATE SEARCH ARGUMENT.              16400002
&C1      SETC  '&C1'.'       '(1,8-K'&C1) PAD TO FULL 8 CHARACTERS.     16450002
.SRCHUP  ANOP                                                           16500002
&A       SETA  &A+&X              CURRENT TABLE SUBSCRIPT + INDEX.      16550002
&S       SETC  '+'                                                      16600002
         AIF   (&A  GT &ITEMNBR  AND  NOT &DIAGITF).SRCHDN1             16650002
         AIF   (&A  LE &ITEMNBR).SRCH00                                 16700002
&C2      SETC  '*-OVER-*'                                               16750002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         16800002
&X       SETA  &X/2               HALVE INDEX VALUE.                    16850002
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X'                     16900002
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      16950002
.NOFIND  MNOTE  20,' ITEMFIND FAILED FOR NAME  &C1  '                   17000002
         MEXIT                                                          17050002
.SRCH00  ANOP                                                           17100002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         17150002
         AIF   (&I LT 0).NOFIND   IF COUNT HAS GONE TO ZERO.            17200002
&X       SETA  &X/2               HALVE INDEX VALUE.                    17250002
&B       SETA  &ITEMNDX(&A)       GET SUBSCRIPT FROM INDEX TABLE.       17300002
&C2      SETC  '&ITEM(&B)'.'       '(1,8-K'&ITEM(&B))  PAD COMPARAND.   17350002
         AIF   (NOT &DIAGITF).COMPARE                                   17400002
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X   POS = &B'          17450002
.COMPARE AIF   ('&C1' LT '&C2').SRCHDWN   ARGUMENT LOWER, MOVE DOWN.    17500002
         AIF   ('&C1' GT '&C2').SRCHUP    ARGUMENT HIGHER, MOVE UP.     17550002
&ITEMXPT SETA  &B                 FOUND. SET INDEX POINTER FOR RETURN.  17600002
&ITEMFST(&I+1)  SETA  &ITEMFST(&I+1)+1                                  17650002
         MEXIT                                                          17700002
.SRCHDWN ANOP                                                           17750002
&A       SETA  &A-&X              CURRENT POINTER MINUS INDEX VALUE.    17800002
&S       SETC  '-'                                                      17850002
         AIF   (&A LE &ITEMNBR).SRCH00    IF WITHIN BOUNDS OF TABLE.    17900002
.SRCHDN1 ANOP                                                           17950002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         18000002
&X       SETA  &X/2               HALVE INDEX VALUE.                    18050002
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      18100002
         MNOTE  20,' ITEMFIND FAILED FOR NAME  &C1  '                   18150002
         MEND                                                           18200002
         MACRO                                                          18250002
         ITEMSORT  &DUMMY                                               18300002
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             18350002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  18400002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  18450002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              18500002
         GBLA  &NRENTS,&SHIFT(3000)                                     18550002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           18600002
