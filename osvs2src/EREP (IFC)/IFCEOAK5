*** THIS MODULE SUPPORTS 3850 DEVICE --- SUPPORT CODE=Y30LPDG ***       00020002
         MACRO                                                          00040002
         BIN   ,                                                        00060002
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   00080002
.*                                      RECORD DSECT                    00100002
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      00120002
.*                                      TO SKIP ON MULTIPLE             00140002
.*                                      PRINT OR NBR OF SPACES          00160002
.*                                      TO BACK UP FROM COLUMN FOR      00180002
.*                                      START OF PRINTING. THE LETTER   00200002
.*                                      B PRECEEDS THE NUMBER (B5) IF   00220002
.*                                      THE LATTER IS DESIRED. OMISSION 00240002
.*                                      OF THIS OPERAND FORCES OPTION 2 00260002
.*                                      WITH BACKUP SPACING EQUAL TO    00280002
.*                                 THE NUMBER OF BITS IN THE ITEM       00300002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      00320002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     00340002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       00360002
         GBLA  &BITS(4000),&DB,&DT,&INITIAL                             00380002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  00400002
         GBLA  &ITEMNDX(4000),&ITEMXPT                                  00420002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              00440002
         GBLA  &NRENTS,&SHIFT(4000)                                     00460002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           00480002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(4000)                       00500002
         GBLC  &LABELS(4000),&LBL(100),&NRENTSX                         00520002
         LCLB  &B,&SUB                                                  00540002
         LCLA  &IBITS,&ISHIFT,&K                                        00560002
         LCLA  &TEMP,&T,&OP                                             00580002
         LCLC  &S,&ARG                                                  00600002
          ACTR  500000                                                  00620002
         AIF   (&DIAGTST EQ 0).NOMSG                                    00640002
         AIF   (&INITIAL EQ 0).NOMSG                                    00660002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    00680002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  00700002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     00720002
*        LINE                                                           00740002
         LINE                                                           00760002
.NOMSG   ANOP                                                           00780002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  00800002
&SUB     SETB  1                                                        00820002
.LOOP    ANOP                                                           00840002
&OP      SETA  &OP+1                                                    00860002
&ARG     SETC  '&SYSLIST(&OP,1)'                                        00880002
         ITEMFIND &ARG                                                  00900002
         AIF   (&ITEMXPT EQ 0).MEXIT                                    00920002
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         00940002
&IBITS   SETA  &BITS(&ITEMXPT)                                          00960002
         AIF   (&SUB EQ 1).SUB00                                        00980002
         AIF   ('&SYSLIST(3)' EQ '').SETT                               01000002
&IBITS   SETA  &SYSLIST(3)                                              01020002
         AGO   .SETT                                                    01040002
.SUB00   AIF   ('&SYSLIST(&OP,3)' EQ '').SETT                           01060002
&IBITS   SETA  &SYSLIST(&OP,3)                                          01080002
.SETT    ANOP                                                           01100002
&T       SETA  &IBITS-1                                                 01120002
&B       SETB  0                                                        01140002
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      01160002
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  01180002
.MEXIT   MEXIT                                                          01200002
.SUB0    AIF   (&SUB EQ 1).SUBA                                         01220002
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             01240002
&K       SETA  K'&SYSLIST(2)                                            01260002
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              01280002
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                01300002
         AGO   .SUBB                                                    01320002
.SUBA    ANOP                                                           01340002
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         01360002
&K       SETA  K'&SYSLIST(&OP,2)                                        01380002
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          01400002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            01420002
.SUBB    ANOP                                                           01440002
&T       SETA  &S                                                       01460002
.BACKUP  ANOP                                                           01480002
&NRENTS  SETA  &NRENTS+1                                                01500002
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            01520002
&TEMP    SETA  &ISHIFT                                                  01540002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                01560002
         AGO   .OUT                                                     01580002
         AIF   (&SUB EQ 1).BIN2                                         01600002
         AIF   ('&SYSLIST(2)' NE '').BIN3                               01620002
         AGO   .BIN2A                                                   01640002
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           01660002
.BIN2A   ANOP                                                           01680002
&B       SETB  1                                                        01700002
.BIN3    ANOP                                                           01720002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    01740002
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             01760002
.OUT     ANOP                                                           01780002
&ITEMXPT     SETA  0                                                    01800002
         AIF   (&SUB EQ 0).OUTA                                         01820002
         AIF   (&OP LT N'&SYSLIST).LOOP                                 01840002
.OUTA    ANOP                                                           01860002
         MEND                                                           01880002
         MACRO                                                          01900002
&NAME    DSGEN ,                                                        01920002
.*                            THIS MACRO GENERATES ONE DSECT WORD       01940002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 01960002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  01980002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    02000002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 02020002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   02040002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 02060002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      02080002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  02100002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    02120002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   02140002
         GBLA  &BITS(4000),&DB,&DT,&INITIAL                             02160002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  02180002
         GBLA  &ITEMNDX(4000),&ITEMXPT                                  02200002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              02220002
         GBLA  &NRENTS,&SHIFT(4000)                                     02240002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           02260002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(4000)                       02280002
         GBLC  &LABELS(4000),&LBL(100),&NRENTSX                         02300002
         LCLB  &ONLY1,&CTIND                                            02320002
         LCLA  &START1,&START2,&END1,&END2                              02340002
         LCLC  &SYMBOL,&EQUSYM                                          02360002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 02380002
         ACTR  100000                                                   02400002
         AIF   (&ITEMITR EQ 0).START                                    02420002
 MNOTE   20,'DSGEN MACRO OCCURRS AFTER ITEMSORT MACRO HAS BEEN ISSUED.' 02440002
         MEXIT                                                          02460002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            02480002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     02500002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   02520002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        02540002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            02560002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             02580002
         AGO   .SET2A                   BRANCH AROUND                   02600002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        02620002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              02640002
         MEXIT                                                          02660002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X02680002
               ).SET2                   TEST FOR NO SECOND              02700002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   02720002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            02740002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              02760002
         AGO   .LOOP                                                    02780002
.SET2    ANOP                                                           02800002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 02820002
         AGO   .LOOP                                                    02840002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             02860002
         MEXIT                                                          02880002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               02900002
         MEXIT                                                          02920002
.LOOP    ANOP                                                           02940002
&OP      SETA  &OP+1                                                    02960002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           02980002
&ITEMNBR SETA  &ITEMNBR+1                                               03000002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            03020002
         MNOTE '**NULL BIT COUNT FIELD**'                               03040002
         MEXIT                                                          03060002
.OK1     ANOP                                                           03080002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        03100002
         AIF   ('&SYMBOL' NE '').OK                                     03120002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         03140002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         03160002
         AGO   .START                   GET NEXT                        03180002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        03200002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          03220002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        03240002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         03260002
         AGO   .NO3                     BRANCH AROUND                   03280002
.OK2     ANOP                                                           03300002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              03320002
.NO3     ANOP                                                           03340002
&SYMBOL  EQU   *+&BYTE                                                  03360002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             03380002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    03400002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    03420002
.CNT     ANOP                                                           03440002
&CTIND   SETB  1                        SET COUNT INDICATOR             03460002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   03480002
&BYTE    SETA  &COUNT/8                                                 03500002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        03520002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        03540002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             03560002
&END1    SETA  &END1+1                  INCREMENT COUNT                 03580002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             03600002
         AGO   .LOOP                    GENERATE NEXT                   03620002
.TSTST   ANOP                                                           03640002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               03660002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        03680002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      03700002
.NSET1   ANOP                                                           03720002
&OP      SETA  &OP-1                    DECREMENT INDEX                 03740002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           03760002
         AGO   .LOOP                    GO DO NEXT                      03780002
.LPDONE  ANOP                                                           03800002
&ONLY1   SETB  0                        CLEAR INDICATOR                 03820002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      03840002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 03860002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               03880002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             03900002
&NAME    DS    &COUNT.C                                                 03920002
.END     MEXIT                                                          03940002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          03960002
         MEXIT                                                          03980002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   04000002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   04020002
         MEXIT                                                          04040002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         04060002
*****               THE FOLLOWING STATEMENT                *****        04080002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        04100002
.EQU2    ANOP                                                           04120002
&EQUCNT  SETA  0                        CLEAR COUNTER                   04140002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 04160002
.EQULOP  ANOP                                                           04180002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 04200002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     04220002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                04240002
         AGO   .EQULOP                  NO-GET NEXT                     04260002
.EQUOUT  ANOP                                                           04280002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  04300002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              04320002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 04340002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             04360002
         AGO   .START                   RETURN FOR NEXT                 04380002
         MEND                                                           04400002
         MACRO                                                          04420002
         CONVT ,                                                        04440002
         GBLA  &BITS(4000),&DB,&DT,&INITIAL                             04460002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  04480002
         GBLA  &ITEMNDX(4000),&ITEMXPT                                  04500002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              04520002
         GBLA  &NRENTS,&SHIFT(4000)                                     04540002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           04560002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(4000)                       04580002
         GBLC  &LABELS(4000),&LBL(100),&NRENTSX                         04600002
         LCLB  &SUB                                                     04620002
         LCLA  &OP                                                      04640002
         LCLA  &OFFSET,&OUT,&IN,&IND,&IBITS,&ISHIFT                     04660002
.*                                                                      04680002
.*       CONVERT BINARY TO DECIMAL                                      04700002
.*             &NAME IS THE REFERENCE INTO THE DSECT RECORD             04720002
.*             &OUT    IS THE NUMBER OF CHARS TO BE PUT ONTO PRINT LINE 04740002
.*                                      DEFAULT IS FOUR MAXIMUM IS 8    04760002
.*             &IN       IS THE NUMBER OF BYTES AS INPUT                04780002
.*                                      ERROR DEFAULT IS 2 MAXIMUM IS 4 04800002
.*                                                                      04820002
         AIF   (&DIAGTST EQ 0).NOMSG                                    04840002
         AIF   (&INITIAL EQ 0).NOMSG                                    04860002
         AIF   (&DIAGLIN EQ 1).NOMSG                                    04880002
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  04900002
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     04920002
*        LINE                                                           04940002
         LINE                                                           04960002
.NOMSG   ANOP                                                           04980002
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LLOP                  05000002
&SUB     SETB  1                                                        05020002
.LLOP    ANOP                                                           05040002
&OP      SETA  &OP+1                                                    05060002
&OUT     SETA  4                        .BR NOT NULL ELSE SET TO 4      05080002
         AIF   (&SUB EQ 1).SUBA                                         05100002
         AIF   ('&SYSLIST(3)' EQ '').B  TEST FOR NULL INPUT             05120002
&OUT     SETA  &SYSLIST(3)             SET &OUT FROM INPUT              05140002
         AGO   .A                                                       05160002
.SUBA    AIF   ('&SYSLIST(&OP,3)' EQ '').B                              05180002
&OUT     SETA  &SYSLIST(&OP,3)          SET &OUT FROM INPUT             05200002
.A       AIF   (&OUT GT 0 AND &OUT LT 9).B   TEST RANGE                 05220002
.AERR    MNOTE '*** BYTES OUT INCORRECTLY SPECIFIED DEFAULT USED ***'   05240002
&OUT     SETA  4                                                        05260002
.B       AIF   (&SUB EQ 1).SUBB                                         05280002
         AIF   ('&SYSLIST(2)' EQ '').LOOP    BRANCH IF NULL             05300002
&IN      SETA  &SYSLIST(2)              .SET &IN FROM INPUT             05320002
         AGO   .BB                                                      05340002
.SUBB    AIF   ('&SYSLIST(&OP,2)' EQ '').LOOP                           05360002
&IN      SETA  &SYSLIST(&OP,2)          .SET &IN FROM INPUT             05380002
.BB      AIF   (&IN GT 0 AND &IN LT 5).D     TEST RANGE                 05400002
.BERR    MNOTE '*** BYTES IN INCORRECTLY SPECIFIED DEFAULT USED ***'    05420002
&IN      SETA  2                                                        05440002
         AGO   .D                                                       05460002
.LOOP    ANOP                           .LOOP TO FIND &NAME IN DSECT    05480002
&IND     SETA  &IND+1                   .SET UP LOOP COUNTER            05500002
         AIF   ('&ITEM(&IND)' EQ '&SYSLIST(&OP,1)').FOUND               05520002
         AIF   (&IND LT &ITEMNBR).LOOP  .TEST FOR END OF DSECT          05540002
         MNOTE '*** LOG ITEM NOT DEFINED ***'                           05560002
         MEXIT                                                          05580002
.FOUND   ANOP                                                           05600002
&IBITS   SETA  &BITS(&IND)              .SET UP NUMBER OF BITS SPEC     05620002
         AIF   (&IBITS-((&IBITS/8)*8) EQ 0).C  .TEST FOR MULTIPLE OF 8  05640002
         MNOTE '*** DATA NOT DEFINED A MULTIPLE OF 8 BITS ***'          05660002
         MEXIT                                                          05680002
.C       ANOP                                                           05700002
&IN      SETA  &IBITS/8                 .COMPUTE NUMBER OF BYTES        05720002
         AIF   (&IN LT 1 OR &IN GT 4).BERR  .TEST RANGE B IF IN ERROR   05740002
&ISHIFT  SETA  &SHIFT(&IND)             .SET UP FOR TEST OF BYTE BNDR   05760002
         AIF   (&ISHIFT EQ 0).D         .TEST FOR BYTE BNDRY            05780002
         MNOTE '*** DATA NOT ALIGNED ON A BYTE BOUNDARY ***'            05800002
         MEXIT                                                          05820002
.*                                                                      05840002
.D       ANOP                                                           05860002
&NRENTS  SETA  &NRENTS+1                                                05880002
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),AL.4(9)                    05900002
         DC    X'&IN',X'&OUT'                                           05920002
&IND     SETA  0                                                        05940002
         AIF   (&SUB EQ 0).OUTA                                         05960002
         AIF   (&OP LT N'&SYSLIST).LLOP                                 05980002
.OUTA    ANOP                                                           06000002
         MEND                                                           06020002
         MACRO                                                          06040002
         ITEMFIND &ARG,&ITFDIAG=                                        06060002
         GBLA  &BITS(4000),&DB,&DT,&INITIAL                             06080002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  06100002
         GBLA  &ITEMNDX(4000),&ITEMXPT                                  06120002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              06140002
         GBLA  &NRENTS,&SHIFT(4000)                                     06160002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           06180002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(4000)                       06200002
         GBLC  &LABELS(4000),&LBL(100),&NRENTSX                         06220002
         LCLA  &A,&B,&X,&I                                              06240002
         LCLC  &C1,&C2,&S                                               06260002
         ACTR  100000                                                   06280002
&ITEMXPT SETA  0                  INDICATE ITEM NOT FOUND.              06300002
         AIF   ('&ARG' NE '').NORM                                      06320002
         AIF   ('&ITFDIAG' EQ 'OFF'  OR  '&ITFDIAG' EQ 'ON').DIAGSET    06340002
  MNOTE 0,'ITEMFIND - OPERAND (&ITFDIAG) INVALID FOR ITFDIAG KEYWORD.'  06360002
         MEXIT                                                          06380002
.DIAGSET ANOP                                                           06400002
&DIAGITF SETB  ('&ITFDIAG' EQ 'ON')                                     06420002
         MEXIT                                                          06440002
.NORM    AIF   (&ITEMITR GT 0).SORTED                                   06460002
         ITEMSORT                                                       06480002
.SORTED  ANOP                                                           06500002
&I       SETA  &ITEMITR           COMPUTED ITTERATION COUNT.            06520002
&X       SETA  &ITEMMDX           COMPUTED MAXIMUM INDEX.               06540002
&C1      SETC  '&ARG'             ISOLATE SEARCH ARGUMENT.              06560002
&C1      SETC  '&C1'.'       '(1,8-K'&C1) PAD TO FULL 8 CHARACTERS.     06580002
.SRCHUP  ANOP                                                           06600002
&A       SETA  &A+&X              CURRENT TABLE SUBSCRIPT + INDEX.      06620002
&S       SETC  '+'                                                      06640002
         AIF   (&A  GT &ITEMNBR  AND  NOT &DIAGITF).SRCHDN1             06660002
         AIF   (&A  LE &ITEMNBR).SRCH00                                 06680002
&C2      SETC  '*-OVER-*'                                               06700002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         06720002
&X       SETA  &X/2               HALVE INDEX VALUE.                    06740002
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X'                     06760002
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      06780002
.NOFIND  MNOTE  20,' ITEMFIND FAILED FOR NAME  &C1  '                   06800002
         MEXIT                                                          06820002
.SRCH00  ANOP                                                           06840002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         06860002
         AIF   (&I LT 0).NOFIND   IF COUNT HAS GONE TO ZERO.            06880002
&X       SETA  &X/2               HALVE INDEX VALUE.                    06900002
&B       SETA  &ITEMNDX(&A)       GET SUBSCRIPT FROM INDEX TABLE.       06920002
&C2      SETC  '&ITEM(&B)'.'       '(1,8-K'&ITEM(&B))  PAD COMPARAND.   06940002
         AIF   (NOT &DIAGITF).COMPARE                                   06960002
 MNOTE *,'&ARG - &C2   ITRCTR = &I   PTR = &A&S &X   POS = &B'          06980002
.COMPARE AIF   ('&C1' LT '&C2').SRCHDWN   ARGUMENT LOWER, MOVE DOWN.    07000002
         AIF   ('&C1' GT '&C2').SRCHUP    ARGUMENT HIGHER, MOVE UP.     07020002
&ITEMXPT SETA  &B                 FOUND. SET INDEX POINTER FOR RETURN.  07040002
&ITEMFST(&I+1)  SETA  &ITEMFST(&I+1)+1                                  07060002
         MEXIT                                                          07080002
.SRCHDWN ANOP                                                           07100002
&A       SETA  &A-&X              CURRENT POINTER MINUS INDEX VALUE.    07120002
&S       SETC  '-'                                                      07140002
         AIF   (&A LE &ITEMNBR).SRCH00    IF WITHIN BOUNDS OF TABLE.    07160002
.SRCHDN1 ANOP                                                           07180002
&I       SETA  &I-1               DECREMENT ITTERATION COUNTER.         07200002
&X       SETA  &X/2               HALVE INDEX VALUE.                    07220002
         AIF   (&I GE 0).SRCHDWN  IF ITTERATION COUNT NOT EXPIRED.      07240002
         MNOTE  20,' ITEMFIND FAILED FOR NAME  &C1  '                   07260002
         MEND                                                           07280002
         MACRO                                                          07300002
         ITEMSORT  &DUMMY                                               07320002
         GBLA  &BITS(4000),&DB,&DT,&INITIAL                             07340002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  07360002
         GBLA  &ITEMNDX(4000),&ITEMXPT                                  07380002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              07400002
         GBLA  &NRENTS,&SHIFT(4000)                                     07420002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           07440002
