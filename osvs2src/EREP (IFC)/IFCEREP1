         ST    @14,NEXTBY                                          0283 00530000
*  CALL PRINTIT;         /*CALL PRINT IT AND PRINT THE DUMP  @Z427VLS*/ 00531000
         BAL   @14,PRINTIT                                         0284 00532000
*  END;                                                    /*@Z427VLS*/ 00533000
         LA    @14,1                                               0285 00534000
         AL    @14,I                                               0285 00535000
         ST    @14,I                                               0285 00536000
@DE00281 C     @14,SORTNUM                                         0285 00537000
         BNH   @DL00281                                            0285 00538000
*END;                                                      /*@Z427VLS*/ 00539000
@EL00011 DS    0H                                                  0286 00540000
