*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/ 00545000
*/*    THIS PROC WILL CONVERT HEX DATA TO PRINTABLE          @Z427VLS*/ 00546000
*/*    NO LIMIT TO THE NUMBER, THE ADD 1 TO THE REGS IS TO COMPENSATE*/ 00547000
*/*    FOR THE 1 PLS TAKES AWAY                              @Z427VLS*/ 00548000
*/*                                                          @Z427VLS*/ 00549000
*CONVERT: PROC;                                            /*@Z427VLS*/ 00550000
CONVERT  STM   @14,@12,12(@13)                                     0287 00551000
*  DO J = NUMBER BY -1 TO 1;            /*# = # OF BYTES TO CONVERT  */ 00552000
         L     @14,NUMBER                                          0288 00553000
         ST    @14,J                                               0288 00554000
         B     @DE00288                                            0288 00555000
@DL00288 DS    0H                                                  0289 00556000
*    R8 = NEXTBYTE(J);                  /*LOAD NEXT BYTE TO CONVERT  */ 00557000
         L     @10,NEXTBY                                          0289 00558000
         BCTR  @10,0                                               0289 00559000
         SLR   R8,R8                                               0289 00560000
         IC    R8,NEXTBYTE(@14,@10)                                0289 00561000
*    X = TWO * J;                       /*AS OUT IS TWICE IN NEED X  */ 00562000
         LR    @10,@14                                             0290 00563000
         ALR   @10,@10                                             0290 00564000
         ST    @10,X                                               0290 00565000
*    SRDL (R8,FOUR);                    /*SPLIT THE BYTE IN HALF     */ 00566000
         SRDL  R8,4                                                0291 00567000
*    SRL  (R9,TWOEIGHT);                /*POSITION THE BYTE CORRECT  */ 00568000
         SRL   R9,28                                               0292 00569000
*    ADDNUM(X-ONE) = CHARS(R8+1);       /*PUT THE PRINT CHAR INTO OUT*/ 00570000
         L     @03,ADDNXT                                          0293 00571000
         ALR   @10,@03                                             0293 00572000
         BCTR  @10,0                                               0293 00573000
         BCTR  @10,0                                               0293 00574000
         LA    @07,CHARS(R8)                                       0293 00575000
         MVC   ADDNUM(1,@10),0(@07)                                0293 00576000
*    ADDNUM(X)     = CHARS(R9+1);       /*PUT IN REST OF BYTE        */ 00577000
         L     @10,X                                               0294 00578000
         ALR   @03,@10                                             0294 00579000
         BCTR  @03,0                                               0294 00580000
         LA    @10,CHARS(R9)                                       0294 00581000
         MVC   ADDNUM(1,@03),0(@10)                                0294 00582000
*  END;                                                    /*@Z427VLS*/ 00583000
         BCTR  @14,0                                               0295 00584000
         ST    @14,J                                               0295 00585000
@DE00288 LTR   @14,@14                                             0295 00586000
         BP    @DL00288                                            0295 00587000
*END;                                                      /*@Z427VLS*/ 00588000
@EL00012 DS    0H                                                  0296 00589000
@EF00012 DS    0H                                                  0296 00590000
@ER00012 LM    @14,@12,12(@13)                                     0296 00591000
         BR    @14                                                 0296 00592000
