@DL00281 DS    0H                                                  0282 00524000
*  CALL CONVERT;         /*GO CHANGE TEN BYTES TO TWENTY IN PRINTABLE*/ 00525000
         BAL   @14,CONVERT                                         0282 00526000
*  NEXTBY = NEXTBY + CHARLINE;                             /*@Z427VLS*/ 00527000
