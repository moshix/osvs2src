*  ADDNXT = ADDR(PRTLINE);/*SET UP ADDRESS OF WHERE TO PUT THE DATA  */ 00517000
         LA    @14,PRTLINE                                         0280 00518000
         ST    @14,ADDNXT                                          0280 00519000
*  DO I = 1 BY 1 TO SORTNUM;/*PROCESS THE ENTIRE SORT TABLE  @Z427VLS*/ 00520000
