*                 VADR=ADDR(PCCACPID)+4;/* SECOND WORD OF CPU ID     */ 00575000
         LA    VADR,PCCACPID(,PCCAPTR)                             0252 00576000
         AL    VADR,@CF00081                                       0252 00577000
*                 CALL ACCESRTN;    /* ACCESS IT                     */ 00578000
         BAL   @14,ACCESRTN                                        0253 00579000
*                 IF RCREG^=ZERO THEN/* ACCESS ERROR                 */ 00580000
         LTR   RCREG,RCREG                                         0254 00581000
         BZ    @RF00254                                            0254 00582000
*                   IDTABLE(INDEX)=BLANKS;/* FILL IN TABLE WITH    0255 00583000
*                                      BLANKS                        */ 00584000
         LR    @14,INDEX                                           0255 00585000
