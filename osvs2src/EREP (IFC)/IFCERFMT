*     DO;                                                          0237 00506000
         B     @RC00231                                            0237 00507000
@RF00231 DS    0H                                                  0238 00508000
*       PCCATPTR=DATAWORD;          /* SAVE PCCAVT ADDRESS           */ 00509000
*                                                                  0238 00510000
         L     PCCATPTR,DATAWORD(,DATAPTR)                         0238 00511000
*       /*************************************************************/ 00512000
*       /*                                                           */ 00513000
*       /* LOOP THROUGH THE PCCAVT FOR ALL PCCAS                     */ 00514000
*       /*                                                           */ 00515000
*       /*************************************************************/ 00516000
*                                                                  0239 00517000
*       DO INDEX=1 TO MP;           /* LOOP TO FIND CPUIDS           */ 00518000
         LA    INDEX,1                                             0239 00519000
@DL00239 DS    0H                                                  0240 00520000
*         VADR=PCCATPTR+(4*(INDEX-1));/* PTR TO PCCA                 */ 00521000
         LR    VADR,INDEX                                          0240 00522000
         BCTR  VADR,0                                              0240 00523000
         SLA   VADR,2                                              0240 00524000
         ALR   VADR,PCCATPTR                                       0240 00525000
*         CALL ACCESRTN;            /* ACCESS IT                     */ 00526000
         BAL   @14,ACCESRTN                                        0241 00527000
*         IF(RCREG^=ZERO)|(DATAWORD=ZERO) THEN/* COULD NOT ACCESS  0242 00528000
*                                      PCCA                          */ 00529000
