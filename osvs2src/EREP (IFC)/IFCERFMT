*     DO;                                                          0287 00730000
*       BUFFER(1:LENGTH(NOBUF))=NOBUF;/* ERROR MESSAGE               */ 00731000
         L     @14,ADPLBUF(,ABDPLPTR)                              0288 00732000
         MVC   BUFFER(89,@14),NOBUF                                0288 00733000
*       CALL PRINT;                 /* PRINT IT                      */ 00734000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0289 00735000
         BALR  @14,@15                                             0289 00736000
*     END;                          /* END ACCESS ERROR              */ 00737000
*   ELSE                            /* FOUND BUFFER                  */ 00738000
*     DO;                                                          0291 00739000
*                                                                  0291 00740000
         B     @RC00286                                            0291 00741000
@RF00286 DS    0H                                                  0292 00742000
*       /*************************************************************/ 00743000
*       /*                                                           */ 00744000
*       /* MOVE THE PRINT DUMP COPY INTO AN INTERNAL BUFFER TO ENSURE*/ 00745000
*       /* THE BUFFER STARTS ON A DOUBLE WORD                        */ 00746000
*       /*                                                           */ 00747000
*       /*************************************************************/ 00748000
*                                                                  0292 00749000
*       RFY                                                        0292 00750000
*        (COPYREG,                                                 0292 00751000
*         COPYSIZE,                                                0292 00752000
*         BUFREG,                                                  0292 00753000
*         BUFSIZE) RSTD;                                           0292 00754000
*       COPYREG=ADDR(COPYBUF);      /* WHERE TO COPY THE LOGREC    0293 00755000
*                                      BUFFER TO                     */ 00756000
         LA    COPYREG,COPYBUF                                     0293 00757000
*       COPYSIZE=LENGTH(COPYBUF);   /* SIZE OF THE LOGREC BUFFER     */ 00758000
