         LA    @06,IDTABLE-6(@14)                                  0243 00539000
         MVC   0(6,@06),BLANKS                                     0243 00540000
*         ELSE                      /* FOUND PCCA                    */ 00541000
*           DO;                                                    0244 00542000
*                                                                  0244 00543000
         B     @RC00242                                            0244 00544000
@RF00242 DS    0H                                                  0245 00545000
*             /*******************************************************/ 00546000
*             /*                                                     */ 00547000
*             /* FILL IN THE CPU ID FROM THE PCCA. THE PCCA CPID     */ 00548000
*             /* FIELD IS IN PRINTABLE FORMAT,AND NEEDS TO BE        */ 00549000
*             /* CONVERTED TO PACKED (ZONED) DECIMAL, IN ORDER TO BE */ 00550000
*             /* COMPARED WITH THE LOGREC ENTRY CPUID                */ 00551000
*             /*                                                     */ 00552000
*             /*******************************************************/ 00553000
*                                                                  0245 00554000
*             PCCAPTR=DATAWORD;     /* SAVE ADDRESS OF PCCA          */ 00555000
         L     PCCAPTR,DATAWORD(,DATAPTR)                          0245 00556000
*             VADR=ADDR(PCCACPID);  /* FIRST WORD OF CPU ID          */ 00557000
         LA    VADR,PCCACPID(,PCCAPTR)                             0246 00558000
*             CALL ACCESRTN;        /* ACCESS IT                     */ 00559000
         BAL   @14,ACCESRTN                                        0247 00560000
*             IF RCREG^=ZERO THEN   /* ACCESS ERROR                  */ 00561000
