         LA    @06,IDTABLE-6(@14)                                  0249 00567000
         MVC   0(6,@06),BLANKS                                     0249 00568000
*             ELSE                  /* FOUND FIRST PART OF CPU ID    */ 00569000
*               DO;                                                0250 00570000
         B     @RC00248                                            0250 00571000
@RF00248 DS    0H                                                  0251 00572000
*                 WORD1=DATAWORD;   /* FILL IN FIRST PART OF CPU ID  */ 00573000
