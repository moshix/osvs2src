*                   DO;                                            0334 00943000
*                     RECPTR=RECPTR+NEXTDWRD;/* INCREMENT TO NEXT  0335 00944000
*                                      DOUBLE WORD                   */ 00945000
         AH    RECPTR,@CH00087                                     0335 00946000
*                     CALL DWORDBDY;/* CALL TO CHECK IF NEXT DWORD IS   00947000
*                                      BEYOND END OF BUFFER          */ 00948000
         BAL   @14,DWORDBDY                                        0336 00949000
*                   END;            /* END DID NOT FIND A RECORD     */ 00950000
