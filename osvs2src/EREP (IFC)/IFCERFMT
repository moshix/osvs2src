         ALR   RECPTR,FRSTFREE                                     0325 00898000
         AL    RECPTR,HDRLNGTH                                     0325 00899000
*               CALL DWORDBDY;      /* MOVE PTR TO NEXT DOUBLEWORD   */ 00900000
*                                                                  0326 00901000
         BAL   @14,DWORDBDY                                        0326 00902000
*               /*****************************************************/ 00903000
*               /*                                                   */ 00904000
*               /* LOOP TO FIND THE FIRST RECORD BY CPUID            */ 00905000
*               /*                                                   */ 00906000
*               /*****************************************************/ 00907000
*                                                                  0327 00908000
*               DO UNTIL((RECPTR=FRSTFREE)|(VALIDCPU=ON));/* LOOP  0327 00909000
*                                      UNTIL THE BUFFER IS EXHAUSTED    00910000
*                                      OR A RECORD IS FOUND          */ 00911000
@DL00327 DS    0H                                                  0328 00912000
*                 DO INDEX=1 TO MP; /* INNER LOOP-SCAN ALL POSSIBLE     00913000
*                                      VALID CPU IDS                 */ 00914000
         LA    INDEX,1                                             0328 00915000
@DL00328 DS    0H                                                  0329 00916000
*                   IF(IDTABLE(INDEX)^=BLANKS)&/* CPUID NOT BLANKS 0329 00917000
*                                      (ONE OF THE ENTRIES IS BLANK     00918000
*                                      IN A UP                 SYSTEM*/ 00919000
