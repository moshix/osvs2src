         CLC   CPID(6,RECPTR),0(@02)                               0329 00928000
         BNE   @RF00329                                            0329 00929000
*                     VALIDCPU=ON;  /* INDICATE A RECORD FOUND       */ 00930000
         OI    VALIDCPU,B'10000000'                                0330 00931000
*                   ELSE                                           0331 00932000
*                     ;             /* A RECORD NOT FOUND            */ 00933000
@RF00329 DS    0H                                                  0332 00934000
*                 END;              /* END INNER LOOP TO SCAN FOR  0332 00935000
*                                      VALID CPU ID                  */ 00936000
         AH    INDEX,@CH00042                                      0332 00937000
         CH    INDEX,@CH00100                                      0332 00938000
         BNH   @DL00328                                            0332 00939000
*                 IF VALIDCPU=OFF THEN/* DID NOT FIND A RECORD       */ 00940000
         TM    VALIDCPU,B'10000000'                                0333 00941000
