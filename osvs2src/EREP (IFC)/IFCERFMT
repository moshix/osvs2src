         LA    @06,IDTABLE-6(@14)                                  0255 00587000
         MVC   0(6,@06),BLANKS                                     0255 00588000
*                 ELSE              /* FOUND SECOND WORD OF CPU ID   */ 00589000
*                   DO;                                            0256 00590000
         B     @RC00254                                            0256 00591000
@RF00254 DS    0H                                                  0257 00592000
*                     WORD2=DATAWORD;/* FILL IN SECOND PART CPU ID   */ 00593000
         MVC   WORD2(4),DATAWORD(DATAPTR)                          0257 00594000
*                     VADR=ADDR(PCCACPID)+8;/* LAST WORD OF CPU ID   */ 00595000
         LA    VADR,PCCACPID(,PCCAPTR)                             0258 00596000
         AH    VADR,@CH00087                                       0258 00597000
*                     CALL ACCESRTN;/* ACCESS IT                     */ 00598000
         BAL   @14,ACCESRTN                                        0259 00599000
*                     IF RCREG^=ZERO THEN/* ACCESS ERROR             */ 00600000
         LTR   RCREG,RCREG                                         0260 00601000
         BZ    @RF00260                                            0260 00602000
*                       IDTABLE(INDEX)=BLANKS;/* FILL IN TABLE WITH     00603000
*                                      BLANKS                        */ 00604000
         LR    @14,INDEX                                           0261 00605000
         MH    @14,@CH00879                                        0261 00606000
         LA    @06,IDTABLE-6(@14)                                  0261 00607000
         MVC   0(6,@06),BLANKS                                     0261 00608000
*                     ELSE          /* FOUND THIRD WORD OF CPU ID    */ 00609000
*                       DO;                                        0262 00610000
         B     @RC00260                                            0262 00611000
@RF00260 DS    0H                                                  0263 00612000
*                         WORD3=DATAWORD;/* FILL IN LAST WORD OF CPU    00613000
*                                      ID                            */ 00614000
         MVC   WORD3(4),DATAWORD(DATAPTR)                          0263 00615000
*                         TR(SAVETBLE,TRANS);/* TRANSLATE TO UNPACKED   00616000
*                                      DIGITS                        */ 00617000
         TR    SAVETBLE(13),TRANS                                  0264 00618000
*                         PACK(TEMPID,SAVETBLE);/* PACK INTO TEMP  0265 00619000
*                                      AREA(TO ELIMINATE ZONES)      */ 00620000
         PACK  TEMPID(7),SAVETBLE(13)                              0265 00621000
*                         IDTABLE(INDEX)=TEMPID(1:6);/* MOVE ID INTO    00622000
*                                      CPU ID TABLE                  */ 00623000
         LR    @14,INDEX                                           0266 00624000
         MH    @14,@CH00879                                        0266 00625000
         LA    @06,IDTABLE-6(@14)                                  0266 00626000
         MVC   0(6,@06),TEMPID                                     0266 00627000
*                       END;        /* END FOUND THIRD WORD OF CPU ID*/ 00628000
*                   END;            /* END FOUND SECOND WORD OF CPU     00629000
*                                      ID                            */ 00630000
@RC00260 DS    0H                                                  0269 00631000
*               END;                /* END FOUND FIRST PART OF CPU ID*/ 00632000
@RC00254 DS    0H                                                  0270 00633000
*           END;                    /* END FOUND PCCA                */ 00634000
@RC00248 DS    0H                                                  0271 00635000
*       END;                        /* END LOOP TO FIND CPU IDS      */ 00636000
*                                                                  0271 00637000
@RC00242 AH    INDEX,@CH00042                                      0271 00638000
         CH    INDEX,@CH00100                                      0271 00639000
         BNH   @DL00239                                            0271 00640000
*       /*************************************************************/ 00641000
*       /*                                                           */ 00642000
*       /* CHECK IF A VALID CPU ID WAS FOUND                         */ 00643000
*       /*                                                           */ 00644000
*       /*************************************************************/ 00645000
*                                                                  0272 00646000
*       IF IDTABLE(1)=IDTABLE(2) THEN/* ONLY EQUAL IF BOTH ARE BLANK-   00647000
*                                      COULD NOT BE FOUND            */ 00648000
         CLC   IDTABLE(6),IDTABLE+6                                0272 00649000
