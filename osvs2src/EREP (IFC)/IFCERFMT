*                                                                  0404 01269000
*   /*****************************************************************/ 01270000
*   /*                                                               */ 01271000
*   /* NO RESTORE REGS SPECIFIED                                     */ 01272000
*   /*                                                               */ 01273000
*   /*****************************************************************/ 01274000
*                                                                  0404 01275000
*   DO;                             /* ENTESTAE PARMREG(DREG)        */ 01276000
*/*  ENTRY CODE FOR ESTAE ROUTINE.                                   */ 01277000
*/*  IF RTCA ALLOCATED THEN RESTORE CODE AND                         */ 01278000
*/*  DATA REGS FROM START OF USERS PARM LIST,                        */ 01279000
*/*  AND ESTABLISH ADDR TO ESTAE SAVE AREA                           */ 01280000
*/*  IF REQUIRED.                                                    */ 01281000
*/*  IF NO RTCA IS AVAILABLE THEN JUST RETURN                        */ 01282000
*/*  TO RTM TO CONTINUE TERMINATION                                  */ 01283000
*                                                                  0405 01284000
*     GEN(USING *,GPR15P);                                         0405 01285000
         USING *,GPR15P                                                 01286000
*     GEN(CH    GPR00F,*+14);       /* CHECK IF RTCA ALLOC FAILED    */ 01287000
         CH    GPR00F,*+14                                              01288000
*     GEN(BNE   *+12);              /* NO,SAVE RTM REGS              */ 01289000
         BNE   *+12                                                     01290000
*     GEN(DROP  GPR15P);            /* DROP LOCAL ADDR               */ 01291000
         DROP  GPR15P                                                   01292000
*     GEN(LA    GPR15P,0);          /* YES, SET CONT TERM RC         */ 01293000
         LA    GPR15P,0                                                 01294000
*     GEN(BR    GPR14P);            /* RETURN TO RTM                 */ 01295000
         BR    GPR14P                                                   01296000
*     GEN(DC    H'12');             /* NO RTCA AVAIL RC              */ 01297000
         DC    H'12'                                                    01298000
*     GEN(STM   GPR14P,GPR12F,12(GPR13P));/* GOOD R13, SAVE RTM'S  0412 01299000
*                                      REGS                          */ 01300000
         STM   GPR14P,GPR12F,12(GPR13P)                                 01301000
*     GEN(L     DREG,0(GPR01P));    /* PTR TO USER PARM LIST         */ 01302000
         L     DREG,0(GPR01P)                                           01303000
*   END;                            /* ENTESTAE PARMREG(DREG)        */ 01304000
*                                                                  0414 01305000
*   /*****************************************************************/ 01306000
*   /*                                                               */ 01307000
*   /* THIS CODE SETS DREG TO BE THE POINTER TO ALL THE AUTOMATIC    */ 01308000
*   /* DATA FOR THIS MODULE                                          */ 01309000
*   /*                                                               */ 01310000
*   /*****************************************************************/ 01311000
*                                                                  0415 01312000
*   LM(GPR02P,GPR12F,REG2TO12);     /* RESTORE THE REGS SAVED FROM 0415 01313000
*                                      THE MAINLINE                  */ 01314000
         LM    GPR02P,GPR12F,REG2TO12                              0415 01315000
*   SDWAPTR=GPR01P;                 /* SDWA ADDR PASSED TO THIS    0416 01316000
*                                      ROUTINE                       */ 01317000
*                                                                  0416 01318000
         ST    GPR01P,SDWAPTR                                      0416 01319000
*   /*****************************************************************/ 01320000
*   /*                                                               */ 01321000
*   /* TO PREVENT MORE THAN ONE DUMP FROM BEING TAKEN FOR AN X37 TYPE*/ 01322000
*   /* ABEND, PERCOLATE TO THE CALLERS RECOVERY. THIS WILL PREVENT   */ 01323000
*   /* THIS ESTAE FROM TAKING A DUMP AND WILL LET THE CALLERS        */ 01324000
*   /* RECOVERY TAKE IT.                                     @ZM43805*/ 01325000
*   /*                                                               */ 01326000
*   /*****************************************************************/ 01327000
*                                                                  0417 01328000
*   IF ABNDTYPE^='37'X THEN         /* PERCOLATE FOR ALL X37 TYPE  0417 01329000
*                                      ABEND CODES           @ZM43805*/ 01330000
         L     @14,SDWAPTR                                         0417 01331000
         MVC   @TS00001(2),ABNDTYPE(@14)                           0417 01332000
         NC    @TS00001(2),@CB02579                                0417 01333000
         CLC   @TS00001(2),@CB02581                                0417 01334000
         BE    @RF00417                                            0417 01335000
*     DO;                           /* NOT AN X37 TYPE ABEND @ZM43805*/ 01336000
*       IF UNEXP2=OFF THEN          /* THE FIRST UNEXPECTED ERROR    */ 01337000
         TM    UNEXP2,B'10000000'                                  0419 01338000
         BNZ   @RF00419                                            0419 01339000
*         DO;                                                      0420 01340000
*           UNEXP2=ON;              /* RETRY ONLY ONCE - PREVENT LOOP*/ 01341000
         OI    UNEXP2,B'10000000'                                  0421 01342000
*           RTYADR=ADDR(TROUBLE);   /* RETRY POINT IN MAIN LINE FOR     01343000
*                                      UNEXPECTED ERRORS             */ 01344000
         LA    @14,TROUBLE                                         0422 01345000
         ST    @14,RTYADR                                          0422 01346000
*           SDWASRSV=RTYREGS;       /* SET RETRY REGS FOR MAINLINE 0423 01347000
*                                      RETRY                         */ 01348000
         L     @02,SDWAPTR                                         0423 01349000
         MVC   SDWASRSV(64,@02),RTYREGS                            0423 01350000
*           ABDPLPTR=RTYREG1;       /* RESTORE PTR TO PRINT DUMP   0424 01351000
*                                      PARMLIST-REG1                 */ 01352000
         L     ABDPLPTR,RTYREG1                                    0424 01353000
*           BUFFER(1:LENGTH(ENDMSG))=ENDMSG;/* ABEND MESSAGE         */ 01354000
         L     @09,ADPLBUF(,ABDPLPTR)                              0425 01355000
         MVC   BUFFER(45,@09),ENDMSG                               0425 01356000
*           PACKAREA=ABNDCMPC;      /* MOVE COMP CODE TO CHAR AREA   */ 01357000
         MVC   PACKAREA(2),ABNDCMPC(@02)                           0426 01358000
*           GARBAGE='04'X;          /* GARBAGE BYTE FOR THE UNPK     */ 01359000
         MVI   GARBAGE,X'04'                                       0427 01360000
*           UNPK(BUFFER(46:50),UNPACK);/* UNPACK INTO THE BUFFER     */ 01361000
         UNPK  BUFFER+45(5,@09),UNPACK(3)                          0428 01362000
*           TR(BUFFER(46:50),TRANS);/* TRANSLATE TO PRINTABLE HEX    */ 01363000
         TR    BUFFER+45(5,@09),TRANS                              0429 01364000
*           BUFFER(49:49)=' ';      /* BLANK OUT SECOND BYTE         */ 01365000
         MVI   BUFFER+48(@09),C' '                                 0430 01366000
*           GPR01P=SDWAPTR;         /* RESTORE REGISTER 1 FOR SETRP  */ 01367000
         LR    GPR01P,@02                                          0431 01368000
*           DO;                     /* SETRP REGS(14,12)DUMP(YES)RC(4   01369000
*                                      )RETADDR(RETRYPT)RETREGS(YES)F   01370000
*                                      RESDWA(YES)                   */ 01371000
*             RESPECIFY                                            0433 01372000
*              (GPR00P,                                            0433 01373000
*               GPR01P,                                            0433 01374000
*               GPR14P,                                            0433 01375000
*               GPR15P) RSTD;                                      0433 01376000
*             GPR01P->SDWARCDE=4;   /* STORE RC INTO SDWA            */ 01377000
         MVI   SDWARCDE(GPR01P),X'04'                              0434 01378000
*             GPR01P->SDWARTYA=ADDR(RETRYPT);/* SAVE RETRY ADDRESS   */ 01379000
         L     @02,RTYADR                                          0435 01380000
         ST    @02,SDWARTYA(,GPR01P)                               0435 01381000
*             GPR01P->SDWAFREE='1'B;/* SET FLAG TO FREE SDWA         */ 01382000
         OI    SDWAFREE(GPR01P),B'00000100'                        0436 01383000
*             GPR01P->SDWAREQ='1'B; /* TURN ON DUMP INDICATOR        */ 01384000
         OI    SDWAREQ(GPR01P),B'10000000'                         0437 01385000
*             GPR01P->SDWAUPRG='1'B;/* TURN ON RETREGS INDICATOR     */ 01386000
         OI    SDWAUPRG(GPR01P),B'00001000'                        0438 01387000
*             GEN(RETURN (14,12));                                 0439 01388000
         RETURN (14,12)                                                 01389000
*             RESPECIFY                                            0440 01390000
*              (GPR00P,                                            0440 01391000
*               GPR01P,                                            0440 01392000
*               GPR14P,                                            0440 01393000
*               GPR15P) UNRSTD;                                    0440 01394000
*           END;                                                   0441 01395000
*         END;                      /* END FIRST UNEXPECTED ERROR    */ 01396000
*       ELSE                        /* PERCOLATE FOR SECOND        0443 01397000
*                                      UNEXPECTED ERROR              */ 01398000
*         DO;                       /* SETRP                       0443 01399000
*                                      REGS(14,12)DUMP(YES)RC(0)     */ 01400000
         B     @RC00419                                            0443 01401000
@RF00419 DS    0H                                                  0444 01402000
*           RESPECIFY                                              0444 01403000
*            (GPR00P,                                              0444 01404000
*             GPR01P,                                              0444 01405000
*             GPR14P,                                              0444 01406000
*             GPR15P) RSTD;                                        0444 01407000
*           GPR01P->SDWARCDE=0;     /* SAVE RC VALUE                 */ 01408000
         MVI   SDWARCDE(GPR01P),X'00'                              0445 01409000
*           GPR01P->SDWAREQ='1'B;   /* TURN ON DUMP INDICATOR        */ 01410000
         OI    SDWAREQ(GPR01P),B'10000000'                         0446 01411000
*           GEN(RETURN (14,12));                                   0447 01412000
         RETURN (14,12)                                                 01413000
*           RESPECIFY                                              0448 01414000
*            (GPR00P,                                              0448 01415000
*             GPR01P,                                              0448 01416000
*             GPR14P,                                              0448 01417000
*             GPR15P) UNRSTD;                                      0448 01418000
*         END;                      /*                       @ZM43805*/ 01419000
*     END;                          /* END NOT AN X37 TYPE ABEND   0450 01420000
*                                                            @ZM43805*/ 01421000
*   ELSE                            /* THIS IS AN X37 ABEND -      0451 01422000
*                                      PERCOLATE             @ZM43805*/ 01423000
*     DO;                           /* SETRP                       0451 01424000
*                                      REGS(14,12)DUMP(YES)RC(0)     */ 01425000
         B     @RC00417                                            0451 01426000
@RF00417 DS    0H                                                  0452 01427000
*       RESPECIFY                                                  0452 01428000
*        (GPR00P,                                                  0452 01429000
*         GPR01P,                                                  0452 01430000
*         GPR14P,                                                  0452 01431000
*         GPR15P) RSTD;                                            0452 01432000
*       GPR01P->SDWARCDE=0;         /* SAVE RC VALUE                 */ 01433000
         MVI   SDWARCDE(GPR01P),X'00'                              0453 01434000
*       GPR01P->SDWAREQ='1'B;       /* TURN ON DUMP INDICATOR        */ 01435000
         OI    SDWAREQ(GPR01P),B'10000000'                         0454 01436000
*       GEN(RETURN (14,12));                                       0455 01437000
         RETURN (14,12)                                                 01438000
*       RESPECIFY                                                  0456 01439000
*        (GPR00P,                                                  0456 01440000
*         GPR01P,                                                  0456 01441000
*         GPR14P,                                                  0456 01442000
*         GPR15P) UNRSTD;                                          0456 01443000
*     END;                          /*                       @ZM43805*/ 01444000
*   END ESTAERTN;                                                  0458 01445000
@EL00007 DS    0H                                                  0458 01446000
@EF00007 DS    0H                                                  0458 01447000
@ER00007 BR    @14                                                 0458 01448000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 01449000
*     GPR00F FIXED(31) REG(0),                                     0459 01450000
*     GPR12F FIXED(31) REG(12),                                    0459 01451000
*     GPR00P PTR(31) REG(0),                                       0459 01452000
*     GPR01P PTR(31) REG(1),                                       0459 01453000
