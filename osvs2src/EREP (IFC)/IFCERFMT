         TITLE 'IFCERFMT IN-STORAGE LOGREC BUFFER FORMAT MODULE'        00001000
IFCERFMT CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'IFCERFMT  79.010'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00009000
         BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         L     @00,@SIZDATD                                        0001 00013000
         GETMAIN  R,LV=(0)                                              00014000
         LR    @03,@01                                             0001 00015000
         LA    @10,4095(,@03)                                      0001 00016000
         LA    @11,4095(,@10)                                      0001 00017000
         USING @DATD,@03                                           0001 00018000
         USING @DATD+4095,@10                                      0001 00019000
         USING @DATD+8190,@11                                      0001 00020000
         ST    @13,@SA00001+4                                      0001 00021000
         LM    @00,@01,20(@13)                                     0001 00022000
         ST    @03,8(,@13)                                         0001 00023000
         LR    @13,@03                                             0001 00024000
*                                                                  0139 00025000
*/********************************************************************/ 00026000
*/* FUNCTION-                                                      0139 00027000
*       ROUTINE TO SET UP THE ESTAE, DETERMINES IF THE RUNNING SYSTEM   00028000
*     HAS THE EREP ENHANCEMENT SU, FILLS IN THE EREP COMMUNICATIONS     00029000
*     TABLE, AND PASSES CONTROL TO EREP TO EDIT THE RECORDS, IF ALL     00030000
*     THE RECORDS WERE FOUND WITHOUT PROBLEM.                      0139 00031000
*                                                                  0139 00032000
*       MESSAGES ISSUED BY THIS ROUTINE ARE:                       0139 00033000
*                                                                  0139 00034000
*               *** IN STORAGE LOGREC BUFFER NOT FORMATTED, EREP   0139 00035000
*                   ENHANCEMENT SU NOT AVAILABLE ON SYSTEM ***     0139 00036000
*          (IF SU 27 IS NOT ON THE SYSTEM)                         0139 00037000
*                                                                  0139 00038000
*               *** IN STORAGE LOGREC BUFFER NOT FORMATTED, PARAMETER   00039000
*                   LIST DID NOT CONTAIN NECESSARY INFORMATION *** 0139 00040000
*          (IF THE CVT POINTER IS ZERO)                            0139 00041000
*                                                                  0139 00042000
*               *** LOGDATA FORMATTING OMITTED, DUMP IS NOT FROM AN     00043000
*                   MVS SYSTEM ***                                 0139 00044000
*          (IF THE OPERATING SYSTEM FLAGS COULD NOT BE ACCESSED OR IF   00045000
*           THE SYSTEM IS NOT MVS)                                 0139 00046000
*                                                                  0139 00047000
*               *** IN-STORAGE LOGREC BUFFER NOT FORMATTED, DATA   0139 00048000
*                   NECESSARY TO LOCATE THE BUFFER UNAVAILABLE *** 0139 00049000
*          (IF THE RTCT COULD NOT BE ACCESSED OR THE ADDRESS IS    0139 00050000
*           NOT ON A 4K BOUNDARY)                                  0139 00051000
*                                                                  0139 00052000
*                                                                  0139 00053000
*    CALLS-                                                        0139 00054000
*         ACCESRTN, CPUID, DEBLOCK, IFCEDSUM, AND PRINT            0139 00055000
*                                                                  0139 00056000
*    CALLED BY -                                                   0139 00057000
*         AMDPRDMP WHEN THE LOGDATA VERB IS ENCOUNTERED            0139 00058000
*                                                                  0139 00059000
*    RETURNS TO -                                                  0139 00060000
*         CALLER WITH A RETURN CODE OF ZERO                        0139 00061000
*                                                                    */ 00062000
*/********************************************************************/ 00063000
*                                                                  0139 00064000
*   /*****************************************************************/ 00065000
*   /*                                                               */ 00066000
*   /* ESTABLISH THE ESTAE                                           */ 00067000
*   /*                                                               */ 00068000
*   /*****************************************************************/ 00069000
*                                                                  0139 00070000
*   ESTAERR=ZERO;                   /* TURN OFF ESTAE BITS   @ZM45067*/ 00071000
         SLR   @14,@14                                             0139 00072000
         ST    @14,ESTAERR                                         0139 00073000
*   STM(VADR,RCREG,RTYREGS);        /* SAVE REGS TO BE USED IN THE 0140 00074000
*                                      ESTAE                 @ZM45067*/ 00075000
         STM   VADR,RCREG,RTYREGS                                  0140 00076000
*   SAVEREG1=ABDPLPTR;              /* SAVE REGISTER 1 ACCESS THE  0141 00077000
*                                      ESTAE                         */ 00078000
         LR    SAVEREG1,ABDPLPTR                                   0141 00079000
*   ESTAEXEC(1:ESTALEN)=ESTALIST(1:ESTALEN);/* MOVE LIST INTO      0142 00080000
*                                      AUTOMATIC AREA                */ 00081000
         L     @14,ESTALEN                                         0142 00082000
         BCTR  @14,0                                               0142 00083000
         EX    @14,@SM02572                                        0142 00084000
*   GEN FLOWS(ESTAERTN)(ESTAE ESTAERTN,PARAM=(DREG),MF=(E,ESTAEXEC));   00085000
         ESTAE ESTAERTN,PARAM=(DREG),MF=(E,ESTAEXEC)                    00086000
*                                   /* THE PARM, FOR THIS ESTAE,   0144 00087000
*                                      POINTS TO ALL THE AUTOMATIC 0144 00088000
*                                      DATA FOR THIS MODULE          */ 00089000
*   IF RCREG=ZERO THEN              /* ESTAE RESOLVED FINE           */ 00090000
         LTR   RCREG,RCREG                                         0144 00091000
         BNZ   @RF00144                                            0144 00092000
*     DO;                                                          0145 00093000
*       ABDPLPTR=SAVEREG1;          /* RESTORE REGISTER 1            */ 00094000
         LR    ABDPLPTR,SAVEREG1                                   0146 00095000
*       EREPBUF=BUFFER;             /* USE PRINT DUMPS BLANK BUFFER     00096000
*                                      TO BLANK OUT EREPS OUTPUT   0147 00097000
*                                      BUFFER                        */ 00098000
*                                                                  0147 00099000
         MVI   EREPBUF+120,C' '                                    0147 00100000
         MVC   EREPBUF+121(13),EREPBUF+120                         0147 00101000
         L     @14,ADPLBUF(,ABDPLPTR)                              0147 00102000
         MVC   EREPBUF(120),BUFFER(@14)                            0147 00103000
*       /*************************************************************/ 00104000
*       /*                                                           */ 00105000
*       /* PUT OUT HEADER                                            */ 00106000
*       /*                                                           */ 00107000
*       /*************************************************************/ 00108000
*                                                                  0148 00109000
*       CALL PRINT;                 /* BLANK LINE                    */ 00110000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0148 00111000
         BALR  @14,@15                                             0148 00112000
*       BUFFER(51:LENGTH(HDRLINE)+50)=HDRLINE;/* LOGDATA HDR         */ 00113000
         L     @14,ADPLBUF(,ABDPLPTR)                              0149 00114000
         MVC   BUFFER+50(31,@14),HDRLINE                           0149 00115000
*       CALL PRINT;                 /* PRINT IT                      */ 00116000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0150 00117000
         BALR  @14,@15                                             0150 00118000
*       CALL PRINT;                 /* SPACE A LINE                  */ 00119000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0151 00120000
         BALR  @14,@15                                             0151 00121000
*       CALL PRINT;                 /* SPACE A LINE                  */ 00122000
*                                                                  0152 00123000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0152 00124000
         BALR  @14,@15                                             0152 00125000
*       /*************************************************************/ 00126000
*       /*                                                           */ 00127000
*       /* CHECK IF EREP ENHANCEMENT SU IS ON THE RUNNING SYSTEM,    */ 00128000
*       /* (NOT THE SYSTEM ON WHICH THE DUMP WAS TAKEN               */ 00129000
*       /*                                                           */ 00130000
*       /*************************************************************/ 00131000
*                                                                  0153 00132000
*       CVTPTR=LOC16;               /* REAL RUNNING SYSTEM CVT ADDR  */ 00133000
         L     CVTPTR,LOC16                                        0153 00134000
*       IF SU27BIT=OFF THEN         /* EREP ENHANCEMENT SU NOT ON  0154 00135000
*                                                              SYSTEM*/ 00136000
         L     @14,CVTIHASU(,CVTPTR)                               0154 00137000
         TM    SU27BIT(@14),B'00010000'                            0154 00138000
         BNZ   @RF00154                                            0154 00139000
*         BUFFER(1:LENGTH(NOSU27))=NOSU27;/* ERROR MESSAGE           */ 00140000
         L     @14,ADPLBUF(,ABDPLPTR)                              0155 00141000
         MVC   BUFFER(91,@14),NOSU27                               0155 00142000
*       ELSE                        /* EREP ENHANCEMENT IS ON SYSTEM */ 00143000
*         DO;                                                      0156 00144000
*                                                                  0156 00145000
         B     @RC00154                                            0156 00146000
@RF00154 DS    0H                                                  0157 00147000
*           /*********************************************************/ 00148000
*           /*                                                       */ 00149000
*           /* CHECK IF A CVT ADDRESS WAS PASSED IN THE PARMLIST     */ 00150000
*           /*                                                       */ 00151000
*           /*********************************************************/ 00152000
*                                                                  0157 00153000
*           IF ADPLCVT=ZERO THEN    /* NO CVT ADDRESS PASSED         */ 00154000
         ICM   @14,15,ADPLCVT(ABDPLPTR)                            0157 00155000
         BNZ   @RF00157                                            0157 00156000
*             BUFFER(1:LENGTH(NOINF))=NOINF;/* ERROR MESSAGE         */ 00157000
         L     @14,ADPLBUF(,ABDPLPTR)                              0158 00158000
         MVC   BUFFER(100,@14),NOINF                               0158 00159000
*           ELSE                    /* A CVT IS PASSED               */ 00160000
*             DO;                                                  0159 00161000
*                                                                  0159 00162000
         B     @RC00157                                            0159 00163000
@RF00157 DS    0H                                                  0160 00164000
*               /*****************************************************/ 00165000
*               /*                                                   */ 00166000
*               /* DETERMINE IF THE SYSTEM ON WHICH THE DUMP WAS     */ 00167000
*               /* TAKEN WAS AN MVS SYSTEM                           */ 00168000
*               /*                                                   */ 00169000
*               /*****************************************************/ 00170000
*                                                                  0160 00171000
*               CVTPTR=ADPLCVT;     /* REFERENCED CVT IS NOW THE DUMP   00172000
*                                      CVT                           */ 00173000
         L     CVTPTR,ADPLCVT(,ABDPLPTR)                           0160 00174000
*               VADR=ADDR(CVTDCB);  /* OPERATING SYSTEM FLAGS        */ 00175000
         LA    VADR,CVTDCB(,CVTPTR)                                0161 00176000
*               CALL ACCESRTN;      /* ACCESS IT                     */ 00177000
         BAL   @14,ACCESRTN                                        0162 00178000
*               IF RCREG^=ZERO THEN /* ACCESS ERROR                  */ 00179000
         LTR   RCREG,RCREG                                         0163 00180000
         BZ    @RF00163                                            0163 00181000
*                 BUFFER(1:LENGTH(INCOMP))=INCOMP;/* ERROR MSG       */ 00182000
         L     @14,ADPLBUF(,ABDPLPTR)                              0164 00183000
         MVC   BUFFER(66,@14),INCOMP                               0164 00184000
*               ELSE                /* FOUND OPERATING SYSTEM FLAGS  */ 00185000
*                 DO;                                              0165 00186000
         B     @RC00163                                            0165 00187000
@RF00163 DS    0H                                                  0166 00188000
*                   IF ADPLSYTM=ON|(DATABYTE&LOWBIT)^=LOWBIT THEN/*     00189000
*                                      NOT AN MVS SYSTEM             */ 00190000
         TM    ADPLSYTM(ABDPLPTR),B'01000000'                      0166 00191000
         BO    @RT00166                                            0166 00192000
         SLR   @14,@14                                             0166 00193000
         IC    @14,LOWBIT                                          0166 00194000
         SLR   @08,@08                                             0166 00195000
         IC    @08,DATABYTE(,DATAPTR)                              0166 00196000
         NR    @08,@14                                             0166 00197000
         CR    @08,@14                                             0166 00198000
         BE    @RF00166                                            0166 00199000
@RT00166 DS    0H                                                  0167 00200000
*                     BUFFER(1:LENGTH(INCOMP))=INCOMP;/* ERROR     0167 00201000
*                                      MESSAGE                       */ 00202000
         L     @14,ADPLBUF(,ABDPLPTR)                              0167 00203000
         MVC   BUFFER(66,@14),INCOMP                               0167 00204000
*                   ELSE            /* IS AN MVS SYSTEM              */ 00205000
*                     DO;                                          0168 00206000
*                                                                  0168 00207000
         B     @RC00166                                            0168 00208000
@RF00166 DS    0H                                                  0169 00209000
*                       /*********************************************/ 00210000
*                       /*                                           */ 00211000
*                       /* LOCATE THE IN-STORAGE LOGREC BUFFER THE   */ 00212000
*                       /* BUFFER CONSISTS OF THREE PARTS, THE RCB   */ 00213000
*                       /* WHICH IS THE HEADER FOR THE BUFFER. IT    */ 00214000
*                       /* INDICATES THE BEGINNING ADDRESS OF THE    */ 00215000
*                       /* BUFFER, THE END ADDRESS, AND THE POINTER  */ 00216000
*                       /* TO THE FIRST FREE ADDRESS. BEFORE EACH    */ 00217000
*                       /* ENTRY IN THE BUFFER, THERE ARE TWO        */ 00218000
*                       /* HEADERS. THE FIRST, THE BEHDR, HAS CONTROL*/ 00219000
*                       /* INFORMATION ABOUT THE RECORD. THE         */ 00220000
*                       /* SECOND,THE LRB, HAS INFORMATION ABOUT THE */ 00221000
*                       /* LOGREC ENTRY ITSELF.                      */ 00222000
*                       /*                                           */ 00223000
*                       /*********************************************/ 00224000
*                                                                  0169 00225000
*                       VADR=ADDR(CVTRTMCT);/* PTR TO RTCT           */ 00226000
         LA    VADR,CVTRTMCT(,CVTPTR)                              0169 00227000
*                       CALL ACCESRTN;/* ACCESS IT                   */ 00228000
         BAL   @14,ACCESRTN                                        0170 00229000
*                       IF RCREG^=ZERO THEN/* COULD NOT ACCESS CVT   */ 00230000
         LTR   RCREG,RCREG                                         0171 00231000
         BZ    @RF00171                                            0171 00232000
*                         BUFFER(1:LENGTH(LOCERR))=LOCERR;/* ERROR 0172 00233000
*                                      MESSAGE                       */ 00234000
         L     @14,ADPLBUF(,ABDPLPTR)                              0172 00235000
         MVC   BUFFER(95,@14),LOCERR                               0172 00236000
*                       ELSE        /* FOUND RTCT PTR                */ 00237000
*                         DO;                                      0173 00238000
         B     @RC00171                                            0173 00239000
@RF00171 DS    0H                                                  0174 00240000
*                           RTCTPTR=DATAWORD;/* SAVE THE RTCT ADDRESS*/ 00241000
         L     @14,DATAWORD(,DATAPTR)                              0174 00242000
         ST    @14,RTCTPTR                                         0174 00243000
*                           VADR=ADDR(RTCTRCB);/* PTR TO IN-STORAGE     00244000
*                                      LOGREC BUFFER                 */ 00245000
         LA    VADR,RTCTRCB(,@14)                                  0175 00246000
*                           CALL ACCESRTN;/* ACCESS IT               */ 00247000
         BAL   @14,ACCESRTN                                        0176 00248000
*                           IF(RCREG^=ZERO)|((DATAWORD&FOURKBDY)^= 0177 00249000
*                               DATAWORD) THEN/* EITHER THE RTCT   0177 00250000
*                                      COULD NOT BE ACCESSED OR THE     00251000
*                                      POINTER TO THE BUFFER IS BAD  */ 00252000
         LTR   RCREG,RCREG                                         0177 00253000
         BNZ   @RT00177                                            0177 00254000
         L     @14,DATAWORD(,DATAPTR)                              0177 00255000
         LR    @08,@14                                             0177 00256000
         N     @08,FOURKBDY                                        0177 00257000
         CLR   @08,@14                                             0177 00258000
         BE    @RF00177                                            0177 00259000
@RT00177 DS    0H                                                  0178 00260000
*                             BUFFER(1:LENGTH(LOCERR))=LOCERR;/*   0178 00261000
*                                      ERROR MESSAGE                 */ 00262000
         L     @14,ADPLBUF(,ABDPLPTR)                              0178 00263000
         MVC   BUFFER(95,@14),LOCERR                               0178 00264000
*                           ELSE    /* FOUND IN-STORAGE LOGREC BUFFER*/ 00265000
*                             DO;                                  0179 00266000
         B     @RC00177                                            0179 00267000
@RF00177 DS    0H                                                  0180 00268000
*                               RECPTR=DATAWORD;/* SAVE BUFFER     0180 00269000
*                                      ADDRESS                       */ 00270000
         L     RECPTR,DATAWORD(,DATAPTR)                           0180 00271000
*                               CALL CPUID;/* DETERMINE THE VALID CPU   00272000
*                                      IDS                           */ 00273000
         BAL   @14,CPUID                                           0181 00274000
*                               IF RCREG=ZERO THEN/* FOUND CPU IDS OK*/ 00275000
         LTR   RCREG,RCREG                                         0182 00276000
         BNZ   @RF00182                                            0182 00277000
*                                 DO;                              0183 00278000
*                                   CALL DEBLOCK;/* ROUTINE TO     0184 00279000
*                                      DEBLOCK THE BUFFER AND FILL IN   00280000
*                                      THE SORT TABLE                */ 00281000
         BAL   @14,DEBLOCK                                         0184 00282000
*                                   IF SORTINDX^=ZERO THEN/* THERE 0185 00283000
*                                      ARE RECORDS TO BE EDITED      */ 00284000
         L     @14,SORTINDX                                        0185 00285000
         LTR   @14,@14                                             0185 00286000
         BZ    @RF00185                                            0185 00287000
*                                    DO;                           0186 00288000
*                                                                  0186 00289000
*                                     /*******************************/ 00290000
*                                     /*                             */ 00291000
*                                     /* FILL IN COMMUNICATIONS TABLE*/ 00292000
*                                     /* FOR USE BY THE EREP MODULES */ 00293000
*                                     /*                             */ 00294000
*                                     /*******************************/ 00295000
*                                                                  0187 00296000
*                                     PARMP1=''B;/* ZERO OUT FIRST 0187 00297000
*                                      PART OF TABLE                 */ 00298000
         XC    PARMP1(255),PARMP1                                  0187 00299000
*                                     RFY                          0188 00300000
*                                      (PARMPTR) RSTD;/*     @G33DGVS*/ 00301000
*                                     PARMPTR=LENGTH(PARM)-256;/*  0189 00302000
*                                                            @G33DGVS*/ 00303000
         LA    PARMPTR,92                                          0189 00304000
*                                     PARM(PARMPTR:LENGTH(PARM))=PARM(  00305000
*                                         PARMPTR:LENGTH(PARM))&&PARM(  00306000
*                                         PARMPTR:LENGTH(PARM));/* 0190 00307000
*                                                            @G33DGVS*/ 00308000
         LA    @08,PARM-1(PARMPTR)                                 0190 00309000
         LA    @07,348                                             0190 00310000
         SLR   @07,PARMPTR                                         0190 00311000
         EX    @07,@SX02574                                        0190 00312000
*                                     RFY                          0191 00313000
*                                      (PARMPTR) UNRSTD;/*   @G33DGVS*/ 00314000
*                                     RECTYPE=ALLON;/* ALL RECORD  0192 00315000
*                                      TYPES WANTED                  */ 00316000
         MVC   RECTYPE(2),ALLON                                    0192 00317000
*                                     DEBUG19=ON;/* BREAK FOR NEW  0193 00318000
*                                      EDIT TABLE AFTER EACH RECORD  */ 00319000
         OI    DEBUG19,B'00010000'                                 0193 00320000
*                                     SHORTOBR=ON;/* SHORT OBR     0194 00321000
*                                      RECORDS WANTED                */ 00322000
*                                     NOSDR=ON;/* DUMP OF SDR NOT  0195 00323000
*                                      WANTED                        */ 00324000
         OI    SHORTOBR,B'00100001'                                0195 00325000
*                                     PEDIT=ON;/* RECORDS ARE TO BE     00326000
*                                      EDITED                        */ 00327000
         OI    PEDIT,B'00010000'                                   0196 00328000
*                                     TABSIZE=LENGTH(SORTABLE);/*  0197 00329000
*                                      FILL IN SIZE OF THE SORT TABLE*/ 00330000
         MVC   TABSIZE(4),@CF02569                                 0197 00331000
*                                     SORTNUM=SORTINDX;/* NUMBER OF     00332000
*                                      RECORDS FOR THE EDITOR TO   0198 00333000
*                                      FORMAT                        */ 00334000
         ST    @14,SORTNUM                                         0198 00335000
*                                     ADIOHND=ADDR(IFCFMTIO);/* I/O     00336000
*                                      ROUTINE USED BY EREP          */ 00337000
         L     @14,@CV02051                                        0199 00338000
         ST    @14,ADIOHND                                         0199 00339000
*                                     ADIFCMSG=ADDR(IFCFMTIO);/* MSG    00340000
*                                      HANDLER USED BY EREP          */ 00341000
         L     @14,@CV02051                                        0200 00342000
         ST    @14,ADIFCMSG                                        0200 00343000
*                                     SORTADR=ADDR(SORTABLE);/* PTR     00344000
*                                      TO SORT TABLE                 */ 00345000
         LA    @14,SORTABLE                                        0201 00346000
         ST    @14,SORTADR                                         0201 00347000
*                                     ADEDITB=ADDR(EDITLOAD);/* PTR     00348000
*                                      TO TABLE OF LOADS             */ 00349000
         LA    @14,EDITLOAD                                        0202 00350000
         ST    @14,ADEDITB                                         0202 00351000
*                                     PRINTADR=ADDR(EREPBUF);/*    0203 00352000
*                                      OUTPUT BUFFER USED BY THE EREP   00353000
*                                      ROUTINES                      */ 00354000
         LA    @14,EREPBUF                                         0203 00355000
         ST    @14,PRINTADR                                        0203 00356000
*                                     LINECT=PAGEMAX;/* SET NUM LINES   00357000
*                                      FOR PAGE                      */ 00358000
         MVC   LINECT(2),@CH02077                                  0204 00359000
*                                     PDMPLIST=ABDPLPTR;/* SAVE PTR     00360000
*                                      TO IHAABDPL                   */ 00361000
         ST    ABDPLPTR,PDMPLIST                                   0205 00362000
*                                     UNWRPBUF=ADDR(NEWBUF);/* SAVE     00363000
*                                      PTR TO READ BUFFER            */ 00364000
         LA    @14,NEWBUF                                          0206 00365000
         ST    @14,UNWRPBUF                                        0206 00366000
*                                     ENDADR=BUFEND;/* END OF PRINT     00367000
*                                      DUMPS COPY OF THE IN-STORAGE     00368000
*                                      LOGREC BUFFER                 */ 00369000
         MVC   ENDADR(4),BUFEND                                    0207 00370000
*                                     BEGINADR=BUFSTART;/* START OF     00371000
*                                      PRINT DUMPS COPY OF THE     0208 00372000
*                                      IN-STORAGE LOGREC BUFFER      */ 00373000
         MVC   BEGINADR(4),BUFSTART                                0208 00374000
*                                     RFY                          0209 00375000
*                                      (PARMPTR) RSTD;             0209 00376000
*                                     PARMPTR=ADDR(PARM);/* PASS THE    00377000
*                                      COMMUNICATIONS TABLE ADDRESS     00378000
*                                      IN REG 2                      */ 00379000
*                                                                  0210 00380000
         LA    PARMPTR,PARM                                        0210 00381000
*                                     /*******************************/ 00382000
*                                     /*                             */ 00383000
*                                     /* FORMAT ALL VALID ENTRIES IN */ 00384000
*                                     /* THE LOGREC BUFFER. (NOTE    */ 00385000
*                                     /* THAT IFCFMTIO WILL BE CALLED*/ 00386000
*                                     /* BY THE EREP EDITOR TO PUT   */ 00387000
*                                     /* EACH FORMATTED LINE AND     */ 00388000
*                                     /* ERROR MESSAGE INTO THE DUMP */ 00389000
*                                     /* OUTPUT                      */ 00390000
*                                     /*                             */ 00391000
*                                     /*******************************/ 00392000
*                                                                  0211 00393000
*                                     GEN FLOWS(IFCEDSUM)          0211 00394000
*                                         (LINK EP=IFCEDSUM);      0211 00395000
         LINK EP=IFCEDSUM                                               00396000
*                                    END;/* END THERE ARE RECORDS FOR   00397000
*                                      EDIT                          */ 00398000
*                                   ELSE                           0213 00399000
*                                    ;/* NO RECORDS TO BE EDITED     */ 00400000
@RF00185 DS    0H                                                  0214 00401000
*                                 END;/* END FOUND CPU IDS OK        */ 00402000
*                               ELSE                               0215 00403000
*                                 ; /* COULD NOT DETERMINE VALID CPU    00404000
*                                      IDS                           */ 00405000
@RF00182 DS    0H                                                  0216 00406000
*                             END;  /* END FOUND IN STORAGE BUFFER   */ 00407000
*                         END;      /* END FOUND RTCT PTR            */ 00408000
@RC00177 DS    0H                                                  0218 00409000
*                     END;          /* END IS AN MVS SYSTEM          */ 00410000
@RC00171 DS    0H                                                  0219 00411000
*                 END;              /* END FOUND OPERATING SYSTEM  0219 00412000
*                                      FLAGS                         */ 00413000
@RC00166 DS    0H                                                  0220 00414000
*             END;                  /* END A CVT ADDRESS IS PASSED   */ 00415000
@RC00163 DS    0H                                                  0221 00416000
*         END;                      /* END EREP ENHANCEMENT SU IS ON    00417000
*                                                              SYSTEM*/ 00418000
@RC00157 DS    0H                                                  0222 00419000
*TROUBLE:                           /* PLACE TO RETRY AT             */ 00420000
*       CALL PRINT;                 /* BLANK LINE OR ERROR MESSAGE   */ 00421000
@RC00154 DS    0H                                                  0222 00422000
TROUBLE  L     @15,ADPLPRNT(,ABDPLPTR)                             0222 00423000
         BALR  @14,@15                                             0222 00424000
*       GEN(ESTAE 0);               /* DROP THE ESTAE                */ 00425000
         ESTAE 0                                                        00426000
*     END;                          /* END ESTAE RESOLVED FINE       */ 00427000
*   ELSE                                                           0225 00428000
*     ;                             /* ERROR ESTABLISHING THE ESTAE  */ 00429000
@RF00144 DS    0H                                                  0226 00430000
*   RFY                                                            0226 00431000
*    (PARMPTR) UNRSTD;                                             0226 00432000
*   RETURN CODE(0);                                                0227 00433000
