         LR    BUFSIZE,@14                                         0296 00764000
*       MVCL(COPYREG,BUFREG);       /* MOVE INTO INTERNAL BUFFER     */ 00765000
         MVCL  COPYREG,BUFREG                                      0297 00766000
*       RFY                                                        0298 00767000
*        (COPYREG,                                                 0298 00768000
*         COPYSIZE,                                                0298 00769000
*         BUFREG,                                                  0298 00770000
*         BUFSIZE) UNRSTD;                                         0298 00771000
*                                                                  0298 00772000
*       /*************************************************************/ 00773000
*       /*                                                           */ 00774000
*       /* DETERMINE IF THE LOGREC HEADER INFORMATION IS IN REAL     */ 00775000
*       /* ADDRESSES INSTEAD OF VIRTUAL ADDRESSES. THIS HAPPENS WHEN */ 00776000
*       /* THE EMERGENCY RECORDING IS DONE WITHOUT THE VIRTUAL       */ 00777000
*       /* TRANSLATION FEATURE. THE WAY TO DETERMINE THIS IS THE RSTF*/ 00778000
*       /* BIT IN THE RTCT. THE BIT IS ON WHEN THE EMERGENCY         */ 00779000
*       /* RECORDING IS WRITING ITS RECORDS AND IS TRANSLATING THE   */ 00780000
*       /* HEADER TO REAL ADDRESSES.                                 */ 00781000
*       /*                                                           */ 00782000
*       /*************************************************************/ 00783000
*                                                                  0299 00784000
*       REALADR=RECPTR;             /* SAVE VIRTUAL LOGREC BUFFER  0299 00785000
*                                      ADDR TO CALCULATE START AND 0299 00786000
*                                      END OFFSETS FOR THE COPY OF 0299 00787000
*                                      THE BUFFER                    */ 00788000
         LR    REALADR,RECPTR                                      0299 00789000
*       RECPTR=ADDR(COPYBUF);       /* ADDRESS OF THE COPY OF THE  0300 00790000
*                                      BUFFER                        */ 00791000
         LA    RECPTR,COPYBUF                                      0300 00792000
*       VALIDCPU=OFF;               /* INITIALIZE FOUND FIRST RECORD    00793000
*                                      BY CPU ID OFF                 */ 00794000
         NI    VALIDCPU,B'01111111'                                0301 00795000
*       VADR=ADDR(RTCTRFLG);        /* RTCT RECORDING FLAGS          */ 00796000
         L     @14,RTCTPTR                                         0302 00797000
         LA    VADR,RTCTRFLG(,@14)                                 0302 00798000
*       CALL ACCESRTN;              /* ACCESS IT                     */ 00799000
         BAL   @14,ACCESRTN                                        0303 00800000
*       IF RCREG^=ZERO THEN         /* ERROR ACCESSING THE RTCT      */ 00801000
         LTR   RCREG,RCREG                                         0304 00802000
         BZ    @RF00304                                            0304 00803000
*         DO;                                                      0305 00804000
*           BUFFER(1:LENGTH(LOCERR))=LOCERR;/* ERROR MESSAGE         */ 00805000
         L     @14,ADPLBUF(,ABDPLPTR)                              0306 00806000
         MVC   BUFFER(95,@14),LOCERR                               0306 00807000
*           CALL PRINT;             /* PRINT IT                      */ 00808000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0307 00809000
         BALR  @14,@15                                             0307 00810000
*         END;                      /* END ACCESS ERROR              */ 00811000
*       ELSE                        /* FOUND RECORDING FLAGS         */ 00812000
*         DO;                                                      0309 00813000
         B     @RC00304                                            0309 00814000
@RF00304 DS    0H                                                  0310 00815000
*           IF STFBIT=ON THEN       /* THE BUFFER POINTERS ARE REAL     00816000
*                                      ADDRESSES NOT VIRTUAL ADDRS   */ 00817000
         TM    STFBIT(DATAPTR),B'00100000'                         0310 00818000
         BNO   @RF00310                                            0310 00819000
*             REALADR=RCBBUFB&FOURKBDY;/* MASK OUT LOW ORDER BITS TO    00820000
*                                      DETERMINE THE BUFFERS       0311 00821000
*                                      BEGINNING REAL ADDR TO BE USED   00822000
*                                      FOR CALCULATING START AND END    00823000
*                                      ADDRESS FOR THE INTERNAL COPY    00824000
*                                      OF THE BUFFER                 */ 00825000
         L     REALADR,RCBBUFB(,RECPTR)                            0311 00826000
         N     REALADR,FOURKBDY                                    0311 00827000
*           ELSE                                                   0312 00828000
*             ;                     /* BUFFERS POINTERS ARE VIRTUAL     00829000
*                                      AND THE VIRTUAL ADDRESS HAS 0312 00830000
*                                      ALREADY BEEN SAVED IN REALADR */ 00831000
*                                                                  0312 00832000
@RF00310 DS    0H                                                  0313 00833000
*           /*********************************************************/ 00834000
*           /*                                                       */ 00835000
*           /* DETERMINE THE FIRST AND LAST POINTERS FOR THE COPIED  */ 00836000
*           /* BUFFER THAT THE IN-STORAGE BUFFER HAS BEEN COPIED     */ 00837000
*           /* INTO, SINCE THAT IS THE BUFFER BEING REFERENCED, NOT  */ 00838000
*           /* THE ACTUAL BUFFER                                     */ 00839000
*           /*                                                       */ 00840000
*           /*********************************************************/ 00841000
*                                                                  0313 00842000
*           BUFSTART=RCBBUFB-REALADR;/* OFFSET OF BEGINNING ADDR OF     00843000
*                                      REAL BUFFER                   */ 00844000
         L     @14,RCBBUFB(,RECPTR)                                0313 00845000
         SLR   @14,REALADR                                         0313 00846000
         ST    @14,BUFSTART                                        0313 00847000
*           BUFSTART=BUFSTART+RECPTR;/* BEGINNING ADDR IN COPIED   0314 00848000
*                                      BUFFER                        */ 00849000
         ALR   @14,RECPTR                                          0314 00850000
         ST    @14,BUFSTART                                        0314 00851000
*           BUFEND=RCBBUFE-REALADR; /* OFFSET OF END ADDR OF REAL  0315 00852000
*                                      BUFFER                        */ 00853000
         L     @04,RCBBUFE(,RECPTR)                                0315 00854000
         SLR   @04,REALADR                                         0315 00855000
         ST    @04,BUFEND                                          0315 00856000
*           BUFEND=BUFEND+RECPTR;   /* END ADDRESS OF COPIED BUFFER  */ 00857000
         ALR   @04,RECPTR                                          0316 00858000
         ST    @04,BUFEND                                          0316 00859000
*           FRSTFREE=RCBFREE-REALADR;/* OFFSET OF FIRST FREE AREA IN    00860000
*                                      REAL BUF                      */ 00861000
         L     FRSTFREE,RCBFREE(,RECPTR)                           0317 00862000
         SLR   FRSTFREE,REALADR                                    0317 00863000
*           FRSTFREE=FRSTFREE+RECPTR;/* PTR TO FIRST FREE AREA IN  0318 00864000
*                                      COPIED BUF                    */ 00865000
         ALR   FRSTFREE,RECPTR                                     0318 00866000
*           IF((RECPTR+FOURK)^=BUFEND)|/* BUFFER IS NOT 4K         0319 00867000
*                                      LONG(IMPROPER LENGTH)         */ 00868000
*               (FRSTFREE<BUFSTART)|/* BAD FIRST FREE POINTER        */ 00869000
*               (FRSTFREE>BUFEND) THEN/* BAD FIRST FREE ADDRESS      */ 00870000
         LR    @02,RECPTR                                          0319 00871000
         AH    @02,@CH01275                                        0319 00872000
         CR    @02,@04                                             0319 00873000
         BNE   @RT00319                                            0319 00874000
         CR    FRSTFREE,@14                                        0319 00875000
         BL    @RT00319                                            0319 00876000
         CR    FRSTFREE,@04                                        0319 00877000
         BNH   @RF00319                                            0319 00878000
@RT00319 DS    0H                                                  0320 00879000
*             DO;                                                  0320 00880000
*               BUFFER(1:LENGTH(BADBUF))=BADBUF;/* ERROR MESSAGE     */ 00881000
         L     @14,ADPLBUF(,ABDPLPTR)                              0321 00882000
         MVC   BUFFER(80,@14),BADBUF                               0321 00883000
*               CALL PRINT;         /* PRINT IT                      */ 00884000
         L     @15,ADPLPRNT(,ABDPLPTR)                             0322 00885000
         BALR  @14,@15                                             0322 00886000
*             END;                  /* END BAD BUFFER ENCOUNTERED    */ 00887000
*           ELSE                    /* FOUND A VALID BUFFER          */ 00888000
*             DO;                                                  0324 00889000
