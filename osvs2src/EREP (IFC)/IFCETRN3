.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             02300002
         MEXIT                                                          02350002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               02400002
         MEXIT                                                          02450002
.LOOP    ANOP                                                           02500002
&OP      SETA  &OP+1                                                    02550002
