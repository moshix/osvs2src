&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          17160002
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            17180002
.SUBF    ANOP                                                           17200002
.BACKUP  ANOP                                                           17220002
