*** THIS MODULE SUPPORTS 3850 DEVICE --- SUPPORT CODE=Y30LPDG ***       00020002
         MACRO                                                          00040002
         SYSRELN  &LOGITEM,&SKIP$OR,&BYTES                              00040200
.*                            THIS MACRO FACILITATES GENERATION OF THE  00040400
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    00040600
.*                                 CONVERSIONS.                         00040800
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        00041000
.*                                 CONVERTED                            00041200
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    00041400
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   00041600
.*                                 BACK UP FROM COLUMN FOR START OF     00041800
.*                                 PRINTING. THE LETTER B PRECEEDS THE  00042000
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  00042200
.*                                 DESIRED. OMISSION OF THIS OPERAND    00042400
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  00042600
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  00042800
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 00043000
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    00043200
.*                                 IMPLICITLY B).                       00043400
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   00043600
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     00043800
.*                                 WITH AN L,R, OR B TO STIPULATE       00044000
.*                                 PRINTING OF THE LEFT HALF, RIGHT     00044200
.*                                 HALF, OR BOTH HALVES OF THE BYTE     00044400
.*                                 RESPECTIVELY. OMISSION OF THIS       00044600
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  00044800
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 00045000
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 00045200
.*                                 SARY. THIS MACRO MUST BE THE FIRST   00045400
.*                                 MACRO INSTRUCTION ISSUED FOR THE     00045600
.*                                 SUBJECT LINE(S).                     00045800
         GBLC &ITEM(3000),&DSECT                                        00046000
         GBLA &ITEMNBR,&BITS(3000),&SHIFT(3000),&NRENTS                 00046200
         LCLB  &B                                                       00046400
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  00046600
         LCLC  &S,&LETTER                                               00046800
.LOOP    ANOP                                                           00047000
&NDX     SETA  &NDX+1                                                   00047200
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      00047400
         AIF   (&NDX LT &ITEMNBR).LOOP                                  00047600
         MNOTE *,'LOG ITEM NOT DEFINED'                                 00047800
         MEXIT                                                          00048000
.FOUND   ANOP                                                           00048200
&ISHIFT  SETA  &SHIFT(&NDX)                                             00048400
&IBITS   SETA  &BITS(&NDX)                                              00048600
&CODE    SETA  1                                                        00048800
&K       SETA  K'&BYTES                                                 00049000
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            00049200
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   00049400
         MEXIT                                                          00049600
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 00049800
&LETTER  SETC  '&BYTES'(&K,1)                                           00050000
&S       SETC  '&BYTES'(1,&K-1)                                         00050200
&BYTESP  SETA  &S                                                       00050400
         AIF   ('&LETTER' EQ 'B').BOTH                                  00050600
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       00050800
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            00051000
         MEXIT                                                          00051200
.USEDEFN ANOP                                                           00051400
&BYTESP  SETA  &IBITS/8                                                 00051600
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       00051800
&CODE    SETA  3                                                        00052000
&T       SETA  &BYTESP*2-1                                              00052200
&S       SETC  '&T'                                                     00052400
.OK2     AIF   (&ISHIFT EQ 0).L                                         00052600
&CODE    SETA  2                                                        00052800
.L       ANOP                                                           00053000
&B       SETB  1                                                        00053200
         AIF   ('&SKIP$OR' EQ '').BACKUP                                00053400
&K       SETA  K'&SKIP$OR                                               00053600
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 00053800
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   00054000
.BACKUP  ANOP                                                           00054200
&NRENTS  SETA  &NRENTS+1                                                00054400
         DC    AL.12(&LOGITEM-&DSECT),XL.4'7'                           00054600
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              00054800
.OUT     ANOP                                                           00055000
         MEND                                                           00055200
         MACRO                                                          00055400
         BIN   ,                                                        00060000
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   00070000
.*                                      RECORD DSECT                    00080000
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      00090000
.*                                      TO SKIP ON MULTIPLE             00100000
.*                                      PRINT OR NBR OF SPACES          00110000
.*                                      TO BACK UP FROM COLUMN FOR      00120000
.*                                      START OF PRINTING. THE LETTER   00130000
.*                                      B PRECEEDS THE NUMBER (B5) IF   00140000
.*                                      THE LATTER IS DESIRED. OMISSION 00150000
.*                                      OF THIS OPERAND FORCES OPTION 2 00160000
.*                                      WITH BACKUP SPACING EQUAL TO    00170000
.*                                 THE NUMBER OF BITS IN THE ITEM       00180000
.*                                 DEFINITION (VIA MACRO 'DSGEN').      00190000
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     00200000
.*                                 TOTAL NUMBER OF BITS TO PRINT.       00210000
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             00380002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  00400002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  00420002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              00440002
         GBLA  &NRENTS,&SHIFT(3000)                                     00460002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           00480002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       00500002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         00520002
         LCLB  &B,&SUB                                                  00540000
         LCLA  &IBITS,&ISHIFT,&K                                        00550000
         LCLA  &TEMP,&T,&OP                                             00560000
         LCLC  &S,&ARG                                                  00570000
          ACTR  500000                                                  00580000
         AIF   (&DIAGTST EQ 0).NOMSG                                    00590000
         AIF   (&INITIAL EQ 0).NOMSG                                    00600000
         AIF   (&DIAGLIN EQ 1).NOMSG                                    00610000
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  00620000
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     00630000
*        LINE                                                           00640000
         LINE                                                           00650000
.NOMSG   ANOP                                                           00660000
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  00670000
&SUB     SETB  1                                                        00680000
.LOOP    ANOP                                                           00690000
&OP      SETA  &OP+1                                                    00700000
&ARG     SETC  '&SYSLIST(&OP,1)'                                        00710000
         ITEMFIND &ARG                                                  00720000
         AIF   (&ITEMXPT EQ 0).MEXIT                                    00730000
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         00740000
&IBITS   SETA  &BITS(&ITEMXPT)                                          00750000
         AIF   (&SUB EQ 1).SUB00                                        00760000
         AIF   ('&SYSLIST(3)' EQ '').SETT                               00770000
&IBITS   SETA  &SYSLIST(3)                                              00780000
         AGO   .SETT                                                    00790000
.SUB00   AIF   ('&SYSLIST(&OP,3)' EQ '').SETT                           00800000
&IBITS   SETA  &SYSLIST(&OP,3)                                          00810000
.SETT    ANOP                                                           00820000
&T       SETA  &IBITS-1                                                 00830000
&B       SETB  0                                                        00840000
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      00850000
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  00860000
.MEXIT   MEXIT                                                          00870000
.SUB0    AIF   (&SUB EQ 1).SUBA                                         00880000
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             00890000
&K       SETA  K'&SYSLIST(2)                                            00900000
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              00910000
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                00920000
         AGO   .SUBB                                                    00930000
.SUBA    ANOP                                                           00940000
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         00950000
&K       SETA  K'&SYSLIST(&OP,2)                                        00960000
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          00970000
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            00980000
.SUBB    ANOP                                                           00990000
&T       SETA  &S                                                       01000000
.BACKUP  ANOP                                                           01010000
&NRENTS  SETA  &NRENTS+1                                                01020000
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            01030000
&TEMP    SETA  &ISHIFT                                                  01040000
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                01050000
         AGO   .OUT                                                     01060000
         AIF   (&SUB EQ 1).BIN2                                         01070000
         AIF   ('&SYSLIST(2)' NE '').BIN3                               01080000
         AGO   .BIN2A                                                   01090000
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           01100000
.BIN2A   ANOP                                                           01110000
&B       SETB  1                                                        01120000
.BIN3    ANOP                                                           01130000
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    01140000
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             01150000
.OUT     ANOP                                                           01160000
&ITEMXPT     SETA  0                                                    01170000
         AIF   (&SUB EQ 0).OUTA                                         01180000
         AIF   (&OP LT N'&SYSLIST).LOOP                                 01190000
.OUTA    ANOP                                                           01200000
         MEND                                                           01210000
         MACRO                                                          01220000
&NAME    DSGEN ,                                                        01230000
.*                            THIS MACRO GENERATES ONE DSECT WORD       01240000
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 01250000
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  01260000
.*                                 FIRST IS THE NAME OF THE LOG ITEM    01270000
.*                                 WHILE THE SECOND IS THE # OF BITS TO 01280000
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   01290000
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 01300000
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      01310000
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  01320000
.*                                 THE LOG ITEMS MAY THEN BE USED IN    01330000
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   01340000
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             02160002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  02180002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  02200002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              02220002
         GBLA  &NRENTS,&SHIFT(3000)                                     02240002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           02260002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       02280002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         02300002
         LCLB  &ONLY1,&CTIND                                            02320000
         LCLA  &START1,&START2,&END1,&END2                              02330000
         LCLC  &SYMBOL,&EQUSYM                                          02340000
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 02350000
         ACTR  100000                                                   02360000
         AIF   (&ITEMITR EQ 0).START                                    02370000
 MNOTE   20,'DSGEN MACRO OCCURRS AFTER ITEMSORT MACRO HAS BEEN ISSUED.' 02380000
         MEXIT                                                          02390000
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            02400000
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     02410000
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   02420000
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        02430000
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            02440000
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             02450000
         AGO   .SET2A                   BRANCH AROUND                   02460000
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        02470000
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              02480000
         MEXIT                                                          02490000
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X02500000
               ).SET2                   TEST FOR NO SECOND              02510000
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   02520000
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            02530000
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              02540000
         AGO   .LOOP                                                    02550000
.SET2    ANOP                                                           02560000
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 02570000
         AGO   .LOOP                                                    02580000
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             02590000
         MEXIT                                                          02600000
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               02610000
         MEXIT                                                          02620000
.LOOP    ANOP                                                           02630000
&OP      SETA  &OP+1                                                    02640000
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           02650000
&ITEMNBR SETA  &ITEMNBR+1                                               02660000
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            02670000
         MNOTE '**NULL BIT COUNT FIELD**'                               02680000
         MEXIT                                                          02690000
.OK1     ANOP                                                           02700000
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        02710000
         AIF   ('&SYMBOL' NE '').OK                                     02720000
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         02730000
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         02740000
         AGO   .START                   GET NEXT                        02750000
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        02760000
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          02770000
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        02780000
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         02790000
         AGO   .NO3                     BRANCH AROUND                   02800000
.OK2     ANOP                                                           02810000
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              02820000
.NO3     ANOP                                                           02830000
&SYMBOL  EQU   *+&BYTE                                                  02840000
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             02850000
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    02860000
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    02870000
.CNT     ANOP                                                           02880000
&CTIND   SETB  1                        SET COUNT INDICATOR             02890000
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   02900000
&BYTE    SETA  &COUNT/8                                                 02910000
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        02920000
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        02930000
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             02940000
&END1    SETA  &END1+1                  INCREMENT COUNT                 02950000
&OP      SETA  &OP-1                    DECREMENT INCREMENT             02960000
         AGO   .LOOP                    GENERATE NEXT                   02970000
.TSTST   ANOP                                                           02980000
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               02990000
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        03000000
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      03010000
.NSET1   ANOP                                                           03020000
&OP      SETA  &OP-1                    DECREMENT INDEX                 03030000
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           03040000
         AGO   .LOOP                    GO DO NEXT                      03050000
.LPDONE  ANOP                                                           03060000
&ONLY1   SETB  0                        CLEAR INDICATOR                 03070000
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      03080000
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 03090000
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               03100000
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             03110000
&NAME    DS    &COUNT.C                                                 03120000
.END     MEXIT                                                          03130000
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          03140000
         MEXIT                                                          03150000
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   03160000
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   03170000
         MEXIT                                                          03180000
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         03190000
*****               THE FOLLOWING STATEMENT                *****        03200000
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        03210000
.EQU2    ANOP                                                           03220000
&EQUCNT  SETA  0                        CLEAR COUNTER                   03230000
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 03240000
.EQULOP  ANOP                                                           03250000
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 03260000
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     03270000
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                03280000
         AGO   .EQULOP                  NO-GET NEXT                     03290000
.EQUOUT  ANOP                                                           03300000
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  03310000
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              03320000
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 03330000
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             03340000
         AGO   .START                   RETURN FOR NEXT                 03350000
         MEND                                                           03360000
         MACRO                                                          03370000
         CONVT ,                                                        03380000
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             04460002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  04480002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  04500002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              04520002
         GBLA  &NRENTS,&SHIFT(3000)                                     04540002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           04560002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       04580002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         04600002
         LCLB  &SUB                                                     04620000
         LCLA  &OP                                                      04630000
         LCLA  &OFFSET,&OUT,&IN,&IND,&IBITS,&ISHIFT                     04640000
.*                                                                      04650000
.*       CONVERT BINARY TO DECIMAL                                      04660000
.*             &NAME IS THE REFERENCE INTO THE DSECT RECORD             04670000
.*             &OUT    IS THE NUMBER OF CHARS TO BE PUT ONTO PRINT LINE 04680000
.*                                      DEFAULT IS FOUR MAXIMUM IS 8    04690000
.*             &IN       IS THE NUMBER OF BYTES AS INPUT                04700000
.*                                      ERROR DEFAULT IS 2 MAXIMUM IS 4 04710000
.*                                                                      04720000
         AIF   (&DIAGTST EQ 0).NOMSG                                    04730000
         AIF   (&INITIAL EQ 0).NOMSG                                    04740000
         AIF   (&DIAGLIN EQ 1).NOMSG                                    04750000
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  04760000
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     04770000
*        LINE                                                           04780000
         LINE                                                           04790000
.NOMSG   ANOP                                                           04800000
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LLOP                  04810000
&SUB     SETB  1                                                        04820000
.LLOP    ANOP                                                           04830000
&OP      SETA  &OP+1                                                    04840000
&OUT     SETA  4                        .BR NOT NULL ELSE SET TO 4      04850000
         AIF   (&SUB EQ 1).SUBA                                         04860000
         AIF   ('&SYSLIST(3)' EQ '').B  TEST FOR NULL INPUT             04870000
&OUT     SETA  &SYSLIST(3)             SET &OUT FROM INPUT              04880000
         AGO   .A                                                       04890000
.SUBA    AIF   ('&SYSLIST(&OP,3)' EQ '').B                              04900000
&OUT     SETA  &SYSLIST(&OP,3)          SET &OUT FROM INPUT             04910000
.A       AIF   (&OUT GT 0 AND &OUT LT 9).B   TEST RANGE                 04920000
.AERR    MNOTE '*** BYTES OUT INCORRECTLY SPECIFIED DEFAULT USED ***'   04930000
&OUT     SETA  4                                                        04940000
.B       AIF   (&SUB EQ 1).SUBB                                         04950000
         AIF   ('&SYSLIST(2)' EQ '').LOOP    BRANCH IF NULL             04960000
&IN      SETA  &SYSLIST(2)              .SET &IN FROM INPUT             04970000
         AGO   .BB                                                      04980000
.SUBB    AIF   ('&SYSLIST(&OP,2)' EQ '').LOOP                           04990000
&IN      SETA  &SYSLIST(&OP,2)          .SET &IN FROM INPUT             05000000
.BB      AIF   (&IN GT 0 AND &IN LT 5).D     TEST RANGE                 05010000
.BERR    MNOTE '*** BYTES IN INCORRECTLY SPECIFIED DEFAULT USED ***'    05020000
&IN      SETA  2                                                        05030000
         AGO   .D                                                       05040000
.LOOP    ANOP                           .LOOP TO FIND &NAME IN DSECT    05050000
&IND     SETA  &IND+1                   .SET UP LOOP COUNTER            05060000
         AIF   ('&ITEM(&IND)' EQ '&SYSLIST(&OP,1)').FOUND               05070000
         AIF   (&IND LT &ITEMNBR).LOOP  .TEST FOR END OF DSECT          05080000
         MNOTE '*** LOG ITEM NOT DEFINED ***'                           05090000
         MEXIT                                                          05100000
.FOUND   ANOP                                                           05110000
&IBITS   SETA  &BITS(&IND)              .SET UP NUMBER OF BITS SPEC     05120000
         AIF   (&IBITS-((&IBITS/8)*8) EQ 0).C  .TEST FOR MULTIPLE OF 8  05130000
         MNOTE '*** DATA NOT DEFINED A MULTIPLE OF 8 BITS ***'          05140000
         MEXIT                                                          05150000
.C       ANOP                                                           05160000
&IN      SETA  &IBITS/8                 .COMPUTE NUMBER OF BYTES        05170000
         AIF   (&IN LT 1 OR &IN GT 4).BERR  .TEST RANGE B IF IN ERROR   05180000
&ISHIFT  SETA  &SHIFT(&IND)             .SET UP FOR TEST OF BYTE BNDR   05190000
         AIF   (&ISHIFT EQ 0).D         .TEST FOR BYTE BNDRY            05200000
         MNOTE '*** DATA NOT ALIGNED ON A BYTE BOUNDARY ***'            05210000
         MEXIT                                                          05220000
.*                                                                      05230000
.D       ANOP                                                           05240000
&NRENTS  SETA  &NRENTS+1                                                05250000
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),AL.4(9)                    05260000
         DC    X'&IN',X'&OUT'                                           05270000
&IND     SETA  0                                                        05280000
         AIF   (&SUB EQ 0).OUTA                                         05290000
         AIF   (&OP LT N'&SYSLIST).LLOP                                 05300000
.OUTA    ANOP                                                           05310000
         MEND                                                           05320000
         MACRO                                                          05330000
         ITEMFIND &ARG,&ITFDIAG=                                        05340000
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             06080002
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  06100002
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  06120002
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              06140002
         GBLA  &NRENTS,&SHIFT(3000)                                     06160002
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           06180002
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       06200002
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         06220002
         LCLA  &A,&B,&X,&I                                              06240000
