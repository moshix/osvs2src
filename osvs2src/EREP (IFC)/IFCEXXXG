         DC    BL.8'&PARAM'                                             14460000
         DC    AL2(&ROUTINE-&CSECT)                                     14470000
         DC    AL2(&COUNTR)                                             14480000
&ADD     SETA  1                                                        14490000
         AIF   ('&SPACE' EQ '').NORM                                    14500000
&ADD     SETA  &SPACE                                                   14510000
.NORM    ANOP                                                           14520000
&COUNTR  SETA  &COUNTR+&ADD                                             14530000
         MEND                                                           14540000
         MACRO                                                          14550000
         HEX   ,                                                        14560000
.*                            THIS MACRO FACILITATES GENERATION OF THE  14570000
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    14580000
.*                                 CONVERSIONS.                         14590000
.*                            &SYSLIST(&OP,1) - NAME OF DATA ITEM TO BE 14600000
.*                                 CONVERTED                            14610000
.*                            &SYSLIST(&OP,2) - # OF SPACES TO SKIP ON  14620000
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   14630000
.*                                 BACK UP FROM COLUMN FOR START OF     14640000
.*                                 PRINTING. THE LETTER B PRECEEDS THE  14650000
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  14660000
.*                                 DESIRED. OMISSION OF THIS OPERAND    14670000
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  14680000
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  14690000
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 14700000
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    14710000
.*                                 IMPLICITLY B).                       14720000
.*                            &SYSLIST(&OP,3) - NUMBER OF BYTES TO      14730000
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     14740000
.*                                 WITH AN L,R, OR B TO STIPULATE       14750000
.*                                 PRINTING OF THE LEFT HALF, RIGHT     14760000
.*                                 HALF, OR BOTH HALVES OF THE BYTE     14770000
.*                                 RESPECTIVELY. OMISSION OF THIS       14780000
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  14790000
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 14800000
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 14810000
.*                                 SARY. THIS MACRO MUST BE THE FIRST   14820000
.*                                 MACRO INSTRUCTION ISSUED FOR THE     14830000
.*                                 SUBJECT LINE(S).                     14840000
         GBLA  &BITS(3000),&DB,&DT,&INITIAL                             14850000
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  14860000
         GBLA  &ITEMNDX(3000),&ITEMXPT                                  14870000
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              14880000
         GBLA  &NRENTS,&SHIFT(3000)                                     14890000
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           14900000
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(3000)                       14910000
         GBLC  &LABELS(3000),&LBL(100),&NRENTSX                         14920000
         LCLB  &B,&SUB                                                  14930000
         LCLA  &IBITS,&ISHIFT,&K,&T,&CODE,&BYTESP,&OP                   14940000
         LCLC  &S,&LETTER,&ARG                                          14950000
       ACTR  500000                                                     14960000
         AIF   (&DIAGTST EQ 0).NOMSG                                    14970000
         AIF   (&INITIAL EQ 0).NOMSG                                    14980000
         AIF   (&DIAGLIN EQ 1).NOMSG                                    14990000
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  15000000
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     15010000
*        LINE                                                           15020000
         LINE                                                           15030000
.NOMSG   ANOP                                                           15040000
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  15840002
&SUB     SETB  1                                                        15860002
.LOOP    ANOP                                                           15880002
&OP      SETA  &OP+1                                                    15900002
&ARG     SETC  '&SYSLIST(&OP,1)'                                        15920002
         ITEMFIND  &ARG                                                 15940002
         AIF   (&ITEMXPT EQ 0).MEXIT                                    15960002
.FOUND   ANOP                                                           15980002
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         16000002
&IBITS   SETA  &BITS(&ITEMXPT)                                          16020002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).ALIGN                     16040002
.ERR     MNOTE '**DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY**'          16060002
.MEXIT   MEXIT                                                          16080002
.ALIGN   ANOP                                                           16100002
&CODE    SETA  1                                                        16120002
         AIF   (&SUB EQ 1).SUBA                                         16140002
&K       SETA  K'&SYSLIST(3)                                            16160002
         AGO   .SUBB                                                    16180002
.SUBA    ANOP                                                           16200002
&K       SETA  K'&SYSLIST(&OP,3)                                        16220002
.SUBB    ANOP                                                           16240002
         AIF   (&K EQ 1).ERR3                                           16260002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            16280002
         MNOTE '**BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL**' 16300002
         MEXIT                                                          16320002
.OK      AIF   (&SUB EQ 1).SUBC                                         16340002
         AIF   ('&SYSLIST(3)' EQ '').USEDEFN                            16360002
&LETTER  SETC  '&SYSLIST(3)'(&K,1)                                      16380002
&S       SETC  '&SYSLIST(3)'(1,&K-1)                                    16400002
         AGO   .SUBD                                                    16420002
.SUBC    ANOP                                                           16440002
         AIF   ('&SYSLIST(&OP,3)' EQ '').USEDEFN                        16460002
&LETTER  SETC  '&SYSLIST(&OP,3)'(&K,1)                                  16480002
&S       SETC  '&SYSLIST(&OP,3)'(1,&K-1)                                16500002
.SUBD    ANOP                                                           16520002
&BYTESP  SETA  &S                                                       16540002
         AIF   ('&LETTER' EQ 'B').BOTH                                  16560002
         AIF   ('&LETTER' EQ 'L').LEFT                                  16580002
         AIF   ('&LETTER' EQ 'R').RIGHT                                 16600002
.ERR3    MNOTE '**PARAMETER 3 INCOMPLETE-DSGEN DEFINITION USED**'       16620002
         AGO   .USEDEFN                                                 16640002
.RIGHT   ANOP                                                           16660002
&CODE    SETA  2                                                        16680002
         AGO   .L                                                       16700002
.LEFT    ANOP                                                           16720002
&CODE    SETA  1                                                        16740002
         AGO   .L                                                       16760002
.USEDEFN ANOP                                                           16780002
&BYTESP  SETA  &IBITS/8                                                 16800002
