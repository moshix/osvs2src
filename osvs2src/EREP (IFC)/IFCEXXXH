SETCPUNO STM   R5,R8,XSAVE                                              56100002
         SR    R5,R5               CLEAR R5                             56150002
         IC    R5,L0(R4)           PICK UP MODEL NO                     56200002
         SLL   R5,L8               IN TO                                56250002
         IC    R5,L1(R4)           R5                                   56300002
         LTR   R5,R5                    TEST FOR ZERO MODEL      S21021 56350002
         LR    R5,R3                    POINT TO DATA FIELD      S21021 56400002
         BNZ   SETSER                   BRANCH IF MODEL NOT 0    S21021 56450002
         LA    R6,L46                   BACK UP TO SAY 'NONE'    S21021 56500002
         SR    R5,R6                    BACKUP BUFFER            S21021 56550002
         MVC   L0(L29,R5),UNIVERS       SAY NONE, MODEL UNIVERSALS21021 56600002
         B     RETURN                   RETURN  CONTROL                 56650002
