         B     RETURN                                                   60000002
CCTYPE   STM   R5,R8,XSAVE       SAVE REGISTERS                @ZA35362 60001000
         L     R8,XLABELP        GET ADDR OF LABEL             @ZA35362 60002000
         CLI   0(R4),HEX01        IS IT GENERAL                @ZA35362 60003000
         BNE   CCTYPE0            NO,CONTINUE CHECK            @ZA35362 60004000
         MVC   L0(L12,R8),GEN     SET UP LABEL                 @ZA35362 60005000
         B     RETURN             RETURN                       @ZA35362 60006000
CCTYPE0  CLI   0(R4),HEX02        IS IT MICROFILM              @ZA35362 60007000
         BNE   CCTYPE1            NO,CONTINUE CHECK            @ZA35362 60008000
         MVC   L0(L12,R8),MICRFILM  SET UP LABEL               @ZA35362 60009000
         B     RETURN             RETURN                       @ZA35362 60010000
CCTYPE1  CLI   0(R4),HEX20        IS IT MICR                   @ZA35362 60011000
         BNE   CCTYPE2            NO,CONTINUE CHECK            @ZA35362 60012000
         MVC   L0(L12,R8),MICR    SET UP LABEL                 @ZA35362 60013000
         B     RETURN             RETURN                       @ZA35362 60014000
