@RF00534 DS    0H                                                  0537 01024000
*     IF WORKTYPE = HEX30 THEN                                     0537 01025000
         CLI   WORKTYPE,X'30'                                      0537 01026000
         BNE   @RF00537                                            0537 01027000
*       CALL OBRFILT;                                       /*@Z427VLS* 01028000
         BAL   @14,OBRFILT                                         0538 01029000
*     ELSE                                                         0539 01030000
*       DO;                                                 /*@Z427VLS* 01031000
         B     @RC00537                                            0539 01032000
@RF00537 DS    0H                                                  0540 01033000
*         IF WORKTYPE = HEX90 THEN                                 0540 01034000
         CLI   WORKTYPE,X'90'                                      0540 01035000
