@ER00007 LM    @14,@12,@SA00007                                    0569 01108000
         BR    @14                                                 0569 01109000
* SIFTSORT: PROC;                                           /*@Z427VLS* 01110000
SIFTSORT STM   @14,@12,@SA00008                                    0570 01111000
* /* SORT THAT ONLY EXAMINES AN ENTRY ONCE IF DATA IS IN ORDER*/   0571 01112000
* IF SORTNUM > 1 & DEBUG23 = OFF THEN /*IF AT LEAST TWO ENTRIES AND*/   01113000
         L     @14,SORTNUM(,R2)                                    0571 01114000
         C     @14,@CF00055                                        0571 01115000
         BNH   @RF00571                                            0571 01116000
         TM    DEBUG23(R2),B'00000001'                             0571 01117000
         BNZ   @RF00571                                            0571 01118000
*   DO;  /*DEBUG INPUT DATA ORDER OPTION IS ON THEN           @Z427VLS* 01119000
*  SRTCONT = ((PASSNUM -1 )*11) + SORTADR;                  /*@D35UGGB* 01120000
         L     @14,PASSNUM                                         0573 01121000
         BCTR  @14,0                                               0573 01122000
         MH    @14,@CH00033                                        0573 01123000
         AL    @14,SORTADR(,R2)                                    0573 01124000
         ST    @14,SRTCONT                                         0573 01125000
* /* THIS SORT IS DONE IN BAL IN ORDER TO GET MIN PROCESSING TIME    */ 01126000
* /* SOME 35 INSTRUCTIONS WERE CUT FROM THE BEST THAT PLS WOULD DO   */ 01127000
* /* THIS CUT THE RUN TIME DOWN BY APP. 50 PER CENT.          @Z427VLS* 01128000
*       GENERATE REFS(SORTKEY1,SORTNEW);                           0574 01129000
           LA    R15,11        INCREMENT VALUE                 @D3506GB 01130000
          L      R3,SORTADR(R2)      LOAD THE START OF THE TABLE        01131000
          L      R4,SRTCONT          LOAD THE MAX LIMIT                 01132000
          LR     R1,R3               SET UP THE STOP LIMIT ON BACK UP   01133000
          B      DOIT                GO AROUND THIS CODE FIRST TIME     01134000
LOOPBCK1  ALR    R3,R15              MOVE THE POINTER TO NEXT ENTRY     01135000
          CR     R3,R4               ARE WE POINTING TO LAST ENTRY      01136000
          BNL    DONE                FINISHED WITH THE SORT SO EXIT     01137000
DOIT       CLC   SORTNEW(6,R3),SORTNEW+11(R3)                  @D3506GB 01138000
          BNH    LOOPBCK1            DONT NEED TO SORT ITEM             01139000
           XC    SORTNEW(11,R3),SORTNEW+11(R3)                 @D3506GB 01140000
           XC    SORTNEW+11(11,R3),SORTNEW(R3)                 @D3506GB 01141000
           XC    SORTNEW(11,R3),SORTNEW+11(R3)                 @D3506GB 01142000
          LR     R6,R3               SET NEW POINTER(BACKWARD)          01143000
LOOPBCK2  SR     R6,R15              POINT TO LAST ENTRY                01144000
          CR     R6,R1               DONT BACK OUT OF THE TABLE         01145000
          BL     LOOPBCK1            START WITH NEXT ENTRY              01146000
           CLC   SORTNEW(6,R6),SORTNEW+11(R6)                  @D3506GB 01147000
          BNH    LOOPBCK1            NO FUTHER SHUFFEL THIS SAMPLE      01148000
