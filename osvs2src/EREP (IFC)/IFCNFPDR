*    CVD(R4,DBLWORD);                                              1456 06183000
         CVD   R4,DBLWORD                                          1456 06184000
*    GEN(UNPK WORK(8),DBLWORD(8));                                 1457 06185000
         UNPK WORK(8),DBLWORD(8)                                        06186000
*    WORK(8)=WORK(8)|'F0'X;                                        1458 06187000
         OI    WORK+7,X'F0'                                        1458 06188000
*    SWITCH1=OFF;                                                  1459 06189000
         NI    SWITCH1,B'11011111'                                 1459 06190000
*    DO J=1 TO 7 BY 1 WHILE(SWITCH1=OFF);                          1460 06191000
         LA    @15,1                                               1460 06192000
         STH   @15,J                                               1460 06193000
@DL01460 TM    SWITCH1,B'00100000'                                 1460 06194000
         BNZ   @DC01460                                            1460 06195000
*      IF WORK(J)=FOXZERO THEN                                     1461 06196000
         LA    @14,WORK-1(@15)                                     1461 06197000
         CLC   0(1,@14),FOXZERO                                    1461 06198000
         BNE   @RF01461                                            1461 06199000
*        WORK(J)=BLANK;                                            1462 06200000
         LA    @14,WORK-1(@15)                                     1462 06201000
