         SH    @15,J                                               1465 06216000
         L     @14,@PC00046                                        1465 06217000
         C     @15,BNDECNUM(,@14)                                  1465 06218000
         BNH   @RF01465                                            1465 06219000
*      DO;                                                         1466 06220000
*        GEN(MVI  WORKOVER,X'F9');                                 1467 06221000
         MVI  WORKOVER,X'F9'                                            06222000
*        GEN(MVC  WORKOVER+1(7),WORKOVER);                         1468 06223000
         MVC  WORKOVER+1(7),WORKOVER                                    06224000
*      END;                                                        1469 06225000
*    RESPECIFY                                                     1470 06226000
*     (R6) RESTRICTED;                                             1470 06227000
@RF01465 DS    0H                                                  1471 06228000
*    R6=DECADDR;                                                   1471 06229000
