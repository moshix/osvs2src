* /******************************************************************** 06097000
* /*                   CONVERT HEX TO PRINTABLE                       * 06098000
* /*    THIS PROC WILL CONVERT ANY NUMBER OF HEX CHARACTERS TO PRINTAB* 06099000
*CONVERT1:                                                         1437 06100000
*    PROC(NUMBER,ADDNXT,NEXTBY);                                   1437 06101000
CONVERT1 STM   @14,@12,12(@13)                                     1437 06102000
         MVC   @PC00045(12),0(@01)                                 1437 06103000
*    DCL                                                           1438 06104000
*      NUMBER FIXED(15);             /* NUMBER OF BYTES TO CONVERT    * 06105000
*    DCL                                                           1439 06106000
*      NEXTBY POINTER(31);           /* WHERE THE BYTES ARE COMING 1439 06107000
*                                       FROM                          * 06108000
*    DCL                                                           1440 06109000
*      ADDNXT POINTER(31);           /* WHERE THE CONVERTED BYTES ARE   06110000
*                                       GOING                         * 06111000
*    RESPECIFY                                                     1441 06112000
*     (R4,                                                         1441 06113000
*      R5) RESTRICTED;                                             1441 06114000
*    DO J=NUMBER BY-1 TO ONE;                                      1442 06115000
         L     @15,@PC00045                                        1442 06116000
         LH    @15,NUMBER(,@15)                                    1442 06117000
         STH   @15,J                                               1442 06118000
         B     @DE01442                                            1442 06119000
@DL01442 DS    0H                                                  1443 06120000
*      R4=NEXTBYTE(J);               /* PICK UP THE NEXT BYTE TO SPLIT* 06121000
         L     @14,@PC00045+8                                      1443 06122000
         L     @01,NEXTBY(,@14)                                    1443 06123000
         BCTR  @01,0                                               1443 06124000
         SLR   R4,R4                                               1443 06125000
         IC    R4,NEXTBYTE(@15,@01)                                1443 06126000
*      SRDL(R4,FOUR);                /* SPLIT THE BYTE ACROSS 2 REGS  * 06127000
         SRDL  R4,4                                                1444 06128000
*      SRL(R5,28);                   /* RIGHT JUSTIFY THE DATA IN REGS* 06129000
         SRL   R5,28                                               1445 06130000
*      T=TWO*J;                      /* OUTPUT IS TWICE THE INPUT     * 06131000
         LR    @14,@15                                             1446 06132000
         MH    @14,TWO                                             1446 06133000
         STH   @14,T                                               1446 06134000
*      ADDNUM(T-ONE)=CHAR1(R4+1);    /* MOVE THE CHAR THE REG ='S OUT * 06135000
