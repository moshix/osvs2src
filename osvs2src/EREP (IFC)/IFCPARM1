*                           CALL OUTMSG(SYNTAX);           /*@Z427VLS*/ 03101000
         LA    @01,@AL00957                                        0957 03102000
         BAL   @14,OUTMSG                                          0957 03103000
*                         ELSE                                     0958 03104000
*                           DEVINCL = ON;                  /*@Z427VLS*/ 03105000
         B     @RC00956                                            0958 03106000
@RF00956 OI    DEVINCL,B'00001000'                                 0958 03107000
*                         CALL FINDDEV;                    /*@Z427VLS*/ 03108000
@RC00956 BAL   @14,FINDDEV                                         0959 03109000
*                       END;                               /*@Z427VLS*/ 03110000
*                     ELSE                                         0961 03111000
*                       DO;                                /*@Z427VLS*/ 03112000
         B     @RC00954                                            0961 03113000
@RF00954 DS    0H                                                  0962 03114000
*                         CALL OUTMSG(SYNTAX);    /* WRONG LNGTH OPER * 03115000
