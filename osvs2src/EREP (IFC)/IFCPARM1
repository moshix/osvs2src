*          CALL OUTMSG(SYNTAX);                            /*@Z427VLS*/ 00555000
         LA    @01,@AL00251                                        0251 00556000
         BAL   @14,OUTMSG                                          0251 00557000
*        END;                                              /*@Z427VLS*/ 00558000
*    END;                                                  /*@Z427VLS*/ 00559000
@RF00249 LA    @08,1                                               0253 00560000
         AL    @08,INDEX3                                          0253 00561000
         ST    @08,INDEX3                                          0253 00562000
@DE00240 CR    @08,I                                               0253 00563000
         BNH   @DL00240                                            0253 00564000
* END;                                                     /*@Z427VLS*/ 00565000
@EL00006 DS    0H                                                  0254 00566000
@EF00006 DS    0H                                                  0254 00567000
@ER00006 LM    @14,@12,@SA00006                                    0254 00568000
         BR    @14                                                 0254 00569000
*/********************************************************************/ 00570000
*/*                                                          @Z427VLS*/ 00571000
*/*  SCAN THE PARAMETER FIELD FOR THE NEXT COMMA.  THE START OF THE  */ 00572000
*/*  SCAN IS INDICATED BY THE VALUE OF INDEX1.  THE NUMBER OF        */ 00573000
