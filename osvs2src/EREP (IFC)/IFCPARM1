*                 DO UNTIL (RPORC = OFF);                  /*@Z427VLS*/ 03054000
@DL00943 DS    0H                                                  0944 03055000
*                   CALL SCANCRP;             /* FIND THE DELIMITER  */ 03056000
         BAL   @14,SCANCRP                                         0944 03057000
*                   FOUND = OFF;                           /*@Z427VLS*/ 03058000
         NI    FOUND,B'11101111'                                   0945 03059000
*                   /*                                       @Z427VLS*/ 03060000
*                   /* EITHER AN OPERAND OF LENGTH 5 OR LENGTH 4 IS  */ 03061000
*                   /* VALID.  IF THE LENGTH IS 5, THE EXLUSION OF   */ 03062000
*                   /* DEVICES IS ASSUMED AND ALL OPERANDS MUST      */ 03063000
*                   /* SPECIFY THE LEADING 'N'.  IF THE LENGTH IS 4, */ 03064000
*                   /* THE INCLUSION OF DEVICES IS ASSUMED AND ALL   */ 03065000
*                   /* OPERANDS MUST NOT SPECIFY THE LEADING 'N'.    */ 03066000
*                   /* THE PROCEDURE FINDDEV WILL VALIDATE THE DEVICE*/ 03067000
*                   /* SPECIFICATION AGAINST DEVTABLE.       @Z427VLS*/ 03068000
*                   /*                                       @Z427VLS*/ 03069000
*                   IF NEXTC = 5 & PARMFLD(INDEX1) = 'N' THEN      0946 03070000
         CLC   NEXTC(4),@CF00079                                   0946 03071000
         BNE   @RF00946                                            0946 03072000
         LA    @08,PARMFLD-1(INDEX1)                               0946 03073000
         CLI   0(@08),C'N'                                         0946 03074000
         BNE   @RF00946                                            0946 03075000
*                     DO;                                  /*@Z427VLS*/ 03076000
*                        IF DEVINCL = ON THEN                      0948 03077000
         TM    DEVINCL,B'00001000'                                 0948 03078000
         BNO   @RF00948                                            0948 03079000
*                          CALL OUTMSG(SYNTAX);            /*@Z427VLS*/ 03080000
         LA    @01,@AL00949                                        0949 03081000
         BAL   @14,OUTMSG                                          0949 03082000
*                        ELSE                                      0950 03083000
*                          DEVEXCL = ON;                   /*@Z427VLS*/ 03084000
         B     @RC00948                                            0950 03085000
@RF00948 OI    DEVEXCL(R2),B'00100000'                             0950 03086000
*                        INDEX1 = INDEX1 + 1;     /* STEP OVER 'N'   */ 03087000
@RC00948 AL    INDEX1,@CF00055                                     0951 03088000
*                        CALL FINDDEV;                     /*@Z427VLS*/ 03089000
         BAL   @14,FINDDEV                                         0952 03090000
*                     END;                                 /*@Z427VLS*/ 03091000
*                   ELSE                                           0954 03092000
*                     IF NEXTC = 4 THEN                            0954 03093000
         B     @RC00946                                            0954 03094000
@RF00946 CLC   NEXTC(4),@CF00064                                   0954 03095000
         BNE   @RF00954                                            0954 03096000
*                       DO;                                /*@Z427VLS*/ 03097000
*                         IF DEVEXCL = ON THEN                     0956 03098000
         TM    DEVEXCL(R2),B'00100000'                             0956 03099000
