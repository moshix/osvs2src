         BL    @DL00194                                            0213 00400000
*      END;                                                /*@Z427VLS*/ 00401000
*    IF FOUND = OFF THEN                                           0215 00402000
@RC00126 TM    FOUND,B'00010000'                                   0215 00403000
         BNZ   @RF00215                                            0215 00404000
*      CALL OUTMSG(SYNTAX);               /* DEVICE NOT VALID.       */ 00405000
         LA    @01,@AL00216                                        0216 00406000
         BAL   @14,OUTMSG                                          0216 00407000
*    INDEX1 = INDEX1 + 5;                 /* INCR TO NEXT OPERAND    */ 00408000
@RF00215 AL    INDEX1,@CF00079                                     0217 00409000
* END;                                                     /*@Z427VLS*/ 00410000
@EL00004 DS    0H                                                  0218 00411000
@EF00004 DS    0H                                                  0218 00412000
@ER00004 L     @14,@SA00004                                        0218 00413000
         L     @01,@SA00004+4                                      0218 00414000
         BR    @14                                                 0218 00415000
*/********************************************************************/ 00416000
*/*                                                          @Z427VLS*/ 00417000
*/*  VALID THE FIELD STARTING AT PARMFLD(INDEX1) FOR A LENGTH AS     */ 00418000
*/*  GIVEN AS THE INPUT PARAMETER.  IF A CHARACTER IS NOT FOUND TO   */ 00419000
*/*  BE A VALID HEX, THE FIELD IS INDICATED AS HAVING A SYNTAX       */ 00420000
*/*  ERROR.  THE CONTENTS OF THE ORIGINAL FIELD ARE CHANGED TO THE   */ 00421000
*/*  HEXADECIMAL EQUIVALENT FOR THE CHARACTER.  THOSE VALUES CAN THEN*/ 00422000
*/*  BE EASILY CONVERTED FOR THE NECESSARY FIELD.            @Z427VLS*/ 00423000
*/*                                                          @Z427VLS*/ 00424000
*/********************************************************************/ 00425000
* VALIDHEX: PROC (LENGTHH) ;                               /*@Z427VLS*/ 00426000
VALIDHEX STM   @14,@12,@SA00005                                    0219 00427000
         MVC   @PC00005(4),0(@01)                                  0219 00428000
*    DCL LENGTHH FIXED(31);               /* LENGTH OF THE INPUT FLD */ 00429000
*    I = INDEX1 + LENGTHH - 1;            /* LAST CHARACTER POSITION */ 00430000
         L     @08,@PC00005                                        0221 00431000
         LR    I,INDEX1                                            0221 00432000
         AL    I,LENGTHH(,@08)                                     0221 00433000
         BCTR  I,0                                                 0221 00434000
*    DO INDEX3 = INDEX1 TO I BY 1;        /* STEP THROUGH FIELD      */ 00435000
         LR    @08,INDEX1                                          0222 00436000
         ST    @08,INDEX3                                          0222 00437000
         B     @DE00222                                            0222 00438000
@DL00222 DS    0H                                                  0223 00439000
*      VALID = OFF;                                        /*@Z427VLS*/ 00440000
         NI    VALID,B'01111111'                                   0223 00441000
*      /*                                                    @Z427VLS*/ 00442000
*      /* CHECK TO SEE IF THE FIELD IS A VALID HEXADECIMAL NUMBER.   */ 00443000
*      /* IF YES, TERMINATE THE SEARCH, AND PLACE THE HEX EQUIVALENT */ 00444000
*      /* OF THE CHARACTER IN THE PARAMETER FIELD. IF NO, PRINT      */ 00445000
*      /* AN ERROR MESSAGE FOR THAT OPERAND.                 @Z427VLS*/ 00446000
*      /*                                                    @Z427VLS*/ 00447000
*      DO J = 16 TO 1 BY -1 WHILE (VALID = OFF);           /*@Z427VLS*/ 00448000
         LA    J,16                                                0224 00449000
@DL00224 TM    VALID,B'10000000'                                   0224 00450000
         BNZ   @DC00224                                            0224 00451000
*        IF PARMFLD(INDEX3) = NUMTAB(J) THEN                       0225 00452000
         L     @08,INDEX3                                          0225 00453000
         LA    @07,PARMFLD-1(@08)                                  0225 00454000
         LA    @03,NUMTAB-1(J)                                     0225 00455000
         CLC   0(1,@07),0(@03)                                     0225 00456000
         BNE   @RF00225                                            0225 00457000
*          DO;                                             /*@Z427VLS*/ 00458000
*            VALID = ON;                                   /*@Z427VLS*/ 00459000
         OI    VALID,B'10000000'                                   0227 00460000
*            PARMFLD(INDEX3) = HEXTAB(J);                  /*@Z427VLS*/ 00461000
         LA    @07,PARMFLD-1(@08)                                  0228 00462000
         LA    @08,HEXTAB-1(J)                                     0228 00463000
         MVC   0(1,@07),0(@08)                                     0228 00464000
*          END;                                            /*@Z427VLS*/ 00465000
*      END;                                                /*@Z427VLS*/ 00466000
@RF00225 BCTR  J,0                                                 0230 00467000
         LTR   J,J                                                 0230 00468000
         BP    @DL00224                                            0230 00469000
@DC00224 DS    0H                                                  0231 00470000
*      IF VALID = OFF THEN                                         0231 00471000
         TM    VALID,B'10000000'                                   0231 00472000
         BNZ   @RF00231                                            0231 00473000
*        DO PLACE = INDEX1 - INDEX3;                       /*@Z427VLS*/ 00474000
         LR    @08,INDEX1                                          0232 00475000
         SL    @08,INDEX3                                          0232 00476000
         ST    @08,PLACE                                           0232 00477000
*          CALL OUTMSG(SYNTAX);                            /*@Z427VLS*/ 00478000
         LA    @01,@AL00233                                        0233 00479000
         BAL   @14,OUTMSG                                          0233 00480000
*        END;                                              /*@Z427VLS*/ 00481000
*    END;                                                  /*@Z427VLS*/ 00482000
@RF00231 LA    @08,1                                               0235 00483000
         AL    @08,INDEX3                                          0235 00484000
         ST    @08,INDEX3                                          0235 00485000
@DE00222 CR    @08,I                                               0235 00486000
         BNH   @DL00222                                            0235 00487000
* END;                                                     /*@Z427VLS*/ 00488000
@EL00005 DS    0H                                                  0236 00489000
@EF00005 DS    0H                                                  0236 00490000
@ER00005 LM    @14,@12,@SA00005                                    0236 00491000
         BR    @14                                                 0236 00492000
*/********************************************************************/ 00493000
*/*                                                          @Z427VLS*/ 00494000
*/*  VALID THE FIELD STARTING AT PARMFLD(INDEX1) FOR A LENGTH AS     */ 00495000
