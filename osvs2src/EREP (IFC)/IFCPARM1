*                 DEVFLG = ON;                             /*@Z427VLS*/ 03032000
         OI    DEVFLG,B'00001000'                                  0934 03033000
*                 /*                                         @Z427VLS*/ 03034000
*                 /* GET SPACE FOR THE TABLE.  ESTABLISH THE BASE    */ 03035000
*                 /* POINTER. CLEAR THE TABLE.               @Z427VLS*/ 03036000
*                 /*                                         @Z427VLS*/ 03037000
*                 RESPECIFY (R1) RESTRICTED;               /*@Z427VLS*/ 03038000
*                 R1 = 256;                                /*@0001VLS*/ 03039000
         LA    R1,256                                              0936 03040000
*                 GEN(GETMAIN R,LV=(1),SP=20);             /*@Z427VLS*/ 03041000
         GETMAIN R,LV=(1),SP=20                                         03042000
*                 DEVPTR = R1;                             /*@Z427VLS*/ 03043000
         ST    R1,DEVPTR(,R2)                                      0938 03044000
*                 RESPECIFY (R1) UNRESTRICTED;             /*@Z427VLS*/ 03045000
*                 DEVICE = DEVICE && DEVICE;               /*@Z427VLS*/ 03046000
         L     @08,DEVPTR(,R2)                                     0940 03047000
         XC    DEVICE(254,@08),DEVICE(@08)                         0940 03048000
*                 INDEX2 = ZERO;                           /*@Z427VLS*/ 03049000
         SLR   @08,@08                                             0941 03050000
         ST    @08,INDEX2                                          0941 03051000
*                 INDEX3 = ZERO;                           /*@Z427VLS*/ 03052000
