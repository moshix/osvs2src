         TITLE 'IFCPARM1-PARAMETER MODULE FOR EREP'                     00001000
IFCPARM1 CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'IFCPARM1  79.011'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  ST    @14,12(,@13)                                        0001 00009000
         STM   @00,@12,20(@13)                                     0001 00010000
         BALR  @09,0                                               0001 00011000
@PSTART  LA    @10,4095(,@09)                                      0001 00012000
         LA    @11,4095(,@10)                                      0001 00013000
         LA    @12,4095(,@11)                                      0001 00014000
         USING @PSTART,@09                                         0001 00015000
         USING @PSTART+4095,@10                                    0001 00016000
         USING @PSTART+8190,@11                                    0001 00017000
         USING @PSTART+12285,@12                                   0001 00018000
         ST    @13,@SA00001+4                                      0001 00019000
         LA    @14,@SA00001                                        0001 00020000
         ST    @14,8(,@13)                                         0001 00021000
         LR    @13,@14                                             0001 00022000
         B     @PB00002                                            0098 00023000
DUPLICAT ST    @14,@SA00002                                        0098 00024000
         ST    @01,@SA00002+4                                      0098 00025000
*      CALL OUTMSG(DUPE);                                  /*@Z427VLS*/ 00026000
         LA    @01,@AL00099                                        0099 00027000
         BAL   @14,OUTMSG                                          0099 00028000
*      IF NEXTRPC = OFF THEN                                       0100 00029000
         TM    NEXTRPC,B'00000100'                                 0100 00030000
         BNZ   @RF00100                                            0100 00031000
*        DO;                                               /*@Z427VLS*/ 00032000
*          CALL SCANRP;                                    /*@Z427VLS*/ 00033000
         BAL   @14,SCANRP                                          0102 00034000
*          INDEX1 = INDEX1 + NEXTC + 1;                    /*@Z427VLS*/ 00035000
         LR    @08,INDEX1                                          0103 00036000
         AL    @08,NEXTC                                           0103 00037000
         AL    @08,@CF00055                                        0103 00038000
         LR    INDEX1,@08                                          0103 00039000
*        END;                                              /*@Z427VLS*/ 00040000
*      CALL SCANC;                                         /*@Z427VLS*/ 00041000
@RF00100 BAL   @14,SCANC                                           0105 00042000
*      INDEX1 = INDEX1 + NEXTC;                            /*@Z427VLS*/ 00043000
         AL    INDEX1,NEXTC                                        0106 00044000
*   END;                                                   /*@Z427VLS*/ 00045000
@EL00002 DS    0H                                                  0107 00046000
@EF00002 DS    0H                                                  0107 00047000
@ER00002 L     @14,@SA00002                                        0107 00048000
         L     @01,@SA00002+4                                      0107 00049000
         BR    @14                                                 0107 00050000
*/********************************************************************/ 00051000
*/*                                                          @Z427VLS*/ 00052000
*/*  OUTPUT AN ERROR MESSAGE.  THE FLAG BEING PASSED AS A PARAMETER  */ 00053000
*/*  INDICATES THE TYPE OF ERROR.  IF THE INPUT PARAMETER STRING HAS */ 00054000
*/*  NOT BEEN OUTPUT, A HEADING IS WRITTEN OUT AND THE PARAMETER     */ 00055000
*/*  STRING IS WRITTEN OUT.  THEN THE ERROR MESSAGE IS WRITTEN OUT   */ 00056000
*/*  WITH AN ASTERISK IN THE PLACE WHERE THE SCAN ROUTINE IS LOCATED.*/ 00057000
*/*  THIS SHOULD INDICATE THE LOCATION OF THE ERROR.         @Z427VLS*/ 00058000
*/********************************************************************/ 00059000
*OUTMSG:   PROC (ERROR) ;                                  /*@Z427VLS*/ 00060000
OUTMSG   STM   @14,@12,@SA00003                                    0108 00061000
         MVC   @PC00003(4),0(@01)                                  0108 00062000
*   DCL ERROR  FIXED(31);                                  /*@Z427VLS*/ 00063000
*  IF INDEX1 <= 100 THEN                                           0110 00064000
         C     INDEX1,@CF00430                                     0110 00065000
         BH    @RF00110                                            0110 00066000
*    DO;                                                   /*@Z427VLS*/ 00067000
*      INDEX1 = INDEX1 - PLACE;          /* LOCATION FOR ASTERISK    */ 00068000
         SL    INDEX1,PLACE                                        0112 00069000
*      IF ERROR = SYNTAX THEN                                      0113 00070000
         L     @08,@PC00003                                        0113 00071000
         CLC   ERROR(4,@08),@CF00055                               0113 00072000
         BNE   @RF00113                                            0113 00073000
*        CALL IFCMSG(65,ADDR(MSGNO));    /* SYNTAX ERROR     @Z427VLS*/ 00074000
         LA    @08,MSGNO                                           0114 00075000
         ST    @08,@AFTEMPS                                        0114 00076000
         L     @15,ADIFCMSG(,R2)                                   0114 00077000
         LA    @01,@AL00114                                        0114 00078000
         BALR  @14,@15                                             0114 00079000
*      IF ERROR = DUPE THEN                                        0115 00080000
@RF00113 L     @08,@PC00003                                        0115 00081000
         CLC   ERROR(4,@08),@CF00074                               0115 00082000
         BNE   @RF00115                                            0115 00083000
*        CALL IFCMSG(66,ADDR(MSGNO));    /* DUPLICATE ERROR  @Z427VLS*/ 00084000
         LA    @08,MSGNO                                           0116 00085000
         ST    @08,@AFTEMPS                                        0116 00086000
         L     @15,ADIFCMSG(,R2)                                   0116 00087000
         LA    @01,@AL00116                                        0116 00088000
         BALR  @14,@15                                             0116 00089000
*      PARMPRT(INDEX1) = STAR;           /* INSERT ASTERISK  @Z427VLS*/ 00090000
@RF00115 LA    @08,PARMPRT-1(INDEX1)                               0117 00091000
