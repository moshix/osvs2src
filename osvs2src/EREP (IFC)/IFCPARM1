         LA    @04,PARMFLD-1(@08)                                  0270 00664000
         CLI   0(@04),C','                                         0270 00665000
         BNE   @DL00269                                            0270 00666000
@DC00269 DS    0H                                                  0271 00667000
*    IF PARMFLD(NEXTC) = RPAREN THEN                               0271 00668000
         L     @08,NEXTC                                           0271 00669000
         LA    @04,PARMFLD-1(@08)                                  0271 00670000
         CLI   0(@04),C')'                                         0271 00671000
         BNE   @RF00271                                            0271 00672000
*      RPORC = OFF;                                        /*@Z427VLS*/ 00673000
         NI    RPORC,B'01111111'                                   0272 00674000
*    NEXTC = NEXTC - INDEX1;                               /*@Z427VLS*/ 00675000
@RF00271 L     @08,NEXTC                                           0273 00676000
         SLR   @08,INDEX1                                          0273 00677000
         ST    @08,NEXTC                                           0273 00678000
*END;                                                      /*@Z427VLS*/ 00679000
@EL00009 DS    0H                                                  0274 00680000
@EF00009 DS    0H                                                  0274 00681000
@ER00009 L     @14,12(,@13)                                        0274 00682000
         L     @01,24(,@13)                                        0274 00683000
         BR    @14                                                 0274 00684000
*/********************************************************************/ 00685000
*/*                                                          @Z427VLS*/ 00686000
*/*  GENERATE A JULIAN YY/DDD FIELD FROM THE INPUT STREAM AND PLACE  */ 00687000
*/*  IT IN THE VARIABLE YYDDD.                               @Z427VLS*/ 00688000
*/*                                                          @Z427VLS*/ 00689000
*/********************************************************************/ 00690000
*YEARDAY: PROC OPTIONS(SAVE(1,14));                        /*@Z427VLS*/ 00691000
YEARDAY  ST    @14,@SA00010                                        0275 00692000
         ST    @01,@SA00010+4                                      0275 00693000
*    /*                                                      @Z427VLS*/ 00694000
*    /* THE FIELD IS FIRST CHECKED FOR VALID NUMERICS.  THE FIRST    */ 00695000
*    /* FOUR HEX VALUES ARE THEN PACKED INTO THE FIELD HOLD, AND THEN*/ 00696000
*    /* PLACED IN THE YEAR/DAY HOLD AREA (YYDDD).  THE REMAINING HEX */ 00697000
*    /* VALUE IS SHIFTED AND ORED WITH A SIGN FLAG.  THE FIELD IS    */ 00698000
*    /* THEN CHECKED AGAINST DAY 366.  IF GREATER, AN ERROR MESSAGE  */ 00699000
*    /* IS OUTPUT.                                           @Z427VLS*/ 00700000
*    /*                                                      @Z427VLS*/ 00701000
*    CALL VALIDNUM(5);                                     /*@Z427VLS*/ 00702000
         LA    @01,@AL00276                                        0276 00703000
         BAL   @14,VALIDNUM                                        0276 00704000
*    CALL HEXPACK(4);                                      /*@Z427VLS*/ 00705000
         LA    @01,@AL00277                                        0277 00706000
         BAL   @14,HEXPACK                                         0277 00707000
*    YY = HOLD(1);                                         /*@Z427VLS*/ 00708000
         MVC   YY(1),HOLD                                          0278 00709000
*    DD = HOLD(2);                                         /*@Z427VLS*/ 00710000
         MVC   DD(1),HOLD+1                                        0279 00711000
*    SLL(PARMFLD(INDEX1),FOUR);                            /*@Z427VLS*/ 00712000
         SLR   @08,@08                                             0280 00713000
         IC    @08,PARMFLD-1(INDEX1)                               0280 00714000
         SLL   @08,4                                               0280 00715000
         STC   @08,PARMFLD-1(INDEX1)                               0280 00716000
*    DS = PARMFLD(INDEX1) | SIGNFLAG;                      /*@Z427VLS*/ 00717000
         LA    @08,PARMFLD-1(INDEX1)                               0281 00718000
         MVC   DS(1),0(@08)                                        0281 00719000
         OI    DS,X'0F'                                            0281 00720000
*    IF DDDS > '366F'X | DDDS = '000F'X THEN                       0282 00721000
         CLC   DDDS(2),@CB00749                                    0282 00722000
         BH    @RT00282                                            0282 00723000
         CLC   DDDS(2),@CB00750                                    0282 00724000
         BNE   @RF00282                                            0282 00725000
@RT00282 DS    0H                                                  0283 00726000
*      DO PLACE = 2;                                       /*@Z427VLS*/ 00727000
         LA    @08,2                                               0283 00728000
         ST    @08,PLACE                                           0283 00729000
*        CALL OUTMSG(SYNTAX);                              /*@Z427VLS*/ 00730000
         LA    @01,@AL00284                                        0284 00731000
         BAL   @14,OUTMSG                                          0284 00732000
*      END;                                                /*@Z427VLS*/ 00733000
*    INDEX1 = INDEX1 + 1;                                  /*@Z427VLS*/ 00734000
@RF00282 AL    INDEX1,@CF00055                                     0286 00735000
*END;                                                      /*@Z427VLS*/ 00736000
@EL00010 DS    0H                                                  0287 00737000
@EF00010 DS    0H                                                  0287 00738000
@ER00010 L     @14,@SA00010                                        0287 00739000
         L     @01,@SA00010+4                                      0287 00740000
         BR    @14                                                 0287 00741000
*/********************************************************************/ 00742000
*/*                                                          @Z427VLS*/ 00743000
*/*  PACK A FIELD OF A GIVEN LENGTH INTO THE VARIABLE HOLD.  THE     */ 00744000
*/*  INPUT IS ASSUMED TO BE HEX CHARACTERS, WITH A LEADING ZERO FOR  */ 00745000
*/*  EACH TWO DIGITS.                                        @Z427VLS*/ 00746000
*/********************************************************************/ 00747000
*HEXPACK: PROC (HEXLEN) OPTIONS(SAVE(1,14));               /*@Z427VLS*/ 00748000
HEXPACK  ST    @14,12(,@13)                                        0288 00749000
         ST    @01,24(,@13)                                        0288 00750000
         MVC   @PC00011(4),0(@01)                                  0288 00751000
*   DCL HEXLEN FIXED(31);                                  /*@Z427VLS*/ 00752000
*   INDEX3 = HEXLEN / 2;                                   /*@Z427VLS*/ 00753000
         L     @08,@PC00011                                        0290 00754000
         L     @00,HEXLEN(,@08)                                    0290 00755000
         SRDA  @00,32                                              0290 00756000
         D     @00,@CF00074                                        0290 00757000
         ST    @01,INDEX3                                          0290 00758000
*   DO I = 1 TO INDEX3 BY 1;                               /*@Z427VLS*/ 00759000
         LA    I,1                                                 0291 00760000
         B     @DE00291                                            0291 00761000
@DL00291 DS    0H                                                  0292 00762000
*     /*                                                     @Z427VLS*/ 00763000
*     /* THE DATA IN PARMFLD IS ACCESSED TWO CHARACTERS AT A TIME.   */ 00764000
*     /* THE FIRST CHARACTER IS SHIFTED LEFT FOUR BITS AND THEN ORED */ 00765000
*     /* WITH THE SECOND CHARACTER.  THE SECOND CHARACTER IS ASSUMED */ 00766000
*     /* TO HAVE THE FIRST FOUR BITS AS ZEROES.              @Z427VLS*/ 00767000
*     /*                                                     @Z427VLS*/ 00768000
*     HOLD(I) = PARMFLD(INDEX1);                           /*@Z427VLS*/ 00769000
         LA    @08,HOLD-1(I)                                       0292 00770000
         LA    @07,PARMFLD-1(INDEX1)                               0292 00771000
         MVC   0(1,@08),0(@07)                                     0292 00772000
*     SLL (HOLD(I),FOUR);                                  /*@Z427VLS*/ 00773000
         SLR   @08,@08                                             0293 00774000
         IC    @08,HOLD-1(I)                                       0293 00775000
         SLL   @08,4                                               0293 00776000
         STC   @08,HOLD-1(I)                                       0293 00777000
*     HOLD(I) = HOLD(I) | PARMFLD(INDEX1 + 1);             /*@Z427VLS*/ 00778000
         LA    @08,HOLD-1(I)                                       0294 00779000
         LA    @07,PARMFLD(INDEX1)                                 0294 00780000
         OC    0(1,@08),0(@07)                                     0294 00781000
*     INDEX1 = INDEX1 + 2;                                 /*@Z427VLS*/ 00782000
         AL    INDEX1,@CF00074                                     0295 00783000
*   END;                                                   /*@Z427VLS*/ 00784000
         AL    I,@CF00055                                          0296 00785000
@DE00291 C     I,INDEX3                                            0296 00786000
         BNH   @DL00291                                            0296 00787000
*END;                                                      /*@Z427VLS*/ 00788000
@EL00011 DS    0H                                                  0297 00789000
@EF00011 DS    0H                                                  0297 00790000
@ER00011 L     @14,12(,@13)                                        0297 00791000
         L     @01,24(,@13)                                        0297 00792000
         BR    @14                                                 0297 00793000
*/********************************************************************/ 00794000
*/*                                                          @Z427VLS*/ 00795000
*/*  VALIDATE THE FIELD STARTING AT PARMFLD(INDEX1) FOR THE VALUES   */ 00796000
*/*  OF '=Y', '=N', OR NOTHING.  IF NOTHING IS THERE, THE VALUES OF  */ 00797000
*/*  '=Y' IS ASSUMED.  THE LENGTH OF THE KEYWORD IS PASSED, THE      */ 00798000
*/*  DUPLICATE SWITCH FIELD AND THE FLAG IN THE PARAMETER TABLE.     */ 00799000
*/*                                                          @Z427VLS*/ 00800000
*/********************************************************************/ 00801000
*VALIDYN: PROC(INCR) OPTIONS(SAVE(1,14));                  /*@Z427VLS*/ 00802000
VALIDYN  ST    @14,@SA00012                                        0298 00803000
         ST    @01,@SA00012+4                                      0298 00804000
         MVC   @PC00012(4),0(@01)                                  0298 00805000
*    DCL INCR FIXED(31);                 /* LENGTH OF THE PARAMETER  */ 00806000
*    INDEX1 = INDEX1 + INCR;             /* STEP OVER PARM   @Z427VLS*/ 00807000
         L     @08,@PC00012                                        0300 00808000
         AL    INDEX1,INCR(,@08)                                   0300 00809000
*    PARMFLG = OFF;                      /* ASSUME PARM=N VALUE      */ 00810000
         NI    PARMFLG,B'11011111'                                 0301 00811000
*    IF PARMFLD(INDEX1) = COMMA THEN                               0302 00812000
         LA    @08,PARMFLD-1(INDEX1)                               0302 00813000
         CLI   0(@08),C','                                         0302 00814000
         BNE   @RF00302                                            0302 00815000
*      PARMFLG = ON;                     /* NULL VALUE, ASSUME = Y   */ 00816000
         OI    PARMFLG,B'00100000'                                 0303 00817000
*    ELSE                                                          0304 00818000
*      IF PARMFLD(INDEX1:INDEX1+1) = EQUALY THEN                   0304 00819000
         B     @RC00302                                            0304 00820000
@RF00302 LA    @08,PARMFLD-1(INDEX1)                               0304 00821000
         CLC   0(2,@08),@CC00666                                   0304 00822000
         BNE   @RF00304                                            0304 00823000
*        DO;                                               /*@Z427VLS*/ 00824000
*          PARMFLG = ON;                 /* SPECIFIED = Y    @Z427VLS*/ 00825000
         OI    PARMFLG,B'00100000'                                 0306 00826000
*          INDEX1 = INDEX1 + 2;          /* STEP OVER =Y     @Z427VLS*/ 00827000
         AL    INDEX1,@CF00074                                     0307 00828000
*        END;                                              /*@Z427VLS*/ 00829000
*      ELSE                                                        0309 00830000
*        IF PARMFLD(INDEX1:INDEX1+1) = EQUALN THEN                 0309 00831000
         B     @RC00304                                            0309 00832000
@RF00304 LA    @08,PARMFLD-1(INDEX1)                               0309 00833000
         CLC   0(2,@08),@CC00668                                   0309 00834000
         BNE   @RF00309                                            0309 00835000
*            INDEX1 = INDEX1 + 2;        /* STEP OVER = N    @Z427VLS*/ 00836000
         AL    INDEX1,@CF00074                                     0310 00837000
*        ELSE                                                      0311 00838000
*          DO;                                             /*@Z427VLS*/ 00839000
         B     @RC00309                                            0311 00840000
@RF00309 DS    0H                                                  0312 00841000
*            CALL OUTMSG(SYNTAX);                          /*@Z427VLS*/ 00842000
         LA    @01,@AL00312                                        0312 00843000
         BAL   @14,OUTMSG                                          0312 00844000
*            CALL SCANC;                                   /*@Z427VLS*/ 00845000
         BAL   @14,SCANC                                           0313 00846000
*            INDEX1 = INDEX1 + NEXTC;                      /*@Z427VLS*/ 00847000
         AL    INDEX1,NEXTC                                        0314 00848000
*          END;                                            /*@Z427VLS*/ 00849000
*END;                                                      /*@Z427VLS*/ 00850000
@EL00012 DS    0H                                                  0316 00851000
@EF00012 DS    0H                                                  0316 00852000
@ER00012 L     @14,@SA00012                                        0316 00853000
         L     @01,@SA00012+4                                      0316 00854000
         BR    @14                                                 0316 00855000
*/********************************************************************/ 00856000
*/*                                                          @Z427VLS*/ 00857000
*/*  GENERATE A VALID CPU/CUA ENTRY.  THE ENTRY HAS A CPU SERIAL     */ 00858000
*/*  NUMBER FIRST, FOLLOWED BY A CUA.  THIS IS TRANSLATED INTO A     */ 00859000
*/*  HEX VALUE AND PLACED IN THE NEXT CPUCUAV ENTRY, INDEXED BY THE  */ 00860000
*/*  VARIABLE INDEX2.                                        @Z427VLS*/ 00861000
*/*                                                          @Z427VLS*/ 00862000
*/********************************************************************/ 00863000
*VALCPUCU: PROC OPTIONS(SAVE(1,14));                       /*@Z427VLS*/ 00864000
VALCPUCU ST    @14,@SA00013                                        0317 00865000
         ST    @01,@SA00013+4                                      0317 00866000
*  IF NEXTC = 10 & PARMFLD(INDEX1+6) = PERIOD THEN                 0318 00867000
         CLC   NEXTC(4),@CF00034                                   0318 00868000
         BNE   @RF00318                                            0318 00869000
         LA    @08,PARMFLD+5(INDEX1)                               0318 00870000
         CLI   0(@08),C'.'                                         0318 00871000
         BNE   @RF00318                                            0318 00872000
*    DO;                                                   /*@Z427VLS*/ 00873000
*      /*                                                    @Z427VLS*/ 00874000
*      /* CHECK THE CPU SERIAL NUMBER FOR VALID DECIMAL NUMBERS.     */ 00875000
*      /* A ZERO IS PLACED OVER THE COMMA, AND THE CUA FIELD IS      */ 00876000
*      /* VALIDATED FOR HEXADECIMAL CHARACTERS.  THE ENTIRE FIELD IS */ 00877000
*      /* PACKED FOR THE CPUCUA TABLE.                       @Z427VLS*/ 00878000
*      /*                                                    @Z427VLS*/ 00879000
*      CUAHOLD = PARMFLD(INDEX1+7:INDEX1+9);     /* SAVE THE CUA     */ 00880000
         LA    @08,PARMFLD+6(INDEX1)                               0320 00881000
         MVC   CUAHOLD(3),0(@08)                                   0320 00882000
*      IF CUAHOLD(3) = 'X' THEN                                    0321 00883000
         CLI   CUAHOLD+2,C'X'                                      0321 00884000
         BNE   @RF00321                                            0321 00885000
*        PARMFLD(INDEX1+9) = '0';         /* CHANGE X TO ZERO        */ 00886000
         LA    @08,PARMFLD+8(INDEX1)                               0322 00887000
         MVI   0(@08),C'0'                                         0322 00888000
*      CALL VALIDNUM(6);                                   /*@Z427VLS*/ 00889000
@RF00321 LA    @01,@AL00323                                        0323 00890000
         BAL   @14,VALIDNUM                                        0323 00891000
*      INDEX1 = INDEX1 + 6;                                /*@Z427VLS*/ 00892000
         AL    INDEX1,@CF00081                                     0324 00893000
*      PARMFLD(INDEX1) = '0';                              /*@Z427VLS*/ 00894000
         LA    @08,PARMFLD-1(INDEX1)                               0325 00895000
         MVI   0(@08),C'0'                                         0325 00896000
*      CALL VALIDHEX(4);                                   /*@Z427VLS*/ 00897000
         LA    @01,@AL00326                                        0326 00898000
         BAL   @14,VALIDHEX                                        0326 00899000
*      INDEX1 = INDEX1 - 6;                                /*@Z427VLS*/ 00900000
         SL    INDEX1,@CF00081                                     0327 00901000
*      CALL HEXPACK(10);                                   /*@Z427VLS*/ 00902000
         LA    @01,@AL00328                                        0328 00903000
         BAL   @14,HEXPACK                                         0328 00904000
*      INDEX1 = INDEX1 + 1;                 /* INCR OVER DELIMITER   */ 00905000
         AL    INDEX1,@CF00055                                     0329 00906000
*    END;                                                  /*@Z427VLS*/ 00907000
*  ELSE                                                            0331 00908000
*    DO;                                    /* BAD INPUT LENGTH      */ 00909000
         B     @RC00318                                            0331 00910000
@RF00318 DS    0H                                                  0332 00911000
*      CALL OUTMSG(SYNTAX);                                /*@Z427VLS*/ 00912000
         LA    @01,@AL00332                                        0332 00913000
         BAL   @14,OUTMSG                                          0332 00914000
*      INDEX1 = INDEX1 + NEXTC + 1;                        /*@Z427VLS*/ 00915000
         LR    @08,INDEX1                                          0333 00916000
         AL    @08,NEXTC                                           0333 00917000
         AL    @08,@CF00055                                        0333 00918000
         LR    INDEX1,@08                                          0333 00919000
*    END;                                                  /*@Z427VLS*/ 00920000
*      /*                                                    @Z427VLS*/ 00921000
*      /* INCREMENT THE INDEX TO THE TABLE OR PRINT AN ERROR MSG.    */ 00922000
*      /*                                                    @Z427VLS*/ 00923000
*  IF INDEX2 < DIM(CPUCUAV) THEN                                   0335 00924000
@RC00318 L     @08,INDEX2                                          0335 00925000
         C     @08,@CF00064                                        0335 00926000
         BNL   @RF00335                                            0335 00927000
*    INDEX2 = INDEX2 + 1;                   /* DETERMINE INDEX VALUE */ 00928000
         AL    @08,@CF00055                                        0336 00929000
         ST    @08,INDEX2                                          0336 00930000
*  ELSE                                                            0337 00931000
*    DO PLACE = NEXTC + 1;                                 /*@Z427VLS*/ 00932000
         B     @RC00335                                            0337 00933000
@RF00335 LA    @08,1                                               0337 00934000
         AL    @08,NEXTC                                           0337 00935000
         ST    @08,PLACE                                           0337 00936000
*      CALL OUTMSG(SYNTAX);                                /*@Z427VLS*/ 00937000
         LA    @01,@AL00338                                        0338 00938000
         BAL   @14,OUTMSG                                          0338 00939000
*    END;                                                  /*@Z427VLS*/ 00940000
*  CPUCUX1(INDEX2) = HOLDAREA(1:3);                        /*@0099VLS*/ 00941000
@RC00335 L     @08,INDEX2                                          0340 00942000
         MH    @08,@CH00083                                        0340 00943000
         L     @04,CPCUAPTR(,R2)                                   0340 00944000
         ST    @08,@TF00001                                        0340 00945000
         ALR   @08,@04                                             0340 00946000
         AL    @08,@CF01052                                        0340 00947000
         MVC   CPUCUX1-2(3,@08),HOLDAREA                           0340 00948000
*  CPUCUX2(INDEX2) = HOLDAREA(4:5);                        /*@0099VLS*/ 00949000
         AL    @04,@TF00001                                        0341 00950000
         BCTR  @04,0                                               0341 00951000
         BCTR  @04,0                                               0341 00952000
         MVC   CPUCUX2-5(2,@04),HOLDAREA+3                         0341 00953000
*  IF CUAHOLD(3) = 'X' THEN                                        0342 00954000
         CLI   CUAHOLD+2,C'X'                                      0342 00955000
         BNE   @RF00342                                            0342 00956000
*    HOLDAREA(5) = HOLDAREA(5) | SIGNFLAG;  /* CHANGE FOR HIGH CUA   */ 00957000
         OI    HOLDAREA+4,X'0F'                                    0343 00958000
*  CPUCUX3(INDEX2) = HOLDAREA(4:5);                        /*@0099VLS*/ 00959000
@RF00342 L     @08,INDEX2                                          0344 00960000
         MH    @08,@CH00083                                        0344 00961000
