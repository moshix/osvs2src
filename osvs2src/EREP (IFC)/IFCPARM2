*    DCL WORDLEN2 FIXED(31);                               /*@Z427VLS*/ 00548000
*    DCL WORDSTR2 CHAR(*);                                 /*@Z427VLS*/ 00549000
*    DO I = 1 TO WORDLEN2 BY 1;                            /*@Z427VLS*/ 00550000
