*      DO;                                                 /*@Z427VLS*/ 00188000
*        /*                                                  @Z427VLS*/ 00189000
*        /* CHECK FOR THE DEVICE LEADING TWO CHARACTERS IN THE OBR   */ 00190000
*        /* DEVICE TABLE.  IF IT IS FOUND, THEN CHECK TO SEE IF THE  */ 00191000
*        /* DEVICE WAS SPECIFIED PREVIOUSLY.  IF YES, IT IS AN ERROR.*/ 00192000
*        /* IF NOT, ENTER THE DEVICE CLASS/TYPE INTO DEVICE  @Z427VLS*/ 00193000
*        /* SELECTION TABLE.  CLEAR THE DEVICE CLASS/TYPE FROM       */ 00194000
*        /* DEVTABLE.                                        @Z427VLS*/ 00195000
*        /*                                                  @Z427VLS*/ 00196000
*        J = ZERO;                                         /*@G51DGVS*/ 00197000
         SLR   J,J                                                 0149 00198000
*        DO WHILE (J < EIGHT);                             /*@G51DGVS*/ 00199000
         B     @DE00150                                            0150 00200000
@DL00150 DS    0H                                                  0151 00201000
*          RESPECIFY (R15) RESTRICTED;                     /*@G51DGVS*/ 00202000
*          CALL IFCDEVIC(5,PARMFLD(INDEX1),HOLDAREA);      /*@G51DGVS*/ 00203000
         MVC   @AL00001(12),@AL00152                               0152 00204000
         LA    @14,PARMFLD-1(INDEX1)                               0152 00205000
         ST    @14,@AL00001+4                                      0152 00206000
         L     @15,@CV00753                                        0152 00207000
         LA    @01,@AL00001                                        0152 00208000
         BALR  @14,@15                                             0152 00209000
*          J = R15;                                        /*@G51DGVS*/ 00210000
         LR    J,R15                                               0153 00211000
*          RESPECIFY (R15) UNRESTRICTED;                   /*@G51DGVS*/ 00212000
*          IF J = ZERO THEN                                        0155 00213000
         LTR   J,J                                                 0155 00214000
