*CHARPACK: PROC OPTIONS(SAVE(1,14));                       /*@Z427VLS*/ 00496000
CHARPACK ST    @14,12(,@13)                                        0230 00497000
         ST    @01,24(,@13)                                        0230 00498000
*  SLL (HOLDAREA(1:2),FOUR);                               /*@Z427VLS*/ 00499000
         SLR   @08,@08                                             0231 00500000
         ICM   @08,3,HOLDAREA                                      0231 00501000
         SLL   @08,4                                               0231 00502000
         STCM  @08,3,HOLDAREA                                      0231 00503000
*  SRL (HOLDAREA(2:2),FOUR);                               /*@Z427VLS*/ 00504000
         SLR   @08,@08                                             0232 00505000
         IC    @08,HOLDAREA+1                                      0232 00506000
         SRL   @08,4                                               0232 00507000
         STC   @08,HOLDAREA+1                                      0232 00508000
*  INDEX2 = HOLDAREA(1) + 1;                               /*@Z427VLS*/ 00509000
         LA    @08,1                                               0233 00510000
         SLR   @07,@07                                             0233 00511000
         IC    @07,HOLDAREA                                        0233 00512000
         ALR   @07,@08                                             0233 00513000
         ST    @07,INDEX2                                          0233 00514000
*  KEYWORD(2*J-1) = NUMTAB(INDEX2);                        /*@Z427VLS*/ 00515000
         LR    @03,J                                               0234 00516000
         ALR   @03,@03                                             0234 00517000
         LA    @01,KEYWORD-2(@03)                                  0234 00518000
         LA    @15,NUMTAB-1(@07)                                   0234 00519000
         MVC   0(1,@01),0(@15)                                     0234 00520000
*  INDEX2 = HOLDAREA(2) + 1;                               /*@Z427VLS*/ 00521000
         SLR   @07,@07                                             0235 00522000
         IC    @07,HOLDAREA+1                                      0235 00523000
         ALR   @08,@07                                             0235 00524000
         ST    @08,INDEX2                                          0235 00525000
*  KEYWORD(2*J) = NUMTAB(INDEX2);                          /*@Z427VLS*/ 00526000
         LA    @07,KEYWORD-1(@03)                                  0236 00527000
         LA    @03,NUMTAB-1(@08)                                   0236 00528000
         MVC   0(1,@07),0(@03)                                     0236 00529000
*END;                                                      /*@Z427VLS*/ 00530000
@EL00009 DS    0H                                                  0237 00531000
@EF00009 DS    0H                                                  0237 00532000
@ER00009 L     @14,12(,@13)                                        0237 00533000
         L     @01,24(,@13)                                        0237 00534000
         BR    @14                                                 0237 00535000
