*UNPACK: PROC (OPLEN) OPTIONS(SAVE(1,14));                 /*@Z427VLS*/ 00111000
UNPACK   ST    @14,@SA00004                                        0131 00112000
         ST    @01,@SA00004+4                                      0131 00113000
         MVC   @PC00004(4),0(@01)                                  0131 00114000
*      DCL OPLEN   FIXED(31);            /* NUMBER OF CHAR TO CONVERT*/ 00115000
*      DO J = 1 TO OPLEN BY 1;                             /*@Z427VLS*/ 00116000
         LA    J,1                                                 0133 00117000
         B     @DE00133                                            0133 00118000
@DL00133 DS    0H                                                  0134 00119000
*        HOLDAREA = HOLDAREA && HOLDAREA;  /* CHEAR HOLD AREA        */ 00120000
         XC    HOLDAREA(5),HOLDAREA                                0134 00121000
*        HOLDAREA(2) = PARMFLD(J);       /* CHARACTERS ARE IN PARMFLD*/ 00122000
         LA    @08,PARMFLD-1(J)                                    0135 00123000
         MVC   HOLDAREA+1(1),0(@08)                                0135 00124000
*        SLL (HOLDAREA(1:2),FOUR);       /* SHIFT FIRST TO FIRST BYTE*/ 00125000
         SLR   @08,@08                                             0136 00126000
         ICM   @08,3,HOLDAREA                                      0136 00127000
         SLL   @08,4                                               0136 00128000
         STCM  @08,3,HOLDAREA                                      0136 00129000
*        SRL (HOLDAREA(2:2),FOUR);       /* SECOND TO SECOND BYTE    */ 00130000
         SLR   @08,@08                                             0137 00131000
         IC    @08,HOLDAREA+1                                      0137 00132000
         SRL   @08,4                                               0137 00133000
         STC   @08,HOLDAREA+1                                      0137 00134000
*        INDEX3 = HOLDAREA(1) + 1;       /* GET VALUE AS INDEX       */ 00135000
         LA    @08,1                                               0138 00136000
         SLR   INDEX3,INDEX3                                       0138 00137000
         IC    INDEX3,HOLDAREA                                     0138 00138000
         ALR   INDEX3,@08                                          0138 00139000
*        PARMPT(2*J+1) = NUMTAB(INDEX3);  /* STORE CHARACTER @Z427VLS*/ 00140000
         LR    @07,J                                               0139 00141000
         ALR   @07,@07                                             0139 00142000
         LA    @03,PARMPT(@07)                                     0139 00143000
         LA    @01,NUMTAB-1(INDEX3)                                0139 00144000
         MVC   0(1,@03),0(@01)                                     0139 00145000
*        INDEX3 = HOLDAREA(2) + 1;       /* GET VALUE AS INDEX       */ 00146000
         SLR   INDEX3,INDEX3                                       0140 00147000
         IC    INDEX3,HOLDAREA+1                                   0140 00148000
         ALR   @08,INDEX3                                          0140 00149000
         LR    INDEX3,@08                                          0140 00150000
*        PARMPT(2*J+2) = NUMTAB(INDEX3);  /* STORE CHARACTER @Z427VLS*/ 00151000
         LA    @08,PARMPT+1(@07)                                   0141 00152000
         LA    @07,NUMTAB-1(INDEX3)                                0141 00153000
         MVC   0(1,@08),0(@07)                                     0141 00154000
*      END;                                                /*@Z427VLS*/ 00155000
         AL    J,@CF00058                                          0142 00156000
@DE00133 L     @08,@PC00004                                        0142 00157000
         C     J,OPLEN(,@08)                                       0142 00158000
         BNH   @DL00133                                            0142 00159000
*      PRINTADR = ADDR(PARMPT);                            /*@Z427VLS*/ 00160000
         LA    @08,PARMPT                                          0143 00161000
         ST    @08,PRINTADR(,R2)                                   0143 00162000
*      CALL IOHND(RITEPRTR);                               /*@Z427VLS*/ 00163000
         L     @15,ADIOHND(,R2)                                    0144 00164000
         LA    @01,@AL00144                                        0144 00165000
         BALR  @14,@15                                             0144 00166000
*   END;                                                   /*@Z427VLS*/ 00167000
@EL00004 DS    0H                                                  0145 00168000
@EF00004 DS    0H                                                  0145 00169000
@ER00004 L     @14,@SA00004                                        0145 00170000
         L     @01,@SA00004+4                                      0145 00171000
         BR    @14                                                 0145 00172000
*/********************************************************************/ 00173000
*/*                                                          @Z427VLS*/ 00174000
*/*  FIND THE SPECIFIED ENTRY IN THE PARAMETER FIELD IN THE DEVICE   */ 00175000
*/*  TABLE. THE DEVICE TABLE ENTRY IS THEN PLACED IN THE DEVICE      */ 00176000
*/*  SELECTION TABLE.                                        @Z427VLS*/ 00177000
*/*  THE DEVICE SPECIFICATION IS EITHER NNXX OR NNNN.        @Z427VLS*/ 00178000
*/*                                                          @Z427VLS*/ 00179000
*/********************************************************************/ 00180000
* FINDDEV: PROC OPTIONS(SAVE(1,14));                       /*@Z427VLS*/ 00181000
FINDDEV  ST    @14,@SA00005                                        0146 00182000
