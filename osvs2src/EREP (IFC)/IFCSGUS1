*********************************************************************** 56550042
*              DETERMINE TYPE OF RECORD AND SUM                         56600042
*********************************************************************** 56650042
SNSSUM   ST    R5,XSAVE                 SAVE WORK REG                   56700042
         LH    R5,L0(R3)                LOAD CNT OF REC WITH SNS DATA   56750042
         LA    R5,L1(R5)                INCR CNT                        56800042
         STH   R5,L0(R3)                STORE NEW CNT                   56850042
         TM    XB3SNSTY,HEX01           Q/ IS THIS HARDWARE-VERF SNS    56900042
         BO    HWSENSE                  YES                             56950042
         TM    XB3SNSTY,HEX02           Q/ IS THIS DATA-RELATED SNS     57000042
         BO    DATAREL                  YES                             57050042
         TM    XB3SNSTY,HEX04           Q/ IS THIS NOT-INITIALIZED SNS  57060042
         BO    NOTIN                    YES                             57070042
         LH    R5,L2(R3)                GET CNT OF REC WITH EQ-REL SNS  57100042
         LA    R5,L1(R5)                INCR CNT                        57150042
         STH   R5,L2(R3)                STORE NEW CNT                   57200042
SNSRETN  L     R5,XSAVE                 RESTORE WORK REG                57250042
         BR    R14                      RETURN                          57300042
HWSENSE  LH    R5,L4(R3)                GET CNT OF REC WITH HW-VER SNS  57350042
         LA    R5,L1(R5)                INCR CNT                        57400042
         STH   R5,L4(R3)                STORE NEW CNT                   57450042
         B     SNSRETN                                                  57500042
DATAREL  LH    R5,L6(R3)                GET CNT OF REC WITH DAT-REL SNS 57550042
         LA    R5,L1(R5)                INCR CNT                        57600042
         STH   R5,L6(R3)                STORE NEW CNT                   57650042
         B     SNSRETN                                                  57700042
NOTIN    LH    R5,L8(R3)                GET CNT OF REC WITH NOT-INT SNS 57710042
         LA    R5,L1(R5)                INCR CNT                        57720042
         STH   R5,L8(R3)                SAVE NEW CNT                    57730042
         B     SNSRETN                                                  57740042
*********************************************************************** 57750042
*              TEST FOR SHORT OBR RECORD                                57800042
*********************************************************************** 57850042
SHRTREC  TM    SW2B0,HEX20              Q/ IS THIS A SHORT OBR REC      57900042
         BO    SHORTY                   YES                             57950042
         ST    R5,XSAVE                 SAVE WORK REG                   58000042
         LA    R5,NORMREC               GET ADDR OF NEXT SUM TAB ENTRY  58050042
         ST    R5,REG8                  UPDATE PTR TO CURRENT ENTRY     58100042
         L     R5,XSAVE                 RESTORE WORK REG                58150042
SHORTY   BR    R14                                                      58200042
*********************************************************************** 58250042
*              DETERMINE WHAT PART OF SUM TAB TO PROCESS FOR SENSE DATA 58300042
*********************************************************************** 58350042
SNSTYPE  ST    R5,XSAVE                 SAVE WORK REG                   58400042
         TM    XB3SNSTY,HEX01           Q/ IS THIS HW-VER SENSE DATA    58450042
         BO    HWSNS1                   YES                             58500042
         TM    XB3SNSTY,HEX02           Q/ IS THIS DATAREL SENSE DATA   58550042
         BO    DATREL1                  YES                             58600042
         TM    XB3SNSTY,HEX04           Q/ IS THIS NOT-INIT SENSE DATA  58610042
         BO    NOTIN1                   YES                             58620042
         BR    R14                      MUST BE EQ-REL NO CHANGE REQD.  58650042
HWSNS1   LA    R5,HWVERSNS              GET ADDR OF 1ST SUM TAB ENTRY   58700042
         ST    R5,REG8                  UPDATE PTR TO CURRENT ENTRY     58750042
         LA    R3,L42(R3)               GET ADDR OF 1ST SUM TAB ENTRY   58800042
SNSEXIT  L     R5,XSAVE                 RESTORE WORK REG                58850042
         BR    R14                                                      58900042
         BR    R14                      RETURN                          58950042
DATREL1  LA    R5,DRSENSE               GET ADDR OF 1ST SUM TAB ENTRY   59000042
         ST    R5,REG8                  UPDATE PTR TO CURRENT ENTRY     59050042
         LA    R3,L54(R3)               GET ADDR OF 1ST CTR FOR DAT-REL 59100042
         B     SNSEXIT                  RETURN                          59150042
NOTIN1   LA    R5,NISENSE               GET ADDR OF 1ST SUM TABLE ENTRY 59160042
         ST    R5,REG8                  UPDATE PTR TO CURRENT ENTRY     59170042
         LA    R3,L112(R3)              GET ADDR OF 1ST CTR FOR NOT-INT 59180042
         B     SNSEXIT                                                  59190042
*********************************************************************** 59200042
*              SUM BYTES 8 AND 9 FOR HW-VER SENSE DATA                  59250042
*********************************************************************** 59300042
HWCNT    STM   R5,R8,XSAVE              SAVE WORK REGS                  59350042
         CLC   L0(L2,R3),HEXFF          Q/ IS CTR FULL                  59400042
         BE    HWNEXT                   YES                             59450042
         SR    R5,R5                    CLEAR REG                       59500042
         IC    R5,XB8BIT0               PICK UP INCREMENT               59550042
         LH    R6,L0(R3)                PICK UP CTR                     59600042
         AR    R6,R5                    INCR CTR                        59650042
         STH   R6,L0(R3)                STORE CTR                       59700042
HWNEXT   CLC   L2(L2,R3),HEXFF          Q/ IS CTR FULL                  59750042
         BE    HWNEXT1                  YES                             59800042
         SR    R5,R5                    CLEAR REG                       59850042
         IC    R5,XB9BIT0               PICK UP INCR                    59900042
         LH    R6,L2(R3)                PICK UP CTR                     59950042
         AR    R6,R5                    INCR CTR                        60000042
         STH   R6,L2(R3)                STORE CTR                       60050042
HWNEXT1  LM R5,R8,XSAVE                 RESTORE WORK REGS               60100042
         BR    R14                                                      60150042
*********************************************************************** 60200003
*        SUM 8 BITS OF EACH BYTE FOR BYTES 10-19 (HW-VER SENSE DATA)    60210003
*********************************************************************** 60220003
HW1019   STM   R5,R8,XSAVE              SAVE WORK REGS                  60250003
         SR    R7,R7                    CLEAR REG                       60270003
         LA    R7,L10                   LOAD NUMBER OF BYTES TO SUM     60300003
         LA    R5,XB10BIT0              LOAD ADDR OF 1ST BYTE TO SUM    60310003
HWN0     TM    L0(R5),HEX80             Q/ IS BIT 0 ON                  60320003
         BNO   HWN1                     NO, SKIP SUM                    60350003
         BAL   R6,ADD0                  YES, SUM IT                     60370003
HWN1     TM    L0(R5),HEX40             Q/ IS BIT 1 ON                  60400003
         BNO   HWN2                     NO, SKIP SUM                    60410003
         BAL   R6,ADD1                  YES, SUM IT                     60420003
HWN2     TM    L0(R5),HEX20             Q/ IS BIT 2 ON                  60450003
         BNO   HWN3                     NO, SKIP SUM                    60470003
         BAL   R6,ADD2                  YES, SUM IT                     60500003
HWN3     TM    L0(R5),HEX10             Q/ IS BIT 3 ON                  60510003
         BNO   HWN4                     NO, SKIP SUM                    60520003
         BAL   R6,ADD3                  YES, SUM IT                     60550003
HWN4     TM    L0(R5),HEX08             Q/ IS BIT 4 ON                  60570003
         BNO   HWN5                     NO, SKIP SUM                    60600003
         BAL   R6,ADD4                  YES, SUM IT                     60610003
HWN5     TM    L0(R5),HEX04             Q/ IS BIT 5 ON                  60620003
         BNO   HWN6                     NO, SKIP SUM                    60650003
         BAL   R6,ADD5                  YES, SUM IT                     60670003
HWN6     TM    L0(R5),HEX02             Q/ IS BIT 6 ON                  60700003
         BNO   HWN7                     NO, SKIP SUM                    60710003
         BAL   R6,ADD6                  YES, SUM IT                     60720003
HWN7     TM    L0(R5),HEX01             Q/ IS BIT 7 ON                  60750003
         BNO   HWTEST                   NO, SKIP SUM                    60770003
         BAL   R6,ADD7                  YES, SUM IT                     60800003
HWTEST   LA    R5,L1(R5)                GET ADDR OF NEXT BYTE TO SUM    60810003
         BCT   R7,HWN0                  DECR. NUMBER OF BYTES TO SUM    60820003
         LM    R5,R8,XSAVE              DONE, SO RESTORE REGS           60850003
         BR    R14                      AND THEN EXIT                   60870003
ADD0     CLC   L0(L2,R3),HEXFF          Q/ IS COUNTER FULL              60900003
         BE    ADD0R                    YES, SKIP SUM                   60910003
         LH    R8,L0(R3)                PICK UP COUNTER                 60920003
         LA    R8,L1(R8)                INCR COUNT OF BITS FOUND ON     60950003
         STH   R8,L0(R3)                STORE NEW COUNTER VALUE         60970003
ADD0R    BR    R6                       EXIT BACK TO BYTE SUM LOOP      61000003
ADD1     CLC   L2(L2,R3),HEXFF          Q/ IS COUNTER FULL              61010003
         BE    ADD1R                    YES, SKIP SUM                   61020003
         LH    R8,L2(R3)                PICK UP COUNTER                 61050003
         LA    R8,L1(R8)                INCR COUNT OF BITS FOUND ON     61070003
         STH   R8,L2(R3)                STORE NEW COUNTER VALUE         61100003
ADD1R    BR    R6                       EXIT BACK TO BYTE SUM LOOP      61110003
