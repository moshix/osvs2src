*********************************************************************** 00200042
*  DSGEN MACRO DSGEN MACRO DSGEN MACRO DSGEN MACRO DSGEN MACRO          00250042
         MACRO                                                          00300042
&NAME    DSGEN ,                                                        00350042
.*                            THIS MACRO GENERATES ONE DSECT WORD       00400042
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00450042
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00500042
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00550042
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00600042
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00650042
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00700042
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00750042
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00800042
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00850042
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   00900042
         GBLA  &BITS(2600),&DB,&DT,&INITIAL                             00950042
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  01000042
         GBLA  &ITEMNDX(2600),&ITEMXPT                                  01050042
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              01100042
         GBLA  &NRENTS,&SHIFT(2600)                                     01150042
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           01200042
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(2600)                       01250042
         GBLC  &LABELS(2600),&LBL(100),&NRENTSX                         01300042
         LCLB  &ONLY1,&CTIND                                            01350042
         LCLA  &START1,&START2,&END1,&END2                              01400042
         LCLC  &SYMBOL,&EQUSYM                                          01450042
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 01500042
         ACTR  500000                                                   01550042
         AIF   (&ITEMITR EQ 0).START                                    01600042
 MNOTE   20,'DSGEN MACRO OCCURRS AFTER ITEMSORT MACRO HAS BEEN ISSUED.' 01650042
         MEXIT                                                          01700042
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            01750042
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     01800042
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   01850042
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        01900042
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            01950042
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             02000042
         AGO   .SET2A                   BRANCH AROUND                   02050042
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        02100042
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              02150042
         MEXIT                                                          02200042
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X02250042
               ).SET2                   TEST FOR NO SECOND              02300042
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   02350042
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            02400042
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              02450042
         AGO   .LOOP                                                    02500042
.SET2    ANOP                                                           02550042
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 02600042
         AGO   .LOOP                                                    02650042
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             02700042
         MEXIT                                                          02750042
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               02800042
         MEXIT                                                          02850042
.LOOP    ANOP                                                           02900042
&OP      SETA  &OP+1                                                    02950042
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           03000042
&ITEMNBR SETA  &ITEMNBR+1                                               03050042
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            03100042
         MNOTE '**NULL BIT COUNT FIELD**'                               03150042
         MEXIT                                                          03200042
.OK1     ANOP                                                           03250042
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        03300042
         AIF   ('&SYMBOL' NE '').OK                                     03350042
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         03400042
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         03450042
         AGO   .START                   GET NEXT                        03500042
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        03550042
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          03600042
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        03650042
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         03700042
         AGO   .NO3                     BRANCH AROUND                   03750042
.OK2     ANOP                                                           03800042
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              03850042
.NO3     ANOP                                                           03900042
&SYMBOL  EQU   *+&BYTE                                                  03950042
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             04000042
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    04050042
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    04100042
.CNT     ANOP                                                           04150042
&CTIND   SETB  1                        SET COUNT INDICATOR             04200042
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   04250042
&BYTE    SETA  &COUNT/8                                                 04300042
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        04350042
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        04400042
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             04450042
&END1    SETA  &END1+1                  INCREMENT COUNT                 04500042
&OP      SETA  &OP-1                    DECREMENT INCREMENT             04550042
         AGO   .LOOP                    GENERATE NEXT                   04600042
.TSTST   ANOP                                                           04650042
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               04700042
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        04750042
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      04800042
.NSET1   ANOP                                                           04850042
&OP      SETA  &OP-1                    DECREMENT INDEX                 04900042
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           04950042
         AGO   .LOOP                    GO DO NEXT                      05000042
.LPDONE  ANOP                                                           05050042
&ONLY1   SETB  0                        CLEAR INDICATOR                 05100042
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      05150042
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 05200042
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               05250042
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             05300042
&NAME    DS    &COUNT.C                                                 05350042
.END     MEXIT                                                          05400042
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          05450042
         MEXIT                                                          05500042
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   05550042
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   05600042
         MEXIT                                                          05650042
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         05700042
*****               THE FOLLOWING STATEMENT                *****        05750042
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        05800042
.EQU2    ANOP                                                           05850042
&EQUCNT  SETA  0                        CLEAR COUNTER                   05900042
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 05950042
.EQULOP  ANOP                                                           06000042
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 06050042
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     06100042
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                06150042
         AGO   .EQULOP                  NO-GET NEXT                     06200042
.EQUOUT  ANOP                                                           06250042
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  06300042
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              06350042
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 06400042
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             06450042
         AGO   .START                   RETURN FOR NEXT                 06500042
         MEND                                                           06550042
*                                                                       06600042
*********************************************************************** 06650042
* HEX MACRO  HEX MACRO  HEX MACRO  HEX MACRO  HEX MACRO  HEX MACRO      06700042
         MACRO                                                          06750042
         HEX   ,                                                        06800042
.*                            THIS MACRO FACILITATES GENERATION OF THE  06850042
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    06900042
.*                                 CONVERSIONS.                         06950042
.*                            &SYSLIST(&OP,1) - NAME OF DATA ITEM TO BE 07000042
.*                                 CONVERTED                            07050042
.*                            &SYSLIST(&OP,2) - # OF SPACES TO SKIP ON  07100042
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   07150042
.*                                 BACK UP FROM COLUMN FOR START OF     07200042
.*                                 PRINTING. THE LETTER B PRECEEDS THE  07250042
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  07300042
.*                                 DESIRED. OMISSION OF THIS OPERAND    07350042
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  07400042
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  07450042
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 07500042
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    07550042
.*                                 IMPLICITLY B).                       07600042
.*                            &SYSLIST(&OP,3) - NUMBER OF BYTES TO      07650042
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     07700042
.*                                 WITH AN L,R, OR B TO STIPULATE       07750042
.*                                 PRINTING OF THE LEFT HALF, RIGHT     07800042
.*                                 HALF, OR BOTH HALVES OF THE BYTE     07850042
.*                                 RESPECTIVELY. OMISSION OF THIS       07900042
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  07950042
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 08000042
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 08050042
.*                                 SARY. THIS MACRO MUST BE THE FIRST   08100042
.*                                 MACRO INSTRUCTION ISSUED FOR THE     08150042
.*                                 SUBJECT LINE(S).                     08200042
         GBLA  &BITS(2600),&DB,&DT,&INITIAL                             08250042
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  08300042
         GBLA  &ITEMNDX(2600),&ITEMXPT                                  08350042
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              08400042
         GBLA  &NRENTS,&SHIFT(2600)                                     08450042
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           08500042
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(2600)                       08550042
         GBLC  &LABELS(2600),&LBL(100),&NRENTSX                         08600042
         LCLB  &B,&SUB                                                  08650042
         LCLA  &IBITS,&ISHIFT,&K,&T,&CODE,&BYTESP,&OP                   08700042
         LCLC  &S,&LETTER,&ARG                                          08750042
       ACTR  500000                                                     08800042
         AIF   (&DIAGTST EQ 0).NOMSG                                    08850042
         AIF   (&INITIAL EQ 0).NOMSG                                    08900042
         AIF   (&DIAGLIN EQ 1).NOMSG                                    08950042
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  09000042
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     09050042
*        LINE                                                           09100042
         LINE                                                           09150042
.NOMSG   ANOP                                                           09200042
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  09250042
&SUB     SETB  1                                                        09300042
.LOOP    ANOP                                                           09350042
&OP      SETA  &OP+1                                                    09400042
&ARG     SETC  '&SYSLIST(&OP,1)'                                        09450042
         ITEMFIND  &ARG                                                 09500042
         AIF   (&ITEMXPT EQ 0).MEXIT                                    09550042
.FOUND   ANOP                                                           09600042
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         09650042
&IBITS   SETA  &BITS(&ITEMXPT)                                          09700042
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).ALIGN                     09750042
.ERR     MNOTE '**DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY**'          09800042
.MEXIT   MEXIT                                                          09850042
.ALIGN   ANOP                                                           09900042
&CODE    SETA  1                                                        09950042
         AIF   (&SUB EQ 1).SUBA                                         10000042
&K       SETA  K'&SYSLIST(3)                                            10050042
         AGO   .SUBB                                                    10100042
.SUBA    ANOP                                                           10150042
&K       SETA  K'&SYSLIST(&OP,3)                                        10200042
.SUBB    ANOP                                                           10250042
         AIF   (&K EQ 1).ERR3                                           10300042
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            10350042
         MNOTE '**BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL**' 10400042
         MEXIT                                                          10450042
.OK      AIF   (&SUB EQ 1).SUBC                                         10500042
         AIF   ('&SYSLIST(3)' EQ '').USEDEFN                            10550042
&LETTER  SETC  '&SYSLIST(3)'(&K,1)                                      10600042
&S       SETC  '&SYSLIST(3)'(1,&K-1)                                    10650042
         AGO   .SUBD                                                    10700042
.SUBC    ANOP                                                           10750042
         AIF   ('&SYSLIST(&OP,3)' EQ '').USEDEFN                        10800042
&LETTER  SETC  '&SYSLIST(&OP,3)'(&K,1)                                  10850042
&S       SETC  '&SYSLIST(&OP,3)'(1,&K-1)                                10900042
.SUBD    ANOP                                                           10950042
&BYTESP  SETA  &S                                                       11000042
         AIF   ('&LETTER' EQ 'B').BOTH                                  11050042
         AIF   ('&LETTER' EQ 'L').LEFT                                  11100042
         AIF   ('&LETTER' EQ 'R').RIGHT                                 11150042
.ERR3    MNOTE '**PARAMETER 3 INCOMPLETE-DSGEN DEFINITION USED**'       11200042
         AGO   .USEDEFN                                                 11250042
.RIGHT   ANOP                                                           11300042
&CODE    SETA  2                                                        11350042
         AGO   .L                                                       11400042
.LEFT    ANOP                                                           11450042
&CODE    SETA  1                                                        11500042
         AGO   .L                                                       11550042
.USEDEFN ANOP                                                           11600042
&BYTESP  SETA  &IBITS/8                                                 11650042
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       11700042
&CODE    SETA  3                                                        11750042
&T       SETA  &BYTESP*2-1                                              11800042
&S       SETC  '&T'                                                     11850042
.OK2     AIF   (&ISHIFT EQ 0).L                                         11900042
&CODE    SETA  2                                                        11950042
.L       ANOP                                                           12000042
&B       SETB  1                                                        12050042
         AIF   (&SUB EQ 1).SUBE                                         12100042
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             12150042
&K       SETA  K'&SYSLIST(2)                                            12200042
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              12250042
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                12300042
         AGO   .SUBF                                                    12350042
.SUBE    ANOP                                                           12400042
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         12450042
&K       SETA  K'&SYSLIST(&OP,2)                                        12500042
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          12550042
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            12600042
.SUBF    ANOP                                                           12650042
.BACKUP  ANOP                                                           12700042
&NRENTS  SETA  &NRENTS+1                                                12750042
         AIF   (&BYTESP GT 0 AND &BYTESP LT 16).SUB0                    12800042
         MNOTE '**NO. OF BYTES OUTSIDE RANGE OF 1-15**'                 12850042
         MEXIT                                                          12900042
.SUB0    AIF   (&SUB EQ 1).SUBG                                         12950042
         DC    AL.12(&SYSLIST(1)-&DSECT),XL.4'A'                        13000042
         AGO   .SUBH                                                    13050042
.SUBG    ANOP                                                           13100042
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'A'                    13150042
.SUBH    ANOP                                                           13200042
         DC    BL.1'&B',FL.7'&S',AL.4(&BYTESP),FL.4'&CODE'              13250042
.OUT     ANOP                                                           13300042
&ITEMXPT     SETA  0                                                    13350042
         AIF   (&SUB EQ 0).OUTA                                         13400042
         AIF   (&OP LT N'&SYSLIST).LOOP                                 13450042
.OUTA    ANOP                                                           13500042
         MEND                                                           13550042
