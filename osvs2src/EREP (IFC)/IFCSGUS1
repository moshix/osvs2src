*********************************************************************** 40200042
*   SPECIAL MACRO    SPECIAL MACRO    SPECIAL MACRO    SPECIAL MACRO  * 40250042
         MACRO                                                          40300042
         SPECIAL  &ROUTINE,&PARAM,&SPACE=                               40350042
.*                            THIS MACRO GENERATES A SPECIAL ROUTINE    40400042
.*                                 ENTRY TO BE PROCESSED BY THE SUM. M. 40450042
.*                            &ROUTINE - SYMBOL ON SPECIAL ROUTINE EWT. 40500042
.*                            &PARAM - AN 8 BIT BINARY REPRESENTATION   40550042
.*                                 TO OCCUPY THE 'MASK' FIELD BUT TO BE 40600042
.*                                 INTERPRETED AS AN ARGUMENT OF THE    40650042
.*                                 SPECIAL ROUTINE.                     40700042
.*                            &SPACE - THE NUMBER OF 2BYTE COUNTERS TO  40750042
.*                                 ALLOCATE FOR REQUIRED PROCESSING.    40800042
         GBLC  &CSECT                                                   40850042
         GBLA  &COUNTR                                                  40900042
         LCLA  &ADD                                                     40950042
         DC    AL1(128)                                                 41000042
         DC    BL.8'&PARAM'                                             41050042
         DC    AL2(&ROUTINE-&CSECT)                                     41100042
         DC    AL2(&COUNTR)                                             41150042
&ADD     SETA  1                                                        41200042
         AIF   ('&SPACE' EQ '').NORM                                    41250042
&ADD     SETA  &SPACE                                                   41300042
.NORM    ANOP                                                           41350042
&COUNTR  SETA  &COUNTR+&ADD                                             41400042
         MEND                                                           41450042
*                                                                       41500042
*********************************************************************** 41550042
*  SUM MACRO  SUM MACRO  SUM MACRO  SUM MACRO  SUM MACRO  SUM MACRO   * 41600042
         MACRO                                                          41650042
         SUM   &LOGITEM,&MASK                                           41700042
.*                            THIS MACRO GENERATES A NORMAL EDIT ENTRY  41750042
.*                                 FOR PROCESSING BY THE SUMMARY MODULE 41800042
.*                            &LOGITEM - NAME OF DATA ITEM TO BE SUMMED 41850042
.*                            &MASK - BYTE MASK FOR BITS TO BE TESTED.  41900042
.*                                 THIS OPERAND SHOULD BE OMITTED FOR   41950042
.*                                 SINGLE BIT ITEMS IN WHICH CASE THE   42000042
.*                                 MASK WILL BE SET ACCORDING TO THE    42050042
.*                                 OFF BYTE BIT DISPLACEMENT OF THE     42100042
.*                                ITEM AS DEFINED BY THE MACRO 'DSGEN'  42150042
         GBLA  &COUNTR,&ITEMNBR,&BITS(2600),&SHIFT(2600)                42200042
         GBLC  &ITEM(2600),&DSECT                                       42250042
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           42300042
.LOOP    ANOP                                                           42350042
&NDX     SETA  &NDX+1                                                   42400042
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      42450042
         AIF   (&NDX LT &ITEMNBR).LOOP                                  42500042
         MNOTE *,'LOGITEM NOT FOUND'                                    42550042
         MEXIT                                                          42600042
.FOUND   ANOP                                                           42650042
         DC    AL1(0)                                                   42700042
&ISHIFT  SETA  &SHIFT(&NDX)                                             42750042
&IBITS   SETA  &BITS(&NDX)                                              42800042
         AIF   ('&MASK' EQ '').FORM                                     42850042
         DC    B'&MASK'                                                 42900042
         AGO   .ITEM                                                    42950042
.FORM    ANOP                                                           43000042
&BITNBR  SETA  &ISHIFT-7                                                43050042
&BITNBR  SETA  0-&BITNBR                                                43100042
&M       SETA  1                                                        43150042
.LOOP2   ANOP                                                           43200042
         AIF   (&BITNBR EQ 0).GOTIT                                     43250042
&M       SETA  &M*2                                                     43300042
&BITNBR  SETA  &BITNBR-1                                                43350042
         AGO   .LOOP2                                                   43400042
.GOTIT   ANOP                                                           43450042
         DC    AL1(&M)                                                  43500042
.ITEM    DC    AL2(&LOGITEM-&DSECT)                                     43550042
         DC    AL2(&COUNTR)                                             43600042
&COUNTR  SETA  &COUNTR+1                                                43650042
         MEND                                                           43700042
         GBLC  &DSECT,&CSECT                                            43750042
&CSECT   SETC  'IFCSGUS1'                                               43800042
&DSECT   SETC  'GUS1'                                                   43850042
IFCSGUS1 CSECT                                                          43900042
