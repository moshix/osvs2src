*********************************************************************** 13650042
* BIN MACRO  BIN MACRO  BIN MACRO  BIN MACRO  BIN MACRO  BIN MACRO      13700042
         MACRO                                                          13750042
         BIN   ,                                                        13800042
.*                                 &SYSLIST(&OP,1) - NAME OF LOG ITEM   13850042
.*                                      RECORD DSECT                    13900042
.*                                 $SYSLIST(&OP,2) - NBR OF SPACES      13950042
.*                                      TO SKIP ON MULTIPLE             14000042
.*                                      PRINT OR NBR OF SPACES          14050042
.*                                      TO BACK UP FROM COLUMN FOR      14100042
.*                                      START OF PRINTING. THE LETTER   14150042
.*                                      B PRECEEDS THE NUMBER (B5) IF   14200042
.*                                      THE LATTER IS DESIRED. OMISSION 14250042
.*                                      OF THIS OPERAND FORCES OPTION 2 14300042
.*                                      WITH BACKUP SPACING EQUAL TO    14350042
.*                                 THE NUMBER OF BITS IN THE ITEM       14400042
.*                                 DEFINITION (VIA MACRO 'DSGEN').      14450042
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     14500042
.*                                 TOTAL NUMBER OF BITS TO PRINT.       14550042
         GBLA  &BITS(2600),&DB,&DT,&INITIAL                             14600042
         GBLA  &ITEMITR,&ITEMFST(20),&ITEMNBR,&ITEMMDX                  14650042
         GBLA  &ITEMNDX(2600),&ITEMXPT                                  14700042
         GBLA  &LABELNR,&LASTCC,&LB,&LBLCTR(100),&LPTR,&LT              14750042
         GBLA  &NRENTS,&SHIFT(2600)                                     14800042
         GBLB  &DIAGITF,&DIAGLIN,&DIAGSET,&DIAGTST,&FSW,&TEST           14850042
         GBLC  &CARRCON,&CSECT,&DSECT,&ITEM(2600)                       14900042
         GBLC  &LABELS(2600),&LBL(100),&NRENTSX                         14950042
         LCLB  &B,&SUB                                                  15000042
         LCLA  &IBITS,&ISHIFT,&K                                        15050042
         LCLA  &TEMP,&T,&OP                                             15100042
         LCLC  &S,&ARG                                                  15150042
          ACTR  500000                                                  15200042
         AIF   (&DIAGTST EQ 0).NOMSG                                    15250042
         AIF   (&INITIAL EQ 0).NOMSG                                    15300042
         AIF   (&DIAGLIN EQ 1).NOMSG                                    15350042
         MNOTE '**NO LINE ASSOCIATED WITH THIS DATA**'                  15400042
*****THE FOLLOWING GENERATED FOR TEST PURPOSES*****                     15450042
*        LINE                                                           15500042
         LINE                                                           15550042
.NOMSG   ANOP                                                           15600042
         AIF   (K'&SYSLIST(1) EQ K'&SYSLIST(1,1)).LOOP                  15650042
&SUB     SETB  1                                                        15700042
.LOOP    ANOP                                                           15750042
&OP      SETA  &OP+1                                                    15800042
&ARG     SETC  '&SYSLIST(&OP,1)'                                        15850042
         ITEMFIND &ARG                                                  15900042
         AIF   (&ITEMXPT EQ 0).MEXIT                                    15950042
&ISHIFT  SETA  &SHIFT(&ITEMXPT)                                         16000042
&IBITS   SETA  &BITS(&ITEMXPT)                                          16050042
         AIF   (&SUB EQ 1).SUB00                                        16100042
         AIF   ('&SYSLIST(3)' EQ '').SETT                               16150042
&IBITS   SETA  &SYSLIST(3)                                              16200042
         AGO   .SETT                                                    16250042
.SUB00   AIF   ('&SYSLIST(&OP,3)' EQ '').SETT                           16300042
&IBITS   SETA  &SYSLIST(&OP,3)                                          16350042
.SETT    ANOP                                                           16400042
&T       SETA  &IBITS-1                                                 16450042
&B       SETB  0                                                        16500042
         AIF   (&IBITS GT 0 AND &IBITS LT 16).SUB0                      16550042
         MNOTE '**NO. OF BITS OUTSIDE RANGE OF 1-15**'                  16600042
.MEXIT   MEXIT                                                          16650042
.SUB0    AIF   (&SUB EQ 1).SUBA                                         16700042
         AIF   ('&SYSLIST(2)' EQ '').BACKUP                             16750042
&K       SETA  K'&SYSLIST(2)                                            16800042
&B       SETB  ('&SYSLIST(2)'(1,1) EQ 'B')                              16850042
&S       SETC  '&SYSLIST(2)'(&B+1,&K-&B)                                16900042
         AGO   .SUBB                                                    16950042
.SUBA    ANOP                                                           17000042
         AIF   ('&SYSLIST(&OP,2)' EQ '').BACKUP                         17050042
&K       SETA  K'&SYSLIST(&OP,2)                                        17100042
&B       SETB  ('&SYSLIST(&OP,2)'(1,1) EQ 'B')                          17150042
&S       SETC  '&SYSLIST(&OP,2)'(&B+1,&K-&B)                            17200042
.SUBB    ANOP                                                           17250042
&T       SETA  &S                                                       17300042
.BACKUP  ANOP                                                           17350042
&NRENTS  SETA  &NRENTS+1                                                17400042
&TEMP    SETA  &ISHIFT                                                  17450042
         AIF   (&IBITS NE 1 OR &B NE 0).BIN2                            17500042
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'&TEMP'                17550042
         AGO   .OUT                                                     17600042
         AIF   (&SUB EQ 1).BIN2                                         17650042
         AIF   ('&SYSLIST(2)' NE '').BIN3                               17700042
         AGO   .BIN2A                                                   17750042
.BIN2    AIF   ('&SYSLIST(&OP,2)' NE '').BIN3                           17800042
.BIN2A   ANOP                                                           17850042
&B       SETB  1                                                        17900042
.BIN3    ANOP                                                           17950042
         DC    AL.12(&SYSLIST(&OP,1)-&DSECT),XL.4'C'                    18000042
         DC    BL.1'&B',AL.7(&T),AL.4(&IBITS),FL.4'&ISHIFT'             18050042
.OUT     ANOP                                                           18100042
&ITEMXPT     SETA  0                                                    18150042
         AIF   (&SUB EQ 0).OUTA                                         18200042
         AIF   (&OP LT N'&SYSLIST).LOOP                                 18250042
.OUTA    ANOP                                                           18300042
         MEND                                                           18350042
