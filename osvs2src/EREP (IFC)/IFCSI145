         MACRO                                                          00432002
&NAME    DSGEN                                                          00504002
.*                            THIS MACRO GENERATES ONE DSECT WORD       00576002
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00648002
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00720002
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00792002
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00864002
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00936002
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 01008002
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      01080002
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  01152002
.*                                 THE LOG ITEMS MAY THEN BE USED IN    01224002
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   01296002
         GBLC &ITEM(1000)                                               01368002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         01440002
         LCLB  &ONLY1,&CTIND                                            01512002
         LCLA  &START1,&START2,&END1,&END2                              01584002
         LCLC  &SYMBOL,&EQUSYM                                          01656002
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 01728002
         ACTR  100000                                                   01800002
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            01872002
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     01944002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' AND '&SYSLIST(&OP+1,4)' EQ '').ERR1  02016002
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   02088002
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        02160002
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            02232002
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             02304002
         AGO   .SET2A                   BRANCH AROUND                   02376002
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              02448002
         MEXIT                                                          02520002
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X02592002
               ).SET2                   TEST FOR NO SECOND              02664002
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   02736002
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        02808002
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            02880002
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              02952002
         AGO   .LOOP                                                    03024002
.SET2    ANOP                                                           03096002
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 03168002
         AGO   .LOOP                                                    03240002
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             03312002
         MEXIT                                                          03384002
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               03456002
         MEXIT                                                          03528002
.LOOP    ANOP                                                           03600002
&OP      SETA  &OP+1                                                    03672002
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           03744002
&ITEMNBR SETA  &ITEMNBR+1                                               03816002
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            03888002
         MNOTE '**NULL BIT COUNT FIELD**'                               03960002
         MEXIT                                                          04032002
.OK1     ANOP                                                           04104002
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        04176002
         AIF   ('&SYMBOL' NE '').OK                                     04248002
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         04320002
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         04392002
         AGO   .START                   GET NEXT                        04464002
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        04536002
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          04608002
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        04680002
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         04752002
         AGO   .NO3                     BRANCH AROUND                   04824002
.OK2     ANOP                                                           04896002
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              04968002
.NO3     ANOP                                                           05040002
&SYMBOL  EQU   *+&BYTE                                                  05112002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             05184002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    05256002
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    05328002
.CNT     ANOP                                                           05400002
&CTIND   SETB  1                        SET COUNT INDICATOR             05472002
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   05544002
&BYTE    SETA  &COUNT/8                                                 05616002
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        05688002
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        05760002
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             05832002
&END1    SETA  &END1+1                  INCREMENT COUNT                 05904002
&OP      SETA  &OP-1                    DECREMENT INCREMENT             05976002
         AGO   .LOOP                    GENERATE NEXT                   06048002
.TSTST   ANOP                                                           06120002
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               06192002
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        06264002
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      06336002
.NSET1   ANOP                                                           06408002
&OP      SETA  &OP-1                    DECREMENT INDEX                 06480002
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           06552002
         AGO   .LOOP                    GO DO NEXT                      06624002
.LPDONE  ANOP                                                           06696002
&ONLY1   SETB  0                        CLEAR INDICATOR                 06768002
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      06840002
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 06912002
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               06984002
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             07056002
&NAME    DS    &COUNT.C                                                 07128002
.END     MEXIT                                                          07200002
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          07272002
         MEXIT                                                          07344002
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   07416002
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   07488002
         MEXIT                                                          07560002
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         07632002
*****               THE FOLLOWING STATEMENT                *****        07704002
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        07776002
.EQU2    ANOP                                                           07848002
&EQUCNT  SETA  0                        CLEAR COUNTER                   07920002
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 07992002
.EQULOP  ANOP                                                           08064002
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 08136002
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     08208002
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                08280002
         AGO   .EQULOP                  NO-GET NEXT                     08352002
.EQUOUT  ANOP                                                           08424002
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  08496002
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              08568002
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 08640002
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             08712002
         AGO   .START                   RETURN FOR NEXT                 08784002
         MEND                                                           08856002
         MACRO                                                          08928002
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      09000002
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     09072002
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    09144002
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 09216002
.*                                 ENTRIES. LASTLY IT DETERMINES THE    09288002
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    09360002
.*                                 THE PREVIOUS LINE AND STORES THAT #  09432002
.*                                 INTO THE APPROPRIATE BYTE.           09504002
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    09576002
.*                                 DATA.                                09648002
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 09720002
.*                                 DATA COLUMN STARTING CHARACTERS.     09792002
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   09864002
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 09936002
.*                                 STARTING CHARACTERS.                 10008002
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   10080002
.*                                 TO BEGIN NEXT PRINTED LINE.          10152002
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       10224002
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   10296002
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 10368002
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   10440002
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 10512002
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    10584002
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   10656002
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   10728002
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  10800002
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   10872002
.*                                 THE SUBJECT LINE                     10944002
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  11016002
         GBLA  &LASTCC,&INITIAL                                         11088002
         GBLC  &NRENTSX                                                 11160002
         GBLC  &CARRCON                                                 11232002
         GBLB  &TEST                                                    11304002
         LCLA  &S                                                       11376002
&NAME    DS    0C                                                       11448002
         AIF   (&INITIAL EQ 0).FIRSTLN                                  11520002
         LINEND                                                         11592002
         AGO   .NTFIRST                                                 11664002
.FIRSTLN ANOP                                                           11736002
&NRENTSX SETC  'N&SYSNDX'                                               11808002
&INITIAL SETA  1                                                        11880002
N&SYSNDX EQU   *                                                        11952002
.NTFIRST ANOP                                                           12024002
         DC    AL1(0)                                                   12096002
         AIF   ('&DATA(1)' EQ '').CKDTABS                               12168002
         AIF   (&DATA(1) EQ &DB).CKDTABS                                12240002
&DB      SETA  &DATA(1)                                                 12312002
         AGO   .AA                                                      12384002
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                12456002
         AIF   (&DATA(2) EQ &DT).NCDATA                                 12528002
         AGO   .CC                                                      12600002
.AA      AIF   ('&DATA(2)' EQ '').BB                                    12672002
         AIF   (&DATA(2) EQ &DT).BB                                     12744002
.CC      ANOP                                                           12816002
&DT      SETA  &DATA(2)                                                 12888002
.BB      DC    XL2'FFFE'                                                12960002
         DC    FL1'&DB'                                                 13032002
         DC    FL1'&DT'                                                 13104002
&NRENTS  SETA  &NRENTS+1                                                13176002
.NCDATA  ANOP                                                           13248002
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              13320002
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               13392002
&LB      SETA  &LABEL(1)                                                13464002
         AGO   .AAA                                                     13536002
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              13608002
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               13680002
         AGO   .CCC                                                     13752002
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  13824002
         AIF   (&LABEL(2) EQ &LT).BBB                                   13896002
.CCC     ANOP                                                           13968002
&LT      SETA  &LABEL(2)                                                14040002
.BBB     DC    XL2'FFFF'                                                14112002
         DC    FL1'&LB'                                                 14184002
         DC    FL1'&LT'                                                 14256002
&NRENTS  SETA  &NRENTS+1                                                14328002
.NCLABEL ANOP                                                           14400002
         AIF   ('&SKIP' NE '').CONT                                     14472002
         AIF   (&TEST EQ 0).NOCHANG                                     14544002
&S       SETA  &LASTCC                                                  14616002
         AGO   .DEFAULT                                                 14688002
.CONT    ANOP                                                           14760002
         AIF   (&TEST EQ 0).AB                                          14832002
&S       SETA  &SKIP                                                    14904002
.DEFAULT ANOP                                                           14976002
         ORG   &CARRCON+3                                               15048002
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 15120002
         ORG                                                            15192002
&LASTCC  SETA  &S                                                       15264002
&TEST    SETB  0                                                        15336002
         AGO   .DD                                                      15408002
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               15480002
A&SYSNDX DC    XL2'FFFB'                                                15552002
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              15624002
         DC    FL1'9'                                                   15696002
&LASTCC  SETA  &SKIP                                                    15768002
&TEST    SETB  1                                                        15840002
&CARRCON SETC  'A&SYSNDX'                                               15912002
&NRENTS  SETA  &NRENTS+1                                                15984002
.DD      ANOP                                                           16056002
.NOCHANG ANOP                                                           16128002
         MEND                                                           16200002
         MACRO                                                          16272002
         BIN   &LOGITEM,&SKIP$OR                                        16344002
.*                                 &LOGITEM - NAME OF ITEM IN LOG       16416002
.*                                      RECORD DSECT                    16488002
.*                                 &SKIP$OR - NBR OF SPACES TO SKIP ON  16560002
.*                                 MULTIPLE PRINT OR NBR OF SPACES      16632002
.*                                      TO BACK UP FROM COLUMN FOR      16704002
.*                                      START OF PRINTING. THE LETTER   16776002
.*                                      B PRECEEDS THE NUMBER (B5) IF   16848002
.*                                      THE LATTER IS DESIRED. OMISSION 16920002
.*                                      OF THIS OPERAND FORCES OPTION 2 16992002
.*                                      WITH BACKUP SPACING EQUAL TO    17064002
.*                                 THE NUMBER OF BITS IN THE ITEM       17136002
.*                                 DEFINITION (VIA MACRO 'DSGEN').      17208002
.*                            A LIMIT OF 15 BITS IS IMPOSED FOR THE     17280002
.*                                 TOTAL NUMBER OF BITS TO PRINT.       17352002
         GBLC &ITEM(1000)                                               17424002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 17496002
         GBLC  &DSECT                                                   17568002
         LCLB  &B                                                       17640002
         LCLA  &IBITS,&NDX,&ISHIFT,&K                                   17712002
         LCLA  &TEMP                                                    17784002
         LCLC  &S                                                       17856002
.LOOP    ANOP                                                           17928002
&NDX     SETA  &NDX+1                                                   18000002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      18072002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  18144002
         MNOTE *,'YOU BLEW IT'                                          18216002
         ROUTINE DUM,SKIPPER                                            18288002
         MEXIT                                                          18360002
.FOUND   ANOP                                                           18432002
&ISHIFT  SETA  &SHIFT(&NDX)                                             18504002
&IBITS   SETA  &BITS(&NDX)                                              18576002
&S       SETC  '&IBITS'                                                 18648002
&B       SETB  1                                                        18720002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                18792002
&K       SETA  K'&SKIP$OR                                               18864002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 18936002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   19008002
.BACKUP  ANOP                                                           19080002
&NRENTS  SETA  &NRENTS+1                                                19152002
&TEMP    SETA  &ISHIFT+1                                                19224002
A&SYSNDX DC    AL.12(&LOGITEM-&DSECT),XL.4'&TEMP'                       19296002
         AIF (&IBITS EQ 1 AND &B EQ 0).OUT                              19368002
         ORG   A&SYSNDX                                                 19440002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'C'                           19512002
         DC    BL.1'&B',AL.7(&S),AL.4(&IBITS),FL.4'&ISHIFT'             19584002
.OUT     ANOP                                                           19656002
         MEND                                                           19728002
         MACRO                                                          19800002
         HEX   &LOGITEM,&SKIP$OR,&BYTES                                 19872002
.*                            THIS MACRO FACILITATES GENERATION OF THE  19944002
.*                                 TABLE ENTRIES SPECIFYING HEX TYPE    20016002
.*                                 CONVERSIONS.                         20088002
.*                            &LOGITEM - NAME OF DATA ITEM TO BE        20160002
.*                                 CONVERTED                            20232002
.*                            &SKIP$OR - NUMBER OF SPACES TO SKIP ON    20304002
.*                                 MULTIPLE PRINT OR NBR OF SPACES TO   20376002
.*                                 BACK UP FROM COLUMN FOR START OF     20448002
.*                                 PRINTING. THE LETTER B PRECEEDS THE  20520002
.*                                 NUMBER (B8) IF THE LATTER OPTION IS  20592002
.*                                 DESIRED. OMISSION OF THIS OPERAND    20664002
.*                                 FORCES OPTION 2 WITH BACKUP SPACING  20736002
.*                                 EQUAL TO THE NUMBER OF BYTES SPECI-  20808002
.*                                 FIED BY THE NEXT OPERAND (TWICE THIS 20880002
.*                                 NUMBER IF SUFFIX IS EXPLICITLY OR    20952002
.*                                 IMPLICITLY B).                       21024002
.*                            &BYTES - NUMBER OF TOTAL INPUT BYTES TO   21096002
.*                                 PROCESS. THIS NUMBER IS SUFFIXED     21168002
.*                                 WITH AN L,R, OR B TO STIPULATE       21240002
.*                                 PRINTING OF THE LEFT HALF, RIGHT     21312002
.*                                 HALF, OR BOTH HALVES OF THE BYTE     21384002
.*                                 RESPECTIVELY. OMISSION OF THIS       21456002
.*                                 OPERAND DEFAULTS TO B WITH THE NUM-  21528002
.*                                 BER OF BYTES EQUAL TO THE ITEM LNGTH 21600002
.*                            A NEW ENTRY IS GENERATED ONLY WHEN NECES- 21672002
.*                                 SARY. THIS MACRO MUST BE THE FIRST   21744002
.*                                 MACRO INSTRUCTION ISSUED FOR THE     21816002
.*                                 SUBJECT LINE(S).                     21888002
         GBLC &ITEM(1000),&DSECT                                        21960002
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000),&NRENTS                 22032002
         LCLB  &B                                                       22104002
         LCLA  &IBITS,&NDX,&ISHIFT,&K,&T,&CODE,&BYTESP                  22176002
         LCLC  &S,&LETTER                                               22248002
.LOOP    ANOP                                                           22320002
&NDX     SETA  &NDX+1                                                   22392002
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      22464002
         AIF   (&NDX LT &ITEMNBR).LOOP                                  22536002
         MNOTE *,'LOG ITEM NOT DEFINED'                                 22608002
         MEXIT                                                          22680002
.FOUND   ANOP                                                           22752002
&ISHIFT  SETA  &SHIFT(&NDX)                                             22824002
&IBITS   SETA  &BITS(&NDX)                                              22896002
&CODE    SETA  1                                                        22968002
&K       SETA  K'&BYTES                                                 23040002
         AIF   (&IBITS-(&IBITS/4)*4 EQ 0).OK                            23112002
         MNOTE *,'BIT LENGTH SPECIFICATION INCONSISTENT WITH CONTROL'   23184002
         MEXIT                                                          23256002
.OK      AIF   ('&BYTES' EQ '').USEDEFN                                 23328002
&LETTER  SETC  '&BYTES'(&K,1)                                           23400002
&S       SETC  '&BYTES'(1,&K-1)                                         23472002
&BYTESP  SETA  &S                                                       23544002
         AIF   ('&LETTER' EQ 'B').BOTH                                  23616002
         AIF   (&ISHIFT EQ 0 OR &ISHIFT EQ 4).OK2                       23688002
.ERR     MNOTE *,'DATA NOT ALLIGNED ON HEX OR BYTE BOUNDARY'            23760002
         MEXIT                                                          23832002
.USEDEFN ANOP                                                           23904002
&BYTESP  SETA  &IBITS/8                                                 23976002
.BOTH    AIF   (&ISHIFT NE 0).ERR                                       24048002
&CODE    SETA  3                                                        24120002
&T       SETA  &BYTESP*2                                                24192002
&S       SETC  '&T'                                                     24264002
.OK2     AIF   (&ISHIFT EQ 0).L                                         24336002
&CODE    SETA  2                                                        24408002
.L       ANOP                                                           24480002
&B       SETB  1                                                        24552002
         AIF   ('&SKIP$OR' EQ '').BACKUP                                24624002
&K       SETA  K'&SKIP$OR                                               24696002
&B       SETB  ('&SKIP$OR'(1,1) EQ 'B')                                 24768002
&S       SETC  '&SKIP$OR'(&B+1,&K-&B)                                   24840002
.BACKUP  ANOP                                                           24912002
&NRENTS  SETA  &NRENTS+1                                                24984002
         DC    AL.12(&LOGITEM-&DSECT),XL.4'A'                           25056002
         DC    BL.1'&B',FL.7'&S',FL.4'&BYTESP',FL.4'&CODE'              25128002
.OUT     ANOP                                                           25200002
         MEND                                                           25272002
         MACRO                                                          25344002
&NAME    ROUTINE &LOGITEM,&RNAME                                        25416002
         GBLA  &NRENTS                                                  25488002
         GBLC  &DSECT,&CSECT                                            25560002
&NAME    DC    AL.12(&LOGITEM-&DSECT),XL.4'D'                           25632002
         DC    AL2(&RNAME-&CSECT)                                       25704002
