         MACRO                                                          25920002
         LABEL &LABEL1,&LABEL2,&LABEL3,&LABEL4                          25992002
.*                            THIS MACRO ALLOWS UP TO FOUR LABELS TO    26064002
.*                                 BE SPECIFIED FOR A GIVEN LINE. THE   26136002
.*                                 LABELS SHOULD BE ENCLOSED IN QUOTA-  26208002
.*                                 TION MARKS. THE COLUMN AND LINE END  26280002
.*                                 CONTROLS WILL BE INSERTED BY THE     26352002
.*                                 MACRO. OMITTED OPERANDS MEAN NO      26424002
.*                                 LABEL REQUIRED FOR RESPECTIVE        26496002
.*                                 COLUMN. THIS MACRO WILL ALSO ALERT   26568002
.*                                 THE PROGRAMMER TO EXACTLY WHERE THE  26640002
.*                                 TABLE MUST BE BROKEN IF NECESSARY    26712002
.*                                 TO AVOID EXCEEDING THE MAXIMUM LIMIT 26784002
         GBLC &LABELS(1000)                                             26856002
         GBLA  &LABELNR                                                 26928002
         GBLA  &NRENTS                                                  27000002
         LCLC  &MARGIN,&TACK                                            27072002
         LCLC  &TEXT                                                    27144002
         LCLA  &OP,&K,&START,&NRCHAR,&REMAIN                            27216002
&MARGIN  SETC  '*'                                                      27288002
.LOOPOUT ANOP                                                           27360002
&OP      SETA  &OP+1                                                    27432002
&K       SETA  K'&SYSLIST(&OP)                                          27504002
         AIF (&K GE 2).NORM                                             27576002
&K       SETA  2                                                        27648002
.NORM    ANOP                                                           27720002
&START   SETA  2                                                        27792002
         AIF (&OP LT N'&SYSLIST).NOTLAST                                27864002
&MARGIN  SETC  '$'                                                      27936002
.NOTLAST ANOP                                                           28008002
&NRCHAR  SETA  8                                                        28080002
&TACK    SETC  ''                                                       28152002
.LOOPIN  ANOP                                                           28224002
&REMAIN  SETA  &K-&START                                                28296002
         AIF (&REMAIN GE 8).MORE                                        28368002
&TACK    SETC  '&MARGIN'                                                28440002
&NRCHAR  SETA  &REMAIN                                                  28512002
.MORE    ANOP                                                           28584002
&TEXT    SETC  ''                                                       28656002
         AIF (&REMAIN EQ 0).ENDTEXT                                     28728002
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          28800002
.ENDTEXT ANOP                                                           28872002
&LABELNR SETA  &LABELNR+1                                               28944002
&LABELS(&LABELNR) SETC '&TEXT'.'&TACK'                                  29016002
&START   SETA  &START+8                                                 29088002
         AIF ('&TACK' EQ '').LOOPIN                                     29160002
         AIF ('&TACK' EQ '*').LOOPOUT                                   29232002
         MEND                                                           29304002
         MACRO                                                          29376002
         LSTART                                                         29448002
.*                            THIS MACRO IS ISSUED WHEN THE LABEL AREA  29520002
.*                                 OF THE TABLE IS TO BEGIN.            29592002
         GBLC &LABELS(1000)                                             29664002
         GBLA  &LABELNR                                                 29736002
