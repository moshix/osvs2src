&TEXT    SETC  ''                                                       23900002
         AIF (&REMAIN EQ 0).ENDTEXT                                     24000002
&TEXT    SETC  '&SYSLIST(&OP)'(&START,&NRCHAR)                          24100002
.ENDTEXT ANOP                                                           24200002
