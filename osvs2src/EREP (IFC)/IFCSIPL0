XSAVE1   DC    5F'0'                                                    56500002
ASWITCH  DC    A(SWITCH)                                                56600002
ONE      DC    H'1'                                                     56700002
XFF      DC    C'FF'                                                    56800002
SUMTAB   DC    C'NMIEIMMEUNOPUPENCEDFFF'                                56900002
RECODE   STM   R5,R8,XSAVE1             SAVE REGS                       57000002
         LA    R5,SUMTAB                GET ADDR OF SUMTABLE            57100002
         LA    R6,L20(R3)               POINT TO ERROR    ENTRY         57200002
NEXTONE  CLC   L0(L2,R5),IPLRCODE       DOES CODE CORRESPOND            57300002
         BE    ADDNOW                   BR-YES                          57400002
         CLC   L0(L2,R5),XFF            IS IT AN FF                     57500002
         BE    STOPADD                  BR-STOP                         57600002
         LA    R3,L2(R3)                POINT TO NEXT COUNTER           57700002
         LA    R5,L2(R5)                POINT TO NEXT CODE              57800002
         B     NEXTONE                  TRY AGAIN                       57900002
ADDNOW   LH    R7,L0(R3)                LOAD COUNT                      58000002
         AH    R7,ONE                   ADD ONE TO COUNT                58100002
         STH   R7,L0(R3)                STORE NEW TOTAL                 58200002
         B     RETURN1                  GO BACK                         58300002
STOPADD  LH    R7,L0(R6)                GET ERROR COUNT                 58400002
         AH    R7,ONE                   ADD ONE                         58500002
         STH   R7,L0(R6)                STORE ERROR COUNT               58600002
         B     RETURN1                  GO-BACK                         58700002
XDATE1   STM   R5,R8,XSAVE1            SAVE REGS                        58800002
         L     R6,ASWITCH               GET ADDRESS OF SWITCH           58900002
         MVC   L0(L1,R6),CLASRC         PUT CLASS REC IN SECOND PART    59000002
         TM    CLASRC,X50               IS IT A IPL RECORD              59100002
         BO    SKIP1                    BR YES                          59200002
         LA    R7,QUIT-L6               POINT TO END OF SUMMARY         59300002
         ST    R7,REG8                  STORE POINTER                   59400002
SKIP1    EQU   *                                                        59500002
         LA    R5,L0(R3)                GET OUTDSELT POINTER RIGHT      59600002
         CLC   DAY(L4),HEX00           IS DATE ZERO                     59700002
         BE    RETURN1                 BR-ZERO                          59800002
         CLC   L0(L4,R3),HEX00          IS LOW DATE ZERO                59900002
         BNE   XDATE2                   BR-NO                           60000002
         MVC   L0(L4,R3),DAY           MOVE IN DATE                     60100002
XDATE2   CLC   DAY(L4),L0(R3)          CHECK FOR LOW DATE               60200002
         BL    SAVELOW                  LOWER SAVE IT                   60300002
         CLC   DAY(L4),L4(R3)          CHECK FOR HI                     60400002
         BNH   RETURN1                                                  60500002
SAVEHI   MVC   L4(L4,R3),DAY           MOVE HI DATE                     60600002
         B     RETURN1                                                  60700002
SAVELOW  MVC   L0(L4,R3),DAY           MOVE LOW DATE                    60800002
         B     RETURN1                                                  60900002
RETURN1  LM    R5,R8,XSAVE1            RESTORE REGS                     61000002
SKIP     BR    R14                                                      61100002
ADDSUM   STM   R5,R8,XSAVE1             SAVE REGS                       61200002
         L     R8,REG8                  GET POINTER TO ENTRY            61300002
         CLC   SSID(L1),L1(R8)          USE MASK TO TEST                61400002
         BE    ADD                      IF EQUAL - THEN ADD             61500002
         B     RETURN1                  IF NOT - RETURN                 61600002
ADD      LH    R5,L0(R3)                GET HALFWORD COUNTER            61700002
         AH    R5,ONE                   ADD ONE TO COUNT                61800002
         STH   R5,L0(R3)                STORE NEW TOTAL                 61900002
         B     RETURN1                  RETURN                          62000002
