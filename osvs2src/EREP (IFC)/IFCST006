         TITLE 'IFCST006 - SUM TABLE - LINKNAME=IFCST006'               00050042
*  DSGEN MACRO     DSGEN MACRO     DSGEN MACRO     DSGEN MACRO        * 00100042
         SPACE 2                                                        00150042
         MACRO                                                          00200042
&NAME    DSGEN                                                          00250042
.*                            THIS MACRO GENERATES ONE DSECT WORD       00300042
.*                                 FORMATTED FOR THE SUBJECT LOG RECORD 00350042
.*                                 EACH OPERAND HAS TWO SUB LISTS. THE  00400042
.*                                 FIRST IS THE NAME OF THE LOG ITEM    00450042
.*                                 WHILE THE SECOND IS THE # OF BITS TO 00500042
.*                                 ALLOCATE FOR THIS ITEM. ITEMS MUST   00550042
.*                                 BE DEFINED CONTIGUOUSLY WITHOUT GAPS 00600042
.*                                 UNUSED BIT GROUPS MUST BE GIVEN      00650042
.*                                 DUMMY NAMES. THE NAMES PROVIDED FOR  00700042
.*                                 THE LOG ITEMS MAY THEN BE USED IN    00750042
.*                                 THE CONVERSION MACRO INSTRUCTIONS.   00800042
         GBLC &ITEM(1000)                                               00850042
         GBLA &ITEMNBR,&BITS(1000),&SHIFT(1000)                         00900042
         LCLB  &ONLY1,&CTIND                                            00950042
         LCLA  &START1,&START2,&END1,&END2                              01000042
         LCLC  &SYMBOL,&EQUSYM                                          01050042
         LCLA  &OP,&COUNT,&BYTE,&EQUCNT                                 01100042
         ACTR  100000                                                   01150042
.START   AIF   (N'&SYSLIST(&OP+1) LT 3).LOOP                            01200042
         AIF   ('&SYSLIST(&OP+1,3)' EQ 'EQU').LOOP     TEST FOR EQU     01250042
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' AND '&SYSLIST(&OP+1,4)' EQ '').ERR1  01300042
  AIF   ('&SYSLIST(&OP+1,3)' EQ '' OR '&SYSLIST(&OP+1,4)' EQ '').ERR1   01350042
         AIF   ('&SYSLIST(&OP+1,3)' GT '&SYSLIST(&OP+1,4)').ERR3        01400042
&START1  SETA  &SYSLIST(&OP+1,3)        GET START COUNT OF 1            01450042
&START2  SETA  &SYSLIST(&OP+1,4)        GET FIRST END COUNT             01500042
         AGO   .SET2A                   BRANCH AROUND                   01550042
.ERR1    MNOTE '**FIRST SYMBOL CONCATENATION INCOMPLETE**'              01600042
         MEXIT                                                          01650042
.SET2A   AIF   ('&SYSLIST(&OP+1,5)' EQ ''AND '&SYSLIST(&OP+1,6)' EQ '' X01700042
               ).SET2                   TEST FOR NO SECOND              01750042
  AIF   ('&SYSLIST(&OP+1,5)' EQ '' OR '&SYSLIST(&OP+1,6)' EQ '').ERR2   01800042
         AIF   ('&SYSLIST(&OP+1,5)' GT '&SYSLIST(&OP+1,6)').ERR3        01850042
&END1    SETA  &SYSLIST(&OP+1,5)        GET START COUNT OF 2            01900042
&END2    SETA  &SYSLIST(&OP+1,6)        GET END COUNT OF 2              01950042
         AGO   .LOOP                                                    02000042
.SET2    ANOP                                                           02050042
&ONLY1   SETB  1                        INDICATE ONLY ONE CONCATENATION 02100042
         AGO   .LOOP                                                    02150042
.ERR2    MNOTE '**SECOND SYMBOL CONCATENATION INCOMPLETE**'             02200042
         MEXIT                                                          02250042
.ERR3    MNOTE '**START GREATER THAN END CONCATENATION**'               02300042
         MEXIT                                                          02350042
.LOOP    ANOP                                                           02400042
&OP      SETA  &OP+1                                                    02450042
         AIF   (&OP GT N'&SYSLIST).DONE      TEST IF ALL DONE           02500042
&ITEMNBR SETA  &ITEMNBR+1                                               02550042
         AIF   ('&SYSLIST(&OP,2)' NE '').OK1                            02600042
         MNOTE '**NULL BIT COUNT FIELD**'                               02650042
         MEXIT                                                          02700042
.OK1     ANOP                                                           02750042
&SYMBOL  SETC  '&SYSLIST(&OP,1)'                                        02800042
         AIF   ('&SYMBOL' NE '').OK                                     02850042
*              *****     LOG ITEM SYMBOL NOT PROVIDED     *****         02900042
         AIF   ('&SYSLIST(&OP,3)' NE 'EQU').CNT                         02950042
         AGO   .START                   GET NEXT                        03000042
.OK      AIF   (N'&SYSLIST(&OP) LT 3).NO3    TEST FOR OLD FORMAT        03050042
         AIF   ('&SYSLIST(&OP,3)' EQ 'EQU').EQU   TEST FOR EQU          03100042
         AIF   (&ONLY1).OK2             TEST FOR ONLY ONE CONCAT        03150042
&SYMBOL  SETC  '&SYMBOL.&START1.&END1'       GET 1 AND 2 CONCAT         03200042
         AGO   .NO3                     BRANCH AROUND                   03250042
.OK2     ANOP                                                           03300042
&SYMBOL  SETC  '&SYMBOL.&START1'        GET 1 CONCATENATED              03350042
.NO3     ANOP                                                           03400042
&SYMBOL  EQU   *+&BYTE                                                  03450042
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL IN GLOBAL             03500042
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)                                    03550042
&SHIFT(&ITEMNBR) SETA &COUNT-&BYTE*8                                    03600042
.CNT     ANOP                                                           03650042
&CTIND   SETB  1                        SET COUNT INDICATOR             03700042
&COUNT   SETA  &COUNT+&SYSLIST(&OP,2)                                   03750042
&BYTE    SETA  &COUNT/8                                                 03800042
         AIF   (N'&SYSLIST(&OP) LT 3).START  TEST FOR OLD FORMAT        03850042
         AIF   (&ONLY1).TSTST           TEST FOR ONLY ONE CONCAT        03900042
         AIF   (&END1 EQ &END2).TSTST   TEST FOR SECOND END             03950042
&END1    SETA  &END1+1                  INCREMENT COUNT                 04000042
&OP      SETA  &OP-1                    DECREMENT INCREMENT             04050042
         AGO   .LOOP                    GENERATE NEXT                   04100042
.TSTST   ANOP                                                           04150042
         AIF   (&START1 EQ &START2).LPDONE   TEST FOR END               04200042
         AIF   (&ONLY1).NSET1           TEST FOR ONLY ONE CONCAT        04250042
&END1    SETA  &SYSLIST(&OP,5)          RE-INIT SECOND START COUNT      04300042
.NSET1   ANOP                                                           04350042
&OP      SETA  &OP-1                    DECREMENT INDEX                 04400042
&START1  SETA  &START1+1                INCREMENT FIRST COUNT           04450042
         AGO   .LOOP                    GO DO NEXT                      04500042
.LPDONE  ANOP                                                           04550042
&ONLY1   SETB  0                        CLEAR INDICATOR                 04600042
         AGO   .START                   GO TEST FOR MORE SUB-LISTS      04650042
.DONE    AIF   (&COUNT-&COUNT/8*8 NE 0).ERR  TEST FOR MULTIPLE OF EIGHT 04700042
         AIF   (&CTIND EQ 0).END        NO DS IF NO COUNT               04750042
&COUNT   SETA  &COUNT/8                 GET NUMBER OF BYTES             04800042
&NAME    DS    &COUNT.C                                                 04850042
.END     MEXIT                                                          04900042
.ERR     MNOTE '**BIT COUNT FLDS DO NOT TOTAL MULTIPLE OF 8**'          04950042
         MEXIT                                                          05000042
.EQU     AIF   ('&SYSLIST(&OP,4)' NE '').EQU1     TEST FOR NULL LABEL   05050042
.ERR4    MNOTE '**EQU OPERAND OMITTED OR NOT FOUND**'                   05100042
         MEXIT                                                          05150042
.EQU1    AIF   (&SYSLIST(&OP,2) LT 16).EQU2  COUNT OVER FIFTEEN         05200042
*****               THE FOLLOWING STATEMENT                *****        05250042
*****     NUMBER OF BITS EXCEED 15 - DO NOT USE AS BIN     *****        05300042
.EQU2    ANOP                                                           05350042
&EQUCNT  SETA  0                        CLEAR COUNTER                   05400042
&EQUSYM  SETC  '&SYSLIST(&OP,4)'        GET EQU OPERAND                 05450042
.EQULOP  ANOP                                                           05500042
&EQUCNT  SETA  &EQUCNT+1                INCREMENT INDEX                 05550042
         AIF   ('&ITEM(&EQUCNT)' EQ '&EQUSYM').EQUOUT  LABEL FOUND?     05600042
         AIF   (&EQUCNT EQ &ITEMNBR).ERR4    ALL TESTED?                05650042
         AGO   .EQULOP                  NO-GET NEXT                     05700042
.EQUOUT  ANOP                                                           05750042
&ITEM(&ITEMNBR) SETC '&SYMBOL'          GET LABEL NAME                  05800042
&BITS(&ITEMNBR) SETA &SYSLIST(&OP,2)    GET NUMBER OF BITS              05850042
&SHIFT(&ITEMNBR) SETA &SHIFT(&EQUCNT)   GET SHIFT COUNT                 05900042
&SYMBOL  EQU   &EQUSYM                  THIS SYMBOL EQUATED             05950042
         AGO   .START                   RETURN FOR NEXT                 06000042
         MEND                                                           06050042
         EJECT                                                          06100042
*  LINE MACRO     LINE MACRO     LINE MACRO     LINE MACRO            * 06150042
         SPACE 2                                                        06200042
         MACRO                                                          06250042
&NAME    LINE  &DATA,&LABEL,&SKIP=                                      06300042
.*                            THIS MACRO ESTABLISHES THE MARGINS OR     06350042
.*                                 COLUMN SETTINGS FOR EACH LINE. IT    06400042
.*                                 GENERATES NECESSARY CARRIAGE CONTROL 06450042
.*                                 ENTRIES. LASTLY IT DETERMINES THE    06500042
.*                                 NUMBER OF ENTRIES ASSOCIATED WITH    06550042
.*                                 THE PREVIOUS LINE AND STORES THAT #  06600042
.*                                 INTO THE APPROPRIATE BYTE.           06650042
.*                            &DATA(1) - CHARACTER POSITION TO BEGIN    06700042
.*                                 DATA.                                06750042
.*                            &DATA(2) - # OF SPACES BETWEEN SUCCESSIVE 06800042
.*                                 DATA COLUMN STARTING CHARACTERS.     06850042
.*                            &LABEL(1) - CHAR. POS. TO BEGIN LABELS.   06900042
.*                            &LABEL(2) - # SPACES BETWEEN LINE'S LABEL 06950042
.*                                 STARTING CHARACTERS.                 07000042
.*                            &SKIP= - # OF LINES TO ADVANCE CARRIAGE   07050042
.*                                 TO BEGIN NEXT PRINTED LINE.          07100042
.*                            NEW ENTRIES ARE GENERATED ONLY WHEN       07150042
.*                                 NECESSARY. OMISSION OF ITEMS MEANS   07200042
.*                                 THEY HAVE NOT CHANGED SINCE PREVIOUS 07250042
.*                                 SETTINGS. A LINE MACRO IS REQUIRED   07300042
.*                                 FOR EACH PRINTED LINE. IF NO SETTING 07350042
.*                                 CHANGES ARE REQUIRED ALL OPERANDS    07400042
.*                                 ARE OMITTED IN WHICH CASE ONLY THE   07450042
.*                                 # OF ENTRIES FOR THE PREVIOUS LINE   07500042
.*                                 IS COMPUTED. THIS MACRO MUST BE THE  07550042
.*                                 FIRST MACRO INSTRUCTION ISSUED FOR   07600042
.*                                 THE SUBJECT LINE                     07650042
         GBLA  &DB,&DT,&LB,&LT,&NRENTS                                  07700042
         GBLA  &LASTCC,&INITIAL                                         07750042
         GBLC  &NRENTSX                                                 07800042
         GBLC  &CARRCON                                                 07850042
         GBLB  &TEST                                                    07900042
         LCLA  &S                                                       07950042
&NAME    DS    0C                                                       08000042
         AIF   (&INITIAL EQ 0).FIRSTLN                                  08050042
         LINEND                                                         08100042
         AGO   .NTFIRST                                                 08150042
.FIRSTLN ANOP                                                           08200042
&NRENTSX SETC  'N&SYSNDX'                                               08250042
&INITIAL SETA  1                                                        08300042
N&SYSNDX EQU   *                                                        08350042
.NTFIRST ANOP                                                           08400042
         DC    AL1(0)                                                   08450042
         AIF   ('&DATA(1)' EQ '').CKDTABS                               08500042
         AIF   (&DATA(1) EQ &DB).CKDTABS                                08550042
&DB      SETA  &DATA(1)                                                 08600042
         AGO   .AA                                                      08650042
.CKDTABS AIF   ('&DATA(2)' EQ '').NCDATA                                08700042
         AIF   (&DATA(2) EQ &DT).NCDATA                                 08750042
         AGO   .CC                                                      08800042
.AA      AIF   ('&DATA(2)' EQ '').BB                                    08850042
         AIF   (&DATA(2) EQ &DT).BB                                     08900042
.CC      ANOP                                                           08950042
&DT      SETA  &DATA(2)                                                 09000042
.BB      DC    XL2'FFFE'                                                09050042
         DC    FL1'&DB'                                                 09100042
         DC    FL1'&DT'                                                 09150042
&NRENTS  SETA  &NRENTS+1                                                09200042
.NCDATA  ANOP                                                           09250042
         AIF   ('&LABEL(1)' EQ '').CKLTABS                              09300042
         AIF   (&LABEL(1) EQ &LB).CKLTABS                               09350042
&LB      SETA  &LABEL(1)                                                09400042
         AGO   .AAA                                                     09450042
.CKLTABS AIF   ('&LABEL(2)' EQ '').NCLABEL                              09500042
         AIF   (&LABEL(2) EQ &LT).NCLABEL                               09550042
         AGO   .CCC                                                     09600042
.AAA     AIF   ('&LABEL(2)' EQ '').BBB                                  09650042
         AIF   (&LABEL(2) EQ &LT).BBB                                   09700042
.CCC     ANOP                                                           09750042
&LT      SETA  &LABEL(2)                                                09800042
.BBB     DC    XL2'FFFF'                                                09850042
         DC    FL1'&LB'                                                 09900042
         DC    FL1'&LT'                                                 09950042
&NRENTS  SETA  &NRENTS+1                                                10000042
.NCLABEL ANOP                                                           10050042
         AIF   ('&SKIP' NE '').CONT                                     10100042
         AIF   (&TEST EQ 0).NOCHANG                                     10150042
&S       SETA  &LASTCC                                                  10200042
         AGO   .DEFAULT                                                 10250042
.CONT    ANOP                                                           10300042
         AIF   (&TEST EQ 0).AB                                          10350042
&S       SETA  &SKIP                                                    10400042
.DEFAULT ANOP                                                           10450042
         ORG   &CARRCON+3                                               10500042
         DC    AL.3(0),AL.2(&S),AL.3(1)                                 10550042
         ORG                                                            10600042
&LASTCC  SETA  &S                                                       10650042
&TEST    SETB  0                                                        10700042
         AGO   .DD                                                      10750042
.AB      AIF   (&SKIP EQ &LASTCC).NOCHANG                               10800042
A&SYSNDX DC    XL2'FFFB'                                                10850042
         DC    AL.3(0),AL.2(&SKIP),AL.3(1)                              10900042
         DC    FL1'9'                                                   10950042
&LASTCC  SETA  &SKIP                                                    11000042
&TEST    SETB  1                                                        11050042
&CARRCON SETC  'A&SYSNDX'                                               11100042
&NRENTS  SETA  &NRENTS+1                                                11150042
.DD      ANOP                                                           11200042
.NOCHANG ANOP                                                           11250042
         MEND                                                           11300042
         EJECT                                                          11350042
