         GBLC  &ITEM(1000),&DSECT                                       34425042
         LCLA  &IBITS,&NDX,&ISHIFT,&M,&BITNBR                           34500042
.LOOP    ANOP                                                           34575042
&NDX     SETA  &NDX+1                                                   34650042
         AIF   ('&ITEM(&NDX)' EQ '&LOGITEM').FOUND                      34725042
         AIF   (&NDX LT &ITEMNBR).LOOP                                  34800042
         MNOTE *,'LOGITEM NOT FOUND'                                    34875042
         MEXIT                                                          34950042
.FOUND   ANOP                                                           35025042
         DC    AL1(0)                                                   35100042
&ISHIFT  SETA  &SHIFT(&NDX)                                             35175042
&IBITS   SETA  &BITS(&NDX)                                              35250042
         AIF   ('&MASK' EQ '').FORM                                     35325042
         DC    B'&MASK'                                                 35400042
         AGO   .ITEM                                                    35475042
.FORM    ANOP                                                           35550042
&BITNBR  SETA  &ISHIFT-7                                                35625042
&BITNBR  SETA  0-&BITNBR                                                35700042
&M       SETA  1                                                        35775042
.LOOP2   ANOP                                                           35850042
         AIF   (&BITNBR EQ 0).GOTIT                                     35925042
&M       SETA  &M*2                                                     36000042
&BITNBR  SETA  &BITNBR-1                                                36075042
         AGO   .LOOP2                                                   36150042
.GOTIT   ANOP                                                           36225042
         DC    AL1(&M)                                                  36300042
.ITEM    DC    AL2(&LOGITEM-&DSECT)                                     36375042
         DC    AL2(&COUNTR)                                             36450042
&COUNTR  SETA  &COUNTR+1                                                36525042
         MEND                                                           36600042
         GBLC  &DSECT,&CSECT                                            36675042
