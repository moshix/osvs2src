         SR    R6,R6       CLEAR REGISTER                               70620042
         ICM   R6,X'3',WORK1  LOAD PAPER COUNT                          70650042
         LR    R5,R3                    GET COUNTER POINTER             70680042
         XC    WORK1(L8),WORK1          CLEAR WORK AREA                 70740042
         MVC   WORK1(L4),L2(R5)         GET CURRENT COUNTER TOTAL       70800042
         AL    R6,WORK1                 ADD TOTAL TO CC FROM RCD        70860042
         BC    NOCARRY,CTROK            BRANCH IF NO OVERFLOW           70920042
*                                       IF OVERFLOW DID OCCUR           70980042
         LH    R7,L0(R5)                GET OVERFLOW COUNTER            71040042
         LA    R7,L1(R7)                INCREMENT IT BY ONE             71100042
         STH   R7,L0(R5)                PUT IT BACK IN COUNTER          71160042
CTROK    EQU   *                                                        71220042
         ST    R6,WORK1                 STORE TOTAL INTO WORK AREA      71280042
         TM    WORK1,X80                HI ORDER BIT ON ?               71340042
         BNO   NOVFLO                   NO, BRANCH                      71400042
         NI    WORK1,X7F                TURN HI ORDER BIT OFF           71460042
         LH    R7,L0(R5)                GET OVERFLOW COUNTER            71520042
         LA    R7,L1(R7)                INCREMENT IT BY ONE             71580042
         STH   R7,L0(R5)                PUT IT BACK IN COUNTER          71640042
NOVFLO   EQU   *                                                        71700042
         MVC   L2(L4,R5),WORK1          MOVE TOTAL TO COUNTER           71760042
         B     RETURN                                                   71820042
ADDFF    EQU   *                        ADD CFS FOLDS                   71880042
         STM   R5,R8,REGSAVE            SAVE REGS                       71940042
         MVC   WORK1(L2),CFSFOLDS       GET CFS FOLDS COUNT             72000042
         B     COMADD                   BRANCH TO COMMON ADD            72060042
ADDPP    EQU   *                        ADD PAPER FOOTAGE               72120042
         STM   R5,R8,REGSAVE            SAVE REGS                       72180042
         MVC   WORK1(L2),PAPERPRS       GET PAPER FOOTAGE COUNT         72240042
         B     COMADD                   BRANCH TO COMMON ADD            72300042
SETSER   EQU   *                        PUT IN SERIAL NUMBER            72360042
         STM   R5,R8,REGSAVE            SAVE REGS                       72420042
         LR    R5,R3                                                    72480042
         MVC   L0(L2,R5),SERIALNO       MOVE IN SERIAL NUMBER           72540042
         B     RETURN                                                   72600042
*********************************************************************** 72601001
*                                                                     * 72602001
*             SUM 4 BIT STATISTICAL DATA COUNTERS UP TO 9999          * 72603001
*                                                                     * 72604001
*********************************************************************** 72605001
SDRSUM   EQU   *                                               @XA20147 72606001
         MVC   SDRWRK(L4),SDR02    PLACE SDR DATA ON FULLWD BOUNDARY    72607001
         CLC   SDRWRK(L4),FULLZERO CHECK IF ZERO               @XA20147 72608001
         BER   R14                 NO NEED TO GO FURTHER       @XA20147 72609001
         STM   R5,R8,REGSAVE       START OF SUM ROUTINE FOR SDR DATA    72610001
         ST    R0,R0SAVE           SAVE REG0                   @XA20147 72611001
         LA    R0,L7               SEVEN PIECES OF DATA TO BE SUMMED    72612001
         LR    R8,R3               GET TWO BYTE CTR POINTER    @XA20147 72613001
         L     R7,SDRWRK           PUT SDR DATA INTO REG       @XA20147 72614001
SDRSUM1  SR    R6,R6               CLEAR REG6                  @XA20147 72615001
         SLDL  R6,4                SHIFT LEFT 4 BIT POS REGS 6 AND 7    72616001
         LH    R5,L0(R8)           GET PREVIOUS SUMMED DATA    @XA20147 72617001
         AR    R6,R5               ADD NEW DATA TO PREVIOUS SUM@XA20147 72618001
         CH    R6,CTR9999          CHK SUM FOR MAX PRNT TOTL OF 9999    72619001
         BNH   SDRSUM2                                         @XA20147 72620001
         L     R6,CTR9999          SET REG TO STORE MAX VALUE  @XA20147 72621001
SDRSUM2  STH   R6,L0(R8)           STORE SUMMED OR MAX VALUE   @XA20147 72622001
         LA    R8,L2(R8)           INCR POINTER BY 2           @XA20147 72623001
         BCT   R0,SDRSUM1          IF COUNT NOT ZERO, LOOP     @XA20147 72624001
         L     R0,R0SAVE           RESTORE REG0                @XA20147 72625001
         B     RETURN                                          @XA20147 72626001
         DS    0F                                              @XA20147 72627001
SDRWRK   DC    1F'0'                                           @XA20147 72628001
FULLZERO DC    1F'0'                                           @XA20147 72629001
CTR9999  DC    H'9999'             MAX PRINTABLE CTR VALUE IS 9999      72630001
R0SAVE   DC    1F'0'                                           @XA20147 72631001
