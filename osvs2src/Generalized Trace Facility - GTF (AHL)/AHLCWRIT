*       M039DD=WRITERDD;            /* MOVE THE DDNAME INTO THE MSG  */ 06138000
         L     @14,@CA02286                                        0410 06139000
         MVC   M039DD-M039DD(8,@14),WRITERDD(PCTPTR)               0410 06140000
*       DO;                         /* WTOP MF(E,AHL039)             */ 06141000
*         RFY                                                      0412 06142000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 06143000
*         GPR01P=ADDR(AHL039);      /* ADDRESS CONTROL PROGRAM LIST  */ 06144000
         L     GPR01P,@CA02282                                     0413 06145000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 06146000
         SVC   35                                                  0414 06147000
*         RFY                                                      0415 06148000
*           GPR01P UNRSTD;          /* RELEASE REG 1                 */ 06149000
*       END;                        /* WTOP MF(E,AHL039)             */ 06150000
*       GO TO CLEANUP;              /* TERMINATE                     */ 06151000
         B     CLEANUP                                             0417 06152000
*     END;                                                         0418 06153000
*   RFY                                                            0419 06154000
*     RETCODE UNRSTD;                                              0419 06155000
@RF00408 DS    0H                                                  0420 06156000
*   GEN CODE REFS(TRCDCB) NODEFS SETS(DCBOFLGS) NOEXIT NOFLOWS SEQFLOW  06157000
*       (OPEN (TRCDCB,(OUTPUT,DISP)));/* OPEN THE TRACE DATASET      */ 06158000
         OPEN (TRCDCB,(OUTPUT,DISP))                                    06159000
*   IF DCBOFOPN^=ON                 /* CHECK TO BE SURE THE DCB DID     06160000
*                                      OPEN                          */ 06161000
*     THEN                                                         0421 06162000
         LA    @10,TRCDCB                                          0421 06163000
         TM    DCBOFOPN+40(@10),B'00010000'                        0421 06164000
         BO    @RF00421                                            0421 06165000
*     DO;                           /* IF NOT TERMINATE              */ 06166000
*       M038DD=WRITERDD;            /* MOVE THE TRACE DATASET NAME 0423 06167000
*                                      FROM THE DCB INTO THE MESSAGE */ 06168000
         L     @10,@CA02285                                        0423 06169000
         MVC   M038DD-M038DD(8,@10),WRITERDD(PCTPTR)               0423 06170000
*       DO;                         /* WTOP MF(E,AHL038)             */ 06171000
*         RFY                                                      0425 06172000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 06173000
*         GPR01P=ADDR(AHL038);      /* ADDRESS CONTROL PROGRAM LIST  */ 06174000
         L     GPR01P,@CA02281                                     0426 06175000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 06176000
         SVC   35                                                  0427 06177000
*         RFY                                                      0428 06178000
*           GPR01P UNRSTD;          /* RELEASE REG 1                 */ 06179000
*       END;                        /* WTOP MF(E,AHL038) WRITE OUT 0429 06180000
*                                      UNABLE TO OPEN MSG            */ 06181000
*       GO TO CLEANUP;                                             0430 06182000
         B     CLEANUP                                             0430 06183000
*     END;                                                         0431 06184000
*                                                                  0431 06185000
*   /*****************************************************************/ 06186000
*   /*                                                               */ 06187000
*   /* INITIALIZATION OF THE WRITER-STACK SERVICE MODULES, CONTROL   */ 06188000
*   /* BLOCKS, AND DATA AREAS IS NOW COMPLETE AND FUNCTIONING. THE   */ 06189000
*   /* GTFCMECB IS NOW POSTED TO INFORM THE MOTHER TASK              */ 06190000
*   /*                                                               */ 06191000
*   /*****************************************************************/ 06192000
*                                                                  0432 06193000
*/*%WSF15: S POST: GTFCMECB TO INDICATE INIT COMPLETE                */ 06194000
*/* P CLEAR AFINIT                                                   */ 06195000
*                                                                  0432 06196000
*   DO;                             /* POST (GTFCMECB,0)             */ 06197000
@RF00421 DS    0H                                                  0433 06198000
*     RESPECIFY                                                    0433 06199000
*      (GPR00F,                                                    0433 06200000
*       GPR01P) RESTRICTED;                                        0433 06201000
*     GPR00F=0;                     /* POST CODE IN R0               */ 06202000
         SLR   GPR00F,GPR00F                                       0434 06203000
*     GPR01P=ADDR(GTFCMECB);        /* ECB ADDRESS                   */ 06204000
         LA    GPR01P,GTFCMECB(,PCTPTR)                            0435 06205000
*     SVC(2);                       /* ISSUE POST SVC                */ 06206000
         SVC   2                                                   0436 06207000
*     RESPECIFY                                                    0437 06208000
*      (GPR00F,                                                    0437 06209000
*       GPR01P) UNRESTRICTED;                                      0437 06210000
*   END;                            /* POST (GTFCMECB,0)             */ 06211000
*   AFINIT=OFF;                     /* SHOW INITIALIZATION COMPLETE  */ 06212000
         NI    AFINIT(PCTPTR),B'11101111'                          0439 06213000
*                                                                  0440 06214000
*   /*****************************************************************/ 06215000
*   /*                                                               */ 06216000
*   /* THIS IS THE BEGINNING OR TOP OF THE LOOP THAT THE WRITER GOES */ 06217000
*   /* AROUND DURING NORMAL EXECUTION. IT WAITS HERE AT THE TOP FOR  */ 06218000
*   /* WORK TO BECOME AVAILABLE AND THEN PROGRESSES AROUND THE LOOP  */ 06219000
*   /* CHECKING EACH TASK IT IS TO DO IN PRIORITY ORDER. FOR EACH    */ 06220000
*   /* TASK THAT HAS WORK TO DO IT COMPLETES ALL THE WORK POSSIBLE AT*/ 06221000
*   /* THAT TIME AND THEN GOES ON TO THE NEXT TASK. WHEN ALL TASKS   */ 06222000
*   /* HAVE COMPLETED IT RETURNS TO THE TOP AND WAITS                */ 06223000
*   /*                                                               */ 06224000
*   /*****************************************************************/ 06225000
*                                                                  0440 06226000
*/*WAIT: P SET AFWAIT                                                */ 06227000
*/* P IOECBADD= NEXTCHK WAIT ONLY ON ONE ECB                         */ 06228000
*/* S WAIT: ON ECBLIST                                               */ 06229000
*/* P CLEAR AFWAIT                                                   */ 06230000
*                                                                  0440 06231000
*WAIT:                                                             0440 06232000
*   AFWAIT=ON;                                                     0440 06233000
WAIT     OI    AFWAIT(PCTPTR),B'00000100'                          0440 06234000
*   IOECBADD=NEXTCHK;               /* BECAUSE THE POSTING OF      0441 06235000
*                                      MULTIPLE BSAM WRITES MIGHT NOT   06236000
*                                      OCCURR IN THE SAME SEQUENCE AS   06237000
*                                      THE WRITES, ONLY ONE IS WAITED   06238000
*                                      ON AT A TIME. THIS FORCES THEM   06239000
*                                      TO BE NOTICED IN ORDER.       */ 06240000
         ST    NEXTCHK,IOECBADD                                    0441 06241000
*   DO;                             /* WAIT ECBLIST(ECBALIST)        */ 06242000
*     RESPECIFY                                                    0443 06243000
*      (GPR00P,                                                    0443 06244000
*       GPR01P) RESTRICTED;                                        0443 06245000
*     GPR00P=1;                     /* LOAD WAIT COUNT               */ 06246000
         LA    GPR00P,1                                            0444 06247000
*     GPR01P=-ADDR(ECBALIST);       /* ADDRESS OF LIST OF ECB'S      */ 06248000
         LA    GPR01P,ECBALIST                                     0445 06249000
         LCR   GPR01P,GPR01P                                       0445 06250000
*     SVC(1);                       /* WAIT SVC                      */ 06251000
         SVC   1                                                   0446 06252000
*     RESPECIFY                                                    0447 06253000
*      (GPR00P,                                                    0447 06254000
*       GPR01P) UNRESTRICTED;                                      0447 06255000
*   END;                            /* WAIT ECBLIST(ECBALIST)        */ 06256000
*   AFWAIT=OFF;                                                    0449 06257000
*                                                                  0449 06258000
         NI    AFWAIT(PCTPTR),B'11111011'                          0449 06259000
*   /*****************************************************************/ 06260000
*   /*                                                               */ 06261000
*   /* SHOULD THE ESTAE ROUTINE BE ENTERED WHILE AFWAIT IS ON IT WILL*/ 06262000
*   /* RETRY, DISABLE STACK & AHLSBUF, POST THE WRTRERR ECB, AND THEN*/ 06263000
*   /* BRANCH TO THIS LOCATION TO ALLOW THE WRITER TO TERMINATE      */ 06264000
*   /* NORMALLY                                                      */ 06265000
*   /*                                                               */ 06266000
*   /*****************************************************************/ 06267000
*                                                                  0450 06268000
*/*NOWAIT: P                                                         */ 06269000
*/*RETRYPT7: P PUT 0 INTO WRTRCOM ECB                                */ 06270000
*                                                                  0450 06271000
*NOWAIT:                            /* THIS LABEL IS USED TO BRANCH     06272000
*                                      BACK TO THE BEGINNING OF THE     06273000
*                                      TRACE WRITER LOOP BUT AVOIDING   06274000
*                                      THE WAIT                      */ 06275000
*RETRYPT7:                                                         0450 06276000
*   WRTRCOM=0;                      /* ZERO OUT THIS ECB NOW SO THAT,   06277000
*                                      IF AHLSBUF SHOULD MOVE MORE 0450 06278000
*                                      DATA ONTO THE WRITE QUEUE   0450 06279000
*                                      AFTER THE TRACE WRITER IS DONE   06280000
*                                      CHECKING IT BUT BEFORE IT   0450 06281000
*                                      RETURNS TO THE TOP OF THE   0450 06282000
*                                      LOOP, THE ECB CAN BE POSTED 0450 06283000
*                                      AGAIN                         */ 06284000
NOWAIT   DS    0H                                                  0450 06285000
RETRYPT7 SLR   @10,@10                                             0450 06286000
         ST    @10,WRTRCOM(,PCTPTR)                                0450 06287000
*                                                                  0451 06288000
*   /*****************************************************************/ 06289000
*   /*                                                               */ 06290000
*   /* THE FIRST TASK PERFORMED BY THE TRACE WRITER IS TO REMOVE AS  */ 06291000
*   /* MANY GTFBCB FROM THE WRITE QUEUE AS POSSIBLE.                 */ 06292000
*   /*                                                               */ 06293000
*   /*****************************************************************/ 06294000
*                                                                  0451 06295000
*/*%IOLOOP: P SET AFBUFOFF                                           */ 06296000
*/* D (YES,,NO,%IE34) DIAGNOSTIC: IS EQLENGTH <EQMIN                 */ 06297000
*/* P DIAGNOSTIC: EQMIN=EQLENGTH                                     */ 06298000
*/*%IE34: P (INT,%NOCHK,EXT,CHKLOOP)                                 */ 06299000
*                                                                  0451 06300000
*   AFBUFOFF=ON;                    /* TURN ON THE AUDIT FLAG FOR  0451 06301000
*                                      THIS SECTION OF CODE          */ 06302000
         OI    AFBUFOFF(PCTPTR),B'00010000'                        0451 06303000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06304000
*                                                                  0452 06305000
*   IF EQLENGTH<EQMIN               /* SAVE THE LOWEST NUMBER OF   0452 06306000
*                                      AVAILABLE GTFBCB              */ 06307000
*     THEN                                                         0452 06308000
         L     @10,EQLENGTH(,PCTPTR)                               0452 06309000
         C     @10,EQMIN(,PCTPTR)                                  0452 06310000
         BNL   @RF00452                                            0452 06311000
*     EQMIN=EQLENGTH;                                              0453 06312000
*                                                                  0453 06313000
         ST    @10,EQMIN(,PCTPTR)                                  0453 06314000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06315000
*                                                                  0454 06316000
*                                                                  0454 06317000
*   /*****************************************************************/ 06318000
*   /*                                                               */ 06319000
*   /* BEFORE CHECKING TO SEE IF ANY GTFBUFR CAN BE WRITTEN OUT AN   */ 06320000
*   /* EFFORT IS MADE TO FREE UP AS MANY DECB AS POSSIBLE FOR USE IN */ 06321000
*   /* WRITTING OUT THE GTFBUFR                                      */ 06322000
*   /*                                                               */ 06323000
*   /*****************************************************************/ 06324000
*                                                                  0454 06325000
*/*CHKLOOP: D (YES,,NO,%WRITES) IS NEXTCHK-> DECB POSTED             */ 06326000
*/* S CHECK: NEXTCHK->DECB                                           */ 06327000
*/* P DIAGNOSTIC: INCREMENT NEXTCHK-> DECBCHK BY 1                   */ 06328000
*/* P CLEAR THE ECB IN NEXTCHK->DECB                                 */ 06329000
*/* P RLSEBUF= NEXTCHK-> DECBBCB NEXTCHK-> DECBBCB=0                 */ 06330000
*/* P MOVE NEXTCHK ON TO THE NEXT DECB IN THE CHAIN                  */ 06331000
*                                                                  0454 06332000
*CHKLOOP:                                                          0454 06333000
*   IF NEXTCHK->DECBPOST=ON         /* IS THE NEXT TO BE CHECKED DECB   06334000
*                                      POSTED                        */ 06335000
*     THEN                                                         0454 06336000
@RF00452 DS    0H                                                  0454 06337000
CHKLOOP  TM    DECBPOST(NEXTCHK),B'01000000'                       0454 06338000
         BNO   @RF00454                                            0454 06339000
*     DO;                           /* YES, THEN THE I/O IS COMPLETE */ 06340000
*       GEN REFS(NEXTCHK) SETS(DECBMAP) NOEXIT NOFLOWS NODEFS SEQFLOW   06341000
*           (CHECK  (NEXTCHK));     /* FREE UP THE DECB              */ 06342000
         CHECK  (NEXTCHK)                                               06343000
*       NEXTCHK->DECBCHK=NEXTCHK->DECBCHK+1;                       0457 06344000
         LA    @10,1                                               0457 06345000
         L     @09,DECBCHK(,NEXTCHK)                               0457 06346000
         ALR   @09,@10                                             0457 06347000
         ST    @09,DECBCHK(,NEXTCHK)                               0457 06348000
*       NEXTCHK->DECBECB=0;         /* CLEAR THE ECB FOR THE NEXT USE*/ 06349000
         SLR   @09,@09                                             0458 06350000
         ST    @09,DECBECB(,NEXTCHK)                               0458 06351000
*       RLSEBUF=NEXTCHK->DECBBCB;   /* GET THE ADDR OF THE GTFBCB  0459 06352000
*                                      ASSOCIATED WITH THE GTFBUFR 0459 06353000
*                                      JUST WRITTEN                  */ 06354000
         L     @07,DECBBCB(,NEXTCHK)                               0459 06355000
         ST    @07,RLSEBUF                                         0459 06356000
*       NEXTCHK->DECBBCB=0;         /* THIS MARKS THE DECB AS      0460 06357000
*                                      AVAILABLE                     */ 06358000
         ST    @09,DECBBCB(,NEXTCHK)                               0460 06359000
*       NEXTCHK=NEXTCHK->DECBCHN;   /* MOVE THE NEXT TO BE CHECKED 0461 06360000
*                                      PTR ON TO THE NEXT DECB ON THE   06361000
*                                      CHAIN                         */ 06362000
*                                                                  0461 06363000
         L     NEXTCHK,DECBCHN(,NEXTCHK)                           0461 06364000
*       /*************************************************************/ 06365000
*       /*                                                           */ 06366000
*       /* DECIDE IF THIS GTFBCB NEEDS ONLY TO HAVE ITS USE COUNT    */ 06367000
*       /* DECREMENTED OR IF IT MUST BE PUT ON THE RELEASE QUEUE     */ 06368000
*       /*                                                           */ 06369000
*       /*************************************************************/ 06370000
*                                                                  0462 06371000
*/*%RBX: P DIAGNOSTIC: INCREMENT NUMOFBUF & RLSEBUF-> BCBFREQ BY 1   */ 06372000
*/*IOCS: P SWAPBCB= RLSEBUF-> BCBSWAP FOR CS LOGIC                   */ 06373000
*/* D (YES,,NO,%A2) SWAPUSE=1                                        */ 06374000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06375000
*                                                                  0462 06376000
*       NUMOFBUF=NUMOFBUF+1;        /* KEEP A TOTAL OF THE NUMBER OF    06377000
*                                      GTFBCB PROCESSED              */ 06378000
         L     @09,NUMOFBUF(,PCTPTR)                               0462 06379000
         ALR   @09,@10                                             0462 06380000
         ST    @09,NUMOFBUF(,PCTPTR)                               0462 06381000
*       RLSEBUF->BCBFREQ=RLSEBUF->BCBFREQ+1;                       0463 06382000
*                                                                  0463 06383000
         AL    @10,BCBFREQ(,@07)                                   0463 06384000
         ST    @10,BCBFREQ(,@07)                                   0463 06385000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06386000
*                                                                  0464 06387000
*IOCS:                                                             0464 06388000
*       SWAPBCB=RLSEBUF->BCBSWAP;   /* BECAUSE THE PORTIONS OF THE 0464 06389000
*                                      GTFBCB CONTAINED IN BCBSWAP 0464 06390000
*                                      REQUIRE MP SERIALIZATION VIA     06391000
*                                      CS THE VALUES MUST BE SAVED 0464 06392000
*                                      BEFORE REFERENCING THEM & THEN   06393000
*                                      ONLY THE STABLE COPY MAY BE 0464 06394000
*                                      REFERENCED                    */ 06395000
IOCS     LA    @10,WORKAREA                                        0464 06396000
         L     @09,RLSEBUF                                         0464 06397000
         L     @07,BCBSWAP(,@09)                                   0464 06398000
         ST    @07,SWAPBCB(,@10)                                   0464 06399000
*       IF SWAPUSE=1                /* WAS THE GTFBCB ON ONLY 1 QUEUE*/ 06400000
*         THEN                                                     0465 06401000
         CLC   SWAPUSE(2,@10),@CH00052                             0465 06402000
         BNE   @RF00465                                            0465 06403000
*         DO;                       /* YES                           */ 06404000
*                                                                  0466 06405000
*           /*********************************************************/ 06406000
*           /*                                                       */ 06407000
*           /* BECAUSE THE GTFBCB WAS ON ONLY 1 QUEUE IT IS NOW NOT  */ 06408000
*           /* ON ANY QUEUE. UNDER THESE CONDITIONS ONLY THIS        */ 06409000
*           /* EXECUTION OF THIS CODE CAN LOCATE IT. THEREFORE UNTIL */ 06410000
*           /* IT IS PLACED ONTO ANOTHER QUEUE NO SPECIAL HANDLING,  */ 06411000
*           /* SERIALIZATION, OR MP LOGIC IS NEEDED TO MODIFY IT.    */ 06412000
*           /* ALSO IT MUST BE MADE AVAILABLE AGAIN BY BEING PUT ONTO*/ 06413000
*           /* THE RELEASE QUEUE                                     */ 06414000
*           /*                                                       */ 06415000
*           /*********************************************************/ 06416000
*                                                                  0467 06417000
*/* N (EXT,,INT,%DGF2) MP LOGIC NOT NEEDED BECAUSE SWAPUSE=1         */ 06418000
*/* P (,%DGF2A) DIAGNOSTIC: RLSEBUF-> DECBFLAG=OFF & RQFLAG=ON       */ 06419000
*/*%DGF2: P DIAGNOSTIC: RLSEBUF-> WQFLAG=OFF & RQFLAG=ON             */ 06420000
*/*%DGF2A: P SAVE A STABLE COPY OF RQHEAD                            */ 06421000
*/*OFFCS: P RLSEBUF-> RQCHAIN=STABLE COPY RQHEAD                     */ 06422000
*/* P CS RLSEBUF INTO RQHEAD                                         */ 06423000
*/* D (FAIL,OFFCS,OK,%EOL) CS?                                       */ 06424000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06425000
*                                                                  0467 06426000
*           RLSEBUF->DECBFLAG=OFF;  /* TURN OFF THE GTFBCB FLAG THAT    06427000
*                                      SHOWS IT ON THE DECB          */ 06428000
*           RLSEBUF->RQFLAG=ON;     /* TURN ON THE RELEASE QUEUE FLAG*/ 06429000
         OI    RQFLAG(@09),B'00000100'                             0468 06430000
         NI    DECBFLAG(@09),B'11111101'                           0468 06431000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06432000
*                                                                  0469 06433000
*           RFY                                                    0469 06434000
*             COMPREG RSTD;                                        0469 06435000
*           COMPREG=RQHEAD;         /* MAKE A STABLE COPY OF RQHEAD  */ 06436000
         L     COMPREG,RQHEAD(,PCTPTR)                             0470 06437000
*OFFCS:                                                            0471 06438000
*           RLSEBUF->RQCHAIN=COMPREG;/* PUT RQHEAD INTO CHAIN FIELD     06439000
*                                      OF THE GTFBCB TO BE ADDED     */ 06440000
OFFCS    L     @10,RLSEBUF                                         0471 06441000
         ST    COMPREG,RQCHAIN(,@10)                               0471 06442000
*           CS(COMPREG,RLSEBUF,RQHEAD);/* VERIFY RQHEAD HASN'T     0472 06443000
*                                      CHANGED AND ADD THE GTFBCB TO    06444000
*                                      THE HEAD OF THE QUEUE         */ 06445000
         CS    COMPREG,@10,RQHEAD(PCTPTR)                          0472 06446000
*           BC(4,OFFCS);            /* IF IT FAILED TRY AGAIN        */ 06447000
         BC    4,OFFCS                                             0473 06448000
*           RFY                                                    0474 06449000
*             COMPREG UNRSTD;                                      0474 06450000
*         END;                                                     0475 06451000
*                                                                  0476 06452000
*       /*************************************************************/ 06453000
*       /*                                                           */ 06454000
*       /* IF THE USE COUNT IS NOT 1 THE GTFBCB IS STILL IN USE ON   */ 06455000
*       /* ANOTHER QUEUE SO JUST DECREMENT IT                        */ 06456000
*       /*                                                           */ 06457000
*       /*************************************************************/ 06458000
*                                                                  0476 06459000
*/*%A2: P (EXT,,INT,%DGF1)                                           */ 06460000
*/* P (,%DGF1A) DECREMENT SWAPUSE 1 & DIAGNOSTIC DECBFSWA=OFF        */ 06461000
*/*%DGF1: P DECREMENT SWAPUSE 1 & DIAGNOSTIC WQFSWAP=OFF             */ 06462000
*/*%DGF1A: P CS NEW VALUES FROM SWAPBCB INTO RLSEBUF-> BCBSWAP       */ 06463000
*/* D (OK,,FAIL,IOCS) CS?                                            */ 06464000
*/*%EOL: P (INT,MOCKIO,EXT,CHKLOOP) END OF LOOP                      */ 06465000
*                                                                  0476 06466000
*       ELSE                                                       0476 06467000
*         DO;                       /* NO, THE GTFBCB IS STILL ON AT    06468000
*                                      LEAST ONE QUEUE               */ 06469000
         B     @RC00465                                            0476 06470000
@RF00465 DS    0H                                                  0477 06471000
*           COMPBCB=SWAPBCB;        /* MAKE ANOTHER COPY FOR THE   0477 06472000
*                                      COMPARE BEFORE MODIFYING THE     06473000
*                                      ONE FOR THE SWAP              */ 06474000
         LA    @10,WORKAREA                                        0477 06475000
         L     @09,SWAPBCB(,@10)                                   0477 06476000
         ST    @09,COMPBCB(,@10)                                   0477 06477000
*           SWAPUSE=SWAPUSE-1;      /* PREPARE A DECREMENTED BCBUSE     06478000
*                                      FOR THE CS                    */ 06479000
         LH    @09,SWAPUSE(,@10)                                   0478 06480000
         BCTR  @09,0                                               0478 06481000
         STH   @09,SWAPUSE(,@10)                                   0478 06482000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06483000
*                                                                  0479 06484000
*           DECBFSWA=OFF;           /* PREPARE THE NEW QUEUE FLAGS   */ 06485000
         NI    DECBFSWA(@10),B'11111101'                           0479 06486000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06487000
*                                                                  0480 06488000
*           CS(COMPBCB,SWAPBCB,RLSEBUF->BCBSWAP);/* THIS WILL EITHER    06489000
*                                      PLACE THE UPDATED VALUES INTO    06490000
*                                      THE GTFBCB OR NOT, IF THEY  0480 06491000
*                                      HAVE BECOME INVALID           */ 06492000
         L     @09,COMPBCB(,@10)                                   0480 06493000
         L     @07,SWAPBCB(,@10)                                   0480 06494000
         L     @04,RLSEBUF                                         0480 06495000
         CS    @09,@07,BCBSWAP(@04)                                0480 06496000
         BZ    @BC00480                                            0480 06497000
         ST    @09,COMPBCB(,@10)                                   0480 06498000
@BC00480 DS    0H                                                  0481 06499000
*           BC(4,IOCS);             /* IF THE DATA WAS INVALIDATED,     06500000
*                                      GO BACK AND START OVER AGAIN  */ 06501000
         BC    4,IOCS                                              0481 06502000
*         END;                                                     0482 06503000
*       GO TO CHKLOOP;              /* GO BACK AND SEE IF THE NEXT 0483 06504000
*                                      DECB IS POSTED                */ 06505000
         B     CHKLOOP                                             0483 06506000
*     END;                                                         0484 06507000
*                                                                  0485 06508000
*   /*****************************************************************/ 06509000
*   /*                                                               */ 06510000
*   /* THIS SECTION OF CODE CHECKS FOR A GTFBCB ON THE WRITE QUEUE   */ 06511000
*   /* AND AN AVAILABLE DECB. IF BOTH ARE FOUND THEY ARE PAIRED UP   */ 06512000
*   /* AND A WRITE IS DONE ON THE GTFBUFR                            */ 06513000
*   /*                                                               */ 06514000
*   /*****************************************************************/ 06515000
*                                                                  0485 06516000
*/*%WRITES: D (YES,,NO,%NOWRT) WQHEAD^=0 & NEXTWRT-> DECBBCB =0      */ 06517000
*/* D (YES,,NO,%RBX3) DIAGNOSTIC: WQLENGTH> WQMAX                    */ 06518000
*/* P DIAGNOSTIC: WQMAX=WQLENGTH                                     */ 06519000
*/*%RBX3: S GETQLOCK: OBTAIN THE GTFBCB QUEUE LOCK                   */ 06520000
*/* P NEXTWRT-> DECBBCB=WQHEAD WQHEAD= WQHEAD->WQCHAIN               */ 06521000
*/* P DIAGNOSTIC: DECREMENT WQLENGTH BY 1                            */ 06522000
*/* S FREQLOCK: RELEASE THE GTFBCB QUEUE LOCK                        */ 06523000
*/*A41CS: P DIAGNOSTIC: CS NEXTWRT-> DECBBCB-> WQFLAG=OFF DECBFLAG=ON   06524000
*/*                                                                  */ 06525000
*/* D (OK,,FAIL,A41CS) CS?                                           */ 06526000
*/* P PUT GTFBUFR ADDR INTO THE NEXTWRT-> DECB                       */ 06527000
*/*WRITE: S WRITE: THE GTFBUFR                                       */ 06528000
*/* P (D.A.,,TAPE,%XRB7) DIAGNOSTIC: INCREMENT NEXTWRT-> DECBWRT BY 1   06529000
*/*                                                                  */ 06530000
*                                                                  0485 06531000
*   IF WQHEAD^=0&NEXTWRT->DECBBCB=0 THEN                           0485 06532000
@RF00454 SLR   @10,@10                                             0485 06533000
         C     @10,WQHEAD(,PCTPTR)                                 0485 06534000
         BE    @RF00485                                            0485 06535000
         C     @10,DECBBCB(,NEXTWRT)                               0485 06536000
         BNE   @RF00485                                            0485 06537000
*     DO;                           /* YES, EVERYTHING NEEDED FOR A     06538000
*                                      WRITE IS PRESENT              */ 06539000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06540000
*                                                                  0487 06541000
*       IF WQLENGTH>WQMAX           /* SAVE THE HIGHEST NUMBER OF  0487 06542000
*                                      GTFBCB EVER ON THE WRITE QUEUE   06543000
*                                      AT ONE TIME                   */ 06544000
*         THEN                                                     0487 06545000
         L     @10,WQLENGTH(,PCTPTR)                               0487 06546000
         C     @10,WQMAX(,PCTPTR)                                  0487 06547000
         BNH   @RF00487                                            0487 06548000
*         WQMAX=WQLENGTH;                                          0488 06549000
*                                                                  0488 06550000
         ST    @10,WQMAX(,PCTPTR)                                  0488 06551000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06552000
*                                                                  0489 06553000
*       CALL GETQLOCK;                                             0489 06554000
@RF00487 BAL   @14,GETQLOCK                                        0489 06555000
*       NEXTWRT->DECBBCB=WQHEAD;    /* TAKE THE NEXT GTFBCB OFF THE     06556000
*                                      WRITE QUEUE AND HOOK IT UP  0490 06557000
*                                      WITH THE DECB TO BE USED FOR     06558000
*                                      THE NEXT WRITE                */ 06559000
         L     @10,WQHEAD(,PCTPTR)                                 0490 06560000
         ST    @10,DECBBCB(,NEXTWRT)                               0490 06561000
*       WQHEAD=WQHEAD->WQCHAIN;                                    0491 06562000
*                                                                  0491 06563000
         L     @10,WQCHAIN(,@10)                                   0491 06564000
         ST    @10,WQHEAD(,PCTPTR)                                 0491 06565000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06566000
*                                                                  0492 06567000
*       WQLENGTH=WQLENGTH-1;                                       0492 06568000
*                                                                  0492 06569000
         L     @10,WQLENGTH(,PCTPTR)                               0492 06570000
         BCTR  @10,0                                               0492 06571000
         ST    @10,WQLENGTH(,PCTPTR)                               0492 06572000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06573000
*                                                                  0493 06574000
*       CALL FREQLOCK;                                             0493 06575000
*                                                                  0493 06576000
         BAL   @14,FREQLOCK                                        0493 06577000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06578000
*                                                                  0494 06579000
*       COMPBCB=NEXTWRT->DECBBCB->BCBSWAP;/* THE AUDIT FLAGS MUST BE    06580000
*                                      UPDATE WITH MP LOGIC          */ 06581000
         LA    @10,WORKAREA                                        0494 06582000
         L     @01,DECBBCB(,NEXTWRT)                               0494 06583000
         L     @09,BCBSWAP(,@01)                                   0494 06584000
         ST    @09,COMPBCB(,@10)                                   0494 06585000
*A41CS:                                                            0495 06586000
*       SWAPBCB=COMPBCB;                                           0495 06587000
A41CS    LA    @10,WORKAREA                                        0495 06588000
         L     @09,COMPBCB(,@10)                                   0495 06589000
         ST    @09,SWAPBCB(,@10)                                   0495 06590000
*       WQFSWAP=OFF;                                               0496 06591000
*       DECBFSWA=ON;                                               0497 06592000
         OI    DECBFSWA(@10),B'00000010'                           0497 06593000
         NI    WQFSWAP(@10),B'10111111'                            0497 06594000
*       CS(COMPBCB,SWAPBCB,NEXTWRT->DECBBCB->BCBSWAP);             0498 06595000
         L     @09,COMPBCB(,@10)                                   0498 06596000
         L     @07,SWAPBCB(,@10)                                   0498 06597000
         L     @01,DECBBCB(,NEXTWRT)                               0498 06598000
         CS    @09,@07,BCBSWAP(@01)                                0498 06599000
         BZ    @BC00498                                            0498 06600000
         ST    @09,COMPBCB(,@10)                                   0498 06601000
@BC00498 DS    0H                                                  0499 06602000
*       BC(4,A41CS);                                               0499 06603000
*                                                                  0499 06604000
         BC    4,A41CS                                             0499 06605000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06606000
*                                                                  0500 06607000
*       NEXTWRT->DECBBUFR=NEXTWRT->DECBBCB->GBUFRPTR;/* PUT THE ADDR    06608000
*                                      OF THE GTFBUFR TO BE WRITTEN     06609000
*                                      IN THE DECB                   */ 06610000
         L     @01,DECBBCB(,NEXTWRT)                               0500 06611000
         L     @10,GBUFRPTR(,@01)                                  0500 06612000
         ST    @10,DECBBUFR(,NEXTWRT)                              0500 06613000
*WRITE:                                                            0501 06614000
*       GEN CODE REFS(NEXTWRT,GTFBUFR,DECBMAP) NOSETS NOEXIT NOFLOWS    06615000
*           SEQFLOW NODEFS(WRITE  (NEXTWRT),SF,MF=E);/* INITIATE I/O */ 06616000
WRITE    WRITE  (NEXTWRT),SF,MF=E                                       06617000
*       NEXTWRT->DECBWRT=NEXTWRT->DECBWRT+1;                       0502 06618000
         LA    @10,1                                               0502 06619000
         AL    @10,DECBWRT(,NEXTWRT)                               0502 06620000
         ST    @10,DECBWRT(,NEXTWRT)                               0502 06621000
*                                                                  0503 06622000
*       /*************************************************************/ 06623000
*       /*                                                           */ 06624000
*       /* ONCE THE WRITE IS COMPLETE POINT TO THE NEXT DECB TO BE   */ 06625000
*       /* USED FOR A WRITE AND BRANCH BACK TO CHECK FOR ANY WRITES  */ 06626000
*       /* THAT HAVE COMPLETED                                       */ 06627000
*       /*                                                           */ 06628000
*       /*************************************************************/ 06629000
*                                                                  0503 06630000
*/*%XRB7: P (,CHKLOOP) MOVE NEXTWRT TO THE NEXT DECB ON THE CHAIN    */ 06631000
*                                                                  0503 06632000
*       NEXTWRT=NEXTWRT->DECBCHN;   /* MOVE THE NEXT TO BE WRITTEN 0503 06633000
*                                      DECB PTR TO THE NEXT ONE ON 0503 06634000
*                                      THE CHAIN                     */ 06635000
         L     NEXTWRT,DECBCHN(,NEXTWRT)                           0503 06636000
*       GO TO CHKLOOP;              /* GO BACK AND CHECK TO SEE IF 0504 06637000
*                                      ANY PREVIOUS I/O HAS COMPLETED   06638000
*                                      AND IF ANYMORE CAN BE       0504 06639000
*                                      INITIATED                     */ 06640000
         B     CHKLOOP                                             0504 06641000
*/*%NOWRT: P CLEAR AFBUFOFF                                          */ 06642000
*                                                                  0505 06643000
*       /*************************************************************/ 06644000
*       /*                                                           */ 06645000
*       /* WHEN EXECUTION REACHES THIS POINT AS MANY GTFBCB AS       */ 06646000
*       /* POSSIBLE HAVE BEEN REMOVED FROM THE WRITE QUEUE           */ 06647000
*       /*                                                           */ 06648000
*       /*************************************************************/ 06649000
*                                                                  0505 06650000
*     END;                                                         0505 06651000
*   AFBUFOFF=OFF;                   /* TURN OFF THE AUDIT FLAG FOR 0506 06652000
*                                      THIS SECTION OF CODE          */ 06653000
@RF00485 DS    0H                                                  0507 06654000
*                                                                  0507 06655000
*   /*****************************************************************/ 06656000
*   /*                                                               */ 06657000
*   /* THIS SECTION OF CODE REMOVES ALL THE GTFBCB CURRENTLY ON THE  */ 06658000
*   /* RELEASE QUEUE, DOES A PAGE RELEASE ON EACH ASSOCIATED GTFBUFR,*/ 06659000
*   /* AND THEN PUTS THE GTFBCB ONTO THE EMPTY QUEUE                 */ 06660000
*   /*                                                               */ 06661000
*   /*****************************************************************/ 06662000
*                                                                  0507 06663000
*/* P SET AFPGRLSE                                                   */ 06664000
*/* D (YES,%NOIOX,NO,) RQHEAD=0                                      */ 06665000
*/* P BCBHOLDR=RQHEAD                                                */ 06666000
*/*CSRQHEAD: P CS 0 INTO RQHEAD                                      */ 06667000
*/* D (FAIL,CSRQHEAD,OK,) CS                                         */ 06668000
*/* P DIAGNOSTIC: INCREMENT RQPURGE BY 1 & RQCOUNT=0                 */ 06669000
*/* P RLSEBUF= BCBHOLDR                                              */ 06670000
*/*RLSELOOP: S PGRLSE: RLSEBUF-> GTFBCB-> GTFBUFR                    */ 06671000
*/* P DIAGNOSTIC: RLSEBUF-> RQFLAG=OFF & EQFLAG=ON, INCR RQCOUNT BY 1   06672000
*/*                                                                  */ 06673000
*/* D (NO,,YES,%J) RLSEBUF-> RQCHAIN=0                               */ 06674000
*/* P (,RLSELOOP) RLSEBUF= RLSEBUF-> RQCHAIN                         */ 06675000
*/*%J: P DIAGNOSTIC: RQTOTAL= RQTOTAL+RQCOUNT                        */ 06676000
*/* S GETQLOCK: OBTAIN THE GTFBCB QUEUE LOCK                         */ 06677000
*/* P RLSEBUF-> EQCHAIN= EQHEAD                                      */ 06678000
*/* N EQCHAIN & RQCHAIN BOTH REFER TO THE SAME GTFBCB FIELD          */ 06679000
*/* P EQHEAD=BCBHOLDR                                                */ 06680000
*/* P DIAGNOSTIC: EQLENGTH= EQLENGTH+ RQCOUNT                        */ 06681000
*/* S FREQLOCK: RELEASE THE GTFBCB QUEUE LOCK                        */ 06682000
*/*%NOIOX: P CLEAR AFPGRLSE                                          */ 06683000
*                                                                  0507 06684000
*   AFPGRLSE=ON;                    /* SET THE FLAG FOR THIS SECTION    06685000
*                                      OF CODE AUDIT                 */ 06686000
         OI    AFPGRLSE(PCTPTR),B'00000100'                        0507 06687000
         NI    AFBUFOFF(PCTPTR),B'11101111'                        0507 06688000
*   IF RQHEAD^=0                    /* ARE THERE ANY GTFBCB ON THE 0508 06689000
*                                      RELEASE QUEUE? OF NOT SKIP  0508 06690000
*                                      THIS SECTION                  */ 06691000
*     THEN                                                         0508 06692000
         L     @10,RQHEAD(,PCTPTR)                                 0508 06693000
         LTR   @10,@10                                             0508 06694000
         BZ    @RF00508                                            0508 06695000
*     DO;                           /* YES, THERE ARE GTFBCB ON THE     06696000
*                                      RELEASE QUEUE                 */ 06697000
*       BCBHOLDR=RQHEAD;            /* GET THE ADDRESS OF THE FIRST     06698000
*                                      GTFBCB ON THE RELEASE QUEUE   */ 06699000
         ST    @10,BCBHOLDR                                        0510 06700000
*CSRQHEAD:                                                         0511 06701000
*       CS(BCBHOLDR,0,RQHEAD);      /* ATTEMPT TO PUT A 0 IN RQHEAD     06702000
*                                      TO REMOVE ALL THE GTFBCB      */ 06703000
CSRQHEAD L     @10,BCBHOLDR                                        0511 06704000
         SLR   @09,@09                                             0511 06705000
         CS    @10,@09,RQHEAD(PCTPTR)                              0511 06706000
         BZ    @BC00511                                            0511 06707000
         ST    @10,BCBHOLDR                                        0511 06708000
@BC00511 DS    0H                                                  0512 06709000
*       BC(4,CSRQHEAD);             /* IF THE CS FAILS A GTFBCB WAS     06710000
*                                      ADDED TO THE RELEASE QUEUE  0512 06711000
*                                      WHICH MAKES BCBHOLDR INVALID     06712000
*                                      SO GO BACK & START OVER. THE     06713000
*                                      CS HAS ALREADY STORED THE   0512 06714000
*                                      LATEST RQHEAD INTO BCBHOLDR   */ 06715000
         BC    4,CSRQHEAD                                          0512 06716000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06717000
*                                                                  0513 06718000
*       RQPURGE=RQPURGE+1;                                         0513 06719000
         LA    @10,1                                               0513 06720000
         AL    @10,RQPURGE(,PCTPTR)                                0513 06721000
         ST    @10,RQPURGE(,PCTPTR)                                0513 06722000
*       RQCOUNT=0;                                                 0514 06723000
*                                                                  0514 06724000
         SLR   @10,@10                                             0514 06725000
         ST    @10,RQCOUNT                                         0514 06726000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06727000
*                                                                  0515 06728000
*       RLSEBUF=BCBHOLDR;           /* START WITH THE FIRST GTFBCB IN   06729000
*                                      THE GROUP                     */ 06730000
         L     @10,BCBHOLDR                                        0515 06731000
         ST    @10,RLSEBUF                                         0515 06732000
*RLSELOOP:                                                         0516 06733000
*       RFY                                                        0516 06734000
*        (MACREG1,                                                 0516 06735000
*         MACREG2) RSTD;                                           0516 06736000
RLSELOOP DS    0H                                                  0517 06737000
*       MACREG1=ADDR(RLSEBUF->GBUFRPTR->GTFBUFR);                  0517 06738000
         L     @10,RLSEBUF                                         0517 06739000
         L     MACREG1,GBUFRPTR(,@10)                              0517 06740000
*       MACREG2=MACREG1+LENGTH(GTFBUFR);                           0518 06741000
         LR    MACREG2,MACREG1                                     0518 06742000
         AL    MACREG2,@CF02153                                    0518 06743000
*       GEN CODE(PGRLSE  LA=(MACREG1),HA=(MACREG2))                0519 06744000
*       REFS(MACREG1,MACREG2) SETS(GTFBUFR) NOEXIT NODEFS NOFLOWS  0519 06745000
*           SEQFLOW;                /* THIS WILL CAUSE ALL THE REAL     06746000
*                                      STORAGE AND AUX STORAGE     0519 06747000
*                                      ASSOCIATED WITH THE VIRTUAL 0519 06748000
*                                      STORAGE TO THIS GTFBUFR TO BE    06749000
*                                      RETURNED TO THE SYSTEM. THE 0519 06750000
*                                      CONTENTS ARE DISCARDED. THIS     06751000
*                                      IS DONE SO THAT AHLSBUF WILL     06752000
*                                      NOT CAUSE A PAGE IN WHEN IT 0519 06753000
*                                      MOVES MORE DATA INTO THIS   0519 06754000
*                                      GTFBUFR                       */ 06755000
         PGRLSE  LA=(MACREG1),HA=(MACREG2)                              06756000
*       RFY                                                        0520 06757000
*        (MACREG1,                                                 0520 06758000
*         MACREG2) UNRSTD;                                         0520 06759000
*                                                                  0520 06760000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06761000
*                                                                  0521 06762000
*       RLSEBUF->RQFLAG=OFF;                                       0521 06763000
*       RLSEBUF->EQFLAG=ON;                                        0522 06764000
         L     @10,RLSEBUF                                         0522 06765000
         OI    EQFLAG(@10),B'10000000'                             0522 06766000
         NI    RQFLAG(@10),B'11111011'                             0522 06767000
*       RQCOUNT=RQCOUNT+1;                                         0523 06768000
*                                                                  0523 06769000
         LA    @09,1                                               0523 06770000
         AL    @09,RQCOUNT                                         0523 06771000
         ST    @09,RQCOUNT                                         0523 06772000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06773000
*                                                                  0524 06774000
*       IF RLSEBUF->RQCHAIN^=0      /* HAS THE END OF THE GROUP BEEN    06775000
*                                      REACHED                       */ 06776000
*         THEN                                                     0524 06777000
         L     @10,RQCHAIN(,@10)                                   0524 06778000
         LTR   @10,@10                                             0524 06779000
         BZ    @RF00524                                            0524 06780000
*         DO;                       /* YES, GO PUT THE GROUP ONTO THE   06781000
*                                      ENPTY QUEUE                   */ 06782000
*           RLSEBUF=RLSEBUF->RQCHAIN;/* NO, THEN MOVE ON TO PROCESS     06783000
*                                      THE NEXT ONE                  */ 06784000
         ST    @10,RLSEBUF                                         0526 06785000
*           GO TO RLSELOOP;                                        0527 06786000
         B     RLSELOOP                                            0527 06787000
*         END;                      /* ALL PAGE RELEASES HAVE BEEN 0528 06788000
*                                      DONE                          */ 06789000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06790000
*                                                                  0529 06791000
*       RQTOTAL=RQTOTAL+RQCOUNT;                                   0529 06792000
*                                                                  0529 06793000
@RF00524 L     @10,RQTOTAL(,PCTPTR)                                0529 06794000
         AL    @10,RQCOUNT                                         0529 06795000
         ST    @10,RQTOTAL(,PCTPTR)                                0529 06796000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06797000
*                                                                  0530 06798000
*       CALL GETQLOCK;              /* OBTAIN CONTROL OF THE QLOCK SO   06799000
*                                      THAT EQHEAD MAY BE ALTERED    */ 06800000
         BAL   @14,GETQLOCK                                        0530 06801000
*       RLSEBUF->EQCHAIN=EQHEAD;    /* RLSEBUF NOW POINTS TO THE LAST   06802000
*                                      GTFBCB ON THE STRING AND    0531 06803000
*                                      BCBHOLDR TO THE FIRST         */ 06804000
         L     @10,EQHEAD(,PCTPTR)                                 0531 06805000
         L     @09,RLSEBUF                                         0531 06806000
         ST    @10,EQCHAIN(,@09)                                   0531 06807000
*       EQHEAD=BCBHOLDR;            /* INSERT THE GROUP OF GTFBCB AT    06808000
*                                      THE HEAD OF THE EMPTY QUEUE   */ 06809000
         L     @10,BCBHOLDR                                        0532 06810000
         ST    @10,EQHEAD(,PCTPTR)                                 0532 06811000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06812000
*                                                                  0533 06813000
*       EQLENGTH=EQLENGTH+RQCOUNT;  /* INCREMENT THE COUNT OF GTFBCB    06814000
*                                      ON THE EMPTY QUEUE BY THE   0533 06815000
*                                      NUMBER IN THE GROUP           */ 06816000
         L     @10,EQLENGTH(,PCTPTR)                               0533 06817000
         AL    @10,RQCOUNT                                         0533 06818000
         ST    @10,EQLENGTH(,PCTPTR)                               0533 06819000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06820000
*                                                                  0534 06821000
*       CALL FREQLOCK;              /* RELEASE THE QLOCK             */ 06822000
         BAL   @14,FREQLOCK                                        0534 06823000
*     END;                                                         0535 06824000
*   AFPGRLSE=OFF;                   /* TURN OFF THE AUDIT FLAG FOR 0536 06825000
*                                      THIS SECTION                  */ 06826000
@RF00508 DS    0H                                                  0537 06827000
*                                                                  0537 06828000
*   /*****************************************************************/ 06829000
*   /*                                                               */ 06830000
*   /* THIS SECTION OF CODE CHECKS TO SEE IF THE NUMBER OF AVAILABLE */ 06831000
*   /* GTFBCB (GTFBUFR) ON THE EMPTY QUEUE IS LESS THAN THE DESIRED  */ 06832000
*   /* MINIMUM RESERVE AND GETS MORE GTFBUFR IF POSSIBLE             */ 06833000
*   /*                                                               */ 06834000
*   /*****************************************************************/ 06835000
*                                                                  0537 06836000
*/* P SET AFEQFIX                                                    */ 06837000
*/* P BCBHOLDR= EQHEAD, I=1                                          */ 06838000
*/*%ET: D (YES,,NO,%ELOP) BCBHOLDR=0                                 */ 06839000
*/* N NO REPAIRS DURING TERMINATION OR IN THE CASE OF AN ERROR       */ 06840000
*/* D (YES,XWRITEST,NO,) IS WSERFLAG SET                             */ 06841000
*/* N DIAGNOSTIC MESSAGE: "EQ<MIN", EQLENGTH, & WQLENGTH             */ 06842000
*/* P DIAGNOSTIC: INCREMENT EQSHORT BY 1                             */ 06843000
*/* S EBUFGET: GET A STRING OF GTFBCB & GTFBUFR                      */ 06844000
*/* D (YES,EQAFOFF,NO,) GQHEAD=0                                     */ 06845000
*/* S GETQLOCK: OBTAIN THE GTFBCB QUEUE LOCK                         */ 06846000
*/* P GQTAIL-> EQCHAIN=EQHEAD & EQHEAD=GQHEAD                        */ 06847000
*/* D (YES,,NO,%DAG3) DIAGNOSTIC: EQLENGTH< EQMIN                    */ 06848000
*/* P DIAGNOSTIC: EQMIN=EQLENGTH                                     */ 06849000
*/*%DAG3: P DIAGNOSTIC: INCR EQLENGTH BY THE NUM OF GTFBCB IN 1 PAGE */ 06850000
*/* S (,EQAFOFF) FREQLOCK: RELEASE THE GTFBCB QUEUE LOCK             */ 06851000
*/*%ELOP: P BCBHOLDR= BCBHOLDR-> EQCHAIN, I=I+1                      */ 06852000
*/* D (YES,EQAFOFF,NO,%ET) I=EQRESERV                                */ 06853000
*/*EQAFOFF: P CLEAR AFEQFIX                                          */ 06854000
*                                                                  0537 06855000
*   AFEQFIX=ON;                     /* SET THE AUDIT FLAG FOR THIS 0537 06856000
*                                      SECTION OF CODE               */ 06857000
         OI    AFEQFIX(PCTPTR),B'00000010'                         0537 06858000
         NI    AFPGRLSE(PCTPTR),B'11111011'                        0537 06859000
*   BCBHOLDR=EQHEAD;                /* THIS LOOP CHECKS TO BE SURE 0538 06860000
*                                      THERE IS AT LEAST A MINIMUM 0538 06861000
*                                      NUMBER OF GTFBCB ON THE EMPTY    06862000
*                                      QUEUE                         */ 06863000
         L     @10,EQHEAD(,PCTPTR)                                 0538 06864000
         ST    @10,BCBHOLDR                                        0538 06865000
*   DO I=1 TO EQRESERV;                                            0539 06866000
         LA    @10,1                                               0539 06867000
         ST    @10,I                                               0539 06868000
@DL00539 DS    0H                                                  0540 06869000
*     IF BCBHOLDR=0                 /* HAS THE END OF THE QUEUE BEEN    06870000
*                                      REACHED YET                   */ 06871000
*       THEN                                                       0540 06872000
         L     @10,BCBHOLDR                                        0540 06873000
         LTR   @10,@10                                             0540 06874000
         BNZ   @RF00540                                            0540 06875000
*       DO;                         /* IF THE NUMBER IS LESS THAN THE   06876000
*                                      MINIMUM RESERVE TRY TO GET  0541 06877000
*                                      MORE                          */ 06878000
*         IF WSERFLAG=ON            /* IF ANY OF THE ERROR CONDITION    06879000
*                                      INDICATED BY THIS FLAG HAS  0542 06880000
*                                      OCCURRED THE TRACE WRITER IS     06881000
*                                      GOING THROUGH ERROR         0542 06882000
*                                      TERMINATION AND THEREFORE ALL    06883000
*                                      UNNECESSARY PROCESSING IS   0542 06884000
*                                      BEING BYPASSED TO AVOID     0542 06885000
*                                      TROUBLE                       */ 06886000
*           THEN                                                   0542 06887000
         TM    WSERFLAG(PCTPTR),B'00000010'                        0542 06888000
         BO    @RT00542                                            0542 06889000
*           GO TO XWRITEST;                                        0543 06890000
*                                                                  0543 06891000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06892000
*/* THIS WOULD BE A GOOD PLACE FOR A WTO                             */ 06893000
*                                                                  0544 06894000
*         EQSHORT=EQSHORT+1;                                       0544 06895000
*                                                                  0544 06896000
         LA    @10,1                                               0544 06897000
         AL    @10,EQSHORT(,PCTPTR)                                0544 06898000
         ST    @10,EQSHORT(,PCTPTR)                                0544 06899000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06900000
*                                                                  0545 06901000
*         CALL EBUFGET;             /* THIS SUBROUTINE WILL RETURN A    06902000
*                                      STRING OF ONE PAGE FULL OF  0545 06903000
*                                      GTFBCB READY TO GO ONTO THE 0545 06904000
*                                      EMPTY QUEUE                   */ 06905000
         BAL   @14,EBUFGET                                         0545 06906000
*         IF GQHEAD=0               /* CHECK TO SEE IF EBUFGET FAILED*/ 06907000
*           THEN                                                   0546 06908000
         L     @10,GQHEAD                                          0546 06909000
         LTR   @10,@10                                             0546 06910000
         BZ    @RT00546                                            0546 06911000
*           GO TO EQAFOFF;          /* YES,NO MORE GTFBCB CAN BE   0547 06912000
*                                      GOTTEN. EITHER A GETMAIN OR 0547 06913000
*                                      PGFIX FAILED OR THE MAX NUMBER   06914000
*                                      HAS BEEN REACHED              */ 06915000
*         CALL GETQLOCK;            /* OBTAIN THE QLOCK SO THE NEW 0548 06916000
*                                      GTFBCB MAY BE ADDED TO THE  0548 06917000
*                                      EMPTY QUEUE                   */ 06918000
         BAL   @14,GETQLOCK                                        0548 06919000
*         GQTAIL->EQCHAIN=EQHEAD;   /* GQTAIL WILL POINT TO THE LAST    06920000
*                                      GTFBCB IN THE STRING          */ 06921000
         L     @10,EQHEAD(,PCTPTR)                                 0549 06922000
         L     @09,GQTAIL                                          0549 06923000
         ST    @10,EQCHAIN(,@09)                                   0549 06924000
*         EQHEAD=GQHEAD;            /* GQHEAD WILL POINT TO THE FIRST   06925000
*                                      GTFBCB IN THE STRING          */ 06926000
         L     @10,GQHEAD                                          0550 06927000
         ST    @10,EQHEAD(,PCTPTR)                                 0550 06928000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 06929000
*                                                                  0551 06930000
*         IF EQLENGTH<EQMIN THEN                                   0551 06931000
         L     @10,EQLENGTH(,PCTPTR)                               0551 06932000
