*                                      COMBINED INTO RQTOTAL         */ 04980000
*   DCL                                                            0120 04981000
*     1 LOSTBLOK,                   /* THIS IS A PROTOTYPE LOST    0120 04982000
*                                      GTFBLOK RECORD                */ 04983000
*      2 * CHAR(6) INIT('001200000003'X),/* AID=00 FID=03            */ 04984000
*      2 LBTZ CHAR(4),                                             0120 04985000
*      2 LBTIME CHAR(8);                                           0120 04986000
*   DCL                                                            0121 04987000
*     1 PDQASTCB,                   /* PARAMETERS FOR THE PURGEDQ  0121 04988000
*                                      MACRO                         */ 04989000
*      2 * PTR(16) INIT('0'),                                      0121 04990000
*      2 PDQASID FIXED(15),                                        0121 04991000
*      2 PDQTCB PTR(31),                                           0121 04992000
*     PDQRMTR PTR(31);                                             0121 04993000
*   DCL                                                            0122 04994000
*     WORKAREA CHAR(8) BDY(WORD);   /* THIS IS USED ALONG WITH A   0122 04995000
*                                      PORTION OF THE GTFBCB MAPPING    04996000
*                                      FOR DOING CS                  */ 04997000
*   RFY                                                            0123 04998000
*     CMPWKA BASED(ADDR(WORKAREA));                                0123 04999000
*   DCL                                                            0124 05000000
*     SBLOKSAV PTR(31) INIT(0),                                    0124 05001000
*     EPCTPTR PTR(31) STATIC;       /* SAVEAREA FOR THE GTFPCT ADDR     05002000
*                                      FOR REFRESH OF PCTPTR IN THE     05003000
*                                      ESTAE ROUTINE                 */ 05004000
*   DCL                                                            0125 05005000
*     FAKEWSA STATIC CHAR(LENGTH(MCAWSA)) BDY(WORD);               0125 05006000
*                                                                  0125 05007000
*   /*****************************************************************/ 05008000
*   /*                                                               */ 05009000
*   /* TRACE DATASET DCB FOR EXTERNAL TO TAPE                        */ 05010000
*   /*                                                               */ 05011000
*   /*****************************************************************/ 05012000
*                                                                  0126 05013000
*   GEN DATA DEFS(TRCDCB);                                         0126 05014000
*   DCL                                                            0127 05015000
*     1 DECBMAP BASED,              /* MAPPING OF BSAM DECB          */ 05016000
*      2 DECBECB BIT(32),           /* ECB POST WHEN I/O COM LETE    */ 05017000
*       3 DECBECB1 BIT(8),          /* USED TO TEST THE POST CODE    */ 05018000
*        4 * BIT(1),                                               0127 05019000
*        4 DECBPOST BIT(1),         /* USED TO TEST POST BIT         */ 05020000
*      2 * PTR(31),                                                0127 05021000
*      2 * PTR(31),                                                0127 05022000
*      2 DECBBUFR PTR(31),          /* ADDR OF GTFBUFR BEING WRITTEN */ 05023000
*      2 * PTR(31),                                                0127 05024000
*      2 * PTR(31),                                                0127 05025000
*      2 DECBBCB PTR(31),           /* ADDR OF THE GTFBCB FOR THE  0127 05026000
*                                      GTFBUFR BEING WRITTEN THIS IS    05027000
*                                      SET TO 0 TO INDICATE THAT THE    05028000
*                                      DECB IS AVAILABLE             */ 05029000
*      2 DECBCHN PTR(31),           /* CHAIN FIELD POINTING TO THE 0127 05030000
*                                      NEXT DECB                     */ 05031000
*      2 DECBWRT PTR(31),           /* # OF NORMAL WRITES            */ 05032000
*      2 DECBWWRT PTR(31),          /* # OF REWITES FOR WRAP         */ 05033000
*      2 DECBCHK PTR(31),           /* # OF NORMAL CHECKS            */ 05034000
*      2 DECBWCHK PTR(31);          /* # OR CHECKS FOR WRAP          */ 05035000
*   DCL                                                            0128 05036000
*     1 DECBLOCK,                   /* DECB FOR WRITES TO THE TRACE     05037000
*                                      DATASET                       */ 05038000
*      2 DECB1,                                                    0128 05039000
*       3 IOECB1 PTR(31) INIT(0),                                  0128 05040000
*       3 * PTR(31) INIT(0),                                       0128 05041000
*       3 * PTR(31) INIT(ADDR(TRCDCB)),                            0128 05042000
*       3 * PTR(31) INIT(0),                                       0128 05043000
*       3 * PTR(31) INIT(0),                                       0128 05044000
*       3 * PTR(31) INIT(0),                                       0128 05045000
*       3 * PTR(31) INIT(0),                                       0128 05046000
*       3 * PTR(31) INIT(ADDR(DECB2)),                             0128 05047000
*       3 * PTR(31) INIT(0),                                       0128 05048000
*       3 * PTR(31) INIT(0),                                       0128 05049000
*       3 * PTR(31) INIT(0),                                       0128 05050000
*       3 * PTR(31) INIT(0),                                       0128 05051000
*      2 DECB2,                                                    0128 05052000
*       3 IOECB2 PTR(31) INIT(0),                                  0128 05053000
*       3 * PTR(31) INIT(0),                                       0128 05054000
*       3 * PTR(31) INIT(ADDR(TRCDCB)),                            0128 05055000
*       3 * PTR(31) INIT(0),                                       0128 05056000
*       3 * PTR(31) INIT(0),                                       0128 05057000
*       3 * PTR(31) INIT(0),                                       0128 05058000
*       3 * PTR(31) INIT(0),                                       0128 05059000
*       3 * PTR(31) INIT(ADDR(DECB3)),                             0128 05060000
*       3 * PTR(31) INIT(0),                                       0128 05061000
*       3 * PTR(31) INIT(0),                                       0128 05062000
*       3 * PTR(31) INIT(0),                                       0128 05063000
*       3 * PTR(31) INIT(0),                                       0128 05064000
*      2 DECB3,                                                    0128 05065000
*       3 IOECB3 PTR(31) INIT(0),                                  0128 05066000
*       3 * PTR(31) INIT(0),                                       0128 05067000
*       3 * PTR(31) INIT(ADDR(TRCDCB)),                            0128 05068000
*       3 * PTR(31) INIT(0),                                       0128 05069000
*       3 * PTR(31) INIT(0),                                       0128 05070000
*       3 * PTR(31) INIT(0),                                       0128 05071000
*       3 * PTR(31) INIT(0),                                       0128 05072000
*       3 * PTR(31) INIT(ADDR(DECB4)),                             0128 05073000
*       3 * PTR(31) INIT(0),                                       0128 05074000
*       3 * PTR(31) INIT(0),                                       0128 05075000
*       3 * PTR(31) INIT(0),                                       0128 05076000
*       3 * PTR(31) INIT(0),                                       0128 05077000
*      2 DECB4,                                                    0128 05078000
*       3 IOECB4 PTR(31) INIT(0),                                  0128 05079000
*       3 * PTR(31) INIT(0),                                       0128 05080000
*       3 * PTR(31) INIT(ADDR(TRCDCB)),                            0128 05081000
*       3 * PTR(31) INIT(0),                                       0128 05082000
*       3 * PTR(31) INIT(0),                                       0128 05083000
*       3 * PTR(31) INIT(0),                                       0128 05084000
*       3 * PTR(31) INIT(0),                                       0128 05085000
*       3 * PTR(31) INIT(ADDR(DECB1)),                             0128 05086000
*       3 * PTR(31) INIT(0),                                       0128 05087000
*       3 * PTR(31) INIT(0),                                       0128 05088000
*       3 * PTR(31) INIT(0),                                       0128 05089000
*       3 * PTR(31) INIT(0);                                       0128 05090000
*   DCL                                                            0129 05091000
*     DTWORK CHAR(24) BDY(WORD);    /* WORK SPACE FOR THE DEVTYPE  0129 05092000
*                                      MACRO                         */ 05093000
*   DCL                                                            0130 05094000
*     PGADDR PTR(31),               /* USED IN SUBROUTINE PGLSTIN    */ 05095000
*     ASTYPE BIT(8),                                               0130 05096000
*     ITEM CHAR(1),                                                0130 05097000
*     ILENGTH PTR(31);                                             0130 05098000
*                                                                  0131 05099000
*   /*****************************************************************/ 05100000
*   /*                                                               */ 05101000
*   /* THIS SEGMENT CONTAINS THE CODE FOR INITIALIZATION OF THE TRACE*/ 05102000
*   /* WRITERS PLUS AHLSBLOK, AHLSFEOB, AHLSBUF, AND AHLREADR        */ 05103000
*   /*                                                               */ 05104000
*   /*****************************************************************/ 05105000
*                                                                  0131 05106000
*/* P GET THE GTFPCT PTR FROM THE MCQE                               */ 05107000
*/* P EPCTPTR=PCTPTR SAVE THE GTFPCT ADDR FOR AHLWSTAE               */ 05108000
*/* S MODESET: SUPER STATE                                           */ 05109000
*                                                                  0131 05110000
*   PCTPTR=PARAMETR->MCQEAT;        /* INIT GTFPCT BASE FROM THE MCQE   05111000
*                                      PASSED BY THE MOTHER TASK     */ 05112000
         L     PCTPTR,MCQEAT(,PARAMETR)                            0131 05113000
*   RFY                                                            0132 05114000
*     PARAMETR UNRSTD;                                             0132 05115000
*   RFY                                                            0133 05116000
*     MCQE BASED(MCQEPTR);          /* THE MCQE CAN NOW BE LOCATED 0133 05117000
*                                      VIA THE GTFPCT                */ 05118000
*   EPCTPTR=PCTPTR;                 /* SAVE THE GTFPCT ADDRESS FOR 0134 05119000
*                                      POSSIBLE USE BY THE ESTAE   0134 05120000
*                                      ROUTINE AHLWSTAE              */ 05121000
         ST    PCTPTR,EPCTPTR                                      0134 05122000
*   DO;                             /* MODESET MODE(SUP)             */ 05123000
*     I10701='00000000'B;           /* OR MASK                       */ 05124000
         MVI   I10701,B'00000000'                                  0136 05125000
*     I10702='00000000'B;           /* AND MASK                      */ 05126000
         MVI   I10702,B'00000000'                                  0137 05127000
*     I10703='00000000'B;           /* RESERVED                      */ 05128000
         MVI   I10703,B'00000000'                                  0138 05129000
*     I10704='00001100'B;                                          0139 05130000
         MVI   I10704,B'00001100'                                  0139 05131000
*     RESPECIFY                                                    0140 05132000
*       GPR01F RESTRICTED;                                         0140 05133000
*     GPR01F=I107;                  /* MODESET PARMLIST              */ 05134000
         L     GPR01F,I107                                         0141 05135000
*     SVC(107);                     /* ISSUE MODESET SVC             */ 05136000
         SVC   107                                                 0142 05137000
*     RESPECIFY                                                    0143 05138000
*      (GPR01F) UNRESTRICTED;                                      0143 05139000
*   END;                            /* MODESET MODE(SUP) CHANGE TO 0144 05140000
*                                      SUPER STATE AS WELL AS KEY 0  */ 05141000
*                                                                  0144 05142000
*   /*****************************************************************/ 05143000
*   /*                                                               */ 05144000
*   /* THIS ESTABLISHES AN ESTAE ENVIRONMENT WHICH WILL REMAIN IN    */ 05145000
*   /* EFFECT UNTIL JUST BEFORE TERMINATION OF THE TRACE WRITER      */ 05146000
*   /*                                                               */ 05147000
*   /*****************************************************************/ 05148000
*                                                                  0145 05149000
*/* S ESTAE: ESTABLISH AHLWSTAE AS THE ESTAE E.P.                    */ 05150000
*/* D (OK,%EQK,BAD,) RC=0 ESTAE OK                                   */ 05151000
*/* S WTO: MESSAGE AHL037I                                           */ 05152000
*/* R RETURN                                                         */ 05153000
*/*%EQK: P (INT,,TAPE,%DL10,D.A.,%DL20) SET AFINIT                   */ 05154000
*/* P (,%DL30) SET AFIWRIT                                           */ 05155000
*/*%DL10: P (,%DL30) SET AFCWRIT                                     */ 05156000
*/*%DL20: P SET AFWWRIT                                              */ 05157000
*/*%DL30: P DIAGNOSTIC: WTRDLST= ADDR(SDUMPLIST)                     */ 05158000
*/* N SDUMPLST HAS THE WRITER MODULE CODE & DATA AREAS AS ITS FIRST     05159000
*/*ENTRY                                                             */ 05160000
*                                                                  0145 05161000
*   RFY                                                            0145 05162000
*    (MACREG1,                                                     0145 05163000
*     RETCODE) RSTD;                                               0145 05164000
*   MACREG1=ADDR(GTFPCT);           /* THIS IS DONE TO GUARANTEE THAT   05165000
*                                      THE ADDRESS OF THE GTFPCT IS     05166000
*                                      IN A REG FOR USE IN THE ESTAE    05167000
*                                      MACRO                         */ 05168000
         LR    MACREG1,PCTPTR                                      0146 05169000
*   GEN CODE(ESTAE   AHLWSTAE,CT,PARAM=(MACREG1),ASYNCH=YES,RECORD=YES) 05170000
*   REFS(PCTPTR,MACREG1) SETS(RETCODE) NOFLOWS NODEFS SEQFLOW;     0147 05171000
         ESTAE   AHLWSTAE,CT,PARAM=(MACREG1),ASYNCH=YES,RECORD=YES      05172000
*   IF RETCODE^=0 THEN                                             0148 05173000
         LTR   RETCODE,RETCODE                                     0148 05174000
         BZ    @RF00148                                            0148 05175000
*     DO;                           /* CONTINUE ONLY IF THE ESTAE  0149 05176000
*                                      RETURN CODE IS 0, TOTALLY   0149 05177000
*                                      GOOD. OTHERWISE TERMINATE     */ 05178000
*       RFY                                                        0150 05179000
*        (RETCODE,                                                 0150 05180000
*         MACREG1) UNRSTD;                                         0150 05181000
*       DO;                         /* WTOP MF(E,AHL037)             */ 05182000
*         RFY                                                      0152 05183000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 05184000
*         GPR01P=ADDR(AHL037);      /* ADDRESS CONTROL PROGRAM LIST  */ 05185000
         L     GPR01P,@CA02280                                     0153 05186000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 05187000
         SVC   35                                                  0154 05188000
*         RFY                                                      0155 05189000
*           GPR01P UNRSTD;          /* RELEASE REG 1                 */ 05190000
*       END;                        /* WTOP MF(E,AHL037) WRITE OUT 0156 05191000
*                                      THE MESSAGE FOR ESTAE FAILURE */ 05192000
*       RETURN;                     /* TERMINATE THE TRACE WRITER    */ 05193000
@EL00001 L     @13,4(,@13)                                         0157 05194000
@EF00001 DS    0H                                                  0157 05195000
@ER00001 LM    @14,@12,12(@13)                                     0157 05196000
         BR    @14                                                 0157 05197000
*     END;                                                         0158 05198000
*   AFINIT=ON;                      /* SET THE AUDIT FLAG TO SHOW  0159 05199000
*                                      INITIALIZATION IN PROGRESS    */ 05200000
@RF00148 DS    0H                                                  0160 05201000
*   AFCWRIT=ON;                     /* SET THE AUDIT FLAG TO INDICATE   05202000
*                                      THE CONTINUOUS TRACE WRITER 0160 05203000
*                                      (EXTERNAL TO TAPE) HAS BEEN 0160 05204000
*                                      ATTACHED                      */ 05205000
         OI    AFINIT(PCTPTR),B'01010000'                          0160 05206000
*   WRTDLST=ADDR(SDUMPMAC);         /* DIAGNOSTIC: PUT THE ADDRESS OF   05207000
*                                      THE SDUMP MACRO BUILT BY THE     05208000
*                                      TRACE WRITER INTO THE GTFPCT     05209000
*                                      FOR USE BY AHLSFRR, AHLSBFRR,    05210000
*                                      & AHLRDFRR                    */ 05211000
         LA    @10,SDUMPMAC                                        0161 05212000
         ST    @10,WRTDLST(,PCTPTR)                                0161 05213000
*                                                                  0162 05214000
*   /*****************************************************************/ 05215000
*   /*                                                               */ 05216000
*   /* THIS SUBROUTINE WILL GETMAIN ONE PAGE OF ADDRESS SPACE SP=7   */ 05217000
*   /* AND DIVIDE IT INTO AS MANY SLE AS POSSIBLE. THE SLE WILL BE   */ 05218000
*   /* INITIALIZED AND CHAINED TOGETHER. GQHEAD WILL POINT TO THE    */ 05219000
*   /* FIRST SLE IN THE CHAIN AND GQTAIL WILL POINT TO THE LAST.     */ 05220000
*   /* SHOULD THE GETMAIN FAIL AHL032I WILL BE WRITTEN AND GQHEAD    */ 05221000
*   /* WILL BE 0                                                     */ 05222000
*   /*                                                               */ 05223000
*   /*****************************************************************/ 05224000
*                                                                  0162 05225000
*/* S SLEGET: GET THE PAGE OF SLE                                    */ 05226000
*/* D (YES,CLEANUP,NO,) GQHEAD=0                                     */ 05227000
*/* P SLQHEAD=GQHEAD                                                 */ 05228000
*                                                                  0162 05229000
*   CALL SLEGET;                                                   0162 05230000
         BAL   @14,SLEGET                                          0162 05231000
*   IF GQHEAD=0 THEN                                               0163 05232000
         L     @10,GQHEAD                                          0163 05233000
         LTR   @10,@10                                             0163 05234000
         BZ    @RT00163                                            0163 05235000
*     GO TO CLEANUP;                /* IF GQHEAD IS 0 THE SLE GETMAIN   05236000
*                                      FAILED. TERMINATE             */ 05237000
*   SLQHEAD=GQHEAD;                 /* PUT THE STRING OF SLE ONTO THE   05238000
*                                      SLE QUEUE, PREVIOUSLY EMPTY   */ 05239000
         ST    @10,SLQHEAD(,PCTPTR)                                0165 05240000
*                                                                  0166 05241000
*   /*****************************************************************/ 05242000
*   /*                                                               */ 05243000
*   /* HERE ADDRESS SPACE IS GOTTEN FROM FIXED COMMON SQA FOR USE AS */ 05244000
*   /* AN SRB TO SCHEDULE AHLSBUF. IT IS COMPLETELY INITIALIZED      */ 05245000
*   /* EXCEPT FOR SRBEP & SRBRMTR WHICH ARE FILLED IN AFTER AHLSBUF  */ 05246000
*   /* IS LOADED AN ADDITIONAL 8 BYTES IS GOTTEN FOR AHLSBUF TO USE  */ 05247000
*   /* AS AS A WORKAREA (SEE THE SRB MAPPING)                        */ 05248000
*   /*                                                               */ 05249000
*   /*****************************************************************/ 05250000
*                                                                  0166 05251000
*/* P SET AFGMSRB                                                    */ 05252000
*/* S GETMAIN: GET FROM SQA AN SRB & WORKAREA FOR AHLSBUF            */ 05253000
*/* D (BAD,,OK,%CC) RC?                                              */ 05254000
*/* S (,CLEANUP) WTO: MESSAGE AHL033I                                */ 05255000
*/*%CC: P SAVE SRB ADDRESS IN INTERNAL SRBPTR WITH AVAIL BIT ON      */ 05256000
*                                                                  0166 05257000
*   AFGMSRB=ON;                     /* TURN ON THE AUDIT FLAG FOR  0166 05258000
*                                      THIS SECTION OF CODE          */ 05259000
         OI    AFGMSRB(PCTPTR),B'00000010'                         0166 05260000
*   RFY                                                            0167 05261000
*    (RETCODE) RSTD;                                               0167 05262000
*   DO;                             /* GETMAIN (EC)                0168 05263000
*                                      LV(LENGTH(SRBSECT)) A(GETWORD)   05264000
*                                      SP(SQASP)                     */ 05265000
*     RESPECIFY                                                    0169 05266000
*      (GPR01P) RESTRICTED;                                        0169 05267000
*     I004=I004&&I004;              /* INIT LIST                     */ 05268000
         XC    I004(10),I004                                       0170 05269000
*     I00404='20'X;                 /* TYPE                          */ 05270000
         MVI   I00404,X'20'                                        0171 05271000
*     I00401=LENGTH(SRBSECT);       /* LENGTH                        */ 05272000
         MVC   I00401(4),@CF00338                                  0172 05273000
*     I00403=ADDR(GETWORD);         /* ADDR OF ADDR LIST             */ 05274000
         LA    @14,GETWORD                                         0173 05275000
         STCM  @14,7,I00403                                        0173 05276000
*     I00405=SQASP;                 /* SUBPOOL VALUE                 */ 05277000
         MVI   I00405,X'F5'                                        0174 05278000
*     GPR01P=ADDR(I004);            /* REG1 POINTS TO LIST           */ 05279000
         LA    GPR01P,I004                                         0175 05280000
*     SVC(4);                       /* STANDARD-FORM OF GETMAIN      */ 05281000
         SVC   4                                                   0176 05282000
*     RESPECIFY                                                    0177 05283000
*      (GPR01P) UNRESTRICTED;                                      0177 05284000
*   END;                            /* GETMAIN (EC)                0178 05285000
*                                      LV(LENGTH(SRBSECT)) A(GETWORD)   05286000
*                                      SP(SQASP)                     */ 05287000
*   IF RETCODE^=0 THEN                                             0179 05288000
         LTR   RETCODE,RETCODE                                     0179 05289000
         BZ    @RF00179                                            0179 05290000
*     DO;                           /* CHECK THE RETURN CODE AND IF     05291000
*                                      IT IS NONZERO ISSUE THE     0180 05292000
*                                      INSUFFICIENT SQA MSG          */ 05293000
*       DO;                         /* WTOP MF(E,AHL033)             */ 05294000
*         RFY                                                      0182 05295000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 05296000
*         GPR01P=ADDR(AHL033);      /* ADDRESS CONTROL PROGRAM LIST  */ 05297000
         L     GPR01P,@CA02276                                     0183 05298000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 05299000
         SVC   35                                                  0184 05300000
*         RFY                                                      0185 05301000
*           GPR01P UNRSTD;          /* RELEASE REG 1                 */ 05302000
*       END;                        /* WTOP MF(E,AHL033)             */ 05303000
*       GO TO CLEANUP;              /* TERMINATE THE TRACE WRITER    */ 05304000
         B     CLEANUP                                             0187 05305000
*     END;                                                         0188 05306000
*   SRBPTR=GETWORD|'80000000'X;     /* SAVE THE ADDR OF THE GOTTEN 0189 05307000
*                                      ADDR SPACE WITH THE FIRST BIT    05308000
*                                      TURNED ON SO THIS CAN BE USED    05309000
*                                      TO REFRESH GTFSSRB            */ 05310000
@RF00179 L     @14,GETWORD                                         0189 05311000
         O     @14,@CF02555                                        0189 05312000
         ST    @14,SRBPTR                                          0189 05313000
*   RFY                                                            0190 05314000
*    (RETCODE) UNRSTD;                                             0190 05315000
*                                                                  0191 05316000
*   /*****************************************************************/ 05317000
*   /*                                                               */ 05318000
*   /* INITIALIZE THE SRB                                            */ 05319000
*   /*                                                               */ 05320000
*   /*****************************************************************/ 05321000
*                                                                  0191 05322000
*/* P INIT THE SRB FOR AHLSBUF                                       */ 05323000
*/* P CLEAR AFGMSRB                                                  */ 05324000
*                                                                  0191 05325000
*   SRBID='SRB ';                   /* PRINTABLE ID                  */ 05326000
         MVC   SRBID(4,@14),@CC02556                               0191 05327000
*   SRBASCB=PSAAOLD;                /* AHLSBUF IS TO RUN IN THIS   0192 05328000
*                                      ADDRESS SPACE                 */ 05329000
         L     @10,PSAAOLD                                         0192 05330000
         ST    @10,SRBASCB(,@14)                                   0192 05331000
*   SRBPKF=KEYZERO;                 /* AHLSBUF IS TO RUN KEY 0       */ 05332000
         MVI   SRBPKF(@14),X'00'                                   0193 05333000
*   SRBSAVE=0;                                                     0194 05334000
         SLR   @10,@10                                             0194 05335000
         ST    @10,SRBSAVE(,@14)                                   0194 05336000
*   SRBPARM=ADDR(GTFPCT);           /* PASS GTFPCT ADDR TO AHLSBUF R1*/ 05337000
         ST    PCTPTR,SRBPARM(,@14)                                0195 05338000
*   SRBCPAFF='0000'X;               /* NO CPU AFFINITY               */ 05339000
         MVC   SRBCPAFF(2,@14),@CB01851                            0196 05340000
*   SRBPRIOR=SYSPRIOR;                                             0197 05341000
         MVI   SRBPRIOR(@14),X'00'                                 0197 05342000
*   SRBPTCB=0;                      /* AHLSBUF SHOULD NOT BE       0198 05343000
*                                      ASSOCIATED WITH THIS TCB    0198 05344000
*                                                            @YM01896*/ 05345000
         ST    @10,SRBPTCB(,@14)                                   0198 05346000
*   SRBPASID=ASCBASID;              /* FROM THE CURRENT ASCB         */ 05347000
         L     @10,ASCBPTR                                         0199 05348000
         LH    @10,ASCBASID(,@10)                                  0199 05349000
         N     @10,@CF02741                                        0199 05350000
         STH   @10,SRBPASID(,@14)                                  0199 05351000
*   AFGMSRB=OFF;                    /* THE SRB IS COMPLETE           */ 05352000
*                                                                  0201 05353000
*   /*****************************************************************/ 05354000
*   /*                                                               */ 05355000
*   /* THIS SECTION OF CODE OBTAINS FROM FIXED COMMON SQA, ADDRESS   */ 05356000
*   /* SPACE FOR THE GTFBLOK. SHOULD THE REAL CORE SIZE BE 768K OR   */ 05357000
*   /* LESS, ONE LESS GTFBLOK IS GOTTEN. THE GTFBLOK ARE INITIALIZED,*/ 05358000
*   /* CHAINED TOGETHER, AND THE CURRENT ONE IS TIME STAMPED TO MAKE */ 05359000
*   /* IT READY FOR USE. SHOULD A GETMAIN FAIL THE INSUFFICIENT SQA  */ 05360000
*   /* MESSAGE IS WRITTEN THEN THE TRACE WRITER IS TERMINATED        */ 05361000
*   /*                                                               */ 05362000
*   /*****************************************************************/ 05363000
*                                                                  0201 05364000
*/* P SET AFGMBLOK                                                   */ 05365000
*/* D (YES,,NO,%GETBLOK) IS THIS A SMALL REAL STORAGE                */ 05366000
*/* P CHANGE NUMBER OF GTFBLOK TO BE GOTTEN FROM NORMAL TO 3         */ 05367000
*/*%GETBLOK: D (NO,,YES,%BLKINIT) HAVE ENOUGH GTFBLOK BEEN GOTTEN    */ 05368000
*/* P INCREMENT BLOKINDX BY 1                                        */ 05369000
*/* S GETMAIN: GET ONE GTFBLOK FROM SQA                              */ 05370000
*/* D (OK,%N,BAD,) RC?                                               */ 05371000
*/*%Q: S WTO: MESSAGE AHL033I                                        */ 05372000
*/* P (,CLEANUP) DECREMENT BLOKINDX BY 1                             */ 05373000
*/*%N: P (,%GETBLOK) ENTER ADDRESS OF NEW GTFBLOK INTO BLOKLIST      */ 05374000
*                                                                  0201 05375000
*   AFGMBLOK=ON;                    /* SET THE AUDIT FLAG FOR THIS 0201 05376000
*                                      SECTION OF CODE               */ 05377000
         OI    AFGMBLOK(PCTPTR),B'00000001'                        0201 05378000
         NI    AFGMSRB(PCTPTR),B'11111101'                         0201 05379000
*   IF CVTEORM<=SMALLRMS            /* IS THIS A SMALL REAL MAIN   0202 05380000
*                                      STORAGE                       */ 05381000
*     THEN                                                         0202 05382000
         L     @10,CVTPTR                                          0202 05383000
         CLC   CVTEORM(4,@10),@CF02323                             0202 05384000
         BH    @RF00202                                            0202 05385000
*     BLOKNUM=3;                    /* YES, SO DECREMENT BLOKNUM BY     05386000
*                                      ONE TO REDUCE THE FIXED     0203 05387000
*                                      STORAGE REQUIREMENT           */ 05388000
