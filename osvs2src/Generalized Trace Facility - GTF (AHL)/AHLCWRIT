*/* OPTIONAL DIAGNOSTIC AID                                          */ 05710000
*                                                                  0286 05711000
*       END;                                                       0286 05712000
*   END;                                                           0287 05713000
@RF00271 LA    @10,1                                               0287 05714000
         AL    @10,I                                               0287 05715000
@DE00264 ST    @10,I                                               0287 05716000
         C     @10,HQLENGTH(,PCTPTR)                               0287 05717000
         BNH   @DL00264                                            0287 05718000
*   QTAIL->HQCHAIN=0;               /* THE LAST GTFBCB ON THE HISTORY   05719000
*                                      QUEUE HAS A CHAIN FIELD OF 0  */ 05720000
*                                                                  0288 05721000
         L     @10,QTAIL(,PCTPTR)                                  0288 05722000
         SLR   @09,@09                                             0288 05723000
         ST    @09,HQCHAIN(,@10)                                   0288 05724000
*   /*****************************************************************/ 05725000
*   /*                                                               */ 05726000
*   /* FINISH THE ECBLIST                                            */ 05727000
*   /*                                                               */ 05728000
*   /*****************************************************************/ 05729000
*                                                                  0289 05730000
*/* P PUT PTRS TO WRTRCOM & TRMWRIT INTO ECBLIST                     */ 05731000
*/* P TURN ON HIGH ORDER BIT OF LAST PTR IN ECBLIST                  */ 05732000
*                                                                  0289 05733000
*   TERMECB=ADDR(TRMWRIT);                                         0289 05734000
         LA    @10,TRMWRIT(,PCTPTR)                                0289 05735000
         ST    @10,TERMECB                                         0289 05736000
*   WRTRECB=ADDR(WRTRCOM);                                         0290 05737000
         LA    @10,WRTRCOM(,PCTPTR)                                0290 05738000
         ST    @10,WRTRECB                                         0290 05739000
*   ECBEND=ON;                      /* TURN ON END OF LIST INDICATOR */ 05740000
         OI    ECBEND,B'10000000'                                  0291 05741000
*                                                                  0292 05742000
*   /*****************************************************************/ 05743000
*   /*                                                               */ 05744000
*   /* THIS SECTION OF CODE LOADS AND FIXES OTHER MODULES OF THE     */ 05745000
*   /* WRITER-STACK SERVICE. EACH E.P. ADDRESS IS OBTAINED BY A      */ 05746000
*   /* SEPARATE LOAD TO SIMPLIFY THE CODE & ALLOW FOR EASE OF        */ 05747000
*   /* MODIFICATION                                                  */ 05748000
*   /*                                                               */ 05749000
*   /*****************************************************************/ 05750000
*                                                                  0292 05751000
*/* P SET AFLDFX                                                     */ 05752000
*/* P DIAGNOSTIC: PUT THE ADDRESS OF AHLREADR INTO SDUMPLST FROM MCHEAD 05753000
*/*                                                                  */ 05754000
*/* S (OK,,FAIL,RET4) LOAD: AHLWSMOD                                 */ 05755000
*/* S PGFIX: AHLWSMOD                                                */ 05756000
*/* S WAIT: FOR PGFIX                                                */ 05757000
*/*%X3: P SAVE THE ADDR OF AHLWSMOD IN THE GTFPCT (WSMODBEG WSMODEND)   05758000
*/*                                                                  */ 05759000
*/* P DIAGNOSTIC: PUT THE ADDR OF AHLWSMOD INTO SDUMPLST             */ 05760000
