*       I00405=SQASP;               /* SUBPOOL VALUE                 */ 05413000
         MVI   I00405,X'F5'                                        0213 05414000
*       GPR01P=ADDR(I004);          /* REG1 POINTS TO LIST           */ 05415000
         LA    GPR01P,I004                                         0214 05416000
*       SVC(4);                     /* STANDARD-FORM OF GETMAIN      */ 05417000
         SVC   4                                                   0215 05418000
*       RESPECIFY                                                  0216 05419000
*        (GPR01P) UNRESTRICTED;                                    0216 05420000
*     END;                          /* GETMAIN (EC)                0217 05421000
*                                      LV(LENGTH(GTFBLOK)) A(GETWORD)   05422000
*                                      SP(SQASP)                     */ 05423000
*     IF RETCODE^=0 THEN                                           0218 05424000
         LTR   RETCODE,RETCODE                                     0218 05425000
         BZ    @RF00218                                            0218 05426000
*       DO;                         /* CHECK THE RETURN CODE AND IF     05427000
*                                      IT IS NONZERO ISSUE THE     0219 05428000
*                                      INSUFFICIENT SQA MSG          */ 05429000
*         DO;                       /* WTOP MF(E,AHL033)             */ 05430000
*           RFY                                                    0221 05431000
*             GPR01P RSTD;          /* RESTRICT REG 1                */ 05432000
*           GPR01P=ADDR(AHL033);    /* ADDRESS CONTROL PROGRAM LIST  */ 05433000
         L     GPR01P,@CA02276                                     0222 05434000
*           SVC(35);                /* ISSUE WTO SVC                 */ 05435000
         SVC   35                                                  0223 05436000
*           RFY                                                    0224 05437000
*             GPR01P UNRSTD;        /* RELEASE REG 1                 */ 05438000
*         END;                      /* WTOP MF(E,AHL033)             */ 05439000
*         BLOKINDX=BLOKINDX-1;                                     0226 05440000
         L     @14,BLOKINDX                                        0226 05441000
