         L     @07,CVTPTR                                          0232 05483000
         MVC   RECVTTZ(4,@09),CVTTZ(@07)                           0232 05484000
*   ADDR(BLOKINIT)->RECGTFOP=GTFOPTS;/* THE GTF OPTIONS IN EFFECT    */ 05485000
         MVC   RECGTFOP(4,@09),GTFOPTS(PCTPTR)                     0233 05486000
*   CURRBLOK=BLOKLIST(BLOKINDX);                                   0234 05487000
         SLA   @10,2                                               0234 05488000
         L     @10,BLOKLIST-4(@10)                                 0234 05489000
         ST    @10,CURRBLOK(,PCTPTR)                               0234 05490000
*   DO I=1 TO BLOKINDX;                                            0235 05491000
         LA    @10,1                                               0235 05492000
         B     @DE00235                                            0235 05493000
@DL00235 DS    0H                                                  0236 05494000
*     NEXTBLOK=BLOKLIST(I);                                        0236 05495000
         LR    @09,@10                                             0236 05496000
         SLA   @09,2                                               0236 05497000
         L     @09,BLOKLIST-4(@09)                                 0236 05498000
         ST    @09,NEXTBLOK(,PCTPTR)                               0236 05499000
*     CURRBLOK->GTFBLOK(1:LENGTH(BLOKINIT))=BLOKINIT;              0237 05500000
         L     @07,CURRBLOK(,PCTPTR)                               0237 05501000
         MVC   GTFBLOK(38,@07),BLOKINIT                            0237 05502000
*     CURRBLOK->BLKCHAIN=NEXTBLOK;                                 0238 05503000
         ST    @09,BLKCHAIN(,@07)                                  0238 05504000
*     CURRBLOK=NEXTBLOK;                                           0239 05505000
         ST    @09,CURRBLOK(,PCTPTR)                               0239 05506000
*   END;                                                           0240 05507000
