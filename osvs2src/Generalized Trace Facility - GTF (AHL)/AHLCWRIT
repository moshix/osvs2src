         L     @01,CURRBLOK(,PCTPTR)                               0664 07449000
         ST    @10,BLKCHAIN(,@01)                                  0664 07450000
*       GETWORD->RECTOD=COMPREG->RECTOD;/* COPY TS FROM THE OLD    0665 07451000
*                                      GTFBLOK INTO THE NEW GTFBLOK  */ 07452000
