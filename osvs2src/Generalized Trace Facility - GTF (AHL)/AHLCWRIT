@DE00235 ST    @10,I                                               0240 05509000
         C     @10,BLOKINDX                                        0240 05510000
         BNH   @DL00235                                            0240 05511000
*   RFY                                                            0241 05512000
*     CLOCKREG RSTD;                                               0241 05513000
*   CLOCKREG=ADDR(CURRBLOK->RECTOD);                               0242 05514000
         L     CLOCKREG,CURRBLOK(,PCTPTR)                          0242 05515000
         LA    CLOCKREG,RECTOD(,CLOCKREG)                          0242 05516000
*   GEN CODE(STCK  0(CLOCKREG))                                    0243 05517000
*   REFS(CLOCKREG) SETS(RECTOD) NOFLOWS NOEXIT SEQFLOW NODEFS;/*   0243 05518000
*                                      TIMESTAMP THE CURRENT GFTBLOK    05519000
*                                      TO PREPARE IT FOR USE         */ 05520000
         STCK  0(CLOCKREG)                                              05521000
*   RFY                                                            0244 05522000
*     CLOCKREG UNRSTD;                                             0244 05523000
*   CURRBLOK->BLOKUSE=0;            /* SET THE USE COUNT TO 0 TO   0245 05524000
*                                      INDICATE THAT ALL THE RESERVED   05525000
*                                      SPACE HAS BEEN FILLED         */ 05526000
         L     @10,CURRBLOK(,PCTPTR)                               0245 05527000
         SLR   @09,@09                                             0245 05528000
         STH   @09,BLOKUSE(,@10)                                   0245 05529000
*   AFGMBLOK=OFF;                   /* TURN OFF AUDIT FLAG FOR THIS     05530000
*                                      SECTION                       */ 05531000
         NI    AFGMBLOK(PCTPTR),B'11111110'                        0246 05532000
*                                                                  0247 05533000
*   /*****************************************************************/ 05534000
*   /*                                                               */ 05535000
*   /* THIS SECTION OF CODE INITIAIZES THE EMPTY AND HISTORY QUEUES  */ 05536000
*   /* AND THE POINTERS TO THEM                                      */ 05537000
*   /*                                                               */ 05538000
*   /*****************************************************************/ 05539000
*                                                                  0247 05540000
*/* S EBUFGET: GET A STRING OF GTFBCB & GTFBUFR                      */ 05541000
*/* D (YES,,NO,%X1) GQHEAD=0                                         */ 05542000
*/* S (,CLEANUP) WTO: AHL032I                                        */ 05543000
*/*%X1: P EQHEAD=GQHEAD                                              */ 05544000
*/* P DIAGNOSTIC: EQLENGTH=THE NUMBER OF GTFBCB THAT FIT IN 1 PAGE   */ 05545000
*/* P DIAGNOSTIC: EQMIN=EQLENGTH                                     */ 05546000
*                                                                  0247 05547000
*   ;                                                              0247 05548000
*                                                                  0247 05549000
*   /*****************************************************************/ 05550000
*   /*                                                               */ 05551000
*   /* INTERNAL SUBROUTINE EBUFGET WILL RETURN A STRING OF GTFBCB &  */ 05552000
*   /* GTFBUFR READY TO BE PLACED ON THE EMPTY QUEUE. THE QUANTITY OF*/ 05553000
*   /* GTFBCB IN THE STRING IS EQUAL TO 1 PAGE DIVIDED BY THE LENGTH */ 05554000
*   /* OF A GTFBCB. ALL THE GTFBCB WILL BE ON ONE PAGE. GQHEAD WILL  */ 05555000
*   /* CONTAIN THE ADDRESS OF THE FIRST GTFBCB, GQTAIL WILL POINT TO */ 05556000
*   /* THE LAST GTFBCB. IF A GETMAIN FAILS THE INSUFFICIENT ADDRESS  */ 05557000
*   /* SPACE MSG WILL BE WRITTEN AND GQHEAD WILL BE 0                */ 05558000
*   /*                                                               */ 05559000
*   /*****************************************************************/ 05560000
*                                                                  0248 05561000
*   CALL EBUFGET;                                                  0248 05562000
         BAL   @14,EBUFGET                                         0248 05563000
*   IF GQHEAD=0 THEN                                               0249 05564000
         L     @10,GQHEAD                                          0249 05565000
         LTR   @10,@10                                             0249 05566000
         BNZ   @RF00249                                            0249 05567000
*     DO;                           /* IF GQHEAD IS 0 EBUFGET FAILED    05568000
*                                      AND THE TRACE WRITER HAS TO BE   05569000
*                                      TERMINATED                    */ 05570000
*       DO;                         /* WTOP MF(E,AHL032)             */ 05571000
*         RFY                                                      0252 05572000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 05573000
*         GPR01P=ADDR(AHL032);      /* ADDRESS CONTROL PROGRAM LIST  */ 05574000
         L     GPR01P,@CA02275                                     0253 05575000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 05576000
         SVC   35                                                  0254 05577000
*         RFY                                                      0255 05578000
*           GPR01P UNRSTD;          /* RELEASE REG 1                 */ 05579000
*       END;                        /* WTOP MF(E,AHL032)             */ 05580000
*       GO TO CLEANUP;                                             0257 05581000
         B     CLEANUP                                             0257 05582000
*     END;                                                         0258 05583000
*   EQHEAD=GQHEAD;                  /* PUT THE STRING OF GTFBCB ON 0259 05584000
*                                      THE EMPTY QUEUE               */ 05585000
@RF00249 L     @10,GQHEAD                                          0259 05586000
         ST    @10,EQHEAD(,PCTPTR)                                 0259 05587000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05588000
*                                                                  0260 05589000
*   EQLENGTH=PGBYTES/LENGTH(GTFBCB);/* INITIALIZE THE COUNT OF     0260 05590000
*                                      AVAILABLE GTFBCB ON THE EMPTY    05591000
*                                      QUEUE                         */ 05592000
         LA    @09,170                                             0260 05593000
