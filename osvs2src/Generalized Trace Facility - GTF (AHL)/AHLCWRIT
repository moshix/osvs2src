*                                      ACCURATE BLOCK COUNT (EXCP)   */ 00342000
*         5 DCBMRPT1 BIT(1),        /* POINT (WHICH IMPLIES NOTE)  0010 00343000
*                                      (BSAM, BPAM)                  */ 00344000
*          6 DCBMRSBG BIT(1),       /* SUBSTITUTE MODE OF GET (QSAM) */ 00345000
*           7 DCBMRDBF BIT(1),      /* DYNAMIC BUFFERING (BISAM,   0010 00346000
*                                      BDAM) --- ALWAYS ZERO (QISAM)    00347000
*                                      --- RESERVED (QTAM, BTAM)     */ 00348000
*        4 DCBMRCRL BIT(1),         /* CNTRL (BSAM, QSAM)            */ 00349000
*         5 DCBMRCHK BIT(1),        /* CHECK (BISAM)                 */ 00350000
*          6 DCBMRRDX BIT(1),       /* READ EXCLUSIVE (BDAM)         */ 00351000
*           7 DCBPGFXA BIT(1),      /* PAGE FIX APPENDAGE IS       0010 00352000
*                                      SPECIFIED (EXCP) --- RESERVED    00353000
*                                      (BTAM, BPAM, QISAM, QTAM)     */ 00354000
*        4 DCBMRDMG BIT(1),         /* DATA MODE OF GET (QSAM)       */ 00355000
*         5 DCBMRCK BIT(1),         /* CHECK (BDAM) --- RESERVED   0010 00356000
*                                      (EXCP, BSAM, BPAM, BISAM,   0010 00357000
*                                      QISAM, QTAM, BTAM)            */ 00358000
*       3 DCBMACR2 CHAR(1),         /* SECOND BYTE OF DCBMACR        */ 00359000
*        4 DCBMRSTL BIT(1),         /* SETL (QISAM) --- ALWAYS ZERO     00360000
*                                      (BSAM, QSAM, BPAM, BISAM,   0010 00361000
*                                      BDAM) --- RESERVED (EXCP,   0010 00362000
*                                      QTAM, BTAM)                   */ 00363000
*        4 DCBMRPUT BIT(1),         /* PUT (QSAM, TCAM) - PUT OR PUTX   00364000
*                                      (QISAM)                       */ 00365000
*         5 DCBMRGTQ BIT(1),        /* GET FOR MESSAGE GROUP (QTAM)     00366000
*                                      --- ALWAYS ZERO (BSAM, BPAM,     00367000
*                                      BISAM, BDAM) --- RESERVED   0010 00368000
*                                      (EXCP, BTAM)                  */ 00369000
*        4 DCBMRWRT BIT(1),         /* WRITE (BSAM, BPAM, BISAM,   0010 00370000
*                                      BDAM, BTAM)                   */ 00371000
*         5 DCBMRRDQ BIT(1),        /* READ FOR LINE GROUP (QTAM) ---   00372000
*                                      ALWAYS ZERO (QSAM, QISAM) ---    00373000
*                                      RESERVED (EXCP)               */ 00374000
*        4 DCBMRMVP BIT(1),         /* MOVE MODE OF PUT (QSAM, QISAM)*/ 00375000
*         5 DCBMRWRK BIT(1),        /* KEY SEGMENT WITH WRITE (BDAM)    00376000
*                                      --- ALWAYS ZERO (BISAM) --- 0010 00377000
*                                      RESERVED (EXCP, BSAM, BPAM, 0010 00378000
