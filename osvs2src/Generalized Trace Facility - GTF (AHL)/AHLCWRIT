*                                      ROUTINE, USED TO WRITE ERROR     04134000
*                                      MESSAGES                      */ 04135000
*                                                                  0040 04136000
*/********************************************************************/ 04137000
*/*                               SLE                                */ 04138000
*/********************************************************************/ 04139000
*/*                                                                  */ 04140000
*/* LEVEL OF APPLICABILITY                                           */ 04141000
*/*   OS/VS2-R2                                                      */ 04142000
*/*                                                                  */ 04143000
*/* FUNCTION                                                         */ 04144000
*/*   THIS MAPPING DESCRIBES THE GTF SAVE LIST ELEMENTS.             */ 04145000
*/*   THE SLE ARE USED TO IDENTIFY STRINGS OF GTFBCB ARE BEING       */ 04146000
*/*   SAVED FOR PROCESSING BY EITHER SVCDUMP OR IGC0F05A.            */ 04147000
*/*                                                                  */ 04148000
*/* INITIALIZATION                                                   */ 04149000
*/*   THIS CONTROL BLOCK IS OBTAINED AND INITIALIZED BY THE TRACE    */ 04150000
*/*   WRITER FOR USE BY AHLSBUF AND AHLREADR                         */ 04151000
*/*                                                                  */ 04152000
*/* RESIDENCE                                                        */ 04153000
*/*   THIS CONTROL BLOCK IS OBTAINED FROM SUBPOOL 7, AND IS FIXED,   */ 04154000
*/*   THEREFORE IT IS FIXED, PRIVATE.                                */ 04155000
*/*                                                                  */ 04156000
*/* PHILOSOPHY OF DESIGN                                             */ 04157000
*/*   THIS CONTROL BLOCK MAPPING ASSUMES THAT THE USER WILL QUALIFY  */ 04158000
*/*   ALL ADDRESSING TO IT.                                          */ 04159000
*/*                                                                  */ 04160000
*/********************************************************************/ 04161000
*                                                                  0040 04162000
*   DECLARE                                                        0040 04163000
*     1 SLE BDY(WORD) BASED,                                       0040 04164000
*      2 SLEID CHAR(8),             /* EBCIDIC IDENTIFIER            */ 04165000
*      2 SLQCHAIN PTR(31),          /* @ NEXT SLE ON QUEUE OR 0 FOR     04166000
*                                      THE END OF THE QUEUE          */ 04167000
*      2 SLEHOOK CHAR(8) BDY(WORD), /* THIS PORTION OF THE SLE IS  0040 04168000
*                                      WHERE THE DATA FROM THE     0040 04169000
*                                      SAVEHOOK IS SAVED             */ 04170000
*       3 SLEEID BIT(16),           /* EID OF SAVEHOOK FOR WHICH THIS   04171000
*                                      SLE WAS CERATED               */ 04172000
*       3 SLEDUMP CHAR(6) BDY(WORD,3),/* THIS FORMS THE UNIQUE DUMP     04173000
*                                      IDENTIFIER PASSED IN THE SAVE    04174000
*                                      HOOK RECORD AND THEN MATCHED     04175000
*                                      TO THE ID IN THE PARMLIST   0040 04176000
*                                      PASSED TO AHLREADR            */ 04177000
*        4 SLEASID PTR(15),         /* ASID OF ADDRESS SPACE THAT  0040 04178000
*                                      ISSUED SAVE HOOK              */ 04179000
*        4 SLETCBA PTR(31),         /* @ TCB FOR WHICH ABDUMP IS   0040 04180000
*                                      DUMPING, OR 0 IF THIS IS FOR     04181000
*                                      SVCDUMP                       */ 04182000
*      2 SLEPGLST PTR(31),          /* PTR TO A LIST OF PAGES THAT 0040 04183000
*                                      CONTAIN ALL THE PORTIONS OF 0040 04184000
*                                      THE TASK WRITER'S ADDRESS   0040 04185000
*                                      SPACE THAT EDIT WILL NEED TO     04186000
*                                      LOCATE AND FORMAT THE DATA IN    04187000
*                                      AN SVCDUMP. AHLREADR WILL PASS   04188000
*                                      ALL THESE PAGES ACROSS TO   0040 04189000
*                                      SVCDUMP                       */ 04190000
*      2 SLEBCBQ PTR(31),           /* PTR TO THE STRING OF GTFBCB 0040 04191000
*                                      SAVED FOR THIS DUMP           */ 04192000
*      2 SLEQCNT PTR(31),           /* THIS FIELD INDICATES HOW MANY    04193000
*                                      GTFBCB ARE ON THE SLEBCBQ. IT    04194000
*                                      IS SET BY AHLSBUF & DECR BY 0040 04195000
*                                      AHLREADR AS GTFBCB ARE TAKEN     04196000
*                                      OFF THE SLEBCBQ               */ 04197000
*      2 SLEFLAGS CHAR(4),                                         0040 04198000
*       3 SLEINUSE BIT(1),          /* THIS IS SET WHEN THE SLE IS IN   04199000
*                                      USE. AHLSBUF USES THIS BIT TO    04200000
*                                      IDENTIFY AN AVAILABLE SLE     */ 04201000
*       3 * BIT(31);                                               0040 04202000
*   DECLARE                                                        0041 04203000
*     1 SVCPLIST BASED(SLEPGLST),   /* LIST OF PAGES TO BE PASSED TO    04204000
*                                      SVCDUMP BY AHLREADR. THIS LIST   04205000
*                                      IS BUILT BY AHLREADR THE FIRST   04206000
*                                      TIME IT ACCESSES AND SLE      */ 04207000
*      2 PLINDEX PTR(31),           /* INDEX USED BY AHLREADR TO STEP   04208000
*                                      THROUGH THE LIST THIS IS    0041 04209000
*                                      INITIALIZED AT THE MAX AND  0041 04210000
*                                      DECREMENTED EACH TIME A PAGE     04211000
*                                      IS PASSED                     */ 04212000
*      2 PAGELIST(274) PTR(31),     /* THE LIST OF PAGE ADDRESSES 1     04213000
*                                      MCHEAD, 1 MCCE, 10 MCQE, 1  0041 04214000
