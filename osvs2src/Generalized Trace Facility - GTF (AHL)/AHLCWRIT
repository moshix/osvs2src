*         GO TO CLEANUP;            /* CLEAN UP AND TERMINATE THE  0227 05444000
*                                      TRACE WRITER                  */ 05445000
         B     CLEANUP                                             0227 05446000
*       END;                                                       0228 05447000
*     BLOKLIST(BLOKINDX)=GETWORD;   /* SAVE THE ADDRESS OF THIS    0229 05448000
*                                      GTFBLOK                       */ 05449000
@RF00218 L     @14,BLOKINDX                                        0229 05450000
         SLA   @14,2                                               0229 05451000
         L     @10,GETWORD                                         0229 05452000
         ST    @10,BLOKLIST-4(@14)                                 0229 05453000
*     RFY                                                          0230 05454000
*      (RETCODE) UNRSTD;                                           0230 05455000
*   END;                                                           0231 05456000
*                                                                  0231 05457000
@DE00204 L     @10,BLOKINDX                                        0231 05458000
         C     @10,BLOKNUM                                         0231 05459000
         BL    @DL00204                                            0231 05460000
*   /*****************************************************************/ 05461000
*   /*                                                               */ 05462000
*   /* THIS LOOP INITIALIZES THE GTFBLOK AND POINTERS TO THEM        */ 05463000
*   /*                                                               */ 05464000
*   /*****************************************************************/ 05465000
*                                                                  0232 05466000
*/*%BLKINIT: N THIS LOOP INITIALIZES THE GTFBLOK                     */ 05467000
*/* P COPY CVTTZ & GTFOPTS INTO THE PROTOTYPE TIMESTAMP RECORD       */ 05468000
*/* P CURRBLOK=LAST PTR IN THE BLOKLIST, & I=1                       */ 05469000
*/*%BSLOOP: D (NO,,YES,%BSEND) I=BLOKINDX                            */ 05470000
*/* P NEXTBLOK= BLOKLIST(I)                                          */ 05471000
*/* P COPY NEXTBLOK PTR & PROTOTYPE INTO CURRBLOK-> GTFBLOK          */ 05472000
*/* P CURRBLOK= NEXTBLOK                                             */ 05473000
*/* P (,%BSLOOP) INCREMENT I BY 1                                    */ 05474000
*/*%BSEND:  P STORE TOD CLOCK INTO CURRBLOK-> GTFBLOK & SET BLOKUSE=0   05475000
*/*                                                                  */ 05476000
*/* P CLEAR AFGMBLOK                                                 */ 05477000
*                                                                  0232 05478000
*   ADDR(BLOKINIT)->RECVTTZ=CVTTZ;  /* COMPLETE THE PROTOTYPE FOR THE   05479000
*                                      GTFBLOK WITH THE TIMEZONE   0232 05480000
*                                      VALUE AND                     */ 05481000
