*   DO WHILE(BLOKINDX<BLOKNUM);     /* LOOP TO OBTAIN GTFBLOK        */ 05390000
@RF00202 B     @DE00204                                            0204 05391000
@DL00204 DS    0H                                                  0205 05392000
*     BLOKINDX=BLOKINDX+1;                                         0205 05393000
         LA    @10,1                                               0205 05394000
         AL    @10,BLOKINDX                                        0205 05395000
         ST    @10,BLOKINDX                                        0205 05396000
*     RFY                                                          0206 05397000
*      (RETCODE) RSTD;                                             0206 05398000
*     DO;                           /* GETMAIN (EC)                0207 05399000
*                                      LV(LENGTH(GTFBLOK)) A(GETWORD)   05400000
*                                      SP(SQASP)                     */ 05401000
*       RESPECIFY                                                  0208 05402000
*        (GPR01P) RESTRICTED;                                      0208 05403000
*       I004=I004&&I004;            /* INIT LIST                     */ 05404000
