*       I00401=LENGTH(GTFBLOK);     /* LENGTH                        */ 05408000
         MVC   I00401(4),@CF02720                                  0211 05409000
*       I00403=ADDR(GETWORD);       /* ADDR OF ADDR LIST             */ 05410000
         LA    @14,GETWORD                                         0212 05411000
