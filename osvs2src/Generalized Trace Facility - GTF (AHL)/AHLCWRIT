*      2 FIXLEN PTR(31),            /* LENGTH OF FIXTAB              */ 03826000
*      2 ECBADRLT PTR(31),          /* ADDRESS OF ADDRESS LIST OF  0039 03827000
*                                      ECBS FOR AHLTMON POSTED FOR 0039 03828000
*                                      NORMAL OR ABNORMAL TERMINATION   03829000
*                                      OF GTF                        */ 03830000
*      2 STOPECBA PTR(31),          /* ADDR STOP ECB                 */ 03831000
*      2 AHLECB PTR(32),            /* ECB FOR WTOR AHL018A POSTED 0039 03832000
*                                      WHEN OPERATOR REPLIES         */ 03833000
*      2 ATTCHECB PTR(31),          /* ECB USED WHEN WRITER IS     0039 03834000
*                                      ATTACHED POSTED BY SYSTEM WHEN   03835000
*                                      WRITER TERMINATES             */ 03836000
*      2 WMTABPTR PTR(31),          /* @ WMSGTAB                     */ 03837000
*      2 WTASKECB PTR(31),          /* AHLWTASK COMMUNICATION ECB- 0039 03838000
*                                      POSTED BY AHLWPOST            */ 03839000
*      2 WTSKINIT PTR(31),          /* ECB POSTED WHEN AHLWTASK INIT-   03840000
*                                      IALIZATION COMPLETE           */ 03841000
*      2 WTSKATTH PTR(31),          /* ECB USED WHEN AHLWTASK IS   0039 03842000
*                                      ATTACHED, POSTED BY SYSTEM  0039 03843000
*                                      WHEN AHLWTASK TERMINATES      */ 03844000
*      2 TRMWTASK PTR(31),          /* ECB POSTED TO CAUSE AHLWTASK     03845000
*                                      TO TERMINATE                  */ 03846000
*      2 WRTRTCBA PTR(31),          /* @ WRITERS TCB                 */ 03847000
*      2 WTSKTCBA PTR(31),          /* @ AHLWTASK TCB                */ 03848000
*      2 WPOSTSRB PTR(31),          /* @ SRB USED USED BY WTO SERVICE*/ 03849000
*      2 EQHEAD PTR(31),            /* @ 1ST GTFBCB ON EMPTY QUEUE OR   03850000
*                                      0 IF IT IS NULL. THE TRACE  0039 03851000
*                                      WRITER USES THIS PTR TO ADD 0039 03852000
*                                      GTFBCB TO THE HEAD OF THE   0039 03853000
*                                      EMPTY QUEUE & AHLSBUF USES IT    03854000
*                                      TO REMOVE THEM. THE END OF  0039 03855000
*                                      THIS QUEUE IS A 0 IN THE    0039 03856000
*                                      BCBCHAIN FIELD OF THE LAST  0039 03857000
*                                      GTFBCB.                       */ 03858000
*      2 RQHEAD PTR(31),            /* @ 1ST GTFBCB ON RELEASE QUEUE    03859000
*                                      OR 0 IF IT IS NULL. AHLSBUF 0039 03860000
*                                      USES THIS PTR TO ADD GTFBCB TO   03861000
*                                      THE HEAD OF THE RELEASE QUEUE    03862000
*                                      AND THE TRACE WRITER USES IT     03863000
*                                      TO REMOVE THEM. THE END OF  0039 03864000
*                                      THIS QUEUE IS A 0 IN THE    0039 03865000
*                                      BCBCHAIN FIELD OF THE LAST  0039 03866000
*                                      GTFBCB.                       */ 03867000
*      2 WQHEAD PTR(31),            /* THIS PTR POINTS TO THE GTFBCB    03868000
*                                      THAT IS TO BE PROCESSED NEXT     03869000
*                                      BY THE TRACE WRITER, IT IS 0     03870000
*                                      IF THERE ARE NONE. GTFBCB ON     03871000
*                                      THIS QUEUE MAY ALSO BE ON THE    03872000
*                                      HISTORY AND SAVELIST QUEUES.     03873000
*                                      THE END OF THIS QUEUE IS A 0     03874000
*                                      IN THE BCBCHAIN FIELD OF THE     03875000
*                                      LAST GTFBCB. AHLSBUF UPDATES     03876000
*                                      THIS PTR ONLY IF IT WAS 0 WHEN   03877000
