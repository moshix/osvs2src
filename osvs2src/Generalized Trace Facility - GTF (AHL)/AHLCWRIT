*NOFIX:                                                            0649 07390000
*         DO;                       /* ABORT THE FIX                 */ 07391000
NOFIX    DS    0H                                                  0650 07392000
*           GBCDS=OFF;              /* LET AHLSBLOK RUN AGAIN        */ 07393000
         NI    GBCDS(PCTPTR),B'01111111'                           0650 07394000
*           CALL FREQLOCK;          /* RELEASE THE QLOCK AND LET   0651 07395000
*                                      AHLSBUF RUN                   */ 07396000
         BAL   @14,FREQLOCK                                        0651 07397000
*           DO;                     /* FREEMAIN(R) LV(LENGTH(GTFBLOK)   07398000
*                                      )SP(SQASP)A(GETWORD->GTFBLOK) */ 07399000
*             RESPECIFY                                            0653 07400000
*              (GPR01F,                                            0653 07401000
*               GPR00F) RESTRICTED;                                0653 07402000
*             GPR00F=LENGTH(GTFBLOK);/* LENGTH REQUESTED             */ 07403000
         L     GPR00F,@CF02720                                     0654 07404000
*             GPR01F=SQASP;         /* SP IN REG 1                   */ 07405000
         LA    GPR01F,245                                          0655 07406000
*             GEN(SLL   GPR01F,24)                                 0656 07407000
*             NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;         0656 07408000
         SLL   GPR01F,24                                                07409000
*             GPR00F=GPR00F|GPR01F; /* SP IN HI BYTE                 */ 07410000
         OR    GPR00F,GPR01F                                       0657 07411000
*             GPR01F=ADDR(GETWORD->GTFBLOK);/* SET REGISTER ONE      */ 07412000
