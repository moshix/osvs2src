         ST    @09,BLOKLIST-4(@10)                                 0670 07473000
*       RFY                                                        0671 07474000
*        (COMPREG) UNRSTD;                                         0671 07475000
*FIXMSG:                                                           0672 07476000
*       M036AF='ATTEMPTED';         /* SET UP THE FIX ATTEMPTED MSG  */ 07477000
FIXMSG   L     @10,@CA02284                                        0672 07478000
         MVC   M036AF-M036AF(9,@10),@CC02621                       0672 07479000
*       DO;                         /* WTOP MF(E,AHL036)             */ 07480000
*         RFY                                                      0674 07481000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 07482000
*         GPR01P=ADDR(AHL036);      /* ADDRESS CONTROL PROGRAM LIST  */ 07483000
         L     GPR01P,@CA02279                                     0675 07484000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 07485000
         SVC   35                                                  0676 07486000
*         RFY                                                      0677 07487000
*           GPR01P UNRSTD;          /* RELEASE REG 1                 */ 07488000
*       END;                        /* WTOP MF(E,AHL036) WRITE THE 0678 07489000
*                                      MSG␀                          */ 07490000
*     END;                                                         0679 07491000
*   AFBLKFIX=OFF;                   /* TURN OFF THE AUDIT FLAG FOR 0680 07492000
*                                      THIS SECTION OF CODE          */ 07493000
@RF00572 NI    AFBLKFIX(PCTPTR),B'11111110'                        0680 07494000
*                                                                  0681 07495000
*   /*****************************************************************/ 07496000
*   /*                                                               */ 07497000
*   /* THIS SECTION OF CODE TESTS TO SEE IF THE TRACE WRITER HAS MET */ 07498000
*   /* THE CRITERION FOR TERMINATION AND IF NOT IT BRANCHES BACK TO  */ 07499000
*   /* THE WAIT TO CONTINUE PROCESSING IF IT HAS PREVIOUSLY BEEN MET */ 07500000
*   /* IT GOES ON WITH TERMINATION, AND IF IT HAS JUST NOW BEEN MET  */ 07501000
*   /* (TRMWRIT POSTED) IT INITIALIZES TERMINATION                   */ 07502000
*   /*                                                               */ 07503000
*   /*****************************************************************/ 07504000
*                                                                  0681 07505000
*/*XWRITEST: D (YES,,NO,TFTEST) AHLRDPLQ^=0                          */ 07506000
*/* P AFXWRIT=ON                                                     */ 07507000
*/* S AHLXWRIT: FILL ONE REQUEST FOR TRACE DATA                      */ 07508000
*/* P (,NOWAIT) AFXWRIT=OFF                                          */ 07509000
*/*TFTEST: D (YES,,NO,%TRM1) IS TERMFLAG SET                         */ 07510000
*/* P (INT,FINALTRM,EXT,%TRM2)                                       */ 07511000
*/*%TRM1: D (NO,WAIT,YES,) IS TRMWRIT ECB POSTED                     */ 07512000
*/* P ZERO SBLOKRTN FIELD TO DISABLE STACK                           */ 07513000
*/* P (INT,FINALTRM,EXT,) SET TERMFLAG                               */ 07514000
*                                                                  0681 07515000
*XWRITEST:                                                         0681 07516000
*   IF AHLRDPLQ^=0                  /* ARE THERE REQUESTS FOR TRACE     07517000
*                                      DATA TO BE TRANSFERED TO    0681 07518000
*                                      ANOTHER PROGRAM               */ 07519000
*     THEN                                                         0681 07520000
XWRITEST L     @10,AHLRDPLQ(,PCTPTR)                               0681 07521000
         LTR   @10,@10                                             0681 07522000
         BZ    @RF00681                                            0681 07523000
*     DO;                           /* YES                           */ 07524000
*       CALL AHLXWRIT;              /* FILL ONE REQUEST              */ 07525000
         BAL   @14,AHLXWRIT                                        0683 07526000
*       GO TO NOWAIT;               /* BEFORE FILLING ANY MORE     0684 07527000
*                                      REQUESTS BE SURE THAT OTHER 0684 07528000
*                                      MORE IMPORTANT WORK ISN'T   0684 07529000
*                                      WAITING TO BE DONE            */ 07530000
         B     NOWAIT                                              0684 07531000
*     END;                                                         0685 07532000
*TFTEST:                                                           0686 07533000
*   IF TERMFLAG^=ON                 /* CHECK TO SEE IF THE FIRST   0686 07534000
*                                      REQUIREMENT FOR TERMINATION 0686 07535000
*                                      (POSTING OF THE TRMWRIT ECB)     07536000
*                                      HAS PREVIOUSLY BEEN MET       */ 07537000
