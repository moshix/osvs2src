*/* OPTIONAL DIAGNOSTIC AID                                          */ 05597000
*                                                                  0262 05598000
*   ;                                                              0262 05599000
*                                                                  0263 05600000
*   /*****************************************************************/ 05601000
*   /*                                                               */ 05602000
*   /* TRANSFER GTFBCB FROM THE EMPTY QUEUE TO THE HISTORY QUEUE AND */ 05603000
*   /* INITIALIZE THE ASSOCIATED GTFBUFR BY COPYING THE CURRENT      */ 05604000
*   /* GTFBLOK INTO THEM                                             */ 05605000
*   /*                                                               */ 05606000
*   /*****************************************************************/ 05607000
*                                                                  0263 05608000
*/* N THIS LOOP SETS UP THE HISTORY QUEUE                            */ 05609000
*/* P HQHEAD=EQHEAD                                                  */ 05610000
*/*%CX: D (YES,%CX1,NO,) HAS HQLENGTH BEEN REACHED                   */ 05611000
*/* P QTAIL=EQHEAD                                                   */ 05612000
*/* P EQHEAD= EQHEAD-> EQCHAIN                                       */ 05613000
*/* P DIAGNOSTIC: DECREMENT EQLENGTH BY 1                            */ 05614000
*/* P DIAGNOSTIC QTAIL-> EQFLAG=OFF & HQFLAG=ON                      */ 05615000
*/* N EQCHAIN & HQCHAIN BOTH REFER TO THE SAME GTFBCB FIELD          */ 05616000
*/* P QTAIL-> GBUFRPTR-> GTFBUFR= CURRBLOK-> GTFBLOK                 */ 05617000
*/* D (YES,,NO,%CX) EQHEAD-> EQCHAIN=0                               */ 05618000
*/* S EBUFGET: GET A STRING OF GTFBCB & GTFBUFR                      */ 05619000
*/* D (YES,,NO,%X2) GQHEAD=0                                         */ 05620000
*/* S (,CLEANUP) WTO: AHL032I                                        */ 05621000
*/*%X2: P EQHEAD=GQHEAD                                              */ 05622000
*/* P (,%CX) DIAGNOSTIC: INCR EQLENGTH BY NUMBER OF GTFBCB IN 1 PAGE */ 05623000
*/*%CX1: P QTAIL-> HQCHAIN=0                                         */ 05624000
*                                                                  0263 05625000
*   HQHEAD=EQHEAD;                  /* REMOVE A GROUP OF GTFBCB FROM    05626000
*                                      THE HEAD OF THE EMPTY QUEUE TO   05627000
*                                      GO ON THE HISTORY QUEUE       */ 05628000
         ST    @10,HQHEAD(,PCTPTR)                                 0263 05629000
*   DO I=1 TO HQLENGTH;             /* GO THROUGH EACH GTFBCB TO BE     05630000
*                                      TRANSFERED                    */ 05631000
         LA    @10,1                                               0264 05632000
         B     @DE00264                                            0264 05633000
@DL00264 DS    0H                                                  0265 05634000
*     QTAIL=EQHEAD;                 /* QTAIL IS TO POINT TO THE LAST    05635000
*                                      GTFBCB ON THE HISTORY QUEUE   */ 05636000
         L     @10,EQHEAD(,PCTPTR)                                 0265 05637000
         ST    @10,QTAIL(,PCTPTR)                                  0265 05638000
*     EQHEAD=EQHEAD->EQCHAIN;       /* REMOVE THIS GTFBCB FROM THE 0266 05639000
*                                      EMPTY QUEUE                   */ 05640000
         L     @09,EQCHAIN(,@10)                                   0266 05641000
         ST    @09,EQHEAD(,PCTPTR)                                 0266 05642000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05643000
*                                                                  0267 05644000
*     EQLENGTH=EQLENGTH-1;          /* UPDATE COUNT OF GTFBCB ON THE    05645000
*                                      EMPTY QUEUE                   */ 05646000
         L     @07,EQLENGTH(,PCTPTR)                               0267 05647000
         BCTR  @07,0                                               0267 05648000
         ST    @07,EQLENGTH(,PCTPTR)                               0267 05649000
*     QTAIL->EQFLAG=OFF;                                           0268 05650000
*     QTAIL->HQFLAG=ON;                                            0269 05651000
*                                                                  0269 05652000
         OI    HQFLAG(@10),B'00001000'                             0269 05653000
         NI    EQFLAG(@10),B'01111111'                             0269 05654000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05655000
*                                                                  0270 05656000
*     QTAIL->GBUFRPTR->GTFBUFR(1:LENGTH(RECMAP)+4)=CURRBLOK->BLOKAREA(1 05657000
*         :LENGTH(RECMAP)+4);       /* BECAUSE ANY GTFBCB ON THE   0270 05658000
*                                      HISTORY QUEUE COULD BE PUT ON    05659000
*                                      AN SLE THE ASSOCIATED GTFBUFR    05660000
*                                      MUST BE OF VALID GTF FORMAT.     05661000
*                                      THE CURRENT GTFBLOK IS OF   0270 05662000
*                                      VALID FORMAT SO IT IS COPIED     05663000
*                                      INTO EACH GTFBUFR             */ 05664000
         L     @10,GBUFRPTR(,@10)                                  0270 05665000
         L     @07,CURRBLOK(,PCTPTR)                               0270 05666000
         MVC   GTFBUFR(26,@10),BLOKAREA(@07)                       0270 05667000
*     IF EQHEAD->EQCHAIN=0          /* IF HQLENGTH IS LARGE GET EXTRA   05668000
*                                      GTFBCB                        */ 05669000
*       THEN                                                       0271 05670000
         L     @10,EQCHAIN(,@09)                                   0271 05671000
         LTR   @10,@10                                             0271 05672000
         BNZ   @RF00271                                            0271 05673000
*       DO;                                                        0272 05674000
*         CALL EBUFGET;             /* GET ANOTHER STRING OF GTFBCB  */ 05675000
         BAL   @14,EBUFGET                                         0273 05676000
*         IF GQHEAD=0 THEN                                         0274 05677000
         L     @10,GQHEAD                                          0274 05678000
         LTR   @10,@10                                             0274 05679000
         BNZ   @RF00274                                            0274 05680000
*           DO;                     /* IF GQHEAD IS 0 EBUFGET FAILED    05681000
*                                      AND THE TRACE WRITER HAS TO BE   05682000
*                                      TERMINATED                    */ 05683000
*             DO;                   /* WTOP MF(E,AHL032)             */ 05684000
*               RFY                                                0277 05685000
*                 GPR01P RSTD;      /* RESTRICT REG 1                */ 05686000
*               GPR01P=ADDR(AHL032);/* ADDRESS CONTROL PROGRAM LIST  */ 05687000
         L     GPR01P,@CA02275                                     0278 05688000
*               SVC(35);            /* ISSUE WTO SVC                 */ 05689000
         SVC   35                                                  0279 05690000
*               RFY                                                0280 05691000
*                 GPR01P UNRSTD;    /* RELEASE REG 1                 */ 05692000
*             END;                  /* WTOP MF(E,AHL032)             */ 05693000
*             GO TO CLEANUP;                                       0282 05694000
         B     CLEANUP                                             0282 05695000
*           END;                                                   0283 05696000
*         EQHEAD->EQCHAIN=GQHEAD;   /* PUT THE NEW STRING OF GTFBCB     05697000
*                                      ON THE EMPTY QUEUE    @YM03665*/ 05698000
@RF00274 L     @10,EQHEAD(,PCTPTR)                                 0284 05699000
         L     @09,GQHEAD                                          0284 05700000
         ST    @09,EQCHAIN(,@10)                                   0284 05701000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05702000
*                                                                  0285 05703000
*         EQLENGTH=EQLENGTH+PGBYTES/LENGTH(GTFBCB);/* UPDATE THE   0285 05704000
*                                      COUNT OF GTFBCB ON THE EMPTY     05705000
