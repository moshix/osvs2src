*      (GPR00P,                                                    0303 05094000
*       GPR01P) RESTRICTED;                                        0303 05095000
*     GPR00P=1;                     /* LOAD WAIT COUNT               */ 05096000
         LA    GPR00P,1                                            0304 05097000
*     GPR01P=ADDR(PGECB);           /* ADDRESS OF ECB                */ 05098000
         LA    GPR01P,PGECB                                        0305 05099000
