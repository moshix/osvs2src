*                                      SECTION OF CODE               */ 04995000
         OI    AFLDFX(PCTPTR),B'00100000'                          0269 04996000
*   RFY                                                            0270 04997000
*    (LOADREG,                                                     0270 04998000
*     LOADLEN) RSTD;                                               0270 04999000
*   SDENTRY=SDENTRY+1;              /* PREPARE TO ADD AHLREADR TO THE   05000000
*                                      SDUMPLST                      */ 05001000
         LA    @10,1                                               0271 05002000
