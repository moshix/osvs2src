*       RESPECIFY                                                  0193 04642000
*        (GPR01P) UNRESTRICTED;                                    0193 04643000
*     END;                          /* GETMAIN (EC)                0194 04644000
*                                      LV(LENGTH(GTFBLOK)) A(GETWORD)   04645000
*                                      SP(SQASP)                     */ 04646000
*     IF RETCODE^=0 THEN                                           0195 04647000
         LTR   RETCODE,RETCODE                                     0195 04648000
