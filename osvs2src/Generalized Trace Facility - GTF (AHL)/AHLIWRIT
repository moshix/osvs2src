         C     @10,HQLENGTH(,PCTPTR)                               0264 04940000
         BNH   @DL00241                                            0264 04941000
*   QTAIL->HQCHAIN=0;               /* THE LAST GTFBCB ON THE HISTORY   04942000
*                                      QUEUE HAS A CHAIN FIELD OF 0  */ 04943000
*                                                                  0265 04944000
         L     @10,QTAIL(,PCTPTR)                                  0265 04945000
         SLR   @09,@09                                             0265 04946000
         ST    @09,HQCHAIN(,@10)                                   0265 04947000
*   /*****************************************************************/ 04948000
*   /*                                                               */ 04949000
*   /* FINISH THE ECBLIST                                            */ 04950000
*   /*                                                               */ 04951000
*   /*****************************************************************/ 04952000
*                                                                  0266 04953000
*/* P PUT PTRS TO WRTRCOM & TRMWRIT INTO ECBLIST                     */ 04954000
*/* P TURN ON HIGH ORDER BIT OF LAST PTR IN ECBLIST                  */ 04955000
*                                                                  0266 04956000
*   TERMECB=ADDR(TRMWRIT);                                         0266 04957000
         LA    @10,TRMWRIT(,PCTPTR)                                0266 04958000
