         AL    @10,I                                               0490 06006000
         ST    @10,I                                               0490 06007000
         C     @10,@CF01832                                        0490 06008000
         BNH   @DL00471                                            0490 06009000
*EQAFOFF:                                                          0491 06010000
*   AFEQFIX=OFF;                    /* TURN OFF THE AUDIT FLAG FOR 0491 06011000
*                                      THIS SECTION OF CODE          */ 06012000
EQAFOFF  NI    AFEQFIX(PCTPTR),B'11111101'                         0491 06013000
*                                                                  0492 06014000
*   /*****************************************************************/ 06015000
*   /*                                                               */ 06016000
*   /* BECAUSE OF THE POSSIBILITY THAT AHLSBUF MAY HAVE DISABLED ITS */ 06017000
*   /* SELF IF THERE WHERE NO GTFBCB AVAILABLE TO IT ON THE EMPTY    */ 06018000
*   /* QUEUE, GTFSSRB MAY NEED TO BE REFRESHED                       */ 06019000
*   /*                                                               */ 06020000
*   /*****************************************************************/ 06021000
*                                                                  0492 06022000
*/*%EQOK: D (NO,%MA,YES,) GTFSSRB=0                                  */ 06023000
*/* N AHLSBUF SHOULD NOT BE REENABLED IF A SERIOUS ERROR HAS OCCURRED   06024000
*/*                                                                  */ 06025000
*/* D (YES,XWRITEST,NO,) IS WSERFLAG SET                             */ 06026000
*/* S GETLOCK: OBTAIN THE QLOCK                                      */ 06027000
*/* P REFRESH GTFSSRB FROM SRBPTR                                    */ 06028000
*/* S FREQLOCK: RELEASE THE QLOCK                                    */ 06029000
*/* P DIAGNOSTIC: INCREMENT SBUFDISA BY 1                            */ 06030000
*                                                                  0492 06031000
*   IF GTFSSRB=0                    /* IS AHLSBUF DISABLED           */ 06032000
*     THEN                                                         0492 06033000
