*       GPR01P) UNRESTRICTED;                                      0307 05104000
*   END;                            /* WAIT ECB(PGECB) WAIT FOR THE     05105000
*                                      PGFIX TO COMPLETE             */ 05106000
*   WSMODBEG=FIXWORD(1);                                           0309 05107000
         L     @10,FIXWORD                                         0309 05108000
         ST    @10,WSMODBEG(,PCTPTR)                               0309 05109000
*   WSMODEND=FIXWORD(2);                                           0310 05110000
         L     @09,FIXWORD+4                                       0310 05111000
         ST    @09,WSMODEND(,PCTPTR)                               0310 05112000
*   SDENTRY=SDENTRY+1;              /* PREPARE TO ADD AHLWSMOD TO THE   05113000
*                                      SDUMPLST                      */ 05114000
         LA    @01,1                                               0311 05115000
         AL    @01,SDENTRY                                         0311 05116000
         ST    @01,SDENTRY                                         0311 05117000
*   SDBEGAD(SDENTRY)=FIXWORD(1);    /* PUT IN THE BEGINNING ADDRESS  */ 05118000
         SLA   @01,3                                               0312 05119000
         ST    @10,SDBEGAD-8(@01)                                  0312 05120000
*   SDENDAD(SDENTRY)=FIXWORD(2);    /* PUT IN THE END ADDRESS        */ 05121000
         ST    @09,SDENDAD-8(@01)                                  0313 05122000
*   SDENDBIT(SDENTRY)=ON;           /* FLAG THIS AS THE NEW END OF 0314 05123000
*                                      THE LIST                      */ 05124000
         LA    @10,SDENDBIT-8(@01)                                 0314 05125000
         OI    0(@10),B'10000000'                                  0314 05126000
*   SDENDBIT(SDENTRY-1)=OFF;        /* REMOVE THE OLD END FLAG       */ 05127000
         LA    @10,SDENDBIT-16(@01)                                0315 05128000
         NI    0(@10),B'01111111'                                  0315 05129000
*   DO;                             /* LOAD EP('AHLSBUF') ERRET(RET1)*/ 05130000
*     RESPECIFY                                                    0317 05131000
*      (GPR00P,                                                    0317 05132000
*       GPR01P,                                                    0317 05133000
*       GPR15F) RESTRICTED;                                        0317 05134000
*     GPR00P=ADDR('AHLSBUF ');      /* LOAD ENTRY POINT NAME ADDRESS */ 05135000
         LA    GPR00P,@CC02159                                     0318 05136000
*     GPR01P=0;                     /* JOBLIB OR LINKLIB DCB         */ 05137000
         SLR   GPR01P,GPR01P                                       0319 05138000
*     GPR01P=GPR01P|'80000000'X;    /* ERROR RETURN LABEL SPECIFIED  */ 05139000
         O     GPR01P,@CF02148                                     0320 05140000
*     SVC(8);                       /* ISSUE LOAD SVC                */ 05141000
         SVC   8                                                   0321 05142000
*     IF GPR15F^=0 THEN             /* LOAD WAS UNSUCCESSFUL         */ 05143000
         LTR   GPR15F,GPR15F                                       0322 05144000
         BNZ   @RT00322                                            0322 05145000
*       GOTO RET1;                  /* BRANCH TO ERROR LOCATION      */ 05146000
*     RESPECIFY                                                    0324 05147000
*      (GPR00P,                                                    0324 05148000
*       GPR01P,                                                    0324 05149000
*       GPR15F) UNRESTRICTED;                                      0324 05150000
*   END;                            /* LOAD EP('AHLSBUF') ERRET(RET1)*/ 05151000
*   SRBEP=LOADREG;                  /* STORE ADDR OF AHLSBUF EP IN 0326 05152000
