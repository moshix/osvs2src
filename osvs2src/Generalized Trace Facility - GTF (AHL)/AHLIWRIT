*   SDENDBIT(SDENTRY)=ON;           /* FLAG THIS AS THE NEW END OF 0274 05015000
*                                      THE LIST                      */ 05016000
         LA    @09,SDENDBIT-8(@10)                                 0274 05017000
         OI    0(@09),B'10000000'                                  0274 05018000
*   SDENDBIT(SDENTRY-1)=OFF;        /* REMOVE THE OLD END FLAG       */ 05019000
         LA    @09,SDENDBIT-16(@10)                                0275 05020000
         NI    0(@09),B'01111111'                                  0275 05021000
*   DO;                             /* LOAD EP('AHLWSMOD')         0276 05022000
*                                      ERRET(RET4)                   */ 05023000
*     RESPECIFY                                                    0277 05024000
*      (GPR00P,                                                    0277 05025000
*       GPR01P,                                                    0277 05026000
*       GPR15F) RESTRICTED;                                        0277 05027000
*     GPR00P=ADDR('AHLWSMOD');      /* LOAD ENTRY POINT NAME ADDRESS */ 05028000
         LA    GPR00P,@CC02153                                     0278 05029000
*     GPR01P=0;                     /* JOBLIB OR LINKLIB DCB         */ 05030000
         SLR   GPR01P,GPR01P                                       0279 05031000
*     GPR01P=GPR01P|'80000000'X;    /* ERROR RETURN LABEL SPECIFIED  */ 05032000
         O     GPR01P,@CF02148                                     0280 05033000
*     SVC(8);                       /* ISSUE LOAD SVC                */ 05034000
         SVC   8                                                   0281 05035000
*     IF GPR15F^=0 THEN             /* LOAD WAS UNSUCCESSFUL         */ 05036000
         SLR   @14,@14                                             0282 05037000
         CR    GPR15F,@14                                          0282 05038000
         BNE   @RT00282                                            0282 05039000
*       GOTO RET4;                  /* BRANCH TO ERROR LOCATION      */ 05040000
*     RESPECIFY                                                    0284 05041000
*      (GPR00P,                                                    0284 05042000
*       GPR01P,                                                    0284 05043000
*       GPR15F) UNRESTRICTED;                                      0284 05044000
*   END;                            /* LOAD EP('AHLWSMOD')         0285 05045000
*                                      ERRET(RET4) AHLSBUF, AHLSBLOK,   05046000
*                                      & AHLSFEOB                    */ 05047000
*   REG1=REG1&'00FFFFFF'X;          /* ZERO HIGH ORDER BYTE OF LENGTH   05048000
*                                      REGISTER              @AZ14120*/ 05049000
         N     REG1,@CF02155                                       0286 05050000
*   FIXWORD(1)=LOADREG;             /* PREPARE THE PGFIX LOW ADDR    */ 05051000
         ST    LOADREG,FIXWORD                                     0287 05052000
*   FIXWORD(2)=LOADREG+LOADLEN*8;   /* PREPARE THE PGFIX HIGH ADDR   */ 05053000
         LR    @10,LOADLEN                                         0288 05054000
         SLA   @10,3                                               0288 05055000
         ALR   @10,LOADREG                                         0288 05056000
         ST    @10,FIXWORD+4                                       0288 05057000
*   PGECB=0;                                                       0289 05058000
         ST    @14,PGECB                                           0289 05059000
*   DO;                             /* PGFIX (R) A(FIXWORD(1)->PLACE)   05060000
*                                      EA(FIXWORD(2)->PLACE)       0290 05061000
*                                      ECB(PGECB) LONG(Y)            */ 05062000
*     RESPECIFY                                                    0291 05063000
*      (GPR00P,                                                    0291 05064000
*       GPR01P) RESTRICTED;                                        0291 05065000
*     GPR00P=ADDR(PGECB);           /* ECB ADDR                      */ 05066000
         LA    GPR00P,PGECB                                        0292 05067000
*     GPR01P=ADDR(FIXWORD(1)->PLACE);/* PAGE ADDR                    */ 05068000
         L     GPR01P,FIXWORD                                      0293 05069000
*     RESPECIFY                                                    0294 05070000
*       GPR15P RESTRICTED;                                         0294 05071000
*     GPR15P=ADDR(FIXWORD(2)->PLACE);/* VSL ADDR                     */ 05072000
         LR    GPR15P,@10                                          0295 05073000
*     GPR01P=GPR01P|'52000000'X;    /* R, PGFIX, RELEASE & LONG FLAGS*/ 05074000
         O     GPR01P,@CF02157                                     0296 05075000
*     SVC(113);                     /* ISSUE PGFIX SVC               */ 05076000
         SVC   113                                                 0297 05077000
*     RESPECIFY                                                    0298 05078000
*      (GPR00P,                                                    0298 05079000
*       GPR01P) UNRESTRICTED;                                      0298 05080000
*     RESPECIFY                                                    0299 05081000
*       GPR15P UNRESTRICTED;                                       0299 05082000
*   END;                            /* PGFIX (R) A(FIXWORD(1)->PLACE)   05083000
*                                      EA(FIXWORD(2)->PLACE)       0300 05084000
*                                      ECB(PGECB) LONG(Y) THIS WILL     05085000
*                                      FIX THE ENTIRE AHLWSMOD LOAD     05086000
*                                      MODULE WHICH CONTAINS AHLSBUF,   05087000
*                                      AHLSBLOK, AHLSFEOB, & THE   0300 05088000
*                                      ASSOCIATED ERROR RECOVERY RTNS*/ 05089000
*   RFY                                                            0301 05090000
