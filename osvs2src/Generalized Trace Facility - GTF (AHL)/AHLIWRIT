*       GPR15F) UNRESTRICTED;                                      0347 05209000
*   END;                            /* LOAD EP('AHLSFEOB')         0348 05210000
*                                      ERRET(RET3)                   */ 05211000
*   SFEOBRTN=LOADREG;               /* SAVE EP ADDR IN THE GTFPCT    */ 05212000
         ST    LOADREG,SFEOBRTN(,PCTPTR)                           0349 05213000
*   DO;                             /* LOAD EP('AHLSBLOK')         0350 05214000
*                                      ERRET(RET7)                   */ 05215000
*     RESPECIFY                                                    0351 05216000
*      (GPR00P,                                                    0351 05217000
