*     DO;                           /* YES AHLSBUF WAS WANTING IT    */ 07526000
*       SCHEDBUF=CLEAR;             /* CLEAR THE REQUEST NOW THAT IT    07527000
*                                      IS GOING TO BE TAKEN CARE OF  */ 07528000
         MVI   SCHEDBUF(PCTPTR),X'00'                              0899 07529000
*       RFY                                                        0900 07530000
*        (COMPREG,                                                 0900 07531000
*         SWAPREG) RSTD;            /* TRY TO GET CONTROL OF GTFSSRB */ 07532000
*       COMPREG=GTFSSRB|'80000000'X;/* THIS WILL BE THE CONTROL AND     07533000
*                                      WILL VERIFY THAT THE SRBAVAIL    07534000
*                                      BIT WAS ON AT THE TIME OF THE    07535000
*                                      CS                            */ 07536000
         L     COMPREG,GTFSSRB(,PCTPTR)                            0901 07537000
         O     COMPREG,@CF02148                                    0901 07538000
*       SWAPREG=COMPREG&'7FFFFFFF'X;/* THIS WILL BE THE UPDATE AND 0902 07539000
*                                      WILL TURN OFF THE SRBAVAIL BIT*/ 07540000
         LR    SWAPREG,COMPREG                                     0902 07541000
