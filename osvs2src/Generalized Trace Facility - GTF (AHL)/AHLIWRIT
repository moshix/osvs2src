*   AFQLWAIT=ON;                                                   0883 07463000
         OI    AFQLWAIT(PCTPTR),B'00010000'                        0883 07464000
*   DO;                             /* WAIT ECB(QLOCKECB)            */ 07465000
*     RESPECIFY                                                    0885 07466000
*      (GPR00P,                                                    0885 07467000
*       GPR01P) RESTRICTED;                                        0885 07468000
*     GPR00P=1;                     /* LOAD WAIT COUNT               */ 07469000
         LA    GPR00P,1                                            0886 07470000
*     GPR01P=ADDR(QLOCKECB);        /* ADDRESS OF ECB                */ 07471000
