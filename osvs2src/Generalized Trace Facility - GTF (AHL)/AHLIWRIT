*   M035NAME='AHLSFEOB';                                           0369 05287000
RET3     L     @10,@CA01950                                        0369 05288000
         MVC   M035NAME-M035NAME(8,@10),@CC02163                   0369 05289000
*   GO TO LOADMSG;                                                 0370 05290000
         B     LOADMSG                                             0370 05291000
*RET7:                                                             0371 05292000
*   M035NAME='AHLSBLOK';                                           0371 05293000
RET7     L     @10,@CA01950                                        0371 05294000
         MVC   M035NAME-M035NAME(8,@10),@CC02165                   0371 05295000
*LOADMSG:                                                          0372 05296000
*   DO;                             /* WTOP MF(E,AHL035)             */ 05297000
LOADMSG  DS    0H                                                  0373 05298000
*     RFY                                                          0373 05299000
*       GPR01P RSTD;                /* RESTRICT REG 1                */ 05300000
*     GPR01P=ADDR(AHL035);          /* ADDRESS CONTROL PROGRAM LIST  */ 05301000
         L     GPR01P,@CA01945                                     0374 05302000
*     SVC(35);                      /* ISSUE WTO SVC                 */ 05303000
         SVC   35                                                  0375 05304000
*     RFY                                                          0376 05305000
*       GPR01P UNRSTD;              /* RELEASE REG 1                 */ 05306000
*   END;                            /* WTOP MF(E,AHL035)             */ 05307000
*   GO TO CLEANUP;                  /* CLEAN UP AND TERMINATE THE  0378 05308000
*                                      TRACE WRITER                  */ 05309000
*                                                                  0378 05310000
         B     CLEANUP                                             0378 05311000
*   /*****************************************************************/ 05312000
*   /*                                                               */ 05313000
*   /* LOADING AND FIXING SUCCESSFUL                                 */ 05314000
*   /*                                                               */ 05315000
*   /*****************************************************************/ 05316000
*                                                                  0379 05317000
*/*LDFXDONE: N AT THIS POINT STACK BEGINS ACCEPTING DATA             */ 05318000
*/* P (INT,%WSF15,EXT,) CLEAR AFLDFX                                 */ 05319000
*                                                                  0379 05320000
*LDFXDONE:                                                         0379 05321000
*   AFLDFX=OFF;                     /* TURN OFF THE AUDIT FLAG FOR 0379 05322000
*                                      THE PREVIOUS SECTION OF CODE  */ 05323000
LDFXDONE NI    AFLDFX(PCTPTR),B'11011111'                          0379 05324000
*                                                                  0380 05325000
*   /*****************************************************************/ 05326000
*   /*                                                               */ 05327000
*   /* INITIALIZATION OF THE WRITER-STACK SERVICE MODULES, CONTROL   */ 05328000
*   /* BLOCKS, AND DATA AREAS IS NOW COMPLETE AND FUNCTIONING. THE   */ 05329000
*   /* GTFCMECB IS NOW POSTED TO INFORM THE MOTHER TASK              */ 05330000
*   /*                                                               */ 05331000
*   /*****************************************************************/ 05332000
*                                                                  0380 05333000
*/*%WSF15: S POST: GTFCMECB TO INDICATE INIT COMPLETE                */ 05334000
*/* P CLEAR AFINIT                                                   */ 05335000
*                                                                  0380 05336000
*   DO;                             /* POST (GTFCMECB,0)             */ 05337000
*     RESPECIFY                                                    0381 05338000
*      (GPR00F,                                                    0381 05339000
*       GPR01P) RESTRICTED;                                        0381 05340000
*     GPR00F=0;                     /* POST CODE IN R0               */ 05341000
         SLR   GPR00F,GPR00F                                       0382 05342000
*     GPR01P=ADDR(GTFCMECB);        /* ECB ADDRESS                   */ 05343000
         LA    GPR01P,GTFCMECB(,PCTPTR)                            0383 05344000
*     SVC(2);                       /* ISSUE POST SVC                */ 05345000
         SVC   2                                                   0384 05346000
*     RESPECIFY                                                    0385 05347000
*      (GPR00F,                                                    0385 05348000
*       GPR01P) UNRESTRICTED;                                      0385 05349000
*   END;                            /* POST (GTFCMECB,0)             */ 05350000
*   AFINIT=OFF;                     /* SHOW INITIALIZATION COMPLETE  */ 05351000
         NI    AFINIT(PCTPTR),B'11101111'                          0387 05352000
*                                                                  0388 05353000
*   /*****************************************************************/ 05354000
*   /*                                                               */ 05355000
*   /* THIS IS THE BEGINNING OR TOP OF THE LOOP THAT THE WRITER GOES */ 05356000
*   /* AROUND DURING NORMAL EXECUTION. IT WAITS HERE AT THE TOP FOR  */ 05357000
*   /* WORK TO BECOME AVAILABLE AND THEN PROGRESSES AROUND THE LOOP  */ 05358000
*   /* CHECKING EACH TASK IT IS TO DO IN PRIORITY ORDER. FOR EACH    */ 05359000
*   /* TASK THAT HAS WORK TO DO IT COMPLETES ALL THE WORK POSSIBLE AT*/ 05360000
*   /* THAT TIME AND THEN GOES ON TO THE NEXT TASK. WHEN ALL TASKS   */ 05361000
*   /* HAVE COMPLETED IT RETURNS TO THE TOP AND WAITS                */ 05362000
*   /*                                                               */ 05363000
*   /*****************************************************************/ 05364000
*                                                                  0388 05365000
*/*WAIT: P SET AFWAIT                                                */ 05366000
*/* P IOECBADD= NEXTCHK WAIT ONLY ON ONE ECB                         */ 05367000
*/* S WAIT: ON ECBLIST                                               */ 05368000
*/* P CLEAR AFWAIT                                                   */ 05369000
*                                                                  0388 05370000
*WAIT:                                                             0388 05371000
*   AFWAIT=ON;                                                     0388 05372000
WAIT     OI    AFWAIT(PCTPTR),B'00000100'                          0388 05373000
*   IOECBADD=NEXTCHK;               /* BECAUSE THE POSTING OF      0389 05374000
*                                      MULTIPLE BSAM WRITES MIGHT NOT   05375000
*                                      OCCURR IN THE SAME SEQUENCE AS   05376000
*                                      THE WRITES, ONLY ONE IS WAITED   05377000
*                                      ON AT A TIME. THIS FORCES THEM   05378000
*                                      TO BE NOTICED IN ORDER.       */ 05379000
         L     @10,NEXTCHK                                         0389 05380000
         ST    @10,IOECBADD                                        0389 05381000
*   DO;                             /* WAIT ECBLIST(ECBALIST)        */ 05382000
*     RESPECIFY                                                    0391 05383000
*      (GPR00P,                                                    0391 05384000
*       GPR01P) RESTRICTED;                                        0391 05385000
*     GPR00P=1;                     /* LOAD WAIT COUNT               */ 05386000
         LA    GPR00P,1                                            0392 05387000
*     GPR01P=-ADDR(ECBALIST);       /* ADDRESS OF LIST OF ECB'S      */ 05388000
         LA    GPR01P,ECBALIST                                     0393 05389000
         LCR   GPR01P,GPR01P                                       0393 05390000
*     SVC(1);                       /* WAIT SVC                      */ 05391000
         SVC   1                                                   0394 05392000
*     RESPECIFY                                                    0395 05393000
*      (GPR00P,                                                    0395 05394000
*       GPR01P) UNRESTRICTED;                                      0395 05395000
*   END;                            /* WAIT ECBLIST(ECBALIST)        */ 05396000
*   AFWAIT=OFF;                                                    0397 05397000
*                                                                  0397 05398000
         NI    AFWAIT(PCTPTR),B'11111011'                          0397 05399000
*   /*****************************************************************/ 05400000
*   /*                                                               */ 05401000
*   /* SHOULD THE ESTAE ROUTINE BE ENTERED WHILE AFWAIT IS ON IT WILL*/ 05402000
*   /* RETRY, DISABLE STACK & AHLSBUF, POST THE WRTRERR ECB, AND THEN*/ 05403000
*   /* BRANCH TO THIS LOCATION TO ALLOW THE WRITER TO TERMINATE      */ 05404000
*   /* NORMALLY                                                      */ 05405000
*   /*                                                               */ 05406000
*   /*****************************************************************/ 05407000
*                                                                  0398 05408000
*/*NOWAIT: P                                                         */ 05409000
*/*RETRYPT7: P PUT 0 INTO WRTRCOM ECB                                */ 05410000
*                                                                  0398 05411000
*NOWAIT:                            /* THIS LABEL IS USED TO BRANCH     05412000
*                                      BACK TO THE BEGINNING OF THE     05413000
*                                      TRACE WRITER LOOP BUT AVOIDING   05414000
*                                      THE WAIT                      */ 05415000
*RETRYPT7:                                                         0398 05416000
*   WRTRCOM=0;                      /* ZERO OUT THIS ECB NOW SO THAT,   05417000
*                                      IF AHLSBUF SHOULD MOVE MORE 0398 05418000
*                                      DATA ONTO THE WRITE QUEUE   0398 05419000
*                                      AFTER THE TRACE WRITER IS DONE   05420000
*                                      CHECKING IT BUT BEFORE IT   0398 05421000
*                                      RETURNS TO THE TOP OF THE   0398 05422000
*                                      LOOP, THE ECB CAN BE POSTED 0398 05423000
*                                      AGAIN                         */ 05424000
NOWAIT   DS    0H                                                  0398 05425000
RETRYPT7 SLR   @10,@10                                             0398 05426000
         ST    @10,WRTRCOM(,PCTPTR)                                0398 05427000
*                                                                  0399 05428000
*   /*****************************************************************/ 05429000
*   /*                                                               */ 05430000
*   /* THE FIRST TASK PERFORMED BY THE TRACE WRITER IS TO REMOVE AS  */ 05431000
*   /* MANY GTFBCB FROM THE WRITE QUEUE AS POSSIBLE.                 */ 05432000
*   /*                                                               */ 05433000
*   /*****************************************************************/ 05434000
*                                                                  0399 05435000
*/*%IOLOOP: P SET AFBUFOFF                                           */ 05436000
*/* D (YES,,NO,%IE34) DIAGNOSTIC: IS EQLENGTH <EQMIN                 */ 05437000
*/* P DIAGNOSTIC: EQMIN=EQLENGTH                                     */ 05438000
*/*%IE34: P (INT,%NOCHK,EXT,CHKLOOP)                                 */ 05439000
*                                                                  0399 05440000
*   AFBUFOFF=ON;                    /* TURN ON THE AUDIT FLAG FOR  0399 05441000
*                                      THIS SECTION OF CODE          */ 05442000
         OI    AFBUFOFF(PCTPTR),B'00010000'                        0399 05443000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05444000
*                                                                  0400 05445000
*   IF EQLENGTH<EQMIN               /* SAVE THE LOWEST NUMBER OF   0400 05446000
*                                      AVAILABLE GTFBCB              */ 05447000
*     THEN                                                         0400 05448000
         L     @10,EQLENGTH(,PCTPTR)                               0400 05449000
         C     @10,EQMIN(,PCTPTR)                                  0400 05450000
         BNL   @RF00400                                            0400 05451000
*     EQMIN=EQLENGTH;                                              0401 05452000
*                                                                  0401 05453000
         ST    @10,EQMIN(,PCTPTR)                                  0401 05454000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05455000
*/* IN THE CASE OF THE INTERNAL TRACE WRITER ALL GTFBCB WILL BE REMOVED 05456000
*   BECAUSE NOTHING HAS TO BE DONE WITH THE DATA                     */ 05457000
*/*%NOCHK: D (YES,%NOWRT,NO,) WQHEAD=0                               */ 05458000
*/* D (YES,,NO,%K) DIAGNOSTIC: IS WQLENGTH >WQMAX                    */ 05459000
*/* P DIAGNOSTIC: WQMAX=WQLENGTH                                     */ 05460000
*/*%K: S GETQLOCK: OBTAIN THE GTFBCB QUEUE LOCK                      */ 05461000
*/* P BCBHOLDR=WQHEAD WQHEAD=0                                       */ 05462000
*/*%C: P DIAGNOSTIC: WQLENGTH=0                                      */ 05463000
*/* S FREQLOCK: RELEASE THE GTFBCB QUEUE LOCK                        */ 05464000
*                                                                  0402 05465000
*   IF WQHEAD^=0 THEN                                              0402 05466000
@RF00400 L     @10,WQHEAD(,PCTPTR)                                 0402 05467000
         LTR   @10,@10                                             0402 05468000
         BZ    @RF00402                                            0402 05469000
*     DO;                           /* IF THE WRITE QUEUE IS NOT   0403 05470000
*                                      EMPTY DO THE FOLLOWING TO   0403 05471000
*                                      REMOVE GTFBCB FROM IT         */ 05472000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05473000
*                                                                  0404 05474000
*       IF WQLENGTH>WQMAX           /* SAVE THE HIGHEST NUM OF GTFBCB   05475000
*                                      EVER ON THE WRITE QUEUE AT ONE   05476000
*                                      TIME                          */ 05477000
*         THEN                                                     0404 05478000
         L     @10,WQLENGTH(,PCTPTR)                               0404 05479000
         C     @10,WQMAX(,PCTPTR)                                  0404 05480000
         BNH   @RF00404                                            0404 05481000
*         WQMAX=WQLENGTH;                                          0405 05482000
*                                                                  0405 05483000
         ST    @10,WQMAX(,PCTPTR)                                  0405 05484000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05485000
*                                                                  0406 05486000
*       CALL GETQLOCK;              /* INTERNAL SUBROUTINE TO OBTAIN    05487000
*                                      QLOCK                         */ 05488000
@RF00404 BAL   @14,GETQLOCK                                        0406 05489000
*       BCBHOLDR=WQHEAD;            /* TAKE ALL THE GTFBCB OFF OF THE   05490000
*                                      WRITE QUEUE                   */ 05491000
         L     @10,WQHEAD(,PCTPTR)                                 0407 05492000
         ST    @10,BCBHOLDR                                        0407 05493000
*       WQHEAD=0;                                                  0408 05494000
*                                                                  0408 05495000
         SLR   @10,@10                                             0408 05496000
         ST    @10,WQHEAD(,PCTPTR)                                 0408 05497000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05498000
*                                                                  0409 05499000
*       WQLEN=WQLENGTH;             /* SAVE CURRENT NUMBER OF W-Q  0409 05500000
*                                      GTFBCB                @ZA27751*/ 05501000
         L     @09,WQLENGTH(,PCTPTR)                               0409 05502000
         ST    @09,WQLEN                                           0409 05503000
*       WQLENGTH=0;                 /* KEEP TRACK OF THE NUM OF    0410 05504000
*                                      GTFBCB ON THE WRITE QUEUE     */ 05505000
         ST    @10,WQLENGTH(,PCTPTR)                               0410 05506000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05507000
*                                                                  0411 05508000
*       CALL FREQLOCK;              /* AN INTERNAL SUBROUTINE TO   0411 05509000
*                                      RELEASE THE QLOCK             */ 05510000
         BAL   @14,FREQLOCK                                        0411 05511000
*                                                                  0412 05512000
*       /*************************************************************/ 05513000
*       /*                                                           */ 05514000
*       /* THIS LOOP TAKES THE GTFBCB JUST REMOVED FROM THE WRITE    */ 05515000
*       /* QUEUE AND ALTERS THEM AS IF THEY HAD BEEN WRITTEN OUT TO A*/ 05516000
*       /* TRACE DATASET SO THAT THEY MAY USED AGAIN                 */ 05517000
*       /*                                                           */ 05518000
*       /*************************************************************/ 05519000
*                                                                  0412 05520000
*/*MOCKIO: D (YES,%NOWRT,NO,) BCBHOLDR=0                             */ 05521000
*/* P (,%RBX) RLSEBUF= BCBHOLDR BCBHOLDR= BCBHOLDR-> BCBCHAIN        */ 05522000
*                                                                  0412 05523000
*MOCKIO:                                                           0412 05524000
*       DO I=1 TO WQLEN;            /* SERIES OF WRITE Q'S   @ZA27751*/ 05525000
MOCKIO   LA    @10,1                                               0412 05526000
         B     @DE00412                                            0412 05527000
@DL00412 DS    0H                                                  0413 05528000
*         RLSEBUF=BCBHOLDR;         /* TAKE THE NEXT GTFBCB OFF OF 0413 05529000
*                                      THE STRING REMOVED FROM THE 0413 05530000
*                                      WRITE QUEUE                   */ 05531000
         L     @10,BCBHOLDR                                        0413 05532000
         ST    @10,RLSEBUF                                         0413 05533000
*         BCBHOLDR=BCBHOLDR->BCBCHAIN;/* RETAIN THE REST OF THE    0414 05534000
*                                      STRING FOR THE NEXT TIME    0414 05535000
*                                      THROUGH THE LOOP              */ 05536000
*                                                                  0414 05537000
         L     @09,BCBCHAIN(,@10)                                  0414 05538000
         ST    @09,BCBHOLDR                                        0414 05539000
*         /***********************************************************/ 05540000
*         /*                                                         */ 05541000
*         /* DECIDE IF THIS GTFBCB NEEDS ONLY TO HAVE ITS USE COUNT  */ 05542000
*         /* DECREMENTED OR IF IT MUST BE PUT ON THE RELEASE QUEUE   */ 05543000
*         /*                                                         */ 05544000
*         /***********************************************************/ 05545000
*                                                                  0415 05546000
*/*%RBX: P DIAGNOSTIC: INCREMENT NUMOFBUF & RLSEBUF-> BCBFREQ BY 1   */ 05547000
*/*IOCS: P SWAPBCB= RLSEBUF-> BCBSWAP FOR CS LOGIC                   */ 05548000
*/* D (YES,,NO,%A2) SWAPUSE=1                                        */ 05549000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05550000
*                                                                  0415 05551000
*         NUMOFBUF=NUMOFBUF+1;      /* KEEP A TOTAL OF THE NUMBER OF    05552000
*                                      GTFBCB PROCESSED              */ 05553000
         LA    @09,1                                               0415 05554000
         L     @01,NUMOFBUF(,PCTPTR)                               0415 05555000
         ALR   @01,@09                                             0415 05556000
         ST    @01,NUMOFBUF(,PCTPTR)                               0415 05557000
*         RLSEBUF->BCBFREQ=RLSEBUF->BCBFREQ+1;                     0416 05558000
*                                                                  0416 05559000
         AL    @09,BCBFREQ(,@10)                                   0416 05560000
         ST    @09,BCBFREQ(,@10)                                   0416 05561000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05562000
*                                                                  0417 05563000
*IOCS:                                                             0417 05564000
*         SWAPBCB=RLSEBUF->BCBSWAP; /* BECAUSE THE PORTIONS OF THE 0417 05565000
*                                      GTFBCB CONTAINED IN BCBSWAP 0417 05566000
*                                      REQUIRE MP SERIALIZATION VIA     05567000
*                                      CS THE VALUES MUST BE SAVED 0417 05568000
*                                      BEFORE REFERENCING THEM & THEN   05569000
*                                      ONLY THE STABLE COPY MAY BE 0417 05570000
*                                      REFERENCED                    */ 05571000
IOCS     LA    @10,WORKAREA                                        0417 05572000
         L     @09,RLSEBUF                                         0417 05573000
         L     @01,BCBSWAP(,@09)                                   0417 05574000
         ST    @01,SWAPBCB(,@10)                                   0417 05575000
*         IF SWAPUSE=1              /* WAS THE GTFBCB ON ONLY 1 QUEUE*/ 05576000
*           THEN                                                   0418 05577000
         CLC   SWAPUSE(2,@10),@CH00070                             0418 05578000
         BNE   @RF00418                                            0418 05579000
*           DO;                     /* YES                           */ 05580000
*                                                                  0419 05581000
*             /*******************************************************/ 05582000
*             /*                                                     */ 05583000
*             /* BECAUSE THE GTFBCB WAS ON ONLY 1 QUEUE IT IS NOW NOT*/ 05584000
*             /* ON ANY QUEUE. UNDER THESE CONDITIONS ONLY THIS      */ 05585000
*             /* EXECUTION OF THIS CODE CAN LOCATE IT. THEREFORE     */ 05586000
*             /* UNTIL IT IS PLACED ONTO ANOTHER QUEUE NO SPECIAL    */ 05587000
*             /* HANDLING, SERIALIZATION, OR MP LOGIC IS NEEDED TO   */ 05588000
*             /* MODIFY IT. ALSO IT MUST BE MADE AVAILABLE AGAIN BY  */ 05589000
*             /* BEING PUT ONTO THE RELEASE QUEUE                    */ 05590000
*             /*                                                     */ 05591000
*             /*******************************************************/ 05592000
*                                                                  0420 05593000
*/* N (EXT,,INT,%DGF2) MP LOGIC NOT NEEDED BECAUSE SWAPUSE=1         */ 05594000
*/* P (,%DGF2A) DIAGNOSTIC: RLSEBUF-> DECBFLAG=OFF & RQFLAG=ON       */ 05595000
*/*%DGF2: P DIAGNOSTIC: RLSEBUF-> WQFLAG=OFF & RQFLAG=ON             */ 05596000
*/*%DGF2A: P SAVE A STABLE COPY OF RQHEAD                            */ 05597000
*/*OFFCS: P RLSEBUF-> RQCHAIN=STABLE COPY RQHEAD                     */ 05598000
*/* P CS RLSEBUF INTO RQHEAD                                         */ 05599000
*/* D (FAIL,OFFCS,OK,%EOL) CS?                                       */ 05600000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05601000
*                                                                  0420 05602000
*             RLSEBUF->WQFLAG=OFF;  /* TURN OFF THE GTFBCB FLAG THAT    05603000
*                                      SHOWS IT ON THE WRITE QUEUE   */ 05604000
*             RLSEBUF->RQFLAG=ON;   /* TURN ON THE RELEASE QUEUE FLAG*/ 05605000
         OI    RQFLAG(@09),B'00000100'                             0421 05606000
         NI    WQFLAG(@09),B'10111111'                             0421 05607000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05608000
*                                                                  0422 05609000
*             RFY                                                  0422 05610000
*               COMPREG RSTD;                                      0422 05611000
*             COMPREG=RQHEAD;       /* MAKE A STABLE COPY OF RQHEAD  */ 05612000
         L     COMPREG,RQHEAD(,PCTPTR)                             0423 05613000
*OFFCS:                                                            0424 05614000
*             RLSEBUF->RQCHAIN=COMPREG;/* PUT RQHEAD INTO CHAIN FIELD   05615000
*                                      OF THE GTFBCB TO BE ADDED     */ 05616000
OFFCS    L     @10,RLSEBUF                                         0424 05617000
         ST    COMPREG,RQCHAIN(,@10)                               0424 05618000
*             CS(COMPREG,RLSEBUF,RQHEAD);/* VERIFY RQHEAD HASN'T   0425 05619000
*                                      CHANGED AND ADD THE GTFBCB TO    05620000
*                                      THE HEAD OF THE QUEUE         */ 05621000
         CS    COMPREG,@10,RQHEAD(PCTPTR)                          0425 05622000
*             BC(4,OFFCS);          /* IF IT FAILED TRY AGAIN        */ 05623000
         BC    4,OFFCS                                             0426 05624000
*             RFY                                                  0427 05625000
*               COMPREG UNRSTD;                                    0427 05626000
*           END;                                                   0428 05627000
*                                                                  0429 05628000
*         /***********************************************************/ 05629000
*         /*                                                         */ 05630000
*         /* IF THE USE COUNT IS NOT 1 THE GTFBCB IS STILL IN USE ON */ 05631000
*         /* ANOTHER QUEUE SO JUST DECREMENT IT                      */ 05632000
*         /*                                                         */ 05633000
*         /***********************************************************/ 05634000
*                                                                  0429 05635000
*/*%A2: P (EXT,,INT,%DGF1)                                           */ 05636000
*/* P (,%DGF1A) DECREMENT SWAPUSE 1 & DIAGNOSTIC DECBFSWA=OFF        */ 05637000
*/*%DGF1: P DECREMENT SWAPUSE 1 & DIAGNOSTIC WQFSWAP=OFF             */ 05638000
*/*%DGF1A: P CS NEW VALUES FROM SWAPBCB INTO RLSEBUF-> BCBSWAP       */ 05639000
*/* D (OK,,FAIL,IOCS) CS?                                            */ 05640000
*/*%EOL: P (INT,MOCKIO,EXT,CHKLOOP) END OF LOOP                      */ 05641000
*                                                                  0429 05642000
*         ELSE                                                     0429 05643000
*           DO;                     /* NO, THE GTFBCB IS STILL ON AT    05644000
*                                      LEAST ONE QUEUE               */ 05645000
         B     @RC00418                                            0429 05646000
@RF00418 DS    0H                                                  0430 05647000
*             COMPBCB=SWAPBCB;      /* MAKE ANOTHER COPY FOR THE   0430 05648000
*                                      COMPARE BEFORE MODIFYING THE     05649000
*                                      ONE FOR THE SWAP              */ 05650000
         LA    @10,WORKAREA                                        0430 05651000
         L     @09,SWAPBCB(,@10)                                   0430 05652000
         ST    @09,COMPBCB(,@10)                                   0430 05653000
*             SWAPUSE=SWAPUSE-1;    /* PREPARE A DECREMENTED BCBUSE     05654000
*                                      FOR THE CS                    */ 05655000
         LH    @09,SWAPUSE(,@10)                                   0431 05656000
         BCTR  @09,0                                               0431 05657000
         STH   @09,SWAPUSE(,@10)                                   0431 05658000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05659000
*                                                                  0432 05660000
*             WQFSWAP=OFF;          /* PREPARE THE NEW QUEUE FLAGS   */ 05661000
         NI    WQFSWAP(@10),B'10111111'                            0432 05662000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05663000
*                                                                  0433 05664000
*             CS(COMPBCB,SWAPBCB,RLSEBUF->BCBSWAP);/* THIS WILL    0433 05665000
*                                      EITHER PLACE THE UPDATED    0433 05666000
*                                      VALUES INTO THE GTFBCB OR NOT,   05667000
*                                      IF THEY HAVE BECOME INVALID   */ 05668000
         L     @09,COMPBCB(,@10)                                   0433 05669000
         L     @01,SWAPBCB(,@10)                                   0433 05670000
         L     @15,RLSEBUF                                         0433 05671000
         CS    @09,@01,BCBSWAP(@15)                                0433 05672000
         BZ    @BC00433                                            0433 05673000
         ST    @09,COMPBCB(,@10)                                   0433 05674000
@BC00433 DS    0H                                                  0434 05675000
*             BC(4,IOCS);           /* IF THE DATA WAS INVALIDATED,     05676000
*                                      GO BACK AND START OVER AGAIN  */ 05677000
         BC    4,IOCS                                              0434 05678000
*           END;                                                   0435 05679000
*       END MOCKIO;                 /* ALL THE GTFBCB REMOVE FROM THE   05680000
*                                      WRITE QUEUE HAVE BEEN DISPOSED   05681000
*                                      OF                            */ 05682000
@RC00418 LA    @10,1                                               0436 05683000
         AL    @10,I                                               0436 05684000
@DE00412 ST    @10,I                                               0436 05685000
         C     @10,WQLEN                                           0436 05686000
         BNH   @DL00412                                            0436 05687000
*                                                                  0437 05688000
*/*%NOWRT: P CLEAR AFBUFOFF                                          */ 05689000
*                                                                  0437 05690000
*       /*************************************************************/ 05691000
*       /*                                                           */ 05692000
*       /* WHEN EXECUTION REACHES THIS POINT AS MANY GTFBCB AS       */ 05693000
*       /* POSSIBLE HAVE BEEN REMOVED FROM THE WRITE QUEUE           */ 05694000
*       /*                                                           */ 05695000
*       /*************************************************************/ 05696000
*                                                                  0437 05697000
*     END;                                                         0437 05698000
*   AFBUFOFF=OFF;                   /* TURN OFF THE AUDIT FLAG FOR 0438 05699000
*                                      THIS SECTION OF CODE          */ 05700000
@RF00402 DS    0H                                                  0439 05701000
*                                                                  0439 05702000
*   /*****************************************************************/ 05703000
*   /*                                                               */ 05704000
*   /* THIS SECTION OF CODE REMOVES ALL THE GTFBCB CURRENTLY ON THE  */ 05705000
*   /* RELEASE QUEUE, DOES A PAGE RELEASE ON EACH ASSOCIATED GTFBUFR,*/ 05706000
*   /* AND THEN PUTS THE GTFBCB ONTO THE EMPTY QUEUE                 */ 05707000
*   /*                                                               */ 05708000
*   /*****************************************************************/ 05709000
*                                                                  0439 05710000
*/* P SET AFPGRLSE                                                   */ 05711000
*/* D (YES,%NOIOX,NO,) RQHEAD=0                                      */ 05712000
*/* P BCBHOLDR=RQHEAD                                                */ 05713000
*/*CSRQHEAD: P CS 0 INTO RQHEAD                                      */ 05714000
*/* D (FAIL,CSRQHEAD,OK,) CS                                         */ 05715000
*/* P DIAGNOSTIC: INCREMENT RQPURGE BY 1 & RQCOUNT=0                 */ 05716000
*/* P RLSEBUF= BCBHOLDR                                              */ 05717000
*/*RLSELOOP: S PGRLSE: RLSEBUF-> GTFBCB-> GTFBUFR                    */ 05718000
*/* P DIAGNOSTIC: RLSEBUF-> RQFLAG=OFF & EQFLAG=ON, INCR RQCOUNT BY 1   05719000
*/*                                                                  */ 05720000
*/* D (NO,,YES,%J) RLSEBUF-> RQCHAIN=0                               */ 05721000
*/* P (,RLSELOOP) RLSEBUF= RLSEBUF-> RQCHAIN                         */ 05722000
*/*%J: P DIAGNOSTIC: RQTOTAL= RQTOTAL+RQCOUNT                        */ 05723000
*/* S GETQLOCK: OBTAIN THE GTFBCB QUEUE LOCK                         */ 05724000
*/* P RLSEBUF-> EQCHAIN= EQHEAD                                      */ 05725000
*/* N EQCHAIN & RQCHAIN BOTH REFER TO THE SAME GTFBCB FIELD          */ 05726000
*/* P EQHEAD=BCBHOLDR                                                */ 05727000
*/* P DIAGNOSTIC: EQLENGTH= EQLENGTH+ RQCOUNT                        */ 05728000
*/* S FREQLOCK: RELEASE THE GTFBCB QUEUE LOCK                        */ 05729000
*/*%NOIOX: P CLEAR AFPGRLSE                                          */ 05730000
*                                                                  0439 05731000
*   AFPGRLSE=ON;                    /* SET THE FLAG FOR THIS SECTION    05732000
*                                      OF CODE AUDIT                 */ 05733000
         OI    AFPGRLSE(PCTPTR),B'00000100'                        0439 05734000
         NI    AFBUFOFF(PCTPTR),B'11101111'                        0439 05735000
*   IF RQHEAD^=0                    /* ARE THERE ANY GTFBCB ON THE 0440 05736000
*                                      RELEASE QUEUE? OF NOT SKIP  0440 05737000
*                                      THIS SECTION                  */ 05738000
*     THEN                                                         0440 05739000
         L     @10,RQHEAD(,PCTPTR)                                 0440 05740000
         LTR   @10,@10                                             0440 05741000
         BZ    @RF00440                                            0440 05742000
*     DO;                           /* YES, THERE ARE GTFBCB ON THE     05743000
*                                      RELEASE QUEUE                 */ 05744000
*       BCBHOLDR=RQHEAD;            /* GET THE ADDRESS OF THE FIRST     05745000
*                                      GTFBCB ON THE RELEASE QUEUE   */ 05746000
         ST    @10,BCBHOLDR                                        0442 05747000
*CSRQHEAD:                                                         0443 05748000
*       CS(BCBHOLDR,0,RQHEAD);      /* ATTEMPT TO PUT A 0 IN RQHEAD     05749000
*                                      TO REMOVE ALL THE GTFBCB      */ 05750000
CSRQHEAD L     @10,BCBHOLDR                                        0443 05751000
         SLR   @09,@09                                             0443 05752000
         CS    @10,@09,RQHEAD(PCTPTR)                              0443 05753000
         BZ    @BC00443                                            0443 05754000
         ST    @10,BCBHOLDR                                        0443 05755000
@BC00443 DS    0H                                                  0444 05756000
*       BC(4,CSRQHEAD);             /* IF THE CS FAILS A GTFBCB WAS     05757000
*                                      ADDED TO THE RELEASE QUEUE  0444 05758000
*                                      WHICH MAKES BCBHOLDR INVALID     05759000
*                                      SO GO BACK & START OVER. THE     05760000
*                                      CS HAS ALREADY STORED THE   0444 05761000
*                                      LATEST RQHEAD INTO BCBHOLDR   */ 05762000
         BC    4,CSRQHEAD                                          0444 05763000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05764000
*                                                                  0445 05765000
*       RQPURGE=RQPURGE+1;                                         0445 05766000
         LA    @10,1                                               0445 05767000
         AL    @10,RQPURGE(,PCTPTR)                                0445 05768000
         ST    @10,RQPURGE(,PCTPTR)                                0445 05769000
*       RQCOUNT=0;                                                 0446 05770000
*                                                                  0446 05771000
         SLR   @10,@10                                             0446 05772000
         ST    @10,RQCOUNT                                         0446 05773000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05774000
*                                                                  0447 05775000
*       RLSEBUF=BCBHOLDR;           /* START WITH THE FIRST GTFBCB IN   05776000
*                                      THE GROUP                     */ 05777000
         L     @10,BCBHOLDR                                        0447 05778000
         ST    @10,RLSEBUF                                         0447 05779000
*RLSELOOP:                                                         0448 05780000
*       RFY                                                        0448 05781000
*        (MACREG1,                                                 0448 05782000
*         MACREG2) RSTD;                                           0448 05783000
RLSELOOP DS    0H                                                  0449 05784000
*       MACREG1=ADDR(RLSEBUF->GBUFRPTR->GTFBUFR);                  0449 05785000
         L     @10,RLSEBUF                                         0449 05786000
         L     MACREG1,GBUFRPTR(,@10)                              0449 05787000
*       MACREG2=MACREG1+LENGTH(GTFBUFR);                           0450 05788000
         LR    MACREG2,MACREG1                                     0450 05789000
         AL    MACREG2,@CF01818                                    0450 05790000
*       GEN CODE(PGRLSE  LA=(MACREG1),HA=(MACREG2))                0451 05791000
*       REFS(MACREG1,MACREG2) SETS(GTFBUFR) NOEXIT NODEFS NOFLOWS  0451 05792000
*           SEQFLOW;                /* THIS WILL CAUSE ALL THE REAL     05793000
*                                      STORAGE AND AUX STORAGE     0451 05794000
*                                      ASSOCIATED WITH THE VIRTUAL 0451 05795000
*                                      STORAGE TO THIS GTFBUFR TO BE    05796000
*                                      RETURNED TO THE SYSTEM. THE 0451 05797000
*                                      CONTENTS ARE DISCARDED. THIS     05798000
*                                      IS DONE SO THAT AHLSBUF WILL     05799000
*                                      NOT CAUSE A PAGE IN WHEN IT 0451 05800000
*                                      MOVES MORE DATA INTO THIS   0451 05801000
*                                      GTFBUFR                       */ 05802000
         PGRLSE  LA=(MACREG1),HA=(MACREG2)                              05803000
*       RFY                                                        0452 05804000
*        (MACREG1,                                                 0452 05805000
*         MACREG2) UNRSTD;                                         0452 05806000
*                                                                  0452 05807000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05808000
*                                                                  0453 05809000
*       RLSEBUF->RQFLAG=OFF;                                       0453 05810000
*       RLSEBUF->EQFLAG=ON;                                        0454 05811000
         L     @10,RLSEBUF                                         0454 05812000
         OI    EQFLAG(@10),B'10000000'                             0454 05813000
         NI    RQFLAG(@10),B'11111011'                             0454 05814000
*       RQCOUNT=RQCOUNT+1;                                         0455 05815000
*                                                                  0455 05816000
         LA    @09,1                                               0455 05817000
         AL    @09,RQCOUNT                                         0455 05818000
         ST    @09,RQCOUNT                                         0455 05819000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05820000
*                                                                  0456 05821000
*       IF RLSEBUF->RQCHAIN^=0      /* HAS THE END OF THE GROUP BEEN    05822000
*                                      REACHED                       */ 05823000
*         THEN                                                     0456 05824000
         L     @10,RQCHAIN(,@10)                                   0456 05825000
         LTR   @10,@10                                             0456 05826000
         BZ    @RF00456                                            0456 05827000
*         DO;                       /* YES, GO PUT THE GROUP ONTO THE   05828000
*                                      ENPTY QUEUE                   */ 05829000
*           RLSEBUF=RLSEBUF->RQCHAIN;/* NO, THEN MOVE ON TO PROCESS     05830000
*                                      THE NEXT ONE                  */ 05831000
         ST    @10,RLSEBUF                                         0458 05832000
*           GO TO RLSELOOP;                                        0459 05833000
         B     RLSELOOP                                            0459 05834000
*         END;                      /* ALL PAGE RELEASES HAVE BEEN 0460 05835000
*                                      DONE                          */ 05836000
