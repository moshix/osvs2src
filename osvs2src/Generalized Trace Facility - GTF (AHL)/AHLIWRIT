*                                      ADDRESS SPACE                 */ 04552000
         L     @10,PSAAOLD                                         0169 04553000
         ST    @10,SRBASCB(,@14)                                   0169 04554000
*   SRBPKF=KEYZERO;                 /* AHLSBUF IS TO RUN KEY 0       */ 04555000
         MVI   SRBPKF(@14),X'00'                                   0170 04556000
*   SRBSAVE=0;                                                     0171 04557000
         SLR   @10,@10                                             0171 04558000
         ST    @10,SRBSAVE(,@14)                                   0171 04559000
*   SRBPARM=ADDR(GTFPCT);           /* PASS GTFPCT ADDR TO AHLSBUF R1*/ 04560000
         ST    PCTPTR,SRBPARM(,@14)                                0172 04561000
*   SRBCPAFF='0000'X;               /* NO CPU AFFINITY               */ 04562000
         MVC   SRBCPAFF(2,@14),@CB01516                            0173 04563000
*   SRBPRIOR=SYSPRIOR;                                             0174 04564000
         MVI   SRBPRIOR(@14),X'00'                                 0174 04565000
*   SRBPTCB=0;                      /* AHLSBUF SHOULD NOT BE       0175 04566000
*                                      ASSOCIATED WITH THIS TCB    0175 04567000
*                                                            @YM01896*/ 04568000
         ST    @10,SRBPTCB(,@14)                                   0175 04569000
*   SRBPASID=ASCBASID;              /* FROM THE CURRENT ASCB         */ 04570000
         L     @10,ASCBPTR                                         0176 04571000
         LH    @10,ASCBASID(,@10)                                  0176 04572000
         N     @10,@CF02334                                        0176 04573000
         STH   @10,SRBPASID(,@14)                                  0176 04574000
*   AFGMSRB=OFF;                    /* THE SRB IS COMPLETE           */ 04575000
*                                                                  0178 04576000
*   /*****************************************************************/ 04577000
*   /*                                                               */ 04578000
*   /* THIS SECTION OF CODE OBTAINS FROM FIXED COMMON SQA, ADDRESS   */ 04579000
*   /* SPACE FOR THE GTFBLOK. SHOULD THE REAL CORE SIZE BE 768K OR   */ 04580000
*   /* LESS, ONE LESS GTFBLOK IS GOTTEN. THE GTFBLOK ARE INITIALIZED,*/ 04581000
*   /* CHAINED TOGETHER, AND THE CURRENT ONE IS TIME STAMPED TO MAKE */ 04582000
*   /* IT READY FOR USE. SHOULD A GETMAIN FAIL THE INSUFFICIENT SQA  */ 04583000
*   /* MESSAGE IS WRITTEN THEN THE TRACE WRITER IS TERMINATED        */ 04584000
*   /*                                                               */ 04585000
*   /*****************************************************************/ 04586000
*                                                                  0178 04587000
*/* P SET AFGMBLOK                                                   */ 04588000
*/* D (YES,,NO,%GETBLOK) IS THIS A SMALL REAL STORAGE                */ 04589000
*/* P CHANGE NUMBER OF GTFBLOK TO BE GOTTEN FROM NORMAL TO 3         */ 04590000
*/*%GETBLOK: D (NO,,YES,%BLKINIT) HAVE ENOUGH GTFBLOK BEEN GOTTEN    */ 04591000
*/* P INCREMENT BLOKINDX BY 1                                        */ 04592000
