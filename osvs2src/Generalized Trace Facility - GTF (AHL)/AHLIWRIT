*/* OPTIONAL DIAGNOSTIC AID                                          */ 05867000
*                                                                  0466 05868000
*       CALL FREQLOCK;              /* RELEASE THE QLOCK             */ 05869000
         BAL   @14,FREQLOCK                                        0466 05870000
*     END;                                                         0467 05871000
*   AFPGRLSE=OFF;                   /* TURN OFF THE AUDIT FLAG FOR 0468 05872000
*                                      THIS SECTION                  */ 05873000
@RF00440 DS    0H                                                  0469 05874000
*                                                                  0469 05875000
*   /*****************************************************************/ 05876000
*   /*                                                               */ 05877000
*   /* THIS SECTION OF CODE CHECKS TO SEE IF THE NUMBER OF AVAILABLE */ 05878000
*   /* GTFBCB (GTFBUFR) ON THE EMPTY QUEUE IS LESS THAN THE DESIRED  */ 05879000
*   /* MINIMUM RESERVE AND GETS MORE GTFBUFR IF POSSIBLE             */ 05880000
*   /*                                                               */ 05881000
*   /*****************************************************************/ 05882000
*                                                                  0469 05883000
*/* P SET AFEQFIX                                                    */ 05884000
*/* P BCBHOLDR= EQHEAD, I=1                                          */ 05885000
*/*%ET: D (YES,,NO,%ELOP) BCBHOLDR=0                                 */ 05886000
*/* N NO REPAIRS DURING TERMINATION OR IN THE CASE OF AN ERROR       */ 05887000
*/* D (YES,XWRITEST,NO,) IS WSERFLAG SET                             */ 05888000
*/* N DIAGNOSTIC MESSAGE: "EQ<MIN", EQLENGTH, & WQLENGTH             */ 05889000
*/* P DIAGNOSTIC: INCREMENT EQSHORT BY 1                             */ 05890000
*/* S EBUFGET: GET A STRING OF GTFBCB & GTFBUFR                      */ 05891000
*/* D (YES,EQAFOFF,NO,) GQHEAD=0                                     */ 05892000
*/* S GETQLOCK: OBTAIN THE GTFBCB QUEUE LOCK                         */ 05893000
*/* P GQTAIL-> EQCHAIN=EQHEAD & EQHEAD=GQHEAD                        */ 05894000
*/* D (YES,,NO,%DAG3) DIAGNOSTIC: EQLENGTH< EQMIN                    */ 05895000
*/* P DIAGNOSTIC: EQMIN=EQLENGTH                                     */ 05896000
*/*%DAG3: P DIAGNOSTIC: INCR EQLENGTH BY THE NUM OF GTFBCB IN 1 PAGE */ 05897000
*/* S (,EQAFOFF) FREQLOCK: RELEASE THE GTFBCB QUEUE LOCK             */ 05898000
*/*%ELOP: P BCBHOLDR= BCBHOLDR-> EQCHAIN, I=I+1                      */ 05899000
*/* D (YES,EQAFOFF,NO,%ET) I=EQRESERV                                */ 05900000
*/*EQAFOFF: P CLEAR AFEQFIX                                          */ 05901000
*                                                                  0469 05902000
*   AFEQFIX=ON;                     /* SET THE AUDIT FLAG FOR THIS 0469 05903000
*                                      SECTION OF CODE               */ 05904000
         OI    AFEQFIX(PCTPTR),B'00000010'                         0469 05905000
         NI    AFPGRLSE(PCTPTR),B'11111011'                        0469 05906000
*   BCBHOLDR=EQHEAD;                /* THIS LOOP CHECKS TO BE SURE 0470 05907000
*                                      THERE IS AT LEAST A MINIMUM 0470 05908000
*                                      NUMBER OF GTFBCB ON THE EMPTY    05909000
*                                      QUEUE                         */ 05910000
