*   EQMIN=EQLENGTH;                 /* INIT EQMIN                    */ 04818000
         ST    @09,EQMIN(,PCTPTR)                                  0238 04819000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 04820000
*                                                                  0239 04821000
*   ;                                                              0239 04822000
*                                                                  0240 04823000
*   /*****************************************************************/ 04824000
*   /*                                                               */ 04825000
*   /* TRANSFER GTFBCB FROM THE EMPTY QUEUE TO THE HISTORY QUEUE AND */ 04826000
*   /* INITIALIZE THE ASSOCIATED GTFBUFR BY COPYING THE CURRENT      */ 04827000
*   /* GTFBLOK INTO THEM                                             */ 04828000
*   /*                                                               */ 04829000
*   /*****************************************************************/ 04830000
*                                                                  0240 04831000
*/* N THIS LOOP SETS UP THE HISTORY QUEUE                            */ 04832000
*/* P HQHEAD=EQHEAD                                                  */ 04833000
*/*%CX: D (YES,%CX1,NO,) HAS HQLENGTH BEEN REACHED                   */ 04834000
*/* P QTAIL=EQHEAD                                                   */ 04835000
*/* P EQHEAD= EQHEAD-> EQCHAIN                                       */ 04836000
*/* P DIAGNOSTIC: DECREMENT EQLENGTH BY 1                            */ 04837000
*/* P DIAGNOSTIC QTAIL-> EQFLAG=OFF & HQFLAG=ON                      */ 04838000
*/* N EQCHAIN & HQCHAIN BOTH REFER TO THE SAME GTFBCB FIELD          */ 04839000
*/* P QTAIL-> GBUFRPTR-> GTFBUFR= CURRBLOK-> GTFBLOK                 */ 04840000
*/* D (YES,,NO,%CX) EQHEAD-> EQCHAIN=0                               */ 04841000
*/* S EBUFGET: GET A STRING OF GTFBCB & GTFBUFR                      */ 04842000
*/* D (YES,,NO,%X2) GQHEAD=0                                         */ 04843000
*/* S (,CLEANUP) WTO: AHL032I                                        */ 04844000
*/*%X2: P EQHEAD=GQHEAD                                              */ 04845000
*/* P (,%CX) DIAGNOSTIC: INCR EQLENGTH BY NUMBER OF GTFBCB IN 1 PAGE */ 04846000
*/*%CX1: P QTAIL-> HQCHAIN=0                                         */ 04847000
*                                                                  0240 04848000
*   HQHEAD=EQHEAD;                  /* REMOVE A GROUP OF GTFBCB FROM    04849000
*                                      THE HEAD OF THE EMPTY QUEUE TO   04850000
*                                      GO ON THE HISTORY QUEUE       */ 04851000
         ST    @10,HQHEAD(,PCTPTR)                                 0240 04852000
*   DO I=1 TO HQLENGTH;             /* GO THROUGH EACH GTFBCB TO BE     04853000
*                                      TRANSFERED                    */ 04854000
         LA    @10,1                                               0241 04855000
         B     @DE00241                                            0241 04856000
@DL00241 DS    0H                                                  0242 04857000
*     QTAIL=EQHEAD;                 /* QTAIL IS TO POINT TO THE LAST    04858000
*                                      GTFBCB ON THE HISTORY QUEUE   */ 04859000
         L     @10,EQHEAD(,PCTPTR)                                 0242 04860000
         ST    @10,QTAIL(,PCTPTR)                                  0242 04861000
*     EQHEAD=EQHEAD->EQCHAIN;       /* REMOVE THIS GTFBCB FROM THE 0243 04862000
*                                      EMPTY QUEUE                   */ 04863000
         L     @09,EQCHAIN(,@10)                                   0243 04864000
         ST    @09,EQHEAD(,PCTPTR)                                 0243 04865000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 04866000
*                                                                  0244 04867000
*     EQLENGTH=EQLENGTH-1;          /* UPDATE COUNT OF GTFBCB ON THE    04868000
*                                      EMPTY QUEUE                   */ 04869000
         L     @15,EQLENGTH(,PCTPTR)                               0244 04870000
         BCTR  @15,0                                               0244 04871000
         ST    @15,EQLENGTH(,PCTPTR)                               0244 04872000
*     QTAIL->EQFLAG=OFF;                                           0245 04873000
*     QTAIL->HQFLAG=ON;                                            0246 04874000
*                                                                  0246 04875000
         OI    HQFLAG(@10),B'00001000'                             0246 04876000
         NI    EQFLAG(@10),B'01111111'                             0246 04877000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 04878000
*                                                                  0247 04879000
*     QTAIL->GBUFRPTR->GTFBUFR(1:LENGTH(RECMAP)+4)=CURRBLOK->BLOKAREA(1 04880000
*         :LENGTH(RECMAP)+4);       /* BECAUSE ANY GTFBCB ON THE   0247 04881000
*                                      HISTORY QUEUE COULD BE PUT ON    04882000
*                                      AN SLE THE ASSOCIATED GTFBUFR    04883000
*                                      MUST BE OF VALID GTF FORMAT.     04884000
*                                      THE CURRENT GTFBLOK IS OF   0247 04885000
*                                      VALID FORMAT SO IT IS COPIED     04886000
*                                      INTO EACH GTFBUFR             */ 04887000
         L     @10,GBUFRPTR(,@10)                                  0247 04888000
