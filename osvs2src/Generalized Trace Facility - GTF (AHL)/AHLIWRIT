@DL00212 DS    0H                                                  0213 04717000
*     NEXTBLOK=BLOKLIST(I);                                        0213 04718000
         LR    @09,@10                                             0213 04719000
         SLA   @09,2                                               0213 04720000
         L     @09,BLOKLIST-4(@09)                                 0213 04721000
         ST    @09,NEXTBLOK(,PCTPTR)                               0213 04722000
*     CURRBLOK->GTFBLOK(1:LENGTH(BLOKINIT))=BLOKINIT;              0214 04723000
         L     @15,CURRBLOK(,PCTPTR)                               0214 04724000
         MVC   GTFBLOK(38,@15),BLOKINIT                            0214 04725000
*     CURRBLOK->BLKCHAIN=NEXTBLOK;                                 0215 04726000
         ST    @09,BLKCHAIN(,@15)                                  0215 04727000
*     CURRBLOK=NEXTBLOK;                                           0216 04728000
         ST    @09,CURRBLOK(,PCTPTR)                               0216 04729000
*   END;                                                           0217 04730000
         AL    @10,@CF00070                                        0217 04731000
@DE00212 ST    @10,I                                               0217 04732000
         C     @10,BLOKINDX                                        0217 04733000
         BNH   @DL00212                                            0217 04734000
*   RFY                                                            0218 04735000
*     CLOCKREG RSTD;                                               0218 04736000
*   CLOCKREG=ADDR(CURRBLOK->RECTOD);                               0219 04737000
         L     CLOCKREG,CURRBLOK(,PCTPTR)                          0219 04738000
         LA    CLOCKREG,RECTOD(,CLOCKREG)                          0219 04739000
*   GEN CODE(STCK  0(CLOCKREG))                                    0220 04740000
*   REFS(CLOCKREG) SETS(RECTOD) NOFLOWS NOEXIT SEQFLOW NODEFS;/*   0220 04741000
*                                      TIMESTAMP THE CURRENT GFTBLOK    04742000
*                                      TO PREPARE IT FOR USE         */ 04743000
         STCK  0(CLOCKREG)                                              04744000
*   RFY                                                            0221 04745000
*     CLOCKREG UNRSTD;                                             0221 04746000
*   CURRBLOK->BLOKUSE=0;            /* SET THE USE COUNT TO 0 TO   0222 04747000
*                                      INDICATE THAT ALL THE RESERVED   04748000
*                                      SPACE HAS BEEN FILLED         */ 04749000
         L     @10,CURRBLOK(,PCTPTR)                               0222 04750000
         SLR   @09,@09                                             0222 04751000
         STH   @09,BLOKUSE(,@10)                                   0222 04752000
*   AFGMBLOK=OFF;                   /* TURN OFF AUDIT FLAG FOR THIS     04753000
*                                      SECTION                       */ 04754000
         NI    AFGMBLOK(PCTPTR),B'11111110'                        0223 04755000
*                                                                  0224 04756000
*   /*****************************************************************/ 04757000
*   /*                                                               */ 04758000
*   /* THIS SECTION OF CODE INITIAIZES THE EMPTY AND HISTORY QUEUES  */ 04759000
*   /* AND THE POINTERS TO THEM                                      */ 04760000
*   /*                                                               */ 04761000
*   /*****************************************************************/ 04762000
*                                                                  0224 04763000
*/* S EBUFGET: GET A STRING OF GTFBCB & GTFBUFR                      */ 04764000
*/* D (YES,,NO,%X1) GQHEAD=0                                         */ 04765000
*/* S (,CLEANUP) WTO: AHL032I                                        */ 04766000
*/*%X1: P EQHEAD=GQHEAD                                              */ 04767000
*/* P DIAGNOSTIC: EQLENGTH=THE NUMBER OF GTFBCB THAT FIT IN 1 PAGE   */ 04768000
*/* P DIAGNOSTIC: EQMIN=EQLENGTH                                     */ 04769000
*                                                                  0224 04770000
*   ;                                                              0224 04771000
*                                                                  0224 04772000
*   /*****************************************************************/ 04773000
*   /*                                                               */ 04774000
*   /* INTERNAL SUBROUTINE EBUFGET WILL RETURN A STRING OF GTFBCB &  */ 04775000
*   /* GTFBUFR READY TO BE PLACED ON THE EMPTY QUEUE. THE QUANTITY OF*/ 04776000
*   /* GTFBCB IN THE STRING IS EQUAL TO 1 PAGE DIVIDED BY THE LENGTH */ 04777000
*   /* OF A GTFBCB. ALL THE GTFBCB WILL BE ON ONE PAGE. GQHEAD WILL  */ 04778000
*   /* CONTAIN THE ADDRESS OF THE FIRST GTFBCB, GQTAIL WILL POINT TO */ 04779000
*   /* THE LAST GTFBCB. IF A GETMAIN FAILS THE INSUFFICIENT ADDRESS  */ 04780000
*   /* SPACE MSG WILL BE WRITTEN AND GQHEAD WILL BE 0                */ 04781000
*   /*                                                               */ 04782000
*   /*****************************************************************/ 04783000
*                                                                  0225 04784000
*   CALL EBUFGET;                                                  0225 04785000
         BAL   @14,EBUFGET                                         0225 04786000
*   IF GQHEAD=0 THEN                                               0226 04787000
         L     @10,GQHEAD                                          0226 04788000
         LTR   @10,@10                                             0226 04789000
         BNZ   @RF00226                                            0226 04790000
*     DO;                           /* IF GQHEAD IS 0 EBUFGET FAILED    04791000
*                                      AND THE TRACE WRITER HAS TO BE   04792000
*                                      TERMINATED                    */ 04793000
*       DO;                         /* WTOP MF(E,AHL032)             */ 04794000
*         RFY                                                      0229 04795000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 04796000
*         GPR01P=ADDR(AHL032);      /* ADDRESS CONTROL PROGRAM LIST  */ 04797000
         L     GPR01P,@CA01942                                     0230 04798000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 04799000
         SVC   35                                                  0231 04800000
*         RFY                                                      0232 04801000
*           GPR01P UNRSTD;          /* RELEASE REG 1                 */ 04802000
*       END;                        /* WTOP MF(E,AHL032)             */ 04803000
*       GO TO CLEANUP;                                             0234 04804000
         B     CLEANUP                                             0234 04805000
*     END;                                                         0235 04806000
*   EQHEAD=GQHEAD;                  /* PUT THE STRING OF GTFBCB ON 0236 04807000
*                                      THE EMPTY QUEUE               */ 04808000
@RF00226 L     @10,GQHEAD                                          0236 04809000
         ST    @10,EQHEAD(,PCTPTR)                                 0236 04810000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 04811000
*                                                                  0237 04812000
*   EQLENGTH=PGBYTES/LENGTH(GTFBCB);/* INITIALIZE THE COUNT OF     0237 04813000
*                                      AVAILABLE GTFBCB ON THE EMPTY    04814000
*                                      QUEUE                         */ 04815000
         LA    @09,170                                             0237 04816000
