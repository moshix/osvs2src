*         GPR01P=ADDR(I004);        /* REG1 POINTS TO LIST           */ 06262000
         LA    GPR01P,I004                                         0541 06263000
*         SVC(4);                   /* STANDARD-FORM OF GETMAIN      */ 06264000
         SVC   4                                                   0542 06265000
*         RESPECIFY                                                0543 06266000
*          (GPR01P) UNRESTRICTED;                                  0543 06267000
*       END;                        /* GETMAIN (EC)                0544 06268000
*                                      LV(LENGTH(GTFBLOK)) A(GETWORD)   06269000
