*             GPR01F=ADDR(GETWORD->GTFBLOK);/* SET REGISTER ONE      */ 06459000
         L     GPR01F,GETWORD                                      0590 06460000
*             SVC(10);              /* R-FORM OF GETMAIN             */ 06461000
         SVC   10                                                  0591 06462000
*             RESPECIFY                                            0592 06463000
*              (GPR01F,                                            0592 06464000
*               GPR00F) UNRESTRICTED;                              0592 06465000
*           END;                    /* FREEMAIN(R) LV(LENGTH(GTFBLOK)   06466000
*                                      )SP(SQASP)A(GETWORD->GTFBLOK) */ 06467000
*/* FREE THE NEW GTFBLOK                                             */ 06468000
*                                                                  0594 06469000
*           GO TO FIXMSG;           /* GO WRITE OUT THE FIX ATTEMPTED   06470000
*                                      MSG                           */ 06471000
         B     FIXMSG                                              0594 06472000
*         END;                                                     0595 06473000
*                                                                  0596 06474000
*       /*************************************************************/ 06475000
*       /*                                                           */ 06476000
*       /* NOW THAT ALL IS READY SWAP THE NEW GTFBLOK FOR THE BAD ONE*/ 06477000
*       /*                                                           */ 06478000
*       /*************************************************************/ 06479000
*                                                                  0596 06480000
*/*FIXIT: P PUT PTR TO NEW GTFBLOK IN CURRBLOK-> BLKCHAIN            */ 06481000
*/* P PUT NEXTBLOK-> BLKCHAIN INTO NEW GTFBLOK BLKCHAIN              */ 06482000
*/* P PUT OLD GTFBLOK TOD VALUE INTO NEW GTFBLOK                     */ 06483000
