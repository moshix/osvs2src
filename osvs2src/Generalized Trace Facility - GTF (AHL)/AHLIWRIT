*                                      FROM THE RELEASE QUEUE AND PUT   04289000
*                                      ON THE EMPTY QUEUE BY THE   0101 04290000
*                                      TRACE WRITER IN EACH PASS.  0101 04291000
*                                      THESE SUBTOTALS ARE THEN    0101 04292000
*                                      COMBINED INTO RQTOTAL         */ 04293000
*   DCL                                                            0102 04294000
*     1 LOSTBLOK,                   /* THIS IS A PROTOTYPE LOST    0102 04295000
*                                      GTFBLOK RECORD                */ 04296000
*      2 * CHAR(6) INIT('001200000003'X),/* AID=00 FID=03            */ 04297000
*      2 LBTZ CHAR(4),                                             0102 04298000
*      2 LBTIME CHAR(8);                                           0102 04299000
*   DCL                                                            0103 04300000
*     1 PDQASTCB,                   /* PARAMETERS FOR THE PURGEDQ  0103 04301000
*                                      MACRO                         */ 04302000
*      2 * PTR(16) INIT('0'),                                      0103 04303000
*      2 PDQASID FIXED(15),                                        0103 04304000
*      2 PDQTCB PTR(31),                                           0103 04305000
*     PDQRMTR PTR(31);                                             0103 04306000
*   DCL                                                            0104 04307000
*     WORKAREA CHAR(8) BDY(WORD);   /* THIS IS USED ALONG WITH A   0104 04308000
*                                      PORTION OF THE GTFBCB MAPPING    04309000
*                                      FOR DOING CS                  */ 04310000
*   RFY                                                            0105 04311000
*     CMPWKA BASED(ADDR(WORKAREA));                                0105 04312000
*   DCL                                                            0106 04313000
*     SBLOKSAV PTR(31) INIT(0),                                    0106 04314000
*     EPCTPTR PTR(31) STATIC;       /* SAVEAREA FOR THE GTFPCT ADDR     04315000
*                                      FOR REFRESH OF PCTPTR IN THE     04316000
*                                      ESTAE ROUTINE                 */ 04317000
*   DCL                                                            0107 04318000
*     PGADDR PTR(31),               /* USED IN SUBROUTINE PGLSTIN    */ 04319000
*     ASTYPE BIT(8),                                               0107 04320000
*     ITEM CHAR(1),                                                0107 04321000
*     ILENGTH PTR(31);                                             0107 04322000
*                                                                  0108 04323000
*   /*****************************************************************/ 04324000
*   /*                                                               */ 04325000
*   /* THIS SEGMENT CONTAINS THE CODE FOR INITIALIZATION OF THE TRACE*/ 04326000
*   /* WRITERS PLUS AHLSBLOK, AHLSFEOB, AHLSBUF, AND AHLREADR        */ 04327000
*   /*                                                               */ 04328000
*   /*****************************************************************/ 04329000
*                                                                  0108 04330000
*/* P GET THE GTFPCT PTR FROM THE MCQE                               */ 04331000
*/* P EPCTPTR=PCTPTR SAVE THE GTFPCT ADDR FOR AHLWSTAE               */ 04332000
*/* S MODESET: SUPER STATE                                           */ 04333000
*                                                                  0108 04334000
*   PCTPTR=PARAMETR->MCQEAT;        /* INIT GTFPCT BASE FROM THE MCQE   04335000
*                                      PASSED BY THE MOTHER TASK     */ 04336000
         L     PCTPTR,MCQEAT(,PARAMETR)                            0108 04337000
*   RFY                                                            0109 04338000
*     PARAMETR UNRSTD;                                             0109 04339000
*   RFY                                                            0110 04340000
*     MCQE BASED(MCQEPTR);          /* THE MCQE CAN NOW BE LOCATED 0110 04341000
*                                      VIA THE GTFPCT                */ 04342000
*   EPCTPTR=PCTPTR;                 /* SAVE THE GTFPCT ADDRESS FOR 0111 04343000
*                                      POSSIBLE USE BY THE ESTAE   0111 04344000
*                                      ROUTINE AHLWSTAE              */ 04345000
         ST    PCTPTR,EPCTPTR                                      0111 04346000
*   DO;                             /* MODESET MODE(SUP)             */ 04347000
*     I10701='00000000'B;           /* OR MASK                       */ 04348000
         MVI   I10701,B'00000000'                                  0113 04349000
*     I10702='00000000'B;           /* AND MASK                      */ 04350000
         MVI   I10702,B'00000000'                                  0114 04351000
*     I10703='00000000'B;           /* RESERVED                      */ 04352000
         MVI   I10703,B'00000000'                                  0115 04353000
*     I10704='00001100'B;                                          0116 04354000
         MVI   I10704,B'00001100'                                  0116 04355000
*     RESPECIFY                                                    0117 04356000
*       GPR01F RESTRICTED;                                         0117 04357000
*     GPR01F=I107;                  /* MODESET PARMLIST              */ 04358000
         L     GPR01F,I107                                         0118 04359000
*     SVC(107);                     /* ISSUE MODESET SVC             */ 04360000
         SVC   107                                                 0119 04361000
*     RESPECIFY                                                    0120 04362000
*      (GPR01F) UNRESTRICTED;                                      0120 04363000
*   END;                            /* MODESET MODE(SUP) CHANGE TO 0121 04364000
*                                      SUPER STATE AS WELL AS KEY 0  */ 04365000
*                                                                  0121 04366000
*   /*****************************************************************/ 04367000
*   /*                                                               */ 04368000
*   /* THIS ESTABLISHES AN ESTAE ENVIRONMENT WHICH WILL REMAIN IN    */ 04369000
*   /* EFFECT UNTIL JUST BEFORE TERMINATION OF THE TRACE WRITER      */ 04370000
*   /*                                                               */ 04371000
*   /*****************************************************************/ 04372000
*                                                                  0122 04373000
*/* S ESTAE: ESTABLISH AHLWSTAE AS THE ESTAE E.P.                    */ 04374000
*/* D (OK,%EQK,BAD,) RC=0 ESTAE OK                                   */ 04375000
*/* S WTO: MESSAGE AHL037I                                           */ 04376000
*/* R RETURN                                                         */ 04377000
*/*%EQK: P (INT,,TAPE,%DL10,D.A.,%DL20) SET AFINIT                   */ 04378000
*/* P (,%DL30) SET AFIWRIT                                           */ 04379000
*/*%DL10: P (,%DL30) SET AFCWRIT                                     */ 04380000
*/*%DL20: P SET AFWWRIT                                              */ 04381000
*/*%DL30: P DIAGNOSTIC: WTRDLST= ADDR(SDUMPLIST)                     */ 04382000
*/* N SDUMPLST HAS THE WRITER MODULE CODE & DATA AREAS AS ITS FIRST     04383000
*/*ENTRY                                                             */ 04384000
*                                                                  0122 04385000
*   RFY                                                            0122 04386000
*    (MACREG1,                                                     0122 04387000
*     RETCODE) RSTD;                                               0122 04388000
*   MACREG1=ADDR(GTFPCT);           /* THIS IS DONE TO GUARANTEE THAT   04389000
*                                      THE ADDRESS OF THE GTFPCT IS     04390000
*                                      IN A REG FOR USE IN THE ESTAE    04391000
*                                      MACRO                         */ 04392000
         LR    MACREG1,PCTPTR                                      0123 04393000
*   GEN CODE(ESTAE   AHLWSTAE,CT,PARAM=(MACREG1),ASYNCH=YES,RECORD=YES) 04394000
*   REFS(PCTPTR,MACREG1) SETS(RETCODE) NOFLOWS NODEFS SEQFLOW;     0124 04395000
         ESTAE   AHLWSTAE,CT,PARAM=(MACREG1),ASYNCH=YES,RECORD=YES      04396000
*   IF RETCODE^=0 THEN                                             0125 04397000
         LTR   RETCODE,RETCODE                                     0125 04398000
         BZ    @RF00125                                            0125 04399000
*     DO;                           /* CONTINUE ONLY IF THE ESTAE  0126 04400000
*                                      RETURN CODE IS 0, TOTALLY   0126 04401000
*                                      GOOD. OTHERWISE TERMINATE     */ 04402000
*       RFY                                                        0127 04403000
*        (RETCODE,                                                 0127 04404000
*         MACREG1) UNRSTD;                                         0127 04405000
*       DO;                         /* WTOP MF(E,AHL037)             */ 04406000
*         RFY                                                      0129 04407000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 04408000
*         GPR01P=ADDR(AHL037);      /* ADDRESS CONTROL PROGRAM LIST  */ 04409000
         L     GPR01P,@CA01947                                     0130 04410000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 04411000
         SVC   35                                                  0131 04412000
*         RFY                                                      0132 04413000
*           GPR01P UNRSTD;          /* RELEASE REG 1                 */ 04414000
*       END;                        /* WTOP MF(E,AHL037) WRITE OUT 0133 04415000
*                                      THE MESSAGE FOR ESTAE FAILURE */ 04416000
*       RETURN;                     /* TERMINATE THE TRACE WRITER    */ 04417000
@EL00001 L     @13,4(,@13)                                         0134 04418000
@EF00001 DS    0H                                                  0134 04419000
@ER00001 LM    @14,@12,12(@13)                                     0134 04420000
         BR    @14                                                 0134 04421000
*     END;                                                         0135 04422000
*   AFINIT=ON;                      /* SET THE AUDIT FLAG TO SHOW  0136 04423000
*                                      INITIALIZATION IN PROGRESS    */ 04424000
@RF00125 DS    0H                                                  0137 04425000
*   AFIWRIT=ON;                     /* SET THE AUDIT FLAG TO INDICATE   04426000
*                                      THE INTERNAL TRACE WRITER HAS    04427000
*                                      BEEN ATTACHED                 */ 04428000
         OI    AFINIT(PCTPTR),B'10010000'                          0137 04429000
*   WRTDLST=ADDR(SDUMPMAC);         /* DIAGNOSTIC: PUT THE ADDRESS OF   04430000
*                                      THE SDUMP MACRO BUILT BY THE     04431000
*                                      TRACE WRITER INTO THE GTFPCT     04432000
*                                      FOR USE BY AHLSFRR, AHLSBFRR,    04433000
*                                      & AHLRDFRR                    */ 04434000
         LA    @10,SDUMPMAC                                        0138 04435000
         ST    @10,WRTDLST(,PCTPTR)                                0138 04436000
*                                                                  0139 04437000
*   /*****************************************************************/ 04438000
*   /*                                                               */ 04439000
*   /* THIS SUBROUTINE WILL GETMAIN ONE PAGE OF ADDRESS SPACE SP=7   */ 04440000
*   /* AND DIVIDE IT INTO AS MANY SLE AS POSSIBLE. THE SLE WILL BE   */ 04441000
*   /* INITIALIZED AND CHAINED TOGETHER. GQHEAD WILL POINT TO THE    */ 04442000
*   /* FIRST SLE IN THE CHAIN AND GQTAIL WILL POINT TO THE LAST.     */ 04443000
*   /* SHOULD THE GETMAIN FAIL AHL032I WILL BE WRITTEN AND GQHEAD    */ 04444000
*   /* WILL BE 0                                                     */ 04445000
*   /*                                                               */ 04446000
*   /*****************************************************************/ 04447000
*                                                                  0139 04448000
*/* S SLEGET: GET THE PAGE OF SLE                                    */ 04449000
*/* D (YES,CLEANUP,NO,) GQHEAD=0                                     */ 04450000
*/* P SLQHEAD=GQHEAD                                                 */ 04451000
*                                                                  0139 04452000
*   CALL SLEGET;                                                   0139 04453000
         BAL   @14,SLEGET                                          0139 04454000
*   IF GQHEAD=0 THEN                                               0140 04455000
         L     @10,GQHEAD                                          0140 04456000
         LTR   @10,@10                                             0140 04457000
         BZ    @RT00140                                            0140 04458000
*     GO TO CLEANUP;                /* IF GQHEAD IS 0 THE SLE GETMAIN   04459000
*                                      FAILED. TERMINATE             */ 04460000
*   SLQHEAD=GQHEAD;                 /* PUT THE STRING OF SLE ONTO THE   04461000
*                                      SLE QUEUE, PREVIOUSLY EMPTY   */ 04462000
         ST    @10,SLQHEAD(,PCTPTR)                                0142 04463000
*                                                                  0143 04464000
*   /*****************************************************************/ 04465000
*   /*                                                               */ 04466000
*   /* HERE ADDRESS SPACE IS GOTTEN FROM FIXED COMMON SQA FOR USE AS */ 04467000
*   /* AN SRB TO SCHEDULE AHLSBUF. IT IS COMPLETELY INITIALIZED      */ 04468000
*   /* EXCEPT FOR SRBEP & SRBRMTR WHICH ARE FILLED IN AFTER AHLSBUF  */ 04469000
*   /* IS LOADED AN ADDITIONAL 8 BYTES IS GOTTEN FOR AHLSBUF TO USE  */ 04470000
*   /* AS AS A WORKAREA (SEE THE SRB MAPPING)                        */ 04471000
*   /*                                                               */ 04472000
*   /*****************************************************************/ 04473000
*                                                                  0143 04474000
*/* P SET AFGMSRB                                                    */ 04475000
*/* S GETMAIN: GET FROM SQA AN SRB & WORKAREA FOR AHLSBUF            */ 04476000
*/* D (BAD,,OK,%CC) RC?                                              */ 04477000
*/* S (,CLEANUP) WTO: MESSAGE AHL033I                                */ 04478000
*/*%CC: P SAVE SRB ADDRESS IN INTERNAL SRBPTR WITH AVAIL BIT ON      */ 04479000
*                                                                  0143 04480000
*   AFGMSRB=ON;                     /* TURN ON THE AUDIT FLAG FOR  0143 04481000
*                                      THIS SECTION OF CODE          */ 04482000
         OI    AFGMSRB(PCTPTR),B'00000010'                         0143 04483000
*   RFY                                                            0144 04484000
*    (RETCODE) RSTD;                                               0144 04485000
*   DO;                             /* GETMAIN (EC)                0145 04486000
*                                      LV(LENGTH(SRBSECT)) A(GETWORD)   04487000
*                                      SP(SQASP)                     */ 04488000
*     RESPECIFY                                                    0146 04489000
*      (GPR01P) RESTRICTED;                                        0146 04490000
*     I004=I004&&I004;              /* INIT LIST                     */ 04491000
         XC    I004(10),I004                                       0147 04492000
*     I00404='20'X;                 /* TYPE                          */ 04493000
         MVI   I00404,X'20'                                        0148 04494000
*     I00401=LENGTH(SRBSECT);       /* LENGTH                        */ 04495000
         MVC   I00401(4),@CF01936                                  0149 04496000
*     I00403=ADDR(GETWORD);         /* ADDR OF ADDR LIST             */ 04497000
         LA    @14,GETWORD                                         0150 04498000
         STCM  @14,7,I00403                                        0150 04499000
*     I00405=SQASP;                 /* SUBPOOL VALUE                 */ 04500000
         MVI   I00405,X'F5'                                        0151 04501000
*     GPR01P=ADDR(I004);            /* REG1 POINTS TO LIST           */ 04502000
         LA    GPR01P,I004                                         0152 04503000
*     SVC(4);                       /* STANDARD-FORM OF GETMAIN      */ 04504000
         SVC   4                                                   0153 04505000
*     RESPECIFY                                                    0154 04506000
*      (GPR01P) UNRESTRICTED;                                      0154 04507000
*   END;                            /* GETMAIN (EC)                0155 04508000
*                                      LV(LENGTH(SRBSECT)) A(GETWORD)   04509000
*                                      SP(SQASP)                     */ 04510000
*   IF RETCODE^=0 THEN                                             0156 04511000
         LTR   RETCODE,RETCODE                                     0156 04512000
         BZ    @RF00156                                            0156 04513000
*     DO;                           /* CHECK THE RETURN CODE AND IF     04514000
*                                      IT IS NONZERO ISSUE THE     0157 04515000
