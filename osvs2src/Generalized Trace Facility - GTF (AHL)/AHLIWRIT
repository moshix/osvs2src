         LTR   @10,@10                                             0917 07645000
         BNZ   @RF00917                                            0917 07646000
*     DO;                           /* IF THE END OF THE SLE QUEUE 0918 07647000
*                                      HAS BEEN REACHED THEN THERE IS   07648000
*                                      NO MATCHING SLE               */ 07649000
*       AHLRDPX2=NOSLEMAT;          /* SET A RETURN CODE TO INDICATE    07650000
*                                      THE FAILURE                   */ 07651000
         MVC   AHLRDPX2(4,AHLRDPTR),@CF00039                       0919 07652000
*       GO TO POSTEXIT;             /* THEN GO POST THE REQUESTOR    */ 07653000
         B     POSTEXIT                                            0920 07654000
*     END;                                                         0921 07655000
*   IF SLEDUMP^=AHLRDDID            /* DOES THIS SLE MATCH THE ID  0922 07656000
