*   /* FREEMAIN THE SQA FOR ANY GTFBLOK THAT HAVE BEEN GOTTEN        */ 08658000
*   /*                                                               */ 08659000
*   /*****************************************************************/ 08660000
*                                                                  1151 08661000
*/*%IS1: P START LOOP AT BEGINNING OF BLOKLIST I=0                   */ 08662000
*/*%FLOOP: D (YES,%BFRET,NO,) IS THIS THE END OF BLOKLIST            */ 08663000
*/* D (YES,%ADVAN,NO,) IS THIS BLOKLIST ENTRY 0                      */ 08664000
*/* N WHEN TRMWRIT IS POSTED ALL USE OF GTFBLOK BY FGBR WILL BE    1151 08665000
*/*COMPLETE                                                          */ 08666000
*/* S FREEMAIN: BLOKLIST(I)-> GTFBLOK                                */ 08667000
*/* P BLOKLIST(I)=0                                                  */ 08668000
*/*%ADVAN: P (,%FLOOP) ADVANCE TO THE NEXT BLOKLIST ENTRY I=I+1      */ 08669000
*/*%BFRET: P BLOKINDX=0                                              */ 08670000
*/* R RETURN                                                         */ 08671000
*/*SQAPGFRE: END                                                     */ 08672000
*                                                                  1151 08673000
*   DO I=1 TO BLOKINDX;                                            1151 08674000
@RF01136 LA    @10,1                                               1151 08675000
         B     @DE01151                                            1151 08676000
@DL01151 DS    0H                                                  1152 08677000
*     IF BLOKLIST(I)^=0 THEN                                       1152 08678000
         SLR   @09,@09                                             1152 08679000
         SLA   @10,2                                               1152 08680000
         L     @01,BLOKLIST-4(@10)                                 1152 08681000
         CR    @01,@09                                             1152 08682000
         BE    @RF01152                                            1152 08683000
*       DO;                                                        1153 08684000
*         GETWORD=BLOKLIST(I);                                     1154 08685000
         ST    @01,GETWORD                                         1154 08686000
*         BLOKLIST(I)=0;                                           1155 08687000
         ST    @09,BLOKLIST-4(@10)                                 1155 08688000
*         DO;                       /* FREEMAIN(R) LV(LENGTH(GTFBLOK)   08689000
*                                      )SP(SQASP)A(GETWORD->GTFBLOK) */ 08690000
*           RESPECIFY                                              1157 08691000
*            (GPR01F,                                              1157 08692000
*             GPR00F) RESTRICTED;                                  1157 08693000
*           GPR00F=LENGTH(GTFBLOK); /* LENGTH REQUESTED              */ 08694000
         L     GPR00F,@CF02311                                     1158 08695000
*           GPR01F=SQASP;           /* SP IN REG 1                   */ 08696000
         LA    GPR01F,245                                          1159 08697000
*           GEN(SLL   GPR01F,24)                                   1160 08698000
*           NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;           1160 08699000
         SLL   GPR01F,24                                                08700000
*           GPR00F=GPR00F|GPR01F;   /* SP IN HI BYTE                 */ 08701000
         OR    GPR00F,GPR01F                                       1161 08702000
*           GPR01F=ADDR(GETWORD->GTFBLOK);/* SET REGISTER ONE        */ 08703000
         L     GPR01F,GETWORD                                      1162 08704000
*           SVC(10);                /* R-FORM OF GETMAIN             */ 08705000
         SVC   10                                                  1163 08706000
*           RESPECIFY                                              1164 08707000
*            (GPR01F,                                              1164 08708000
*             GPR00F) UNRESTRICTED;                                1164 08709000
*         END;                      /* FREEMAIN(R) LV(LENGTH(GTFBLOK)   08710000
*                                      )SP(SQASP)A(GETWORD->GTFBLOK) */ 08711000
*       END;                                                       1166 08712000
*   END;                                                           1167 08713000
