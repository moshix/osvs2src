*   ADDR(BLOKINIT)->RECGTFOP=GTFOPTS;/* THE GTF OPTIONS IN EFFECT    */ 04708000
         MVC   RECGTFOP(4,@09),GTFOPTS(PCTPTR)                     0210 04709000
*   CURRBLOK=BLOKLIST(BLOKINDX);                                   0211 04710000
         SLA   @10,2                                               0211 04711000
         L     @10,BLOKLIST-4(@10)                                 0211 04712000
         ST    @10,CURRBLOK(,PCTPTR)                               0211 04713000
*   DO I=1 TO BLOKINDX;                                            0212 04714000
         LA    @10,1                                               0212 04715000
