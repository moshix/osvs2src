*             DO;                   /* WTOP MF(E,AHL032)             */ 04907000
*               RFY                                                0254 04908000
*                 GPR01P RSTD;      /* RESTRICT REG 1                */ 04909000
*               GPR01P=ADDR(AHL032);/* ADDRESS CONTROL PROGRAM LIST  */ 04910000
         L     GPR01P,@CA01942                                     0255 04911000
