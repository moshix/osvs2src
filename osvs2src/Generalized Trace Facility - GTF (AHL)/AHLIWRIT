*       GOTO RET7;                  /* BRANCH TO ERROR LOCATION      */ 05231000
*     RESPECIFY                                                    0358 05232000
*      (GPR00P,                                                    0358 05233000
*       GPR01P,                                                    0358 05234000
*       GPR15F) UNRESTRICTED;                                      0358 05235000
*   END;                            /* LOAD EP('AHLSBLOK')         0359 05236000
*                                      ERRET(RET7)                   */ 05237000
*   SBLOKRTN=LOADREG;               /* THIS IS FOR USE BY THE STACK     05238000
*                                      MACRO, AND WHEN IT BECOMES  0360 05239000
*                                      NONZERO THE STACK MACRO WILL     05240000
*                                      BEGIN FEEDING DATA INTO THE 0360 05241000
*                                      WRITER-STACK SERVICE.       0360 05242000
*                                      THEREFORE THIS MUST NOT BE  0360 05243000
*                                      DONE UNTIL NEAR LAST          */ 05244000
         ST    LOADREG,SBLOKRTN(,PCTPTR)                           0360 05245000
*   SBLOKSAV=LOADREG;                                              0361 05246000
         ST    LOADREG,SBLOKSAV                                    0361 05247000
*   GO TO LDFXDONE;                 /* BYPASS THE CODE FOR LOAD ERROR   05248000
*                                      CONDITIONS                    */ 05249000
         B     LDFXDONE                                            0362 05250000
*                                                                  0363 05251000
*   /*****************************************************************/ 05252000
*   /*                                                               */ 05253000
*   /* THESE ARE THE ERROR RETURNS FOR THE LOAD MACROS. THE VARIABLE */ 05254000
*   /* FIELDS OF THE MSG ARE FILLED IN AND THEN IT IS WRITTEN OUT.   */ 05255000
*   /* AFTER THAT THE TRACE WRITER IS TERMINATED                     */ 05256000
*   /*                                                               */ 05257000
*   /*****************************************************************/ 05258000
*                                                                  0363 05259000
*/*RET4: P (,LOADMSG) PUT 'AHLWSMOD' IN MESSAGE AHL035I              */ 05260000
*/*RET1: P (,LOADMSG) PUT 'AHLSBUF' IN MESSAGE AHL035I               */ 05261000
*/*RET2: P (,LOADMSG) PUT 'AHLSBCU1' IN MESSAGE AHL035I              */ 05262000
*/*RET3: P (,LOADMSG) PUT 'AHLSFEOB' IN MESSAGE AHL035I              */ 05263000
*/*RET7: P (,LOADMSG) PUT 'AHLSBLOK' IN MESSAGE AHL035I              */ 05264000
*/*LOADMSG: S (,CLEANUP) WTO: PUT 'LOAD' INTO & WRITE MESSAGE AHL035I   05265000
*/*                                                                  */ 05266000
*                                                                  0363 05267000
*RET4:                                                             0363 05268000
*   M035NAME='AHLWSMOD';                                           0363 05269000
RET4     L     @10,@CA01950                                        0363 05270000
         MVC   M035NAME-M035NAME(8,@10),@CC02153                   0363 05271000
*   GO TO LOADMSG;                                                 0364 05272000
         B     LOADMSG                                             0364 05273000
*RET1:                                                             0365 05274000
*   M035NAME='AHLSBUF ';                                           0365 05275000
RET1     L     @10,@CA01950                                        0365 05276000
         MVC   M035NAME-M035NAME(8,@10),@CC02159                   0365 05277000
*   GO TO LOADMSG;                                                 0366 05278000
         B     LOADMSG                                             0366 05279000
*RET2:                                                             0367 05280000
*   M035NAME='AHLSBCU1';                                           0367 05281000
