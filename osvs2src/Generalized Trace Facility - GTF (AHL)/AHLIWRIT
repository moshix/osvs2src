*   TS(QLOCK);                      /* TRY ONE MORE TIME TO GET QLOCK   07454000
*                                      IN CASE AHLSBUF HAS ALREADY 0881 07455000
*                                      RELEASED IT AND THEREFORE   0881 07456000
*                                      CANNOT BE COUNTED ON TO POST     07457000
*                                      QLOCKECB                      */ 07458000
         TS    QLOCK(PCTPTR)                                       0881 07459000
*   BC(8,GOTQLOCK);                 /* WAS THE TS SUCCESSFUL BRANCH     07460000
*                                      IF YES                        */ 07461000
