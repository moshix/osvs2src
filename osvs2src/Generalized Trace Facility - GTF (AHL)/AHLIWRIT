*              (GPR01F,                                            0585 06448000
*               GPR00F) RESTRICTED;                                0585 06449000
*             GPR00F=LENGTH(GTFBLOK);/* LENGTH REQUESTED             */ 06450000
         L     GPR00F,@CF02311                                     0586 06451000
*             GPR01F=SQASP;         /* SP IN REG 1                   */ 06452000
         LA    GPR01F,245                                          0587 06453000
*             GEN(SLL   GPR01F,24)                                 0588 06454000
*             NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;         0588 06455000
         SLL   GPR01F,24                                                06456000
*             GPR00F=GPR00F|GPR01F; /* SP IN HI BYTE                 */ 06457000
