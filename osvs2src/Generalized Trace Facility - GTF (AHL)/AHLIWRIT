         AL    @10,RQCOUNT                                         0461 05842000
         ST    @10,RQTOTAL(,PCTPTR)                                0461 05843000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05844000
*                                                                  0462 05845000
*       CALL GETQLOCK;              /* OBTAIN CONTROL OF THE QLOCK SO   05846000
*                                      THAT EQHEAD MAY BE ALTERED    */ 05847000
         BAL   @14,GETQLOCK                                        0462 05848000
*       RLSEBUF->EQCHAIN=EQHEAD;    /* RLSEBUF NOW POINTS TO THE LAST   05849000
*                                      GTFBCB ON THE STRING AND    0463 05850000
*                                      BCBHOLDR TO THE FIRST         */ 05851000
         L     @10,EQHEAD(,PCTPTR)                                 0463 05852000
         L     @09,RLSEBUF                                         0463 05853000
         ST    @10,EQCHAIN(,@09)                                   0463 05854000
*       EQHEAD=BCBHOLDR;            /* INSERT THE GROUP OF GTFBCB AT    05855000
*                                      THE HEAD OF THE EMPTY QUEUE   */ 05856000
         L     @10,BCBHOLDR                                        0464 05857000
         ST    @10,EQHEAD(,PCTPTR)                                 0464 05858000
*/* OPTIONAL DIAGNOSTIC AID                                          */ 05859000
*                                                                  0465 05860000
*       EQLENGTH=EQLENGTH+RQCOUNT;  /* INCREMENT THE COUNT OF GTFBCB    05861000
*                                      ON THE EMPTY QUEUE BY THE   0465 05862000
*                                      NUMBER IN THE GROUP           */ 05863000
