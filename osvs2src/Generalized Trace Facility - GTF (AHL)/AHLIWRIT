*   END;                                                           0208 04679000
*                                                                  0208 04680000
@DE00181 L     @10,BLOKINDX                                        0208 04681000
         C     @10,BLOKNUM                                         0208 04682000
         BL    @DL00181                                            0208 04683000
*   /*****************************************************************/ 04684000
*   /*                                                               */ 04685000
*   /* THIS LOOP INITIALIZES THE GTFBLOK AND POINTERS TO THEM        */ 04686000
*   /*                                                               */ 04687000
*   /*****************************************************************/ 04688000
*                                                                  0209 04689000
*/*%BLKINIT: N THIS LOOP INITIALIZES THE GTFBLOK                     */ 04690000
*/* P COPY CVTTZ & GTFOPTS INTO THE PROTOTYPE TIMESTAMP RECORD       */ 04691000
*/* P CURRBLOK=LAST PTR IN THE BLOKLIST, & I=1                       */ 04692000
*/*%BSLOOP: D (NO,,YES,%BSEND) I=BLOKINDX                            */ 04693000
*/* P NEXTBLOK= BLOKLIST(I)                                          */ 04694000
*/* P COPY NEXTBLOK PTR & PROTOTYPE INTO CURRBLOK-> GTFBLOK          */ 04695000
*/* P CURRBLOK= NEXTBLOK                                             */ 04696000
*/* P (,%BSLOOP) INCREMENT I BY 1                                    */ 04697000
*/*%BSEND:  P STORE TOD CLOCK INTO CURRBLOK-> GTFBLOK & SET BLOKUSE=0   04698000
*/*                                                                  */ 04699000
*/* P CLEAR AFGMBLOK                                                 */ 04700000
*                                                                  0209 04701000
*   ADDR(BLOKINIT)->RECVTTZ=CVTTZ;  /* COMPLETE THE PROTOTYPE FOR THE   04702000
*                                      GTFBLOK WITH THE TIMEZONE   0209 04703000
*                                      VALUE AND                     */ 04704000
         LA    @09,BLOKINIT                                        0209 04705000
         L     @01,CVTPTR                                          0209 04706000
