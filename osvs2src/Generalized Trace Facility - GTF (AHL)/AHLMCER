         LA    @07,AHLTFGBR                                        0118 00424000
         ST    @07,SDWARTYA(,GPR01P)                               0118 00425000
*         GPR01P->SDWAUPRG='1'B;    /* TURN ON RETREGS INDICATOR     */ 00426000
         OI    SDWAUPRG(GPR01P),B'00001000'                        0119 00427000
*         RESPECIFY                                                0120 00428000
*          (GPR00P,                                                0120 00429000
*           GPR01P,                                                0120 00430000
*           GPR14P,                                                0120 00431000
*           GPR15P)UNRSTD;                                         0120 00432000
*       END;                                                       0121 00433000
*       GO TO RTM;                  /* RETURN TO RTM - RTRY          */ 00434000
         BR    RTMADDR                                             0122 00435000
*     END;                                                         0123 00436000
*                                                                  0124 00437000
*   /*****************************************************************/ 00438000
*   /*                                                               */ 00439000
*   /* FALL THROUGH TO HERE INDICATES ERROR IN AHLMCER. PD ACTION IS */ 00440000
*   /* TAKEN AND A RETRY TO AHLTALL IS SCHEDULED TO TERMINATE ROUTING*/ 00441000
*   /* AND ALL APPLICATIONS                                          */ 00442000
*   /*                                                               */ 00443000
*   /*****************************************************************/ 00444000
*                                                                  0124 00445000
*   CALL AHLSDUMP;                  /* ATTEMP SDUMP FOR PD           */ 00446000
@RF00109 BAL   @14,AHLSDUMP                                        0124 00447000
*   DO;                             /* SETRP RETADDR(AHLTALL)      0125 00448000
*                                      RC(SDWARETY) RETREGS(YES)   0125 00449000
