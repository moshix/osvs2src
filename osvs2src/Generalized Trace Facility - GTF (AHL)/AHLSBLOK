*                                      IS REFERENCED AS CURRENT    0061 03533000
*                                      THROUGH OUT THE BLOCKING OF 0061 03534000
*                                      THIS REC                      */ 03535000
RESTART  L     CURRENT,CURRBLOK(,PCTPTR)                           0061 03536000
*   RFY                                                            0062 03537000
*     RECPTR RSTD;                                                 0062 03538000
*FIT:                                                              0063 03539000
*   DWSWAP=CURRENT->BLOKSWAP;       /* MAKE A COPY OF THE FIRST 2  0063 03540000
*                                      FIELDS OF THE GTFBLOK FOR   0063 03541000
*                                      REFERENCE & ALTERATION. SHOULD   03542000
*                                      THE GTFBLOK FIELDS NEED     0063 03543000
*                                      UPDATING THIS WILL BE USE FOR    03544000
*                                      A CDS                         */ 03545000
FIT      MVC   DWSWAP(8,AWSAPTR),BLOKSWAP(CURRENT)                 0063 03546000
*   DWCOMP=DWSWAP;                  /* KEEP AN UNCHANGING COPY FOR 0064 03547000
*                                      THE CDS ALSO                  */ 03548000
         MVC   DWCOMP(8,AWSAPTR),DWSWAP(AWSAPTR)                   0064 03549000
*   LENFIELD=LENFIELD+TRECLEN;      /* CALC WHAT THE NEW BLOKLEN WILL   03550000
*                                      HAVE TO BE TO ADD THIS RECORD    03551000
*                                      AND UPDATE THE COPY WHICH WILL   03552000
*                                      BE SWAPPED IN IF SPACE IS   0065 03553000
