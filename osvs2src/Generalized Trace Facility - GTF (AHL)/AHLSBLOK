*                                      HAVE ITS TIME FIELD UPDATED   */ 04143000
         STCK RECTOD(BLOKCURR)                                          04144000
*   DWCOMP=BLOKCURR->BLOKSWAP;                                     0154 04145000
         MVC   DWCOMP(8,AWSAPTR),BLOKSWAP(BLOKCURR)                0154 04146000
*TIMECS:                                                           0155 04147000
*   DWSWAP=DWCOMP;                  /* PREPARE TO DECREMENT THE    0155 04148000
*                                      BLOKUSE TO INDICATE THE SPACE    04149000
*                                      RESERVED FOR THE TIME RECORD     04150000
*                                      IS COMPLETED                  */ 04151000
TIMECS   MVC   DWSWAP(8,AWSAPTR),DWCOMP(AWSAPTR)                   0155 04152000
*   USEFIELD=USEFIELD-1;            /* PREPARE THE PROPOSED NEW    0156 04153000
*                                      BLOKUSE                       */ 04154000
         LH    @06,USEFIELD(,AWSAPTR)                              0156 04155000
         BCTR  @06,0                                               0156 04156000
         STH   @06,USEFIELD(,AWSAPTR)                              0156 04157000
*   CDS(DWCOMP,DWSWAP,BLOKCURR->BLOKSWAP);/* ATTEMPT THE UPDATE      */ 04158000
         LM    @00,@01,DWCOMP(AWSAPTR)                             0157 04159000
         LM    @08,@09,DWSWAP(AWSAPTR)                             0157 04160000
         CDS   @00,@08,BLOKSWAP(BLOKCURR)                          0157 04161000
         BZ    @BC00157                                            0157 04162000
         STM   @00,@01,DWCOMP(AWSAPTR)                             0157 04163000
@BC00157 DS    0H                                                  0158 04164000
*   BC(4,TIMECS);                   /* IF THE CDS FAILS TRY AGAIN    */ 04165000
         BC    4,TIMECS                                            0158 04166000
*   RFY                                                            0159 04167000
*     BLOKCURR UNRSTD;                                             0159 04168000
*                                                                  0160 04169000
*   /*****************************************************************/ 04170000
*   /*                                                               */ 04171000
*   /* THIS SECTION OF CODE WILL SKIP SCHEDULING AHLSBUF IF THERE ARE*/ 04172000
*   /* NO FULL GTFBLOK, OR IF THE NEXTBLOK-> GTFBLOK (NEXT TO BE     */ 04173000
*   /* BUFFERED BY AHLSBUF) HAS A NONZERO BLOKUSE AND WRTRCOM ECB IS */ 04174000
*   /* POSTED. THE REASON FOR SCHEDULING AHLSBUF WHEN THE NEXTBLOK-> */ 04175000
*   /* GTFBLOK BLOKUSE IS NONZERO (MEANING AHLSBUF CANNOT BUFFER IT) */ 04176000
*   /* IS THAT AHLSBUF WILL POST THE TRACE WRITER ANYWAY WHICH MAY BE*/ 04177000
*   /* ABLE TO CORRECT THE PROBLEM                                   */ 04178000
*   /*                                                               */ 04179000
*   /*****************************************************************/ 04180000
*                                                                  0160 04181000
*/*NOFEOB: D (YES,,NO,SETRC) NEXTBLOK^= CURRBLOK?                    */ 04182000
*/* D (YES,,NO,%AX2) IS NEXTBLOK-> BLOKUSE^=0                        */ 04183000
*/* D (YES,SETRC,NO,) IS WRTRCOM POSTED?                             */ 04184000
*                                                                  0160 04185000
*NOFEOB:                                                           0160 04186000
*                                                                  0160 04187000
*/*   IF THE NEXT TO BE BUFFERED IS NOT THE SAME AS THE CURRENT BLOCK,  04188000
*  THEN AN ATTEMPT SHOULD BE MADE TO SCHEDULE THE FULL BLOCK(S) FOR BUF 04189000
*RING.  ON THE OTHER HAND, IF THEY ARE THE SAME, THEN ALL BLOCKS HAVE   04190000
*   RECENTLY BEEN BUFFERED AND THE NEXT ONE TO BE BUFFERED IS      0160 04191000
*HE BLOCK CURRENTLY BEING FILLED.  THEREFORE, NO SCHEDULING IS DONE  */ 04192000
*                                                                  0160 04193000
*   IF NEXTBLOK^=CURRBLOK           /* ARE THERE ANY GTFBLOK THAT ARE   04194000
*                                      BOTH FULL AND NOT CURRENT     */ 04195000
*     THEN                                                         0160 04196000
NOFEOB   L     @05,NEXTBLOK(,PCTPTR)                               0160 04197000
         C     @05,CURRBLOK(,PCTPTR)                               0160 04198000
         BE    @RF00160                                            0160 04199000
*     DO;                           /* YES THERE ARE FULL GTFBLOK    */ 04200000
*       IF NEXTBLOK->BLOKUSE^=0     /* THEN IS THE NEXT GTFBLOK TO BE   04201000
*                                      BUFFERED READY FOR BUFFERING  */ 04202000
*         THEN                      /* NO, THE NEXT GTFBLOK IS NOT 0162 04203000
*                                      READY FOR BUFFERING. SCHEDULE    04204000
*                                      AHLSBUF ANYWAY JUST TO GET THE   04205000
*                                      TRACE WRITER POSTED           */ 04206000
         ICM   @15,3,BLOKUSE(@05)                                  0162 04207000
         BZ    @RF00162                                            0162 04208000
*         IF WRTRCOMP=ON            /* UNLESS IT IS ALREADY POSTED.     04209000
*                                      WRTRCOMP IS THE POST BIT OF 0163 04210000
*                                      THE WRTRCOM ECB               */ 04211000
*           THEN                                                   0163 04212000
         TM    WRTRCOMP(PCTPTR),B'01000000'                        0163 04213000
         BO    @RT00163                                            0163 04214000
*           GO TO SETRC;            /* THE NEXT GTFBLOK IS NOT READY    04215000
*                                      FOR BUFFERING AND THE TRACE 0164 04216000
*                                      WRITER IS POSTED SO THERE IS     04217000
*                                      NOTHING AHLSBUF CAN DO      0164 04218000
*                                                                    */ 04219000
*                                                                  0165 04220000
*       /*************************************************************/ 04221000
*       /*                                                           */ 04222000
*       /* IF THERE IS A FULL GTFBLOK AND EITHER IT IS READY TO BE   */ 04223000
*       /* BUFFERED (BLOKUSE=0) OR WRTRCOM ECB IS NOT POSTED THE     */ 04224000
*       /* FOLLOWING CODE IS EXECUTED TO SCHEDULE THE SRB FOR AHLSBUF*/ 04225000
*       /*                                                           */ 04226000
*       /*************************************************************/ 04227000
*                                                                  0165 04228000
*/*%AX2: P OBTAIN A STABLE COPY OF GTFSSRB & FORCE SRBAVAIL OFF IN IT   04229000
*/*                                                                  */ 04230000
*/* P CS TO VERIFY SRBAVAIL IS OFF AND THE TURN IT ON                */ 04231000
*/* D (FAIL,SETRC,OK,) CS?                                           */ 04232000
*/* S SCHEDULE: SCHEDULE AHLSBUF                                     */ 04233000
*                                                                  0165 04234000
*       RFY                                                        0165 04235000
*        (COMPREG,                                                 0165 04236000
*         SWAPREG) RSTD;                                           0165 04237000
@RF00162 DS    0H                                                  0166 04238000
*       COMPREG=GTFSSRB;                                           0166 04239000
         L     COMPREG,GTFSSRB(,PCTPTR)                            0166 04240000
*       SWAPREG=COMPREG;            /* PREPARE TO GET CONTROL OF THE    04241000
*                                      SRB FOR AHLSBUF               */ 04242000
         LR    SWAPREG,COMPREG                                     0167 04243000
*       SWAPREG=SWAPREG&'7FFFFFFF'X;/* THIS FORCES OFF THE SRBAVAIL     04244000
*                                      BIT SO THAT IF THE CS GOES THE   04245000
*                                      SRB WILL NO LONGER BE       0168 04246000
*                                      AVAILABLE                     */ 04247000
         N     SWAPREG,@CF01787                                    0168 04248000
*       COMPREG=COMPREG|'80000000'X;/* THIS FORCES ON THE SRBAVAIL 0169 04249000
*                                      BIT IN THE CONTROL SO THAT IF    04250000
*                                      IT WAS NOT ON IN GTFSSRB THE     04251000
*                                      CS WILL FAIL                  */ 04252000
         O     COMPREG,@CF01788                                    0169 04253000
*       CS(COMPREG,SWAPREG,GTFSSRB);                               0170 04254000
         CS    COMPREG,SWAPREG,GTFSSRB(PCTPTR)                     0170 04255000
*       BC(4,SETRC);                /* IF THE CS FAILED SKIP TRYING     04256000
*                                      TO SCHEDULE AHLSBUF BECAUSE IT   04257000
*                                      IS ALREADY SCHEDULED          */ 04258000
         BC    4,SETRC                                             0171 04259000
*       GEN CODE(SCHEDULE  SRB=(SWAPREG),SCOPE=GLOBAL)             0172 04260000
*       NODEFS NOEXIT NOSETS NOFLOWS SEQFLOW REFS(SRBFLNK,SRBSECT, 0172 04261000
*           CVTGSMQ,CVTMAP,CVTPTR,SWAPREG);                        0172 04262000
         SCHEDULE  SRB=(SWAPREG),SCOPE=GLOBAL                           04263000
*       RFY                                                        0173 04264000
*        (COMPREG,                                                 0173 04265000
*         SWAPREG) UNRSTD;                                         0173 04266000
*     END;                                                         0174 04267000
*                                                                  0175 04268000
*   /*****************************************************************/ 04269000
*   /*                                                               */ 04270000
*   /* THIS IS THE EXIT CODE FOR ALL PATHS THROUGH AHLSFEOB. BEFORE  */ 04271000
*   /* RETURNNING TO THE CALLER IT IS NECESSARY TO SET THE RETURN    */ 04272000
*   /* CODE. THE CRITERION USED IS ACTUALLY WHETHER THE STABLE COPY  */ 04273000
*   /* OF CURRBLOK PASSED BY THE CALLER (CURRENT) HAS BEEN UPDATED OR*/ 04274000
*   /* NOT, INSTEAD OF WHETHER A FEOB HAS BEEN DONE. THE REASON FOR  */ 04275000
*   /* THIS IS THAT AHLSFEOB IS NOT SERIAL, SO A FEOB MIGHT HAVE BEEN*/ 04276000
*   /* DONE BETWEEN THE TIME CURRBLOK WAS SAVED IN CURRENT AND NOW.  */ 04277000
*   /* THE RETURN CODE THEREFORE MUST INDICATE IF A FEOB WAS DONE AT */ 04278000
*   /* ANY TIME NOT JUST NOW.                                        */ 04279000
*   /*                                                               */ 04280000
*   /*****************************************************************/ 04281000
*                                                                  0175 04282000
*/*SETRC: D (YES,,NO,%RCODE)  CURRBLOK^= CURRENT (REG4)              */ 04283000
*/* N CURRENT IS THE VALUE OF CURRBLOK BEING USED BY THE CALLING RTN */ 04284000
*/* P SET RETURN CODE = 0?                                           */ 04285000
*/* N (,%R1) A CHANGE IN CURRBLOK MEANS A FEOB AS FAR AS THE CALLER IS  04286000
*/* CONCERNED                                                        */ 04287000
*/*%RCODE: P SET RETURN CODE = 4                                     */ 04288000
*/* N NO CHANGE IN CURRBLOK MEANS NO FEOB                            */ 04289000
*/*%R1: R RETURN                                                     */ 04290000
*                                                                  0175 04291000
*   RFY                                                            0175 04292000
*     RCREG RSTD;                                                  0175 04293000
@RF00160 DS    0H                                                  0176 04294000
*SETRC:                                                            0176 04295000
*   IF CURRENT^=CURRBLOK            /* WAS ANY FEOB DONE BY ANYONE 0176 04296000
*                                      SINCE CURRENT WAS SET BY THE     04297000
*                                      CALLER                        */ 04298000
*     THEN                          /* YES                           */ 04299000
SETRC    C     CURRENT,CURRBLOK(,PCTPTR)                           0176 04300000
         BE    @RF00176                                            0176 04301000
*     RCREG=RCSUCCSS;               /* SET THE RETURN CODE TO      0177 04302000
*                                      INDICATE THAT A FEOB WAS DONE    04303000
*                                      SINCE SINCE THE CALLER SAVED     04304000
*                                      CURRBLOK IN CURRENT AND THAT     04305000
*                                      CURRENT HAS BEEN UPDATED      */ 04306000
         SLR   RCREG,RCREG                                         0177 04307000
*   ELSE                                                           0178 04308000
*     RCREG=RCFAIL;                 /* SET RETURN CODE FOR NO FEOB   */ 04309000
         B     @RC00176                                            0178 04310000
@RF00176 LA    RCREG,4                                             0178 04311000
*   GEN CODE(LM  @04,@14,MCASFEOB(AWSAPTR))                        0179 04312000
*   NOEXIT REFS(AWSAPTR,MCASFEOB) NOSETS NOFLOWS SEQFLOW NODEFS;/* 0179 04313000
*                                      RESTORE ALL REGS EXCEPT 15  0179 04314000
*                                      WHICH HAS THE RETURN CODE     */ 04315000
@RC00176 DS    0H                                                  0179 04316000
         LM  @04,@14,MCASFEOB(AWSAPTR)                                  04317000
*   GEN CODE(LM @00,@01,MCASFEOB+48(AWSAPTR));                     0180 04318000
         LM @00,@01,MCASFEOB+48(AWSAPTR)                                04319000
*   GEN CODE EXIT(BR @14);          /* RETURN TO THE CALLER          */ 04320000
         BR @14                                                         04321000
*   RFY                                                            0182 04322000
*    (RCREG) UNRSTD;                                               0182 04323000
*                                                                  0183 04324000
*   /*****************************************************************/ 04325000
*   /*                                                               */ 04326000
*   /* FRR CODE FOR AHLSBLOK, AND FOR AHLSFEOB IF IT IS ENTERED BY   */ 04327000
*   /* AHLSBLOK                                                      */ 04328000
*   /*                                                               */ 04329000
*   /*****************************************************************/ 04330000
*                                                                  0183 04331000
*/*AHLSBFRR: E FROM RTM                                              */ 04332000
*/* P SET UP ADDRESSABILITY TO ALL OF AHLSBLOK                       */ 04333000
*/* P SAVE THE RETURN ADDRESS (REG14)                                */ 04334000
*/* P SBLOKRTN=0 GTFSSRB=0 DISABLE AHLSTACK & AHLSBUF                */ 04335000
*/* P SET WSERFG02                                                   */ 04336000
*/* S SDUMP: TAKE AN SVC DUMP                                        */ 04337000
*/* P SDWAEAS=ON                                                     */ 04338000
*/* S SETRP: PERCOLATE TO TERMINATE THE APPLICATION                  */ 04339000
*/* P RESTORE THE RETURN ADDRESS                                     */ 04340000
*/* R RETURN TO RTM                                                  */ 04341000
*                                                                  0183 04342000
*   RFY                                                            0183 04343000
*    (SDWAPTR,                                                     0183 04344000
*     RTRNREG,                                                     0183 04345000
*     R1,                                                          0183 04346000
*     FRR14SAV) RSTD;                                              0183 04347000
*AHLSBFRR:                                                         0184 04348000
*   GEN SETS(BASEREG) NOEXIT NOFLOWS SEQFLOW;                      0184 04349000
AHLSBFRR DS    0H                                                  0184 04350000
          USING  AHLSBFRR,@15   ASSIGN TEMP ADDRESSIBILITY              04351000
          L      BASEREG,SBLKADDR   RESTORE NORMAL ADDRESSIBILITY       04352000
          DROP   @15                                                    04353000
*   SDWAPTR=R1;                                                    0185 04354000
         LR    SDWAPTR,R1                                          0185 04355000
*   RFY                                                            0186 04356000
*     R1 UNRSTD;                                                   0186 04357000
*   FRR14SAV=RTRNREG;               /* SAVE THE RETURN ADDRESS       */ 04358000
         LR    FRR14SAV,RTRNREG                                    0187 04359000
*   PCTPTR=SDWAGR02;                /* RESTORE THE BASE ADDRESS FOR     04360000
*                                      THE GTFPCT                    */ 04361000
         L     PCTPTR,SDWAGR02(,SDWAPTR)                           0188 04362000
*   SBLOKRTN=0;                     /* ZERO OUT THE ADDRESS TO THIS     04363000
*                                      ROUTINE                       */ 04364000
         SLR   @15,@15                                             0189 04365000
         ST    @15,SBLOKRTN(,PCTPTR)                               0189 04366000
*   GTFSSRB=0;                      /* ZERO OUT THE ADDRESS OF THE 0190 04367000
*                                      SRB ROUTINE                   */ 04368000
         ST    @15,GTFSSRB(,PCTPTR)                                0190 04369000
*/*  AN SVC DUMP WILL BE PROVIDED BY THE FGBR FRR                    */ 04370000
*                                                                  0191 04371000
*   WSERFG03=ON;                    /* SET ERROR FLAG                */ 04372000
         OI    WSERFG03(PCTPTR),B'10000000'                        0191 04373000
*   IF SDWAMCHK=ON&SDWAINSF=ON&SDWAREGU=OFF&SDWAPSWU=OFF/* IT THIS A    04374000
*                                      RECOVERABLE ACR MACHINE CHECK    04375000
*                                      TYPE ERROR                    */ 04376000
*     THEN                                                         0192 04377000
         TM    SDWAMCHK(SDWAPTR),B'10000000'                       0192 04378000
         BNO   @RF00192                                            0192 04379000
         TM    SDWAINSF(SDWAPTR),B'00000100'                       0192 04380000
         BNO   @RF00192                                            0192 04381000
         TM    SDWAREGU(SDWAPTR),B'01100000'                       0192 04382000
         BNZ   @RF00192                                            0192 04383000
*     DO;                                                          0193 04384000
*       SBLOKRTN=ADDR(AHLSBLOK);                                   0194 04385000
         L     @15,@CA00028                                        0194 04386000
         ST    @15,SBLOKRTN(,PCTPTR)                               0194 04387000
*       WSERFG03=OFF;                                              0195 04388000
         NI    WSERFG03(PCTPTR),B'01111111'                        0195 04389000
*       DO;                         /* SETRP RECPARM(RECID)        0196 04390000
*                                      WKAREA(SDWAPTR) RECORD(YES) 0196 04391000
*                                      RC(SDWARETY)                0196 04392000
*                                      RETADDR(SDWANXT1->PLACE)      */ 04393000
*         RESPECIFY                                                0197 04394000
*          (GPR00P,                                                0197 04395000
*           GPR01P,                                                0197 04396000
*           GPR14P,                                                0197 04397000
*           GPR15P) RSTD;                                          0197 04398000
*         GPR01P=SDWAPTR;                                          0198 04399000
         LR    GPR01P,SDWAPTR                                      0198 04400000
*         GPR01P->SDWARCDE=SDWARETY;/* STORE RC INTO SDWA            */ 04401000
         MVI   SDWARCDE(GPR01P),X'04'                              0199 04402000
*         GPR01P->SDWARTYA=ADDR(SDWANXT1->PLACE);/* SAVE RETRY     0200 04403000
*                                      ADDRESS                       */ 04404000
         MVC   SDWARTYA(4,GPR01P),SDWANXT1(SDWAPTR)                0200 04405000
*         GPR01P->SDWARCRD='1'B;    /* TURN ON RECORD INDICATOR      */ 04406000
         OI    SDWARCRD(GPR01P),B'10000000'                        0201 04407000
*         GPR15P=ADDR(RECID);       /* ACCESS RECORD PARAMETER LIST  */ 04408000
         LA    GPR15P,RECID                                        0202 04409000
*         GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD      0203 04410000
*                                      PARAMETERS                    */ 04411000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   0203 04412000
*         RESPECIFY                                                0204 04413000
*          (GPR00P,                                                0204 04414000
*           GPR01P,                                                0204 04415000
*           GPR14P,                                                0204 04416000
*           GPR15P) UNRSTD;                                        0204 04417000
*       END;                                                       0205 04418000
*     END;                                                         0206 04419000
*   ELSE                                                           0207 04420000
*     DO;                                                          0207 04421000
*                                                                  0207 04422000
         B     @RC00192                                            0207 04423000
@RF00192 DS    0H                                                  0208 04424000
*/*  PERCOLATE TO CAUSE TERMINATION OF THE APPLICATION (GTF)         */ 04425000
*                                                                  0208 04426000
*       DO;                         /* SETRP RECPARM(RECID)        0208 04427000
*                                      WKAREA(SDWAPTR) RECORD(YES) 0208 04428000
*                                      RC(SDWACWT)                   */ 04429000
*         RESPECIFY                                                0209 04430000
*          (GPR00P,                                                0209 04431000
*           GPR01P,                                                0209 04432000
*           GPR14P,                                                0209 04433000
*           GPR15P) RSTD;                                          0209 04434000
*         GPR01P=SDWAPTR;                                          0210 04435000
         LR    GPR01P,SDWAPTR                                      0210 04436000
*         GPR01P->SDWARCDE=SDWACWT; /* SAVE RC VALUE                 */ 04437000
         MVI   SDWARCDE(GPR01P),X'00'                              0211 04438000
*         GPR01P->SDWARCRD='1'B;    /* TURN ON RECORD INDICATOR      */ 04439000
         OI    SDWARCRD(GPR01P),B'10000000'                        0212 04440000
*         GPR15P=ADDR(RECID);       /* ACCESS RECORD PARAMETER LIST  */ 04441000
         LA    GPR15P,RECID                                        0213 04442000
*         GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD      0214 04443000
*                                      PARAMETERS                    */ 04444000
