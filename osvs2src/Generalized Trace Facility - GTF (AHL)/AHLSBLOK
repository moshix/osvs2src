         STH   @05,USEFIELD(,AWSAPTR)                              0125 03975000
*       CDS(DWCOMP,DWSWAP,BLOKCURR->BLKCHAIN->BLOKSWAP);/* ATTEMPT TO   03976000
*                                      UPDATE THE GTFBLOK CONTROL  0126 03977000
*                                      FIELDS WITH THE NEW VALUES AND   03978000
*                                      VERIFY BLKINUSE IS STILL OFF  */ 03979000
         LM    @00,@01,DWCOMP(AWSAPTR)                             0126 03980000
         LM    @08,@09,DWSWAP(AWSAPTR)                             0126 03981000
         CDS   @00,@08,BLOKSWAP(@06)                               0126 03982000
         BZ    @BC00126                                            0126 03983000
         STM   @00,@01,DWCOMP(AWSAPTR)                             0126 03984000
@BC00126 DS    0H                                                  0127 03985000
*       BC(4,SETRC);                /* IF THE CDS FAILED THE GTFBLOK    03986000
*                                      HAS HAD A FEOB DONE ON IT   0127 03987000
*                                      ALREADY AND ITS STATUS HAS  0127 03988000
*                                      CHANGED, ALSO GTFLOSTR HAS  0127 03989000
*                                      NECESSARILY BEEN SET TO 0   0127 03990000
*                                      SINCE EVERYTHING THAT WAS   0127 03991000
*                                      GOING TO BE DONE HAS ALREADY     03992000
*                                      BEEN DONE BY ANOTHER AHLSFEOB    03993000
*                                      SO JUST RETURN TO THE CALLER     03994000
*                                                                    */ 03995000
         BC    4,SETRC                                             0127 03996000
*                                                                  0128 03997000
*       /*************************************************************/ 03998000
*       /*                                                           */ 03999000
*       /* ONCE SPACE HAS BEEN SUCCESSFULLY RESERVED IN THE GTFBLOK  */ 04000000
*       /* THE LOST EVENT RECORD IS BUILT IN IT                      */ 04001000
*       /*                                                           */ 04002000
*       /*************************************************************/ 04003000
*                                                                  0128 04004000
*/* P CALC THE ADDRESS OF THE SPACE RESERVED FOR THE LE REC          */ 04005000
*/* P SAVE STABLE COUNT OF GTFLOSTR                                  */ 04006000
*/*ZEROCNT: P CS ZERO INTO GTFLOSTR                                  */ 04007000
*/* D (FAIL,ZEROCNT,OK,) CS?                                         */ 04008000
*/* P MOVE LOST REC RECORD PREFIX INTO THE RESERVED SPACE            */ 04009000
*/* P MOVE TIMEZONE INTO THE RESERVED SPACE                          */ 04010000
*/* P STORE THE TOD CLOCK VALUE INTO THE RESERVED SPACE              */ 04011000
*/* P SAVE THE COUNT IN THE RESERVED SPACE                           */ 04012000
*/* N THE LOST EVENT RECORD IS NOW BUILT                             */ 04013000
*/* P OBTAIN A STABLE COPY OF THE GTFBLOK CONTROL FIELDS             */ 04014000
*/*LRRBUD: P DECREMENT THE BLOKUSE IN THE STABLE COPY                */ 04015000
*/* P ATTEMPT TO CS IN THE DECREMENTED BLOKUSE                       */ 04016000
*/* D (FAIL,LRRBUD,OK,) CS?                                          */ 04017000
*                                                                  0128 04018000
*       RFY                                                        0128 04019000
*         LRRECPTR RSTD;                                           0128 04020000
*       LRRECPTR=ADDR(BLOKCURR->BLKCHAIN->BLOKAREA)+OLDLEN;/* GET THE   04021000
*                                      ADDR OF THE GTFBLOK SPACE   0129 04022000
*                                      RESERVED TO BE USED AS A BASE    04023000
*                                      FOR LOSTRECR                  */ 04024000
         L     LRRECPTR,BLKCHAIN(,BLOKCURR)                        0129 04025000
         LA    LRRECPTR,BLOKAREA(,LRRECPTR)                        0129 04026000
         AH    LRRECPTR,OLDLEN(,AWSAPTR)                           0129 04027000
*       RFY                                                        0130 04028000
*        (COMPREG,                                                 0130 04029000
*         SWAPREG) RSTD;                                           0130 04030000
*       SWAPREG=0;                  /* PREPARE NEW VALUE FOR GTFLOSTR*/ 04031000
         SLR   SWAPREG,SWAPREG                                     0131 04032000
*       COMPREG=GTFLOSTR;           /* SAVE STABLE COPY OF GTFLOSTR  */ 04033000
         L     COMPREG,GTFLOSTR(,PCTPTR)                           0132 04034000
*ZEROCNT:                                                          0133 04035000
*       CS(COMPREG,SWAPREG,GTFLOSTR);/* ATTEMPT TO UPDATE GTFLOSTR   */ 04036000
ZEROCNT  CS    COMPREG,SWAPREG,GTFLOSTR(PCTPTR)                    0133 04037000
*       BC(4,ZEROCNT);              /* IF THE CS FAILED TRY AGAIN. 0134 04038000
*                                      GTFLOSTR WAS PROBABLY ZEROED     04039000
*                                      ALREADY, BUT SINCE SPACE HAS     04040000
*                                      ALREADY BEEN RESERVED THE LOST   04041000
*                                      RECORD RECORD MUST BE BUILT 0134 04042000
*                                      EVEN WITH A 0 COUNT           */ 04043000
         BC    4,ZEROCNT                                           0134 04044000
*       LOSTRECR(1:LENGTH(LRRPROTO))=LRRPROTO;/* MOVE IN THE RECORD     04045000
*                                      PREFIX                        */ 04046000
         MVC   LOSTRECR(6,LRRECPTR),LRRPROTO                       0135 04047000
*       LRRCVTTZ=CVTTZ;             /* MOVE IN THE TIMEZONE          */ 04048000
         L     @05,CVTPTR                                          0136 04049000
         MVC   LRRCVTTZ(4,LRRECPTR),CVTTZ(@05)                     0136 04050000
*       GEN CODE(STCK LRRTODCV(LRRECPTR))                          0137 04051000
*       NOEXIT NOFLOWS REFS(LRRECPTR) SETS(LRRTODCV)/* PUT           */ 04052000
*           NODEFS SEQFLOW;         /* THE TOD CLOCK VALUE INTO THE     04053000
*                                      LOST RECORD RECORD            */ 04054000
         STCK LRRTODCV(LRRECPTR)                                        04055000
*       LRRCNT=COMPREG;             /* SAVE THE COUNT IN THE SPACE 0138 04056000
*                                      RESERVED                      */ 04057000
         ST    COMPREG,LRRCNT(,LRRECPTR)                           0138 04058000
*       RFY                                                        0139 04059000
*        (COMPREG,                                                 0139 04060000
*         SWAPREG) UNRSTD;                                         0139 04061000
*       DWCOMP=BLOKCURR->BLKCHAIN->BLOKSWAP;/* NOW THAT THE LOST   0140 04062000
*                                      RECORD RECORD IS BUILT AND IN    04063000
*                                      PLACE                         */ 04064000
         L     @05,BLKCHAIN(,BLOKCURR)                             0140 04065000
         MVC   DWCOMP(8,AWSAPTR),BLOKSWAP(@05)                     0140 04066000
*LRRBUD:                                                           0141 04067000
*       DWSWAP=DWCOMP;              /* PREPARE TO DECREMENT THE    0141 04068000
*                                      BLOKUSE TO INDICATE THE     0141 04069000
*                                      RESERVED SPACE IS FILLED      */ 04070000
LRRBUD   MVC   DWSWAP(8,AWSAPTR),DWCOMP(AWSAPTR)                   0141 04071000
*       USEFIELD=USEFIELD-1;        /* PREPARE NEW BLOKUSE           */ 04072000
         LH    @05,USEFIELD(,AWSAPTR)                              0142 04073000
         BCTR  @05,0                                               0142 04074000
         STH   @05,USEFIELD(,AWSAPTR)                              0142 04075000
*       CDS(DWCOMP,DWSWAP,BLOKCURR->BLKCHAIN->BLOKSWAP);/* ATTEMPT 0143 04076000
*                                      UPDATE                        */ 04077000
         LM    @00,@01,DWCOMP(AWSAPTR)                             0143 04078000
         LM    @08,@09,DWSWAP(AWSAPTR)                             0143 04079000
         L     @05,BLKCHAIN(,BLOKCURR)                             0143 04080000
         CDS   @00,@08,BLOKSWAP(@05)                               0143 04081000
         BZ    @BC00143                                            0143 04082000
         STM   @00,@01,DWCOMP(AWSAPTR)                             0143 04083000
@BC00143 DS    0H                                                  0144 04084000
*       BC(4,LRRBUD);               /* IF CDS FAILS TRY AGAIN        */ 04085000
         BC    4,LRRBUD                                            0144 04086000
*       RFY                                                        0145 04087000
*         LRRECPTR UNRSTD;                                         0145 04088000
*     END;                                                         0146 04089000
*                                                                  0147 04090000
*   /*****************************************************************/ 04091000
*   /*                                                               */ 04092000
*   /* AFTER IT HAS BEEN DETERMINED THAT GTFLOSTR=0 OR ELSE A LOST   */ 04093000
*   /* RECORD RECORD HAS BEEN BUILT THIS SECTION OF CODE IS ENTERED. */ 04094000
*   /* IT EITHER DETERMINES THAT A FEOB HAS ALREADY BEEN DONE (IN    */ 04095000
*   /* WHICH CASE ANOTHER IS NOT NEEDED), OR IT DOES ONE             */ 04096000
*   /*                                                               */ 04097000
*   /*****************************************************************/ 04098000
*                                                                  0147 04099000
*/*%AX3: P MAKE A COPY OF THE CURRENT GTFBLOK PTR (REG4) PASSED BY THE  04100000
*/*CALLER                                                            */ 04101000
*/* N THIS COPY IS USED AS THE CS CONTROL SO CURRENT WIL NOT BE ALTERED 04102000
*/*                                                                  */ 04103000
*/* P CS TO DO FEOB USING CURRBLOK VALUE PASSED IN REG4 (CURRENT)    */ 04104000
*/* N THIS IS DONE SO THAT IF A FEOB WAS ALREADY DONE ANOTHER WILL BE   04105000
*/*AVOIDED                                                           */ 04106000
*/* D (FAIL,NOFEOB,OK,) CS?                                          */ 04107000
*/* P COMPLETE THE TIME REC IN THE GTFBLOK JUST MADE CURRENT         */ 04108000
*/* P OBTAIN A STABLE COPY OF THE CONTROL FIELDS OF THE NEWLY CURRENT   04109000
*/*GTFBLOK                                                           */ 04110000
*/*%TIMECS: P DECREMENT BLOKUSE IN THE STABLE COPY                   */ 04111000
*/* P CS IN THE NEW BLOKUSE                                          */ 04112000
*/* D (FAIL,%TIMECS,OK,) CS?                                         */ 04113000
*                                                                  0147 04114000
*   RFY                                                            0147 04115000
*     COMPREG RSTD;                                                0147 04116000
@RF00118 DS    0H                                                  0148 04117000
*   COMPREG=CURRENT;                /* THIS IS DONE TO AVOID HAVING     04118000
*                                      TH CS INST UPDATE CURRENT IF     04119000
*                                      THE COMPARE SHOULD FAIL       */ 04120000
         LR    COMPREG,CURRENT                                     0148 04121000
*   BLOKCURR=CURRENT->BLKCHAIN;     /* PREPARE TO ATTEMPT FEOB       */ 04122000
         L     BLOKCURR,BLKCHAIN(,CURRENT)                         0149 04123000
*   CS(COMPREG,BLOKCURR,CURRBLOK);  /* IF THE STABLE COPY OF CURRBLOK   04124000
*                                      PASSED BY THE CALLER (CURRENT)   04125000
*                                      IS STILL THE SAME AS CURRBLOK    04126000
*                                      THE CS WILL UPDATE CURRBLOK TO   04127000
*                                      THE NEXT GTFBLOK IN THE CHAIN,   04128000
*                                      THIS IS A FEOB                */ 04129000
         CS    COMPREG,BLOKCURR,CURRBLOK(PCTPTR)                   0150 04130000
*   BC(4,NOFEOB);                   /* IF CURRBLOK HAS CHANGED SINCE    04131000
*                                      CURRENT WAS SET BY THE CALLER    04132000
*                                      NO FEOB NEED BE DONE BECAUSE     04133000
*                                      ONE HAS ALREADY BEEN DONE THAT   04134000
*                                      THE CALLER IS NOT AWARE OF    */ 04135000
         BC    4,NOFEOB                                            0151 04136000
*   RFY                                                            0152 04137000
*     COMPREG UNRSTD;                                              0152 04138000
*   GEN CODE(STCK RECTOD(BLOKCURR))                                0153 04139000
