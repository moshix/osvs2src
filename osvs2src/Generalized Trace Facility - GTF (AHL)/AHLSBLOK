*         RESPECIFY                                                0215 04446000
*          (GPR00P,                                                0215 04447000
*           GPR01P,                                                0215 04448000
*           GPR14P,                                                0215 04449000
*           GPR15P) UNRSTD;                                        0215 04450000
*       END;                                                       0216 04451000
*     END;                                                         0217 04452000
*   RTRNREG=FRR14SAV;               /* RESTORE THE RETURN ADDRESS    */ 04453000
@RC00192 LR    RTRNREG,FRR14SAV                                    0218 04454000
*   GEN CODE EXIT(BR 14);           /* RETURN TO RTM                 */ 04455000
         BR 14                                                          04456000
*   RFY                                                            0220 04457000
*    (SDWAPTR,                                                     0220 04458000
*     RTRNREG,                                                     0220 04459000
*     FRR14SAV) UNRSTD;                                            0220 04460000
*                                                                  0221 04461000
*   /*****************************************************************/ 04462000
*   /*                                                               */ 04463000
