         TITLE 'AHLSCAN-GTF INITIALIZATION SUBROUTINE TO SCAN START COM*00001000
               MAND    '                                                00002000
AHLSCAN  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AHLSCAN   73.240'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*                                                                  0083 00018000
*/********************************************************************/ 00019000
*/* THIS MARKS THE BEGINING OF EXECUTABLE CODE IN AHLSCAN            */ 00020000
*/********************************************************************/ 00021000
*/********************************************************************/ 00022000
*/*     INITIALIZE GTFPCT GTF STATUS FIELDS WITH GTF DEFAULT   Y02014*/ 00023000
*/*                       PARAMETER VALUES                           */ 00024000
*/********************************************************************/ 00025000
*                                                                  0084 00026000
*   GTFMODE='1'B;                   /* SET GTF MODE EXT        Y02014*/ 00027000
*   GTFDEBUG='0'B;                  /* SET DEBUG OFF           Y02014*/ 00028000
         OI    GTFMODE(PCTPTR),B'10000000'                         0085 00029000
         NI    GTFDEBUG(PCTPTR),B'10111111'                        0085 00030000
*   OPTTIME='0'B;                   /* SET NO TIMESTAMP        Y02014*/ 00031000
         NI    OPTTIME(PCTPTR),B'11111110'                         0086 00032000
*   HQLENGTH=10;                    /* SET DEFAULT BUF NUMBER  Y02014*/ 00033000
         MVC   HQLENGTH(4,PCTPTR),@CF00294                         0087 00034000
*                                                                  0088 00035000
*/********************************************************************/ 00036000
*/* CHECK CIBDATLN FIELD TO DETERMINE IF PARAMETERS WERE SPECIFIED   */ 00037000
*/* WITH THE START GTF COMMAND. IF NONE, THEN CHECK EXECUTE STATMENT */ 00038000
*/* PARAMETER FIELD FOR USER DEFAULT VALUES. THEN BUILD ECBLIST.     */ 00039000
*/********************************************************************/ 00040000
*                                                                  0088 00041000
*   IF CIBDATLN=ZEROS               /* IS CIB PARM FIELD BLANK       */ 00042000
*     THEN                          /* NO PARAMETERS FOUND IN CIB    */ 00043000
         L     @14,INITPTR(,PCTPTR)                                0088 00044000
         L     @14,CIBPTR(,@14)                                    0088 00045000
         LH    @03,CIBDATLN(,@14)                                  0088 00046000
         C     @03,ZEROS                                           0088 00047000
         BE    @RT00088                                            0088 00048000
*     GOTO PARMCHK;                 /* GO CHECK EXEC STMT            */ 00049000
*   PARMCTR=CIBDATLN;               /* INITIALIZE PARMCTR WITH LENGTH   00050000
*                                      OF PARM FIELD                 */ 00051000
         ST    @03,PARMCTR                                         0090 00052000
*   SCANPTR=ADDR(CIBDATA);          /* INITIALIZE SCANPTR WITH ADDR     00053000
*                                      OF CIB PARAMETER FIELD        */ 00054000
         LA    SCANPTR,CIBDATA(,@14)                               0091 00055000
*   GOTO LSTBLD;                    /* BRANCH TO BUILD ECBLIST       */ 00056000
         B     LSTBLD                                              0092 00057000
*                                                                  0093 00058000
*/********************************************************************/ 00059000
*/*  SCAN EXECUTE STATEMENT PARAMETER FIELD FOR VALID USER DEFAULT   */ 00060000
*/*             VALUES FOR GTF EXECUTION OPTIONS                     */ 00061000
*/********************************************************************/ 00062000
*                                                                  0093 00063000
*PARMCHK:                                                          0093 00064000
*   SCANPTR=DEFLPARM;               /* INITIALIZE SCANPTR WITH ADDR     00065000
*                                      OF PARM FIELD                 */ 00066000
PARMCHK  L     @14,INITPTR(,PCTPTR)                                0093 00067000
         L     @14,EXECPTR(,@14)                                   0093 00068000
         L     SCANPTR,DEFLPARM(,@14)                              0093 00069000
*   IF PARMLNG^=ZEROS               /* ANY USER DEFULT GTF PARM    0094 00070000
*                                      VALUES                        */ 00071000
*     THEN                          /* YES                           */ 00072000
         LH    @14,PARMLNG(,SCANPTR)                               0094 00073000
         C     @14,ZEROS                                           0094 00074000
         BE    @RF00094                                            0094 00075000
*     DO;                           /* UPDATE POINTERS               */ 00076000
*       PARMCTR=PARMLNG;            /* INITIALIZE PARMCTR WITH LENGTH   00077000
*                                      OF PARM FIELD                 */ 00078000
         ST    @14,PARMCTR                                         0096 00079000
*       SCANPTR=SCANPTR+TWO;        /* INITIALIZE SCANPTR WITH     0097 00080000
*                                      ADDRESS OF EXECUTE STATMENT 0097 00081000
*                                      PARAMETER FIELD               */ 00082000
         AH    SCANPTR,TWO                                         0097 00083000
*       GOTO LSTBLD;                /* BRANCH TO BUILD ECBLIST       */ 00084000
         B     LSTBLD                                              0098 00085000
*     END;                                                         0099 00086000
*   ELSE                            /* NO DEFAULT PARMS FOUND        */ 00087000
*     DO;                           /* INITIALIZE SEITCH             */ 00088000
@RF00094 DS    0H                                                  0101 00089000
*       DEFULTSW='10'X;             /* SET DEFAULT SWITCH ON       0101 00090000
*                                      INDICATING NO SCAN TO BE    0101 00091000
*                                      PREFORMED                     */ 00092000
         MVI   DEFULTSW,X'10'                                      0101 00093000
*     END;                                                         0102 00094000
*LSTBLD:                                                           0103 00095000
*   WTORECBL=ADDR(WTORECB);         /* ADDR OF WTOR ECB        Y02014*/ 00096000
LSTBLD   L     @14,INITPTR(,PCTPTR)                                0103 00097000
         LA    @14,WTORECB(,@14)                                   0103 00098000
         ST    @14,WTORECBL                                        0103 00099000
*   ECBSTOP=STOPECBA;               /* PUT ADDR OF STOP ECB IN     0104 00100000
*                                      ECBLIST                 Y02014*/ 00101000
         MVC   ECBSTOP(3),STOPECBA+1(PCTPTR)                       0104 00102000
*                                                                  0105 00103000
*/********************************************************************/ 00104000
*/*    DETERMINE IF DEFAULT OPTIONS WERE SELECTED, IF SO CALL        */ 00105000
*/*    AHLTCTL1 TO OBTAIN TRACE OPTIONS.  IF DEFAULT OPTIONS WERE    */ 00106000
*/*                NOT SELECTED GO SCAN PARAMETER FIELD              */ 00107000
*/********************************************************************/ 00108000
*                                                                  0105 00109000
*   IF DEFULTSW^='10'X              /* IS DEFULTSW SET TO INDICATE 0105 00110000
*                                      DEFAULT OPTIONS               */ 00111000
*     THEN                          /* NO                            */ 00112000
         CLI   DEFULTSW,16                                         0105 00113000
         BNE   @RT00105                                            0105 00114000
*     GOTO STARTSCN;                /* BRANCH TO BEGIN SCAN OF     0106 00115000
*                                      PARAMETER FIELD               */ 00116000
*   ELSE                            /* YES                           */ 00117000
*     GO TO ENDRETRN;               /* CALL AHLTCTL1 TO OBTAIN TRACE    00118000
*                                      OPTIONS AND EXIT.       Y02014*/ 00119000
*                                                                  0107 00120000
         B     ENDRETRN                                            0107 00121000
*/********************************************************************/ 00122000
*/* FOLLOWING CODE IS THE SCANNING ROUTINE WHICH WILL DETERMINE WHICH*/ 00123000
*/* OPTIONS HAVE BEEN REQUESTED BY THE USER.  THE GTFPCT  WILL       */ 00124000
*/* BE INITIALIZED WITH THE OPTION SELECTIONS                        */ 00125000
*/********************************************************************/ 00126000
*                                                                  0108 00127000
*STARTSCN:                                                         0108 00128000
*   KEYWDPTR=ADDR(KYWRDTAB);        /* INITIALIZE KEYWDPTR WITH ADDR    00129000
*                                      OF KEYWORD TABLE              */ 00130000
*                                                                  0108 00131000
STARTSCN LA    KEYWDPTR,KYWRDTAB                                   0108 00132000
*/********************************************************************/ 00133000
*/* THIS CODE IS USED TO LOOP THROUGH THE KEYWORD TABLE AND COMPARE  */ 00134000
*/* EACH KEYWORD ENTRY IN THE KEYWORD TABLE AGAINST KEYWORD BEING    */ 00135000
*/* SCANNED IN THE PARAMETER FIELD. WHEN A MATCH IS FOUND A BRANCH IS*/ 00136000
*/* TAKEN TO THE CORRESPONDING SUBROUTINE TO DETERMINE IF VALID.     */ 00137000
*/********************************************************************/ 00138000
*                                                                  0109 00139000
*KYWDCOMP:                                                         0109 00140000
*   DO I=ONE TO FOUR;               /* LOOP THROUGH KEYWORD TABLE    */ 00141000
*                                                                  0109 00142000
KYWDCOMP LH    I,ONE                                               0109 00143000
         B     @DE00109                                            0109 00144000
@DL00109 DS    0H                                                  0110 00145000
*/********************************************************************/ 00146000
*/* COMPARE KEYWORDS IN TABLE AND PARMFIELD FOR MATCH                */ 00147000
*/********************************************************************/ 00148000
*                                                                  0110 00149000
*     /***************************************************************/ 00150000
*     /*                                                             */ 00151000
*     /* DOES KEYWORD MATCH                                          */ 00152000
*     /*                                                             */ 00153000
*     /***************************************************************/ 00154000
*                                                                  0110 00155000
*     IF KYWORDS(ONE:KEYWDLN)=COMPAREA(ONE:KEYWDLN)THEN            0110 00156000
         LH    @14,ONE                                             0110 00157000
         LH    @07,KEYWDLN(,KEYWDPTR)                              0110 00158000
         SR    @07,@14                                             0110 00159000
         ST    @14,@TF00001                                        0110 00160000
         ALR   @14,KEYWDPTR                                        0110 00161000
         LR    @03,SCANPTR                                         0110 00162000
         AL    @03,@TF00001                                        0110 00163000
         AL    @03,@CF01201                                        0110 00164000
         EX    @07,@SC01202                                        0110 00165000
         BE    @RT00110                                            0110 00166000
*       GOTO OPSCAN;                /* BRANCH TO BEGIN OPTION VALUE     00167000
*                                      SCAN                          */ 00168000
*     KEYWDPTR=KEYWDPTR+TWELVE;     /* INCREMENT PTR TO NEXT KEYWORD    00169000
*                                      ENTRY IN TABLE                */ 00170000
         AH    KEYWDPTR,TWELVE                                     0112 00171000
*   END;                            /* BRANCH TO COMPARE NEXT KEY  0113 00172000
*                                      WORD IN TABLE                 */ 00173000
         AH    I,@CH00112                                          0113 00174000
@DE00109 CH    I,FOUR                                              0113 00175000
         BNH   @DL00109                                            0113 00176000
*   MSGPTR=ADDR(AHL001);            /* INITIALIZE MSGPTR WITH ADDR OF   00177000
*                                      WTOR AHL001A            Y02014*/ 00178000
         L     MSGPTR,@CV01076                                     0114 00179000
*   GOTO SCANERR;                   /* BRANCH TO PROCESS ERROR     0115 00180000
*                                      CONDITION                     */ 00181000
         B     SCANERR                                             0115 00182000
*                                                                  0116 00183000
*/********************************************************************/ 00184000
*/*  THIS CODE CHECKS FOR THE EQUAL SIGN DELIMITER IN THE PARM FIELD */ 00185000
*/********************************************************************/ 00186000
*                                                                  0116 00187000
*OPSCAN:                                                           0116 00188000
*   SCANPTR=SCANPTR+KEYWDLN;        /* INCREMENT SCANPTR BY LNG OF 0116 00189000
*                                      KEYWORD MATCHED               */ 00190000
OPSCAN   LH    @14,KEYWDLN(,KEYWDPTR)                              0116 00191000
         AR    SCANPTR,@14                                         0116 00192000
*   PARMCTR=PARMCTR-KEYWDLN;        /* DECREMENT PARMCTR BY LNG OF 0117 00193000
*                                      KEYWORD                       */ 00194000
         LCR   @14,@14                                             0117 00195000
         A     @14,PARMCTR                                         0117 00196000
         ST    @14,PARMCTR                                         0117 00197000
*   OPTABPTR=OPADDR;                /* ADDR OF OPTION TABLE          */ 00198000
         SR    OPTABPTR,OPTABPTR                                   0118 00199000
         ICM   OPTABPTR,7,OPADDR(KEYWDPTR)                         0118 00200000
*COMPEQL:                                                          0119 00201000
*   IF DELIMEQL^=COMPAREA(ONE)      /* IS DELIMATOR EQUAL SIGN       */ 00202000
*     THEN                          /* NO                            */ 00203000
COMPEQL  LH    @14,ONE                                             0119 00204000
         ALR   @14,SCANPTR                                         0119 00205000
         AL    @14,@CF01201                                        0119 00206000
         CLC   DELIMEQL(1),COMPAREA(@14)                           0119 00207000
         BE    @RF00119                                            0119 00208000
*     DO;                           /* SET UP ERROR MESSAGE          */ 00209000
*       MSGPTR=ADDR(AHL002);        /* INITIALIZE MSGPTR WITH ADDR OF   00210000
*                                      WTOR AHL002A            Y02014*/ 00211000
*                                                                  0121 00212000
         L     MSGPTR,@CV01077                                     0121 00213000
*/********************************************************************/ 00214000
*/* BRANCH TO SCANERR ROUTINE ISSUE WTOR AHL002A INDICATING INVALID  */ 00215000
*/* DELIMATOR FOUND IN PARAMETER FIELD                               */ 00216000
*/********************************************************************/ 00217000
*                                                                  0122 00218000
*       GOTO SCANERR;               /* BRANCH TO ISSUE WTOR          */ 00219000
         B     SCANERR                                             0122 00220000
*     END;                                                         0123 00221000
*                                                                  0124 00222000
*/********************************************************************/ 00223000
*/* DETERMINE WHICH KEYWORD WAS MATCHED, CHECK TO SEE IF THIS KEYWORD*/ 00224000
*/* HAS BEEN REPEATED. IF REPEATED ISSUE ERROR MESSAGE. IF KEYWORD   */ 00225000
*/* WAS NOT REPEATED BRANCH TO PROCESS OPTIONS ASSOCIATED WITH       */ 00226000
*/* KEYWORD                                                          */ 00227000
*/********************************************************************/ 00228000
*                                                                  0124 00229000
*   SCANPTR=SCANPTR+ONE;            /* INCREMENT SCANPTR BY LNG OF 0124 00230000
*                                      DELIMATOR                     */ 00231000
@RF00119 LH    @14,ONE                                             0124 00232000
         AR    SCANPTR,@14                                         0124 00233000
*   PARMCTR=PARMCTR-ONE;            /* DECREMENT PARMCTR BY LNG OF 0125 00234000
*                                      DELIMETER                     */ 00235000
         L     @07,PARMCTR                                         0125 00236000
         SR    @07,@14                                             0125 00237000
         ST    @07,PARMCTR                                         0125 00238000
*   IF I^=ONE                       /* WAS KEYWORD MODE MATCHED      */ 00239000
*     THEN                          /* NO                            */ 00240000
         CR    I,@14                                               0126 00241000
         BNE   @RT00126                                            0126 00242000
*     GOTO DEBUGCHK;                /* GO CHECK FOR DEBUG KEYWORD  0127 00243000
*                                      MATCH                         */ 00244000
*   IF MODEBIT='0'B                 /* WAS KEYWORD REPEATED          */ 00245000
*     THEN                          /* NO                            */ 00246000
         TM    MODEBIT,B'10000000'                                 0128 00247000
         BZ    @RT00128                                            0128 00248000
*     GOTO MODEOPS;                 /* GO PROCESS MODE OPTIONS       */ 00249000
*   ELSE                            /* YES                           */ 00250000
*     GOTO REPERROR;                /* GOTO RTN TO INITIALIZE PTR  0130 00251000
*                                      BEFORE BRANCHING TO SCANERR TO   00252000
*                                      ISSUE WTOR                    */ 00253000
         B     REPERROR                                            0130 00254000
*DEBUGCHK:                                                         0131 00255000
*   IF I^=TWO                       /* WAS KEYWORD DEBUG MATCHED     */ 00256000
*     THEN                          /* NO                            */ 00257000
DEBUGCHK CH    I,TWO                                               0131 00258000
         BNE   @RT00131                                            0131 00259000
*     GOTO TIMECHK;                 /* GO CHECK FOR TIME KEYOWRD   0132 00260000
*                                      MATCH                         */ 00261000
*   IF DEBUGBIT='0'B                /* WAS KEYWORD REPEATED          */ 00262000
*     THEN                          /* NO                            */ 00263000
         TM    DEBUGBIT,B'01000000'                                0133 00264000
         BZ    @RT00133                                            0133 00265000
*     GOTO DEBUGOPS;                /* GO PROCESS DEBUG OPTION       */ 00266000
*   ELSE                            /* YES                           */ 00267000
*     GOTO REPERROR;                /* GOTO SUBRTN TO INITIALIZE PTRS   00268000
*                                      BEFORE BRANCHING TO SCANERR TO   00269000
*                                      ISSUE WTOR                    */ 00270000
         B     REPERROR                                            0135 00271000
*TIMECHK:                                                          0136 00272000
*   IF I^=THREE                     /* WAS TIME KEY WORD MATCHED     */ 00273000
*     THEN                          /* YES                           */ 00274000
TIMECHK  CH    I,THREE                                             0136 00275000
         BNE   @RT00136                                            0136 00276000
*     GOTO BUFCHK;                  /* GO CHECK FOR BUF KEYWORD      */ 00277000
*   IF TIMEBIT='0'B                 /* WAS KEYWORD REPEATED          */ 00278000
*     THEN                          /* NO                            */ 00279000
         TM    TIMEBIT,B'00100000'                                 0138 00280000
         BZ    @RT00138                                            0138 00281000
*     GOTO TIMEOPS;                 /* GO PROCESS TIME OPTIONS OF  0139 00282000
*                                      WTOR REPLY AREA               */ 00283000
*   ELSE                            /* GO ISSUE ERROR MSG      Y02014*/ 00284000
*     GOTO REPERROR;                /* FOR REPEATING KWD       Y02014*/ 00285000
         B     REPERROR                                            0140 00286000
*BUFCHK:                                                           0141 00287000
*   IF BUFBIT='0'B                  /* WAS BUF KEYWORD REPEATED      */ 00288000
*     THEN                          /* NO                            */ 00289000
BUFCHK   TM    BUFBIT,B'00010000'                                  0141 00290000
         BZ    @RT00141                                            0141 00291000
*     GOTO BUFSCAN;                 /* GOTO TO SCAN BUF VALUE        */ 00292000
*REPERROR:                                                         0143 00293000
*   MSGPTR=ADDR(AHL004);            /* INITIALIZE MSGPTR WITH ADDR OF   00294000
*                                      WTOR AHL004A                  */ 00295000
REPERROR L     MSGPTR,@CV01079                                     0143 00296000
*   GOTO SCANERR;                   /* BRANCH TO PROCESS ERROR       */ 00297000
         B     SCANERR                                             0144 00298000
*                                                                  0145 00299000
*/********************************************************************/ 00300000
*/* THIS SECTION OF CODE SCANS THE PARAMETER FIELD FOR VALID MODE    */ 00301000
*/* OPTIONS                                                          */ 00302000
*/********************************************************************/ 00303000
*                                                                  0145 00304000
*MODEOPS:                                                          0145 00305000
*                                                                  0145 00306000
*/********************************************************************/ 00307000
*/* COMPARE FOR 'EXT' OPTION VALUE                                   */ 00308000
*/********************************************************************/ 00309000
*                                                                  0145 00310000
*   /*****************************************************************/ 00311000
*   /*                                                               */ 00312000
*   /* IS OPTION VALUE - EXT                                         */ 00313000
*   /*                                                               */ 00314000
*   /*****************************************************************/ 00315000
*                                                                  0145 00316000
*   IF OPTVALUE(ONE:OPLENGTH)=COMPAREA(ONE:OPLENGTH)THEN/* YES       */ 00317000
MODEOPS  LH    @14,ONE                                             0145 00318000
         LH    @05,OPLENGTH(,OPTABPTR)                             0145 00319000
         SR    @05,@14                                             0145 00320000
         ST    @14,@TF00001                                        0145 00321000
         ALR   @14,OPTABPTR                                        0145 00322000
         LR    @01,SCANPTR                                         0145 00323000
         AL    @01,@TF00001                                        0145 00324000
         AL    @01,@CF01201                                        0145 00325000
         EX    @05,@SC01204                                        0145 00326000
         BE    @RT00145                                            0145 00327000
*     GOTO ENDMODE;                 /* GOTO INITIALIZE FIRTIMSW      */ 00328000
*   OPTABPTR=OPTABPTR+TEN;          /* INCREMENT OPTABPTR TO NEXT  0147 00329000
*                                      ENTRY IN OPTION TABLE         */ 00330000
*                                                                  0147 00331000
         AH    OPTABPTR,TEN                                        0147 00332000
*/********************************************************************/ 00333000
*/* COMPARE FOR 'INT' OPTION VALUE                                   */ 00334000
*/********************************************************************/ 00335000
*                                                                  0148 00336000
*   /*****************************************************************/ 00337000
*   /*                                                               */ 00338000
*   /* IS OPTION VALUE - INT                                         */ 00339000
*   /*                                                               */ 00340000
*   /*****************************************************************/ 00341000
*                                                                  0148 00342000
*   IF OPTVALUE(ONE:OPLENGTH)=COMPAREA(ONE:OPLENGTH)THEN/* YES       */ 00343000
         LH    @14,OPLENGTH(,OPTABPTR)                             0148 00344000
         S     @14,@TF00001                                        0148 00345000
         LR    @05,OPTABPTR                                        0148 00346000
         AL    @05,@TF00001                                        0148 00347000
         LR    @01,SCANPTR                                         0148 00348000
         AL    @01,@TF00001                                        0148 00349000
         AL    @01,@CF01201                                        0148 00350000
         EX    @14,@SC01206                                        0148 00351000
         BNE   @RF00148                                            0148 00352000
*     DO;                                                          0149 00353000
*       GTFMODE='0'B;               /* SET MODE INT IN PCT     Y02014*/ 00354000
         NI    GTFMODE(PCTPTR),B'01111111'                         0150 00355000
*       GOTO ENDMODE;               /* GOTO INITIALIZE FIRTIMSW      */ 00356000
         B     ENDMODE                                             0151 00357000
*     END;                                                         0152 00358000
*   ELSE                                                           0153 00359000
*     GO TO OPTERR;                 /* PROCESS ERROR           Y02014*/ 00360000
*ENDMODE:                                                          0154 00361000
*   MODEBIT='1'B;                   /* SET SWITCH INDICATING MODE  0154 00362000
*                                      KEYWORD HAS BEEN PROCESSED    */ 00363000
ENDMODE  OI    MODEBIT,B'10000000'                                 0154 00364000
*   GOTO OPSEND;                    /* GOTO INITIALIZE PTRS          */ 00365000
         B     OPSEND                                              0155 00366000
*                                                                  0156 00367000
*/********************************************************************/ 00368000
*/* THIS SECTION SCANS THE PARAMETER FIELD FOR VALID DEBUG OPTION(S) */ 00369000
*/********************************************************************/ 00370000
*                                                                  0156 00371000
*DEBUGOPS:                                                         0156 00372000
*                                                                  0156 00373000
*/********************************************************************/ 00374000
*/* COMPARE FOR 'NO' OPTION VALUE                                    */ 00375000
*/********************************************************************/ 00376000
*                                                                  0156 00377000
*   /*****************************************************************/ 00378000
*   /*                                                               */ 00379000
*   /* IS OPTION VALUE - NO                                          */ 00380000
*   /*                                                               */ 00381000
*   /*****************************************************************/ 00382000
*                                                                  0156 00383000
*   IF OPTVALUE(ONE:OPLENGTH)=COMPAREA(ONE:OPLENGTH)THEN/* YES       */ 00384000
DEBUGOPS LH    @14,ONE                                             0156 00385000
         LH    @05,OPLENGTH(,OPTABPTR)                             0156 00386000
         SR    @05,@14                                             0156 00387000
         ST    @14,@TF00001                                        0156 00388000
         ALR   @14,OPTABPTR                                        0156 00389000
         LR    @01,SCANPTR                                         0156 00390000
         AL    @01,@TF00001                                        0156 00391000
         AL    @01,@CF01201                                        0156 00392000
         EX    @05,@SC01204                                        0156 00393000
         BE    @RT00156                                            0156 00394000
*     GOTO ENDDEBUG;                /* GO INITIALIZE FIRTIMSW        */ 00395000
*   OPTABPTR=OPTABPTR+TEN;          /* INCREMENT PTR TO NEXT ENTRY IN   00396000
*                                      THE TABLE                     */ 00397000
*                                                                  0158 00398000
         AH    OPTABPTR,TEN                                        0158 00399000
*/********************************************************************/ 00400000
*/* COMPARE FOR 'YES' OPTION VALUE                                   */ 00401000
*/********************************************************************/ 00402000
*                                                                  0159 00403000
*   /*****************************************************************/ 00404000
*   /*                                                               */ 00405000
*   /* IS OPTION VALUE - YES                                         */ 00406000
*   /*                                                               */ 00407000
*   /*****************************************************************/ 00408000
*                                                                  0159 00409000
*   IF OPTVALUE(ONE:OPLENGTH)=COMPAREA(ONE:OPLENGTH)THEN/* YES       */ 00410000
         LH    @14,OPLENGTH(,OPTABPTR)                             0159 00411000
         S     @14,@TF00001                                        0159 00412000
         LR    @05,OPTABPTR                                        0159 00413000
         AL    @05,@TF00001                                        0159 00414000
         LR    @01,SCANPTR                                         0159 00415000
         AL    @01,@TF00001                                        0159 00416000
         AL    @01,@CF01201                                        0159 00417000
         EX    @14,@SC01206                                        0159 00418000
         BNE   @RF00159                                            0159 00419000
*     DO;                           /* INITIALIZE HCT                */ 00420000
*       GTFDEBUG='1'B;              /* INITIAL GTFPCT TO INDICATE  0161 00421000
*                                      DEBUG OPTION = YES      Y02014*/ 00422000
         OI    GTFDEBUG(PCTPTR),B'01000000'                        0161 00423000
*     END;                                                         0162 00424000
*   ELSE                            /* NO VALID DEBUG OPTIONS- ERROR */ 00425000
*     GOTO OPTERR;                  /* GO TO ERROR RTN               */ 00426000
*ENDDEBUG:                                                         0164 00427000
*   DEBUGBIT='1'B;                  /* SET SWITCH INDICATING DEBUG 0164 00428000
*                                      KEYWORD HAS BEEN PROCESSED    */ 00429000
ENDDEBUG OI    DEBUGBIT,B'01000000'                                0164 00430000
*   GOTO OPSEND;                    /* GOTO INITIALIZE PTRS          */ 00431000
         B     OPSEND                                              0165 00432000
*                                                                  0166 00433000
*/********************************************************************/ 00434000
*/* THIS SECTION SCANS THE PARAMETER FIELD FOR VALID TIME OPTION(S)  */ 00435000
*/********************************************************************/ 00436000
*                                                                  0166 00437000
*TIMEOPS:                                                          0166 00438000
*                                                                  0166 00439000
*/********************************************************************/ 00440000
*/* COMPARE FOR 'NO' OPTION VALUE                                    */ 00441000
*/********************************************************************/ 00442000
*                                                                  0166 00443000
*   /*****************************************************************/ 00444000
*   /*                                                               */ 00445000
