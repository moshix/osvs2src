*   END;                            /* WAIT (1) ECBLIST(ECBLIST)     */ 00849000
*                                   /* ISSUE WAIT FOR REPLY OR STOP     00850000
*                                      Y02014                        */ 00851000
*                                                                  0262 00852000
*/********************************************************************/ 00853000
*/* TEST TO DETERMINE WHICH ECB WAS POSTED. IF WTOR ECB WAS POSTED   */ 00854000
*/* THEN INITIALIZE DEFAULT VALUES IN THE BYTE INDICATOR FIELDS IN   */ 00855000
*/* THE PCT, THEN SCAN WTOR REPLY. IF STOP COMMAND WAS ISSUED THEN   */ 00856000
*/* EXIT TO AHLGTFI.                                                 */ 00857000
*/********************************************************************/ 00858000
*                                                                  0262 00859000
*   ECBPTR=ADDR(WTORECB);           /* INITIALIZE ECB PTR WITH ADDR     00860000
*                                      OF WTOR ECB                   */ 00861000
         L     ECBPTR,INITPTR(,PCTPTR)                             0262 00862000
         LA    ECBPTR,WTORECB(,ECBPTR)                             0262 00863000
*   IF POSTBIT^='1'B                /* WAS WTOR ECB POSTED           */ 00864000
*     THEN                          /* NO                            */ 00865000
         TM    POSTBIT(ECBPTR),B'01000000'                         0263 00866000
         BNO   @RT00263                                            0263 00867000
*     GO TO STOPRTN;                /* STOP ISSUED             Y02014*/ 00868000
*                                                                  0265 00869000
*/********************************************************************/ 00870000
*/* WRITE  REPLY TO PROGRAMMER                                 Y02014*/ 00871000
*/********************************************************************/ 00872000
*                                                                  0265 00873000
*   AHLRPLA=AHLRPLA|ORBLANKS;       /* MAKE REPLY CAPITALS     Y02014*/ 00874000
         L     @14,@CA01083                                        0265 00875000
         OC    AHLRPLA-AHLRPL(123,@14),ORBLANKS                    0265 00876000
*   DO;                             /* WTOP MF(E,AHLRPL)             */ 00877000
*     RFY                                                          0267 00878000
*       GPR01P RSTD;                /* RESTRICT REG 1                */ 00879000
*     GPR01P=ADDR(AHLRPL);          /* ADDRESS CONTROL PROGRAM LIST  */ 00880000
         LR    GPR01P,@14                                          0268 00881000
*     SVC(35);                      /* ISSUE WTO SVC                 */ 00882000
         SVC   35                                                  0269 00883000
*     RFY                                                          0270 00884000
*       GPR01P UNRSTD;              /* RELEASE REG 1                 */ 00885000
*   END;                            /* WTOP MF(E,AHLRPL)             */ 00886000
*                                   /* EXECUTE WTP Y02014            */ 00887000
*                                                                  0272 00888000
*/********************************************************************/ 00889000
*/*     INITIALIZE PCT WITH GTF DEFAULT PARM VALUES            Y02014*/ 00890000
*/********************************************************************/ 00891000
*                                                                  0272 00892000
*   GTFMODE='1'B;                   /* SET GTF MODE TO EXT     Y02014*/ 00893000
*   GTFDEBUG='0'B;                  /* SET DEBUG =NO           Y02014*/ 00894000
         OI    GTFMODE(PCTPTR),B'10000000'                         0273 00895000
         NI    GTFDEBUG(PCTPTR),B'10111111'                        0273 00896000
*   OPTTIME='0'B;                   /* SET NO TIMESTAMPING     Y02014*/ 00897000
         NI    OPTTIME(PCTPTR),B'11111110'                         0274 00898000
*   HQLENGTH=10;                    /* SET HISTORY QUEUE LENGTH TO 0275 00899000
*                                      DEFAULT NUMOF BUFFERS   Y02014*/ 00900000
         MVC   HQLENGTH(4,PCTPTR),@CF00294                         0275 00901000
*                                                                  0276 00902000
*/********************************************************************/ 00903000
*/* DETERMINE IF REPLY IS 'U' INDICATING THAT GTF DEFAULT VALUES     */ 00904000
*/* BE USED, IF SO CALL AHLCTL1 TO GET TRACE OPTIONS.          Y02014*/ 00905000
*/* IF 'U' WAS NOT REPLY SCAN WTOR REPLY AREA FOR VALID KEYWORDS     */ 00906000
*/* AND OPTIONS.                                                     */ 00907000
*/********************************************************************/ 00908000
*                                                                  0276 00909000
*   IF COMPAREA(ONE)=REPLYU         /* IS REPLY VALUE 'U'            */ 00910000
*     THEN                          /* YES                           */ 00911000
         LH    @14,ONE                                             0276 00912000
         ALR   @14,SCANPTR                                         0276 00913000
         AL    @14,@CF01201                                        0276 00914000
         CLC   COMPAREA(1,@14),REPLYU                              0276 00915000
         BE    @RT00276                                            0276 00916000
*     GO TO ENDRETRN;               /* GO TO AHLTCTL1 TO PROCESS   0277 00917000
*                                      TRACE OPTIONS.          Y02014*/ 00918000
*   ELSE                            /* NO                            */ 00919000
*     DO;                           /* WTOR REPLY HAS TO BE SCANNED  */ 00920000
*       PARMCTR=REPLYLNG;           /* MAX LNG TO BE SCANNED         */ 00921000
         LH    @14,REPLYLNG                                        0279 00922000
         ST    @14,PARMCTR                                         0279 00923000
*       WTORSCAN=WTORSCAN|'10'X;    /* INIITALIZE WTORSCAN TO IN-  0280 00924000
*                                      DICATE SCAN OF WTOR REPLY AREA*/ 00925000
         LA    @14,16                                              0280 00926000
         SR    @05,@05                                             0280 00927000
         IC    @05,WTORSCAN                                        0280 00928000
         OR    @14,@05                                             0280 00929000
         STC   @14,WTORSCAN                                        0280 00930000
*       FIRTIMSW=FIRTIMSW&'00'X;    /* SET FIRTIMSW TO ZERO          */ 00931000
         NI    FIRTIMSW,X'00'                                      0281 00932000
*       ECBPTR=ADDR(WTORECB);       /* ADDR OF WTOR ECB FOR        0282 00933000
*                                      ADDRESSABILITY                */ 00934000
         L     ECBPTR,INITPTR(,PCTPTR)                             0282 00935000
         LA    ECBPTR,WTORECB(,ECBPTR)                             0282 00936000
*       POSTBIT='0'B;               /* SET WTOR ECB POST BIT TO ZERO */ 00937000
         NI    POSTBIT(ECBPTR),B'10111111'                         0283 00938000
*       GOTO STARTSCN;              /* GO TO BEGIN SCANNING THE WTOR    00939000
*                                      REPLY AREA                    */ 00940000
         B     STARTSCN                                            0284 00941000
*     END;                                                         0285 00942000
*                                                                  0286 00943000
*/********************************************************************/ 00944000
*/* THE ENDSCAN SUBROUTINE DETERMINES IF 1. A STOP WAS ISSUED,       */ 00945000
*/* 2. THE END OF THE PARAMETER HAS BEEN REACHED, 3. IF MORE PARMS   */ 00946000
*/* FOLLOW IN THE PARM FIELD, 4. IF AN ERROR CONDITION WAS REACHED   */ 00947000
*/********************************************************************/ 00948000
*                                                                  0286 00949000
*ENDSCAN:                                                          0286 00950000
*   ECBPTR=STMODECB;                /* ADDR OF STOP/MODIFY ECB       */ 00951000
ENDSCAN  L     ECBPTR,STMODECB                                     0286 00952000
*   IF POSTBIT='1'B                 /* IS STOP ECB POSTED            */ 00953000
*     THEN                          /* YES                           */ 00954000
         TM    POSTBIT(ECBPTR),B'01000000'                         0287 00955000
         BO    @RT00287                                            0287 00956000
*     GOTO STOPRTN;                 /* GO SET UP RETURN CODE TO    0288 00957000
*                                      TERMINATE GTF           Y02014*/ 00958000
*                                                                  0288 00959000
*/********************************************************************/ 00960000
*/*          DETERMINE IF SCANING WOTR - REPLY AREA                  */ 00961000
*/********************************************************************/ 00962000
*                                                                  0289 00963000
*   IF WTORSCAN='10'X               /* IS WTOR REPLY AREA BEING    0289 00964000
*                                      SCANNED                       */ 00965000
*     THEN                          /* YES                           */ 00966000
         CLI   WTORSCAN,16                                         0289 00967000
         BNE   @RF00289                                            0289 00968000
*     DO;                           /* CHECK FOR BLANK               */ 00969000
*       IF COMPAREA(ONE)=' '        /* IS BLANK DELIMITER PRESENT  0291 00970000
*                                      INDICATING END OF PARM FIELD     00971000
*                                      IN WTOR REPLY AREA            */ 00972000
*         THEN                      /* YES                           */ 00973000
         LH    @14,ONE                                             0291 00974000
         ALR   @14,SCANPTR                                         0291 00975000
         AL    @14,@CF01201                                        0291 00976000
         CLI   COMPAREA(@14),C' '                                  0291 00977000
         BNE   @RF00291                                            0291 00978000
*         DO;                       /* SCAN TILL END OF PARM FIELD 0292 00979000
*                                      FOR BLANKS                    */ 00980000
*           PARMCTR=PARMCTR-ONE;    /* ADJUST LOOP COUNT             */ 00981000
         LH    @14,ONE                                             0293 00982000
         LCR   @14,@14                                             0293 00983000
         A     @14,PARMCTR                                         0293 00984000
         ST    @14,PARMCTR                                         0293 00985000
*                                                                  0294 00986000
*/********************************************************************/ 00987000
*/*   SCAN TILL END OF PARM FIELD TO VERIFY END OF PARMS             */ 00988000
*/********************************************************************/ 00989000
*                                                                  0294 00990000
*           DO I=PARMCTR TO ONE BY-ONE;/* LOOP CONTROL         Y02014*/ 00991000
         LR    I,@14                                               0294 00992000
         B     @DE00294                                            0294 00993000
@DL00294 DS    0H                                                  0295 00994000
*             SCANPTR=SCANPTR+ONE;  /* POINT TO NEXT BYTE            */ 00995000
         LH    @14,ONE                                             0295 00996000
         AR    SCANPTR,@14                                         0295 00997000
*             IF COMPAREA(ONE)^=' ' /* TEST FOR NONBLANK             */ 00998000
*               THEN                /* NONBLANK FOUND ERROR          */ 00999000
         ALR   @14,SCANPTR                                         0296 01000000
         AL    @14,@CF01201                                        0296 01001000
         CLI   COMPAREA(@14),C' '                                  0296 01002000
         BNE   @RT00296                                            0296 01003000
*               GOTO DELIMMSG;      /* BRANCH TO PROCESS ERROR       */ 01004000
*           END;                    /* LOOP TO CHECK NEXT BYTE       */ 01005000
         LH    @14,ONE                                             0298 01006000
         LCR   @14,@14                                             0298 01007000
         AR    I,@14                                               0298 01008000
@DE00294 CH    I,ONE                                               0298 01009000
         BNL   @DL00294                                            0298 01010000
*           GOTO ENDRETRN;          /* GO TO AHLCTL1 TO PROCESS TRACE   01011000
*                                      OPTIONS                 Y02014*/ 01012000
         B     ENDRETRN                                            0299 01013000
*         END;                                                     0300 01014000
*       ELSE                        /* NO                            */ 01015000
*         GOTO COMCHECK;            /* BRANCH TO CHECK FOR COMMA     */ 01016000
*     END;                                                         0302 01017000
*                                                                  0302 01018000
*/********************************************************************/ 01019000
*/*  DETERMINE IF HAVE REACHED END OF START COMMAND PARAMETER FIELD  */ 01020000
*/********************************************************************/ 01021000
*                                                                  0303 01022000
*   IF PARMCTR=ZEROS                /* IS PARMCTR ZERO INDICAING     */ 01023000
*                                   /* END OF PARM FIELD             */ 01024000
*     THEN                          /* YES                           */ 01025000
@RF00289 CLC   PARMCTR(4),ZEROS                                    0303 01026000
         BE    @RT00303                                            0303 01027000
*     GOTO ENDRETRN;                /* PROCESS TRACE OPTIONS   Y02014*/ 01028000
*                                                                  0305 01029000
*/********************************************************************/ 01030000
*/*      CHECK FOR COMMA INDICATING MORE PARAMETERS TO FOLLOW        */ 01031000
*/********************************************************************/ 01032000
*                                                                  0305 01033000
*COMCHECK:                                                         0305 01034000
*   IF COMPAREA(ONE)=DELIMCOM       /* IS COMMA PRESENT              */ 01035000
*     THEN                          /* YES                           */ 01036000
COMCHECK LH    @14,ONE                                             0305 01037000
         ALR   @14,SCANPTR                                         0305 01038000
         AL    @14,@CF01201                                        0305 01039000
         CLC   COMPAREA(1,@14),DELIMCOM                            0305 01040000
         BNE   @RF00305                                            0305 01041000
*     DO;                           /* MORE KEYWORD(S) FOLLOW IN PARM   01042000
*                                      FIELD                         */ 01043000
*       SCANPTR=SCANPTR+ONE;        /* INCREMENT SCANPTR BY LNG OF 0307 01044000
*                                      DELIMITER                     */ 01045000
         LH    @14,ONE                                             0307 01046000
         AR    SCANPTR,@14                                         0307 01047000
*       PARMCTR=PARMCTR-ONE;        /* DECREMENT PARMCTR BY LNG OF 0308 01048000
*                                      DELIMITER                     */ 01049000
         LCR   @14,@14                                             0308 01050000
         A     @14,PARMCTR                                         0308 01051000
         ST    @14,PARMCTR                                         0308 01052000
*       GOTO STARTSCN;              /* BRANCH TO SCAN NEXT KEYWORD IN   01053000
*                                      PARM FIELD                    */ 01054000
         B     STARTSCN                                            0309 01055000
*     END;                                                         0310 01056000
*   ELSE                            /* NO                            */ 01057000
*     DO;                           /* COMMA WAS NOT FOUND           */ 01058000
@RF00305 DS    0H                                                  0312 01059000
*DELIMMSG:                                                         0312 01060000
*       MSGPTR=ADDR(AHL002);        /* INVALID DELIMITER FOUND -   0312 01061000
*                                      ISSUE ERROR MESSAGE     Y02014*/ 01062000
DELIMMSG L     MSGPTR,@CV01077                                     0312 01063000
*       GOTO SCANERR;               /* BRANCH TO PROCESS ERROR       */ 01064000
         B     SCANERR                                             0313 01065000
*     END;                                                         0314 01066000
*ENDRETRN:                                                         0315 01067000
*   AHLRPLA='';                     /* BLANK OUT REPLY AREA    Y02014*/ 01068000
ENDRETRN L     @14,@CA01083                                        0315 01069000
         MVI   AHLRPLA-AHLRPL(@14),C' '                            0315 01070000
         MVC   AHLRPLA-AHLRPL+1(122,@14),AHLRPLA-AHLRPL(@14)       0315 01071000
*   CALL AHLTCTL1;                  /* CALL AHLTCTL1 TO PROCESS TRACE   01072000
*                                      OPTIONS                 Y02014*/ 01073000
         L     @15,@CV01114                                        0316 01074000
         BALR  @14,@15                                             0316 01075000
*   GPRS15=RETCODE;                 /* SET RETURN CODE         Y02014*/ 01076000
         L     @14,CALLRSA1(,SAVEP)                                0317 01077000
         ST    RETCODE,GPRS15(,@14)                                0317 01078000
*   RETURN;                         /* RETURN TO AHLGTFI       Y02014*/ 01079000
@EL00001 L     @13,4(,@13)                                         0318 01080000
@EF00001 DS    0H                                                  0318 01081000
@ER00001 LM    @14,@12,12(@13)                                     0318 01082000
         BR    @14                                                 0318 01083000
*STOPRTN:                                                          0319 01084000
*   GPRS15=TWELVE;                  /* SET RETURN CODE TO 12 TO    0319 01085000
*                                      INDICATE THAT STOP GTF COMMAND   01086000
*                                      WAS ISSUED.             Y02014*/ 01087000
STOPRTN  L     @14,CALLRSA1(,SAVEP)                                0319 01088000
         LH    @03,TWELVE                                          0319 01089000
         ST    @03,GPRS15(,@14)                                    0319 01090000
*   RETURN;                         /* RETURN TO AHLGTFI TO TERMINATE   01091000
*                                      GTF.                    Y02014*/ 01092000
*                                                                  0320 01093000
         B     @EL00001                                            0320 01094000
*/********************************************************************/ 01095000
*/* AHLSCAN RETURN LINKAGE TO AHLGTF                                 */ 01096000
*/********************************************************************/ 01097000
*                                                                  0321 01098000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 01099000
*     GPR00P PTR(31)REG(0),                                        0321 01100000
*     GPR01P PTR(31)REG(1);                                        0321 01101000
*   DECLARE                         /* COMMON VARIABLES              */ 01102000
*     I256C CHAR(256)BASED,                                        0322 01103000
*     I031F FIXED(31)BASED,                                        0322 01104000
*     I031P PTR(31)BASED,                                          0322 01105000
*     I015F FIXED(15)BASED,                                        0322 01106000
*     I015P PTR(15)BASED,                                          0322 01107000
*     I008P PTR(8)BASED,                                           0322 01108000
*     I001C CHAR(1)BASED;           /* DEFINE LABELS USED BY PLS   0322 01109000
*                                      MACROS Y02014                 */ 01110000
*   END AHLSCAN                     /* END OF MODULE           Y02014*/ 01111000
*                                                                  0323 01112000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 01113000
*/*%INCLUDE SYSLIB  (GTFPCT  )                                       */ 01114000
*/*%INCLUDE SYSLIB  (INITDATA)                                       */ 01115000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 01116000
*/*%INCLUDE SYSLIB  (IEZCIB  )                                       */ 01117000
*                                                                  0323 01118000
*       ;                                                          0323 01119000
@DATA    DS    0H                                                       01120000
@CH00112 DC    H'1'                                                     01121000
@SC01202 CLC   KYWORDS-1(0,@14),COMPAREA(@03)                           01122000
@SC01204 CLC   OPTVALUE-1(0,@14),COMPAREA(@01)                          01123000
