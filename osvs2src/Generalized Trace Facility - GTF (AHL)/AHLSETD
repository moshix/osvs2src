         TITLE 'AHLSETD - SETEVENT DISABLE/CHANGE SERVICE              *00001000
                       '                                                00002000
*********************************************************************** 00003000
*                                                                     * 00004000
*   ENTRY CODE.  USE CDS LOGIC TO PICK UP THE CURRENT MCCE ADDRESS AND  00005000
*   INCREMENT THE USE COUNT IN THE MCHEAD.  THE RETURN CODE IS THEN SET 00006000
*   TO 8 TO INDICATE THAT NOTHING HAS BEEN DONE.  THIS RETURN CODE IS * 00007000
*   UPDATED IF SOME ACTION IS TAKEN LATER IN PROCESSING.              * 00008000
*                                                                     * 00009000
*********************************************************************** 00010000
AHLSETD   CSECT                                                         00011000
          STM   REG0,EPADDR,MCASETEV(AWSAPTR)  SAVE CALLERS REGS        00012000
          LR    SETDBASE,EPADDR     ESTABLISH BASE REGISTER             00013000
          USING AHLSETD,SETDBASE                                        00014000
          L     MCHEADA,FLCCVT      LOAD CVT ADDRESS                    00015000
          L     MCHEADA,CVTGTF(MCHEADA) LOAD MCHEAD ADDR                00016000
          LM    WORK1,WORK2,MCHCUR(MCHEADA) PICK UP MCCE @ AND USE CNT  00017000
AHLCDS1   LR    MCCEAD,WORK1        MOVE TO PERMANENT REGISTER          00018000
          LR    USECOUNT,WORK2      DITTO                               00019000
          LA    USECOUNT,1(USECOUNT) INCREMENT COUNT BY 1               00020000
          CDS   WORK1,MCCEAD,MCHCUR(MCHEADA) STORE NEW USE COUNT        00021000
          BC    7,AHLCDS1           IF WAS CHANGED, REDO                00022000
         LA    FRRLIST,AHLSETDE                             YM01900     00023000
         SETFRR A,FRRAD=(FRRLIST),WRKREGS=(4,5),PARMAD=(FRRLIST)        00024000
*                                                           YM01900     00025000
*   MCCEFRR=MCCEAD;                 /* PLACE MCCE ADDRESS INTO FRR      00026000
*                                      PLIST. THIS WAS THE MCCE 01S0039 00027000
*                                      ASSOCIATED WITH THIS INCREMENT   00028000
*                                      OF THE USE COUNT. THE USE        00029000
*                                      COUNT MUST BE DECREMENTED IN     00030000
*                                      CASE OF AN ERROR              */ 00031000
         ST    MCCEAD,MCCEFRR(,FRRLIST)                         01S0039 00032000
*   RTRNCODE=8;                     /* SET RETURN CODE TO INDICATE      00033000
*                                      THAT NO PROCESSING WAS DONE -    00034000
*                                      THIS WILL BE UPDATED IF  01S0040 00035000
*                                      SOMETHING IS.                 */ 00036000
         LA    @10,MCASETEV+60(,AWSAPTR)                        01S0040 00037000
         MVC   RTRNCODE(4,@10),@CF00055                         01S0040 00038000
*                                                               01S0041 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* CHECK FOR ENQ OR DEQ REQUEST, SIGNIFYING THAT CALLER WANTS    */ 00042000
*   /* SERIALIZATION ON THE MC TABLES                                */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                               01S0041 00046000
*   IF PLISTAD=ENQREQST THEN                                    01S0041 00047000
         LTR   PLISTAD,PLISTAD                                  01S0041 00048000
         BNZ   @RF00041                                         01S0041 00049000
*     DO;                           /* Q. IS THIS AN ENQ REQEST      */ 00050000
*       MCASETEV(1:4)=MCCEAD;       /* MOVE MCCE ADDRESS INTO R0        00051000
*                                      RESTORE LOCATION              */ 00052000
         ST    MCCEAD,MCASETEV(,AWSAPTR)                        02S0043 00053000
*       GO TO AHLXIT2;              /* EXIT WITHOUT DECREMENTING        00054000
*                                      COUNT                         */ 00055000
         B     AHLXIT2                                          02S0044 00056000
*     END;                                                      01S0045 00057000
*   IF PLISTAD=DEQREQST THEN                                    01S0046 00058000
@RF00041 C     PLISTAD,DEQREQST                                 01S0046 00059000
         BNE   @RF00046                                         01S0046 00060000
*     DO;                           /* Q. IS THIS A DEQ REQUEST      */ 00061000
*                                                               02S0048 00062000
*       /*************************************************************/ 00063000
*       /*                                                           */ 00064000
*       /* DECREMENT THE USE COUNT IN THE MCCE, THE ADDRESS OF WHICH */ 00065000
*       /* WAS PASSED IN REGISTER ZERO. NOTE -- THIS MCCE MAY STILL  */ 00066000
*       /* BE THE CURRENT ONE. IF THIS IS THE CASE, SINCE THE        */ 00067000
*       /* FOLLOWING CODE WILL DECREMENT THE USE COUNT IN THE MCCE   */ 00068000
*       /* AND NOT THE MCHEAD, THE MCHEAD USE COUNT WILL BE INFLATED */ 00069000
*       /* BY ONE, AND THE MCCE USE COUNT WILL BE A MINUS ONE. THIS  */ 00070000
*       /* DIFFERENCE WILL BE RESOLVED WHEN THE CURRENT MCCE BECOMES */ 00071000
*       /* OBSOLETE, AND THE TWO USE COUNTS GET ADDED TOGETHER.      */ 00072000
*       /*                                                           */ 00073000
*       /*************************************************************/ 00074000
*                                                               02S0048 00075000
*       RFY                                                     02S0048 00076000
*         MCCE BASED(REG0);                                     02S0048 00077000
*       RFY                                                     02S0049 00078000
*         REG0 RSTD;                                            02S0049 00079000
*       IF MCCEIDEN^=MCCETAG THEN                               02S0050 00080000
         LR    @10,REG0                                         02S0050 00081000
         CLC   MCCEIDEN(8,@10),@CC01526                         02S0050 00082000
         BNE   @RT00050                                         02S0050 00083000
*         GO TO AHLXIT1;            /* DONT DECREMENT COUNT IF THE      00084000
*                                      PASSED ADDRESS IS INVALID     */ 00085000
*       WORK2=MCCECNT;              /* PICK UP USE COUNT             */ 00086000
         LR    @10,REG0                                         02S0052 00087000
         L     WORK2,MCCECNT(,@10)                              02S0052 00088000
*MCCEDEQ1:                                                      02S0053 00089000
*       USECOUNT=WORK2;             /* COPY THE USE COUNT            */ 00090000
MCCEDEQ1 LR    USECOUNT,WORK2                                   02S0053 00091000
*       USECOUNT=USECOUNT-1;        /* DECREMENT BY ONE              */ 00092000
         BCTR  USECOUNT,0                                       02S0054 00093000
*       CS(WORK2,USECOUNT,MCCECNT); /* STORE NEW USE COUNT           */ 00094000
         LR    @10,REG0                                         02S0055 00095000
         CS    WORK2,USECOUNT,MCCECNT(@10)                      02S0055 00096000
*       BC(7,MCCEDEQ1);             /* REDO ARITHMETIC UNTIL IT GOES */ 00097000
         BC    7,MCCEDEQ1                                       02S0056 00098000
*       IF USECOUNT=0 THEN                                      02S0057 00099000
         LTR   USECOUNT,USECOUNT                                02S0057 00100000
         BNZ   @RF00057                                         02S0057 00101000
*         DO;                       /* IF THIS COUNT GOES TO ZERO,      00102000
*                                      THE MCCE IS TO BE FREED       */ 00103000
*           SRBPTR=ADDR(MCCESRB);   /* POINT TO SRB TO SCHEDULE      */ 00104000
         LR    SRBPTR,REG0                                      02S0059 00105000
         LA    SRBPTR,MCCESRB(,SRBPTR)                          02S0059 00106000
*           GEN(SCHEDULE SRB=(1));                              02S0060 00107000
         SCHEDULE SRB=(1)                                               00108000
*         END;                                                  02S0061 00109000
*       GO TO AHLXIT1;              /* EXIT FROM MODULE - DEQ HAS       00110000
*                                      BEEN PERFORMED                */ 00111000
         B     AHLXIT1                                          02S0062 00112000
*       RFY                                                     02S0063 00113000
*         REG0 UNRSTD;                                          02S0063 00114000
*       RFY                                                     02S0064 00115000
*         MCCE BASED(MCCEAD);                                   02S0064 00116000
*     END;                                                      01S0065 00117000
*                                                               01S0066 00118000
*   /*****************************************************************/ 00119000
*   /*                                                               */ 00120000
*   /* CHECK THE MCCE ADDRESS LOADED FROM THE MCHEAD. IF ZERO, EXIT. */ 00121000
*   /* IF NOT, SEARCH THE MCQE CHAIN TO FIND AN MCQE WITH THE SAME   */ 00122000
*   /* APPLICATION NAME AND ADDRESS AS GIVEN IN THE PARAMETER LIST.  */ 00123000
*   /*                                                               */ 00124000
*   /*****************************************************************/ 00125000
*                                                               01S0066 00126000
*   IF MCCEAD^=0 THEN                                           01S0066 00127000
@RF00046 LTR   MCCEAD,MCCEAD                                    01S0066 00128000
         BZ    @RF00066                                         01S0066 00129000
*     DO;                           /* IF AN MCCE EXISTS, PROCEDE AS    00130000
*                                      FOLLOWS                       */ 00131000
*       MCQEADDR=MCCEAD->MCCEQEA;   /* LOAD FIRST MCQE ADDRESS ON       00132000
*                                      CHAIN                         */ 00133000
         L     MCQEADDR,MCCEQEA(,MCCEAD)                        01S0068 00134000
*       RESPECIFY                                               01S0069 00135000
*         MCQE BASED(MCQEADDR);                                 01S0069 00136000
*       RESPECIFY                                               01S0070 00137000
*        (MCEE,                                                 01S0070 00138000
*         MCCLE)BASED(ELEMAD);                                  01S0070 00139000
*AHLLOOP1:                                                      01S0071 00140000
*       IF MCQEADDR^=0 THEN                                     01S0071 00141000
AHLLOOP1 SR    @10,@10                                          01S0071 00142000
         CR    MCQEADDR,@10                                     01S0071 00143000
         BE    @RF00071                                         01S0071 00144000
*         DO;                       /* PROCESS ALL MCQES ON CHAIN    */ 00145000
*           IF LISTNAME=MCQENAME&MCQEADDR=MCQEPTR THEN          01S0073 00146000
         CLC   LISTNAME(8,PLISTAD),MCQENAME(MCQEADDR)           01S0073 00147000
         BNE   @RF00073                                         01S0073 00148000
         C     MCQEADDR,MCQEPTR(,PLISTAD)                       01S0073 00149000
         BNE   @RF00073                                         01S0073 00150000
*             DO;                   /* SEARCH CHAIN FOR MATCHING        00151000
*                                      APPLICATION NAME AND ENSURE      00152000
*                                      ADDR IN PLIST IS CORRECT      */ 00153000
*                                                               01S0074 00154000
*               /*****************************************************/ 00155000
*               /*                                                   */ 00156000
*               /* MCQE HAS BEEN FOUND. PROCESS THE REQUEST AS       */ 00157000
*               /* INDICATED IN THE PARAMETER LIST.                  */ 00158000
*               /*                                                   */ 00159000
*               /*****************************************************/ 00160000
*                                                               01S0075 00161000
*               IF DISABLE='1'B THEN                            01S0075 00162000
         TM    DISABLE(PLISTAD),B'00100000'                     01S0075 00163000
         BNO   @RF00075                                         01S0075 00164000
*                 DO;                                           01S0076 00165000
*                                                               01S0076 00166000
*                   /*************************************************/ 00167000
*                   /*                                               */ 00168000
*                   /* DISABLE FUNCTION HAS BEEN SPECIFIED - DISABLE */ 00169000
*                   /* THE ENTIRE MCQE IF NEITHER EIDS NOR CLASSES   */ 00170000
*                   /* HAVE BEEN SPECIFIED. IF EIDS OR CLASSES (OR   */ 00171000
*                   /* BOTH) HAVE BEEN SPECIFIED, DISABLE THOSE      */ 00172000
*                   /* REFLECTED IN THE PARAMETER LIST(S).           */ 00173000
*                   /*                                               */ 00174000
*                   /*************************************************/ 00175000
*                                                               01S0077 00176000
*                   IF ENTRIES=0 THEN                           01S0077 00177000
         CH    @10,ENTRIES(,PLISTAD)                            01S0077 00178000
         BNE   @RF00077                                         01S0077 00179000
*                     DO;           /* IF NO EIDS OR CLASSES, DISABLE   00180000
*                                      ENTIRE MCQE                   */ 00181000
*                                                               02S0079 00182000
*                       /*********************************************/ 00183000
*                       /*                                           */ 00184000
*                       /* THIS ROUTINE DISABLES ALL MCEES AND MCCLES*/ 00185000
*                       /* CHAINED OFF THE FOUND MCQE. IF FIRST SETS */ 00186000
*                       /* THE RETURN CODE TO ZERO, THEN SEARCHES    */ 00187000
*                       /* BOTH THE MCEE AND MCCLE CHAINS OFF THE    */ 00188000
*                       /* MCQE, DISABLING ALL ELEMENTS ON EACH      */ 00189000
*                       /* CHAIN.                                    */ 00190000
*                       /*                                           */ 00191000
*                       /*********************************************/ 00192000
*                                                               02S0079 00193000
*                       RTRNCODE=RTRNCODE&'0111'B;/* SET RETURN CODE    00194000
*                                      TO SUCCESSFUL                 */ 00195000
*                                                               02S0079 00196000
         LA    @10,MCASETEV+60(,AWSAPTR)                        02S0079 00197000
         LA    @09,7                                            02S0079 00198000
         N     @09,RTRNCODE(,@10)                               02S0079 00199000
         ST    @09,RTRNCODE(,@10)                               02S0079 00200000
*                       /*********************************************/ 00201000
*                       /*                                           */ 00202000
*                       /* PROCESS THE MCEES FIRST                   */ 00203000
*                       /*                                           */ 00204000
*                       /*********************************************/ 00205000
*                                                               02S0080 00206000
*                       ELEMAD=MCQEMCEE;/* INIT TO FIRST MCEE OFF       00207000
*                                      MCQE                          */ 00208000
         L     ELEMAD,MCQEMCEE(,MCQEADDR)                       02S0080 00209000
*                       RESPECIFY                               02S0081 00210000
*                         MCEE BASED(ELEMAD);                   02S0081 00211000
*                       DO WHILE(ELEMAD^=0);/* PROCESS IF MCEES EXIST*/ 00212000
         B     @DE00082                                         02S0082 00213000
@DL00082 DS    0H                                               02S0083 00214000
*                         MCEERTN=MCHDIS;/* REPLACE MCEE ROUTINE ADDR   00215000
*                                      WITH DISABLE ADDR             */ 00216000
         MVC   MCEERTN(4,ELEMAD),MCHDIS(MCHEADA)                02S0083 00217000
*                         MCEEDIS='1'B;/* SET DIAGNOSTIC TO SHOW        00218000
*                                      DISABLEMENT                   */ 00219000
         OI    MCEEDIS(ELEMAD),B'10000000'                      02S0084 00220000
*                         ELEMAD=MCEEMCQL;/* LOAD NEXT MCEE ON CHAIN */ 00221000
         L     ELEMAD,MCEEMCQL(,ELEMAD)                         02S0085 00222000
*                       END;        /* LOOP UNTIL END OF MCEE CHAIN  */ 00223000
*                                                               02S0086 00224000
@DE00082 LTR   ELEMAD,ELEMAD                                    02S0086 00225000
         BNZ   @DL00082                                         02S0086 00226000
*                       /*********************************************/ 00227000
*                       /*                                           */ 00228000
*                       /* THEN PROCESS THE MCCLES                   */ 00229000
*                       /*                                           */ 00230000
*                       /*********************************************/ 00231000
*                                                               02S0087 00232000
*                       ELEMAD=MCQEMCCL;/* LOAD FIRST MCCLE ADDRESS  */ 00233000
         L     ELEMAD,MCQEMCCL(,MCQEADDR)                       02S0087 00234000
*                       RESPECIFY                               02S0088 00235000
*                         MCCLE BASED(ELEMAD);                  02S0088 00236000
*                       DO WHILE(ELEMAD^=0);/* PROCESS IF MCCLES        00237000
*                                      EXIST                         */ 00238000
         B     @DE00089                                         02S0089 00239000
@DL00089 DS    0H                                               02S0090 00240000
*                         MCCLRTN=MCHDIS;/* REPLACE MCCLE ROUTINE       00241000
*                                      ADDR WITH DISABLE ADDR        */ 00242000
         MVC   MCCLRTN(4,ELEMAD),MCHDIS(MCHEADA)                02S0090 00243000
*                         MCCLDIS='1'B;/* SET DIAGNOSTIC TO SHOW        00244000
*                                      DISABLEMENT                   */ 00245000
         OI    MCCLDIS(ELEMAD),B'10000000'                      02S0091 00246000
*                         ELEMAD=MCCLMCQL;/* LOAD NEXT MCCLE ON CHAIN*/ 00247000
         L     ELEMAD,MCCLMCQL(,ELEMAD)                         02S0092 00248000
*                       END;        /* LOOP UNTIL END OF MCCLE CHAIN */ 00249000
@DE00089 LTR   ELEMAD,ELEMAD                                    02S0093 00250000
         BNZ   @DL00089                                         02S0093 00251000
*                     END;                                      01S0094 00252000
*                   ELSE                                        01S0095 00253000
*                     DO;           /* OTHERWISE, EITHER EIDS OR        00254000
*                                      CLASSES HAVE BEEN GIVEN       */ 00255000
         B     @RC00077                                         01S0095 00256000
@RF00077 DS    0H                                               01S0096 00257000
*                       IF EIDNUM^=0 THEN                       01S0096 00258000
         CLI   EIDNUM(PLISTAD),0                                01S0096 00259000
         BE    @RF00096                                         01S0096 00260000
*                         DO;       /* IF THERE ARE EIDS, PROCESS       00261000
*                                      THEM                          */ 00262000
*                                                               02S0098 00263000
*                           /*****************************************/ 00264000
*                           /*                                       */ 00265000
*                           /* THIS ROUTINE DISABLES THE ROUTINE     */ 00266000
*                           /* ADDRESSES IN THE MCEES ASSOCIATED WITH*/ 00267000
*                           /* THE EID(S) IN THE PARAMETER LIST. THE */ 00268000
*                           /* MCEE CHAIN IS SEARCHED FOR EACH EID IN*/ 00269000
*                           /* THE LIST. WHEN AN MCEE IS FOUND WHICH */ 00270000
*                           /* CONTAINS A MATCHING EID, THE VALUE OF */ 00271000
*                           /* MCHDIS IS MOVED INTO THE MCEERTN      */ 00272000
*                           /* FIELD, DISABLING THAT MCEE.           */ 00273000
*                           /*                                       */ 00274000
*                           /*****************************************/ 00275000
*                                                               02S0098 00276000
*                           TEMPNUM=EIDNUM;/* INIT NUMBER OF ELEMENTS   00277000
*                                      IN LIST                       */ 00278000
         SR    TEMPNUM,TEMPNUM                                  02S0098 00279000
         IC    TEMPNUM,EIDNUM(,PLISTAD)                         02S0098 00280000
*                           ELEMAD=MCQEMCEE;/* LOAD FIRST MCEE  02S0099 00281000
*                                      ADDRESS                       */ 00282000
         L     ELEMAD,MCQEMCEE(,MCQEADDR)                       02S0099 00283000
*                           LISTELM=ELISTA;/* INITIALIZE TO FIRST       00284000
*                                      ENTRY IN EIDLIST              */ 00285000
         L     LISTELM,ELISTA(,PLISTAD)                         02S0100 00286000
*                           IF ELEMAD^=0 THEN                   02S0101 00287000
         LTR   ELEMAD,ELEMAD                                    02S0101 00288000
         BZ    @RF00101                                         02S0101 00289000
*                             DO WHILE(TEMPNUM^=0);             02S0102 00290000
*                                                               02S0102 00291000
         B     @DE00102                                         02S0102 00292000
@DL00102 DS    0H                                               02S0103 00293000
*                               /*************************************/ 00294000
*                               /*                                   */ 00295000
*                               /* MCEES EXIST OFF MCQE. USING EID IN*/ 00296000
*                               /* EACH ENTRY IN THE EIDLIST, SEARCH */ 00297000
*                               /* MCEE CHAIN FOR AN MCEE CONTAINING */ 00298000
*                               /* THAT EID.                         */ 00299000
*                               /*                                   */ 00300000
*                               /*************************************/ 00301000
*                                                               02S0103 00302000
*                               DO WHILE(LISTEID^=MCEEEID);/* LOOP      00303000
*                                      UNTIL APPLICABLE EID FOUND    */ 00304000
         B     @DE00103                                         02S0103 00305000
@DL00103 DS    0H                                               02S0104 00306000
*                                 ELEMAD=MCEEMCQL;/* PICK UP NEXT       00307000
*                                      MCEE ADDRESS                  */ 00308000
         L     ELEMAD,MCEEMCQL(,ELEMAD)                         02S0104 00309000
*                                 IF ELEMAD=0 THEN              02S0105 00310000
         LTR   ELEMAD,ELEMAD                                    02S0105 00311000
         BNZ   @RF00105                                         02S0105 00312000
*                                   DO;                         02S0106 00313000
*                                                               02S0106 00314000
*                                    /********************************/ 00315000
*                                    /*                              */ 00316000
*                                    /* END OF MCEE CHAIN HAS BEEN   */ 00317000
*                                    /* REACHED, EID IS NOT DEFINED  */ 00318000
*                                    /*                              */ 00319000
*                                    /********************************/ 00320000
*                                                               02S0107 00321000
*                                    RTRNCODE=RTRNCODE|'0100'B;/*       00322000
*                                      INDICATE UNSUCCESSFUL TRY     */ 00323000
         LA    @10,MCASETEV+60(,AWSAPTR)                        02S0107 00324000
         LA    @09,4                                            02S0107 00325000
         O     @09,RTRNCODE(,@10)                               02S0107 00326000
         ST    @09,RTRNCODE(,@10)                               02S0107 00327000
*                                    GO TO AHLSUB1;/* GO TRY NEXT EID   00328000
*                                      IN LIST                       */ 00329000
         B     AHLSUB1                                          02S0108 00330000
*                                   END;                        02S0109 00331000
*                               END;/* LOOP UNTIL EIDS MATCH         */ 00332000
*                                                               02S0110 00333000
@RF00105 DS    0H                                               02S0110 00334000
@DE00103 CLC   LISTEID(2,LISTELM),MCEEEID(ELEMAD)               02S0110 00335000
         BNE   @DL00103                                         02S0110 00336000
*                               /*************************************/ 00337000
*                               /*                                   */ 00338000
*                               /* MATCHING EID HAS BEEN FOUND IN AN */ 00339000
*                               /* MCEE. DISABLE THAT MCEE.          */ 00340000
*                               /*                                   */ 00341000
*                               /*************************************/ 00342000
*                                                               02S0111 00343000
*                               MCEERTN=MCHDIS;/* REPLACE ADDRESS IN    00344000
*                                      MCEE WITH ADDRESS OF BR 14    */ 00345000
         MVC   MCEERTN(4,ELEMAD),MCHDIS(MCHEADA)                02S0111 00346000
*                               MCEEDIS='1'B;/* SET DIAGNOSTIC  02S0112 00347000
*                                      INDICATOR TO SHOW DISABLEMENT */ 00348000
         OI    MCEEDIS(ELEMAD),B'10000000'                      02S0112 00349000
*                               RTRNCODE=RTRNCODE&'0111'B;/* SET        00350000
*                                      SUCCESSFUL RETURN CODE        */ 00351000
*                                                               02S0113 00352000
         LA    @10,MCASETEV+60(,AWSAPTR)                        02S0113 00353000
         LA    @09,7                                            02S0113 00354000
         N     @09,RTRNCODE(,@10)                               02S0113 00355000
         ST    @09,RTRNCODE(,@10)                               02S0113 00356000
*                               /*************************************/ 00357000
*                               /*                                   */ 00358000
*                               /* INCREMENT TO NEXT ENTRY IN PLIST, */ 00359000
*                               /* AND START WITH TOP OF MCEE CHAIN  */ 00360000
*                               /*                                   */ 00361000
*                               /*************************************/ 00362000
*                                                               02S0114 00363000
*AHLSUB1:                                                       02S0114 00364000
*                               ELEMAD=MCQEMCEE;/* PICK UP FIRST MCEE   00365000
*                                      ADDRESS                       */ 00366000
AHLSUB1  L     ELEMAD,MCQEMCEE(,MCQEADDR)                       02S0114 00367000
*                               TEMPNUM=TEMPNUM-1;/* DECREMENT NUMBER   00368000
*                                      OF EIDS IN LIST               */ 00369000
         BCTR  TEMPNUM,0                                        02S0115 00370000
*                               LISTELM=LISTELM+LENGTH(EIDLIST);/*      00371000
*                                      INCR POINTER TO NEXT EID ENTRY*/ 00372000
         AH    LISTELM,@CH00736                                 02S0116 00373000
*                             END;                              02S0117 00374000
*                                                               02S0117 00375000
@DE00102 LTR   TEMPNUM,TEMPNUM                                  02S0117 00376000
         BNZ   @DL00102                                         02S0117 00377000
*                           /*****************************************/ 00378000
*                           /*                                       */ 00379000
*                           /* IF NO MCEES EXIST, SET UNSUCCESSFUL   */ 00380000
*                           /* RETURN CODE                           */ 00381000
*                           /*                                       */ 00382000
*                           /*****************************************/ 00383000
*                                                               02S0118 00384000
*                           ELSE                                02S0118 00385000
*                             RTRNCODE=RTRNCODE|'0100'B;        02S0118 00386000
         B     @RC00101                                         02S0118 00387000
@RF00101 LA    @10,MCASETEV+60(,AWSAPTR)                        02S0118 00388000
         LA    @09,4                                            02S0118 00389000
         O     @09,RTRNCODE(,@10)                               02S0118 00390000
         ST    @09,RTRNCODE(,@10)                               02S0118 00391000
*                         END;                                  01S0119 00392000
@RC00101 DS    0H                                               01S0120 00393000
*                       IF CLSNUM^=0 THEN                       01S0120 00394000
@RF00096 CLI   CLSNUM(PLISTAD),0                                01S0120 00395000
         BE    @RF00120                                         01S0120 00396000
*                         DO;       /* IF THERE ARE CLASSES, PROCESS    00397000
*                                      THEM                          */ 00398000
*                                                               02S0122 00399000
*                           /*****************************************/ 00400000
*                           /*                                       */ 00401000
*                           /* THIS ROUTINE DISABLES THE ROUTINE     */ 00402000
*                           /* ADDRESSES IN THE MCCLES ASSOCIATED    */ 00403000
*                           /* WITH CLASSES IN THE PARAMETER LIST.   */ 00404000
*                           /* THE MCCLE CHAIN IS SEARCHED FOR EACH  */ 00405000
*                           /* CLASS IN THE LIST. WHEN AN MCCLE IS   */ 00406000
*                           /* FOUND WHICH CONTAINS A MATCHING CLASS,*/ 00407000
*                           /* THE VALUE OF MCHDIS IS MOVED INTO THE */ 00408000
*                           /* MCCLERTN FIELD, DISABLING THAT MCCLE. */ 00409000
