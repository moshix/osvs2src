@RF00073 DS    0H                                               01S0198 00826000
*               MCQEADDR=MCQEMCQE;                              01S0198 00827000
         L     MCQEADDR,MCQEMCQE(,MCQEADDR)                     01S0198 00828000
*               GO TO AHLLOOP1;     /* LOOP BACK UNTIL END OF MCQE      00829000
*                                      CHAIN                         */ 00830000
         B     AHLLOOP1                                         01S0199 00831000
*             END;                                              01S0200 00832000
*         END;                      /* LOOP THROUGH ENTIRE MCQE CHAIN*/ 00833000
@RC00073 DS    0H                                               01S0202 00834000
*     END;                          /* END OF NONZERO MCCE PATH      */ 00835000
@RF00071 DS    0H                                               01S0203 00836000
*                                                               01S0203 00837000
*   /*****************************************************************/ 00838000
*   /*                                                               */ 00839000
*   /* EXIT CODE. DECREMENT THE USE COUNT IN THE MCHEAD AND VERIFY   */ 00840000
*   /* THAT THE MCCE ADDRESS HAS NOT CHANGED VIA CDS LOGIC. IF THE   */ 00841000
*   /* MCCE ADDRESS HAS CHANGED, DECREMENT THE USE COUNT IN THE MCCE */ 00842000
*   /* THAT WAS USED DURING AHLSETD PROCESSING. IF THIS USE COUNT    */ 00843000
*   /* GOES TO ZERO, SCHEDULE THE SRB LOCATED WITHIN THAT MCCE TO    */ 00844000
*   /* FREE ANY TABLES THAT ARE NOW ABLE TO BE FREED. ONCE THIS TABLE*/ 00845000
*   /* SERIALIZING IS COMPLETE, RELOAD ALL SAVED REGISTERS AND RETURN*/ 00846000
*   /* TO THE CALLER.                                                */ 00847000
*   /*                                                               */ 00848000
*   /*****************************************************************/ 00849000
*                                                               01S0203 00850000
*   GENERATE;                                                   01S0203 00851000
@RF00066 DS    0H                                               01S0203 00852000
AHLXIT1  L     WORK2,MCHCNT(MCHEADA)   /* LOAD USE COUNT FROM MCHEAD */ 00853000
         LR    WORK1,MCCEAD            /* CREATE BACKUP FOR MCCE @   */ 00854000
AHLCSD2  LR    USECOUNT,WORK2          /* CREATE COPY OF USE COUNT   */ 00855000
         BCTR  USECOUNT,0              /* DECREMENT USE COUNT BY 1   */ 00856000
         CDS   WORK1,MCCEAD,MCHCUR(MCHEADA) DECR COUNT AND VERIFY       00857000
*                                         MCCE ADDRESS               */ 00858000
         BC    8,AHLXIT2               /* IF SUCCESSFUL, EXIT        */ 00859000
         CR    WORK1,MCCEAD            /* Q. DID TABLE ADDR CHANGE   */ 00860000
         BE    AHLCSD2                 /* IF NOT, REDO TO SET CORRECT   00861000
*                                         COUNT                      */ 00862000
         LTR   MCCEAD,MCCEAD           /* Q. WAS OLD ONE ZERO        */ 00863000
         BE    AHLXIT2                 /* IF SO, EXIT                */ 00864000
         L     WORK2,MCCECNT(MCCEAD)   /* LOAD USE COUNT FROM MCCE   */ 00865000
AHLCSD3  LR    USECOUNT,WORK2          /* CREATE COPY OF COUNT       */ 00866000
         BCTR  USECOUNT,0              /* DECREMENT BY 1             */ 00867000
         CS    WORK2,USECOUNT,MCCECNT(MCCEAD) /* PLACE IN MCCE       */ 00868000
         BC    7,AHLCSD3               /* IF DIDNT GO, REDO UNTIL OK */ 00869000
         LTR   USECOUNT,USECOUNT       /* Q. DID COUNT BECOME ZERO   */ 00870000
         BNE   AHLXIT2                 /* IF NOT, EXIT               */ 00871000
         LA    SRBPTR,MCCESRB(MCCEAD)  /* SRB ADDR TO REG 1          */ 00872000
         SCHEDULE SRB=(1)              /* SCHEDULE SRB TO FREE TABLES*/ 00873000
AHLXIT2  SETFRR  D,WRKREGS=(4,5)                                        00874000
         LM    REG0,EPADDR,MCASETEV(AWSAPTR) /* RELOAD CALLERS REGS  */ 00875000
         BR    RTRNAD                  /* RETURN TO CALLER           */ 00876000
*                                                               02S0204 00877000
*   /*****************************************************************/ 00878000
*   /*                                                               */ 00879000
*   /* THIS IS THE FRR ROUTINE FO AHLSETD. IT ENSURES THAT THE USE   */ 00880000
*   /* COUNT IN THE MCCE IS DECREMENTED, SINCE AN ERROR OCCURED WHILE*/ 00881000
*   /* THE USE COUNT HAD BEEN INCREMENTED                            */ 00882000
*   /*                                                               */ 00883000
*   /*****************************************************************/ 00884000
*                                                               02S0204 00885000
*   RFY                                                         02S0204 00886000
*    (SDWAPTR,                                                  02S0204 00887000
*     RTRNAD,                                                   02S0204 00888000
*     MCCEAD,                                                   02S0204 00889000
*     WORK2,                                                    02S0204 00890000
*     USECOUNT)RSTD;                                            02S0204 00891000
*   RFY                                                         02S0205 00892000
*     FRRPARM BASED(SDWAPARM);                                  02S0205 00893000
*AHLSETDE:                                                      02S0206 00894000
*   GENERATE;                                                   02S0206 00895000
AHLSETDE DS    0H                                               02S0206 00896000
          BALR  SETDBASE,0                                              00897000
          USING *,SETDBASE                                              00898000
*   SDWAPTR=SDWAADDR;               /* SAVE RTCA ADDRESS             */ 00899000
         LR    SDWAPTR,SDWAADDR                                 02S0207 00900000
*   MCCEAD=MCCEFRR;                 /* LOAD MCCE ADDRESS FROM RTCA      00901000
*                                      PARMS                         */ 00902000
         L     @10,SDWAPARM(,SDWAPTR)                           02S0208 00903000
         L     MCCEAD,MCCEFRR(,@10)                             02S0208 00904000
*   WORK2=MCCECNT;                  /* PICK UP USE COUNT             */ 00905000
         L     WORK2,MCCECNT(,MCCEAD)                           02S0209 00906000
*MCCEDEQ2:                                                      02S0210 00907000
*   USECOUNT=WORK2;                 /* COPY THE USE COUNT            */ 00908000
MCCEDEQ2 LR    USECOUNT,WORK2                                   02S0210 00909000
*   USECOUNT=USECOUNT-1;            /* DECREMENT BY ONE              */ 00910000
         BCTR  USECOUNT,0                                       02S0211 00911000
*   CS(WORK2,USECOUNT,MCCECNT);     /* STORE NEW USE COUNT           */ 00912000
         CS    WORK2,USECOUNT,MCCECNT(MCCEAD)                   02S0212 00913000
*   BC(7,MCCEDEQ2);                 /* REDO ARITHMETIC UNTIL IT GOES */ 00914000
         BC    7,MCCEDEQ2                                       02S0213 00915000
*   IF USECOUNT=0 THEN                                          02S0214 00916000
         LTR   USECOUNT,USECOUNT                                02S0214 00917000
         BNZ   @RF00214                                         02S0214 00918000
*     DO;                           /* IF THIS COUNT GOES TO ZERO,      00919000
*                                      THE MCCE IS TO BE FREED       */ 00920000
*       RFY                                                     02S0216 00921000
*         SRBPTR RSTD;                                          02S0216 00922000
*       SRBPTR=ADDR(MCCESRB);       /* POINT TO SRB TO SCHEDULE      */ 00923000
         LA    SRBPTR,MCCESRB(,MCCEAD)                          02S0217 00924000
*       GEN(SCHEDULE SRB=(1));                                  02S0218 00925000
         SCHEDULE SRB=(1)                                               00926000
*       RFY                                                     02S0219 00927000
*         SRBPTR UNRSTD;                                        02S0219 00928000
*     END;                                                      02S0220 00929000
*   DO;                             /* SETRP WKAREA(SDWAPTR)    02S0221 00930000
*                                      RC(SDWACWT)                   */ 00931000
@RF00214 DS    0H                                               02S0222 00932000
*     RESPECIFY                                                 02S0222 00933000
*      (GPR00P,                                                 02S0222 00934000
*       GPR01P,                                                 02S0222 00935000
*       GPR14P,                                                 02S0222 00936000
*       GPR15P)RSTD;                                            02S0222 00937000
*     GPR01P=SDWAPTR;                                           02S0223 00938000
         LR    GPR01P,SDWAPTR                                   02S0223 00939000
*     GPR01P->SDWARCDE=SDWACWT;     /* SAVE RC VALUE                 */ 00940000
         MVI   SDWARCDE(GPR01P),X'00'                           02S0224 00941000
*     RESPECIFY                                                 02S0225 00942000
*      (GPR00P,                                                 02S0225 00943000
*       GPR01P,                                                 02S0225 00944000
*       GPR14P,                                                 02S0225 00945000
*       GPR15P)UNRSTD;                                          02S0225 00946000
*   END;                                                        02S0226 00947000
*   GO TO RTRNAD;                                               02S0227 00948000
         BR    RTRNAD                                           02S0227 00949000
*   RFY                                                         02S0228 00950000
*    (SDWAPTR,                                                  02S0228 00951000
*     RTRNAD,                                                   02S0228 00952000
*     MCCEAD,                                                   02S0228 00953000
*     WORK2,                                                    02S0228 00954000
*     USECOUNT)UNRSTD;                                          02S0228 00955000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00956000
*     GPR00P PTR(31)REG(0),                                     01S0229 00957000
*     GPR01P PTR(31)REG(1),                                     01S0229 00958000
*     GPR14P PTR(31)REG(14),                                    01S0229 00959000
*     GPR15P PTR(31)REG(15);                                    01S0229 00960000
*   DECLARE                         /* COMMON VARIABLES              */ 00961000
*     I256C CHAR(256)BASED,                                     01S0230 00962000
*     I031F FIXED(31)BASED,                                     01S0230 00963000
*     I031P PTR(31)BASED,                                       01S0230 00964000
*     I015F FIXED(15)BASED,                                     01S0230 00965000
*     I015P PTR(15)BASED,                                       01S0230 00966000
*     I008P PTR(8)BASED,                                        01S0230 00967000
*     I001C CHAR(1)BASED;                                       01S0230 00968000
*   END AHLSETD                                                 01S0231 00969000
*                                                               01S0231 00970000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00971000
*/*%INCLUDE SYSUT5  (PLOG    )                                       */ 00972000
*/*%INCLUDE SYSUT5  (DCLS    )                                       */ 00973000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 00974000
*/*%INCLUDE SYSLIB  (IHAPSA  )                                       */ 00975000
*/*%INCLUDE SYSLIB  (IHASRB  )                                       */ 00976000
*/*%INCLUDE SYSLIB  (IHAFRRS )                                       */ 00977000
*/*%INCLUDE SYSLIB  (IHASDWA )                                       */ 00978000
*/*%INCLUDE SYSLIB  (MCHEAD  )                                       */ 00979000
*/*%INCLUDE SYSLIB  (MCAWSA  )                                       */ 00980000
*/*%INCLUDE SYSLIB  (MCCE    )                                       */ 00981000
*/*%INCLUDE SYSLIB  (MCQE    )                                       */ 00982000
*/*%INCLUDE SYSLIB  (MCEE    )                                       */ 00983000
*/*%INCLUDE SYSLIB  (MCCLE   )                                       */ 00984000
*/*%INCLUDE SYSUT5  (SETDENQ )                                       */ 00985000
*/*%INCLUDE SYSUT5  (SETDDEQ )                                       */ 00986000
