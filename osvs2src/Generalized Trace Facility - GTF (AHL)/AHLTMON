         TITLE 'AHLTMON - TASK MONITOR ROUTINE                         *00001000
                        '                                               00002000
AHLTMON  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AHLTMON   75.226'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*   PCTPTR=REG1;                    /* RETRIEVE GTFPCT ADDR    Y02014*/ 00018000
         LR    PCTPTR,REG1                                         0050 00019000
*   ESTAEPCT=PCTPTR;                /* PUT GTFPCT PTR IN ESTAE PARM     00020000
*                                      LIST                    Y02014*/ 00021000
         ST    PCTPTR,ESTAEPCT                                     0051 00022000
*   RFY                                                            0052 00023000
*     REG1 UNRSTD;                                                 0052 00024000
*   SYSCODE=8;                      /* SET UP SYSTEM RETURN CODE TO     00025000
*                                      INDICATE THE DEFAULT IS GTF 0053 00026000
*                                      FAILED ON ERROR CONDITION     */ 00027000
         MVC   SYSCODE(4),@CF00049                                 0053 00028000
*   TRACESAV=CVTTRACE;              /* SAVE STATUS OF SYSTEM TRACE 0054 00029000
*                                                            @YM03608*/ 00030000
         L     @14,CVTPTR                                          0054 00031000
         MVC   TRACESAV(2),CVTTRACE(@14)                           0054 00032000
*   GENREG=ADDR(AHLTESTA);          /* GET ADDRESS OF ESTAE RTN    0055 00033000
*                                                              Y02014*/ 00034000
         LA    GENREG,AHLTESTA                                     0055 00035000
*   GEN(ESTAE (GENREG),CT,PARAM=ESTAELST,TERM=YES);/*          Y02014*/ 00036000
         ESTAE (GENREG),CT,PARAM=ESTAELST,TERM=YES                      00037000
*   RFY                                                            0057 00038000
*     GENREG UNRSTD;                                               0057 00039000
*                                                                  0057 00040000
*   /*****************************************************************/ 00041000
*   /*                                                               */ 00042000
*   /* THE ESTAE RETURN CODES AND AHLTMONS REACTIONS ARE AS FOLLOWS: */ 00043000
*   /* 1) 0--SUCCESSFUL. 2) 4--N/A, NOT AN ERROR. 3) 8--N/A, NOT AN  */ 00044000
*   /* ERROR. 4) 12--UNSUCCESSFUL CANCEL. 5) 16--UNEXPECTED ERROR. 6)*/ 00045000
*   /* 20--UNABLE TO OBTAIN STORAGE FOR AN SCB.                      */ 00046000
*   /*                                                               */ 00047000
*   /*****************************************************************/ 00048000
*                                                                  0058 00049000
*   IF RETCODE>0 THEN               /* CHECK STATUS OF ESTAE   Y02014*/ 00050000
         LTR   RETCODE,RETCODE                                     0058 00051000
         BNP   @RF00058                                            0058 00052000
*     DO;                           /* ESTAE ERROR PATH        Y02014*/ 00053000
*       IF RETCODE=20 THEN          /* GETMAIN ERROR IN ESTAE? Y02014*/ 00054000
         C     RETCODE,@CF01355                                    0060 00055000
         BNE   @RF00060                                            0060 00056000
*         DO;                       /* GETMAIN ERROR PATH      Y02014*/ 00057000
*           DO;                     /* WTOP MF(E,AHL130)             */ 00058000
*             RFY                                                  0063 00059000
*               GPR01P RSTD;        /* RESTRICT REG 1                */ 00060000
*             GPR01P=ADDR(AHL130);  /* ADDRESS CONTROL PROGRAM LIST  */ 00061000
         L     GPR01P,@CA01303                                     0064 00062000
*             SVC(35);              /* ISSUE WTO SVC                 */ 00063000
         SVC   35                                                  0065 00064000
*             RFY                                                  0066 00065000
*               GPR01P UNRSTD;      /* RELEASE REG 1                 */ 00066000
*           END;                    /* WTOP MF(E,AHL130) INSUFFICIENT   00067000
*                                      STORAGE MSG Y02014            */ 00068000
*           DO;                     /* WTOP MF(E,AHL016)             */ 00069000
*             RFY                                                  0069 00070000
*               GPR01P RSTD;        /* RESTRICT REG 1                */ 00071000
*             GPR01P=ADDR(AHL016);  /* ADDRESS CONTROL PROGRAM LIST  */ 00072000
         L     GPR01P,@CA01301                                     0070 00073000
*             SVC(35);              /* ISSUE WTO SVC                 */ 00074000
         SVC   35                                                  0071 00075000
*             RFY                                                  0072 00076000
*               GPR01P UNRSTD;      /* RELEASE REG 1                 */ 00077000
*           END;                    /* WTOP MF(E,AHL016) ISSUE GTF 0073 00078000
*                                      INIT FAILED MSG Y02014        */ 00079000
*           GO TO AHLTERM2;         /* BRANCH TO TERMINATION RTN   0074 00080000
*                                                              Y02014*/ 00081000
         B     AHLTERM2                                            0074 00082000
*         END;                      /* END OF GETMAIN ERR PATH Y02014*/ 00083000
*       ELSE                                                       0076 00084000
*         DO;                       /* OTHER ESTAE ERR PATH    Y02014*/ 00085000
@RF00060 DS    0H                                                  0077 00086000
*           DO;                     /* WTOP MF(E,AHL015)             */ 00087000
*             RFY                                                  0078 00088000
*               GPR01P RSTD;        /* RESTRICT REG 1                */ 00089000
*             GPR01P=ADDR(AHL015);  /* ADDRESS CONTROL PROGRAM LIST  */ 00090000
         L     GPR01P,@CA01300                                     0079 00091000
*             SVC(35);              /* ISSUE WTO SVC                 */ 00092000
         SVC   35                                                  0080 00093000
*             RFY                                                  0081 00094000
*               GPR01P UNRSTD;      /* RELEASE REG 1                 */ 00095000
*           END;                    /* WTOP MF(E,AHL015) ESTAE     0082 00096000
*                                      UNSUCCESSFUL MSG Y02014       */ 00097000
*           DO;                     /* WTOP MF(E,AHL016)             */ 00098000
*             RFY                                                  0084 00099000
*               GPR01P RSTD;        /* RESTRICT REG 1                */ 00100000
*             GPR01P=ADDR(AHL016);  /* ADDRESS CONTROL PROGRAM LIST  */ 00101000
         L     GPR01P,@CA01301                                     0085 00102000
*             SVC(35);              /* ISSUE WTO SVC                 */ 00103000
         SVC   35                                                  0086 00104000
*             RFY                                                  0087 00105000
*               GPR01P UNRSTD;      /* RELEASE REG 1                 */ 00106000
*           END;                    /* WTOP MF(E,AHL016) ISSUE GTF 0088 00107000
*                                      INIT FAILED MSG Y02014        */ 00108000
*           GO TO AHLTERM2;         /* BRANCH TO TERMINATION RTN   0089 00109000
*                                                              Y02014*/ 00110000
         B     AHLTERM2                                            0089 00111000
*         END;                      /* END OF ESTAE ERR PATH   Y02014*/ 00112000
*     END;                          /* END OF ALL ESTAE ERR PATH   0091 00113000
*                                                              Y02014*/ 00114000
*                                                                  0092 00115000
*   /*****************************************************************/ 00116000
*   /*                                                               */ 00117000
*   /* COMPLETE GTF INITIALIZATION THE FOLLOWING SETEVENT IS ISSUED  */ 00118000
*   /* TO ACTIVATE GTF MONITOR CALL ROUTING TABLES, INITIALIZE EACH  */ 00119000
*   /* CPUS CONTROL REGISTER 8,AND OBTAIN THE MCWRSA. THE RETURN     */ 00120000
*   /* CODES FOR SETEVENT AND AHLTMONS ACTIONS ARE AS FOLLOWS: 1)    */ 00121000
*   /* 0--SUCCESSFUL. 2) 20--SETEVENT UNABLE TO INITIALIZE CONTROL   */ 00122000
*   /* REGISTER 8, AHLTMON GIVES OPERATOR OPTION OF CONTINUING OR    */ 00123000
*   /* TERMINATING GTF. 3) 4,8,12,16,24,28,32--ERRORS, AHLTMON ISSUES*/ 00124000
*   /* AHL019I AND GOES TO TERMINATION RTN.                          */ 00125000
*   /*                                                               */ 00126000
*   /*****************************************************************/ 00127000
*                                                                  0092 00128000
*   DO;                             /* SETEVENT NAME('GTF')MCQE(MCQE)   00129000
*                                      ACTION(ACTIVAT)               */ 00130000
*                                                                  0092 00131000
@RF00058 DS    0H                                                  0093 00132000
*     /***************************************************************/ 00133000
*     /*                                                             */ 00134000
*     /* THE FOLLOWING SECTION OF CODE IS GENERATED BY THE SETEVENT  */ 00135000
*     /* MACRO. THIS CODE WILL BE EITHER AN INITIALIZED PARAMETER    */ 00136000
*     /* LIST, OR IF IT IS THE RESULT OF STANDARD OR EXECUTE FORM IT */ 00137000
*     /* WILL FILL in the list, GENERATE ALL LINKAGES AND CALL THE   */ 00138000
*     /* PROPER sERVICE, EITHER AHLSETEV OR AHLSETD                  */ 00139000
*     /*                                                             */ 00140000
*     /***************************************************************/ 00141000
*                                                                  0093 00142000
*     DCL                                                          0093 00143000
*       AHLPPTR REG(1) PTR(31) RSTD;/* PARAMETER LIST ADDRESS        */ 00144000
*     DCL                                                          0094 00145000
*       AHLSETEV ENTRY OPTIONS(EXIT,SEQFLOW) BASED(MCHSETE) VALRG(0);   00146000
*                                   /* ENTRY FOR ADD ACTIVAT,FREE    */ 00147000
*     DCL                                                          0095 00148000
*       AHLSETD ENTRY OPTIONS(EXIT,SEQFLOW) BASED(MCHSETD) VALRG(0);    00149000
*                                   /* ENTRY FOR DISABLE AND CHANGE  */ 00150000
*     DCL                                                          0096 00151000
*       1 SEPL BASED(AHLPPTR) BDY(WORD),/* DSECT PARAMETER LIST      */ 00152000
*        2 SEFG PTR(8),             /* ACTION GLAGS                  */ 00153000
*        2 * PTR(8),                                               0096 00154000
*        2 SEEN PTR(8),             /* EID COUNT                     */ 00155000
*        2 SECN PTR(8),             /* CLASS COUNT                   */ 00156000
*        2 SENM CHAR(8),            /* APPLICATION NMAE              */ 00157000
*        2 SEEL PTR(31),            /* ADDRESS OF LIST OF EIDS       */ 00158000
*        2 SECL PTR(31),            /* ADDRESS OF LIST OF CLASSES    */ 00159000
*        2 SEMC PTR(31);            /* MCQE ADDRESS                  */ 00160000
*     DCL                                                          0097 00161000
*       1 SEPL0012 STATIC LOCAL BDY(WORD),/* ACTUAL PLIST FOR      0097 00162000
*                                      STANDARD INVOCATION           */ 00163000
*        2 SEFG0012 PTR(8),                                        0097 00164000
*        2 * PTR(8),                                               0097 00165000
*        2 SEEN0012 PTR(8),                                        0097 00166000
*        2 SECN0012 PTR(8),                                        0097 00167000
*        2 SENM0012 CHAR(8),                                       0097 00168000
*        2 SEEL0012 PTR(31),                                       0097 00169000
*        2 SECL0012 PTR(31),                                       0097 00170000
*        2 SEMC0012 PTR(31);                                       0097 00171000
*     AHLPPTR=ADDR(SEPL0012);       /* INIT PLIST PTR                */ 00172000
         LA    AHLPPTR,SEPL0012                                    0098 00173000
*     SEPL=SEPL&&SEPL;              /* ZERO OUT PLIST                */ 00174000
         XC    SEPL(24,AHLPPTR),SEPL(AHLPPTR)                      0099 00175000
*     SEMC=ADDR(MCQE);              /* INIT MCQE PTR                 */ 00176000
         L     @14,MCQEPTR(,PCTPTR)                                0100 00177000
         ST    @14,SEMC(,AHLPPTR)                                  0100 00178000
*     SENM='GTF';                   /* MOVE IN NAME                  */ 00179000
         MVI   SENM+3(AHLPPTR),C' '                                0101 00180000
         MVC   SENM+4(4,AHLPPTR),SENM+3(AHLPPTR)                   0101 00181000
         MVC   SENM(3,AHLPPTR),@CC01263                            0101 00182000
*     SEFG=64;                      /* MOVE IN FLAGS                 */ 00183000
         MVI   SEFG(AHLPPTR),X'40'                                 0102 00184000
*     CALL AHLSETEV;                /* CALL SERVICE                  */ 00185000
         L     @03,CVTPTR                                          0103 00186000
         L     @03,CVTGTF(,@03)                                    0103 00187000
         L     @15,MCHSETE(,@03)                                   0103 00188000
         BALR  @14,@15                                             0103 00189000
*   END;                            /* Y02014                        */ 00190000
*   IF RETCODE^=0 THEN              /* CHECK RETURN CODE       Y02014*/ 00191000
         LTR   RETCODE,RETCODE                                     0105 00192000
         BZ    @RF00105                                            0105 00193000
*     DO;                           /* ANY OTHER STEVNT ERR PATH   0106 00194000
*                                                              Y02014*/ 00195000
*       IF RETCODE=20 THEN          /* CHECK RETURN CODE       Y02014*/ 00196000
         C     RETCODE,@CF01355                                    0107 00197000
         BNE   @RF00107                                            0107 00198000
*         DO;                       /* UNABLE TO INIT CPU PATH Y02014*/ 00199000
*           AHLECB=AHLECB&&AHLECB;  /* ZERO OUT REPLY ECB      Y02014*/ 00200000
         L     @14,AHLECB(,PCTPTR)                                 0109 00201000
         LR    @03,@14                                             0109 00202000
         XR    @03,@14                                             0109 00203000
         ST    @03,AHLECB(,PCTPTR)                                 0109 00204000
*           DO;                     /* WTORP(REPLY,,AHLECB)        0110 00205000
*                                      MF(E,AHL018)                  */ 00206000
*             AHL01801=AHLECB;                                     0111 00207000
         L     @14,@CA01304                                        0111 00208000
         STC   @03,AHL01801-AHL018(,@14)                           0111 00209000
*             RFY                                                  0112 00210000
*               GPR01P RSTD;        /* RESTRICT REG 1                */ 00211000
*             GPR01P=ADDR(AHL018);  /* ADDRESS CONTROL PROGRAM LIST  */ 00212000
         LR    GPR01P,@14                                          0113 00213000
*             SVC(35);              /* ISSUE WTOR SVC                */ 00214000
         SVC   35                                                  0114 00215000
*             RFY                                                  0115 00216000
*               GPR01P UNRSTD;      /* RELEASE REG 1                 */ 00217000
*           END;                    /* WTORP(REPLY,,AHLECB)        0116 00218000
*                                      MF(E,AHL018) INDICATE ERROR 0116 00219000
*                                      MESSAGE Y02014                */ 00220000
*           DO;                     /* WAIT ECB(AHLECB)              */ 00221000
*             RESPECIFY                                            0118 00222000
*              (GPR00P,                                            0118 00223000
*               GPR01P) RESTRICTED;                                0118 00224000
*             GPR00P=1;             /* LOAD WAIT COUNT               */ 00225000
         LA    GPR00P,1                                            0119 00226000
*             GPR01P=ADDR(AHLECB);  /* ADDRESS OF ECB                */ 00227000
         LA    GPR01P,AHLECB(,PCTPTR)                              0120 00228000
*             SVC(1);               /* WAIT SVC                      */ 00229000
         SVC   1                                                   0121 00230000
*             RESPECIFY                                            0122 00231000
*              (GPR00P,                                            0122 00232000
*               GPR01P) UNRESTRICTED;                              0122 00233000
*           END;                    /* WAIT ECB(AHLECB) WAIT FOR   0123 00234000
*                                      REPLY Y02014                  */ 00235000
*           IF REPLY='END' THEN     /* OPERATOR REQUEST STOP?  Y02014*/ 00236000
         CLC   REPLY(3),@CC01382                                   0124 00237000
         BNE   @RF00124                                            0124 00238000
*             DO;                   /* REQUEST END PATH        Y02014*/ 00239000
*               DO;                 /* WTOP MF(E,AHL016)             */ 00240000
*                 RFY                                              0127 00241000
*                   GPR01P RSTD;    /* RESTRICT REG 1                */ 00242000
*                 GPR01P=ADDR(AHL016);/* ADDRESS CONTROL PROGRAM LIST*/ 00243000
         L     GPR01P,@CA01301                                     0128 00244000
*                 SVC(35);          /* ISSUE WTO SVC                 */ 00245000
         SVC   35                                                  0129 00246000
*                 RFY                                              0130 00247000
*                   GPR01P UNRSTD;  /* RELEASE REG 1                 */ 00248000
*               END;                /* WTOP MF(E,AHL016) ISSUE GTF 0131 00249000
*                                      FAILED MSG Y02014             */ 00250000
*               GEN(ESTAE 0);       /* CANCEL ESTAE            Y02014*/ 00251000
         ESTAE 0                                                        00252000
*               GO TO AHLTERM3;     /* GO TERMINATE            Y02014*/ 00253000
         B     AHLTERM3                                            0133 00254000
*             END;                  /* END OF REQUEST END PATH Y02014*/ 00255000
*         END;                      /* END OF INIT CPU PATH    Y02014*/ 00256000
*       ELSE                                                       0136 00257000
*         DO;                       /* START OTHER ERR PATH    Y02014*/ 00258000
         B     @RC00107                                            0136 00259000
@RF00107 DS    0H                                                  0137 00260000
*           BINRYRC=RETCODE;        /* SET RETURN CODE IN MIDDLE OF     00261000
*                                      THREE BYTE AREA         Y02014*/ 00262000
         STCM  RETCODE,3,BINRYRC                                   0137 00263000
*           UNPK(TEMPRC,RC);        /* UNPACK RETURN CODE      Y02014*/ 00264000
         UNPK  TEMPRC(3),RC(3)                                     0138 00265000
*           RCIND=TEMPRC;           /* MOVE TO TWO BYTE AREA   Y02014*/ 00266000
         L     @14,@CA01270                                        0139 00267000
         MVC   RCIND-AHL019(2,@14),TEMPRC                          0139 00268000
*           TR(RCIND,DEFTABLE);     /* TRANSLATE UNPACKED BYTES    0140 00269000
*                                                              Y02014*/ 00270000
         TR    RCIND-AHL019(2,@14),DEFTABLE                        0140 00271000
*           DO;                     /* WTOP MF(E,AHL019)             */ 00272000
*             RFY                                                  0142 00273000
*               GPR01P RSTD;        /* RESTRICT REG 1                */ 00274000
*             GPR01P=ADDR(AHL019);  /* ADDRESS CONTROL PROGRAM LIST  */ 00275000
         LR    GPR01P,@14                                          0143 00276000
*             SVC(35);              /* ISSUE WTO SVC                 */ 00277000
         SVC   35                                                  0144 00278000
*             RFY                                                  0145 00279000
*               GPR01P UNRSTD;      /* RELEASE REG 1                 */ 00280000
*           END;                    /* WTOP MF(E,AHL019) INDICATE  0146 00281000
*                                      ERROR MESSAGE Y02014          */ 00282000
*           DO;                     /* WTOP MF(E,AHL016)             */ 00283000
*             RFY                                                  0148 00284000
*               GPR01P RSTD;        /* RESTRICT REG 1                */ 00285000
*             GPR01P=ADDR(AHL016);  /* ADDRESS CONTROL PROGRAM LIST  */ 00286000
         L     GPR01P,@CA01301                                     0149 00287000
*             SVC(35);              /* ISSUE WTO SVC                 */ 00288000
         SVC   35                                                  0150 00289000
*             RFY                                                  0151 00290000
*               GPR01P UNRSTD;      /* RELEASE REG 1                 */ 00291000
*           END;                    /* WTOP MF(E,AHL016) ISSUE GTF 0152 00292000
*                                      INIT FAILED MSG Y02014        */ 00293000
*           GEN(ESTAE 0 );          /* CANCEL ESTAE            Y02014*/ 00294000
         ESTAE 0                                                        00295000
*           GO TO AHLTERM2;         /* GO ISSUE MSG & TERMINATE    0154 00296000
*                                                              Y02014*/ 00297000
         B     AHLTERM2                                            0154 00298000
*         END;                      /* END OF ANY OTHER ERR PATH   0155 00299000
*                                                              Y02014*/ 00300000
*     END;                          /* END OF ALL ERR PATHS    Y02014*/ 00301000
@RC00107 DS    0H                                                  0157 00302000
*                                                                  0157 00303000
*   /*****************************************************************/ 00304000
*   /*                                                               */ 00305000
*   /* ACTIVATE CVT ACTIVE BIT,TURN OFF SYSTEM TRACE, AND CREATE ECB */ 00306000
*   /* ADDRESS LIST                                                  */ 00307000
*   /*                                                               */ 00308000
*   /*****************************************************************/ 00309000
*                                                                  0157 00310000
*   CVTGTFAV='1'B;                  /* SET GTF ACTIVE BITS     Y02014*/ 00311000
@RF00105 L     @14,CVTPTR                                          0157 00312000
         OI    CVTGTFAV(@14),B'10000000'                           0157 00313000
*   IF OPTRNIO='1'B THEN            /* CHECK FOR RNIO REQUEST  Y02014*/ 00314000
         TM    OPTRNIO(PCTPTR),B'01000000'                         0158 00315000
         BNO   @RF00158                                            0158 00316000
*     DO;                           /* START OF RNIO REQUEST PATH  0159 00317000
*                                                              Y02014*/ 00318000
*       CVTRNIO='1'B;               /* TURN ON RNIO INDICATOR IN CVT    00319000
*                                                              Y02014*/ 00320000
         OI    CVTRNIO(@14),B'00000010'                            0160 00321000
*     END;                          /* END OF RNIO REQUEST PATH    0161 00322000
*                                                              Y02014*/ 00323000
*   IF OPTUSR='1'B THEN             /* IF USER EVENTS SELECTED     0162 00324000
*                                                            @Y30AI3M*/ 00325000
@RF00158 TM    OPTUSR(PCTPTR),B'00010000'                          0162 00326000
         BNO   @RF00162                                            0162 00327000
*     DO;                           /* TURN ON USER INDICATOR IN CVT    00328000
*                                                            @Y30AI3M*/ 00329000
*       CVTUSR='1'B;                /*                       @Y30AI3M*/ 00330000
         L     @14,CVTPTR                                          0164 00331000
         OI    CVTUSR(@14),B'00000100'                             0164 00332000
*     END;                          /*                       @Y30AI3M*/ 00333000
*   CVTTRACE=TRACEOFF;              /* TURN OFF SYSTEM TRACE @YM03608*/ 00334000
@RF00162 L     @14,CVTPTR                                          0166 00335000
         MVC   CVTTRACE(2,@14),TRACEOFF                            0166 00336000
*   ECBADRLT=ADDR(ECBLIST);         /* PUT ECB LIST ADDR IN GTFPCT 0167 00337000
*                                                              Y02014*/ 00338000
         LA    @14,ECBLIST                                         0167 00339000
         ST    @14,ECBADRLT(,PCTPTR)                               0167 00340000
*   ECBATERM=ADDR(MCQETECB);        /* FILL ECBADDR LIST WITH  Y02014*/ 00341000
         L     @14,MCQEPTR(,PCTPTR)                                0168 00342000
         LA    @14,MCQETECB(,@14)                                  0168 00343000
         ST    @14,ECBATERM                                        0168 00344000
*   ECBAWTR=ADDR(ATTCHECB);         /* WRITERS ATTACH ECB      Y02014*/ 00345000
         LA    @14,ATTCHECB(,PCTPTR)                               0169 00346000
         ST    @14,ECBAWTR                                         0169 00347000
*   ECBAWERR=ADDR(WRTRERR);         /* WRITERS ERROR ECB       Y02014*/ 00348000
         LA    @14,WRTRERR(,PCTPTR)                                0170 00349000
         ST    @14,ECBAWERR                                        0170 00350000
*   ECBAWTSK=ADDR(WTSKATTH);        /* ATTACH ECB              Y02014*/ 00351000
         LA    @14,WTSKATTH(,PCTPTR)                               0171 00352000
         ST    @14,ECBAWTSK                                        0171 00353000
*   ECBASTOP=STOPECBA;              /* STOP ECB ADDR           Y02014*/ 00354000
         L     @14,STOPECBA(,PCTPTR)                               0172 00355000
         ST    @14,ECBASTOP                                        0172 00356000
*   ECBSTOPE='1'B;                  /* INDICATE LAST ECB IN LIST   0173 00357000
*                                                              Y02014*/ 00358000
         OI    ECBSTOPE,B'10000000'                                0173 00359000
*   DO;                             /* WTOP MF(E,AHL031)             */ 00360000
*     RFY                                                          0175 00361000
*       GPR01P RSTD;                /* RESTRICT REG 1                */ 00362000
*     GPR01P=ADDR(AHL031);          /* ADDRESS CONTROL PROGRAM LIST  */ 00363000
         L     GPR01P,@CA01302                                     0176 00364000
*     SVC(35);                      /* ISSUE WTO SVC                 */ 00365000
         SVC   35                                                  0177 00366000
*     RFY                                                          0178 00367000
*       GPR01P UNRSTD;              /* RELEASE REG 1                 */ 00368000
*   END;                            /* WTOP MF(E,AHL031) INDICATE GTF   00369000
*                                      INIT COMPLET Y02014           */ 00370000
*                                                                  0180 00371000
*   /*****************************************************************/ 00372000
*   /*                                                               */ 00373000
*   /* ISSUE WAIT ON ECBLIST ONE OF FIVE ECBS MAY BE POSTED, THEY ARE*/ 00374000
*   /* AS FOLLOWS: 1) STOP COMMAND ECB--OPERATOR ISSUES STOP COMMAND */ 00375000
*   /* 2) WRITER ATTACH ECB--WRITER IS TERMINATING ABNORMALLY 3)     */ 00376000
*   /* AHLWTASK ATTACH ECB--AHLWTASK IS TERMINATING ABNORMALLY 4)    */ 00377000
*   /* SYSTEM TERMINATION ECB 5) WRITER ERROR ECB                    */ 00378000
*   /*                                                               */ 00379000
*   /*****************************************************************/ 00380000
*                                                                  0180 00381000
*   DO;                             /* WAIT ECBLIST(ECBLIST)         */ 00382000
*     RESPECIFY                                                    0181 00383000
*      (GPR00P,                                                    0181 00384000
*       GPR01P) RESTRICTED;                                        0181 00385000
*     GPR00P=1;                     /* LOAD WAIT COUNT               */ 00386000
         LA    GPR00P,1                                            0182 00387000
*     GPR01P=-ADDR(ECBLIST);        /* ADDRESS OF LIST OF ECB'S      */ 00388000
         LA    GPR01P,ECBLIST                                      0183 00389000
         LCR   GPR01P,GPR01P                                       0183 00390000
