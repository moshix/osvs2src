*     RFY                                                          0383 01013000
*       R14 UNRSTD;                                                0383 01014000
*     RFY                                                          0384 01015000
*       LCCA BASED(PSALCCAV);                                      0384 01016000
*     RFY                                                          0385 01017000
*       MCRWSA BASED(WSACGTF);                                     0385 01018000
*     AWSAPTR=MCRMCA;               /* GET ADDRESS OF MCAWSA AT ERROR*/ 01019000
         L     @06,PSALCCAV                                        0386 01020000
         L     @06,LCCACPUS(,@06)                                  0386 01021000
         L     @06,WSACGTF(,@06)                                   0386 01022000
         L     AWSAPTR,MCRMCA(,@06)                                0386 01023000
*     RFY                                                          0387 01024000
*       MCRWSA BASED(MCAMCR);                                      0387 01025000
*     SVREG1=R1;                    /* SAVE SDWA BASE                */ 01026000
         ST    R1,SVREG1(,AWSAPTR)                                 0388 01027000
*     MCAERROR=ON;                  /* INDICATE ERROR ROUTINE IN   0389 01028000
*                                      CONTROL                       */ 01029000
         OI    MCAERROR(AWSAPTR),B'00000100'                       0389 01030000
*     R0=MCROUTSA(1:4);             /* GET ADDRESS OF MCQE AT TIME OF   01031000
*                                      ERROR                         */ 01032000
         L     @06,MCAMCR(,AWSAPTR)                                0390 01033000
         ICM   R0,15,MCROUTSA(@06)                                 0390 01034000
*     PCTPTR=R0->MCQEAT;            /* GET ADDRESS OF PCT AT TIME OF    01035000
*                                      ERROR                         */ 01036000
         LR    @06,R0                                              0391 01037000
         L     PCTPTR,MCQEAT(,@06)                                 0391 01038000
*                                                                  0392 01039000
*     /***************************************************************/ 01040000
*     /*                                                             */ 01041000
*     /* DETERMINE WHAT THE ERROR WAS                                */ 01042000
*     /*                                                             */ 01043000
*     /***************************************************************/ 01044000
*                                                                  0392 01045000
*     ;                             /* CHECK FOR ALTERNATE CPU     0392 01046000
*                                      RECOVERY (ACR) ERROR          */ 01047000
*     IF SDWAMCHK=ON THEN           /* IF MACHINE CHECK              */ 01048000
*                                                                  0393 01049000
         TM    SDWAMCHK(SDWAPTR),B'10000000'                       0393 01050000
         BNO   @RF00393                                            0393 01051000
*       /*************************************************************/ 01052000
*       /*                                                           */ 01053000
*       /* PROCESSING FOR MACHINE CHECK FAILURE - IF THE FAILURE IS  */ 01054000
*       /* AN INSTRUCTION FAILURE AND BOTH THE REGISTERS AND PSW ARE */ 01055000
*       /* KNOWN REQUEST CONTINUATION AT THE NEXT SEQUENTIAL         */ 01056000
*       /* INSTRUCTION. OTHERWISE CATASTROPHIC ERROR PROCESSING IS   */ 01057000
*       /* PERFORMED                                                 */ 01058000
*       /*                                                           */ 01059000
*       /*************************************************************/ 01060000
*                                                                  0394 01061000
*       DO;                                                        0394 01062000
*         IF SDWAINSF=OFF THEN      /* IF NOT INSTRUCTION FAILURE    */ 01063000
         TM    SDWAINSF(SDWAPTR),B'00000100'                       0395 01064000
         BZ    @RT00395                                            0395 01065000
*           GOTO CATERR;            /* CATASTROPHIC ERROR            */ 01066000
*         ELSE                                                     0397 01067000
*           IF SDWAREGU=ON|SDWAPSWU=ON THEN/* IF REGS OR PSW       0397 01068000
*                                      UNPREDICTABLE                 */ 01069000
         TM    SDWAREGU(SDWAPTR),B'01100000'                       0397 01070000
         BNZ   @RT00397                                            0397 01071000
*             GOTO CATERR;          /* CATASTROPHIC ERROR            */ 01072000
*           ELSE                                                   0399 01073000
*             DO;                   /* RC=RETRY, GET RETRY ADDRESS 0399 01074000
*                                      FROM SDWA                     */ 01075000
*               DO;                 /* SETRP RECPARM(RECID)RETADDR(AC   01076000
*                                      RADDR)RECORD(YES)RC(SDWARETY) */ 01077000
*                 RESPECIFY                                        0401 01078000
