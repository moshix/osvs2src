*         ELSE                                                     0149 00199000
*           PICDENAM=NOTAPP;        /* OTHERWISE, NOT APPLICABLE   0149 00200000
*                                                            @ZA09668*/ 00201000
         B     @RC00147                                            0149 00202000
@RF00147 MVC   PICDENAM(8,AWSAPTR),@CC02967                        0149 00203000
*         GO TO ENDCD;                                             0150 00204000
         B     ENDCD                                               0150 00205000
*CDE40A:                            /* RBSTAB1 = '4-'                */ 00206000
*         PICDENAM=IRBCDE;          /* CDENAME='**IRB***'            */ 00207000
CDE40A   MVC   PICDENAM(8,AWSAPTR),@CC02977                        0151 00208000
*         GO TO ENDCD;                                             0152 00209000
         B     ENDCD                                               0152 00210000
*CDE80A:                            /* RBSTAB1 = '8-'                */ 00211000
*         PICDENAM=RBEXRTNM;        /* CDENAME=ERROR EXIT ROUTINE    */ 00212000
CDE80A   MVC   PICDENAM(8,AWSAPTR),RBEXRTNM(RBPTR)                 0153 00213000
*         IF PINMCHK=0 THEN         /* CHECK FOR 1ST CHAR OF NAME=0  */ 00214000
         CLI   PINMCHK(AWSAPTR),0                                  0154 00215000
         BNE   @RF00154                                            0154 00216000
*           PICDENAM(1:1)=ERRIND;   /* OVERLAY 1ST CHAR W/ '*'       */ 00217000
