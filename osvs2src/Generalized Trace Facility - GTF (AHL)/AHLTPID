*                                                                  0619 01844000
CATERR   DS    0H                                                  0620 01845000
*     /***************************************************************/ 01846000
*     /*                                                             */ 01847000
*     /* IF AN UNRECOVERABLE ERROR IS ENCOUNTERED AND DEBUG=NO THIS  */ 01848000
*     /* ROUTINE WILL BE CALLED TO STACK THE ERROR RECORD, TURN OFF  */ 01849000
*     /* THE ACTIVE FUNCTION BIT FOR THE EVENT BEING TRACED, ISSUE   */ 01850000
*     /* THE SETEVENT MACRO TO DISABLE TRACING ON THE EVENT(S) BEING */ 01851000
*     /* TRACED WHEN THE ERROR OCCURRED, CALLING THE DUMPING ROUTINE */ 01852000
*     /* AND THEN RETURN TO MCIH WITHOUT TERMINATING ALL OF GTF. IF  */ 01853000
*     /* DSP,SRB OR LSR WAS BEING TRACED, TRACING MUST BE TERMINATED */ 01854000
*     /* FOR ALL SINCE THEY ARE ALL STARTED WITH THE SAME TRACE      */ 01855000
*     /* OPTION KEYWORD.                                             */ 01856000
*     /*                                                             */ 01857000
*     /***************************************************************/ 01858000
*                                                                  0620 01859000
*     ERRASCB=CERRIND;              /* INDICATE CATASTROPHIC ERROR IN   01860000
*                                      FIRST OF RECORD               */ 01861000
         MVC   ERRASCB(2,AWSAPTR),@CB02981                         0620 01862000
*     DO;                           /* STACK THE RECORD              */ 01863000
*       DO;                         /* AHLSTACK DLENGTH(DEFLENG)AID(A   01864000
*                                      IDC)FID(FIDC)                 */ 01865000
*                                                                  0622 01866000
*         /***********************************************************/ 01867000
*         /*                                                         */ 01868000
*         /* THE FOLLOWING CODE IS GENERATED BY THE AHLSTACK MACRO.  */ 01869000
*         /* THIS CODE BUILDS THE GTF RECORD PREFIX AND THEN CALLS   */ 01870000
*         /* AHLSBLOK TO STACK THE RECORD.                           */ 01871000
*         /*                                                         */ 01872000
*         /***********************************************************/ 01873000
*                                                                  0623 01874000
*         RFY                                                      0623 01875000
*          (AHLRECPR,                                              0623 01876000
*           AHLSBREG,                                              0623 01877000
*           AHLRETCD) RSTD;                                        0623 01878000
*         IF OPTTIME='1'B THEN      /* CHECK FOR TIMESTAMP REQUESTED */ 01879000
         TM    OPTTIME(PCTPTR),B'00000001'                         0624 01880000
         BNO   @RF00624                                            0624 01881000
*           DO;                     /* YES, TIMESTAMP                */ 01882000
*             AHLRECPR=ADDR(MCARECPR);/* SETUP ADDRESSIBILITY TO   0626 01883000
*                                      PREFIX FOR PREFIX WITH      0626 01884000
*                                      TIMESTAMP                     */ 01885000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0626 01886000
*             AHLGLGTH=DEFLENG+16;  /* SET LENGTH TO INCLUDE ***   0627 01887000
*                                      DLENGTH *** PLUS TIMESTAMP  0627 01888000
*                                      PREFIX                        */ 01889000
         MVC   AHLGLGTH(2,AHLRECPR),@CH03166                       0627 01890000
*             GEN                                                  0628 01891000
*(STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX )         0628 01892000
*             SETS(AHLGTST) NOEXIT SEQFLOW NOFLOWS;                0628 01893000
         STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX         01894000
*             AHLGEIDL=MCAMCR->MCREID;/* *** EID *** NOT SPECIFIED 0629 01895000
*                                      PUT DEFAULT INTO PREFIX       */ 01896000
         L     @06,MCAMCR(,AWSAPTR)                                0629 01897000
         LH    @06,MCREID(,@06)                                    0629 01898000
         N     @06,@CF03169                                        0629 01899000
         STH   @06,AHLGEIDL(,AHLRECPR)                             0629 01900000
*           END;                                                   0630 01901000
*         ELSE                      /* NO TIMESTAMP TO BE DONE, BUILD   01902000
*                                      SHORT PREFIX                  */ 01903000
*           DO;                                                    0631 01904000
         B     @RC00624                                            0631 01905000
@RF00624 DS    0H                                                  0632 01906000
*             AHLRECPR=ADDR(MCARECPR)+8;/* SETUP ADDRESSIBILITY TO 0632 01907000
*                                      FOR SHORT PREFIX WITHOUT    0632 01908000
*                                      TIMESTAMP                     */ 01909000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0632 01910000
         AL    AHLRECPR,@CF00038                                   0632 01911000
*             AHLGLGTH=DEFLENG+8;   /* PUT TOTAL RECORD LENGTH INTO     01912000
*                                      PREFIX *** DLENGTH *** PLUS 0633 01913000
*                                      PREFIX LENGTH                 */ 01914000
         MVC   AHLGLGTH(2,AHLRECPR),@CH03167                       0633 01915000
*             AHLGEIDS=MCAMCR->MCREID;/* *** EID *** NOT SPECIFIED 0634 01916000
*                                      PUT DEFAULT INTO PREFIX       */ 01917000
         L     @06,MCAMCR(,AWSAPTR)                                0634 01918000
         LH    @06,MCREID(,@06)                                    0634 01919000
         N     @06,@CF03169                                        0634 01920000
         STH   @06,AHLGEIDS(,AHLRECPR)                             0634 01921000
*           END;                                                   0635 01922000
*         AHLGRES=0;                /* ZERO OUT FOR QSAM           0636 01923000
*                                      COMPATIBILITY                 */ 01924000
@RC00624 SLR   @06,@06                                             0636 01925000
         STH   @06,AHLGRES(,AHLRECPR)                              0636 01926000
*         AHLGAID=AIDC;             /* PUT *** AID *** INTO PREFIX   */ 01927000
         MVI   AHLGAID(AHLRECPR),X'FF'                             0637 01928000
*         AHLGFID=FIDC;             /* PUT *** FID *** INTO PREFIX   */ 01929000
         MVI   AHLGFID(AHLRECPR),X'02'                             0638 01930000
*         AHLRETCD=4;               /* SET DEFAULT RETURN CODE       */ 01931000
         LA    AHLRETCD,4                                          0639 01932000
*         AHLSBREG=SBLOKRTN;        /* GET A STABLE COPY OF AHLSBLOK    01933000
*                                      ADDR                          */ 01934000
         L     AHLSBREG,SBLOKRTN(,PCTPTR)                          0640 01935000
*         IF AHLSBREG^=0 THEN       /* CHECK TO SEE IF AHLSBLOK IS 0641 01936000
*                                      AVAILABLE                     */ 01937000
         CR    AHLSBREG,@06                                        0641 01938000
         BE    @RF00641                                            0641 01939000
*           CALL AHLSBLOK;          /* YES, SO CALL AHLSBLOK TO BLOCK   01940000
*                                      RECORD                        */ 01941000
         LR    @15,AHLSBREG                                        0642 01942000
         BALR  @14,@15                                             0642 01943000
*         RFY                                                      0643 01944000
*          (AHLRECPR,                                              0643 01945000
*           AHLSBREG,                                              0643 01946000
*           AHLRETCD) UNRSTD;                                      0643 01947000
@RF00641 DS    0H                                                  0644 01948000
*       END;                                                       0644 01949000
*       R1=SVREG1;                  /* RESTORE SDWA BASE             */ 01950000
*                                                                  0645 01951000
         L     R1,SVREG1(,AWSAPTR)                                 0645 01952000
*       /*************************************************************/ 01953000
*       /*                                                           */ 01954000
*       /* TAKE A DUMP AND SET UP FOR WTO                            */ 01955000
*       /*                                                           */ 01956000
*       /*************************************************************/ 01957000
*                                                                  0646 01958000
*       R0=ADDR(SDUMPLST);          /* POINT TO SDUMP PLIST          */ 01959000
         LA    R0,SDUMPLST                                         0646 01960000
*       CALL DUMPMOD;                                              0647 01961000
         L     @15,PCTDMPMD(,PCTPTR)                               0647 01962000
         BALR  @14,@15                                             0647 01963000
*       DUMPIND=R15CODE;            /* STORE INDICATOR IN WTO LIST   */ 01964000
         STH   R15CODE,DUMPIND(,AWSAPTR)                           0648 01965000
*       WTOPARM(1:LENGTH(WTOSKELT))=WTOSKELT;/* MOVE IN BODY OF WTO     01966000
*                                      LIST                          */ 01967000
         MVC   WTOPARM(14,AWSAPTR),WTOSKELT                        0649 01968000
*       R0=MCROUTSA(1:4);           /* RESTORE MCQE ADDRESS          */ 01969000
         L     @06,MCAMCR(,AWSAPTR)                                0650 01970000
         ICM   R0,15,MCROUTSA(@06)                                 0650 01971000
*     END;                                                         0651 01972000
*                                                                  0651 01973000
*     /***************************************************************/ 01974000
*     /*                                                             */ 01975000
*     /* DETERMINE WHICH FUNCTION(S) IS ACTIVE AND TURN OFF ITS      */ 01976000
*     /* CORRESPONDING BIT(S) IN THE PCTCATF FIELD                   */ 01977000
*     /*                                                             */ 01978000
*     /***************************************************************/ 01979000
*                                                                  0652 01980000
*     ;                                                            0652 01981000
*     IF MCREID=IEAPINT|MCREID=IEATINT THEN                        0653 01982000
         CLC   MCREID(2,@06),@CH01378                              0653 01983000
         BE    @RT00653                                            0653 01984000
         CLC   MCREID(2,@06),@CH01382                              0653 01985000
         BNE   @RF00653                                            0653 01986000
@RT00653 DS    0H                                                  0654 01987000
*       DO;                         /* CHECK FOR PI EVENT            */ 01988000
*         TRAILSAV=PIONBIT;         /* IF SO SET PI BIT ONLY         */ 01989000
         MVC   TRAILSAV(4,R7PARM),@CB02959                         0655 01990000
*         WTOOPT=PIIND;             /* MOVE INDICATOR TO WTO LIST    */ 01991000
         MVC   WTOOPT(4,AWSAPTR),PIIND                             0656 01992000
*       END;                                                       0657 01993000
*     ELSE                                                         0658 01994000
*       DO;                                                        0658 01995000
         B     @RC00653                                            0658 01996000
@RF00653 DS    0H                                                  0659 01997000
*         TRAILSAV=DSPONBIT;        /* OTHERWISE SET DSP BIT ONLY    */ 01998000
