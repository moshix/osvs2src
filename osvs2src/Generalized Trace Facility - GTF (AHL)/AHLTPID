*   DO;                                                            0378 00983000
*                                                                  0378 00984000
*     /***************************************************************/ 00985000
*     /*                                                             */ 00986000
*     /* THIS IS THE FRR FOR AHLTPID. IF AN ERROR OCCURS DURING THE  */ 00987000
*     /* EXECUTION OF THE MODULE THIS ROUTINE GAINS CONTROL. CERTAIN */ 00988000
*     /* ERROR CONDITIONS ARE CHECKED AND APPROPRIATE ACTION IS TAKEN*/ 00989000
*     /* DEPENDENT ON THE ERROR                                      */ 00990000
*     /*                                                             */ 00991000
*     /***************************************************************/ 00992000
*                                                                  0379 00993000
*     ;                                                            0379 00994000
*                                                                  0379 00995000
*     /***************************************************************/ 00996000
*     /*                                                             */ 00997000
*     /* GET THE FRR PARAMETER FIELD FROM THE SDWA, SAVE REG14 FOR   */ 00998000
*     /* RETURN TO RTM, RE-ESTABLISH ADDRESSABILITY TO THE MCAWSA,   */ 00999000
*     /* TURN ON THE ERROR BIT IN THE MCATRAIL, AND RE-ESTABLISH     */ 01000000
*     /* ADDRESSABILITY TO THE GTFPCT AND THE MCQE.                  */ 01001000
*     /*                                                             */ 01002000
*     /***************************************************************/ 01003000
*                                                                  0380 01004000
*     RFY                                                          0380 01005000
*      (R1,                                                        0380 01006000
*       R14) RSTD;                                                 0380 01007000
*     R7PARM=SDWAPARM;              /* PICK UP ADDRESS OF FRR PARM 0381 01008000
*                                      FIELD                         */ 01009000
         L     R7PARM,SDWAPARM(,SDWAPTR)                           0381 01010000
*     SVR14FRR=R14;                 /* SAVE RETURN ADDRESS OF RTM    */ 01011000
