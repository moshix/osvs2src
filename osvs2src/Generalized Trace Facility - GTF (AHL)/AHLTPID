*                                                                  0176 00278000
*AHLPIJ:                                                           0176 00279000
*     MCATRL6=FRPIJ;                /* INDICATE RETURN POINT FOR   0176 00280000
*                                      ERRORS GATHERING THE JOBNAME  */ 00281000
@RC00130 DS    0H                                                  0176 00282000
AHLPIJ   MVI   MCATRL6(AWSAPTR),X'80'                              0176 00283000
*     MCATRL8=UNRECOV;              /* RESET LPDE INDICATOR  @YM09150*/ 00284000
         MVI   MCATRL8(AWSAPTR),X'00'                              0177 00285000
*     R8WORK=ASCBJBNI;              /* GET FIRST JOBNAME ADDRESS     */ 00286000
         L     R8WORK,ASCBJBNI(,ASCBPTR)                           0178 00287000
*     IF R8WORK=0 THEN              /* CHECK FOR ADDRESS OF 0        */ 00288000
         SLR   @09,@09                                             0179 00289000
         CR    R8WORK,@09                                          0179 00290000
         BNE   @RF00179                                            0179 00291000
*       DO;                                                        0180 00292000
*         R8WORK=ASCBJBNS;          /* IF SO GET SECOND JOBNAME @    */ 00293000
         L     R8WORK,ASCBJBNS(,ASCBPTR)                           0181 00294000
*         RFY                                                      0182 00295000
*          (ASCBPTR) UNRSTD;                                       0182 00296000
*         IF R8WORK=0 THEN          /* CHECK FOR ADDRESS OF 0        */ 00297000
         CR    R8WORK,@09                                          0183 00298000
         BE    @RT00183                                            0183 00299000
*           GO TO NOJBNM;           /* IF SO JOBNAME IS N/A          */ 00300000
*       END;                                                       0185 00301000
*     IF JBNMTEST^=0 THEN           /* CHECK FOR VALID JOBNAME       */ 00302000
@RF00179 L     @09,JBNMTEST(,R8WORK)                               0186 00303000
         LTR   @09,@09                                             0186 00304000
         BZ    @RF00186                                            0186 00305000
*       PIJBNM=JOBNAME;             /* IF SO MOVE IT TO RECORD       */ 00306000
         MVC   PIJBNM(8,AWSAPTR),JOBNAME(R8WORK)                   0187 00307000
*     ELSE                                                         0188 00308000
*NOJBNM:                                                           0188 00309000
*       PIJBNM=NOTAPP;              /* OTHERWISE JOBNAME = 'N/A'     */ 00310000
         B     @RC00186                                            0188 00311000
@RF00186 DS    0H                                                  0188 00312000
