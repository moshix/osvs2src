*AHLPIEND:                                                         0189 00314000
*     MCATRL6=UNRECOV;              /* RESET AUDIT TRAIL TO 0        */ 00315000
@RC00186 DS    0H                                                  0189 00316000
AHLPIEND MVI   MCATRL6(AWSAPTR),X'00'                              0189 00317000
*     RECLENG=LENGTH(PIREC);        /* GET RECORD LENGTH             */ 00318000
         LA    RECLENG,102                                         0190 00319000
*     GOTO AHLDOUT;                 /* BRANCH TO STACK RECORD,     0191 00320000
*                                      CLEANUP, AND EXIT             */ 00321000
         B     AHLDOUT                                             0191 00322000
*     RFY                                                          0192 00323000
*       R8WORK UNRSTD;                                             0192 00324000
