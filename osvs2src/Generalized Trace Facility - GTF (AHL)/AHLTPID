         L     @09,MCAMCR(,AWSAPTR)                                0357 00921000
         LH    @09,MCREID(,@09)                                    0357 00922000
         N     @09,@CF03169                                        0357 00923000
         STH   @09,AHLGEIDS(,AHLRECPR)                             0357 00924000
*           END;                                                   0358 00925000
*         AHLGRES=0;                /* ZERO OUT FOR QSAM           0359 00926000
*                                      COMPATIBILITY                 */ 00927000
@RC00347 SLR   @09,@09                                             0359 00928000
         STH   @09,AHLGRES(,AHLRECPR)                              0359 00929000
*         AHLGAID=AIDC;             /* PUT *** AID *** INTO PREFIX   */ 00930000
         MVI   AHLGAID(AHLRECPR),X'FF'                             0360 00931000
*         AHLGFID=FIDC;             /* PUT *** FID *** INTO PREFIX   */ 00932000
         MVI   AHLGFID(AHLRECPR),X'02'                             0361 00933000
*         AHLRETCD=4;               /* SET DEFAULT RETURN CODE       */ 00934000
         LA    AHLRETCD,4                                          0362 00935000
*         AHLSBREG=SBLOKRTN;        /* GET A STABLE COPY OF AHLSBLOK    00936000
*                                      ADDR                          */ 00937000
         L     AHLSBREG,SBLOKRTN(,PCTPTR)                          0363 00938000
*         IF AHLSBREG^=0 THEN       /* CHECK TO SEE IF AHLSBLOK IS 0364 00939000
*                                      AVAILABLE                     */ 00940000
         CR    AHLSBREG,@09                                        0364 00941000
         BE    @RF00364                                            0364 00942000
*           CALL AHLSBLOK;          /* YES, SO CALL AHLSBLOK TO BLOCK   00943000
*                                      RECORD                        */ 00944000
         LR    @15,AHLSBREG                                        0365 00945000
         BALR  @14,@15                                             0365 00946000
*         RFY                                                      0366 00947000
*          (AHLRECPR,                                              0366 00948000
*           AHLSBREG,                                              0366 00949000
*           AHLRETCD) UNRSTD;                                      0366 00950000
@RF00364 DS    0H                                                  0367 00951000
*       END;                                                       0367 00952000
*       R0=SVREG0;                  /* RESTORE MCQE BASE             */ 00953000
         L     R0,SVREG0(,AWSAPTR)                                 0368 00954000
*     END;                                                         0369 00955000
*PIDEXIT:                                                          0370 00956000
*MCIHRTN:                                                          0370 00957000
*     RFY                                                          0370 00958000
*       R14 RSTD;                                                  0370 00959000
