*AHLTDCM:                                                          0300 00687000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0300 00688000
*                                                                  0300 00689000
AHLTDCM  DS    0H                                                  0301 00690000
*   /*****************************************************************/ 00691000
*   /*                                                               */ 00692000
*   /* THIS PROCEDURE PERFORMS COMMON HOUSEKEEPING AND GATHERING     */ 00693000
*   /* FUNCTIONS FOR DISPATCHER EVENTS                               */ 00694000
*   /*                                                               */ 00695000
*   /*****************************************************************/ 00696000
*                                                                  0301 00697000
*   ;                                                              0301 00698000
*                                                                  0301 00699000
*   /*****************************************************************/ 00700000
*   /*                                                               */ 00701000
*   /* HOUSEKEEPING - DEFINE THE ERROR ENVIRONMENT VIA SETFRR, SAVE  */ 00702000
*   /* REG10 (RETURN@ TO MCIH) AND REG14 IN THE PARM AREA, AND       */ 00703000
*   /* DETERMINE IF THE HOOK IS VALID                                */ 00704000
*   /*                                                               */ 00705000
*   /*****************************************************************/ 00706000
*                                                                  0302 00707000
*   RFY                                                            0302 00708000
*    (R14,                                                         0302 00709000
*     R10SAVE,                                                     0302 00710000
*     FRRADDR,                                                     0302 00711000
*     R8WORK) RSTD;                                                0302 00712000
*   FRRADDR=ADDR(AHLTPIDE);         /* GET ADDRESS OF THE RECOVERY 0303 00713000
*                                      ROUTINE                       */ 00714000
         LA    FRRADDR,AHLTPIDE                                    0303 00715000
*   SVREG0=R0;                      /* SAVE MCQE BASE                */ 00716000
         ST    R0,SVREG0(,AWSAPTR)                                 0304 00717000
*   GEN SETS(R7PARM,R8WORK) REFS(FRRADDR,PSACSTK,FRRSCURR,FRRSLAST,     00718000
*       FRRSELEN,FRRSFRRA,FRRSPARM) NOEXIT NOFLOWS SEQFLOW;        0305 00719000
         SETFRR A,FRRAD=(FRRADDR),PARMAD=(R7PARM),                     X00720000
               WRKREGS=(R7PARM,R8WORK)                                  00721000
*   R0=SVREG0;                      /* RESTORE MCQE BASE             */ 00722000
         L     R0,SVREG0(,AWSAPTR)                                 0306 00723000
*   SVR14NRM=R10SAVE;               /* SAVE MCIH'S REG 14            */ 00724000
         ST    R10SAVE,SVR14NRM(,R7PARM)                           0307 00725000
*   SVR14CAL=R14;                   /* SAVE CALLER'S REG 14          */ 00726000
         ST    R14,SVR14CAL(,R7PARM)                               0308 00727000
*   RFY                                                            0309 00728000
*    (R14,                                                         0309 00729000
*     R10SAVE,                                                     0309 00730000
*     R8WORK) UNRSTD;                                              0309 00731000
*   IF KEYTEST=ON THEN              /* CHECK FOR KEY 0 - 7           */ 00732000
         TM    KEYTEST,B'10000000'                                 0310 00733000
         BO    @RT00310                                            0310 00734000
*     GO TO MCIHRTN;                /* INVALID HOOK - BYPASS RECORD     00735000
*                                      BUILD                         */ 00736000
*                                                                  0312 00737000
*   /*****************************************************************/ 00738000
*   /*                                                               */ 00739000
*   /* BUILD THE COMMON RECORD SECTIONS                              */ 00740000
*   /*                                                               */ 00741000
*   /*****************************************************************/ 00742000
*                                                                  0312 00743000
*   RFY                                                            0312 00744000
*     ASCBPTR RSTD;                                                0312 00745000
*   ASCBPTR=PSAAOLD;                /* GET POINTER TO ASCB           */ 00746000
         L     ASCBPTR,PSAAOLD                                     0313 00747000
*   IF ASCBPTR=GTFASCB THEN         /* CHECK FOR GTF ASCB            */ 00748000
         C     ASCBPTR,GTFASCB(,PCTPTR)                            0314 00749000
         BNE   @RF00314                                            0314 00750000
*     IF OPTTRC=OFF THEN            /* ARE WE TRACING GTF ?          */ 00751000
         TM    OPTTRC(PCTPTR),B'00001000'                          0315 00752000
         BZ    @RT00315                                            0315 00753000
*       GO TO MCIHRTN;              /* NO - BYPASS RECORD BUILD      */ 00754000
*                                                                  0316 00755000
*   /*****************************************************************/ 00756000
*   /*                                                               */ 00757000
*   /* FIRST GATHER UNRECOVERABLE FIELDS                             */ 00758000
*   /*                                                               */ 00759000
*   /*****************************************************************/ 00760000
*                                                                  0317 00761000
*   DSPASCB=ASCBPTR;                /* ASCB ADDRESS                  */ 00762000
@RF00314 ST    ASCBPTR,DSPASCB(,AWSAPTR)                           0317 00763000
*   DSPCPUID=PSACPUSA;              /* CPU ID                @YM03489*/ 00764000
         LH    @06,PSACPUSA                                        0318 00765000
         STH   @06,DSPCPUID(,AWSAPTR)                              0318 00766000
*   DSPPSW=PSAPSWSV;                /* PSW                           */ 00767000
*                                                                  0319 00768000
         MVC   DSPPSW(8,AWSAPTR),PSAPSWSV                          0319 00769000
*   /*****************************************************************/ 00770000
*   /*                                                               */ 00771000
*   /* GATHER THE JOBNAME - RECOVERABLE FIELD                        */ 00772000
*   /*                                                               */ 00773000
*   /*****************************************************************/ 00774000
*                                                                  0320 00775000
*   MCATRL6=FRDCMJ;                 /* INDICATE THAT THE JOBNAME IS     00776000
*                                      BEING GATHERED                */ 00777000
         MVI   MCATRL6(AWSAPTR),X'10'                              0320 00778000
*   RFY                                                            0321 00779000
*     R8WORK RSTD;                                                 0321 00780000
*   R8WORK=ASCBJBNI;                /* GET 1ST JOBNAME ADDRESS       */ 00781000
         L     R8WORK,ASCBJBNI(,ASCBPTR)                           0322 00782000
*   IF R8WORK=0 THEN                /* CHECK FOR ADDRESS OF 0        */ 00783000
         SLR   @09,@09                                             0323 00784000
         CR    R8WORK,@09                                          0323 00785000
         BNE   @RF00323                                            0323 00786000
*     DO;                                                          0324 00787000
*       R8WORK=ASCBJBNS;            /* IF SO GET SECOND JOB NAME @   */ 00788000
         L     R8WORK,ASCBJBNS(,ASCBPTR)                           0325 00789000
*       RFY                                                        0326 00790000
*         ASCBPTR UNRSTD;                                          0326 00791000
*       IF R8WORK=0 THEN            /* CHECK FOR ADDRESS OF 0        */ 00792000
         CR    R8WORK,@09                                          0327 00793000
         BE    @RT00327                                            0327 00794000
*         GO TO NOJBNM2;            /* IF SO JOBNAME IS N/A          */ 00795000
*     END;                                                         0329 00796000
*   IF JBNMTEST^=0 THEN             /* CHECK FOR VALID JOBNAME       */ 00797000
@RF00323 L     @09,JBNMTEST(,R8WORK)                               0330 00798000
         LTR   @09,@09                                             0330 00799000
         BZ    @RF00330                                            0330 00800000
*     DSPJBNM=JOBNAME;              /* IF SO MOVE IT TO RECORD       */ 00801000
         MVC   DSPJBNM(8,AWSAPTR),JOBNAME(R8WORK)                  0331 00802000
*   ELSE                                                           0332 00803000
*NOJBNM2:                                                          0332 00804000
*     DSPJBNM=NOTAPP;               /* OTHERWISE JOBNAME = 'N/A'     */ 00805000
         B     @RC00330                                            0332 00806000
@RF00330 DS    0H                                                  0332 00807000
NOJBNM2  MVC   DSPJBNM(8,AWSAPTR),@CC02967                         0332 00808000
*   RFY                                                            0333 00809000
*     R14 RSTD;                                                    0333 00810000
@RC00330 DS    0H                                                  0334 00811000
*   R14=SVR14CAL;                   /* RESTORE REG 14 TO CALLER'S @  */ 00812000
         L     R14,SVR14CAL(,R7PARM)                               0334 00813000
*   GEN REFS(R14) EXIT(BR   R14   RETURN TO CALLER);               0335 00814000
         BR   R14   RETURN TO CALLER                                    00815000
*   RFY                                                            0336 00816000
*    (R14,                                                         0336 00817000
*     R8WORK) UNRSTD;                                              0336 00818000
*   END AHLTDCM;                                                   0337 00819000
@EL00002 DS    0H                                                  0337 00820000
@EF00002 DS    0H                                                  0337 00821000
@ER00002 BR    @14                                                 0337 00822000
*AHLDOUT:                                                          0338 00823000
*   DO;                                                            0338 00824000
*                                                                  0338 00825000
AHLDOUT  DS    0H                                                  0339 00826000
*     /***************************************************************/ 00827000
*     /*                                                             */ 00828000
*     /* THIS PROCEDURE STACKS THE RECORD, DELETES THE ERROR RECOVERY*/ 00829000
*     /* ROUTINE FROM THE LIST AND RETURNS DIRECTLY TO MCIH          */ 00830000
*     /*                                                             */ 00831000
*     /***************************************************************/ 00832000
*                                                                  0339 00833000
*     RFY                                                          0339 00834000
*       R8WORK RSTD;                                               0339 00835000
*     DO;                                                          0340 00836000
*       SVREG0=R0;                  /* SAVE MCQE BASE                */ 00837000
         ST    R0,SVREG0(,AWSAPTR)                                 0341 00838000
*       DO;                         /* AHLSTACK DLENGTH((RECLENG))AID   00839000
*                                      (AIDC)FID(FIDC)               */ 00840000
*                                                                  0342 00841000
*         /***********************************************************/ 00842000
*         /*                                                         */ 00843000
*         /* THE FOLLOWING CODE IS GENERATED BY THE AHLSTACK MACRO.  */ 00844000
*         /* THIS CODE BUILDS THE GTF RECORD PREFIX AND THEN CALLS   */ 00845000
*         /* AHLSBLOK TO STACK THE RECORD.                           */ 00846000
*         /*                                                         */ 00847000
*         /***********************************************************/ 00848000
*                                                                  0343 00849000
*         DCL                                                      0343 00850000
*           1 AHLGREC INT BASED(AHLRECPR) BDY(WORD),/* MAPPING FOR 0343 00851000
*                                      RECORD PREFIX                 */ 00852000
*            2 AHLGLGTH PTR(16),    /* LENGTH OF RECORD PLUS PREFIX  */ 00853000
*            2 AHLGRES PTR(16),     /* FOR COMPATIBILITY TO QSAM     */ 00854000
*            2 AHLGAID PTR(8),      /* APPLICATION ID                */ 00855000
*            2 AHLGFID PTR(8),      /* FORMAT APPENDAGE ID           */ 00856000
*            2 AHLGTST CHAR(8),     /* TIMESTAMP IF SPECIFIED        */ 00857000
*             3 AHLGEIDS PTR(16) BDY(BYTE),/* RECORD EVENT IDENTIFIER   00858000
*                                      IF NO TIMESTAMP               */ 00859000
*            2 AHLGEIDL PTR(16);    /* RECORD EVENT IDENTIFIER WITH     00860000
*                                      TIMESTAMP                     */ 00861000
*         DCL                                                      0344 00862000
*           AHLRECPR PTR(31) REG(1),/* USED AS POINTER TO RECORD   0344 00863000
*                                      PREFIX AREA                   */ 00864000
*           AHLSBREG PTR(31) REG(14),/* USED TO HOLD A STABLE COPY OF   00865000
*                                      THE SBLOKRTN FIELD IN THE   0344 00866000
*                                      GTFPCT                        */ 00867000
*           AHLRETCD PTR(31) REG(15);/* USED TO PASS RETURN CODE TO     00868000
*                                      INVOKER                       */ 00869000
*         DCL                                                      0345 00870000
*           AHLSBLOK ENTRY OPTIONS(NOEXIT,SEQFLOW,REFS(MCAWSA),SETS(    00871000
*               AHLRETCD),NOFLOWS) BASED(AHLSBREG) VALRG(0);/* ENTRY    00872000
*                                      FOR AHLSBLOK                  */ 00873000
*         RFY                                                      0346 00874000
*          (AHLRECPR,                                              0346 00875000
*           AHLSBREG,                                              0346 00876000
*           AHLRETCD) RSTD;                                        0346 00877000
*         IF OPTTIME='1'B THEN      /* CHECK FOR TIMESTAMP REQUESTED */ 00878000
         TM    OPTTIME(PCTPTR),B'00000001'                         0347 00879000
         BNO   @RF00347                                            0347 00880000
*           DO;                     /* YES, TIMESTAMP                */ 00881000
*             AHLRECPR=ADDR(MCARECPR);/* SETUP ADDRESSIBILITY TO   0349 00882000
*                                      PREFIX FOR PREFIX WITH      0349 00883000
*                                      TIMESTAMP                     */ 00884000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0349 00885000
*             AHLGLGTH=(RECLENG)+16;/* SET LENGTH TO INCLUDE ***   0350 00886000
*                                      DLENGTH *** PLUS TIMESTAMP  0350 00887000
*                                      PREFIX                        */ 00888000
         LA    @09,16                                              0350 00889000
         ALR   @09,RECLENG                                         0350 00890000
         STH   @09,AHLGLGTH(,AHLRECPR)                             0350 00891000
*             GEN                                                  0351 00892000
*(STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX )         0351 00893000
*             SETS(AHLGTST) NOEXIT SEQFLOW NOFLOWS;                0351 00894000
         STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX         00895000
*             AHLGEIDL=MCAMCR->MCREID;/* *** EID *** NOT SPECIFIED 0352 00896000
*                                      PUT DEFAULT INTO PREFIX       */ 00897000
         L     @09,MCAMCR(,AWSAPTR)                                0352 00898000
         LH    @09,MCREID(,@09)                                    0352 00899000
         N     @09,@CF03169                                        0352 00900000
         STH   @09,AHLGEIDL(,AHLRECPR)                             0352 00901000
*           END;                                                   0353 00902000
*         ELSE                      /* NO TIMESTAMP TO BE DONE, BUILD   00903000
*                                      SHORT PREFIX                  */ 00904000
*           DO;                                                    0354 00905000
         B     @RC00347                                            0354 00906000
@RF00347 DS    0H                                                  0355 00907000
*             AHLRECPR=ADDR(MCARECPR)+8;/* SETUP ADDRESSIBILITY TO 0355 00908000
*                                      FOR SHORT PREFIX WITHOUT    0355 00909000
*                                      TIMESTAMP                     */ 00910000
         LA    @09,8                                               0355 00911000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0355 00912000
         ALR   AHLRECPR,@09                                        0355 00913000
*             AHLGLGTH=(RECLENG)+8; /* PUT TOTAL RECORD LENGTH INTO     00914000
*                                      PREFIX *** DLENGTH *** PLUS 0356 00915000
*                                      PREFIX LENGTH                 */ 00916000
         ALR   @09,RECLENG                                         0356 00917000
         STH   @09,AHLGLGTH(,AHLRECPR)                             0356 00918000
*             AHLGEIDS=MCAMCR->MCREID;/* *** EID *** NOT SPECIFIED 0357 00919000
