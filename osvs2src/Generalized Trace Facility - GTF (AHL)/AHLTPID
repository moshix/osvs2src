         L     @15,PCTDMPMD(,PCTPTR)                               0431 01182000
         BALR  @14,@15                                             0431 01183000
*             DUMPIND=R15CODE;      /* SAVE INDICATOR FOR WTO ROUTINE*/ 01184000
         STH   R15CODE,DUMPIND(,AWSAPTR)                           0432 01185000
*             WTOPARM(1:LENGTH(WTOSKELT))=WTOSKELT;/* MOVE NAME AND     01186000
*                                      FUNCTION TO WTO LIST          */ 01187000
         MVC   WTOPARM(14,AWSAPTR),WTOSKELT                        0433 01188000
