         TITLE 'AHLTPID FGBR FOR VS2/R2                                *00001000
                        '                                               00002000
AHLTPID  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AHLTPID   76.215'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
RBPRFXLN EQU 64                                                         00011000
RBPRFLNA EQU 32                                                         00012000
PRBLEN   EQU 136                                                        00013000
SIRBLEN  EQU 200                                                        00014000
TIRBLEN  EQU 136                                                        00015000
IRBLEN   EQU 128                                                        00016000
SVRBLEN  EQU 200                                                        00017000
AHLTPI   DS    0H                                                  0096 00018000
          USING AHLTPI,R15ENTRY     SETUP TEMP BASE                     00019000
          L     R12BASE,TPIDBASE    GET BASE ADDRESS UPON ENTRY         00020000
          USING AHLTPID,R12BASE     TELL ASSEMBLER ABOUT OUR BASE       00021000
          DROP  R15ENTRY            TELL ASSEMBLER TO QUIT USING R15    00022000
*   DO;                                                            0097 00023000
*                                                                  0097 00024000
*     /***************************************************************/ 00025000
*     /*                                                             */ 00026000
*     /* THIS CODE GATHERS THE PERTINENT DATA ON A HOOK FOR PROGRAM  */ 00027000
*     /* INTERRUPTS 1-17,19 (EID IEAPINT) AND FOR PI 18 (EID IEATINT)*/ 00028000
*     /*                                                             */ 00029000
*     /***************************************************************/ 00030000
*                                                                  0098 00031000
*     ;                                                            0098 00032000
*                                                                  0098 00033000
*     /***************************************************************/ 00034000
*     /*                                                             */ 00035000
*     /* HOUSEKEEPING - SET THE PI TRAIL BIT ON, DEFINE THE ERROR    */ 00036000
*     /* ENVIRONMENT VIA SETFRR AND SAVE REG14 IN THE PARM AREA      */ 00037000
*     /*                                                             */ 00038000
*     /***************************************************************/ 00039000
*                                                                  0099 00040000
*     RFY                                                          0099 00041000
*      (R14,                                                       0099 00042000
*       FRRADDR,                                                   0099 00043000
*       R8WORK) RSTD;                                              0099 00044000
*     MCATRAIL=PITSET;              /* ZERO OUT TRAIL AND SET PI IN     00045000
*                                      CONTROL                       */ 00046000
         MVC   MCATRAIL(8,AWSAPTR),@CB02993                        0100 00047000
*     SVREG0=R0;                    /* SAVE MCQE BASE                */ 00048000
         ST    R0,SVREG0(,AWSAPTR)                                 0101 00049000
*     FRRADDR=ADDR(AHLTPIDE);       /* GET ADDRESS OF THE RECOVERY 0102 00050000
*                                      ROUTINE                       */ 00051000
         LA    FRRADDR,AHLTPIDE                                    0102 00052000
*     GEN SETS(R7PARM,R8WORK) REFS(FRRADDR,PSACSTK,FRRSCURR,FRRSLAST,   00053000
*         FRRSELEN,FRRSFRRA,FRRSPARM) NOEXIT NOFLOWS SEQFLOW;      0103 00054000
         SETFRR A,FRRAD=(FRRADDR),PARMAD=(R7PARM),                     *00055000
               WRKREGS=(R7PARM,R8WORK)                                  00056000
*     R0=SVREG0;                    /* RESTORE MCQE BASE             */ 00057000
         L     R0,SVREG0(,AWSAPTR)                                 0104 00058000
*     SVR14NRM=R14;                 /* SAVE RETURN ADDRESS           */ 00059000
         ST    R14,SVR14NRM(,R7PARM)                               0105 00060000
*     RFY                                                          0106 00061000
*      (R14,                                                       0106 00062000
*       FRRADDR,                                                   0106 00063000
*       R8WORK) UNRSTD;                                            0106 00064000
*                                                                  0107 00065000
*     /***************************************************************/ 00066000
*     /*                                                             */ 00067000
*     /* BUILD THE PI RECORD                                         */ 00068000
*     /*                                                             */ 00069000
*     /***************************************************************/ 00070000
*                                                                  0107 00071000
*     RFY                                                          0107 00072000
*      (ASCBPTR,                                                   0107 00073000
*       R8WORK) RSTD;                                              0107 00074000
*                                                                  0107 00075000
*     /***************************************************************/ 00076000
*     /*                                                             */ 00077000
*     /* ENSURE VALID ENTRY                                          */ 00078000
*     /*                                                             */ 00079000
*     /***************************************************************/ 00080000
*                                                                  0108 00081000
*     IF FLCPICOD=K40X THEN         /* IGNORE ALL BUT BRANCH ENTRY   */ 00082000
         CLC   FLCPICOD(2),@CH02932                                0108 00083000
         BE    @RT00108                                            0108 00084000
*       GO TO PIDEXIT;                                             0109 00085000
*     ASCBPTR=PSAAOLD;              /* GET POINTER TO ASCB           */ 00086000
         L     ASCBPTR,PSAAOLD                                     0110 00087000
*     IF ASCBPTR=GTFASCB THEN       /* CHECK FOR GTF ASCB            */ 00088000
         C     ASCBPTR,GTFASCB(,PCTPTR)                            0111 00089000
         BNE   @RF00111                                            0111 00090000
*       IF OPTTRC=OFF THEN          /* ARE WE TRACING GTF ?          */ 00091000
         TM    OPTTRC(PCTPTR),B'00001000'                          0112 00092000
         BZ    @RT00112                                            0112 00093000
*         GOTO PIDEXIT;             /* IF NOT EXIT                   */ 00094000
*                                                                  0113 00095000
*     /***************************************************************/ 00096000
*     /*                                                             */ 00097000
*     /* GATHER UNRECOVERABLE FIELDS FIRST                           */ 00098000
*     /*                                                             */ 00099000
*     /***************************************************************/ 00100000
*                                                                  0114 00101000
*     PIASCB=ASCBPTR;               /* ASCB ADDRESS                  */ 00102000
@RF00111 ST    ASCBPTR,PIASCB(,AWSAPTR)                            0114 00103000
*     PICPUID=PSACPUSA;             /* CPU ID                @YM03489*/ 00104000
         LH    @09,PSACPUSA                                        0115 00105000
         STH   @09,PICPUID(,AWSAPTR)                               0115 00106000
*     LCCAPTR=PSALCCAV;             /* GET ADDRESS OF LCCA           */ 00107000
         L     LCCAPTR,PSALCCAV                                    0116 00108000
*     IF MCREID=IEAPINT THEN        /* CHECK EID TYPE                */ 00109000
         L     @09,MCAMCR(,AWSAPTR)                                0117 00110000
         CLC   MCREID(2,@09),@CH01378                              0117 00111000
         BNE   @RF00117                                            0117 00112000
*       DO;                                                        0118 00113000
*         PIREGS=REGSLCCA;          /* REGS 0-15 AT INTERRUPT        */ 00114000
         MVC   PIREGS(64,AWSAPTR),REGSLCCA(LCCAPTR)                0119 00115000
*         PIVPA=LCCAPVAD;           /* GET VIRTUAL PAGE ADDRESS      */ 00116000
         MVC   PIVPA(4,AWSAPTR),LCCAPVAD(LCCAPTR)                  0120 00117000
*         PIPSW=LCCAPPSW;           /* GET PSW                       */ 00118000
         MVC   PIPSW(8,AWSAPTR),LCCAPPSW(LCCAPTR)                  0121 00119000
*         PIINT=INTCODE;            /* OVERLAY PSW WITH INTERRUPT  0122 00120000
*                                      CODE                          */ 00121000
         MVC   PIINT(2,AWSAPTR),INTCODE(LCCAPTR)                   0122 00122000
*       END;                                                       0123 00123000
*     ELSE                                                         0124 00124000
*       DO;                                                        0124 00125000
         B     @RC00117                                            0124 00126000
@RF00117 DS    0H                                                  0125 00127000
*         PIREGS=TINTREGS;          /* REGS 0-15 AT INTERRUPT        */ 00128000
         MVC   PIREGS(64,AWSAPTR),TINTREGS(LCCAPTR)                0125 00129000
*         PIVPA=FLCTEA;             /* GET VIRTUAL PAGE ADDRESS      */ 00130000
         MVC   PIVPA(4,AWSAPTR),FLCTEA                             0126 00131000
*         PIPSW=FLCPOPSW;           /* GET PSW                       */ 00132000
         MVC   PIPSW(8,AWSAPTR),FLCPOPSW                           0127 00133000
*         PIINT=FLCPICOD;           /* OVERLAY PSW WITH INTERRUPT  0128 00134000
*                                      CODE                          */ 00135000
         LH    @09,FLCPICOD                                        0128 00136000
         STH   @09,PIINT(,AWSAPTR)                                 0128 00137000
*       END;                                                       0129 00138000
*     IF LCCASRBM=ON THEN           /* CHECK FOR SRB MODE            */ 00139000
@RC00117 TM    LCCASRBM(LCCAPTR),B'10000000'                       0130 00140000
         BNO   @RF00130                                            0130 00141000
*       DO;                                                        0131 00142000
*         PIINTTCB=NOTAPP;          /* SET INT TCB @ = 'N/A'         */ 00143000
         MVC   PIINTTCB(4,AWSAPTR),@CC02967                        0132 00144000
*         PICDENAM=NOTAPP;          /* SET CDE NAME = 'N/A'          */ 00145000
         MVC   PICDENAM(8,AWSAPTR),@CC02967                        0133 00146000
*       END;                                                       0134 00147000
*     ELSE                                                         0135 00148000
*       DO;                                                        0135 00149000
         B     @RC00130                                            0135 00150000
@RF00130 DS    0H                                                  0136 00151000
*         TCBPTR=PSATOLD;           /* GET TCB POINTER               */ 00152000
         L     TCBPTR,PSATOLD                                      0136 00153000
*         PIINTTCB=TCBPTR;          /* GET INT TCB ADDRESS           */ 00154000
         STCM  TCBPTR,15,PIINTTCB(AWSAPTR)                         0137 00155000
*         MCATRL6=FRPIC;            /* INDICATE RETURN POINT FOR   0138 00156000
*                                      ERRORS GATHERING THE CDE NAME */ 00157000
*                                                                  0138 00158000
         MVI   MCATRL6(AWSAPTR),X'40'                              0138 00159000
*         /***********************************************************/ 00160000
*         /*                                                         */ 00161000
*         /* THIS SECTION OF CODE DETERMINES THE APPROPRIATE CONTENTS*/ 00162000
*         /* OF THE CDENAME FIELD                                    */ 00163000
*         /*                                                         */ 00164000
*         /***********************************************************/ 00165000
*                                                                  0139 00166000
*         IF TCBPTR=TCBRBP THEN     /* CHECK FOR RB ADDR = TCB ADDR  */ 00167000
         C     TCBPTR,TCBRBP(,TCBPTR)                              0139 00168000
         BNE   @RF00139                                            0139 00169000
*           DO;                                                    0140 00170000
*             PICDENAM=WAITTCB;     /* CDENAME = 'WAITTCB'           */ 00171000
         MVC   PICDENAM(8,AWSAPTR),@CC02969                        0141 00172000
*             GO TO ENDCD;                                         0142 00173000
         B     ENDCD                                               0142 00174000
*           END;                                                   0143 00175000
*         RBPTR=TCBRBP;             /* GET RB PTR                    */ 00176000
@RF00139 L     RBPTR,TCBRBP(,TCBPTR)                               0144 00177000
*         GO TO MODARYA(RBSTAB1/16);/* BRANCH TO COMPLETE CDENAME  0145 00178000
*                                      DEPENDING ON THE VALUE OF   0145 00179000
*                                      RBSTAB1                       */ 00180000
         SLR   @09,@09                                             0145 00181000
         IC    @09,RBSTAB1(,RBPTR)                                 0145 00182000
         SRL   @09,4                                               0145 00183000
         SLA   @09,2                                               0145 00184000
         LA    @10,MODARYA0                                        0145 00185000
         L     @09,MODARYA(@09,@10)                                0145 00186000
         BR    @09                                                 0145 00187000
*CDE00A:                            /* RBSTAB1 = '0-'                */ 00188000
*         CDEPTR=RBCDE1;            /* GET CDE POINTER       @YM03657*/ 00189000
CDE00A   L     @09,RBCDE1-1(,RBPTR)                                0146 00190000
         LA    @09,0(,@09)                                         0146 00191000
         LR    CDEPTR,@09                                          0146 00192000
*         IF CDEPTR^=0 THEN         /*                       @YM03657*/ 00193000
         LTR   CDEPTR,CDEPTR                                       0147 00194000
