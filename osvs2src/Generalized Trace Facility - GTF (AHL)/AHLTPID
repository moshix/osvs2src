*     R14=SVR14NRM;                 /* LOAD REG 14 WITH THE RETURN 0371 00962000
*                                      ADDRESS IN MCIH               */ 00963000
         L     R14,SVR14NRM(,R7PARM)                               0371 00964000
*     GEN SETS(R7PARM,R8WORK) REFS(PSACSTK,FRRSCURR,FRRSEMP,FRRSELEN)   00965000
*         NOEXIT NOFLOWS SEQFLOW;                                  0372 00966000
