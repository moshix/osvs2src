*AHLTDSP:                                                          0194 00326000
*   GEN SETS(R12BASE) REFS(TPIDBASE) NOEXIT NOFLOWS SEQFLOW NODEFS;     00327000
AHLTDSP  DS    0H                                                  0194 00328000
          USING AHLTDSP,R15ENTRY    SETUP TEMP BASE                     00329000
          L     R12BASE,TPIDBASE    GET BASE ADDRESS UPON ENTRY         00330000
          USING AHLTPID,R12BASE     TELL ASSEMBLER ABOUT OUR BASE       00331000
          DROP  R15ENTRY            TELL ASSEMBLER TO QUIT USING R15    00332000
*   DO;                                                            0195 00333000
*                                                                  0195 00334000
*     /***************************************************************/ 00335000
*     /*                                                             */ 00336000
*     /* THIS CODE GATHERS THE PERTINENT DATA ON A HOOK FOR DSP      */ 00337000
*     /* EVENTS                                                      */ 00338000
*     /*                                                             */ 00339000
*     /***************************************************************/ 00340000
*                                                                  0196 00341000
*     ;                                                            0196 00342000
*                                                                  0196 00343000
*     /***************************************************************/ 00344000
*     /*                                                             */ 00345000
*     /* HOUSEKEEPING - SET THE DSP TRAIL BIT ON, SAVE REG14 (RETURN */ 00346000
*     /* ADDRESS TO MCIH) IN ANOTHER REG. HOUSEKEEPING WILL BE       */ 00347000
*     /* COMPLETED BY AHLTDCM                                        */ 00348000
*     /*                                                             */ 00349000
*     /***************************************************************/ 00350000
*                                                                  0197 00351000
*     RFY                                                          0197 00352000
*      (R14,                                                       0197 00353000
*       R10SAVE) RSTD;                                             0197 00354000
*     R10SAVE=R14;                  /* SAVE REG 14                   */ 00355000
         LR    R10SAVE,R14                                         0198 00356000
*     RFY                                                          0199 00357000
*       R14 UNRSTD;                                                0199 00358000
*     MCATRAIL=DSPTSET;             /* ZERO OUT MCATRAIL AND INDICATE   00359000
*                                      DSP IN CONTROL                */ 00360000
         MVC   MCATRAIL(8,AWSAPTR),@CB02987                        0200 00361000
*     ;                                                            0201 00362000
*                                                                  0201 00363000
*     /***************************************************************/ 00364000
*     /*                                                             */ 00365000
*     /* BUILD THE DSP RECORD. COMMON RECORD PROCESSING IS PERFORMED */ 00366000
*     /* BY AHLTDCM                                                  */ 00367000
*     /*                                                             */ 00368000
*     /***************************************************************/ 00369000
*                                                                  0202 00370000
*     CALL AHLTDCM;                 /* FILL IN ASCB@,CPUID,JOBNAME 0202 00371000
*                                      AND PSW                       */ 00372000
         BAL   @14,AHLTDCM                                         0202 00373000
*     RFY                                                          0203 00374000
*       R10SAVE UNRSTD;                                            0203 00375000
*AHLDSPC:                                                          0204 00376000
*     MCATRL6=UNRECOV;              /* CLEAR AUDIT TRAIL             */ 00377000
AHLDSPC  MVI   MCATRL6(AWSAPTR),X'00'                              0204 00378000
*     RFY                                                          0205 00379000
*       R8WORK RSTD;                                               0205 00380000
*     TCBPTR=PSATOLD;               /* GET TCB POINTER               */ 00381000
         L     TCBPTR,PSATOLD                                      0206 00382000
*     DSPTCB=TCBPTR;                /* GET TCB ADDRESS               */ 00383000
         STCM  TCBPTR,15,DSPTCB(AWSAPTR)                           0207 00384000
*     MCATRL6=FRDSPC;               /* INDICATE RETURN POINT FOR   0208 00385000
*                                      ERRORS GATHERING CDE NAME     */ 00386000
         MVI   MCATRL6(AWSAPTR),X'20'                              0208 00387000
*     DO;                                                          0209 00388000
*                                                                  0209 00389000
*       /*************************************************************/ 00390000
*       /*                                                           */ 00391000
*       /* THIS SECTION OF CODE DETERMINES THE APPROPRIATE CONTENTS  */ 00392000
*       /* OF THE CDENAME FIELD                                      */ 00393000
*       /*                                                           */ 00394000
*       /*************************************************************/ 00395000
*                                                                  0210 00396000
*       IF TCBPTR=TCBRBP THEN       /* CHECK FOR RB ADDR = TCB ADDR  */ 00397000
         C     TCBPTR,TCBRBP(,TCBPTR)                              0210 00398000
         BNE   @RF00210                                            0210 00399000
*         DO;                                                      0211 00400000
*           DSPCDNAM=WAITTCB;       /* CDENAME = 'WAITTCB'           */ 00401000
         MVC   DSPCDNAM(8,AWSAPTR),@CC02969                        0212 00402000
