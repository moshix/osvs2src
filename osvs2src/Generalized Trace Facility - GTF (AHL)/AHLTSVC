*                                      FOR PREFIX WITH TIMESTAMP     */ 04220000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         1633 04221000
*         AHLGLGTH=DEFLGTH+16;      /* SET LENGTH TO INCLUDE ***   1634 04222000
