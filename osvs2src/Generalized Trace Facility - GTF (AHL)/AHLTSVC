*CATERR:                                                           1623 04174000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1623 04175000
CATERR   DS    0H                                                  1624 04176000
*   RFY                                                            1624 04177000
*     R14 RSTD;                                                    1624 04178000
*                                                                  1624 04179000
*   /*****************************************************************/ 04180000
*   /*                                                               */ 04181000
*   /* IF AN UNRECOVERABLE ERROR IS ENCOUNTERED AND DEBUG = NO THIS  */ 04182000
*   /* ROUTINE WILL BE CALLED TO STACK THE ERROR RECORD, TURN OFF THE*/ 04183000
*   /* ACTICE FUNCTION, ISSUE THE SETEVENT MACRO TO DISABLE TRACING  */ 04184000
*   /* ON SVC, CALL THE DUMPING ROUTINE AND THEN ISSUE THE CODE THAT */ 04185000
*   /* WILL RETURN TO MCIH WITHOUT TERMINATING ALL OF GTF.           */ 04186000
*   /*                                                               */ 04187000
*   /*****************************************************************/ 04188000
*                                                                  1625 04189000
*   RETSAV1=R14;                    /* SAVE RETURN ADDRESS           */ 04190000
         ST    R14,RETSAV1(,AWSAPTR)                               1625 04191000
*   RFY                                                            1626 04192000
*     R14 UNRSTD;                                                  1626 04193000
*   ERRASCB=CATERIND;               /* INDICATE CATAS ERROR IN FIRST    04194000
*                                      OF RECORD                     */ 04195000
         MVC   ERRASCB(2,AWSAPTR),@CB04604                         1627 04196000
*   SDWASAV=SDWAPTR;                /* SAVE PTR TO RTCA SINCE STACK     04197000
*                                      AND SETEVENT DESTROY IT       */ 04198000
         ST    SDWAPTR,SDWASAV(,AWSAPTR)                           1628 04199000
*   DO;                             /* AHLSTACK DLENGTH(DEFLGTH)AID(A   04200000
*                                      IDC)FID(FIDC)                 */ 04201000
*                                                                  1629 04202000
*     /***************************************************************/ 04203000
*     /*                                                             */ 04204000
*     /* THE FOLLOWING CODE IS GENERATED BY THE AHLSTACK MACRO. THIS */ 04205000
*     /* CODE BUILDS THE GTF RECORD PREFIX AND THEN CALLS AHLSBLOK TO*/ 04206000
*     /* STACK THE RECORD.                                           */ 04207000
*     /*                                                             */ 04208000
*     /***************************************************************/ 04209000
*                                                                  1630 04210000
*     RFY                                                          1630 04211000
*      (AHLRECPR,                                                  1630 04212000
*       AHLSBREG,                                                  1630 04213000
*       AHLRETCD) RSTD;                                            1630 04214000
*     IF OPTTIME='1'B THEN          /* CHECK FOR TIMESTAMP REQUESTED */ 04215000
