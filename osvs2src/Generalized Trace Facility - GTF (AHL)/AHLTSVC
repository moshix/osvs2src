         AL    AHLRECPR,@CF00061                                   1639 04242000
*         AHLGLGTH=DEFLGTH+8;       /* PUT TOTAL RECORD LENGTH INTO     04243000
*                                      PREFIX *** DLENGTH *** PLUS 1640 04244000
*                                      PREFIX LENGTH                 */ 04245000
         MVC   AHLGLGTH(2,AHLRECPR),@CH05084                       1640 04246000
*         AHLGEIDS=MCAMCR->MCREID;  /* *** EID *** NOT SPECIFIED PUT    04247000
*                                      DEFAULT INTO PREFIX           */ 04248000
         L     @05,MCAMCR(,AWSAPTR)                                1641 04249000
         MVC   AHLGEIDS(2,AHLRECPR),MCREID(@05)                    1641 04250000
*       END;                                                       1642 04251000
*     AHLGRES=0;                    /* ZERO OUT FOR QSAM           1643 04252000
*                                      COMPATIBILITY                 */ 04253000
@RC01631 SLR   @05,@05                                             1643 04254000
         STH   @05,AHLGRES(,AHLRECPR)                              1643 04255000
*     AHLGAID=AIDC;                 /* PUT *** AID *** INTO PREFIX   */ 04256000
         MVI   AHLGAID(AHLRECPR),X'FF'                             1644 04257000
*     AHLGFID=FIDC;                 /* PUT *** FID *** INTO PREFIX   */ 04258000
         MVI   AHLGFID(AHLRECPR),X'01'                             1645 04259000
*     AHLRETCD=4;                   /* SET DEFAULT RETURN CODE       */ 04260000
         LA    AHLRETCD,4                                          1646 04261000
*     AHLSBREG=SBLOKRTN;            /* GET A STABLE COPY OF AHLSBLOK    04262000
*                                      ADDR                          */ 04263000
         L     AHLSBREG,SBLOKRTN(,PCTPTR)                          1647 04264000
*     IF AHLSBREG^=0 THEN           /* CHECK TO SEE IF AHLSBLOK IS 1648 04265000
*                                      AVAILABLE                     */ 04266000
