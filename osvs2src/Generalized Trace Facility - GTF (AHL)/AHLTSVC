         L     LCCAPTR,PSALCCAV                                    0397 00252000
*       GO TO SVCROUTN(WORK2);      /* BR TO THE PROPER ROUTINE TO 0398 00253000
*                                      HANDLE THE ADDL DATA          */ 00254000
         LR    @05,WORK2                                           0398 00255000
         SLA   @05,2                                               0398 00256000
         L     @05,SVCROUTN-4(@05)                                 0398 00257000
         BR    @05                                                 0398 00258000
*     END;                                                         0399 00259000
*AHLSVSTK:                                                         0400 00260000
*   DO;                             /* AHLSTACK DLENGTH(RECLGTH)AID(A   00261000
*                                      IDC)FID(FIDC)                 */ 00262000
*                                                                  0400 00263000
@RF00394 DS    0H                                                  0400 00264000
AHLSVSTK DS    0H                                                  0401 00265000
*     /***************************************************************/ 00266000
*     /*                                                             */ 00267000
*     /* THE FOLLOWING CODE IS GENERATED BY THE AHLSTACK MACRO. THIS */ 00268000
*     /* CODE BUILDS THE GTF RECORD PREFIX AND THEN CALLS AHLSBLOK TO*/ 00269000
*     /* STACK THE RECORD.                                           */ 00270000
*     /*                                                             */ 00271000
*     /***************************************************************/ 00272000
*                                                                  0401 00273000
*     DCL                                                          0401 00274000
*       1 AHLGREC INT BASED(AHLRECPR) BDY(WORD),/* MAPPING FOR RECORD   00275000
*                                      PREFIX                        */ 00276000
*        2 AHLGLGTH PTR(16),        /* LENGTH OF RECORD PLUS PREFIX  */ 00277000
*        2 AHLGRES PTR(16),         /* FOR COMPATIBILITY TO QSAM     */ 00278000
*        2 AHLGAID PTR(8),          /* APPLICATION ID                */ 00279000
*        2 AHLGFID PTR(8),          /* FORMAT APPENDAGE ID           */ 00280000
*        2 AHLGTST CHAR(8),         /* TIMESTAMP IF SPECIFIED        */ 00281000
*         3 AHLGEIDS PTR(16) BDY(BYTE),/* RECORD EVENT IDENTIFIER IF    00282000
*                                      NO TIMESTAMP                  */ 00283000
*        2 AHLGEIDL PTR(16);        /* RECORD EVENT IDENTIFIER WITH     00284000
*                                      TIMESTAMP                     */ 00285000
*     DCL                                                          0402 00286000
*       AHLRECPR PTR(31) REG(1),    /* USED AS POINTER TO RECORD   0402 00287000
*                                      PREFIX AREA                   */ 00288000
*       AHLSBREG PTR(31) REG(14),   /* USED TO HOLD A STABLE COPY OF    00289000
*                                      THE SBLOKRTN FIELD IN THE   0402 00290000
*                                      GTFPCT                        */ 00291000
*       AHLRETCD PTR(31) REG(15);   /* USED TO PASS RETURN CODE TO 0402 00292000
*                                      INVOKER                       */ 00293000
*     DCL                                                          0403 00294000
*       AHLSBLOK ENTRY OPTIONS(NOEXIT,SEQFLOW,REFS(MCAWSA),SETS(   0403 00295000
*           AHLRETCD),NOFLOWS) BASED(AHLSBREG) VALRG(0);/* ENTRY FOR    00296000
*                                      AHLSBLOK                      */ 00297000
*     RFY                                                          0404 00298000
*      (AHLRECPR,                                                  0404 00299000
*       AHLSBREG,                                                  0404 00300000
*       AHLRETCD) RSTD;                                            0404 00301000
*     IF OPTTIME='1'B THEN          /* CHECK FOR TIMESTAMP REQUESTED */ 00302000
         TM    OPTTIME(PCTPTR),B'00000001'                         0405 00303000
         BNO   @RF00405                                            0405 00304000
*       DO;                         /* YES, TIMESTAMP                */ 00305000
*         AHLRECPR=ADDR(MCARECPR);  /* SETUP ADDRESSIBILITY TO PREFIX   00306000
*                                      FOR PREFIX WITH TIMESTAMP     */ 00307000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0407 00308000
*         AHLGLGTH=RECLGTH+16;      /* SET LENGTH TO INCLUDE ***   0408 00309000
*                                      DLENGTH *** PLUS TIMESTAMP  0408 00310000
*                                      PREFIX                        */ 00311000
         LA    @05,16                                              0408 00312000
         ALR   @05,RECLGTH                                         0408 00313000
         STH   @05,AHLGLGTH(,AHLRECPR)                             0408 00314000
*         GEN(STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX )  00315000
*         SETS(AHLGTST) NOEXIT SEQFLOW NOFLOWS;                    0409 00316000
         STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX         00317000
*         AHLGEIDL=MCAMCR->MCREID;  /* *** EID *** NOT SPECIFIED PUT    00318000
*                                      DEFAULT INTO PREFIX           */ 00319000
         L     @05,MCAMCR(,AWSAPTR)                                0410 00320000
         MVC   AHLGEIDL(2,AHLRECPR),MCREID(@05)                    0410 00321000
*       END;                                                       0411 00322000
*     ELSE                          /* NO TIMESTAMP TO BE DONE, BUILD   00323000
*                                      SHORT PREFIX                  */ 00324000
*       DO;                                                        0412 00325000
         B     @RC00405                                            0412 00326000
@RF00405 DS    0H                                                  0413 00327000
*         AHLRECPR=ADDR(MCARECPR)+8;/* SETUP ADDRESSIBILITY TO FOR 0413 00328000
*                                      SHORT PREFIX WITHOUT TIMESTAMP*/ 00329000
         LA    @05,8                                               0413 00330000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0413 00331000
         ALR   AHLRECPR,@05                                        0413 00332000
*         AHLGLGTH=RECLGTH+8;       /* PUT TOTAL RECORD LENGTH INTO     00333000
*                                      PREFIX *** DLENGTH *** PLUS 0414 00334000
*                                      PREFIX LENGTH                 */ 00335000
         ALR   @05,RECLGTH                                         0414 00336000
         STH   @05,AHLGLGTH(,AHLRECPR)                             0414 00337000
*         AHLGEIDS=MCAMCR->MCREID;  /* *** EID *** NOT SPECIFIED PUT    00338000
*                                      DEFAULT INTO PREFIX           */ 00339000
         L     @05,MCAMCR(,AWSAPTR)                                0415 00340000
         MVC   AHLGEIDS(2,AHLRECPR),MCREID(@05)                    0415 00341000
*       END;                                                       0416 00342000
*     AHLGRES=0;                    /* ZERO OUT FOR QSAM           0417 00343000
*                                      COMPATIBILITY                 */ 00344000
@RC00405 SLR   @05,@05                                             0417 00345000
         STH   @05,AHLGRES(,AHLRECPR)                              0417 00346000
*     AHLGAID=AIDC;                 /* PUT *** AID *** INTO PREFIX   */ 00347000
         MVI   AHLGAID(AHLRECPR),X'FF'                             0418 00348000
*     AHLGFID=FIDC;                 /* PUT *** FID *** INTO PREFIX   */ 00349000
         MVI   AHLGFID(AHLRECPR),X'01'                             0419 00350000
*     AHLRETCD=4;                   /* SET DEFAULT RETURN CODE       */ 00351000
         LA    AHLRETCD,4                                          0420 00352000
*     AHLSBREG=SBLOKRTN;            /* GET A STABLE COPY OF AHLSBLOK    00353000
*                                      ADDR                          */ 00354000
         L     AHLSBREG,SBLOKRTN(,PCTPTR)                          0421 00355000
*     IF AHLSBREG^=0 THEN           /* CHECK TO SEE IF AHLSBLOK IS 0422 00356000
*                                      AVAILABLE                     */ 00357000
         CR    AHLSBREG,@05                                        0422 00358000
         BE    @RF00422                                            0422 00359000
*       CALL AHLSBLOK;              /* YES, SO CALL AHLSBLOK TO BLOCK   00360000
*                                      RECORD                        */ 00361000
         LR    @15,AHLSBREG                                        0423 00362000
         BALR  @14,@15                                             0423 00363000
*     RFY                                                          0424 00364000
*      (AHLRECPR,                                                  0424 00365000
*       AHLSBREG,                                                  0424 00366000
*       AHLRETCD) UNRSTD;                                          0424 00367000
@RF00422 DS    0H                                                  0425 00368000
*   END;                                                           0425 00369000
*   RFY                                                            0426 00370000
*     R14 RSTD;                                                    0426 00371000
*ENDSVC:                                                           0427 00372000
*   R14=SVR14NRM;                   /* RESTORE RETURN ADDRESS        */ 00373000
ENDSVC   L     R14,SVR14NRM(,FRRPARM)                              0427 00374000
*   MCATRL8=0;                      /* RESET SVC16 FLAG      @YM04524*/ 00375000
         MVI   MCATRL8(AWSAPTR),X'00'                              0428 00376000
*   GEN;                                                           0429 00377000
   SETFRR D,WRKREGS=(FRRPARM,WORK1)                                     00378000
*   RETURN;                                                        0430 00379000
@EL00001 DS    0H                                                  0430 00380000
@EF00001 DS    0H                                                  0430 00381000
@ER00001 BR    @14                                                 0430 00382000
*   RFY                                                            0431 00383000
*     R14 UNRSTD;                                                  0431 00384000
*                                                                  0432 00385000
*/********************************************************************/ 00386000
