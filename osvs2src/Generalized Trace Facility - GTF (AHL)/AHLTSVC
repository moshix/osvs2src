*         AHLGEIDL=MCAMCR->MCREID;  /* *** EID *** NOT SPECIFIED PUT    04229000
*                                      DEFAULT INTO PREFIX           */ 04230000
         L     @05,MCAMCR(,AWSAPTR)                                1636 04231000
         MVC   AHLGEIDL(2,AHLRECPR),MCREID(@05)                    1636 04232000
*       END;                                                       1637 04233000
*     ELSE                          /* NO TIMESTAMP TO BE DONE, BUILD   04234000
*                                      SHORT PREFIX                  */ 04235000
*       DO;                                                        1638 04236000
         B     @RC01631                                            1638 04237000
@RF01631 DS    0H                                                  1639 04238000
*         AHLRECPR=ADDR(MCARECPR)+8;/* SETUP ADDRESSIBILITY TO FOR 1639 04239000
*                                      SHORT PREFIX WITHOUT TIMESTAMP*/ 04240000
