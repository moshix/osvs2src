@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AHLTSVC   78.139'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
*                                                            /*ZM30921* 00011000
         USING   AHLTSVC,R15ENTRY    SETUP TEMP BASE REG                00012000
         B       LOADBASE            BRANCH AROUND ADCONS               00013000
NBASEAD1 DC    A(AHLTSVC)            ADDR OF ENTRY POINT                00014000
