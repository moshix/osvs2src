*       AHLRETCD) UNRSTD;                                          1650 04276000
@RF01648 DS    0H                                                  1651 04277000
*   END;                                                           1651 04278000
*   SDWAPTR=SDWASAV;                /* REST PTR SINCE DMPMOD WILL  1652 04279000
*                                      SAVE IT                       */ 04280000
         L     SDWAPTR,SDWASAV(,AWSAPTR)                           1652 04281000
*   RFY                                                            1653 04282000
*    (WORK1,                                                       1653 04283000
*     WORK2) RSTD;                                                 1653 04284000
*                                                                  1654 04285000
*   /*****************************************************************/ 04286000
*   /*                                                               */ 04287000
*   /* TURN OFF ACTIVE BIT IN THE PCTCATF FIELD                      */ 04288000
*   /*                                                               */ 04289000
*   /*****************************************************************/ 04290000
*                                                                  1654 04291000
*CKBIT:                                                            1654 04292000
*   IF PCTSVC=ON THEN               /* CHECK TO SEE IF SVC GATHER  1654 04293000
*                                      FUNCTIONS CURRENT BIT IS STILL   04294000
*                                      ON                            */ 04295000
CKBIT    TM    PCTSVC(PCTPTR),B'10000000'                          1654 04296000
         BNO   @RF01654                                            1654 04297000
*     DO;                           /* YES                           */ 04298000
*       WORK1=PCTCATF;              /* GET BITS FOR CURRENT TRACE  1656 04299000
*                                      FUNCTIONS                     */ 04300000
         L     WORK1,PCTCATF(,PCTPTR)                              1656 04301000
*       WORK2=WORK1&&SVCPCT;        /* SET OFF THE SVC BIT IN THE  1657 04302000
*                                      FIELD                         */ 04303000
         LR    WORK2,WORK1                                         1657 04304000
         X     WORK2,@CF04614                                      1657 04305000
*       CS(WORK1,WORK2,PCTCATF);    /* CHECK FOR SAME STATUS         */ 04306000
         CS    WORK1,@07,PCTCATF(PCTPTR)                           1658 04307000
*       BC(4,CKBIT);                /* IF CHANGED START ALL OVER   1659 04308000
*                                      AGAIN                         */ 04309000
         BC    4,CKBIT                                             1659 04310000
*       CALL DMPMOD;                /* CALL TO GET SDUMP             */ 04311000
         BAL   @14,DMPMOD                                          1660 04312000
*       RFY                                                        1661 04313000
*        (WORK1,                                                   1661 04314000
*         WORK2) UNRSTD;                                           1661 04315000
*                                                                  1661 04316000
*       /*************************************************************/ 04317000
*       /*                                                           */ 04318000
*       /* DISABLE TRACING ON ALL SVCS                               */ 04319000
*       /*                                                           */ 04320000
*       /*************************************************************/ 04321000
*                                                                  1662 04322000
*       PTRMCQE=MCRREG0;            /* RESTORE MCQE PTR TO INSURE  1662 04323000
*                                      THAT IT IS GOOD SINCE IT COULD   04324000
*                                      HAVE BEEN DESTROYED BY A MACRO*/ 04325000
         L     @05,MCAMCR(,AWSAPTR)                                1662 04326000
         L     PTRMCQE,MCRREG0(,@05)                               1662 04327000
*       DO;                         /* SETEVENT NAME('GTF')MCQE(MCQE)   04328000
*                                      ACTION(DISABLE)EIDAD(MCREID)  */ 04329000
*                                                                  1663 04330000
*         /***********************************************************/ 04331000
*         /*                                                         */ 04332000
*         /* THE FOLLOWING SECTION OF CODE IS GENERATED BY THE       */ 04333000
*         /* SETEVENT MACRO. THIS CODE WILL BE EITHER AN INITIALIZED */ 04334000
*         /* PARAMETER LIST, OR IF IT IS THE RESULT OF STANDARD OR   */ 04335000
*         /* EXECUTE FORM IT WILL FILL in the list, GENERATE ALL     */ 04336000
*         /* LINKAGES AND CALL THE PROPER sERVICE, EITHER AHLSETEV OR*/ 04337000
*         /* AHLSETD                                                 */ 04338000
*         /*                                                         */ 04339000
*         /***********************************************************/ 04340000
*                                                                  1664 04341000
*         DCL                                                      1664 04342000
*           AHLPPTR REG(1) PTR(31) RSTD;/* PARAMETER LIST ADDRESS    */ 04343000
*         DCL                                                      1665 04344000
*           AHLSETEV ENTRY OPTIONS(EXIT,SEQFLOW) BASED(MCHSETE) VALRG(0 04345000
*               );                  /* ENTRY FOR ADD ACTIVAT,FREE    */ 04346000
*         DCL                                                      1666 04347000
*           AHLSETD ENTRY OPTIONS(EXIT,SEQFLOW) BASED(MCHSETD) VALRG(0) 04348000
*             ;                     /* ENTRY FOR DISABLE AND CHANGE  */ 04349000
*         DCL                                                      1667 04350000
*           1 SEPL BASED(AHLPPTR) BDY(WORD),/* DSECT PARAMETER LIST  */ 04351000
*            2 SEFG PTR(8),         /* ACTION GLAGS                  */ 04352000
*            2 * PTR(8),                                           1667 04353000
*            2 SEEN PTR(8),         /* EID COUNT                     */ 04354000
*            2 SECN PTR(8),         /* CLASS COUNT                   */ 04355000
*            2 SENM CHAR(8),        /* APPLICATION NMAE              */ 04356000
*            2 SEEL PTR(31),        /* ADDRESS OF LIST OF EIDS       */ 04357000
*            2 SECL PTR(31),        /* ADDRESS OF LIST OF CLASSES    */ 04358000
*            2 SEMC PTR(31);        /* MCQE ADDRESS                  */ 04359000
*         DCL                                                      1668 04360000
*           1 SEPL0080 STATIC LOCAL BDY(WORD),/* ACTUAL PLIST FOR  1668 04361000
*                                      STANDARD INVOCATION           */ 04362000
*            2 SEFG0080 PTR(8),                                    1668 04363000
*            2 * PTR(8),                                           1668 04364000
*            2 SEEN0080 PTR(8),                                    1668 04365000
*            2 SECN0080 PTR(8),                                    1668 04366000
*            2 SENM0080 CHAR(8),                                   1668 04367000
*            2 SEEL0080 PTR(31),                                   1668 04368000
*            2 SECL0080 PTR(31),                                   1668 04369000
*            2 SEMC0080 PTR(31);                                   1668 04370000
*         AHLPPTR=ADDR(SEPL0080);   /* INIT PLIST PTR                */ 04371000
