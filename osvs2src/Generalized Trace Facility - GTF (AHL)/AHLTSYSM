         TITLE 'GTF MINIMAL BUILD ROUTINE                              *00001000
                       '                                                00002000
AHLTSYSM CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AHLTSYSM  74.032'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @11,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@11                                         0001 00012000
         EJECT                                                          00013000
         B     @PB00002                                            0193 00014000
AHLPRECM DS    0H                                                  0194 00015000
               USING AHLTSYSM,@11           NEW ADDRESSABILITY          00016000
               SETFRR A,FRRAD=(RTCAREG),WRKREGS=(7,9),PARMAD=(R9PARM)   00017000
*   SVR14NRM=RTNSAVE;               /* SAVE RETURN ADDR TO MCIH      */ 00018000
         ST    RTNSAVE,SVR14NRM(,R9PARM)                           0195 00019000
*   GENERATE REFS(FLCPOPSW,AHLPSCM2);                              0196 00020000
               TM  FLCPOPSW+1,X'81'           PK 0-7 + SUPER STATE      00021000
               BNZ  AHLPSCM2                 EXIT                       00022000
*   MCATRAIL=ZERO;                  /* ZERO FLAG FIELD               */ 00023000
         MVC   MCATRAIL(8,AWSAPTR),ZERO                            0197 00024000
*   PREASCB=PSAAOLD;                /* GATHER ASCB ADDR              */ 00025000
         L     @12,PSAAOLD                                         0198 00026000
         ST    @12,PREASCB(,AWSAPTR)                               0198 00027000
*   PRECPUID=PSACPUSA;              /* GATHER CPUID          @YM03489*/ 00028000
         LH    @12,PSACPUSA                                        0199 00029000
         STH   @12,PRECPUID(,AWSAPTR)                              0199 00030000
*   RETURN;                                                        0200 00031000
@EL00002 DS    0H                                                  0200 00032000
@EF00002 DS    0H                                                  0200 00033000
@ER00002 BR    @14                                                 0200 00034000
*   END;                                                           0201 00035000
@PB00002 DS    0H                                                  0201 00036000
*   GEN(EJECT);                                                    0202 00037000
         EJECT                                                          00038000
*AHLPSTCM:                                                         0203 00039000
*   MCATRL4='00'X;                  /* CLEAR ANY DEBUG BITS          */ 00040000
AHLPSTCM MVI   MCATRL4(AWSAPTR),X'00'                              0203 00041000
*   IF OPTTRC=ON                    /* TRC?                          */ 00042000
*     THEN                                                         0204 00043000
         TM    OPTTRC(PCTPTR),B'00001000'                          0204 00044000
         BO    @RT00204                                            0204 00045000
*     GOTO AHLPSCM1;                /* YES, TRACE GTF                */ 00046000
*   IF PREASCB=GTFASCB              /* NO; IS THIS GTF?              */ 00047000
*     THEN                                                         0206 00048000
         CLC   PREASCB(4,AWSAPTR),GTFASCB(PCTPTR)                  0206 00049000
         BE    @RT00206                                            0206 00050000
*     GO TO AHLPSCM2;               /* YES, DONT TRACE               */ 00051000
*AHLPSCM1:                                                         0208 00052000
*                                                                  0208 00053000
*   /*****************************************************************/ 00054000
*   /*                                                               */ 00055000
*   /* ISSUE STACK MACRO TO CAUSE TRC ENTRY TO BE MOVED TO GTF       */ 00056000
*   /* BUFFERS                                                       */ 00057000
*   /*                                                               */ 00058000
*   /*****************************************************************/ 00059000
*                                                                  0208 00060000
*   DO;                             /* AHLSTACK DLENGTH(RECLEN)    0208 00061000
*                                      AID(255) FID(03)              */ 00062000
*                                                                  0208 00063000
AHLPSCM1 DS    0H                                                  0209 00064000
*     /***************************************************************/ 00065000
*     /*                                                             */ 00066000
*     /* THE FOLLOWING CODE IS GENERATED BY THE AHLSTACK MACRO. THIS */ 00067000
*     /* CODE BUILDS THE GTF RECORD PREFIX AND THEN CALLS AHLSBLOK TO*/ 00068000
*     /* STACK THE RECORD.                                           */ 00069000
*     /*                                                             */ 00070000
*     /***************************************************************/ 00071000
*                                                                  0209 00072000
*     DCL                                                          0209 00073000
*       1 AHLGREC INT BASED(AHLRECPR)BDY(WORD),/* MAPPING FOR RECORD    00074000
*                                      PREFIX                        */ 00075000
*        2 AHLGLGTH PTR(16),        /* LENGTH OF RECORD PLUS PREFIX  */ 00076000
*        2 AHLGRES PTR(16),         /* FOR COMPATIBILITY TO QSAM     */ 00077000
*        2 AHLGAID PTR(8),          /* APPLICATION ID                */ 00078000
*        2 AHLGFID PTR(8),          /* FORMAT APPENDAGE ID           */ 00079000
*        2 AHLGTST CHAR(8),         /* TIMESTAMP IF SPECIFIED        */ 00080000
*         3 AHLGEIDS PTR(16)BDY(BYTE),/* RECORD EVENT IDENTIFIER IF     00081000
*                                      NO TIMESTAMP                  */ 00082000
*        2 AHLGEIDL PTR(16);        /* RECORD EVENT IDENTIFIER WITH     00083000
*                                      TIMESTAMP                     */ 00084000
*     DCL                                                          0210 00085000
*       AHLRECPR PTR(31)REG(1),     /* USED AS POINTER TO RECORD   0210 00086000
*                                      PREFIX AREA                   */ 00087000
*       AHLSBREG PTR(31)REG(14),    /* USED TO HOLD A STABLE COPY OF    00088000
*                                      THE SBLOKRTN FIELD IN THE   0210 00089000
*                                      GTFPCT                        */ 00090000
*       AHLRETCD PTR(31)REG(15);    /* USED TO PASS RETURN CODE TO 0210 00091000
*                                      INVOKER                       */ 00092000
*     DCL                                                          0211 00093000
*       AHLSBLOK ENTRY OPTIONS(NOEXIT,SEQFLOW,REFS(MCAWSA),SETS(   0211 00094000
*           AHLRETCD),NOFLOWS)BASED(AHLSBREG)VALRG(0);/* ENTRY FOR 0211 00095000
*                                      AHLSBLOK                      */ 00096000
*     RFY                                                          0212 00097000
*      (AHLRECPR,                                                  0212 00098000
*       AHLSBREG,                                                  0212 00099000
*       AHLRETCD)RSTD;                                             0212 00100000
*     IF OPTTIME='1'B THEN          /* CHECK FOR TIMESTAMP REQUESTED */ 00101000
         TM    OPTTIME(PCTPTR),B'00000001'                         0213 00102000
         BNO   @RF00213                                            0213 00103000
*       DO;                         /* YES, TIMESTAMP                */ 00104000
*         AHLRECPR=ADDR(MCARECPR);  /* SETUP ADDRESSIBILITY TO PREFIX   00105000
*                                      FOR PREFIX WITH TIMESTAMP     */ 00106000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0215 00107000
*         AHLGLGTH=RECLEN+16;       /* SET LENGTH TO INCLUDE ***   0216 00108000
*                                      DLENGTH *** PLUS TIMESTAMP  0216 00109000
*                                      PREFIX                        */ 00110000
         LA    @12,16                                              0216 00111000
         AR    @12,RECLEN                                          0216 00112000
         STH   @12,AHLGLGTH(,AHLRECPR)                             0216 00113000
*         GEN(STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX )  00114000
*         SETS(AHLGTST)NOEXIT SEQFLOW NOFLOWS;                     0217 00115000
         STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX         00116000
*         AHLGEIDL=MCAMCR->MCREID;  /* *** EID *** NOT SPECIFIED PUT    00117000
*                                      DEFAULT INTO PREFIX           */ 00118000
         L     @12,MCAMCR(,AWSAPTR)                                0218 00119000
         SLR   @08,@08                                             0218 00120000
         ICM   @08,3,MCREID(@12)                                   0218 00121000
         STH   @08,AHLGEIDL(,AHLRECPR)                             0218 00122000
*       END;                                                       0219 00123000
*     ELSE                          /* NO TIMESTAMP TO BE DONE, BUILD   00124000
*                                      SHORT PREFIX                  */ 00125000
*       DO;                                                        0220 00126000
         B     @RC00213                                            0220 00127000
@RF00213 DS    0H                                                  0221 00128000
*         AHLRECPR=ADDR(MCARECPR)+8;/* SETUP ADDRESSIBILITY TO FOR 0221 00129000
*                                      SHORT PREFIX WITHOUT TIMESTAMP*/ 00130000
         LA    @12,8                                               0221 00131000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0221 00132000
         AR    AHLRECPR,@12                                        0221 00133000
*         AHLGLGTH=RECLEN+8;        /* PUT TOTAL RECORD LENGTH INTO     00134000
*                                      PREFIX *** DLENGTH *** PLUS 0222 00135000
*                                      PREFIX LENGTH                 */ 00136000
         AR    @12,RECLEN                                          0222 00137000
         STH   @12,AHLGLGTH(,AHLRECPR)                             0222 00138000
*         AHLGEIDS=MCAMCR->MCREID;  /* *** EID *** NOT SPECIFIED PUT    00139000
*                                      DEFAULT INTO PREFIX           */ 00140000
         L     @12,MCAMCR(,AWSAPTR)                                0223 00141000
         SLR   @08,@08                                             0223 00142000
         ICM   @08,3,MCREID(@12)                                   0223 00143000
         STH   @08,AHLGEIDS(,AHLRECPR)                             0223 00144000
*       END;                                                       0224 00145000
*     AHLGRES=0;                    /* ZERO OUT FOR QSAM           0225 00146000
*                                      COMPATIBILITY                 */ 00147000
@RC00213 SLR   @12,@12                                             0225 00148000
         STH   @12,AHLGRES(,AHLRECPR)                              0225 00149000
*     AHLGAID=255;                  /* PUT *** AID *** INTO PREFIX   */ 00150000
         MVI   AHLGAID(AHLRECPR),X'FF'                             0226 00151000
*     AHLGFID=03;                   /* PUT *** FID *** INTO PREFIX   */ 00152000
         MVI   AHLGFID(AHLRECPR),X'03'                             0227 00153000
*     AHLRETCD=4;                   /* SET DEFAULT RETURN CODE       */ 00154000
         LA    AHLRETCD,4                                          0228 00155000
*     AHLSBREG=SBLOKRTN;            /* GET A STABLE COPY OF AHLSBLOK    00156000
*                                      ADDR                          */ 00157000
         L     AHLSBREG,SBLOKRTN(,PCTPTR)                          0229 00158000
*     IF AHLSBREG^=0 THEN           /* CHECK TO SEE IF AHLSBLOK IS 0230 00159000
*                                      AVAILABLE                     */ 00160000
         CR    AHLSBREG,@12                                        0230 00161000
         BE    @RF00230                                            0230 00162000
*       CALL AHLSBLOK;              /* YES, SO CALL AHLSBLOK TO BLOCK   00163000
*                                      RECORD                        */ 00164000
         LR    @15,AHLSBREG                                        0231 00165000
         BALR  @14,@15                                             0231 00166000
*     RFY                                                          0232 00167000
*      (AHLRECPR,                                                  0232 00168000
*       AHLSBREG,                                                  0232 00169000
*       AHLRETCD)UNRSTD;                                           0232 00170000
@RF00230 DS    0H                                                  0233 00171000
*   END;                                                           0233 00172000
*AHLPSCM2:                                                         0234 00173000
*   ;                                                              0234 00174000
AHLPSCM2 DS    0H                                                  0235 00175000
*   GEN(SETFRR D,WRKREGS=(7,9));                                   0235 00176000
         SETFRR D,WRKREGS=(7,9)                                         00177000
*   GO TO RTNSAVE;                  /* RESTORE RETURN ADDR AND RETURN   00178000
*                                      TO MCIH                       */ 00179000
         BR    RTNSAVE                                             0236 00180000
*   GEN(EJECT);                                                    0237 00181000
*                                                                  0237 00182000
         EJECT                                                          00183000
*   /*****************************************************************/ 00184000
*   /*                                                               */ 00185000
*   /* GATHER DATA AND BUILD SVC RECORD                              */ 00186000
*   /*                                                               */ 00187000
*   /*****************************************************************/ 00188000
*                                                                  0238 00189000
*AHLSVC:                                                           0238 00190000
*   ;                                                              0238 00191000
AHLSVC   DS    0H                                                  0239 00192000
*   GENERATE;                                                      0239 00193000
         USING *,EPREG                                                  00194000
         L BASEREG,BASEADDR                                             00195000
         DROP  EPREG                                                    00196000
*   RTNSAVE=RTNADDR;                /* SAVE RETURN ADDRESS           */ 00197000
         LR    RTNSAVE,RTNADDR                                     0240 00198000
*   RTCAREG=ADDR(FRRSVC);           /* ERROR ROUTINE ADDR            */ 00199000
         LA    RTCAREG,FRRSVC                                      0241 00200000
*   CALL AHLPRECM;                  /* COMMON INITIALIZER            */ 00201000
         BAL   @14,AHLPRECM                                        0242 00202000
*   MCASVC=ON;                      /* INDICATE SVC RTN              */ 00203000
         OI    MCASVC(AWSAPTR),B'10000000'                         0243 00204000
*   SVCTCB=PSATOLD;                 /* GATHER TCB@                   */ 00205000
         L     @12,PSATOLD                                         0244 00206000
         ST    @12,SVCTCB(,AWSAPTR)                                0244 00207000
*   SVCR15=EVREG15;                 /* GATHER R15                    */ 00208000
         L     @12,LCCAPTR                                         0245 00209000
         MVC   SVCR15(4,AWSAPTR),EVREG15(@12)                      0245 00210000
*   SVCR0R1=EVREG01;                /* GATHER R0 + R1                */ 00211000
         MVC   SVCR0R1(8,AWSAPTR),EVREG01(@12)                     0246 00212000
*   SVCPSW=FLCSOPSW;                /* GATHER PSW                    */ 00213000
         MVC   SVCPSW(8,AWSAPTR),FLCSOPSW                          0247 00214000
*   SVCINT=FLCSVCN;                 /* INT CODE LIKE BC MODE PSW     */ 00215000
         LH    @12,FLCSVCN                                         0248 00216000
         STH   @12,SVCINT(,AWSAPTR)                                0248 00217000
*   RECLEN=LENGTH(SVCREC);                                         0249 00218000
         LA    RECLEN,30                                           0249 00219000
*   GOTO AHLPSTCM;                  /* GOTO EXIT CODE                */ 00220000
         B     AHLPSTCM                                            0250 00221000
*   GEN(EJECT);                                                    0251 00222000
*                                                                  0251 00223000
         EJECT                                                          00224000
*   /*****************************************************************/ 00225000
*   /*                                                               */ 00226000
*   /* GATHER DATA AND BUILD EXT RECORD                              */ 00227000
*   /*                                                               */ 00228000
*   /*****************************************************************/ 00229000
*                                                                  0252 00230000
*AHLEXT:                                                           0252 00231000
*   ;                                                              0252 00232000
AHLEXT   DS    0H                                                  0253 00233000
*   GENERATE;                                                      0253 00234000
         USING *,EPREG                                                  00235000
         L BASEREG,BASEADDR                                             00236000
         DROP  EPREG                                                    00237000
*   RTNSAVE=RTNADDR;                /* SAVE RETURN ADDRESS           */ 00238000
         LR    RTNSAVE,RTNADDR                                     0254 00239000
*   RTCAREG=ADDR(FRREXT);           /* ERROR ROUTINE ADDR            */ 00240000
         LA    RTCAREG,FRREXT                                      0255 00241000
*   GENERATE;                                                      0256 00242000
               SETFRR A,FRRAD=(RTCAREG),WRKREGS=(7,9),PARMAD=(R9PARM)   00243000
*   SVR14NRM=RTNSAVE;               /* SAVE RETURN ADDR TO MCIH      */ 00244000
         ST    RTNSAVE,SVR14NRM(,R9PARM)                           0257 00245000
*   RFY                                                            0258 00246000
*     MCRWSA BASED(MCAMCR);                                        0258 00247000
*   IF MCRBRENT='0'B THEN                                          0259 00248000
         L     @12,MCAMCR(,AWSAPTR)                                0259 00249000
         TM    MCRBRENT(@12),B'00010000'                           0259 00250000
         BZ    @RT00259                                            0259 00251000
*     GO TO AHLPSCM2;               /* IF NOT BRANCH ENTRY, USER   0260 00252000
*                                      ISSUED THE HOOK. EXIT         */ 00253000
*   RFY                                                            0261 00254000
*     MCRWSA BASED(WSACGTF);                                       0261 00255000
*   MCATRAIL=EXTMASK;               /* INDICATE EXT ROUTINE IN     0262 00256000
*                                      CONTROL                       */ 00257000
         MVC   MCATRAIL(8,AWSAPTR),EXTMASK                         0262 00258000
*   EXTASCB=PSAAOLD;                /* GATHER ASCB ADDR              */ 00259000
         L     @12,PSAAOLD                                         0263 00260000
         ST    @12,EXTASCB(,AWSAPTR)                               0263 00261000
*   EXTCPUID=PSACPUSA;              /* GATHER CPUID          @YM03489*/ 00262000
         LH    @12,PSACPUSA                                        0264 00263000
         STH   @12,EXTCPUID(,AWSAPTR)                              0264 00264000
*   EXTPSW=FLCEOPSW;                /* GATHER PSW DATA               */ 00265000
         MVC   EXTPSW(8,AWSAPTR),FLCEOPSW                          0265 00266000
*   EXTINT=FLCEICOD;                /* GATHER INTERRUPT CODE         */ 00267000
         LH    @12,FLCEICOD                                        0266 00268000
         STH   @12,EXTINT(,AWSAPTR)                                0266 00269000
*   IF LCCASRBM=ON                  /* SRB MODE ?                    */ 00270000
*     THEN                                                         0267 00271000
         L     @12,LCCAPTR                                         0267 00272000
         TM    LCCASRBM(@12),B'10000000'                           0267 00273000
         BNO   @RF00267                                            0267 00274000
*     EXTTCB=NOTAPP;                /* NO TCB W SRB MODE             */ 00275000
         MVC   EXTTCB(4,AWSAPTR),@CC02647                          0268 00276000
*   ELSE                                                           0269 00277000
*     EXTTCB=PSATOLD;               /* GATHER TCB DATA               */ 00278000
         B     @RC00267                                            0269 00279000
@RF00267 L     @12,PSATOLD                                         0269 00280000
         ST    @12,EXTTCB(,AWSAPTR)                                0269 00281000
*   MCAMEXT=ON;                     /* FOR ERROR RECOVERY            */ 00282000
@RC00267 OI    MCAMEXT(AWSAPTR),B'10000000'                        0270 00283000
*   RECLEN=LENGTH(EXTREC);                                         0271 00284000
         LA    RECLEN,22                                           0271 00285000
*   IF EICOD=CCINT THEN                                            0272 00286000
         CLC   EICOD(2),@CB02655                                   0272 00287000
         BNE   @RF00272                                            0272 00288000
*     DO;                           /* CLK COMPARATOR INT            */ 00289000
*       IF PCCATQEP^=0              /* NO TQE ?                      */ 00290000
*         THEN                                                     0274 00291000
         L     @12,PCCAPTR                                         0274 00292000
         L     @08,PCCATQEP(,@12)                                  0274 00293000
         LTR   @08,@08                                             0274 00294000
         BZ    @RF00274                                            0274 00295000
*         EXTVAR=TQETCB;            /* TCB W INT                     */ 00296000
         L     @12,TQEPTR(,@12)                                    0275 00297000
         L     @12,TQETCB(,@12)                                    0275 00298000
         ST    @12,EXTVAR(,AWSAPTR)                                0275 00299000
*       ELSE                                                       0276 00300000
*         EXTVAR=UNAVAL;            /* UNAVAILABLE                   */ 00301000
         B     @RC00274                                            0276 00302000
@RF00274 MVC   EXTVAR(4,AWSAPTR),UNAVAL                            0276 00303000
*       GOTO AHLPSTCM;              /* EXIT                          */ 00304000
         B     AHLPSTCM                                            0277 00305000
*     END;                                                         0278 00306000
*   IF INTTYP=SIGPINT               /* SIGNAL PROCESSOR              */ 00307000
*     THEN                                                         0279 00308000
@RF00272 CLI   INTTYP,X'12'                                        0279 00309000
         BNE   @RF00279                                            0279 00310000
*     EXTVAR=PSAEEPSW;              /* PROCESSOR ADDR                */ 00311000
         MVC   EXTVAR(4,AWSAPTR),PSAEEPSW                          0280 00312000
*   ELSE                                                           0281 00313000
*     EXTVAR=NOTAPP;                /* N/A FOR OTHER INTRPTS         */ 00314000
         B     @RC00279                                            0281 00315000
@RF00279 MVC   EXTVAR(4,AWSAPTR),@CC02647                          0281 00316000
*   GOTO AHLPSTCM;                  /* EXIT                          */ 00317000
         B     AHLPSTCM                                            0282 00318000
*   GEN(EJECT);                                                    0283 00319000
*                                                                  0283 00320000
         EJECT                                                          00321000
*   /*****************************************************************/ 00322000
*   /*                                                               */ 00323000
*   /* GATHER DATA AND BUILD IO TRACE RECORD                         */ 00324000
*   /*                                                               */ 00325000
*   /*****************************************************************/ 00326000
*                                                                  0284 00327000
*AHLIO:                                                            0284 00328000
*   ;                                                              0284 00329000
AHLIO    DS    0H                                                  0285 00330000
*   GENERATE;                                                      0285 00331000
         USING *,EPREG                                                  00332000
         L BASEREG,BASEADDR                                             00333000
         DROP  EPREG                                                    00334000
*   RTNSAVE=RTNADDR;                /* SAVE RETURN ADDRESS           */ 00335000
         LR    RTNSAVE,RTNADDR                                     0286 00336000
*   RTCAREG=ADDR(FRRIO);            /* ERROR ROUTINE ADDR            */ 00337000
         LA    RTCAREG,FRRIO                                       0287 00338000
*   CALL AHLPRECM;                                                 0288 00339000
         BAL   @14,AHLPRECM                                        0288 00340000
*   MCAIO=ON;                       /* INDICATE IO RTN               */ 00341000
         OI    MCAIO(AWSAPTR),B'00100000'                          0289 00342000
*   IOCSW=FLCCSW;                   /* GATHER CSW                    */ 00343000
         MVC   IOCSW(8,AWSAPTR),FLCCSW                             0290 00344000
*   IOPSW=FLCIOPSW;                 /* GATHER PSW                    */ 00345000
         MVC   IOPSW(8,AWSAPTR),FLCIOPSW                           0291 00346000
*   IODEV=EVREG6;                   /* GATHER IO DEVICE              */ 00347000
         L     @12,LCCAPTR                                         0292 00348000
         MVC   IODEV(4,AWSAPTR),EVREG6(@12)                        0292 00349000
*   RECLEN=LENGTH(IOREC);                                          0293 00350000
         LA    RECLEN,30                                           0293 00351000
*   IF EVREG2=0                     /* IOSB VALID ?                  */ 00352000
*     THEN                                                         0294 00353000
         L     @12,EVREG2(,@12)                                    0294 00354000
         LTR   @12,@12                                             0294 00355000
         BNZ   @RF00294                                            0294 00356000
*     DO;                           /* PUT U/A IN IOSB DEP FLDS      */ 00357000
*       IOASCB=UNAVAL;                                             0296 00358000
         MVC   IOASCB(4,AWSAPTR),UNAVAL                            0296 00359000
*       IOSENSE=UNAVAL(1:2);                                       0297 00360000
         MVC   IOSENSE(2,AWSAPTR),UNAVAL                           0297 00361000
*       IOTCB=UNAVAL;                                              0298 00362000
         MVC   IOTCB(4,AWSAPTR),UNAVAL                             0298 00363000
*       GOTO AHLPSTCM;                                             0299 00364000
         B     AHLPSTCM                                            0299 00365000
*     END;                                                         0300 00366000
*   MCAMIOSB=ON;                    /* TURN ON IOSB BIT              */ 00367000
@RF00294 OI    MCAMIOSB(AWSAPTR),B'00001000'                       0301 00368000
*   IF IOSERP=0 THEN                /* NO SENSE AVAILABLE            */ 00369000
         L     @12,LCCAPTR                                         0302 00370000
         L     @12,IOSBPTR(,@12)                                   0302 00371000
         L     @12,IOSERP(,@12)                                    0302 00372000
         LTR   @12,@12                                             0302 00373000
         BNZ   @RF00302                                            0302 00374000
*     IOSENSE=NOTAPP;               /* N/ IN SENSE                   */ 00375000
         MVC   IOSENSE(2,AWSAPTR),@CC02647                         0303 00376000
*   ELSE                                                           0304 00377000
*     IOSENSE=EWASNS;               /* GATHER SENSE                  */ 00378000
         B     @RC00302                                            0304 00379000
@RF00302 L     @12,LCCAPTR                                         0304 00380000
         L     @12,IOSBPTR(,@12)                                   0304 00381000
         L     @12,EWAPTR(,@12)                                    0304 00382000
         MVC   IOSENSE(2,AWSAPTR),EWASNS(@12)                      0304 00383000
*   MCAMASCB=ON;                    /* TURN ON DBUG BIT              */ 00384000
@RC00302 OI    MCAMASCB(AWSAPTR),B'00100000'                       0305 00385000
*   IOASCB=ASVTENTY(IOSASID);       /* GATHER ASCB ADDR              */ 00386000
         L     @12,LCCAPTR                                         0306 00387000
         L     @12,IOSBPTR(,@12)                                   0306 00388000
         SLR   @08,@08                                             0306 00389000
         ICM   @08,3,IOSASID(@12)                                  0306 00390000
         SLA   @08,2                                               0306 00391000
         L     @12,CVTPTR                                          0306 00392000
         L     @12,ASVTPTR(,@12)                                   0306 00393000
         L     @12,ASVTENTY-4(@08,@12)                             0306 00394000
         ST    @12,IOASCB(,AWSAPTR)                                0306 00395000
*RCOV1:                                                            0307 00396000
*   MCAMTCB=ON;                     /* TURN ON DBUG BIT              */ 00397000
