         TITLE 'GTF USR BUILD ROUTINE                                  *00001000
                        '                                               00002000
AHLTUSR  CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AHLTUSR   76.282'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  DS    0H                                                  0002 00010000
   USING AHLTUSR,REG15      TEMPORARY ADDRESSIBILITY                    00011000
   L  BASEREG,BASE          ENTRY POINT ADDRESS TO BASE REG             00012000
   USING AHLTUSR,BASEREG    ADDRESSIBLITY SET UP TO MODULE              00013000
   DROP REG15                                                           00014000
   USING @DATD-MCAWORK,AWSAPTR SET UP ADDRESSIBLITY TO COMPILER WORK    00015000
*   RTNADSV=RTNADDR;                /* SAVE RTN ADDR                 */ 00016000
         LR    RTNADSV,RTNADDR                                     0076 00017000
*   MCATRAIL=DEBUGFLG;              /* SET ON MCAUSR                 */ 00018000
         MVC   MCATRAIL(8,AWSAPTR),@CB01633                        0077 00019000
*   REG8=ADDR(AHLTUSRE);            /* SET UP FOR SETFRR             */ 00020000
         LA    REG8,AHLTUSRE                                       0078 00021000
*   GEN(SETFRR A,FRRAD=(8),WRKREGS=(7,9));/* ERROR RECOVERY          */ 00022000
         SETFRR A,FRRAD=(8),WRKREGS=(7,9)                               00023000
*   RFY                                                            0080 00024000
*     REG8 UNRSTD;                                                 0080 00025000
*   PMLSTPTR=LCCAPGR2(2);           /* INITIALIZE THE ADDRESS OF PARM   00026000
*                                      LIST POINTED TO BY THE USER'S    00027000
*                                      REG 1                         */ 00028000
*                                                                  0081 00029000
         L     @08,LCCAPTR                                         0081 00030000
         L     PMLSTPTR,LCCAPGR2+4(,@08)                           0081 00031000
*   /*****************************************************************/ 00032000
*   /*                                                               */ 00033000
*   /* FOLLOWING SECTION VALIDITY CHECKS THE FID, EID, AND LENGTH OF */ 00034000
*   /* THE DATA                                                      */ 00035000
*   /*                                                               */ 00036000
*   /*****************************************************************/ 00037000
*                                                                  0082 00038000
*   IF USRFID<0|USRFID>255          /* IS FID VALID                  */ 00039000
*     THEN                                                         0082 00040000
         LH    @08,USRFID(,PMLSTPTR)                               0082 00041000
         LTR   @08,@08                                             0082 00042000
         BM    @RT00082                                            0082 00043000
         C     @08,@CF01706                                        0082 00044000
         BNH   @RF00082                                            0082 00045000
@RT00082 DS    0H                                                  0083 00046000
*     DO;                                                          0083 00047000
*       USRRC=RCFID;                /* RETURN CODE INVALID FID       */ 00048000
         L     @08,LCCAPTR                                         0084 00049000
         MVC   USRRC(4,@08),@CF00043                               0084 00050000
*       GOTO AHLTUSR1;              /* RETURN WITHOUT TRACE ENTRY    */ 00051000
         B     AHLTUSR1                                            0085 00052000
*     END;                                                         0086 00053000
*   IF USRLNG<1                     /* IS USER LENGTH VALID          */ 00054000
*       |USRLNG>256                 /* MINLEN < USRLNG < MAXLEN      */ 00055000
*     THEN                                                         0087 00056000
@RF00082 LH    @08,USRLNG(,PMLSTPTR)                               0087 00057000
         LTR   @08,@08                                             0087 00058000
         BNP   @RT00087                                            0087 00059000
         C     @08,@CF00699                                        0087 00060000
         BNH   @RF00087                                            0087 00061000
@RT00087 DS    0H                                                  0088 00062000
*     DO;                                                          0088 00063000
*       USRRC=RCLNGTH;              /* RC INVALID DATA LNGHT         */ 00064000
         L     @08,LCCAPTR                                         0089 00065000
         MVC   USRRC(4,@08),@CF00052                               0089 00066000
*       GOTO AHLTUSR1;              /* RETURN WITHOUT TRACE ENTRY    */ 00067000
         B     AHLTUSR1                                            0090 00068000
*     END;                                                         0091 00069000
*                                                                  0091 00070000
*/********************************************************************/ 00071000
*/*  SEE PROLOGUE NOTE FOR ABSENCE EID VALIDITY CHECK        @AZ13027*/ 00072000
*/********************************************************************/ 00073000
*                                                                  0092 00074000
*                                                                  0092 00075000
*   /*****************************************************************/ 00076000
*   /*                                                               */ 00077000
*   /* GET INTO USERS PROTECT KEY TO SECURITY CHECK ADDRESSES        */ 00078000
*   /*                                                               */ 00079000
*   /*****************************************************************/ 00080000
*                                                                  0092 00081000
*   GENERATE;                                                      0092 00082000
*                                                                  0092 00083000
@RF00087 DS    0H                                                  0092 00084000
    IC REG8,FLCPOPSW+1    MOVE PROTECT KEY TO REG 8                     00085000
    SPKA 0(REG8)          GET INTO USERS PROTECT KEY                    00086000
*   /*****************************************************************/ 00087000
*   /*                                                               */ 00088000
*   /* SECURITY CHECK THE ADDRESS OF THE PARMLIST                    */ 00089000
*   /*                                                               */ 00090000
*   /*****************************************************************/ 00091000
*                                                                  0093 00092000
*   RFY                                                            0093 00093000
*     PLINDREG RSTD;                                               0093 00094000
*   PLINDREG=PARMREF;               /* INDICATE USER PARMLIST REF    */ 00095000
         L     PLINDREG,PARMREF                                    0094 00096000
*   GEN(CLC 0(8,PMLSTPTR),0(PMLSTPTR));/* REFERENCE USR PARMLIST     */ 00097000
         CLC 0(8,PMLSTPTR),0(PMLSTPTR)                                  00098000
*   PLINDREG=0;                     /* REMOVE USER PARMLIST REF      */ 00099000
*                                                                  0096 00100000
         SLR   PLINDREG,PLINDREG                                   0096 00101000
*   /*****************************************************************/ 00102000
*   /*                                                               */ 00103000
*   /* SECURITY CHECK THE DATA ADDRESS                               */ 00104000
*   /*                                                               */ 00105000
*   /*****************************************************************/ 00106000
*                                                                  0097 00107000
*   REG15=DATAPTR;                  /* REG15 CONTAINS DATA ADDRESS   */ 00108000
         L     REG15,DATAPTR(,PMLSTPTR)                            0097 00109000
*   GENERATE;                                                      0098 00110000
   TM 0(REG15),X'FF'             REFERENCE FIRST BYTE                   00111000
   AH REG15,USRLNG(0,PMLSTPTR)   LOCATE END OF DATA                     00112000
   BCTR REG15,0                  FIND LAST BYTE                         00113000
   TM 0(REG15),X'FF'             REFERENCE LAST BYTE                    00114000
*   SPKA(ZEROPK);                                                  0099 00115000
         SLR   @15,@15                                             0099 00116000
         SPKA  0(@15)                                              0099 00117000
*   RFY                                                            0100 00118000
*     PLINDREG UNRSTD;                                             0100 00119000
*                                                                  0101 00120000
*   /*****************************************************************/ 00121000
*   /*                                                               */ 00122000
*   /* GATHER ASCB ADDRESS AND INSERT IN TRACE ENTRY                 */ 00123000
*   /*                                                               */ 00124000
*   /*****************************************************************/ 00125000
*                                                                  0101 00126000
*   TRCASCB=PSAAOLD;                /* PICK UP ASCB ADDR             */ 00127000
*                                                                  0101 00128000
         L     @08,PSAAOLD                                         0101 00129000
         ST    @08,TRCASCB(,AWSAPTR)                               0101 00130000
*   /*****************************************************************/ 00131000
*   /*                                                               */ 00132000
*   /* GATHER THE JOBNAME AND INSERTS IT IN TRACE ENTRY              */ 00133000
*   /*                                                               */ 00134000
*   /*****************************************************************/ 00135000
*                                                                  0102 00136000
*   IF ASCBJBNI^=0                  /* PTR TO INITIATOR JOBNAME ZERO    00137000
*                                      ?                             */ 00138000
*     THEN                                                         0102 00139000
         L     @08,ASCBPTR                                         0102 00140000
         L     @08,ASCBJBNI(,@08)                                  0102 00141000
         CR    @08,@15                                             0102 00142000
         BE    @RF00102                                            0102 00143000
*     IF JI^=0                      /* INITIATOR JOBNAME ZERO ?      */ 00144000
*       THEN                                                       0103 00145000
         C     @15,JI(,@08)                                        0103 00146000
         BE    @RF00103                                            0103 00147000
*       TRCJOBN=JBNI;               /* IF NOT ZERO, JOBNAME IS VALID */ 00148000
         MVC   TRCJOBN(8,AWSAPTR),JBNI(@08)                        0104 00149000
*     ELSE                                                         0105 00150000
*       TRCJOBN=NOTAPP;             /* IF ZERO, JOBNAME NOT        0105 00151000
*                                      APPLICALBLE                   */ 00152000
         B     @RC00103                                            0105 00153000
@RF00103 MVC   TRCJOBN(8,AWSAPTR),@CC01639                         0105 00154000
*   ELSE                                                           0106 00155000
*     IF ASCBJBNS^=0                /* PTR TO START/MOUNT/LOGON    0106 00156000
*                                      JOBNAME ZERO ?                */ 00157000
*       THEN                                                       0106 00158000
         B     @RC00102                                            0106 00159000
@RF00102 SLR   @08,@08                                             0106 00160000
         L     @15,ASCBPTR                                         0106 00161000
         L     @15,ASCBJBNS(,@15)                                  0106 00162000
         CR    @15,@08                                             0106 00163000
         BE    @RF00106                                            0106 00164000
*       IF JS^=0                    /* START/MOUNT/LOGON JOBNAME   0107 00165000
*                                      ZERO?                         */ 00166000
*         THEN                                                     0107 00167000
         C     @08,JS(,@15)                                        0107 00168000
         BE    @RF00107                                            0107 00169000
*         TRCJOBN=JBNS;             /* IF NOT, JOBNAME VALID         */ 00170000
         MVC   TRCJOBN(8,AWSAPTR),JBNS(@15)                        0108 00171000
*       ELSE                                                       0109 00172000
*         TRCJOBN=NOTAPP;           /* IF ZERO , JOBNAME NOT       0109 00173000
*                                      APPLICALBLE                   */ 00174000
         B     @RC00107                                            0109 00175000
@RF00107 MVC   TRCJOBN(8,AWSAPTR),@CC01639                         0109 00176000
*     ELSE                                                         0110 00177000
*       TRCJOBN=NOTAPP;             /* IF ASCBJBNS IS ZERO THEN    0110 00178000
*                                      JOBNAME IS NOT APPLICABLE     */ 00179000
         B     @RC00106                                            0110 00180000
@RF00106 MVC   TRCJOBN(8,AWSAPTR),@CC01639                         0110 00181000
*                                                                  0111 00182000
*   /*****************************************************************/ 00183000
*   /*                                                               */ 00184000
*   /* GATHER THE USER DATA AND MOVES INTO TRC ENTRY                 */ 00185000
*   /*                                                               */ 00186000
*   /*****************************************************************/ 00187000
*                                                                  0111 00188000
*   TRCDATA(1:USRLNG)=USRDATA(1:USRLNG);/* MOVE USR DATA TO ENTRY    */ 00189000
@RC00106 DS    0H                                                  0111 00190000
@RC00102 LH    @08,USRLNG(,PMLSTPTR)                               0111 00191000
         LR    @15,@08                                             0111 00192000
         BCTR  @15,0                                               0111 00193000
         L     @14,DATAPTR(,PMLSTPTR)                              0111 00194000
         EX    @15,@SM02174                                        0111 00195000
*   RFY                                                            0112 00196000
*     ULENGTH RSTD;                                                0112 00197000
*   ULENGTH=USRLNG+12;              /* ADD JOBNAME AND ASCB LENGTHS  */ 00198000
         AL    @08,@CF01576                                        0113 00199000
         LR    ULENGTH,@08                                         0113 00200000
*                                                                  0114 00201000
*   /*****************************************************************/ 00202000
*   /*                                                               */ 00203000
*   /* ISSUE STACK MACRO TO MOVE TRACE ENTRY TO GTF BUFFERS          */ 00204000
*   /*                                                               */ 00205000
*   /*****************************************************************/ 00206000
*                                                                  0114 00207000
*   DO;                             /* AHLSTACK DLENGTH(ULENGTH)AID('   00208000
*                                      FF'X)FID(USRFID)              */ 00209000
*                                                                  0114 00210000
*     /***************************************************************/ 00211000
*     /*                                                             */ 00212000
*     /* THE FOLLOWING CODE IS GENERATED BY THE AHLSTACK MACRO. THIS */ 00213000
*     /* CODE BUILDS THE GTF RECORD PREFIX AND THEN CALLS AHLSBLOK TO*/ 00214000
*     /* STACK THE RECORD.                                           */ 00215000
*     /*                                                             */ 00216000
*     /***************************************************************/ 00217000
*                                                                  0115 00218000
*     DCL                                                          0115 00219000
*       1 AHLGREC INT BASED(AHLRECPR) BDY(WORD),/* MAPPING FOR RECORD   00220000
*                                      PREFIX                        */ 00221000
*        2 AHLGLGTH PTR(16),        /* LENGTH OF RECORD PLUS PREFIX  */ 00222000
*        2 AHLGRES PTR(16),         /* FOR COMPATIBILITY TO QSAM     */ 00223000
*        2 AHLGAID PTR(8),          /* APPLICATION ID                */ 00224000
*        2 AHLGFID PTR(8),          /* FORMAT APPENDAGE ID           */ 00225000
*        2 AHLGTST CHAR(8),         /* TIMESTAMP IF SPECIFIED        */ 00226000
*         3 AHLGEIDS PTR(16) BDY(BYTE),/* RECORD EVENT IDENTIFIER IF    00227000
*                                      NO TIMESTAMP                  */ 00228000
*        2 AHLGEIDL PTR(16);        /* RECORD EVENT IDENTIFIER WITH     00229000
*                                      TIMESTAMP                     */ 00230000
*     DCL                                                          0116 00231000
*       AHLRECPR PTR(31) REG(1),    /* USED AS POINTER TO RECORD   0116 00232000
*                                      PREFIX AREA                   */ 00233000
*       AHLSBREG PTR(31) REG(14),   /* USED TO HOLD A STABLE COPY OF    00234000
*                                      THE SBLOKRTN FIELD IN THE   0116 00235000
*                                      GTFPCT                        */ 00236000
*       AHLRETCD PTR(31) REG(15);   /* USED TO PASS RETURN CODE TO 0116 00237000
*                                      INVOKER                       */ 00238000
*     DCL                                                          0117 00239000
*       AHLSBLOK ENTRY OPTIONS(NOEXIT,SEQFLOW,REFS(MCAWSA),SETS(   0117 00240000
*           AHLRETCD),NOFLOWS) BASED(AHLSBREG) VALRG(0);/* ENTRY FOR    00241000
*                                      AHLSBLOK                      */ 00242000
*     RFY                                                          0118 00243000
*      (AHLRECPR,                                                  0118 00244000
*       AHLSBREG,                                                  0118 00245000
*       AHLRETCD) RSTD;                                            0118 00246000
*     IF OPTTIME='1'B THEN          /* CHECK FOR TIMESTAMP REQUESTED */ 00247000
         TM    OPTTIME(PCTPTR),B'00000001'                         0119 00248000
         BNO   @RF00119                                            0119 00249000
*       DO;                         /* YES, TIMESTAMP                */ 00250000
*         AHLRECPR=ADDR(MCARECPR);  /* SETUP ADDRESSIBILITY TO PREFIX   00251000
*                                      FOR PREFIX WITH TIMESTAMP     */ 00252000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0121 00253000
*         AHLGLGTH=ULENGTH+16;      /* SET LENGTH TO INCLUDE ***   0122 00254000
*                                      DLENGTH *** PLUS TIMESTAMP  0122 00255000
*                                      PREFIX                        */ 00256000
         LA    @08,16                                              0122 00257000
         ALR   @08,ULENGTH                                         0122 00258000
         STH   @08,AHLGLGTH(,AHLRECPR)                             0122 00259000
*         GEN(STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX )  00260000
*         SETS(AHLGTST) NOEXIT SEQFLOW NOFLOWS;                    0123 00261000
         STCK AHLGTST(AHLRECPR) PUT TOD CLOCK VALUE INTO PREFIX         00262000
*         AHLGEIDL=MCAMCR->MCREID;  /* *** EID *** NOT SPECIFIED PUT    00263000
*                                      DEFAULT INTO PREFIX           */ 00264000
         L     @08,MCAMCR(,AWSAPTR)                                0124 00265000
         LH    @08,MCREID(,@08)                                    0124 00266000
         N     @08,@CF01631                                        0124 00267000
         STH   @08,AHLGEIDL(,AHLRECPR)                             0124 00268000
*       END;                                                       0125 00269000
*     ELSE                          /* NO TIMESTAMP TO BE DONE, BUILD   00270000
*                                      SHORT PREFIX                  */ 00271000
*       DO;                                                        0126 00272000
         B     @RC00119                                            0126 00273000
@RF00119 DS    0H                                                  0127 00274000
*         AHLRECPR=ADDR(MCARECPR)+8;/* SETUP ADDRESSIBILITY TO FOR 0127 00275000
*                                      SHORT PREFIX WITHOUT TIMESTAMP*/ 00276000
         LA    @08,8                                               0127 00277000
         LA    AHLRECPR,MCARECPR(,AWSAPTR)                         0127 00278000
         ALR   AHLRECPR,@08                                        0127 00279000
*         AHLGLGTH=ULENGTH+8;       /* PUT TOTAL RECORD LENGTH INTO     00280000
*                                      PREFIX *** DLENGTH *** PLUS 0128 00281000
*                                      PREFIX LENGTH                 */ 00282000
         ALR   @08,ULENGTH                                         0128 00283000
         STH   @08,AHLGLGTH(,AHLRECPR)                             0128 00284000
*         AHLGEIDS=MCAMCR->MCREID;  /* *** EID *** NOT SPECIFIED PUT    00285000
*                                      DEFAULT INTO PREFIX           */ 00286000
         L     @08,MCAMCR(,AWSAPTR)                                0129 00287000
         LH    @08,MCREID(,@08)                                    0129 00288000
         N     @08,@CF01631                                        0129 00289000
         STH   @08,AHLGEIDS(,AHLRECPR)                             0129 00290000
*       END;                                                       0130 00291000
*     AHLGRES=0;                    /* ZERO OUT FOR QSAM           0131 00292000
*                                      COMPATIBILITY                 */ 00293000
@RC00119 SLR   @08,@08                                             0131 00294000
         STH   @08,AHLGRES(,AHLRECPR)                              0131 00295000
*     AHLGAID='FF'X;                /* PUT *** AID *** INTO PREFIX   */ 00296000
         MVI   AHLGAID(AHLRECPR),X'FF'                             0132 00297000
*     AHLGFID=USRFID;               /* PUT *** FID *** INTO PREFIX   */ 00298000
         LH    @04,USRFID(,PMLSTPTR)                               0133 00299000
         STC   @04,AHLGFID(,AHLRECPR)                              0133 00300000
*     AHLRETCD=4;                   /* SET DEFAULT RETURN CODE       */ 00301000
         LA    AHLRETCD,4                                          0134 00302000
*     AHLSBREG=SBLOKRTN;            /* GET A STABLE COPY OF AHLSBLOK    00303000
*                                      ADDR                          */ 00304000
         L     AHLSBREG,SBLOKRTN(,PCTPTR)                          0135 00305000
*     IF AHLSBREG^=0 THEN           /* CHECK TO SEE IF AHLSBLOK IS 0136 00306000
*                                      AVAILABLE                     */ 00307000
         CR    AHLSBREG,@08                                        0136 00308000
         BE    @RF00136                                            0136 00309000
*       CALL AHLSBLOK;              /* YES, SO CALL AHLSBLOK TO BLOCK   00310000
*                                      RECORD                        */ 00311000
         LR    @15,AHLSBREG                                        0137 00312000
         BALR  @14,@15                                             0137 00313000
*     RFY                                                          0138 00314000
*      (AHLRECPR,                                                  0138 00315000
*       AHLSBREG,                                                  0138 00316000
*       AHLRETCD) UNRSTD;                                          0138 00317000
@RF00136 DS    0H                                                  0139 00318000
*   END;                                                           0139 00319000
*   RFY                                                            0140 00320000
*     RCREG RSTD;                                                  0140 00321000
*   RFY                                                            0141 00322000
*     ULENGTH UNRSTD;                                              0141 00323000
*   IF RCREG=0                      /* STACK WENT OK                 */ 00324000
*     THEN                                                         0142 00325000
         SLR   @14,@14                                             0142 00326000
         CR    RCREG,@14                                           0142 00327000
         BNE   @RF00142                                            0142 00328000
*     USRRC=RCOK;                   /* INDICATE OK TO USER.          */ 00329000
         L     @08,LCCAPTR                                         0143 00330000
         ST    @14,USRRC(,@08)                                     0143 00331000
*   ELSE                                                           0144 00332000
*     USRRC=RCBUFFL;                /* INDICATE BUFFERS FULL TO USER */ 00333000
         B     @RC00142                                            0144 00334000
@RF00142 L     @14,LCCAPTR                                         0144 00335000
         MVC   USRRC(4,@14),@CF00091                               0144 00336000
*   RFY                                                            0145 00337000
*     RCREG UNRSTD;                                                0145 00338000
@RC00142 DS    0H                                                  0146 00339000
*                                                                  0146 00340000
*   /*****************************************************************/ 00341000
*   /*                                                               */ 00342000
*   /* EXIT CODE                                                     */ 00343000
*   /*                                                               */ 00344000
*   /*****************************************************************/ 00345000
*                                                                  0146 00346000
*AHLTUSR1:                                                         0146 00347000
*   MCATRAIL=CLEAR;                 /* CLEAR DEBUG BITS              */ 00348000
AHLTUSR1 MVC   MCATRAIL(8,AWSAPTR),@CB01636                        0146 00349000
*   GEN(SETFRR D,WRKREGS=(7,9));    /* REMOVE FRR                    */ 00350000
         SETFRR D,WRKREGS=(7,9)                                         00351000
*   BC(15,RTNADSV);                 /* RETURN TO AHLMCIH             */ 00352000
         BCR   15,RTNADSV                                          0148 00353000
*AHLTUSRE:                                                         0149 00354000
*                                                                  0149 00355000
*   /*****************************************************************/ 00356000
*   /*                                                               */ 00357000
*   /* SET UP ADDRESSIBILITY FOR THIS FRR ROUTINE                    */ 00358000
*   /*                                                               */ 00359000
*   /*****************************************************************/ 00360000
*                                                                  0149 00361000
*   GEN;                                                           0149 00362000
*                                                                  0149 00363000
AHLTUSRE DS    0H                                                  0149 00364000
               USING AHLTUSRE,REG15        SET UP TEMP ADDRESSIBILITY   00365000
               L BASEREG,BASE              ENTRY POINT ADDR TO BASEREG  00366000
               DROP REG15                                               00367000
               USING AHLTUSR,BASEREG      SET UP ADDRESSIBILITY IN FRR  00368000
*/* ******************************************************************/ 00369000
*/*                                                                  */ 00370000
*/*  THE SDWA (SYSTEM DIAGNOSTIC WORK AREA) IS USED BY BOTH (E)STA   */ 00371000
*/*  EXIT ROUTINES AND FRR ROUTINES AS THE COMMUNICATION AREA        */ 00372000
*/*  BETWEEN THE RTM AND THE (E)STA OR FRR WHEN ERRORS OCCUR.        */ 00373000
*/*  IT IS ALSO REFERRED TO AS THE RTCA (RECOVERY TERMINATION        */ 00374000
*/*  COMMUNICATION AREA). IT CONTAINS DATA CONCERNING THE ORIGINAL   */ 00375000
*/*  ERROR AND ALSO CONCERNING ACTIONS OF PREVIOUSLY ENTERED         */ 00376000
*/*  RECOVERY ROUTINES.                                              */ 00377000
*/*                                                                  */ 00378000
*/*  METHOD OF ACCESS                                                */ 00379000
*/*  BAL                                                             */ 00380000
*/*              IHASDWA DSECT =  YES                                */ 00381000
*/*                               NO                                 */ 00382000
*/*  DSECT = YES IS THE DEFAULT AND WILL PRODUCE THE SDWA            */ 00383000
*/*             AS A DSECT.                                          */ 00384000
*/*  DSECT = NO WILL PRODUCE THE SDWA AS A CONTINUATION OF           */ 00385000
*/*             DEFINED STORAGE. (NO DSECT CARD PRODUCED)            */ 00386000
*/*  PLS                                                             */ 00387000
*/*    IF MACRO VARIABLE %SDWA9999 IS NOT EQUAL TO ','               */ 00388000
*/*    AND %SDWALVL IS NULL, THE SDWA WILL BE INCLUDED               */ 00389000
*/*    AS A LEVEL 1 STRUCTURE BASED ON SDWAPTR.                      */ 00390000
*/*                                                                  */ 00391000
*/*    IF %SDWALVL IS SPECIFIED AS A NUMBER FROM 1 TO 4              */ 00392000
*/*    THE SDWA WILL BE A NON-BASED STRUCTURE WITH THE               */ 00393000
*/*    TOP LEVEL EQUAL TO %SDWALVL.                                  */ 00394000
*/*                                                                  */ 00395000
*/*    IF SDWA9999 IS EQUAL TO ',' THE SDWA WILL                     */ 00396000
*/*    TERMINATE WITH A COMMA RATHER THAN A                          */ 00397000
*/*    SEMI-COLON.                                                   */ 00398000
*/*                                                                  */ 00399000
*/*                                                                  */ 00400000
*/*  MACMEAN SDWA MEANS SYSTEM DIAGNOSTIC WORK AREA                  */ 00401000
*/*  MACCOMP Y-1 SC1C5/PZD81                                         */ 00402000
*/*  MACSTAT Y-1 72318/Y02703,Y02704                                 */ 00403000
*/*                                                                  */ 00404000
*/* ******************************************************************/ 00405000
*                                                                  0150 00406000
*   DECLARE                                                        0150 00407000
*     1 SDWA BASED(SDWAPTR),                                       0150 00408000
*          6 SDWAPARM PTR(31),      /* PARAMETER LIST ADDRESS IF   0150 00409000
*                                      (E)STA MACRO SPECIFIED PARAM     00410000
*                                      OPTION OR 0. FOR FRRS THIS IS    00411000
*                                      THE ADDRESS OF THE 6 WORD PARM   00412000
*                                      AREA RETURNED BY THE SETFRR 0150 00413000
*                                      MACRO WHEN THE PARMAD KEYWORD    00414000
*                                      IS SPECIFIED ON THE ON THE  0150 00415000
*                                      SETFRR MACRO                  */ 00416000
*          6 SDWAFIOB PTR(31),      /* ADDRESS OF PURGE I/O REQUEST     00417000
*                                      LIST (PIRL), OR 0 IF HALT I/O    00418000
*                                      IS REQUESTED ON ENTRY TO RETRY   00419000
*                                      ROUTINE FOR (E)STA.           */ 00420000
