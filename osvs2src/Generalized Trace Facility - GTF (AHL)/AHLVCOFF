AHLVCOFF CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AHLVCOFF  73.241'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                  /* EQUATES FOR BAL                */ 00014000
SQA      EQU   245                 SUBPOOL NUMBER FOR MCAWSA            00015000
*   MCRPTR=WSACGTF;                 /* GET PTR TO MCRWSA             */ 00016000
         LR    @02,CPUID                                           0021 00017000
         SLA   @02,2                                               0021 00018000
         L     @15,CVTPTR                                          0021 00019000
         L     @15,CVTLCCAT(,@15)                                  0021 00020000
         L     @02,LCCAT00P(@02,@15)                               0021 00021000
         L     @02,LCCACPUS(,@02)                                  0021 00022000
         L     MCRPTR,WSACGTF(,@02)                                0021 00023000
*   AWSAPTR=MCRMCA;                 /* GET PTR TO MCAWSA             */ 00024000
         L     AWSAPTR,MCRMCA(,MCRPTR)                             0022 00025000
*   IF AWSAPTR^=0 THEN              /* IS THERE A MCAWSA             */ 00026000
         SR    @05,@05                                             0023 00027000
         CR    AWSAPTR,@05                                         0023 00028000
         BE    @RF00023                                            0023 00029000
*     DO;                           /* YES - FREE IT                 */ 00030000
*       MCRMCA=0;                   /* ZERO PTR TO MCAWSA IN MCRWSA  */ 00031000
         ST    @05,MCRMCA(,MCRPTR)                                 0025 00032000
*       GEN REFS(AWSAPTR)SEQFLOW NOFLOWS NOEXIT NOSETS;            0026 00033000
         FREEMAIN  R,LV=800,A=(AWSAPTR),SP=SQA                          00034000
*     END;                                                         0027 00035000
*   RETURN;                         /* RETURN TO CALLER              */ 00036000
@EL00001 DS    0H                                                  0028 00037000
@EF00001 DS    0H                                                  0028 00038000
@ER00001 LM    @14,@12,12(@13)                                     0028 00039000
         BR    @14                                                 0028 00040000
