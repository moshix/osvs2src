*   /*****************************************************************/ 01297000
*   /*                                                               */ 01298000
*   /* BEGIN TIME OF DAY CONVERSION. MOVE THE TOD VALUE TO A DOUBLE  */ 01299000
*   /* WORD AREA AND ADD THE CVTTZ VALUE TO TOD BIT POSITIONS 0-31.  */ 01300000
*   /* THIS WILL CONVERT THE TOD VALUE TO LOCAL TIME.                */ 01301000
*   /*                                                               */ 01302000
*   /*****************************************************************/ 01303000
*                                                                  0427 01304000
*   GEN(BALR BASE10,0);             /* ESTABLISH Y01980              */ 01305000
         BALR BASE10,0                                                  01306000
*   GEN(USING *,BASE10);            /* ADDRESSABILITY Y01980         */ 01307000
         USING *,BASE10                                                 01308000
*   CVRTAREA=TODVALUE;              /* MOVE TOD VALUE TO A DOUBLEWORD   01309000
*                                      WORKAREA                Y01980*/ 01310000
         L     @11,TODPTR(,PARMREG1)                               0429 01311000
         MVC   CVRTAREA(8),TODVALUE(@11)                           0429 01312000
*   TODWORD1=TODWORD1+CVTTZVAL;     /* CHANGE TOD TO LOCAL TIMEY01980*/ 01313000
*                                                                  0430 01314000
