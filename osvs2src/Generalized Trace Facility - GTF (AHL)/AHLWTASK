*   /*****************************************************************/ 01319000
*   /*                                                               */ 01320000
*   /* CALCULATE TOTAL NUMBER OF MINUTES SINCE THE EPOCH TIME THE    */ 01321000
*   /* NUMBER OF MICROSECONDS THAT HAVE PASSED IN THE CURRENT MINUTE.*/ 01322000
*   /*                                                               */ 01323000
*   /*****************************************************************/ 01324000
*                                                                  0431 01325000
*   GEN(LM L,W,CVRTAREA);           /* PUT TOD VALUE IN EVEN-ODD   0431 01326000
*                                      REGISTER PAIR           Y01980*/ 01327000
         LM L,W,CVRTAREA                                                01328000
*   GEN(SRDL L,12);                 /* SHIFT TOD VALUE TO REMOVE THE    01329000
*                                      INSIGNIFICANT BITS      Y01980*/ 01330000
         SRDL L,12                                                      01331000
*   GEN(D L,SIXTYMIL);              /* DIVID TOD VALUE BY NUMBER OF     01332000
*                                      MICROSECONDS IN A MINUTE. W -    01333000
*                                      CONTAINS TOTAL MINUTES SINCE     01334000
*                                      THE EPOCH TIME          Y01980   01335000
*                                      L - CONTAINS TOTAL NUMBER OF     01336000
*                                      MICROSECONDS INTO CURRENT   0433 01337000
*                                      MINUTE                  Y01980*/ 01338000
*                                                                  0433 01339000
         D L,SIXTYMIL                                                   01340000
*   /*****************************************************************/ 01341000
*   /*                                                               */ 01342000
*   /* GO TO PROPER CONVERSION ROUTINE FOR PROCESSING THIS TOD       */ 01343000
*   /* CONVERT REQUEST. USE THE REQUEST CODE IN PARMREG2 TO INDEX THE*/ 01344000
*   /* TODBRTAB. THE CODES AND ASSOCIATED ROUTINE ENTRY LABELS ARE AS*/ 01345000
*   /* FOLLOWS: CODE ENTRY TYPE OF CONVERSION 1 FULLDATE CONVERT TO A*/ 01346000
*   /* FULL DATE AND TIME 2 RELTVTME CONVERT TO RELATIVE TIME        */ 01347000
*   /*                                                               */ 01348000
*   /*****************************************************************/ 01349000
*                                                                  0434 01350000
*   GOTO TODBRTAB(PARMREG2);        /* BRANCH ON REQUEST CODE  Y01980*/ 01351000
         LR    @11,PARMREG2                                        0434 01352000
         SLA   @11,2                                               0434 01353000
         L     @11,TODBRTAB-4(@11)                                 0434 01354000
         BR    @11                                                 0434 01355000
*                                                                  0435 01356000
*   /*****************************************************************/ 01357000
*   /*                                                               */ 01358000
*   /* RELATIVE TIME CONVERSION -- THIS ROUTINE RECEIVES CONTROL AT  */ 01359000
*   /* ENTRY 'RELTVTME'. IT CONVERTS THE TOD VALUE TO RELATIVE       */ 01360000
*   /* SECONDS.MICROSECONDS FROM 0 A.M. OF THE CURRENT DAY REFLECTED */ 01361000
*   /* BY THE TOD VALUE. THE CONVERTED TIME IS PLACED LEFT JUSTIFIED */ 01362000
*   /* IN THE OUTPUT AREA PASSED BY THE TODCVRT CALLER AND IS IN THE */ 01363000
*   /* FORM SSSSS.MMMMMM (EBCDIC). UPON ENTRY TO THIS ROUTINE THE TOD*/ 01364000
*   /* VALUE HAS ALREADY BEEN CHANGED TO TOTAL MINUTES (IN W) AND    */ 01365000
*   /* NUMBER OF MICROSECONDS INTO CURRENT MINUTE (IN L). WHEN       */ 01366000
*   /* PROCESSING IS COMPLETED CONTROL IS RETURNED TO THE TODCNVRT   */ 01367000
*   /* CALLER                                                        */ 01368000
*   /*                                                               */ 01369000
*   /*****************************************************************/ 01370000
*                                                                  0435 01371000
*RELTVTME:                                                         0435 01372000
*   IF TODVALUE=ZEROTOD THEN        /* IS TOD VALUE ZERO ?     Y01980*/ 01373000
RELTVTME L     @11,TODPTR(,PARMREG1)                               0435 01374000
         CLC   TODVALUE(8,@11),ZEROTOD                             0435 01375000
         BNE   @RF00435                                            0435 01376000
*     DO;                           /* YES, PASS BACK A TIME OF ZERO */ 01377000
*       TIMEERIA=ZEROTIME;          /* MOVE ZERO TIME INTO OUT AREA     01378000
*                                                              Y01980*/ 01379000
         L     @11,OUTAREA(,PARMREG1)                              0437 01380000
         MVI   TIMEERIA+12(@11),C' '                               0437 01381000
         MVC   TIMEERIA+13(11,@11),TIMEERIA+12(@11)                0437 01382000
         MVC   TIMEERIA(12,@11),ZEROTIME                           0437 01383000
*       RETURN CODE(4);             /* INDICATE ZERO TOD TO CALLER 0438 01384000
*                                                              Y01980*/ 01385000
         LA    @15,4                                               0438 01386000
         L     @14,12(,@13)                                        0438 01387000
         LM    @00,@12,20(@13)                                     0438 01388000
         BR    @14                                                 0438 01389000
*     END;                                                         0439 01390000
*   SECONDS=L/1000000;              /* CALCULATE NUMBER OF SECONDS 0440 01391000
*                                      INTO CURRENT MINUTE.    Y01980*/ 01392000
