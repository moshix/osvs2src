         MACRO -- $PQH -- HASP 3800 PENDING PAGE QUEUE HEADER  @G38ESBB BS001000
         $PQH  &DOC=NO                                         @G38ESBB BS002000
         AIF   ('&DOC' NE 'YES').C                             @G38ESBB BS003000
.C     TITLE 'HASP 3800 PENDING PAGE QUEUE HEADER (PQH) DSECT' @G38ESBB BS004000
PQHDSECT DSECT                     3800 PAGE QUEUE HEADER      @G38ESBB BS005000
PQHFIRST DS    F                   ADR OF OLDEST ACTIVE PQE    @G38ESBB BS006000
PQHLAST  DS    F                   ADR OF NEWEST ACTIVE PQE    @G38ESBB BS007000
PQHFCHN  DS    F                   EXTENT FORWARD CHAIN ADDR   @OZ48003 BS007100
PQHBCHN  DS    F                   EXTENT BACK CHAIN ADDR      @OZ48003 BS007200
PQHPQECT DS    X                   AVAIL PQE COUNT IN EXTENT   @OZ48003 BS007300
PQHPQELM DS    X                   TOTAL PQE COUNT IN EXTENT   @OZ48003 BS007400
PQHPCNT  DS    X                   NUMBER OF PPQ EXTENTS       @OZ48003 BS007500
PQHAFLAG DS    X                   ALLOCATION FLAGS            @OZ48003 BS007600
PQHFREE  DS    F                   ADR OF PQE FREE QUEUE       @G38ESBB BS008000
PQHPQEJ  DS    F                   ADR OF JOB START PQEJ QUEUE @G38ESBB BS009000
PQHPIDE  DS    F                   ADR OF OLDST PQE PENDING ID @G38ESBB BS010000
PQHXJQE  DS    F                   ADR OF TRANSFER STATION JQE @G38ESBB BS011000
PQHOPQE  DS    F                   PQE PREVIOUS TO CMD ORIGIN  @G38ESBB BS012000
PQHTPQE  DS    F                   PQE PREVIOUS TO CMD TARGET  @G38ESBB BS013000
PQHOPG   DS    H                   COMMAND ORIGIN PAGE ID      @G38ESBB BS014000
PQHTPG   DS    H                   TARGET PAGE ID              @OZ53047 BS015000
PQHMAPV  DS    F                   PAGE MAPPING VALUE          @OZ53047 BS015100
PQHFCB   DS    F                   FCB BUFFER ADR FOR MAPPING  @G38ESBB BS016000
PQHFCBLN DS    H                   FCB MAPPING LINE POSITION   @G38ESBB BS017000
PQHLPG   DS    H                   LOCATED XFER STATION PGID   @OZ46351 BS017100
PQHSAVE1 DS    F                   REGISTER SAVE AREA          @G38ESBB BS018000
PQHSAVE2 DS    F                   REGISTER SAVE AREA          @G38ESBB BS019000
PQHPCICH DS    X                   PCI CHAIN BYTE DURING IR    @G38ESBB BS020000
PQHCMDCT DS    X                   DEFERRED COMMAND COUNT      @G38ESBB BS021000
PQHFLAG  DS    X                   FLAG BYTE                   @G38ESBB BS022000
PQHFLAG2 DS    X                   COMMAND FLAGS               @OZ47787 BS022100
PQHCPG   DS    H                   CHANNEL PAGE ID             @OZ49145 BS022200
         SPACE 1                                               @OZ48003 BS022500
PQHALOC  EQU   B'10000000'         SUPPRESS CLEARPRINT FLAG    @OZ48003 BS022600
PQHPBUF0 EQU   B'01000000'         PAGE BUFFER EMPTY FLAG      @OZ48003 BS022650
PQHCPIO  EQU   B'00100000'         CLEARPRINT I/O FLAG         @OZ48003 BS022700
PQHABORT EQU   B'00010000'         PPQ ERROR FLAG              @OZ49145 BS022750
         SPACE 1                                               @G38ESBB BS023000
PQH2CMD  EQU   B'10000000'         DOUBLE COMMAND INDICATOR    @G38ESBB BS024000
PQHHDR   EQU   B'01000000'         JOB HEADER NEEDED           @G38ESBB BS025000
PQHXFER  EQU   B'00100000'         TRANSFER STATION COMMAND    @G38ESBB BS026000
PQHDSVC  EQU   B'00010000'         DEVICE SETUP COMMAND        @OZ45081 BS026500
PQHIPPQM EQU   B'00001000'         INHIBIT PPQ MANAGER         @OZ46674 BS026600
PQHRSTRT EQU   B'00000100'         PPQ RESTART FLAG            @OZ47595 BS026700
PQHDRAIN EQU   B'00000010'         PPQ DRAIN FLAG              @OZ49145 BS026800
PQHCHCMD EQU   B'00000001'         SYNCHRONOUS COMMAND FLAG    @OZ51011 BS026900
         SPACE 1                                               @G38ESBB BS027000
PCMDCEI  EQU   B'10000000'         $C,$E,$I COMMAND            @OZ47787 BS027100
PCMDBKSP EQU   B'01000000'         $B COMMAND                  @OZ47787 BS027200
PCMDFWSP EQU   B'00100000'         $F COMMAND                  @OZ47787 BS027300
PCMDRBDS EQU   B'00010000'         BDS RESTART INDICATOR       @OZ47734 BS027400
         SPACE 1                                               @OZ47787 BS027900
PQHEND   DS    0D                  END OF PQH                  @OZ48003 BS028000
PQHLENG  EQU   PQHEND-PQHDSECT     LENGTH OF PQH               @G38ESBB BS029000
&SYSECT  CSECT                                                 @G38ESBB BS030000
         MEND                                                  @G38ESBB BS031000
