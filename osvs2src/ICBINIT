         TM    ICBV(@04),B'00100000'                               0120 00274000
         BNO   @RF00120                                            0120 00275000
*         DO;                       /*                       @ZA11002*/ 00276000
*           RESPECIFY                                              0122 00277000
*             R0 RESTRICTED;        /* FOR CONVERSION        #Y30LB04*/ 00278000
*           CALL RCTOEBC;           /* GET REASON CONVERTED  #Y30LB04*/ 00279000
         BAL   @14,RCTOEBC                                         0123 00280000
*           TXT=NOESMSG;            /* MOVE IN MESSAGE TEXT  @ZA09529*/ 00281000
