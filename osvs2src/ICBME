         ST    R13,R13SAVE                                         0109 00254000
*   R13=ADDR(MELOCSAV);             /* PROVIDE REGSAVE TO SETLOCK  0110 00255000
*                                                            #Y30LB04*/ 00256000
         LA    R13,MELOCSAV                                        0110 00257000
*IMODESE1:                          /* FIRST ICBME MODESET USE     0111 00258000
*                                                            #Y30LB04*/ 00259000
*   DO;                             /* MODESET KEYADDR(ZERO)         */ 00260000
