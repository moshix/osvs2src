*   RFY                                                            0117 00286000
*     R1 RSTD;                      /* RESTRICT REGISTER 1   @ZA08838*/ 00287000
*   R1=ADDR(STAERL);                /* ADDRESS OF STAE LIST  @ZA08838*/ 00288000
         LA    R1,STAERL                                           0118 00289000
*ISTAE2:                            /* ESTABLISH STAE ENVIRONMENT  0119 00290000
*                                                            @ZA08838*/ 00291000
*   GEN(STAE MEREXIT,PARAM=(R11),MF=(E,(1)))/* RECOVERY EXIT @ZA08838*/ 00292000
*   REFS(MEREXIT,R11,R1) SETS(R15); /* MEREXIT ROUTINE       @ZA08838*/ 00293000
ISTAE2   STAE MEREXIT,PARAM=(R11),MF=(E,(1))                            00294000
*ISETLOC1:                          /*                       #Y30LB04*/ 00295000
*   GEN REFS(R0,R1,R15,PSALITA)     /*                       #Y30LB04*/ 00296000
*       SETS(R15);                                                 0120 00297000
