         L     @10,CVTICB(,@10)                                    0122 00323000
         L     @10,ICBOPT(,@10)                                    0122 00324000
         MVC   ALOBUF(240,@14),SDGLS(@10)                          0122 00325000
*   /*****************************************************************/ 00326000
*   /*                                                               */ 00327000
*   /* FREE THE CMS AND LOCAL LOCKS, NO LONGER REQUIRED      #Y30LB04*/ 00328000
*   /*                                                               */ 00329000
*   /*****************************************************************/ 00330000
*                                                                  0123 00331000
*ISETLOC3:                          /*                       @YD00151*/ 00332000
*   GEN REFS(R0,R1,R15,PSALITA)     /*                       #Y30LB04*/ 00333000
*       SETS(R15);                                                 0123 00334000
ISETLOC3 DS    0H                                                  0123 00335000
*                                                        /* #Y30LB04*/  00336000
       SETLOCK RELEASE,TYPE=CMS,REGS=SAVE,RELATED=(CMSLOCK,ICBME(ISETLOO00337000
               C2))                                           @Y30LB04  00338000
*ISETLOC4:                          /*                       @YD00151*/ 00339000
*   GEN REFS(R0,R1,R15,PSALITA)     /*                       #Y30LB04*/ 00340000
*       SETS(R15);                                                 0124 00341000
ISETLOC4 DS    0H                                                  0124 00342000
*                                                         /*#Y30LB04*/  00343000
       SETLOCK RELEASE,TYPE=LOCAL,REGS=SAVE,RELATED=(LOCALLOCK,ICBME(I**00344000
               SETLOC1))                                      @Y30LB04  00345000
*   IF AVOLWORD=NOVOLID             /* WAS NO VOLUME SPECIFIED     0125 00346000
*                                                            #Y30LB04*/ 00347000
*     THEN                          /* NOT SPECIFIC REQUEST  #Y30LB04*/ 00348000
