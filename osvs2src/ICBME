*     RFY                                                          0086 00135000
*       R14 UNRSTD;                 /* FREE UP REGISTER 14   #Y30LB04*/ 00136000
*                                                                  0086 00137000
*     /***************************************************************/ 00138000
*     /*                                                             */ 00139000
*     /* NOTE -- IEFAB4UV BUILDS THE UNIT LIST AND SETS A    #Y30LB04*/ 00140000
*     /* POINTER TO IT INTO THE ICBME COMMUNICATION AREA     #Y30LB04*/ 00141000
*     /*                                                             */ 00142000
*     /***************************************************************/ 00143000
*                                                                  0087 00144000
*     IF R15^=GOODCODE              /* DID IEFAB4UV BUILD LIST ?   0087 00145000
*                                                            #Y30LB04*/ 00146000
*       THEN                        /* THE LIST WAS NOT BUILT      0087 00147000
*                                                            #Y30LB04*/ 00148000
         LTR   R15,R15                                             0087 00149000
         BZ    @RF00087                                            0087 00150000
*       DO;                         /* FIND OUT WHY & WHETHER TO   0088 00151000
*                                      CONTINUE              #Y30LB04*/ 00152000
*         IF R15=NOTAVAIL           /* CANNOT THIS HOST USE THIS   0089 00153000
*                                      UNIT?                 #Y30LB04*/ 00154000
*           THEN                    /* THIS HOST CANNOT USE THIS   0089 00155000
*                                      UNIT,WILL LOOP        #Y30LB04*/ 00156000
