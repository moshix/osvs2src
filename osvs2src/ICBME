*   BASEVALU=R12;                   /* SAVE BASE ADDRESS FOR MEREXIT    00280000
*                                                            @ZA08838*/ 00281000
         ST    R12,BASEVALU                                        0115 00282000
*   STAERL=''B;                     /* ZERO STAE REMOTE PARM LIST  0116 00283000
*                                                            @ZA08838*/ 00284000
