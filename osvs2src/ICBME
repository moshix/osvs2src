         BR    @14                                                 0101 00208000
*                                                                  0102 00209000
*/********************************************************** #Y30LB04*/ 00210000
*/*                                                          #Y30LB04*/ 00211000
*/*   ICBMEDR: MSSC MOUNT EQUALIZATION AND DATA REUSE        #Y30LB04*/ 00212000
*/*                                                          #Y30LB04*/ 00213000
*/*   INPUT: POINTER TO ALLOCATION PARAMETER LIST            #Y30LB04*/ 00214000
*/*            MSSC ACTIVITY BUFFER (FOUND VIA CVT AND MSSC  #Y30LB04*/ 00215000
*/*                        CONTROL BLOCK)                    #Y30LB04*/ 00216000
*/*                                                          #Y30LB04*/ 00217000
*/* PROCESSING: COPIES MSSC ACTIVITY BUFFER INTO ALLOCATION  #Y30LB04*/ 00218000
*/*               BUFFER                                     #Y30LB04*/ 00219000
*/*            ENTERS KEY ZERO FOR LOCK ENTRIES AND MSSC     #Y30LB04*/ 00220000
*/*              I/O DRIVER USE                              #Y30LB04*/ 00221000
*/*            FOR SPECIFIC VOLUME, DOES DIAGNOSTIC READ TO  #Y30LB04*/ 00222000
*/*              MSC FOR MOUNTED VOLUME TABLE ENTRY          #Y30LB04*/ 00223000
*/*            IF THAT VOLUME WAS MOUNTED, FINDS THE         #Y30LB04*/ 00224000
*/*              CORRESPONDING STAGING DRIVE GROUP IN THE    #Y30LB04*/ 00225000
*/*              MSSC ACTIVITY BUFFER LIST AND MOVES IT TO   #Y30LB04*/ 00226000
*/*              THE TOP OF THE LIST IN ALLOCATION COPY      #Y30LB04*/ 00227000
*/*            RETURNS TO KEY OF SCHEDULER                   #Y30LB04*/ 00228000
*/*                                                          #Y30LB04*/ 00229000
*/*   OUTPUT:  BUFFER FOR ALLOCATION TO USE IN CHOOSING      #Y30LB04*/ 00230000
*/*              VOLUME                                      #Y30LB04*/ 00231000
*/*          R15 IS ZERO IF ALL WENT WELL                    #Y30LB04*/ 00232000
*/*          R15 CONTAINS ERROR CODE IF ANY PROBLEM          #Y30LB04*/ 00233000
*/*                                                          #Y30LB04*/ 00234000
*/********************************************************** #Y30LB04*/ 00235000
*                                                                  0102 00236000
*ICBMEDR:                                                          0102 00237000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/* DATA REUSE       #Y30LB04*/ 00238000
         B     @EL00001                                            0102 00239000
ICBMEDR  DS    0H                                                  0103 00240000
*   DCL                                                            0103 00241000
*     MEDR14 PTR(31) AUTOMATIC;     /* FOR RETURN ADDRESS    @YD00151*/ 00242000
*   DCL                                                            0104 00243000
*     MELOCSAV(5) POINTER(31) AUTOMATIC;/* FOR SETLOCK       #Y30LB04*/ 00244000
*   RFY                                                            0105 00245000
*     R14 RSTD;                     /* RESTRICT REGISTER 14  #Y30LB04*/ 00246000
*   MEDR14=R14;                     /* SAVE RETURN ADDRESS   #Y30LB04*/ 00247000
         ST    R14,MEDR14                                          0106 00248000
*   RFY                                                            0107 00249000
*     R14 UNRSTD;                   /* FREE UP REGISTER 14   #Y30LB04*/ 00250000
*   RFY                                                            0108 00251000
*     R13 RSTD;                     /* RESTRICT REGISTER 13  #Y30LB04*/ 00252000
