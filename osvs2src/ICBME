         ICM   @10,15,AVOLWORD(@14)                                0125 00350000
         BZ    @RT00125                                            0125 00351000
*     ;                             /* SO JUST RETURN        #Y30LB04*/ 00352000
*   ELSE                            /* HAVE SPECIFIC VOLUME REQUEST     00353000
*                                                            #Y30LB04*/ 00354000
*     DO;                           /* SEE IF MOUNTED TO MSC #Y30LB04*/ 00355000
*       CALL VOLENTR;               /* GETS MOUNTED VOLUME TABLE   0128 00356000
*                                      ENTRY                 #Y30LB04*/ 00357000
*                                                                  0128 00358000
         BAL   @14,VOLENTR                                         0128 00359000
*       /*************************************************************/ 00360000
*       /*                                                           */ 00361000
*       /* THERE MAY OR NOT BE A MOUNTED VOLUME TABLE ENTRY  #Y30LB04*/ 00362000
*       /*                                                           */ 00363000
*       /*************************************************************/ 00364000
*                                                                  0129 00365000
*       IF R15^=GOODCODE            /* WAS THERE AN ERROR ?  #Y30LB04*/ 00366000
*         THEN                      /* SOME VOLENTR (ETC) PROBLEM  0129 00367000
*                                                            #Y30LB04*/ 00368000
         LTR   R15,R15                                             0129 00369000
         BNZ   @RT00129                                            0129 00370000
*         ;                         /* NO MORE FOR THIS TIME #Y30LB04*/ 00371000
*       ELSE                        /* NO ERROR FROM VOLENTR #Y30LB04*/ 00372000
*         DO;                       /* CONTINUE PROCESSING   #Y30LB04*/ 00373000
*           IF MOUNTED=NO           /* WAS VOL NOT MOUNTED ? #Y30LB04*/ 00374000
*             THEN                  /* NO MOUNTED VOLUME TABLE ENTRY    00375000
*                                      IN MSC                #Y30LB04*/ 00376000
         TM    MOUNTED,B'10000000'                                 0132 00377000
         BZ    @RT00132                                            0132 00378000
*             ;                     /* ACTIVITY BUFFER RETURNS AS IS    00379000
*                                                            #Y30LB04*/ 00380000
*           ELSE                    /* VOLUME WAS MOUNTED    #Y30LB04*/ 00381000
*             DO;                   /* MAY REARRANGE BUFFER  #Y30LB04*/ 00382000
*               CALL MATCHSRT;      /* MATCH & PUT ON TOP OF BUFFER     00383000
*                                                            @Y30LB04*/ 00384000
         BAL   @14,MATCHSRT                                        0135 00385000
*             END;                  /* PROCESSING WHEN VOLUME MOUNTED   00386000
*                                                            #Y30LB04*/ 00387000
*         END;                      /* SPECIFIC VOLUME REQUEST,NO  0137 00388000
*                                      ERROR                 #Y30LB04*/ 00389000
@RT00132 DS    0H                                                  0138 00390000
*     END;                          /* SPECIFIC VOLUME PROCESSING  0138 00391000
