*     END;                          /* TO AVOID LOW CORE.    @ZA11005*/ 00046000
*   IF ACALLCNT=FRSTCAL             /* FIRST CALL OF ICBME FOR THIS     00047000
*                                      REQ?                  #Y30LB04*/ 00048000
*     THEN                          /* THIS IS FIRST ENTRY   #Y30LB04*/ 00049000
