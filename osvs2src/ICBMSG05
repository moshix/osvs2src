         TITLE 'ICBMSG05  -  MSSC INITIALIZATION                       *00001000
                        '                                               00002000
ICBMSG05 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ICBMSG05  78.188'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @04,0                                               0001 00011000
@PSTART  LA    @05,4095(,@04)                                      0001 00012000
         LA    @12,4095(,@05)                                      0001 00013000
         USING @PSTART,@04                                         0001 00014000
         USING @PSTART+4095,@05                                    0001 00015000
         USING @PSTART+8190,@12                                    0001 00016000
         ST    @13,@SA00001+4                                      0001 00017000
         LA    @14,@SA00001                                        0001 00018000
         ST    @14,8(,@13)                                         0001 00019000
         LR    @13,@14                                             0001 00020000
*                                                                       00021000
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF          00022000
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES      00023000
** WILL ASSEMBLE PROPERLY.                                              00024000
QUERY1   DS    0H                                                  0224 00025000
IICBMNT1 DS    0H                                                  0291 00026000
IPAIR1   DS    0H                                                  0294 00027000
IPAIR2   DS    0H                                                  0296 00028000
FOUR    EQU    X'04'                                                    00029000
*   INITPTR=ADDR(REQLIST1);                                        0346 00030000
         LA    @00,REQLIST1                                        0346 00031000
         ST    @00,INITPTR                                         0346 00032000
*   PURGPTR=R1;                     /* SAVE INPUT PARM REG   @G24LB02*/ 00033000
         ST    R1,PURGPTR                                          0347 00034000
*   RFY                                                            0348 00035000
*     R1 UNRSTD;                    /* FREE REG 1            @G24LB02*/ 00036000
*   IF CVTNIP=ON THEN               /* TEST IF NIP IN CONTROL      0349 00037000
*                                                            @G24LB02*/ 00038000
         L     @15,CVTPTR                                          0349 00039000
         TM    CVTNIP(@15),B'00010000'                             0349 00040000
         BNO   @RF00349                                            0349 00041000
*     NVTPTR=ICBNVT;                /* FIND THE NVT          @G24LB02*/ 00042000
         L     @01,CVTEXT2(,@15)                                   0350 00043000
         L     @01,CVTICB(,@01)                                    0350 00044000
         MVC   NVTPTR(4),ICBNVT(@01)                               0350 00045000
*   BITSW=''B;                      /* CLEAR SWITCH          @G24LB02*/ 00046000
@RF00349 MVI   BITSW,X'00'                                         0351 00047000
*   NMESPTR=ADDR(NMSGST);           /* INITIALIZE MESSAGE PTR      0352 00048000
*                                                            @G24LB02*/ 00049000
         LA    @15,NMSGST                                          0352 00050000
         ST    @15,NMESPTR                                         0352 00051000
*   RESPECIFY                                                      0353 00052000
*     NVT BASED(NVTPTR);            /*                       @G24LB02*/ 00053000
*   RFY                                                            0354 00054000
*    (RCVT,                                                        0354 00055000
*     RNVT) UNRSTD;                 /* INPUT REGISTERS       @G24LB02*/ 00056000
*/********************************************************** @G24LB02*/ 00057000
*                                                                  0355 00058000
*   ECB1=ADDR(ICBECB);              /* INITIALIZE ECB1       @G24LB02*/ 00059000
         L     @15,CVTPTR                                          0355 00060000
         L     @01,CVTEXT2(,@15)                                   0355 00061000
         L     @14,CVTICB(,@01)                                    0355 00062000
         LA    @11,ICBECB(,@14)                                    0355 00063000
         ST    @11,ECB1                                            0355 00064000
*   ECB2=ADDR(ICBACTV);             /* INITIALIZE ECB2       @X70ZC02*/ 00065000
         LA    @11,ICBACTV(,@14)                                   0356 00066000
         ST    @11,ECB2                                            0356 00067000
*   ECB3=ADDR(ICBCOM);              /* INITIALIZE ECB3       @G24LB02*/ 00068000
         LA    @11,ICBCOM(,@14)                                    0357 00069000
         ST    @11,ECB3                                            0357 00070000
*   ECB4=ADDR(ICBIOR);              /* INITIALIZE ECB4       @G24LB02*/ 00071000
         LA    @11,ICBIOR(,@14)                                    0358 00072000
         STCM  @11,7,ECB4                                          0358 00073000
*   ICBECB=ZERO;                    /* CLEAR ECB ICBECB      @G24LB02*/ 00074000
         SLR   @11,@11                                             0359 00075000
         ST    @11,ICBECB(,@14)                                    0359 00076000
*   ICBACTV=ZERO;                   /* CLEAR ECB ICBACTV     @G24LB02*/ 00077000
         ST    @11,ICBACTV(,@14)                                   0360 00078000
*   ICBCOM=ZERO;                    /* CLEAR ECB ICBCOM      @G24LB02*/ 00079000
         ST    @11,ICBCOM(,@14)                                    0361 00080000
*   ICBIOR=ZERO;                    /* CLEAR ECB ICBIOR      @G24LB02*/ 00081000
         ST    @11,ICBIOR(,@14)                                    0362 00082000
*   ICBMSCA=OFF;                    /* INITIALIZE APS BIT    @G24LB05*/ 00083000
*   ICBMSCMP=OFF;                   /* INITIALIZE MP BIT     @G24LB05*/ 00084000
         NI    ICBMSCA(@14),B'11111001'                            0364 00085000
*   INITSW=OFF;                     /* INITIALIZE INITSW TO 0      0365 00086000
*                                                            @G24LB05*/ 00087000
         NI    INITSW,B'11110111'                                  0365 00088000
*   IF CSDCPUOL^=ONE THEN           /* IS THIS A MP SYSTEM?  @G24LB05*/ 00089000
         L     @01,CVTCSD(,@15)                                    0366 00090000
         CLC   CSDCPUOL(2,@01),@CH00130                            0366 00091000
         BE    @RF00366                                            0366 00092000
*     ICBMSCMP=ON;                  /* INDICATE MP SYSTEM?   @G24LB05*/ 00093000
         OI    ICBMSCMP(@14),B'00000100'                           0367 00094000
*/********************************************************** @G24LB02*/ 00095000
*                                                                  0368 00096000
*   CALL INTPRISC;                  /* INITIALIZE THE MSC(S) @G24LB02*/ 00097000
@RF00366 BAL   @14,INTPRISC                                        0368 00098000
*/********************************************************** @G24LB02*/ 00099000
*                                                                  0369 00100000
*   IF CLEANCOD^=ALLCLEAN           /* ANY ERROR SO FAR ?    @G24LB02*/ 00101000
*     THEN                          /* SOME ERROR, NO MSSC   @G24LB02*/ 00102000
         ICM   @15,15,CLEANCOD                                     0369 00103000
         BNZ   @RT00369                                            0369 00104000
*     GOTO NOMSSC;                  /* GO DIRECTLY OUT       @G24LB02*/ 00105000
*   ELSE                            /* CONTINUE WITH MSSC          0371 00106000
*                                      INITIALIZATION        @G24LB02*/ 00107000
*     ;                             /*                       @G24LB02*/ 00108000
*/********************************************************** @G24LB02*/ 00109000
*                                                                  0372 00110000
*   CALL SOCIATE;                   /* ISSUE ASSOCIATE OR          0372 00111000
*                                      DISASSOCIATE TO       @G24LB02*/ 00112000
         BAL   @14,SOCIATE                                         0372 00113000
*/********************************************************** @G24LB02*/ 00114000
*                                                                  0373 00115000
*   /*****************************************************************/ 00116000
*   /*                                                               */ 00117000
*   /* MSC(S) IN APPROPRIATE RELATIONSHIP WITH               @G24LB02*/ 00118000
*   /* PURGE, IF SPECIFIED                                   @G24LB02*/ 00119000
*   /*                                                               */ 00120000
*   /*****************************************************************/ 00121000
*                                                                  0373 00122000
*/********************************************************** @G24LB02*/ 00123000
*                                                                  0373 00124000
*   IF CLEANCOD^=ALLCLEAN           /* ANY ERROR SO FAR ?    @G24LB02*/ 00125000
*     THEN                          /* SOME ERROR, NO MSSC   @G24LB02*/ 00126000
         ICM   @15,15,CLEANCOD                                     0373 00127000
         BNZ   @RT00373                                            0373 00128000
*     GOTO NOMSSC;                  /* GO DIRECTLY OUT       @G24LB02*/ 00129000
*   ELSE                            /* CONTINUE WITH MSSC          0375 00130000
*                                      INITIALIZATION        @G24LB02*/ 00131000
*     ;                             /*                       @G24LB02*/ 00132000
*/********************************************************** @G24LB02*/ 00133000
*                                                                  0376 00134000
*   /*****************************************************************/ 00135000
*   /*                                                               */ 00136000
*   /* IF THIS ROUTINE IS CALLED BY THE MSSC MESSAGE         @G24LB02*/ 00137000
*   /* MODULE (CVTNIP=0) THEN CALL RDLABLS SUB TO            @G24LB02*/ 00138000
*   /* INITIALIZE VUCB(S).                                   @G24LB02*/ 00139000
*   /*                                                               */ 00140000
*   /*****************************************************************/ 00141000
*                                                                  0376 00142000
*   IF CVTNIP=OFF THEN              /* TEST FOR NIP IN CONTROL     0376 00143000
*                                                            @G24LB02*/ 00144000
         L     @01,CVTPTR                                          0376 00145000
         TM    CVTNIP(@01),B'00010000'                             0376 00146000
         BNZ   @RF00376                                            0376 00147000
*     DO;                           /* NIP NOT IN CONTROL CODE     0377 00148000
*                                                            @G24LB02*/ 00149000
*       IF PRGXQTD=OFF THEN                                        0378 00150000
         TM    PRGXQTD,B'00100000'                                 0378 00151000
         BNZ   @RF00378                                            0378 00152000
*         CALL RDLABLS;             /* VUCB INITIALIZATION   @G24LB02*/ 00153000
         BAL   @14,RDLABLS                                         0379 00154000
*       ELSE                                                       0380 00155000
*         CALL MAKNOTRD;            /* MARK ONLINE VUCBS NOTRDY    0380 00156000
*                                                            @G24LB02*/ 00157000
         B     @RC00378                                            0380 00158000
@RF00378 BAL   @14,MAKNOTRD                                        0380 00159000
*     END;                          /* END NOT NIP CODE      @G24LB02*/ 00160000
@RC00378 DS    0H                                                  0382 00161000
*/********************************************************** @G24LB02*/ 00162000
*                                                                  0382 00163000
*NOMSSC:                            /* CHECK IF NO MSSC      @G24LB02*/ 00164000
*   IF ICBI^=ON                     /* ANY MSSC OR MSC PROBLEM ?   0382 00165000
*                                                            @G24LB02*/ 00166000
*     THEN                                                         0382 00167000
@RF00376 DS    0H                                                  0382 00168000
NOMSSC   L     @01,CVTPTR                                          0382 00169000
         L     @01,CVTEXT2(,@01)                                   0382 00170000
         L     @15,CVTICB(,@01)                                    0382 00171000
         TM    ICBI(@15),B'10000000'                               0382 00172000
         BO    @RF00382                                            0382 00173000
*     DO;                           /* UNABLE TO USE MSSC OR 3850  0383 00174000
*                                                            @G24LB02*/ 00175000
*       ICBB=ON;                    /* SET BROKEN BOX FLAG   @G24LB02*/ 00176000
*       ICBMSCMP=OFF;               /* INDICATE NOT MP       @G24LB05*/ 00177000
*       ICBMSCA=ON;                 /* INDICATE POSSIBLE AP  @G24LB05*/ 00178000
         OI    ICBB(@15),B'00010010'                               0386 00179000
         NI    ICBMSCMP(@15),B'11111011'                           0386 00180000
*     END;                          /* END FAILURE PROCESSING      0387 00181000
*                                                            @G24LB02*/ 00182000
*   ELSE                                                           0388 00183000
*     ICBB=OFF;                     /* TURNOFF BROKEN BOX INDICATOR     00184000
*                                                            @G24LB02*/ 00185000
         B     @RC00382                                            0388 00186000
@RF00382 L     @01,CVTPTR                                          0388 00187000
         L     @01,CVTEXT2(,@01)                                   0388 00188000
         L     @01,CVTICB(,@01)                                    0388 00189000
         NI    ICBB(@01),B'11101111'                               0388 00190000
*                                                                  0389 00191000
*/********************************************************** @G24LB02*/ 00192000
*/*                                                          @G24LB02*/ 00193000
*/*   INTPRISC    SEGMENT OF ICBMSG05 WHICH CONTROLS THE     @G24LB02*/ 00194000
*/*            INITIALIZATION OF THE INTERFACE BETWEEN THE   @G24LB02*/ 00195000
*/*            3850 AND THE HOST CPU(S).                     @G24LB02*/ 00196000
*/*               DURING THIS PROCESS, THE MSC UCBS ARE      @G24LB02*/ 00197000
*/*               USED TO SEE WHICH IF ANY MSC(S) ARE ON     @G24LB02*/ 00198000
*/*               LINE, AND THE PRIMARY AND SECONDARY OR     @G24LB02*/ 00199000
*/*               ALTERNATE (IF ANY) MSC(S) ARE IDENTIFIED   @G24LB02*/ 00200000
*/*         NOTE THAT THE TERMS SECONDARY AND ALTERNATE      @G24LB02*/ 00201000
*/*         ARE USED INTERCHANGEABLY.                        @G24LB02*/ 00202000
*/*                                                          @G24LB02*/ 00203000
*/*  INPUT:        THE ADDRESS OR ADDRESSES OF MSC UCB(S) AS @G24LB02*/ 00204000
*/*          FOUND BY THE SEGMENT MSCUCBS                    @G24LB02*/ 00205000
*/*          THE NUMBER OF CPUS ON LINE IS IN THE COMMON     @G24LB02*/ 00206000
*/*          SYSTEM DATA AREA                                @G24LB02*/ 00207000
*/*                                                          @G24LB02*/ 00208000
*/*  PROCESSING:   ESTABLISHES COMMUNICATION WITH THE MSC    @G24LB02*/ 00209000
*/*                WHICH IS TO COMMUNICATE WITH THIS SYSTEM. @G24LB02*/ 00210000
*/*                FOR A MULTIPROCESSOR IPL, BOTH INTERFACES @G24LB02*/ 00211000
*/*                ARE INITIALIZED ON BEHALF OF EACH         @G24LB02*/ 00212000
*/*                HOST, BY JUST ONE INITIALIZE ORDER.       @G24LB02*/ 00213000
*/*                A SUBSEGMENT, INITLIZE, CONSTRUCTS AND    @G24LB02*/ 00214000
*/*                ISSUES THE ORDER                          @G24LB02*/ 00215000
*/*                                                          @G24LB02*/ 00216000
*/*   OUTPUT:      THE MSSC CONTROL BLOCK FIELDS FOR PRIMARY @G24LB02*/ 00217000
*/*                & SECONDARY MSC UCB ADDRESSES ARE FILLED  @G24LB02*/ 00218000
*/*                IN PROPERLY                               @G24LB02*/ 00219000
*/*                THE INITIALIZE BIT IS SET ON IN THE MSSC  @G24LB02*/ 00220000
*/*                CONTROL BLOCK                             @G24LB02*/ 00221000
*/*                ON ERROR, THE FAILURE CODE IS RETURNED TO @G24LB02*/ 00222000
*/*                CALLER, AND A CLEANUP CODE IS SET.        @G24LB02*/ 00223000
*/*                ICBI AND ICBB FLAGS SET TO NOTE IF MSS IS @G24LB02*/ 00224000
*/*                OPERATIVE OR INOPERATIVE.                 @G24LB02*/ 00225000
*/*                                                          @G24LB02*/ 00226000
*/*                                                          @G24LB02*/ 00227000
*/********************************************************** @G24LB02*/ 00228000
*                                                                  0389 00229000
*INTPRISC:                                                         0389 00230000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/* CONTROL OF       @G24LB02*/ 00231000
@EL00001 L     @13,4(,@13)                                         0389 00232000
@EF00001 DS    0H                                                  0389 00233000
@ER00001 LM    @14,@12,12(@13)                                     0389 00234000
         BR    @14                                                 0389 00235000
INTPRISC DS    0H                                                  0390 00236000
*/*  INITIALIZE ORDER AND PRIMARY & SECONDARY MSC            @G24LB02*/ 00237000
*/*  DETERMINATION                                           @G24LB02*/ 00238000
*                                                                  0390 00239000
*   DCL                                                            0390 00240000
*     IPSRET POINTER(31);           /* FOR RETURN ADDRESS    @G24LB02*/ 00241000
*   RFY                                                            0391 00242000
*     R14 RSTD;                     /* USE 14 FOR RETURN     @G24LB02*/ 00243000
*   IPSRET=R14;                     /* SAVE RETURN ADDRESS   @G24LB02*/ 00244000
         ST    R14,IPSRET                                          0392 00245000
*   RESPECIFY                                                      0393 00246000
*    (R14) UNRESTRICTED;            /* FROM QCVADS & PRIOR USE     0393 00247000
*                                                            @G24LB02*/ 00248000
*   NMESPTR=ADDR(NMSGST);           /* INITIALIZE MESSAGE PTR      0394 00249000
*                                                            @G24LB02*/ 00250000
         LA    @15,NMSGST                                          0394 00251000
         ST    @15,NMESPTR                                         0394 00252000
*   MSCUCBA1=ICBPUCB;               /* INITIALIZE A1 POINTER @G24LB02*/ 00253000
         L     @15,CVTPTR                                          0395 00254000
         L     @01,CVTEXT2(,@15)                                   0395 00255000
         L     @14,CVTICB(,@01)                                    0395 00256000
         L     @11,ICBPUCB(,@14)                                   0395 00257000
         ST    @11,MSCUCBA1                                        0395 00258000
*   MSCUCBA2=ICBAUCB;               /* INITIALIZE A2 POINTER @G24LB02*/ 00259000
         L     @10,ICBAUCB(,@14)                                   0396 00260000
         ST    @10,MSCUCBA2                                        0396 00261000
*   UCBPTR=MSCUCBA1;                /* READY FOR FIRST MSC UCB     0397 00262000
*                                                            @G24LB02*/ 00263000
         ST    @11,UCBPTR                                          0397 00264000
*   IF CVTNIP=OFF THEN                                             0398 00265000
         TM    CVTNIP(@15),B'00010000'                             0398 00266000
         BNZ   @RF00398                                            0398 00267000
*     DO;                                                          0399 00268000
*       ICBPUCB->UCBONLI=ON;        /* SET PRIM UCB ONLINE   @G24LB02*/ 00269000
         OI    UCBONLI(@11),B'10000000'                            0400 00270000
*       IF ICBAUCB^=LT2MSCS THEN                                   0401 00271000
         LTR   @10,@10                                             0401 00272000
         BZ    @RF00401                                            0401 00273000
*         ICBAUCB->UCBONLI=ON;      /* SET ALTERNATE UCB ONLINE    0402 00274000
*                                                            @G24LB02*/ 00275000
         OI    UCBONLI(@10),B'10000000'                            0402 00276000
*     END;                          /*                       @G24LB02*/ 00277000
@RF00401 DS    0H                                                  0404 00278000
*   IF MSCUCBA2^=LT2MSCS            /* ARE THERE TWO MSC UCBS ?    0404 00279000
*                                                            @G24LB02*/ 00280000
*     THEN                          /* THERE ARE TWO MSC UCBS HERE 0404 00281000
*                                                            @G24LB02*/ 00282000
@RF00398 ICM   @15,15,MSCUCBA2                                     0404 00283000
         BZ    @RF00404                                            0404 00284000
*     DO;                           /* YES, ANALYZE SITUATION MORE 0405 00285000
*                                                            @G24LB02*/ 00286000
*       CALL TWOMSCS;               /* ROUTINE HANDLES TWO MSC CASE     00287000
*                                                            @G24LB02*/ 00288000
         BAL   @14,TWOMSCS                                         0406 00289000
*     END;                          /* PATH FOR TWO MSCS     @G24LB02*/ 00290000
*   ELSE                            /* THERE IS BUT ONE MSC  @G24LB02*/ 00291000
*     DO;                           /* HANDLE THE LONE MSC   @G24LB02*/ 00292000
         B     @RC00404                                            0408 00293000
@RF00404 DS    0H                                                  0409 00294000
*       IF UCBONLI=ON               /* IS THAT MSC ON LINE ? @G24LB02*/ 00295000
*         THEN                      /* ONE MSC AND IT IS ON LINE   0409 00296000
*                                                            @G24LB02*/ 00297000
         L     @01,UCBPTR                                          0409 00298000
         TM    UCBONLI(@01),B'10000000'                            0409 00299000
         BNO   @RF00409                                            0409 00300000
*         DO;                       /* TRY TO USE THE SINGLE MSC   0410 00301000
*                                                            @G24LB02*/ 00302000
*           CALL INITLIZE;          /* ISSUE INITIALIZE TO IT      0411 00303000
*                                                            @G24LB02*/ 00304000
         BAL   @14,INITLIZE                                        0411 00305000
*           MSC1IRC=FAILCODE;       /* SAVE INITIALIZE RETURN      0412 00306000
*                                                            @G24LB02*/ 00307000
         L     @15,FAILCODE                                        0412 00308000
         ST    @15,MSC1IRC                                         0412 00309000
*           IF MSC1IRC=NLIRC THEN                                  0413 00310000
         SLR   @14,@14                                             0413 00311000
         IC    @14,NLIRC                                           0413 00312000
         CR    @15,@14                                             0413 00313000
         BNE   @RF00413                                            0413 00314000
*             CALL TRYLI;                                          0414 00315000
         BAL   @14,TRYLI                                           0414 00316000
*           IF FAILCODE=GOODIO      /* DID ALL GO WELL ?     @G24LB02*/ 00317000
*             THEN                  /* ALL WENT WELL         @G24LB02*/ 00318000
@RF00413 ICM   @15,15,FAILCODE                                     0415 00319000
         BNZ   @RF00415                                            0415 00320000
*             DO;                   /* WE HAVE INITIALIZED   @G24LB02*/ 00321000
*               ICBI=ON;            /* MARK MSSC CONTROL BLOCK     0417 00322000
*                                                            @G24LB02*/ 00323000
         L     @01,CVTPTR                                          0417 00324000
         L     @01,CVTEXT2(,@01)                                   0417 00325000
         L     @01,CVTICB(,@01)                                    0417 00326000
         OI    ICBI(@01),B'10000000'                               0417 00327000
*             END;                  /* AND THIS IS KNOWN AS PRIMARY     00328000
*                                                            @G24LB02*/ 00329000
*           ELSE                    /* THE ONE LONE MSC DOES NOT   0419 00330000
*                                      INITIALIZE            @G24LB02*/ 00331000
*             CLEANCOD=NOMSCNIT;    /* 1 MSC & IT WONT INITLZ      0419 00332000
*                                                            @G24LB02*/ 00333000
         B     @RC00415                                            0419 00334000
@RF00415 MVC   CLEANCOD(4),@CF02601                                0419 00335000
*         END;                                                     0420 00336000
*       ELSE                                                       0421 00337000
*         DO;                       /*                       @ZA16134*/ 00338000
         B     @RC00409                                            0421 00339000
@RF00409 DS    0H                                                  0422 00340000
*           CLEANCOD=NOMSCNIT;      /* MSC OFFLINE, NO INIT  @ZA16134*/ 00341000
         MVC   CLEANCOD(4),@CF02601                                0422 00342000
*           MSC1IRC='F1'X;          /* MSC OFFLINE REAS CODE @ZA16134*/ 00343000
         LA    @15,241                                             0423 00344000
         ST    @15,MSC1IRC                                         0423 00345000
*         END;                      /*                       @ZA16134*/ 00346000
*       IF CLEANCOD^=ALLCLEAN THEN  /*                       @ZA16134*/ 00347000
@RC00409 SLR   @15,@15                                             0425 00348000
         C     @15,CLEANCOD                                        0425 00349000
         BE    @RF00425                                            0425 00350000
*         DO;                       /*                       @ZA16134*/ 00351000
*           IF CVTNIP=ON THEN                                      0427 00352000
         L     @01,CVTPTR                                          0427 00353000
         TM    CVTNIP(@01),B'00010000'                             0427 00354000
         BNO   @RF00427                                            0427 00355000
*             DO;                   /*                       @G24LB02*/ 00356000
*               NMESNUM=INITMNUM;   /* NUMBER FOR INIT MESSAGE     0429 00357000
*                                                            @G24LB02*/ 00358000
         L     @14,NMESPTR                                         0429 00359000
         MVC   NMESNUM(3,@14),INITMNUM                             0429 00360000
*               NMESTEXT(1:LENGTH(INITTEXT))=INITTEXT;/*     @G24LB02*/ 00361000
         MVC   NMESTEXT(58,@14),INITTEXT                           0430 00362000
*               IF MSC1IRC^=GOODIO  /* WAS FAILURE ONLY ON 1ST MSC?     00363000
*                                                            @G24LB02*/ 00364000
*                 THEN              /* FAILURE ON 1ST MSC ONLY     0431 00365000
*                                                            @G24LB02*/ 00366000
         L     @11,MSC1IRC                                         0431 00367000
         CR    @11,@15                                             0431 00368000
         BE    @RF00431                                            0431 00369000
*                 DO;               /* PUT 1ST MSC UNIT INTO MSG   0432 00370000
*                                                            @G24LB02*/ 00371000
*                   NMESTEXT(42:44)=MSCUCBA1->UCBNAME;/*     @G24LB02*/ 00372000
         L     @15,MSCUCBA1                                        0433 00373000
         MVC   NMESTEXT+41(3,@14),UCBNAME(@15)                     0433 00374000
*                   PLACCONV=MSC1IRC;/* READY TO CONVERT CODE      0434 00375000
*                                                            @G24LB02*/ 00376000
         ST    @11,PLACCONV                                        0434 00377000
*                   RESPECIFY                                      0435 00378000
*                     R0 RESTRICTED;/* FOR CONVERSION        @G24LB02*/ 00379000
*                   CALL RCTOEBC;   /* CODE FROM HEX TO EBCDIC     0436 00380000
*                                                            @G24LB02*/ 00381000
         BAL   @14,RCTOEBC                                         0436 00382000
*                   RESPECIFY                                      0437 00383000
*                     R0 UNRESTRICTED;/* CONVERSION OVER     @G24LB02*/ 00384000
*                   NMESTEXT(56:58)=PLAC234;/* REASON CODE   @G24LB02*/ 00385000
         L     @01,NMESPTR                                         0438 00386000
         MVC   NMESTEXT+55(3,@01),PLAC234                          0438 00387000
*                   MSCUCBA1->UCBONLI=OFF;/* SET UCB OFFLINE @G24LB02*/ 00388000
         L     @01,MSCUCBA1                                        0439 00389000
         NI    UCBONLI(@01),B'01111111'                            0439 00390000
*/*   WRITE INFORMATIONAL MESSAGE TO THE SYSTEM OPERATOR     @G24LB02*/ 00391000
*                                                                  0440 00392000
*IIEAPMNB:                                                         0440 00393000
*                   GEN(IEAPMNIP TYPE=WTO,PARAM=NMSGH)/*     @G24LB02*/ 00394000
*                   REFS(NVTWTO,NMSGH);/* OPERATOR SHOWN ONE LINE  0440 00395000
*                                                            @G24LB02*/ 00396000
IIEAPMNB IEAPMNIP TYPE=WTO,PARAM=NMSGH                                  00397000
*                 END;              /* DID MSG FOR INITLZ FAILURE ON    00398000
*                                      1ST MSC ONLY          @G24LB02*/ 00399000
*               ELSE                                               0442 00400000
*                 ;                                                0442 00401000
@RF00431 DS    0H                                                  0443 00402000
*             END;                  /*                       @G24LB02*/ 00403000
*           ELSE                                                   0444 00404000
*             DO;                   /*                       @G24LB02*/ 00405000
         B     @RC00427                                            0444 00406000
@RF00427 DS    0H                                                  0445 00407000
*               IF MSC1IRC^=GOODIO THEN                            0445 00408000
         L     @15,MSC1IRC                                         0445 00409000
         LTR   @15,@15                                             0445 00410000
         BZ    @RF00445                                            0445 00411000
*                 DO;               /*                       @G24LB02*/ 00412000
*                   PLACCONV=MSC1IRC;/* REASON CODE FOR PUCB @G24LB02*/ 00413000
         ST    @15,PLACCONV                                        0447 00414000
*                   CALL RCTOEBC;   /* TRANSL REAS CODE      @G24LB02*/ 00415000
         BAL   @14,RCTOEBC                                         0448 00416000
*                   MSG321F(16:18)=MSCUCBA1->UCBNAME;/* PUT UCB IN 0449 00417000
*                                      MSG                   @G24LB02*/ 00418000
         L     @15,MSCUCBA1                                        0449 00419000
         MVC   MSG321F+15(3),UCBNAME(@15)                          0449 00420000
*                   MSCUCBA1->UCBONLI=OFF;/* SET UCB OFFLINE @G24LB02*/ 00421000
         NI    UCBONLI(@15),B'01111111'                            0450 00422000
*                   WTOGEN03=MSG321;/* LOAD MSG BUFFER W/321 MSG   0451 00423000
*                                                            @G24LB02*/ 00424000
         MVC   WTOGEN03(73),MSG321                                 0451 00425000
*                   WTOGEN03(54:56)=PLAC234;/* REAS CODE IN MSG    0452 00426000
*                                                            @G24LB02*/ 00427000
         MVC   WTOGEN03+53(3),PLAC234                              0452 00428000
*                   CALL ICBMSG45;  /* WRITE THE MESSAGE     @G24LB02*/ 00429000
         BAL   @14,ICBMSG45                                        0453 00430000
*                   WTOGEN03=MSG320;/* LOAD MSG BUFFER W/320 MSG   0454 00431000
*                                                            @G24LB02*/ 00432000
         MVI   WTOGEN03+51,C' '                                    0454 00433000
         MVC   WTOGEN03+52(21),WTOGEN03+51                         0454 00434000
