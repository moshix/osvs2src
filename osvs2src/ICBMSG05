@ER00010 BR    @14                                                 1448 04152000
*   END UCBSCAN;                    /*                       @G24LB02*/ 04153000
*RDLABLS:                                                          1450 04154000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @G24LB02*/ 04155000
RDLABLS  DS    0H                                                  1451 04156000
*/********************************************************** @G24LB02*/ 04157000
*/*                                                          @G24LB02*/ 04158000
*/* PROCEDURE NAME:  RDLABLS                                 @G24LB02*/ 04159000
*/*                                                          @G24LB02*/ 04160000
*/* INPUT:  NONE                                             @G24LB02*/ 04161000
*/*                                                          @G24LB02*/ 04162000
*/* PROCESS:  THIS ROUTINE DOES A                            @G24LB02*/ 04163000
*/*           GETMAIN FOR SUBPOOL 245 TO GET STORAGE IN      @G24LB02*/ 04164000
*/*           WHICH TO BUILD IOB,DCB,DEB,CCW'S AND A BUFFER. @G24LB02*/ 04165000
*/*           A CALL IS DONE TO UCBSCAN TO FIND 3330V UCB'S. @G24LB02*/ 04166000
*/*           ON RETURN FROM UCBSCAN THE DEB,ECB AND IOB ARE @G24LB02*/ 04167000
*/*           COMPLETED AND AN EXCP IS ISSUED TO READ THE    @G24LB02*/ 04168000
*/*           VOLUME LABEL.  IF SUCCESSFUL I/O COMPLETION    @G24LB02*/ 04169000
*/*           THEN THE UCBVOLI FIELD IS COMPLETED AND A CALL @G24LB02*/ 04170000
*/*           TO TTRCNVT IS DONE TO COMPLETE UCBVTOC.  NEXT  @G24LB02*/ 04171000
*/*           A CALL IS DONE TO DUBLABCK TO CHECK FOR        @G24LB02*/ 04172000
*/*           DUPLICATE VOLUME LABELS IN UCB'S.  THE UCB     @G24LB02*/ 04173000
*/*           POINTER IS INCREMENTED AND THE ECB IS ZEROED.  @G24LB02*/ 04174000
*/*           WHEN THE END OF THE LOOKUP TABLE IS REACHED,   @G24LB02*/ 04175000
*/*           GOTTEN STORAGE IS FREED.                       @G24LB02*/ 04176000
*/*                                                          @G24LB02*/ 04177000
*/* OUTPUT:  UCB'S WITH FIELDS UCBVOLI AND UCBVTOC FILLED IN @G24LB02*/ 04178000
*/*                                                          @G24LB02*/ 04179000
*/* MESSAGE: 'ICB983I VOLUME NOT CONSIDERED IN SYSTEM'WHICH  @G24LB02*/ 04180000
*/*           WILL ONLY APPEAR IF EXCP READ FAILED AND       @G24LB02*/ 04181000
*/*           MESSAGE WILL FOLLOW IEA000I MESSAGE.           @G24LB02*/ 04182000
*/*                                                          @G24LB02*/ 04183000
*/********************************************************** @G24LB02*/ 04184000
*                                                                  1451 04185000
*   RFY                                                            1451 04186000
*    (R14) RSTD;                    /*                       @X50AC01*/ 04187000
*   READRET=R14;                    /*                       @G24LB02*/ 04188000
         ST    R14,READRET                                         1452 04189000
*   RFY                                                            1453 04190000
*    (R14) UNRSTD;                  /*                       @X50AC01*/ 04191000
*   SCANEND=OFF;                    /*                       @G24LB02*/ 04192000
         NI    SCANEND,B'01111111'                                 1454 04193000
*   RFY                                                            1455 04194000
*    (R0,                                                          1455 04195000
*     R1,                                                          1455 04196000
*     R15) RSTD;                    /* RESTRICT FOR GETMAIN  @G24LB02*/ 04197000
*   R0=LENGTH(IOBSTDRD)+LENGTH(IHADCB)+LENGTH(DEBBASIC)+LENGTH(DEBDASD) 04198000
*       +LENGTH(DEB)+LENGTH(RDSNCCWS)+LENGTH(LABLBUFF);/*    @G24LB02*/ 04199000
         LA    R0,273                                              1456 04200000
*   R1=ZERO;                        /* CLEAR REG 1 FOR GETMAIN     1457 04201000
*                                                            @G24LB02*/ 04202000
         SLR   R1,R1                                               1457 04203000
*   R15=SP245;                      /* SET SUB POOL ID       @G24LB02*/ 04204000
         LA    R15,245                                             1458 04205000
*IGETMAI2:                          /* LABEL GETMAIN         @G24LB02*/ 04206000
*   GEN REFS(R0,R1,R15) SETS(R0,R1,R15);                           1459 04207000
IGETMAI2 DS    0H                                                  1459 04208000
*                                           /* GEN FOR GMAIN  @G24LB02* 04209000
