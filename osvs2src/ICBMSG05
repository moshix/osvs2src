*/********************************************************** @G24LB02*/ 04916000
*/*                                                          @G24LB02*/ 04917000
*/* PROCEDURE NAME:  TTRCNVT                                 @G24LB02*/ 04918000
*/*                                                          @G24LB02*/ 04919000
*/* INPUT:  POINTERS TO DEB AND READ LABELS BUFFER           @G24LB02*/ 04920000
*/*                                                          @G24LB02*/ 04921000
*/* PROCESS:  COMPLETES WORK AREA TTRFLD BY MOVING THE CCHHR @G24LB02*/ 04922000
*/*           FROM THE BUFFER INTO IT.CALLS RESIDENT TTR     @G24LB02*/ 04923000
*/*           CONVERT ROUTINE TO CONVERT THE VTOC CCHHR TO   @G24LB02*/ 04924000
*/*           A TTR                                          @G24LB02*/ 04925000
*/*                                                          @G24LB02*/ 04926000
*/* OUTPUT:  CONVERTED TTR IN FIELD CNVRTD                   @G24LB02*/ 04927000
*/*                                                          @G24LB02*/ 04928000
*/********************************************************** @G24LB02*/ 04929000
*                                                                  1630 04930000
*   /*****************************************************************/ 04931000
*   /*                                                               */ 04932000
*   /* ROUTINE TO CALL TTR CONVERT ROUTINE TO CONVERT        @G24LB02*/ 04933000
*   /* THE CCHHR FROM THE VOL 1 LABEL READY TO GO INTO THE FIELD OF  */ 04934000
*   /* THE UCB (UCBVTOC).                                    @G24LB02*/ 04935000
*   /*                                                               */ 04936000
*   /*****************************************************************/ 04937000
*                                                                  1630 04938000
*   DCL                                                            1630 04939000
*     1 TTRFLD,                     /*                       @G24LB02*/ 04940000
*      2 PRECCHHR CHAR(3),          /*                       @G24LB02*/ 04941000
*      2 LABCCHHR CHAR(5);          /*                       @G24LB02*/ 04942000
*   DCL                                                            1631 04943000
*     SAVR12 POINTER(31);           /*                       @G24LB02*/ 04944000
*   DCL                                                            1632 04945000
*     CNVTSAV CHAR(64) BDY(WORD);   /*                       @G24LB02*/ 04946000
*   STM(R0,R15,CNVTSAV);            /*                       @G24LB02*/ 04947000
         STM   R0,R15,CNVTSAV                                      1633 04948000
*   RFY                                                            1634 04949000
*     R1 RSTD;                      /*                       @G24LB02*/ 04950000
*   R1=DEBPTR2;                     /* DEB ADDR FOR USE BY CONVERT 1635 04951000
*                                      RTN                   @G24LB02*/ 04952000
         L     R1,DEBPTR2                                          1635 04953000
*   PRECCHHR=ZERO;                  /* ZERO THREE BYTES READY FOR  1636 04954000
*                                                            @G24LB02   04955000
*                                      PASSING FIELD TO TTR CONVERT.    04956000
*                                                            @G24LB02*/ 04957000
         SLR   @00,@00                                             1636 04958000
         STCM  @00,7,PRECCHHR                                      1636 04959000
*   LABCCHHR=LABVTOC;               /* PUT THE CCHHR TO CONVERT INTO    04960000
*                                                            @G24LB02   04961000
*                                      THE WORKAREA FOR TTR CONVERT     04962000
*                                                            @G24LB02*/ 04963000
         L     @15,BUFFPTR                                         1637 04964000
         MVC   LABCCHHR(5),LABVTOC(@15)                            1637 04965000
*   RFY                                                            1638 04966000
*    (R0,                                                          1638 04967000
*     R2,                                                          1638 04968000
*     R15) RSTD;                    /*                       @G24LB02*/ 04969000
*   R2=ADDR(TTRFLD);                /* MAKE REG 2 POINT TO WORKAREA     04970000
*                                                            @G24LB02*/ 04971000
         LA    R2,TTRFLD                                           1639 04972000
*   R15=CVTPRLTV;                   /* PICK UP ADDR OF TTR CONVERT 1640 04973000
*                                      RTN                   @G24LB02*/ 04974000
         L     @03,CVTPTR                                          1640 04975000
         L     R15,CVTPRLTV(,@03)                                  1640 04976000
*   SAVR12=R12;                     /*                       @G24LB02*/ 04977000
         ST    R12,SAVR12                                          1641 04978000
*   GEN(BALR 14,15 );               /* AND BRANCH TO IT.     @G24LB02*/ 04979000
         BALR 14,15                                                     04980000
*   CNVRTD=R0;                      /* PUT THE CONVERTED TTR INTO  1643 04981000
*                                      FIELD                 @G24LB02*/ 04982000
         ST    R0,CNVRTD                                           1643 04983000
*   R12=SAVR12;                     /*                       @G24LB02*/ 04984000
         L     R12,SAVR12                                          1644 04985000
*   RFY                                                            1645 04986000
*    (R0,                                                          1645 04987000
*     R1,                                                          1645 04988000
*     R2,                                                          1645 04989000
*     R15) UNRSTD;                  /*                       @G24LB02*/ 04990000
*   LM(R0,R15,CNVTSAV);             /* RESTORE CALLERS REGS  @G24LB02*/ 04991000
         LM    R0,R15,CNVTSAV                                      1646 04992000
*   RETURN;                         /* AND RETURN TO CALLER  @G24LB02*/ 04993000
@EL00012 DS    0H                                                  1647 04994000
@EF00012 DS    0H                                                  1647 04995000
@ER00012 BR    @14                                                 1647 04996000
*   END TTRCNVT;                    /*                       @G24LB02*/ 04997000
*DUPLABCK:                                                         1649 04998000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @G24LB02*/ 04999000
