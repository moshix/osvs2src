*           CALL INITLIZE;          /* ISSUE INITLIZE TO SECOND    0479 00543000
*                                                            @G24LB02*/ 00544000
         BAL   @14,INITLIZE                                        0479 00545000
*           MSC2IRC=FAILCODE;       /* SAVE REASON CODE      @G24LB02*/ 00546000
*                                                                  0480 00547000
         L     @15,FAILCODE                                        0480 00548000
         ST    @15,MSC2IRC                                         0480 00549000
*           /*********************************************************/ 00550000
*           /*                                                       */ 00551000
*           /* NOTE THAT ADDRESS OF SECOND IS NOW IN MSSC CONTROL    */ 00552000
*           /*                                               @G24LB02*/ 00553000
*           /* BLOCK AS THE PRIMARY MSC                      @G24LB02*/ 00554000
*           /*                                                       */ 00555000
*           /*********************************************************/ 00556000
*                                                                  0481 00557000
*           IF MSC2IRC=GOODIO       /* IS SECOND THE PRIMARY ?     0481 00558000
*                                                            @G24LB02*/ 00559000
*             THEN                  /* SECOND MSC IS THE PRIMARY   0481 00560000
*                                                            @G24LB02*/ 00561000
         LTR   @15,@15                                             0481 00562000
         BNZ   @RF00481                                            0481 00563000
