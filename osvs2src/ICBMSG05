         LR    R0,@11                                              0921 02299000
*       R1=LENGTH(DATACHAN);        /* MSSC IOSB EXTENSION AND     0922 02300000
*                                                            @G24LB02*/ 02301000
         LR    R1,@10                                              0922 02302000
*       R15=ZERO;                   /* CHANNEL PROGRAM       @G24LB02*/ 02303000
         SLR   R15,R15                                             0923 02304000
*       MVCL(R0,R14);               /*                       @G24LB02*/ 02305000
