*       RFY                                                        1463 04218000
*        (R0,                                                      1463 04219000
*         R1,                                                      1463 04220000
*         R15) UNRSTD;              /*                       @G24LB02*/ 04221000
*       DCBPTR2=IOBSTPT2+LENGTH(IOBSTDRD);/* DCB POINTER     @G24LB02*/ 04222000
