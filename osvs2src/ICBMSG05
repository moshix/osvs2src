*       /*                                                           */ 01241000
*       /*                                                   @G24LB05*/ 01242000
*       /*                                                           */ 01243000
*       /*************************************************************/ 01244000
*                                                                  0657 01245000
*     END;                          /* SPECIAL PATH FOR 1 CPU ON   0657 01246000
*                                      INITIALIZE            @G24LB05*/ 01247000
*   ELSE                                                           0658 01248000
*     ;                             /*                       @G24LB05*/ 01249000
@RF00648 DS    0H                                                  0659 01250000
*   IF R15^=GOODIO                  /* CHECK REASON CODE     @G24LB02*/ 01251000
*     THEN                          /* REASON CODE WAS NOT ZERO    0659 01252000
*                                                            @G24LB02*/ 01253000
         LTR   R15,R15                                             0659 01254000
         BZ    @RF00659                                            0659 01255000
*     DO;                           /* CHECK FOR ANOTHER PERMISSIBLE    01256000
