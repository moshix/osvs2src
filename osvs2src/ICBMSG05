*                   CALL ICBMSG45;  /* WRITE THE MESSAGE     @G24LB02*/ 00436000
         BAL   @14,ICBMSG45                                        0455 00437000
*                 END;              /* PROCESSING IF 1 MSC ON LINE 0456 00438000
*                                                            @G24LB02*/ 00439000
*             END;                  /*                       @G24LB02*/ 00440000
@RF00445 DS    0H                                                  0458 00441000
*         END;                      /*                       @G24LB02*/ 00442000
@RC00427 DS    0H                                                  0459 00443000
*     END;                          /*                       @G24LB02*/ 00444000
@RF00425 DS    0H                                                  0460 00445000
*   RFY                                                            0460 00446000
*     R14 RSTD;                     /* RESTRICT 14 FOR RETURN USE  0460 00447000
*                                                            @G24LB02*/ 00448000
@RC00404 DS    0H                                                  0461 00449000
*   R14=IPSRET;                     /* RESTORE RETURN ADDRESS      0461 00450000
*                                                            @G24LB02*/ 00451000
         L     R14,IPSRET                                          0461 00452000
*   RFY                                                            0462 00453000
*     R14 UNRSTD;                   /* FREE 14 FOR BRANCH BACK     0462 00454000
*                                                            @G24LB02*/ 00455000
*   END INTPRISC;                   /* HAVE INITIALIZED IF POSSIBLE     00456000
*                                                            @G24LB02*/ 00457000
@EL00002 DS    0H                                                  0463 00458000
@EF00002 DS    0H                                                  0463 00459000
@ER00002 BR    @14                                                 0463 00460000
*                                                                  0464 00461000
*/********************************************************** @G24LB02*/ 00462000
*/*                                                          @G24LB02*/ 00463000
*/*    TWOMSCS:  HANDLE CASE OF TWO MSC UCBS                 @G24LB02*/ 00464000
*/*                                                          @G24LB02*/ 00465000
*/*  INPUT: ADDRESS OF TWO MSC UCBS AS FOUND BY THE SEGMENT  @G24LB02*/ 00466000
*/*         MSCUCBS                                          @G24LB02*/ 00467000
*/*         INDICATION IN EACH MSC UCB WHETHER THAT MSC IS   @G24LB02*/ 00468000
*/*         ON LINE                                          @G24LB02*/ 00469000
*/*                                                          @G24LB02*/ 00470000
*/* PROCESSING: DETERMINE WHICH OF THESE IS THE PRIMARY MSC, @G24LB02*/ 00471000
*/*        ENSURE THAT THE INITIALIZE ORDER IS ISSUED TO IT  @G24LB02*/ 00472000
*/*       INFORM THE SYSTEM OPERATOR IF EITHER OF THESE TWO  @G24LB02*/ 00473000
*/*       MSC UCBS IS MARKED OFF LINE OR IF EITHER HAS A BAD @G24LB02*/ 00474000
*/*       REASON CODE FROM INITIALIZE                        @G24LB02*/ 00475000
*/*       UPON SUCCESSFUL INITIALIZATION, SET THE            @G24LB02*/ 00476000
*/*       INITIALIZED BIT IN THE MSSC CONTROL BLOCK          @G24LB02*/ 00477000
*/*                                                          @G24LB02*/ 00478000
*/*  OUTPUT: THE INITIALIZED BIT AND THE POINTERS TO THE     @G24LB02*/ 00479000
*/*         PRIMARY AND ALTERNATE OR SECONDARY (IF ANY) MSC  @G24LB02*/ 00480000
*/*         UCBS ARE SET IN THE MSSC CONTROL BLOCK           @G24LB02*/ 00481000
*/*       ON ERROR, THE REASON CODE FROM THE INITIALIZE IS   @G24LB02*/ 00482000
*/*          PASSED TO THE CALLER                            @G24LB02*/ 00483000
*/*                                                          @G24LB02*/ 00484000
*/* MESSAGES: IEA976I, IEA973I & ICB321I ARE WRITTEN BY      @G24LB02*/ 00485000
*/* TWOMSCS                                                  @G24LB02*/ 00486000
*/*                                                          @G24LB02*/ 00487000
*/********************************************************** @G24LB02*/ 00488000
*                                                                  0464 00489000
*TWOMSCS:                                                          0464 00490000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB02*/ 00491000
TWOMSCS  DS    0H                                                  0465 00492000
*   DCL                                                            0465 00493000
*     TWORET POINTER(31);           /* FOR RETURN ADDRESS    @G24LB02*/ 00494000
*   NMESPTR=ADDR(NMSGST);           /* SETUP MESSAGE PTR     @G24LB02*/ 00495000
         LA    @15,NMSGST                                          0466 00496000
         ST    @15,NMESPTR                                         0466 00497000
*/* NOTE--MUST PRESERVE REASON CODES FROM BAD INITIALIZE(S), @G24LB02*/ 00498000
*/*  FOR  EVENTUAL INCLUSION IN THE MESSAGE IEA977I          @G24LB02*/ 00499000
*                                                                  0467 00500000
*   GEN DATA REFS(ONEMSCF,ONEMSCU);                                0467 00501000
*   GEN DATA REFS(NITF,NITFMSC,NITFRC);                            0468 00502000
*                                                                  0468 00503000
*/*  NOTE THAT INTPRISC SETS UCBPTR TO ADDR OF FIRST MSC UCB @G24LB02*/ 00504000
*/*  FOUND  BEFORE CALLING TWOMSCS                           @G24LB02*/ 00505000
*                                                                  0469 00506000
*   RFY                                                            0469 00507000
*     R14 RSTD;                     /* TO SAVE RETURN ADDRESS      0469 00508000
*                                                            @G24LB02*/ 00509000
*   TWORET=R14;                     /* SAVE RETURN ADDRESS   @G24LB02*/ 00510000
         ST    R14,TWORET                                          0470 00511000
*   RFY                                                            0471 00512000
*     R14 UNRSTD;                   /* FREE UP REGISTER 14   @G24LB02*/ 00513000
*   IF UCBONLI=ON                   /* IS FIRST MSC ON LINE ?      0472 00514000
*                                                            @G24LB02*/ 00515000
*     THEN                          /* THE FIRST MSC FOUND IS ON LINE   00516000
*                                                            @G24LB02*/ 00517000
         L     @01,UCBPTR                                          0472 00518000
         TM    UCBONLI(@01),B'10000000'                            0472 00519000
         BNO   @RF00472                                            0472 00520000
*     DO;                           /* WILL TRY IT FIRST FOR PRIMARY    00521000
*                                                            @G24LB02*/ 00522000
*/*     THE FIRST MSC IS ON LINE                             @G24LB02*/ 00523000
*                                                                  0474 00524000
*       IF MSCUCBA2->UCBONLI=ON     /* IS SECOND MSC UCB ON LINE ? 0474 00525000
*                                                            @G24LB02*/ 00526000
*         THEN                      /* BOTH MSCS ARE ON LINE @G24LB02*/ 00527000
         L     @01,MSCUCBA2                                        0474 00528000
         TM    UCBONLI(@01),B'10000000'                            0474 00529000
         BNO   @RF00474                                            0474 00530000
*         DO;                       /* BOTH MSCS ARE ON LINE @G24LB02*/ 00531000
