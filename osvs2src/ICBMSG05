*     R14 RSTD;                     /*                       @G24LB02*/ 05296000
*   R14=SAVPR14;                    /* RESTORE REG 14        @G24LB02*/ 05297000
         L     R14,SAVPR14                                         1720 05298000
*   RFY                                                            1721 05299000
*     R14 UNRSTD;                   /*                       @G24LB02*/ 05300000
*   RETURN;                         /*                       @G24LB02*/ 05301000
@EL00015 DS    0H                                                  1722 05302000
@EF00015 DS    0H                                                  1722 05303000
@ER00015 BR    @14                                                 1722 05304000
*   END VATLIST;                    /*                       @G24LB02*/ 05305000
*                                                                  1724 05306000
*/********************************************************************/ 05307000
*/*                                                          @G24LB02*/ 05308000
*/* PROCEDURE NAME: WAITRTN                                  @G24LB02*/ 05309000
*/*                                                          @G24LB02*/ 05310000
*/* INPUT: NONE                                              @G24LB02*/ 05311000
*/*                                                          @G24LB02*/ 05312000
*/* PROCESS: THIS ROUTINE ISSUES A MULTIPLE WAIT ON FOUR     @G24LB02*/ 05313000
*/*          ECBS. THE FOUR ECBS ARE:                        @G24LB02*/ 05314000
*/*              ICBECB: THIS ECB IS POSTED WHEN IO COMPLETE @G24LB02*/ 05315000
*/*              ICBACTV - THIS ECB IS USED DURING 3330V     @G24LB02*/ 05316000
*/*                READ LABEL I/O OPERATIONS FOR TIMED IO    @G24LB02*/ 05317000
*/*              ICBCOM - COMMUNICATIONS TASK ECB. COMMANDS  @G24LB02*/ 05318000
*/*                WILL NOT BE PROCESSED UNTIL FULL MESSAGE  @G24LB02*/ 05319000
*/*                TASK OPERATION IS POSSIBLE, SO A SWITCH   @G24LB02*/ 05320000
*/*                WILL BE SET TO INDICATE THE ECB HAS BEEN  @G24LB02*/ 05321000
*/*                POSTED.                                   @G24LB02*/ 05322000
*/*              ICBIOR - NOT USED THIS MODULE               @G24LB02*/ 05323000
*/*                                                          @G24LB02*/ 05324000
*/* OUTPUT: IF ICBACTV WAS POSTED FOR READ LABEL I/O,THE ECB @G24LB02*/ 05325000
*/*          WILL NOT BE CLEARED, BUT WILL BE LEFT FOR       @G24LB02*/ 05326000
*/*          INTERROGATION BY THE READLABL ROUTINE           @G24LB02*/ 05327000
*/*                                                          @G24LB02*/ 05328000
*/********************************************************************/ 05329000
*                                                                  1724 05330000
*WAITRTN:                                                          1724 05331000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @G24LB02*/ 05332000
WAITRTN  DS    0H                                                  1725 05333000
*   RFY                                                            1725 05334000
*    (R14) RSTD;                    /*                       @G24LB02*/ 05335000
*   WAITSV14=R14;                   /*                       @G24LB02*/ 05336000
         ST    R14,WAITSV14                                        1726 05337000
*   RFY                                                            1727 05338000
*    (R14) UNRSTD;                  /*                       @G24LB02*/ 05339000
*                                                                  1727 05340000
*   /*****************************************************************/ 05341000
*   /*                                                               */ 05342000
*   /* ISSUE WAIT ON THE LIST OF FOUR ECBS                   @G24LB02*/ 05343000
*   /*                                                               */ 05344000
*   /*****************************************************************/ 05345000
*                                                                  1728 05346000
*   GEN(WAIT ECBLIST=WAITLIST);     /*                       @G24LB02*/ 05347000
*                                                                  1728 05348000
         WAIT ECBLIST=WAITLIST                                          05349000
*   /*****************************************************************/ 05350000
*   /*                                                               */ 05351000
*   /* TEST TO SEE WHICH ECB WAS POSTED AND TAKE APPROPRIATE @G24LB02*/ 05352000
*   /* ACTION                                                @G24LB02*/ 05353000
*   /*                                                               */ 05354000
*   /*****************************************************************/ 05355000
*                                                                  1729 05356000
*   IF ECB1->BITPATT(2)=ON THEN     /* WAS ICBECB POSTED?    @G24LB02*/ 05357000
         L     @01,ECB1                                            1729 05358000
         TM    BITPATT(@01),B'01000000'                            1729 05359000
         BNO   @RF01729                                            1729 05360000
*     DO;                           /* YES, IO POSTED        @G24LB02*/ 05361000
*       ACTPOST=ON;                 /* SET SWITCH FOR RDLABLS      1731 05362000
*                                                            @G24LB02*/ 05363000
         OI    ACTPOST,B'00100000'                                 1731 05364000
*     END;                          /*                       @G24LB02*/ 05365000
*   IF ECB2->BITPATT(2)=ON THEN     /* WAS ICBACTV POSTED?   @G24LB02*/ 05366000
@RF01729 L     @01,ECB2                                            1733 05367000
         TM    BITPATT(@01),B'01000000'                            1733 05368000
         BNO   @RF01733                                            1733 05369000
*     DO;                           /*                       @G24LB02*/ 05370000
*       ACTPOST=ON;                 /* SET SWITCH FOR RDLABLS      1735 05371000
*                                                            @G24LB02*/ 05372000
*       TMEPOST=ON;                 /* SET TIME END SWITCH   @G24LB02*/ 05373000
         OI    ACTPOST,B'00100100'                                 1736 05374000
*     END;                          /*                       @G24LB02*/ 05375000
*   IF ECB3->BITPATT(2)=ON THEN     /* WAS ICBCOM POSTED?    @G24LB02*/ 05376000
@RF01733 L     @01,ECB3                                            1738 05377000
         TM    BITPATT(@01),B'01000000'                            1738 05378000
         BNO   @RF01738                                            1738 05379000
*     DO;                           /* YES, COMMAND POST     @G24LB02*/ 05380000
*       ICBCOM=ZERO;                /* CLEAR THE ECB         @G24LB02*/ 05381000
         L     @01,CVTPTR                                          1740 05382000
         L     @01,CVTEXT2(,@01)                                   1740 05383000
         L     @01,CVTICB(,@01)                                    1740 05384000
         SLR   @00,@00                                             1740 05385000
         ST    @00,ICBCOM(,@01)                                    1740 05386000
*     END;                          /*                       @G24LB02*/ 05387000
*   IF ECB4->BITPATT(2)=ON THEN     /* WAS ICBIOR POSTED?    @G24LB02*/ 05388000
@RF01738 ICM   @01,7,ECB4                                          1742 05389000
         TM    BITPATT(@01),B'01000000'                            1742 05390000
         BNO   @RF01742                                            1742 05391000
*     DO;                           /* YES                   @G24LB02*/ 05392000
*       ICBIOR=ZERO;                /* CLEAR ECB             @G24LB02*/ 05393000
         L     @01,CVTPTR                                          1744 05394000
         L     @01,CVTEXT2(,@01)                                   1744 05395000
         L     @01,CVTICB(,@01)                                    1744 05396000
         SLR   @00,@00                                             1744 05397000
         ST    @00,ICBIOR(,@01)                                    1744 05398000
*     END;                          /*                       @G24LB02*/ 05399000
*   RFY                                                            1746 05400000
*    (R14) RSTD;                    /*                       @G24LB02*/ 05401000
@RF01742 DS    0H                                                  1747 05402000
*   R14=WAITSV14;                   /* RESTORE REG 14        @G24LB02*/ 05403000
         L     R14,WAITSV14                                        1747 05404000
*   RFY                                                            1748 05405000
*    (R14) UNRSTD;                  /*                       @G24LB02*/ 05406000
*   RETURN;                         /*                       @G24LB02*/ 05407000
@EL00016 DS    0H                                                  1749 05408000
@EF00016 DS    0H                                                  1749 05409000
@ER00016 BR    @14                                                 1749 05410000
*   END WAITRTN;                    /*                       @G24LB02*/ 05411000
*BLDBLOCK:                                                         1751 05412000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @G24LB02*/ 05413000
BLDBLOCK DS    0H                                                  1752 05414000
*/********************************************************** @G24LB02*/ 05415000
*/*                                                          @G24LB02*/ 05416000
*/* PROCEDURE NAME:  BLDBLOCK                                @G24LB02*/ 05417000
*/*                                                          @G24LB02*/ 05418000
*/* INPUT:  POINTERS TO I/O CONTROL BLOCKS IN SP245          @G24LB02*/ 05419000
*/*                                                          @G24LB02*/ 05420000
*/* PROCESS:  COMPLETES NECESSARY FIELDS IN IOB DCB AND DEB  @G24LB02*/ 05421000
*/*           FOR EXCP TO READ VOL LABELS ALSO BUILDS CCW'S  @G24LB02*/ 05422000
*/*           IN SP245 FOR EXCP.                             @G24LB02*/ 05423000
*/*                                                          @G24LB02*/ 05424000
*/* OUTPUT:  PARTIALLY COMPLETED I/O CONTROL BLOCKS AND      @G24LB02*/ 05425000
*/*          COMPLETED CCW'S.                                @G24LB02*/ 05426000
*/*                                                          @G24LB02*/ 05427000
*/********************************************************** @G24LB02*/ 05428000
*                                                                  1752 05429000
*   DCL                                                            1752 05430000
*     BLDRET PTR(31);               /*                       @G24LB02*/ 05431000
*   RFY                                                            1753 05432000
*    (R14) RSTD;                    /*                       @G24LB02*/ 05433000
*   BLDRET=R14;                     /*                       @G24LB02*/ 05434000
         ST    R14,BLDRET                                          1754 05435000
*   RFY                                                            1755 05436000
*    (R14) UNRSTD;                  /*                       @G24LB02*/ 05437000
*   IOBSTDRD=''B;                   /* ZERO IOB FIELD        @G24LB02*/ 05438000
         L     @15,IOBSTPT2                                        1756 05439000
         XC    IOBSTDRD(32,@15),IOBSTDRD(@15)                      1756 05440000
*   IHADCB=''B;                     /* ZERO DCB FIELD        @G24LB02*/ 05441000
         L     @14,DCBPTR2                                         1757 05442000
         XC    IHADCB(52,@14),IHADCB(@14)                          1757 05443000
*   DEBBASIC=''B;                   /* ZERO DEB FIELD        @G24LB02*/ 05444000
         L     @11,DEBPTR2                                         1758 05445000
         XC    DEBBASIC(32,@11),DEBBASIC(@11)                      1758 05446000
*   DEB=''B;                        /* ZERO DEB APPENDAGE VECTOR   1759 05447000
*                                      TABLE                 @G24LB02*/ 05448000
         L     @10,DEBAPPA2                                        1759 05449000
         XC    DEB(36,@10),DEB(@10)                                1759 05450000
*   DEBDASD=''B;                    /* ZERO DASD PORTION OF DEB    1760 05451000
*                                                            @G24LB02*/ 05452000
         L     @09,DEBDAPT2                                        1760 05453000
         XC    DEBDASD(16,@09),DEBDASD(@09)                        1760 05454000
*   IOBECBPB=ADDR(ICBECB);          /* SET UP ECB FOR I/O    @G24LB02*/ 05455000
         L     @01,CVTPTR                                          1761 05456000
         L     @01,CVTEXT2(,@01)                                   1761 05457000
         L     @08,CVTICB(,@01)                                    1761 05458000
         LA    @07,ICBECB(,@08)                                    1761 05459000
         STCM  @07,7,IOBECBPB(@15)                                 1761 05460000
*   IOBDCBPB=DCBPTR2;               /* LOAD DCB ADDRESS INTO IOB   1762 05461000
*                                                            @G24LB02*/ 05462000
         STCM  @14,7,IOBDCBPB(@15)                                 1762 05463000
*   IOBSEEK='0000000000000003'X;    /* SEEK ADDRESS FOR      @G24LB02   05464000
*                                      VOL1 LABEL            @G24LB02*/ 05465000
         MVC   IOBSEEK(8,@15),@CB04144                             1763 05466000
*   DCBDEBAD=DEBPTR2;               /* DEB ADDRESS INTO DCB  @G24LB02*/ 05467000
         ST    @11,DCBDEBAD+40(,@14)                               1764 05468000
*   DEBPROTG='0F'X;                 /* PROTECT KEY INTO DEB  @G24LB02*/ 05469000
         MVI   DEBPROTG(@11),X'0F'                                 1765 05470000
*   DEBDCBB=DCBPTR2;                /* DCB ADDRESS INTO DEB  @G24LB02*/ 05471000
         STCM  @14,7,DEBDCBB(@11)                                  1766 05472000
*   DEBAPPB=DEBAPPA2;               /* APPENDAGE VECTOR TABLE ADDR 1767 05473000
*                                                            @G24LB02*/ 05474000
         STCM  @10,7,DEBAPPB(@11)                                  1767 05475000
*   DEBTCBAD=ICBTCB;                /* SET UP TCB POINTER IN DEB   1768 05476000
*                                                            @G24LB02*/ 05477000
         MVC   DEBTCBAD(4,@11),ICBTCB(@08)                         1768 05478000
*   DEBENDCC='7FFF'X;               /* ENDING CYL OF EXTENT  @G24LB02*/ 05479000
         MVC   DEBENDCC(2,@09),@CB04146                            1769 05480000
*   DEBENDHH='7FFF'X;               /* RD/WR TRK FOR END EXTENT    1770 05481000
*                                                            @G24LB02*/ 05482000
         MVC   DEBENDHH(2,@09),@CB04146                            1770 05483000
*   DEBNMTRK='7FFF'X;               /* NO OF TRK IN EXTENT   @G24LB02*/ 05484000
         MVC   DEBNMTRK(2,@09),@CB04146                            1771 05485000
*   DEBEOEAD=ADDR(ICBDUMAA);        /* RETURN TO EXCP        @G24LB02*/ 05486000
         LA    @11,ICBDUMAA                                        1772 05487000
         STCM  @11,7,DEBEOEAD(@10)                                 1772 05488000
*   DEBSIOAD=ADDR(ICBDUMAA);        /* RETURN TO EXCP        @G24LB02*/ 05489000
         STCM  @11,7,DEBSIOAD(@10)                                 1773 05490000
*   DEBPCIAD=ADDR(ICBDUMAA);        /* RETURN TO EXCP        @G24LB02*/ 05491000
         STCM  @11,7,DEBPCIAD(@10)                                 1774 05492000
*   DEBCEAD=ADDR(ICBDUMAA);         /* RETURN TO EXCP        @G24LB02*/ 05493000
         STCM  @11,7,DEBCEAD(@10)                                  1775 05494000
*   DEBXCEAD=ADDR(ICBDUMAA);        /* RETURN TO EXCP        @G24LB02*/ 05495000
         STCM  @11,7,DEBXCEAD(@10)                                 1776 05496000
*   SRCHCODE='31'X;                 /* SEARCH ID EQ          @G24LB02*/ 05497000
         L     @14,RDCCWPTR                                        1777 05498000
         MVI   SRCHCODE(@14),X'31'                                 1777 05499000
*   SRCHDATA=ADDR(IOBSEEK)+3;       /* ADDRESS OF RECORD 3   @G24LB02*/ 05500000
         LA    @15,IOBSEEK(,@15)                                   1778 05501000
         AH    @15,@CH00127                                        1778 05502000
         STCM  @15,7,SRCHDATA(@14)                                 1778 05503000
*   SCHFLAGS='40'X;                 /* COMMAND CHAIN FLAG    @G24LB02*/ 05504000
         MVI   SCHFLAGS(@14),X'40'                                 1779 05505000
*   SCHCOUNT='000005'X;             /* LENGTH OF SEARCH DATA @G24LB02*/ 05506000
         MVC   SCHCOUNT(3,@14),@CF04147+1                          1780 05507000
*   TICCODE='08'X;                  /* TRANSFER IN CHANNEL CODE    1781 05508000
*                                                            @G24LB02*/ 05509000
         MVI   TICCODE(@14),X'08'                                  1781 05510000
*   TICDATA=ADDR(SRCHCCWS);         /* ADDR OF SEARCH CCW    @G24LB02*/ 05511000
         STCM  @14,7,TICDATA(@14)                                  1782 05512000
*   TICFLAGS='40'X;                 /* COMMAND CHAIN TIC     @G24LB02*/ 05513000
         MVI   TICFLAGS(@14),X'40'                                 1783 05514000
*   TICCOUNT='000001'X;             /* LENGTH                @G24LB02*/ 05515000
         MVC   TICCOUNT(3,@14),@CF04148+1                          1784 05516000
*   READCODE='06'X;                 /* READ DATA CCW CODE    @G24LB02*/ 05517000
         MVI   READCODE(@14),X'06'                                 1785 05518000
*   READDATA=BUFFPTR;               /* LABEL READ AREA       @G24LB02*/ 05519000
         MVC   READDATA(3,@14),BUFFPTR+1                           1786 05520000
*   RDFLAGS='00'X;                  /* NO CHAIN AND WE WANT TO     1787 05521000
*                                                            @G24LB02   05522000
*                                      KNOW ABOUT WRONG LENGTH.    1787 05523000
*                                                            @G24LB02*/ 05524000
