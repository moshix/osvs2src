*           ELSE                    /* NONZERO RETURN FROM ASSOCIATE    03362000
*                                                            @G24LB02*/ 03363000
*             DO;                   /* MSS INOPERATIVE       @G24LB02*/ 03364000
*               FAILCODE=R0;        /* FATAL TO 3850 USE     @G24LB02*/ 03365000
         ST    R0,FAILCODE                                         1234 03366000
*               CLEANCOD=ASOCFAIL;  /* TRACK                 @G24LB02*/ 03367000
