*/*    PROCEDURE NAME - REDRIVIO                             @G24LB61*/ 00298000
*/*                                                          @G24LB61*/ 00299000
*/*    FUNCTION- THIS PROCEDURE SIMULATES CHANNEL-AVAILABLE  @G24LB61*/ 00300000
*/*              INTERRUPTS TO ALL CHANNELS WHICH HAD AT     @G24LB61*/ 00301000
*/*              LEAST ONE DEVICE WITH A MISSING ATTENTION   @G24LB61*/ 00302000
*/*              INTERRUPT, THUS CAUSING THE I/O REQUESTS    @G24LB61*/ 00303000
*/*              THAT WERE WAITING FOR ATTENTIONS TO BE      @G24LB61*/ 00304000
*/*              REDRIVEN.                                   @G24LB61*/ 00305000
*/*                                                          @G24LB61*/ 00306000
*/*    INPUT - (1) CHANNEL MAP CONTAINING UCB ADDRESSES      @G24LB61*/ 00307000
*/*                                                          @G24LB61*/ 00308000
*/*    OUTPUT- (1) SIMULATED CHANNEL-AVAILABLE INTERRUPTS    @G24LB61*/ 00309000
*/*            (2) REINITIALIZED CHANNEL MAP                 @G24LB61*/ 00310000
*/*                                                          @G24LB61*/ 00311000
*/*********************************************************  @G24LB61*/ 00312000
*                                                                  0097 00313000
*REDRIVIO:                                                         0097 00314000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,RETREG);/*           @G24LB61*/ 00315000
REDRIVIO DS    0H                                                  0098 00316000
*   SAVE4=RETRNREG;                 /* SAVE RETURN ADDRESS   @G24LB61*/ 00317000
*                                                                  0098 00318000
         LR    SAVE4,RETRNREG                                      0098 00319000
*   /*****************************************************************/ 00320000
*   /*                                                               */ 00321000
*   /* CHECK EACH CHANNEL MAP ENTRY FOR A (NON-ZERO) UNIT    @G24LB61*/ 00322000
*   /* ADDRESS, INDICATING AN ASSOCIATED CHANNEL NEEDING A   @G24LB61*/ 00323000
*   /* SIMULATED CHANNEL-AVAILABLE INTERRUPT.                @G24LB61*/ 00324000
*   /*                                                               */ 00325000
*   /*****************************************************************/ 00326000
*                                                                  0099 00327000
*   DO INDEX1=1 TO MAXCHNL;         /*                       @G24LB61*/ 00328000
         LA    INDEX1,1                                            0099 00329000
@DL00099 DS    0H                                                  0100 00330000
*     IF CHNLMAP(INDEX1)^=0 THEN                                   0100 00331000
         LR    @15,INDEX1                                          0100 00332000
         SLA   @15,2                                               0100 00333000
         L     @15,CHNLMAP-4(@15)                                  0100 00334000
         LTR   @15,@15                                             0100 00335000
         BZ    @RF00100                                            0100 00336000
*       DO;                         /*                       @G24LB61*/ 00337000
*         UCBPTR=CHNLMAP(INDEX1);   /*                       @G24LB61*/ 00338000
*                                                                  0102 00339000
         LR    UCBPTR,@15                                          0102 00340000
*         /***********************************************************/ 00341000
*         /*                                                         */ 00342000
*         /* SIMULATE THE CHANNEL-AVAILABLE INTERRUPT VIA    @G24LB61*/ 00343000
*         /* THE IOSINTRP MACRO                              @ZA20263*/ 00344000
*         /*                                                         */ 00345000
*         /***********************************************************/ 00346000
*                                                                  0103 00347000
*SIMINT:                                                           0103 00348000
*         WORKREG1=R13;             /* SAVE CALLERS SAVE AREA PTR  0103 00349000
*                                                            @ZA20263*/ 00350000
SIMINT   LR    WORKREG1,R13                                        0103 00351000
*         R13=ADDR(REGSAV);         /* SAVE AREA FOR IOSINTRP      0104 00352000
*                                                            @ZA20263*/ 00353000
         LA    R13,REGSAV                                          0104 00354000
*         GEN REFS(UCBPTR,FLCIOPSW,FLCIOAA,FLCCSW,FLCINPSW,PSA,R13);    00355000
 IOSINTRP   UCB=UCBPTR,VAR=2,BRANCH=YES                     @ZA20263    00356000
*         R13=WORKREG1;             /* RESTORE PTR TO CALLERS SAVE 0106 00357000
*                                      AREA                  @ZA20263*/ 00358000
         LR    R13,WORKREG1                                        0106 00359000
*       END;                        /*                       @G24LB61*/ 00360000
*   END;                            /*                       @G24LB61*/ 00361000
*                                                                  0108 00362000
@RF00100 AL    INDEX1,@CF00049                                     0108 00363000
         C     INDEX1,@CF00354                                     0108 00364000
         BNH   @DL00099                                            0108 00365000
*   /*****************************************************************/ 00366000
*   /*                                                               */ 00367000
*   /* REINITIALIZE THE CHANNEL MAP TO ZEROS.                @G24LB61*/ 00368000
*   /*                                                               */ 00369000
