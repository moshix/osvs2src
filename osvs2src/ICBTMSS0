*                                                                  0083 00208000
         LR    SAVE2,RETRNREG                                      0083 00209000
*   /*****************************************************************/ 00210000
*   /*                                                               */ 00211000
*   /* PERFORM VALIDITY CHECKING ON EACH OF THE INPUT RATIO  @G24LB61*/ 00212000
*   /* VALUES TO INSURE THEY ARE IN THE RANGE X'F0F1' TO     @G24LB61*/ 00213000
*   /* X'F9F9', AND CONVERT THE VALUES FROM EBCDIC TO BINARY @G24LB61*/ 00214000
*   /*                                                               */ 00215000
*   /*****************************************************************/ 00216000
*                                                                  0084 00217000
*   RATIOPTR=ADDR(MSSRVAL1);        /*                       @G24LB61*/ 00218000
         LA    RATIOPTR,MSSRVAL1(,PARMPTR)                         0084 00219000
*   LIMITPTR=ADDR(LIMIT1);          /*                       @G24LB61*/ 00220000
         LA    LIMITPTR,LIMIT1                                     0085 00221000
*   DO INDEX1=1 TO RATIOTOT;        /*                       @G24LB61*/ 00222000
         LA    INDEX1,1                                            0086 00223000
@DL00086 DS    0H                                                  0087 00224000
*     DO INDEX2=1 TO LENGTH(RATIO) WHILE(RATIO(INDEX2)>='0'&RATIO( 0087 00225000
*           INDEX2)<='9');          /*                       @G24LB61*/ 00226000
