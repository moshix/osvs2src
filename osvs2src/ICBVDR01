         B     @EL00003                                            0300 00370000
*     END;                          /*                       @Y30LB24*/ 00371000
*   IF REPLY(1:3)='BAC' THEN        /* BACKUP?               @Y30LB24*/ 00372000
@RF00295 CLC   REPLY(3),@CC02419                                   0302 00373000
         BNE   @RF00302                                            0302 00374000
*     DO;                           /* YES                   @Y30LB24*/ 00375000
*       BACKUP=ON;                  /* SET BACKUP FLAG       @Y30LB24*/ 00376000
         OI    BACKUP,B'10000000'                                  0304 00377000
*       IF REPLY(4:4)=BLANK THEN    /* NO MORE PARMS         @Y30LB24*/ 00378000
         CLC   REPLY+3(1),BLANK                                    0305 00379000
         BNE   @RF00305                                            0305 00380000
