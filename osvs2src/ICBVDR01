         LM    @00,@12,20(@13)                                     0264 00214000
         BR    @14                                                 0264 00215000
*                                                                  0265 00216000
*/* ******************************************************************  00217000
*                                                                  0265 00218000
*     NAME = VDRCWTOR,  WRITE TO OPERATOR FOR PARMS.           @Y30LB24 00219000
*                                                                  0265 00220000
*     INPUT = NONE.                                            @Y30LB24 00221000
*                                                                  0265 00222000
*     PROCESS = THIS SUBROUTINE IS CALLED WHEN PARMS HAVE NOT      0265 00223000
*     BEEN SPECIFIED ON THE EXEC JCL OR THE PARMS SPECIFIED        0265 00224000
*     ARE IN ERROR.                                            @Y30LB24 00225000
*                                                                  0265 00226000
*     OUTPUT = PARMS ARE READ VIA A WTOR COMMAND.              @Y30LB24 00227000
*                                                                  0265 00228000
******************************************************************** */ 00229000
*/*VDRCWTOR: CHART                                                   */ 00230000
*/*HEADER                                                          0265 00231000
*/*JULY 19, 1974                                ICBVDR01 - SEGMENT 0265 00232000
*/*VDRCWTOR - WTOR FOR PARMS                                         */ 00233000
*/*          E                    VDRCWTOR                           */ 00234000
*/*          P                    CLEAR REPLY AREA                   */ 00235000
*/*          P                    CLEAR ECB                          */ 00236000
*/*          L                    ISSUE WTOR                         */ 00237000
*/*%END:     R                    RETURN TO CALLER                   */ 00238000
*/*VDRCWTOR: END                                                     */ 00239000
*                                                                  0265 00240000
*VDRCWTOR:                                                         0265 00241000
*   PROCEDURE OPTIONS(SAVE(14));                                   0265 00242000
@EL00001 L     @13,4(,@13)                                         0265 00243000
@EF00001 DS    0H                                                  0265 00244000
@ER00001 LM    @14,@12,12(@13)                                     0265 00245000
         BR    @14                                                 0265 00246000
VDRCWTOR ST    @14,@SA00002                                        0265 00247000
*   REPLY='';                       /* BLANK WORK AREA       @Y30LB24*/ 00248000
         MVI   REPLY,C' '                                          0266 00249000
         MVC   REPLY+1(99),REPLY                                   0266 00250000
*   WTORECB=ZERO;                   /* CLEAR THE WTOR ECB    @Y30LB24*/ 00251000
         SLR   @14,@14                                             0267 00252000
         ST    @14,WTORECB                                         0267 00253000
*   REG2=ADDR(REPLY);               /* SET REPLY ADDR        @Y30LB24*/ 00254000
         LA    REG2,REPLY                                          0268 00255000
*   REG3=ADDR(WTORECB);             /* SET ECB ADDRESS       @Y30LB24*/ 00256000
         LA    REG3,WTORECB                                        0269 00257000
*LWTOR1:                                                           0270 00258000
*   GEN REFS(REG2,REG3);                                           0270 00259000
LWTOR1   DS    0H                                                  0270 00260000
*                                         /* WTOR FOR PARMS   @Y30LB24* 00261000
         WTOR 'ICB436D ICBVDR01 PARMS REPLY (RECOVER,BACKUP,PRINT)',   *00262000
               (REG2),N7,(REG3)                                         00263000
         WAIT ECB=WTORECB                                               00264000
*   RETURN;                         /* RETURN TO CALLER      @Y30LB24*/ 00265000
@EL00002 DS    0H                                                  0271 00266000
@EF00002 DS    0H                                                  0271 00267000
@ER00002 L     @14,@SA00002                                        0271 00268000
         BR    @14                                                 0271 00269000
*   END VDRCWTOR;                   /* END WTOR PARMS        @Y30LB24*/ 00270000
*                                                                  0273 00271000
*/* ******************************************************************  00272000
*                                                                  0273 00273000
*     NAME = VDRFTYPE,  DETERMINE TYPE OF JOURNAL UTILITY REQUESTED.    00274000
*                                                              @Y30LB24 00275000
*                                                                  0273 00276000
*     INPUT = JOURNAL UTILITY PARAMETERS FROM JCL OR A WTOR.   @Y30LB24 00277000
*                                                                  0273 00278000
*     PROCESS = THIS SUBROUTINE IS CALLED WHEN THE TYPE OF         0273 00279000
*     JOURNAL UTILITY MUST BE DETERMINED FOR ICBVDR01.         @Y30LB24 00280000
*                                                                  0273 00281000
*     OUTPUT = JOURNAL UTILITY TYPE FLAG AND/OR WORK FLAG TO INDICATE   00282000
*     THE SUCCESS OF THE SUBROUTINE.                           @Y30LB24 00283000
*                                                                  0273 00284000
******************************************************************** */ 00285000
*/*VDRFTYPE: CHART                                                   */ 00286000
*/*HEADER                                                          0273 00287000
*/*JULY 19, 1974                                ICBVDR01 - SEGMENT 0273 00288000
*/*VDRFTYPE - DETERMINE JOURNAL UTILITY TYPE                         */ 00289000
*/*          E                    VDRFTYPE                           */ 00290000
*/*          M                    INITIALIZE VARIABLES               */ 00291000
*/*          P                    SET PARMS UPPER CASE               */ 00292000
*/*          D (YES,,NO,%BACK)    RECOVERY REQUESTED?                */ 00293000
*/*          D (YES,,NO,%ERR)     PARMS VALID?                       */ 00294000
*/*          M (,%END)            SET RECOVERY REQUEST FLAG          */ 00295000
*/*%BACK:    D (YES,,NO,%PRT)     BACKUP REQUESTED?                  */ 00296000
*/*          D (YES,,NO,%ERR)     PARMS VALID?                       */ 00297000
*/*          M (,%END)            SET BACKUP REQUEST FLAG            */ 00298000
*/*%PRT:     D (YES,,NO,%ERR)     PRINT REQUESTED?                   */ 00299000
*/*          D (YES,,NO,%ERR)     PARMS VALID?                       */ 00300000
*/*          M (,%END)            SET PRINT REQUEST FLAG             */ 00301000
*/*%ERR:     M                    SET INVALID PARM CODE              */ 00302000
*/*%END:     R                    RETURN TO CALLER                   */ 00303000
*/*VDRFTYPE: END                                                     */ 00304000
*                                                                  0273 00305000
*VDRFTYPE:                                                         0273 00306000
*   PROCEDURE OPTIONS(SAVE(14));                                   0273 00307000
VDRFTYPE ST    @14,12(,@13)                                        0273 00308000
*   BACKUP=OFF;                     /* INITIALIZE SWITCH     @Y30LB24*/ 00309000
         NI    BACKUP,B'01111111'                                  0274 00310000
*   PRINT=OFF;                      /* INITIALIZE SWITCH     @Y30LB24*/ 00311000
         NI    PRINT,B'01111111'                                   0275 00312000
*   RECOVERY=OFF;                   /* INITIALIZE SWITCH     @Y30LB24*/ 00313000
         NI    RECOVERY,B'01111111'                                0276 00314000
*   DO I=1 TO 7;                    /* UPPER CASE LOOP       @Y30LB24*/ 00315000
         LA    I,1                                                 0277 00316000
@DL00277 DS    0H                                                  0278 00317000
*     CAPS(I)=CAPS(I)|BLANK;        /* SET UPPER CASE        @Y30LB24*/ 00318000
         LA    @14,CAPS-1(I)                                       0278 00319000
         OC    0(1,@14),BLANK                                      0278 00320000
*   END;                            /*                       @Y30LB24*/ 00321000
         AL    I,@CF00069                                          0279 00322000
         C     I,@CF00305                                          0279 00323000
         BNH   @DL00277                                            0279 00324000
*   FAILCODE=PARMERR;               /* INIT REASON CODE      @Y30LB24*/ 00325000
         LA    FAILCODE,3                                          0280 00326000
*   IF REPLY(1:7)='RECOVER' THEN    /* RECOVERY?             @Y30LB24*/ 00327000
         CLC   REPLY(7),@CC02416                                   0281 00328000
         BNE   @RF00281                                            0281 00329000
*     DO;                           /* YES                   @Y30LB24*/ 00330000
*       RECOVERY=ON;                /* SET RECOVER FLAG      @Y30LB24*/ 00331000
         OI    RECOVERY,B'10000000'                                0283 00332000
*       IF REPLY(8:8)=BLANK THEN    /* NO MORE PARMS         @Y30LB24*/ 00333000
         CLC   REPLY+7(1),BLANK                                    0284 00334000
         BNE   @RF00284                                            0284 00335000
*         FAILCODE=ZERO;            /* SET VALID PARMS       @Y30LB24*/ 00336000
         SLR   FAILCODE,FAILCODE                                   0285 00337000
*       RETURN;                     /* RETURN TO CALLER      @Y30LB24*/ 00338000
@EL00003 DS    0H                                                  0286 00339000
@EF00003 DS    0H                                                  0286 00340000
@ER00003 L     @14,12(,@13)                                        0286 00341000
         BR    @14                                                 0286 00342000
*     END;                          /*                       @Y30LB24*/ 00343000
*   IF REPLY(1:3)='REC' THEN        /* RECOVERY?             @Y30LB24*/ 00344000
@RF00281 CLC   REPLY(3),@CC02417                                   0288 00345000
         BNE   @RF00288                                            0288 00346000
*     DO;                           /* YES                   @Y30LB24*/ 00347000
*       RECOVERY=ON;                /* SET RECOVER FLAG      @Y30LB24*/ 00348000
         OI    RECOVERY,B'10000000'                                0290 00349000
*       IF REPLY(4:4)=BLANK THEN    /* NO MORE PARMS         @Y30LB24*/ 00350000
         CLC   REPLY+3(1),BLANK                                    0291 00351000
         BNE   @RF00291                                            0291 00352000
*         FAILCODE=ZERO;            /* SET VALID PARMS       @Y30LB24*/ 00353000
         SLR   FAILCODE,FAILCODE                                   0292 00354000
*       RETURN;                     /* RETURN TO CALLER      @Y30LB24*/ 00355000
         B     @EL00003                                            0293 00356000
*     END;                          /*                       @Y30LB24*/ 00357000
*   IF REPLY(1:6)='BACKUP' THEN     /* BACKUP?               @Y30LB24*/ 00358000
@RF00288 CLC   REPLY(6),@CC02418                                   0295 00359000
         BNE   @RF00295                                            0295 00360000
*     DO;                           /* YES                   @Y30LB24*/ 00361000
*       BACKUP=ON;                  /* SET BACKUP FLAG       @Y30LB24*/ 00362000
         OI    BACKUP,B'10000000'                                  0297 00363000
*       IF REPLY(7:7)=BLANK THEN    /* NO MORE PARMS         @Y30LB24*/ 00364000
         CLC   REPLY+6(1),BLANK                                    0298 00365000
