*   IF REPLY(1:5)='PRINT' THEN      /* PRINT?                @Y30LB24*/ 00386000
@RF00302 CLC   REPLY(5),@CC02420                                   0309 00387000
         BNE   @RF00309                                            0309 00388000
*     DO;                           /* YES                   @Y30LB24*/ 00389000
*       PRINT=ON;                   /* SET PRINT FLAG        @Y30LB24*/ 00390000
         OI    PRINT,B'10000000'                                   0311 00391000
*       IF REPLY(6:6)=BLANK THEN    /* NO MORE PARMS         @Y30LB24*/ 00392000
         CLC   REPLY+5(1),BLANK                                    0312 00393000
