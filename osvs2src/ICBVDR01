         TITLE 'ICBVDR01 MSVC RECOVERY/JOURNAL UTILITY V1.0            *00001000
                        '                                               00002000
ICBVDR01 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ICBVDR01  78.157'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  LA    @11,4095(,@12)                                      0001 00012000
         USING @PSTART,@12                                         0001 00013000
         USING @PSTART+4095,@11                                    0001 00014000
         ST    @13,@SA00001+4                                      0001 00015000
         LA    @14,@SA00001                                        0001 00016000
         ST    @14,8(,@13)                                         0001 00017000
         LR    @13,@14                                             0001 00018000
*                                                                  0198 00019000
*   /*****************************************************************/ 00020000
*   /*                                                               */ 00021000
*   /* SET UP ADDRESSABILITY AND INITIALIZE VARIABLES        @Y30LB24*/ 00022000
*   /*                                                               */ 00023000
*   /*****************************************************************/ 00024000
*                                                                  0198 00025000
*   PARMPTR=LISTPTR;                /* SET EXEC PARM PTR     @Y30LB24*/ 00026000
         L     PARMPTR,LISTPTR(,REG1)                              0198 00027000
*   VVIPTR=ICBVVICB;                /* INITIALIZE MSVC CB PTR      0199 00028000
*                                                            @Y30LB24*/ 00029000
         L     @14,CVTPTR                                          0199 00030000
         L     @14,CVTEXT2(,@14)                                   0199 00031000
         L     @14,CVTICB(,@14)                                    0199 00032000
         L     @09,ICBVVICB(,@14)                                  0199 00033000
         ST    @09,VVIPTR                                          0199 00034000
*   OPENSW=OFF;                     /* INITIALIZE SWITCH     @Y30LB24*/ 00035000
         NI    OPENSW,B'01111111'                                  0200 00036000
*   LINECNT=NEWPAGE;                /* FORCE FIRST PAGE      @Y30LB24*/ 00037000
         MVC   LINECNT(4),@CF00130                                 0201 00038000
*   PAGECNT=ZERO;                   /* INITIALIZE PAGE NO    @Y30LB24*/ 00039000
         SLR   @06,@06                                             0202 00040000
         ST    @06,PAGECNT                                         0202 00041000
*   REQCNT=ZERO;                    /* INITIALIZE REQUEST NO @Y30LB24*/ 00042000
         SLR   REQCNT,REQCNT                                       0203 00043000
*   MSGCNT=ZERO;                    /* INITIALIZE MESSAGE CNT      0204 00044000
*                                                            @ZM31134*/ 00045000
         ST    @06,MSGCNT                                          0204 00046000
*   FAILCODE=RCON0;                 /* INITIALIZE FAIL CODE  @Y30LB24*/ 00047000
         SLR   FAILCODE,FAILCODE                                   0205 00048000
*   TOTFCODE=RCON0;                 /* INITIALIZE FAIL CODE  @Y30LB24*/ 00049000
         ST    @06,TOTFCODE                                        0206 00050000
*   RSAVE=ZERO;                     /* INIT REASON CODE      @Y30LB24*/ 00051000
         ST    @06,RSAVE                                           0207 00052000
*   RETSAVE=ZERO;                   /* INIT RETURN CODE      @Y30LB24*/ 00053000
         ST    @06,RETSAVE                                         0208 00054000
*                                                                  0209 00055000
*   /*****************************************************************/ 00056000
*   /*                                                               */ 00057000
*   /* CHECK TO SEE IF MSVC IS GENERATED IN SYSTEM           @Y30LB24*/ 00058000
*   /*                                                               */ 00059000
*   /*****************************************************************/ 00060000
*                                                                  0209 00061000
*   IF CVTICB=ZERO|                 /* MSSC CB PTR ZERO?     @Y30LB24*/ 00062000
*       ICBVVICB=ZERO THEN          /* MSVC CB PTR ZERO?     @Y30LB24*/ 00063000
         CR    @14,@06                                             0209 00064000
         BE    @RT00209                                            0209 00065000
         CR    @09,@06                                             0209 00066000
         BNE   @RF00209                                            0209 00067000
@RT00209 DS    0H                                                  0210 00068000
*     DO;                           /* YES                   @Y30LB24*/ 00069000
*       FAILCODE=NOCB;              /* SET FAIL CODE         @Y30LB24*/ 00070000
         LA    FAILCODE,1                                          0211 00071000
*       CALL VDRNOTOP;              /* CALL MESSAGE ROUTINE  @Y30LB24*/ 00072000
         BAL   @14,VDRNOTOP                                        0212 00073000
*       RETSAVE=FOUR;               /* SET RETURN CODE       @Y30LB24*/ 00074000
         LA    @14,4                                               0213 00075000
         ST    @14,RETSAVE                                         0213 00076000
*       RETURN CODE(RETSAVE);       /* RETURN TO INITIATOR   @Y30LB24*/ 00077000
         LR    @15,@14                                             0214 00078000
         L     @13,4(,@13)                                         0214 00079000
         L     @14,12(,@13)                                        0214 00080000
         LM    @00,@12,20(@13)                                     0214 00081000
         BR    @14                                                 0214 00082000
*     END;                          /*                       @Y30LB24*/ 00083000
*                                                                  0215 00084000
*   /*****************************************************************/ 00085000
*   /*                                                               */ 00086000
*   /* DETERMINE ICBVDR01 PARAMETERS                         @Y30LB24*/ 00087000
*   /*                                                               */ 00088000
*   /*****************************************************************/ 00089000
*                                                                  0216 00090000
*   IF PLENGTH^=ZERO THEN           /* EXEC PARM FIELD?      @Y30LB24*/ 00091000
@RF00209 LH    @14,PLENGTH(,PARMPTR)                               0216 00092000
         LTR   @14,@14                                             0216 00093000
         BZ    @RF00216                                            0216 00094000
*     DO;                           /* YES                   @Y30LB24*/ 00095000
*       REPLY='';                   /* BLANK WORK AREA       @Y30LB24*/ 00096000
         MVI   REPLY,C' '                                          0218 00097000
         MVC   REPLY+1(99),REPLY                                   0218 00098000
*       REPLY=PARMS(1:PLENGTH);     /* MOVE EXEC PARMS       @Y30LB24*/ 00099000
         MVI   REPLY+1,C' '                                        0219 00100000
         MVC   REPLY+2(98),REPLY+1                                 0219 00101000
         BCTR  @14,0                                               0219 00102000
         EX    @14,@SM02853                                        0219 00103000
*     END;                          /*                       @Y30LB24*/ 00104000
*   ELSE                                                           0221 00105000
*     DO;                           /* NO PARM FIELD         @Y30LB24*/ 00106000
         B     @RC00216                                            0221 00107000
@RF00216 DS    0H                                                  0222 00108000
*       CALL VDRCWTOR;              /* CALL WTOR ROUTINE     @Y30LB24*/ 00109000
         BAL   @14,VDRCWTOR                                        0222 00110000
*     END;                          /*                       @Y30LB24*/ 00111000
*   CALL VDRFTYPE;                  /* CALL TYPE ROUTINE     @Y30LB24*/ 00112000
@RC00216 BAL   @14,VDRFTYPE                                        0224 00113000
*   DO WHILE FAILCODE=PARMERR;      /* GOOD REPLY?           @Y30LB24*/ 00114000
         B     @DE00225                                            0225 00115000
@DL00225 DS    0H                                                  0226 00116000
*     CALL VDRNOTOP;                /* CALL MESSAGE ROUTINE  @Y30LB24*/ 00117000
         BAL   @14,VDRNOTOP                                        0226 00118000
*     CALL VDRCWTOR;                /* CALL WTOR ROUTINE     @Y30LB24*/ 00119000
         BAL   @14,VDRCWTOR                                        0227 00120000
*     CALL VDRFTYPE;                /* CALL TYPE ROUTINE     @Y30LB24*/ 00121000
         BAL   @14,VDRFTYPE                                        0228 00122000
*   END;                            /*                       @Y30LB24*/ 00123000
*                                                                  0229 00124000
@DE00225 C     FAILCODE,@CF00125                                   0229 00125000
         BE    @DL00225                                            0229 00126000
*   /*****************************************************************/ 00127000
*   /*                                                               */ 00128000
*   /* EXECUTE THE FUNCTIONS REQUESTED VIA PARAMETERS        @Y30LB24*/ 00129000
*   /*                                                               */ 00130000
*   /*****************************************************************/ 00131000
*                                                                  0230 00132000
*   IF RECOVERY=ON THEN             /* RECOVERY REQUEST      @Y30LB24*/ 00133000
         TM    RECOVERY,B'10000000'                                0230 00134000
         BNO   @RF00230                                            0230 00135000
*     DO;                           /* YES                   @Y30LB24*/ 00136000
*       CALL VDRRECVY;              /* CALL RECOVER ROUTINE  @Y30LB24*/ 00137000
         BAL   @14,VDRRECVY                                        0232 00138000
*       PRINT=ON;                   /* SET PRINT FLAG        @Y30LB24*/ 00139000
         OI    PRINT,B'10000000'                                   0233 00140000
*     END;                          /*                       @Y30LB24*/ 00141000
*   IF BACKUP=ON THEN               /* BACKUP REQUEST        @Y30LB24*/ 00142000
@RF00230 TM    BACKUP,B'10000000'                                  0235 00143000
         BNO   @RF00235                                            0235 00144000
*     DO;                           /* YES                   @Y30LB24*/ 00145000
*       CALL VDRBACUP;              /* CALL BACKUP ROUTINE   @Y30LB24*/ 00146000
         BAL   @14,VDRBACUP                                        0237 00147000
*       CALL VDRPRINT;              /* CALL PRINT ROUTINE    @ZM31134*/ 00148000
         BAL   @14,VDRPRINT                                        0238 00149000
*     END;                          /*                       @Y30LB24*/ 00150000
*   IF PRINT=ON&                    /* PRINT REQUEST &       @Y30LB24*/ 00151000
*       FAILCODE=ZERO THEN          /* NO OTHER FAILURE      @Y30LB24*/ 00152000
@RF00235 TM    PRINT,B'10000000'                                   0240 00153000
         BNO   @RF00240                                            0240 00154000
         LTR   FAILCODE,FAILCODE                                   0240 00155000
         BNZ   @RF00240                                            0240 00156000
*     DO;                           /* YES                   @Y30LB24*/ 00157000
*       CALL VDRPRINT;              /* CALL PRINT ROUTINE    @Y30LB24*/ 00158000
         BAL   @14,VDRPRINT                                        0242 00159000
*     END;                          /*                       @Y30LB24*/ 00160000
*                                                                  0243 00161000
*   /*****************************************************************/ 00162000
*   /*                                                               */ 00163000
*   /* CLEANUP AND ISSUE ERROR MESSAGE IF FAILURE            @Y30LB24*/ 00164000
*   /*                                                               */ 00165000
*   /*****************************************************************/ 00166000
*                                                                  0244 00167000
*   IF OPENSW=ON THEN               /* DATA SETS OPENED      @Y30LB24*/ 00168000
@RF00240 TM    OPENSW,B'10000000'                                  0244 00169000
         BNO   @RF00244                                            0244 00170000
*     DO;                           /* YES                   @Y30LB24*/ 00171000
