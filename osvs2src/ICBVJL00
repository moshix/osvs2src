*   CALL VJLIO;                     /* READ CONTROL RECORD   @ZM30746*/ 01183000
         BAL   @14,VJLIO                                           0535 01184000
*   READSW=OFF;                     /* TURN READ OFF         @ZM30746*/ 01185000
*   CONTSW=OFF;                     /* SET CONTROL SW OFF    @ZM30746*/ 01186000
         NI    READSW,B'01101111'                                  0537 01187000
*   IF RPLVRETC=ZERO                /* READ SUCCESSFUL       @ZM30746*/ 01188000
*     THEN                                                         0538 01189000
