*       CALL VJLWTO;                /* ISSUE THE ERROR MSG   @Y30LB27*/ 00121000
         BAL   @14,VJLWTO                                          0244 00122000
*       VJLMSGID=MSGLSTE;           /* SET MSG COMPLETION INDEX    0245 00123000
*                                                            @Y30LB27*/ 00124000
         MVC   VJLMSGID(4),@CF00032                                0245 00125000
*       CALL VJLWTO;                /* ISSUE THE ERROR MSG   @Y30LB27*/ 00126000
         BAL   @14,VJLWTO                                          0246 00127000
*       RETURN;                     /* RETURN TO CALLER      @Y30LB27*/ 00128000
         B     @EL00001                                            0247 00129000
*     END;                          /* END DCB NOT OPEN      @Y30LB27   00130000
*                                                            @Y30LB27*/ 00131000
*/********************************************************************  00132000
** SET READ FOR THE CONTROL RECORD                           @Y30LB27   00133000
******************************************************************** */ 00134000
*                                                                  0249 00135000
*   CONTAREA=CONTPTR;               /* POINT CONTROL RECORD BUFFER 0249 00136000
*                                                            #Y30LB27*/ 00137000
@RF00239 L     @10,CONTPTR                                         0249 00138000
         ST    @10,CONTAREA(,@10)                                  0249 00139000
*   REQPTR=ADDR(CONTTTR);           /* REQUEST CONTROL RECORD      0250 00140000
*                                                            #Y30LB27*/ 00141000
         LA    REQPTR,CONTTTR(,@10)                                0250 00142000
*   CONTSW=ON;                      /* INDICATE THAT I/O FOR CONTROL    00143000
*                                      RECORD                @Y30LB27*/ 00144000
*   READSW=ON;                      /* READ THE RECORD       @Y30LB27*/ 00145000
         OI    CONTSW,B'10010000'                                  0252 00146000
*   CALL VJLIO;                     /* CALL I/O SEGMENT      @Y30LB27*/ 00147000
