         LA    @08,13                                              0264 00198000
         ALR   @08,@09                                             0264 00199000
         ST    @08,MSGLEN                                          0264 00200000
*       MSG(ONE:RPLVRLN)=MSGBUF(ONE:RPLVRLN);/* SET THE LENGTH       */ 00201000
         BCTR  @09,0                                               0265 00202000
         L     @10,RPLVBUF(,@10)                                   0265 00203000
         EX    @09,@SM02368                                        0265 00204000
*       GEN(STIDP SYSCPU);          /* GET CPUID             @ZA16130*/ 00205000
         STIDP SYSCPU                                                   00206000
*       MSGCPUID=CPUIDNUM;          /* STORE CPUID           @ZA16130*/ 00207000
         MVC   MSGCPUID(5),CPUIDNUM                                0267 00208000
*       GEN(TIME DEC);              /* GET TIME              @ZA16130*/ 00209000
         TIME DEC                                                       00210000
*       MSGTME=REG0;                /* STORE TOD             @ZA16130*/ 00211000
         ST    REG0,MSGTME                                         0269 00212000
*       MOVELEN=MSGLEN;             /* SET THE MSG LENGTH    @Y30LB27*/ 00213000
         L     @10,MSGLEN                                          0270 00214000
         ST    @10,MOVELEN                                         0270 00215000
*       ORDRPTR=ADDR(MSGLST);       /* SET THE REQUEST PTR TO THE MSG   00216000
*                                      REQUEST LIST          @Y30LB27*/ 00217000
         LA    @10,MSGLST                                          0271 00218000
         ST    @10,ORDRPTR                                         0271 00219000
*       REQPTR=ADDR(MSGTTR);        /* SET I/O REQUEST FOR MSG BLOCKS   00220000
*                                                            @Y30LB27*/ 00221000
         L     REQPTR,CONTPTR                                      0272 00222000
         LA    REQPTR,MSGTTR(,REQPTR)                              0272 00223000
*     END;                          /*                       @Y30LB27   00224000
*                                                            @Y30LB27*/ 00225000
*/********************************************************************  00226000
** NO JOURNALING ORDER PARAMETER LIST                        @Y30LB27   00227000
******************************************************************** */ 00228000
*                                                                  0274 00229000
*   ELSE                                                           0274 00230000
*     DO;                           /*                       @Y30LB27*/ 00231000
         B     @RC00261                                            0274 00232000
@RF00261 DS    0H                                                  0275 00233000
*       ORDRSW=ON;                  /* INDICATE I/O FOR DATA RECORDS    00234000
*                                                            @Y30LB27*/ 00235000
