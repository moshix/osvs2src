*                                                            @Y30LB27   00180000
*                                                            @Y30LB27*/ 00181000
         ST    @09,MSGAREA(,@06)                                   0260 00182000
*/********************************************************************  00183000
** JOURNALING LIBRARIAN MESSAGE - BUILD INTERNAL ORDER RPL   @Y30LB27   00184000
******************************************************************** */ 00185000
*                                                                  0261 00186000
*   IF RPLVVOP=ON THEN                                             0261 00187000
         TM    RPLVVOP(@10),B'00000100'                            0261 00188000
         BNO   @RF00261                                            0261 00189000
*     DO;                           /* JOURNALLING A MSG     @Y30LB27*/ 00190000
*       MSGSW=ON;                   /* INDICATE I/O FOR MSG RECORDS     00191000
*                                                            @Y30LB27*/ 00192000
         OI    MSGSW,B'00000100'                                   0263 00193000
*       MSGLEN=RPLVRLN+THIRTEN;     /* CONSTRUCT INTERNAL REQUEST  0264 00194000
*                                      LIST FOR JOURNALING MSGS    0264 00195000
*                                                            @ZA16130*/ 00196000
