*                                      REQD                  @Y30LB27*/ 01131000
         OI    WRITESW,B'01100000'                                 0523 01132000
*       REQPTR=ADDR(CONTTTR);       /* POINT TO CONTROL RECORD TTR 0524 01133000
*                                                            @Y30LB27*/ 01134000
         L     REQPTR,CONTPTR                                      0524 01135000
