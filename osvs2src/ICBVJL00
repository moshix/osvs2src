         BO    @RT00231                                            0231 00092000
*     RETURN;                       /* JOURNAL MESSAGES      @Y30LB27*/ 00093000
*   IF JRLDISAB=ON THEN             /* LOGICAL DISABLE       @ZA09550*/ 00094000
         TM    JRLDISAB,B'10000000'                                0233 00095000
         BNO   @RF00233                                            0233 00096000
*     DO;                           /* YES                   @ZA09550*/ 00097000
*       VJLMSGID=MSGLSTE;           /* SET MESSAGE ID        @ZA09550*/ 00098000
         MVC   VJLMSGID(4),@CF00032                                0235 00099000
*       CALL VJLWTO;                /* WRITE THE MESSAGE     @ZA09550*/ 00100000
         BAL   @14,VJLWTO                                          0236 00101000
*       RETURN;                     /* GO BACK               @ZA09550*/ 00102000
         B     @EL00001                                            0237 00103000
*     END;                          /* END                   @ZA09550*/ 00104000
*   IF VVIDCBAD=ZERO|DCBOFOPN=OFF THEN/* JOURNAL CLOSED      @Y30LB27*/ 00105000
@RF00233 L     @10,VVIDCBAD(,VVIPTR)                               0239 00106000
         LTR   @10,@10                                             0239 00107000
         BZ    @RT00239                                            0239 00108000
         L     @10,DCBPTR(,VVIPTR)                                 0239 00109000
         TM    DCBOFOPN+40(@10),B'00010000'                        0239 00110000
         BNZ   @RF00239                                            0239 00111000
@RT00239 DS    0H                                                  0240 00112000
*     DO;                           /* YES ISSUE ERROR MSG   @Y30LB27*/ 00113000
*       IF INITOPCD=INICODE THEN    /* INIT MSG              @Y30LB27*/ 00114000
         CLI   INITOPCD(INITPTR),X'88'                             0241 00115000
         BE    @RT00241                                            0241 00116000
*         RETURN;                   /* JUST RETURN           @Y30LB27*/ 00117000
*       VJLMSGID=MSGOPNE;           /* SET MSG COMPLETION INDEX    0243 00118000
*                                                            @Y30LB27*/ 00119000
