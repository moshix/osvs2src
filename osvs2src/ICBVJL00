         STCM  @03,3,JRNLFLGS(JRNLPTR)                             0298 00323000
*   JRNLDATE=RPLVDATE;              /* SET CURRENT DATE      @ZA13484*/ 00324000
         MVC   JRNLDATE(4,JRNLPTR),RPLVDATE(@10)                   0299 00325000
*   JRNLDTFG=ON;                    /* DATE PRESENT          @ZA13484*/ 00326000
         OI    JRNLDTFG(JRNLPTR),B'01000000'                       0300 00327000
*   IF RPLFTIME=ON THEN             /* FIRST CALL            @YM02198*/ 00328000
         TM    RPLFTIME(@10),B'10000000'                           0301 00329000
         BNO   @RF00301                                            0301 00330000
*     JRNLFTIM=ON;                  /* SET RECOVERY FLAG     @YM02198*/ 00331000
         OI    JRNLFTIM(JRNLPTR),B'10000000'                       0302 00332000
*   JRNLUTIL(ONE:ONE+MOVELEN-ONE)=ORDRLST(ONE:MOVELEN-ONE);/*      0303 00333000
*                                                            @YM02198*/ 00334000
@RF00301 L     @10,MOVELEN                                         0303 00335000
         LR    @03,@10                                             0303 00336000
         BCTR  @03,0                                               0303 00337000
         L     @09,ORDRPTR                                         0303 00338000
         EX    @03,@SM02374                                        0303 00339000
*   REQREM=REQREM-MOVELEN-TWELVE;   /* DECR BYTES REMAINING  @ZA13484*/ 00340000
         LA    @03,12                                              0304 00341000
         LH    @09,REQREM(,REQPTR)                                 0304 00342000
         SLR   @09,@10                                             0304 00343000
         SLR   @09,@03                                             0304 00344000
         STH   @09,REQREM(,REQPTR)                                 0304 00345000
*   REQNXT=REQNXT+MOVELEN+TWELVE;   /* INCR DISPLACEMENT     @ZA13484*/ 00346000
         AH    @10,REQNXT(,REQPTR)                                 0305 00347000
         ALR   @10,@03                                             0305 00348000
         STH   @10,REQNXT(,REQPTR)                                 0305 00349000
*   JRNLPTR=ADDR(DATAD(REQNXT));    /* SET LIST PTR          @YM02198*/ 00350000
         L     JRNLPTR,DATAPTR                                     0306 00351000
         LA    JRNLPTR,DATAD-1(@10,JRNLPTR)                        0306 00352000
*   IF REQREM=>FOUR THEN            /* CAN END OF BLOCK BE SET     0307 00353000
*                                                            @YM02198*/ 00354000
         C     @09,@CF00037                                        0307 00355000
         BL    @RF00307                                            0307 00356000
*     JRNLLEN=ZERO;                 /* SKIP REST OF BLOCK    @YM02198   00357000
*                                                            @Y30LB27*/ 00358000
         SLR   @10,@10                                             0308 00359000
         STCM  @10,15,JRNLLEN(JRNLPTR)                             0308 00360000
*/********************************************************************  00361000
** INDICATE WRITE - CALL I/O PROCESSOR TO WRITE REQUEST BLOCK@Y30LB27   00362000
******************************************************************** */ 00363000
*                                                                  0309 00364000
*   WRITESW=ON;                     /* INDICATE A WRITE OPERATION  0309 00365000
*                                                            @Y30LB27*/ 00366000
@RF00307 OI    WRITESW,B'01000000'                                 0309 00367000
*   CALL VJLIO;                     /* CALL THE I/O PROCESSOR@Y30LB27   00368000
*                                                            @Y30LB27*/ 00369000
         BAL   @14,VJLIO                                           0310 00370000
*/********************************************************************  00371000
** SET UP TO RE-WRITE CONTROL RECORD                         @Y30LB27   00372000
******************************************************************** */ 00373000
*                                                                  0311 00374000
*   REQPTR=ADDR(CONTTTR);           /* POINT TO CONTROL TTR  @Y30LB27*/ 00375000
         L     REQPTR,CONTPTR                                      0311 00376000
         LA    REQPTR,CONTTTR(,REQPTR)                             0311 00377000
*   CONTSW=ON;                      /* PROCESS CONTROL RECORD@Y30LB27*/ 00378000
*   WRITDATA=ON;                    /* INDICATE ONLY A WRITE DATA  0313 00379000
*                                      REQD                  @Y30LB27*/ 00380000
*   ORDRSW=OFF;                     /* FORGET DATA BLOCKS    @Y30LB27*/ 00381000
*   MSGSW=OFF;                      /* OR MSG BLOCKS         @Y30LB27*/ 00382000
         OI    CONTSW,B'00110000'                                  0315 00383000
         NI    ORDRSW,B'11110011'                                  0315 00384000
*   CALL VJLIO;                     /* CALL THE I/O SEGMENT  @Y30LB27*/ 00385000
         BAL   @14,VJLIO                                           0316 00386000
*   CONTSW=OFF;                     /* PROCESS CONTROL RECORD@Y30LB27*/ 00387000
*   WRITDATA=OFF;                   /* INDICATE ONLY A WRITE DATA  0318 00388000
*                                      REQD                  @Y30LB27*/ 00389000
         NI    CONTSW,B'11001111'                                  0318 00390000
*   RETURN;                         /* RETURN TO CALLER      @Y30LB27*/ 00391000
         B     @EL00001                                            0319 00392000
*                                                                  0320 00393000
*/* ******************************************************************  00394000
**                                                                 0320 00395000
**     NAME = VJLINIT                                              0320 00396000
**                                                                 0320 00397000
**     INPUT = NONE                                                0320 00398000
**                                                                 0320 00399000
**     PROCESS = VJLINIT DOES THE INITIALIZATION FOR ICBVJL00. STORAGE  00400000
**     FOR AN IOB, ECB, CCW AREA, AND I/O AREAS ARE                0320 00401000
**     OBTAINED. THE AREAS ARE INITIALIZED. AN OBTAIN IS           0320 00402000
**     ISSUED FOR THE FMT1 TO DETERMINE IF THE JOURNAL HAS BEEN    0320 00403000
**     INITIALIZED WITH THE REQUIRED CONTROL RECORD. IF NOT, THE CONTRO 00404000
**     RECORD IS CONSTRUCTED AND WRITTEN TO THE DATA SET, THE DATA SET  00405000
**     IS THEN CLOSED AND RE-OPENED TO UPDATE THE VTOC.            0320 00406000
**                                                                 0320 00407000
**     OUTPUT = JOURNAL CONTROL RECORD IS FORMATTED AND WRITTEN TO THE  00408000
**     DATA SET IF REQUIRED AND THE VJL WORK AREA IS INITIALIZED.  0320 00409000
**                                                                 0320 00410000
******************************************************************** */ 00411000
*                                                                  0320 00412000
*VJLINIT:                                                          0320 00413000
*   PROC OPTIONS(SAVE(14));         /*                       @Y30LB27   00414000
*                                                            @Y30LB27*/ 00415000
VJLINIT  ST    @14,@SA00002                                        0320 00416000
*/********************************************************************  00417000
** INITIALIZATION SEGMENT                                    @Y30LB27   00418000
** IF IPL GET CORE FOR WORKAREA AND INITIALIZE               @Y30LB27   00419000
** IF ENABLE - RECONSTRUCT CONTROL RECORD IF REQUIRED        @Y30LB27   00420000
