         C     @10,@CF00037                                        0399 00739000
         BNL   @RF00399                                            0399 00740000
*         DO;                       /* LESS THEN 4 TRKS ALLOCATED  0400 00741000
*                                                            #Y30LB27*/ 00742000
*           VJLMSGID=MSGMIN;        /* SET MSG INDEX         #Y30LB27*/ 00743000
         MVC   VJLMSGID(4),@CF00598                                0401 00744000
*           CALL VJLWTO;            /* ISSUE ERROR MSG       #Y30LB27*/ 00745000
         BAL   @14,VJLWTO                                          0402 00746000
*           RETURN;                 /* RETURN TO CALLER      #Y30LB27*/ 00747000
         B     @EL00002                                            0403 00748000
*         END;                      /*                       #Y30LB27*/ 00749000
*       TRKSREM=VJLNMTRK-TWO;       /* SET NUMBER OF TRKS REMAINING     00750000
*                                                            @Y30LB27*/ 00751000
@RF00399 L     @10,CONTPTR                                         0405 00752000
         LA    @09,2                                               0405 00753000
         LH    @08,VJLNMTRK                                        0405 00754000
         SLR   @08,@09                                             0405 00755000
         STH   @08,TRKSREM(,@10)                                   0405 00756000
*       TRKSTHR=TRKSREM-TWO;        /* CALCULATE THRESHOLD   @Y30LB27   00757000
*                                                            @Y30LB27*/ 00758000
         SLR   @08,@09                                             0406 00759000
         STH   @08,TRKSTHR(,@10)                                   0406 00760000
*/********************************************************************  00761000
** ORDER REQUESTS ARE JOURNALLED FRONT TO BACK               @Y30LB27   00762000
******************************************************************** */ 00763000
*                                                                  0407 00764000
*       ORDRTTR=ZERO;               /* FIRST DATA RECORD IS TTR = TWO   00765000
*                                                            @Y30LB27*/ 00766000
         SLR   @09,@09                                             0407 00767000
         ST    @09,ORDRTTR(,@10)                                   0407 00768000
*       ORDRR=TWO;                  /* FIRST DATA RECORD IS TTR = TWO   00769000
*                                                            @Y30LB27*/ 00770000
