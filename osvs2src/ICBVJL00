         BNH   @DL00356                                            0358 00585000
*   CONTSW=ON;                      /* INDICATE CONTROL RECORD I/O 0359 00586000
*                                                            @Y30LB27*/ 00587000
         OI    CONTSW,B'00010000'                                  0359 00588000
*   CONTAREA=CONTPTR;               /* POINT CONTROL RECORD BUFFER 0360 00589000
