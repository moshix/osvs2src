*     END;                          /*                       @Y30LB27   00463000
*                                                            @Y30LB27*/ 00464000
*/********************************************************************  00465000
** IF ENABLE STORAGE ALREADY OBTAINED                        @Y30LB27   00466000
******************************************************************** */ 00467000
*                                                                  0333 00468000
*   ECBPTR=VVIVJL;                  /* SET ECB PTR           @ZA09550*/ 00469000
@RF00321 L     @10,VVIVJL(,VVIPTR)                                 0333 00470000
         ST    @10,ECBPTR                                          0333 00471000
*   CHNPTR=ECBPTR+LENGTH(ECB);      /* SET CHAIN PTR         @Y30LB27*/ 00472000
         LA    @06,4                                               0334 00473000
         LR    CHNPTR,@10                                          0334 00474000
         ALR   CHNPTR,@06                                          0334 00475000
*   IOBCHN=''B;                     /* CLEAR CHAIN AREA      @Y30LB27   00476000
*                                                            @Y30LB27*/ 00477000
