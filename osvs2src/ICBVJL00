*       MOVELEN=RPLVTLEN-LENGTH(RPLV);/* SET ORDR LENGTH     @Y30LB27*/ 00241000
         LH    @10,RPLVTLEN(,@10)                                  0277 00242000
         SL    @10,@CF02341                                        0277 00243000
         ST    @10,MOVELEN                                         0277 00244000
*       REQPTR=ADDR(ORDRTTR);       /* POINT I/O TO DATA BLOCKS    0278 00245000
*                                                            @Y30LB27*/ 00246000
         L     REQPTR,CONTPTR                                      0278 00247000
         LA    REQPTR,ORDRTTR(,REQPTR)                             0278 00248000
*     END;                          /*                       @Y30LB27   00249000
*                                                            @Y30LB27*/ 00250000
*/********************************************************************  00251000
** WILL REQUEST FIT CURRENT DATA BLOCK                       @Y30LB27   00252000
** IF YES READ CURRENT DATA BLOCK                            @Y30LB27   00253000
******************************************************************** */ 00254000
*                                                                  0280 00255000
*   IF MOVELEN<=REQREM-SIXTEEN THEN /* CURRENT REQUEST FIT CURRENT 0280 00256000
*                                      DATA BLOCK            @ZA13484*/ 00257000
@RC00261 LH    @10,REQREM(,REQPTR)                                 0280 00258000
         SL    @10,@CF00099                                        0280 00259000
         C     @10,MOVELEN                                         0280 00260000
         BL    @RF00280                                            0280 00261000
*     DO;                           /* USE CURRENT BLOCK     @Y30LB27*/ 00262000
*       READSW=ON;                  /* INDICATE READ FOR CURRENT DATA   00263000
*                                      BLOCK                 @Y30LB27*/ 00264000
         OI    READSW,B'10000000'                                  0282 00265000
*       CALL VJLIO;                 /* ALL I/O PROCESSOR TO SET UP 0283 00266000
*                                      NEXT BLOCK            @Y30LB27*/ 00267000
         BAL   @14,VJLIO                                           0283 00268000
*       READSW=OFF;                 /* TURN READ SW OFF      @Y30LB27*/ 00269000
         NI    READSW,B'01111111'                                  0284 00270000
*       IF RPLVRETC^=ZERO THEN      /* JOURNAL ERROR - TRKS EXHAUSTED   00271000
*                                                            @Y30LB27*/ 00272000
         L     @10,RPLVPTR                                         0285 00273000
         LH    @10,RPLVRETC(,@10)                                  0285 00274000
         N     @10,@CF02366                                        0285 00275000
         LTR   @10,@10                                             0285 00276000
         BNZ   @RT00285                                            0285 00277000
*         RETURN;                   /* RETURN TO CALLER      @Y30LB27*/ 00278000
*     END;                          /* END READ OF CURRENT BLOCK   0287 00279000
*                                                            @Y30LB27*/ 00280000
*/********************************************************************  00281000
** IF NO SET UP NEXT DATA BLOCK                              @Y30LB27   00282000
******************************************************************** */ 00283000
*                                                                  0288 00284000
*   ELSE                            /*                       @Y30LB27*/ 00285000
*     DO;                           /* NO NEW BLOCK REQD     @Y30LB27*/ 00286000
         B     @RC00280                                            0288 00287000
@RF00280 DS    0H                                                  0289 00288000
*       CALL VJLCNVRT;              /* CONVERT NEXT TTR TO BE USED 0289 00289000
*                                                            @Y30LB27*/ 00290000
         BAL   @14,VJLCNVRT                                        0289 00291000
*       IF RPLVRETC^=ZERO THEN      /* JRNL TRKS EXHAUSTED   @Y30LB27*/ 00292000
         L     @10,RPLVPTR                                         0290 00293000
