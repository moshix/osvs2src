         ST    @10,IOBSTART(,@06)                                  0352 00558000
*/********************************************************************  00559000
** SET UP FOR CONTROL RECORD INITIALIZATION                  @Y30LB27   00560000
******************************************************************** */ 00561000
*                                                                  0353 00562000
*   DO I=ONE TO MAXREC BY 256;      /* ZERO CONTROL RECORD   @ZM30725*/ 00563000
@RC00350 LA    I,1                                                 0353 00564000
@DL00353 DS    0H                                                  0354 00565000
*     CONTPTR->DATAD(I:I+255)=''B;  /* BUFFER                @ZM30725*/ 00566000
         L     @10,CONTPTR                                         0354 00567000
         ALR   @10,I                                               0354 00568000
