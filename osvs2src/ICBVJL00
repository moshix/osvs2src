*     DO                            /* IPL CALL              @Y30LB27*/ 00427000
*           GETMNLEN=LENGTH(IOBSTDRD)+LENGTH(IOBEXTEN)/* IOB @X04AC27*/ 00428000
*           +LENGTH(ECB)            /* ECB                   @Y30LB27*/ 00429000
*           +FOUR                   /* DWD                   @Y30LB27*/ 00430000
*           +LENGTH(CCWCHN)         /* SEARCH CHN            @Y30LB27*/ 00431000
*           +LENGTH(CONTREC)        /* CONTROL REC@Y30LB27           */ 00432000
