         LR    REG6,@11                                            0172 00244000
*   IF SDWAPTR^=ZERO THEN           /* SDWA AVAILABLE?       @Y30LB24*/ 00245000
         CR    SDWAPTR,@11                                         0173 00246000
         BE    @RF00173                                            0173 00247000
*     DO;                           /* YES                   @Y30LB24*/ 00248000
*       IF REG12=SDWAGR12 THEN      /* ABEND RECURSION       @Y30LB24*/ 00249000
         C     REG12,SDWAGR12(,SDWAPTR)                            0175 00250000
         BNE   @RF00175                                            0175 00251000
*         DO;                       /* YES                   @Y30LB24*/ 00252000
*           REG1=ADDR(MSG2);        /* SET UP REG 1          @Y30LB24*/ 00253000
         LA    REG1,MSG2                                           0177 00254000
*LWTO3:                                                            0178 00255000
*           GEN(WTO MF=(E,(REG1))); /* SEND ERROR MSG2       @Y30LB24*/ 00256000
