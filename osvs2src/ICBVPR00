*   RETURN;                         /* RTRN TO REC TERM      @Y30LB24*/ 00119000
         B     @EL00001                                            0141 00120000
*                                                                  0142 00121000
*/******************************************************************    00122000
*                                                                  0142 00123000
*        NAME = VPRFRLOX, FREE LOCKS                           @G24LB04 00124000
*                                                              @G24LB04 00125000
*        INPUT = BIT INDICATORS IN MSVC CONTROL BLOCK TO SHOW  @G24LB04 00126000
*                WHICH LOCK(S) TO BE FREED                     @G24LB04 00127000
*                                                              @G24LB04 00128000
*        PROCESS = TESTS BITS AND FREES LOCAL AND CMS LOCKS,   @G24LB04 00129000
*                  AS INDICATED                                @G24LB04 00130000
*                                                              @G24LB04 00131000
*        OUTPUT = LOCKS FREED UP, LOCK FLAGS TURNED OFF        @G24LB04 00132000
*                 DESTROYS REGS 6,2,11,12,13                   @G24LB04 00133000
*                                                                  0142 00134000
*                                                                  0142 00135000
******************************************************************** */ 00136000
*/*VPRFRLOX: CHART                                                   */ 00137000
*/*HEADER                                                          0142 00138000
*/*MAY 24, 1976                                ICBVPR00 - SEGMENT  0142 00139000
*/*VPRFRLOX - FREE ANY LOCKS HELD                                    */ 00140000
*/*          E                    VPRFRLOX                           */ 00141000
*/*          P                    FIND MSVC CONTROL BLOCK            */ 00142000
*/*          D (YES,,NO,%END)     IS THERE AN MSVC CONTROL BLOCK ?   */ 00143000
*/*          D (YES,,NO,%END)     ANY LOCKS HELD ?                   */ 00144000
*/*          D (YES,,NO,%LCL)     CMS LOCK HELD ?                    */ 00145000
*/*%CMS:     S                    SETLOCK: FREE CMS LOCK             */ 00146000
*/*%LCL:     S                    SETLOCK: FREE LOCAL LOCK           */ 00147000
*/*          P                    RESET FLAGS TO SHOW NO LOCKS HELD BY  00148000
*/*MSVC TASK OR ITS MODULES                                          */ 00149000
*/*%END:     R                    RETURN                             */ 00150000
*/*VPRFRLOX: END                                                     */ 00151000
*                                                                  0142 00152000
*VPRFRLOX:                                                         0142 00153000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/* FREE LOCKS               */ 00154000
VPRFRLOX DS    0H                                                  0143 00155000
*   RFY                                                            0143 00156000
*     REG2 RSTD;                    /*                       @G24LB04*/ 00157000
*   REG2=REG14;                     /* SAVE RETURN ADDRESS   @G24LB04*/ 00158000
         LR    REG2,REG14                                          0144 00159000
*   GEN(BALR REG6,0);               /* NEED ADDRESSABILITY   @G24LB04*/ 00160000
         BALR REG6,0                                                    00161000
*   GEN(USING *,REG6);              /*                       @G24LB04*/ 00162000
         USING *,REG6                                                   00163000
*   IF VVIFLG7=ON                   /* CMS LOCK HELD ?       @G24LB04*/ 00164000
*     THEN                                                         0147 00165000
         TM    VVIFLG7(VVIPTR),B'00000001'                         0147 00166000
         BNO   @RF00147                                            0147 00167000
*     DO;                           /* YES, FREE IT (MUST HAVE LOCAL)   00168000
*                                                            @G24LB04*/ 00169000
*       GEN REFS(REG11,REG12,REG13,REG14,PSALITA)/* DESTROYS REGS  0149 00170000
*                                                            @G24LB04*/ 00171000
*           SETS(REG11,REG12,REG13,REG14);                         0149 00172000
*                                                          /*@G24LB04*/ 00173000
       SETLOCK RELEASE,TYPE=CMS,RELATED=(CMS,ICBVTP01)         @G24LB04 00174000
*       VVIFLG7=OFF;                /* MARK CMS LOCK NO LONGER HELD     00175000
*                                                            @G24LB04*/ 00176000
         NI    VVIFLG7(VVIPTR),B'11111110'                         0150 00177000
*     END;                          /*                       @G24LB04*/ 00178000
*   IF VVIFLG6=ON                   /* LOCAL LOCK HELD ?     @G24LB04*/ 00179000
*     THEN                                                         0152 00180000
@RF00147 TM    VVIFLG6(VVIPTR),B'00000010'                         0152 00181000
         BNO   @RF00152                                            0152 00182000
*     DO;                           /* YES, FREE IT          @G24LB04*/ 00183000
*       GEN REFS(REG11,REG12,REG13,REG14,PSALITA)/* DESTROYS REGS  0154 00184000
*                                                            @G24LB04*/ 00185000
*           SETS(REG11,REG12,REG13,REG14);                         0154 00186000
*                                                          /*@G24LB04*/ 00187000
       SETLOCK RELEASE,TYPE=LOCAL,RELATED=(LOCAL,ICBVTP01)     @G24LB04 00188000
*       VVIFLG6=OFF;                /* MARK LOCAL LOCK NO LONGER HELD   00189000
*                                                            @G24LB04*/ 00190000
         NI    VVIFLG6(VVIPTR),B'11111101'                         0155 00191000
*     END;                          /*                       @G24LB04*/ 00192000
*   GEN(DROP REG6);                 /*                       @G24LB04*/ 00193000
@RF00152 DS    0H                                                  0157 00194000
         DROP REG6                                                      00195000
*   REG14=REG2;                     /* RESTORE RETURN ADDRESS      0158 00196000
*                                                            @G24LB04*/ 00197000
         LR    REG14,REG2                                          0158 00198000
*   RFY                                                            0159 00199000
*     REG2 UNRSTD;                  /*                       @G24LB04*/ 00200000
*   RETURN;                         /*                       @G24LB04*/ 00201000
@EL00002 DS    0H                                                  0160 00202000
@EF00002 DS    0H                                                  0160 00203000
@ER00002 BR    @14                                                 0160 00204000
*   END VPRFRLOX;                   /*                       @G24LB04*/ 00205000
*                                                                  0162 00206000
*   /*****************************************************************/ 00207000
*   /*                                                               */ 00208000
*   /* THIS IS THE RETRY ROUTINE FOR THE ESTAE PROCESSING. IT IS     */ 00209000
*   /* ENTERED FROM RECOVERY TERMINATION AFTER THE EXIT ROUTINE SETS */ 00210000
*   /* UP RETRY AND RETURNS. ADDRESSABILITY MUST BE SET UP FOR RETRY */ 00211000
*   /* ROUTINE                                               #Y30LB24*/ 00212000
*   /*                                                               */ 00213000
*   /*****************************************************************/ 00214000
*                                                                  0162 00215000
*VPRRTRY1:                                                         0162 00216000
