         BZ    @RF00189                                            0189 00284000
*     DO;                           /* YES                   @Y30LB24*/ 00285000
*       REG0=LENGTH(SDWA);          /* SET FREEMAIN LENGTH@Y30LB24   */ 00286000
         LA    REG0,512                                            0191 00287000
*       GEN(ICM REG0,M8,POOL);      /* INDICATE SDWA SUBPOOL @Y30LB24*/ 00288000
         ICM REG0,M8,POOL                                               00289000
*       REG1=SDWAPTR;               /* SET FREEMAIN ADDR     @Y30LB24*/ 00290000
