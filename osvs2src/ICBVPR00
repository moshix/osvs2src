*                                                            @Y30LB24*/ 00297000
@RF00189 L     @15,@CV01634                                        0196 00298000
         BALR  @14,@15                                             0196 00299000
*   RETURN CODE(0);                 /* RETURN NEVER EXECUTED @Y30LB24*/ 00300000
         SLR   @15,@15                                             0197 00301000
         BR    @14                                                 0197 00302000
*                                                                  0198 00303000
*/******************************************************************    00304000
*                                                                  0198 00305000
*     NAME = VPRFDERR,  FIND ABENDING MODULE.                  @Y30LB24 00306000
*                                                                  0198 00307000
*     INPUT = POINTER TO THE SDWA IF AVAILABLE.                @Y30LB24 00308000
*                                                                  0198 00309000
*     PROCESS = THIS SUBROUTINE IS CALLED TO DETERMINE THE         0198 00310000
*     NAME OF THE FAILING MODULE.                              @Y30LB24 00311000
*                                                                  0198 00312000
*     OUTPUT = THE FAILING MODULE NAME IF IT CAN BE DETERMINED     0198 00313000
*     IS PUT IN A MESSAGE AND IN A WORK AREA..                 @Y30LB24 00314000
*                                                                  0198 00315000
******************************************************************** */ 00316000
*/*VPRFDERR: CHART                                                   */ 00317000
*/*HEADER                                                          0198 00318000
*/*JUNE 14, 1974                                ICBVPR00 - SEGMENT 0198 00319000
*/*VPRFDERR - FIND ABENDING MODULE NAME                              */ 00320000
*/*          E                    VPRFDERR                           */ 00321000
*/*          P                    FILL DUMMY MESSSAGE                */ 00322000
*/*          D (YES,,NO,%END)     DOES SDWA EXIST?                   */ 00323000
*/*          P                    FILL ERROR NAME                    */ 00324000
*/*%END:     R                    RETURN TO CALLER                   */ 00325000
*/*VPRFDERR: END                                                     */ 00326000
*                                                                  0198 00327000
*VPRFDERR:                                                         0198 00328000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0198 00329000
VPRFDERR DS    0H                                                  0199 00330000
*   GEN(BALR  REG6,0);              /* ESTAB LOCAL ADDR      @Y30LB24*/ 00331000
         BALR  REG6,0                                                   00332000
*   GEN(USING *,REG6);              /* USE NEW BASE ADDR     @Y30LB24*/ 00333000
         USING *,REG6                                                   00334000
*   MSG1(43:50)=UNKNOWN;            /* FILL DUMMY MSG        @ZA26243*/ 00335000
         MVC   MSG1+42(8),UNKNOWN                                  0201 00336000
*   FAILNAME=UNKNOWN;               /* FILL WORK AREA        @ZA26243*/ 00337000
         MVC   FAILNAME(8),UNKNOWN                                 0202 00338000
*   WORK1(1:1)=ICBVVICB->VVIOPCD;   /* GET FUNCT CODE        @ZA26243*/ 00339000
         L     @11,CVTPTR                                          0203 00340000
         L     @11,CVTEXT2(,@11)                                   0203 00341000
         L     @11,CVTICB(,@11)                                    0203 00342000
         L     @11,ICBVVICB(,@11)                                  0203 00343000
         MVC   WORK1(1),VVIOPCD(@11)                               0203 00344000
*   WORK1(2:2)=SIGN;                /* PUT IN SIGN           @ZA26243*/ 00345000
         MVC   WORK1+1(1),SIGN                                     0204 00346000
*   GEN(UNPK  WORK2(3),WORK1(2));   /* UNPACK FUNCT CODE     @ZA26243*/ 00347000
         UNPK  WORK2(3),WORK1(2)                                        00348000
*   GEN(TR    WORK2(2),TRANS-240);  /* TRANSLATE             @ZA26243*/ 00349000
         TR    WORK2(2),TRANS-240                                       00350000
*   MSG1(55:56)=WORK2(1:2);         /* INSERT FUNC CODE      @ZA26243*/ 00351000
         MVC   MSG1+54(2),WORK2                                    0207 00352000
*   IF SDWAPTR=ZERO THEN            /* SDWA AVAILABLE?       @Y30LB24*/ 00353000
         LTR   SDWAPTR,SDWAPTR                                     0208 00354000
         BNZ   @RF00208                                            0208 00355000
*     DO;                           /* NO                    @Y30LB24*/ 00356000
*       RETURN;                     /* RETURN TO CALLER      @Y30LB24*/ 00357000
@EL00003 DS    0H                                                  0210 00358000
@EF00003 DS    0H                                                  0210 00359000
@ER00003 BR    @14                                                 0210 00360000
*     END;                          /*                       @Y30LB24*/ 00361000
*   REG15=SDWAGR12;                 /* SET MODULE BASE REG   @Y30LB24*/ 00362000
@RF00208 L     REG15,SDWAGR12(,SDWAPTR)                            0212 00363000
*   GEN(LA    REG15,0(0,REG15));    /* CLEAR HIGH BYTE       @Y30LB24*/ 00364000
         LA    REG15,0(0,REG15)                                         00365000
*   DO REG10=ONE TO TABLENG;        /* FIND ERROR MODULE     @Y30LB24*/ 00366000
         LA    REG10,1                                             0214 00367000
@DL00214 DS    0H                                                  0215 00368000
*     REG4=ABENDTAB(REG10);         /* SET WORK REG          @Y30LB24*/ 00369000
         LR    @11,REG10                                           0215 00370000
         SLA   @11,2                                               0215 00371000
         L     REG4,ABENDTAB-4(@11)                                0215 00372000
*     REG5=REG4+N64;                /* SET WORK REG          @Y30LB24*/ 00373000
         LA    REG5,64                                             0216 00374000
         ALR   REG5,REG4                                           0216 00375000
*     IF REG4<REG15&                /* LOWER LIMIT OK        @Y30LB24*/ 00376000
