         TITLE 'ICBVPR00 MSVC SUBTASK RECOVERY PROGRAM - MVS           *00001000
                        '                                               00002000
ICBVPR00 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ICBVPR00  77.256'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
*                                                                  0103 00013000
*   /*****************************************************************/ 00014000
*   /*                                                               */ 00015000
*   /* IF NO SDWA WAS PASSED BY RECOVERY TERMINATION, THEN NO        */ 00016000
*   /* SAVEAREA IS AVAILABLE AND A WORKAREA PROBABLY MAY NOT BE      */ 00017000
*   /* OBTAINABLE. PROCESSING IN THIS SITUATION MAY NOT REFERENCE    */ 00018000
*   /* SDWA, BUT USE REG FOR RETRY PARMS.                    @Y30LB24*/ 00019000
*   /*                                                               */ 00020000
*   /*****************************************************************/ 00021000
*                                                                  0103 00022000
*   IF REG0=NOSDWA THEN             /* IS THERE A SDWA?      @Y30LB24*/ 00023000
         C     REG0,@CF00032                                       0103 00024000
         BNE   @RF00103                                            0103 00025000
*     DO;                           /* NO                    @Y30LB24*/ 00026000
*       SDWAPTR=ZERO;               /* ZERO SDWA PTR         @Y30LB24*/ 00027000
         SLR   SDWAPTR,SDWAPTR                                     0105 00028000
*       VVIPTR=ICBVVICB;            /* ADDR MSVC CONTROL BLK @G24LB04*/ 00029000
         L     @11,CVTPTR                                          0106 00030000
         L     @11,CVTEXT2(,@11)                                   0106 00031000
         L     @11,CVTICB(,@11)                                    0106 00032000
         L     VVIPTR,ICBVVICB(,@11)                               0106 00033000
*       REG3=REG14;                 /* SAVE LINKREG          @Y30LB24*/ 00034000
         LR    REG3,REG14                                          0107 00035000
*       REG9=REG12;                 /* SAVE BASE REGISTER    @G24LB04*/ 00036000
         LR    REG9,REG12                                          0108 00037000
*       CALL VPRFRLOX;              /* FREES ANY LOCKS       @G24LB04   00038000
*                                      HELD BY MSVC TASK     @G24LB04*/ 00039000
         BAL   @14,VPRFRLOX                                        0109 00040000
*       REG12=REG9;                 /* RESTORE BASE REG      @G24LB04*/ 00041000
         LR    REG12,REG9                                          0110 00042000
*       CALL VPRFDERR;              /* CALL FIND ERROR RTN   @Y30LB24*/ 00043000
         BAL   @14,VPRFDERR                                        0111 00044000
*       REG14=REG3;                 /* RESTORE LINKREG       @Y30LB24*/ 00045000
         LR    REG14,REG3                                          0112 00046000
*       REG3=REG1;                  /* SAVE REG1             @Y30LB24*/ 00047000
         LR    REG3,REG1                                           0113 00048000
*       REG1=ADDR(MSG1);            /* SET UP REG 1          @Y30LB24*/ 00049000
         LA    REG1,MSG1                                           0114 00050000
*LWTO1:                                                            0115 00051000
*       GEN(WTO MF=(E,(REG1)));     /* SEND ERROR MSG1       @Y30LB24*/ 00052000
LWTO1    WTO MF=(E,(REG1))                                              00053000
*       REG1=REG3;                  /* RESTORE REG1          @Y30LB24*/ 00054000
         LR    REG1,REG3                                           0116 00055000
*       REG0=ADDR(VPRRTRY1);        /* SET RETRY ADDRESS     @Y30LB24*/ 00056000
         LA    REG0,VPRRTRY1                                       0117 00057000
*       REG15=FOUR;                 /* SET RETRY REQ PARM    @Y30LB24*/ 00058000
         LA    REG15,4                                             0118 00059000
*       RETURN;                     /* RTRN RECOVERY TERM    @Y30LB24*/ 00060000
@EL00001 DS    0H                                                  0119 00061000
@EF00001 DS    0H                                                  0119 00062000
@ER00001 BR    @14                                                 0119 00063000
*     END;                          /*                       @Y30LB24*/ 00064000
*                                                                  0120 00065000
*   /*****************************************************************/ 00066000
*   /*                                                               */ 00067000
*   /* IF A SDWA WAS PASSED BY RECOVERY TERMINATION, THEN PROCESSING */ 00068000
*   /* IN THIS SITUATION MUST FILL REQUIRED ENTRIES IN THE SDWA      */ 00069000
*   /* BEFORE RETURNING FOR RETRY.                           @Y30LB24*/ 00070000
*   /*                                                               */ 00071000
*   /*****************************************************************/ 00072000
*                                                                  0121 00073000
*   SDWAPTR=REG1;                   /* INIT SDWA PTR         @Y30LB24*/ 00074000
