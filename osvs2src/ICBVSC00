*       GEN(WAIT  1,ECB=(REG1));    /* WAIT FOR COMPLETION   @G24LB18*/ 01705000
LWAIT1   WAIT  1,ECB=(REG1)                                             01706000
*     END;                          /*                       @G24LB18*/ 01707000
*   RFY                                                            0488 01708000
*     REG1 UNRSTD;                  /* UNRESTRICT REG 1      @G24LB18*/ 01709000
*   RFY                                                            0489 01710000
*     REG2 UNRSTD;                  /* UNRESTRICT REG 2      @G24LB18*/ 01711000
*                                                                  0489 01712000
*   /*****************************************************************/ 01713000
*   /*                                                               */ 01714000
