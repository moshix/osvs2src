         ST    @06,VVIRES5(,@10)                                   0474 01665000
*   VVIRES6=ADDR(CVTBRET);          /* SET PARM LIST ERRADDR @ZA28148*/ 01666000
         LA    @07,CVTBRET(,@07)                                   0475 01667000
         ST    @07,VVIRES6(,@10)                                   0475 01668000
*   REG1=ADDR(VVIRES4);             /* SET PARM REG          @G24LB18*/ 01669000
         LA    REG1,VVIRES4(,@10)                                  0476 01670000
*   GEN(ICM   REG1,M8,EID);         /* INDICATE PARM LIST    @G24LB18*/ 01671000
         ICM   REG1,M8,EID                                              01672000
*   REG0=ZERO;                      /* SET POST CODE         @G24LB18*/ 01673000
*                                                                  0478 01674000
         SLR   REG0,REG0                                           0478 01675000
*   /*****************************************************************/ 01676000
*   /*                                                               */ 01677000
*   /* POST SUBTASK ECB IF REQUIRED.                         @G24LB18*/ 01678000
*   /*                                                               */ 01679000
*   /*****************************************************************/ 01680000
*                                                                  0479 01681000
*   IF VVIECB1^=PZERO THEN          /* IS POST REQUIRED?     @G24LB18*/ 01682000
