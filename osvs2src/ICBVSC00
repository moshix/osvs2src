         TITLE '************ ICBVSC00 - MSVC SCREENING MODULE **********00001000
               ****     '                                               00002000
ICBVSC00 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'ICBVSC00  77.355'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
*   PARMPTR=REG1;                   /* SAVE PARM LIST ADDR   @G24LB18*/ 00022000
         LR    PARMPTR,REG1                                        0118 00023000
*   VVIPTR=ICBVVICB;                /* SET UP MSVC CB PTR    @G24LB18*/ 00024000
         L     @10,CVTPTR                                          0119 00025000
         L     @10,CVTEXT2(,@10)                                   0119 00026000
         L     @10,CVTICB(,@10)                                    0119 00027000
         L     @10,ICBVVICB(,@10)                                  0119 00028000
         ST    @10,VVIPTR                                          0119 00029000
*   RFY                                                            0120 00030000
*     REG1 UNRSTD;                  /* UNRESTRICT REG 1      @G24LB18*/ 00031000
*   RFY                                                            0121 00032000
*     UNITP BASED(ADDR(UNITRPL));   /* BASE UNITP PARM LIST  @G24LB18*/ 00033000
*   RFY                                                            0122 00034000
*     TABP BASED(ADDR(TABPRPL));    /* BASE TABP PARM LIST   @G24LB18*/ 00035000
*   RFY                                                            0123 00036000
*     SELRB BASED(PARMPTR);         /* BASE SELECT REQ BLK   @G24LB18*/ 00037000
*   RFY                                                            0124 00038000
*     TRMRB BASED(PARMPTR);         /* BASE TERMINATE REQ BK @G24LB18*/ 00039000
*                                                                  0125 00040000
*   /*****************************************************************/ 00041000
*   /*                                                               */ 00042000
*   /* INITIALIZE RETURN AND REASON CODES                    @G24LB18*/ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0125 00046000
*   RETURNCD=ZERO;                  /* SET RETURN CODE=0     @G24LB18*/ 00047000
         SLR   @10,@10                                             0125 00048000
         ST    @10,RETURNCD                                        0125 00049000
*   REASONCD=SUCCESS;               /* SET REASON CODE=0     @G24LB18*/ 00050000
         ST    @10,REASONCD                                        0126 00051000
*                                                                  0127 00052000
*   /*****************************************************************/ 00053000
*   /*                                                               */ 00054000
*   /* SHIFT THE ENTRY INDICATORS PASSED IN THE HIGH ORDER   @G24LB18*/ 00055000
*   /* BYTE OF REG 0 TO THE LOW ORDER BYTE OF REG 3 AND TEST @G24LB18*/ 00056000
*   /* FOR SECOND ENTRY ON THIS REQUEST. IF SO, RETURN TO    @G24LB18*/ 00057000
*   /* MSSC SVC.                                             @G24LB18*/ 00058000
*   /*                                                               */ 00059000
*   /*****************************************************************/ 00060000
*                                                                  0127 00061000
*   REG3=REG0;                      /* GET ENTRY INDICATORS  @G24LB18*/ 00062000
         LR    REG3,REG0                                           0127 00063000
*   GEN(SRL REG3,M24);              /* SHIFT TO LOW ORD BYTE @G24LB18*/ 00064000
         SRL REG3,M24                                                   00065000
*   IF REG3=SECENTRY THEN           /* IS IT SECOND ENTRY?   @G24LB18*/ 00066000
         SLR   @10,@10                                             0129 00067000
         IC    @10,SECENTRY                                        0129 00068000
         CR    REG3,@10                                            0129 00069000
         BE    @RT00129                                            0129 00070000
*     GO TO EXIT;                   /* YES - EXIT            @G24LB18*/ 00071000
*                                                                  0131 00072000
*   /*****************************************************************/ 00073000
*   /*                                                               */ 00074000
*   /* CHECK THE OPCODE OF THE PASSED REQUEST AND CALL THE   @G24LB18*/ 00075000
*   /* APPROPRIATE PROCEDURE TO PROCESS IT.                  @G24LB18*/ 00076000
*   /*                                                               */ 00077000
*   /*****************************************************************/ 00078000
*                                                                  0131 00079000
*                                                                  0131 00080000
*   /*****************************************************************/ 00081000
*   /*                                                               */ 00082000
*   /* EOV ACQUIRE REQUEST                                   @G24LB18*/ 00083000
*   /*                                                               */ 00084000
*   /*****************************************************************/ 00085000
*                                                                  0131 00086000
*   IF OPCODE=ACQUIRE THEN          /* IS THIS AN ACQUIRE?   @G24LB18*/ 00087000
         CLI   OPCODE(PARMPTR),X'02'                               0131 00088000
         BNE   @RF00131                                            0131 00089000
*     DO;                           /* YES                   @G24LB18*/ 00090000
*       CALL EOVACQRE;              /* CALL EOV ACQUIRE RTN  @G24LB18*/ 00091000
         BAL   @14,EOVACQRE                                        0133 00092000
*       GO TO EXIT;                 /* EXIT                  @G24LB18*/ 00093000
         B     EXIT                                                0134 00094000
*     END;                          /* WITH RETURN CODE =0   @G24LB18*/ 00095000
*                                                                  0135 00096000
*   /*****************************************************************/ 00097000
*   /*                                                               */ 00098000
*   /* ALLOCATION TERMINATE REQUEST                          @G24LB18*/ 00099000
*   /*                                                               */ 00100000
*   /*****************************************************************/ 00101000
*                                                                  0136 00102000
*   IF OPCODE=TERMINAT THEN         /* IS IT A TERMINATE REQ?@G24LB18*/ 00103000
@RF00131 CLI   OPCODE(PARMPTR),X'8A'                               0136 00104000
         BNE   @RF00136                                            0136 00105000
*     DO;                           /* YES                   @G24LB18*/ 00106000
*       CALL ALLOCTRM;              /* CALL TERMINATE RTN    @G24LB18*/ 00107000
         BAL   @14,ALLOCTRM                                        0138 00108000
*       GO TO EXIT;                 /* EXIT                  @G24LB18*/ 00109000
         B     EXIT                                                0139 00110000
*     END;                          /* WITH RETURN CODE =0   @G24LB18*/ 00111000
*                                                                  0140 00112000
*   /*****************************************************************/ 00113000
*   /*                                                               */ 00114000
*   /* ALLOCATION SELECT REQUEST                             @G24LB18*/ 00115000
*   /*                                                       @G24LB18*/ 00116000
*   /* IF THIS IS AN ABNORMAL TERMINATION CALL ON A SELECT   @G24LB18*/ 00117000
*   /* REQUEST, RETURN TO MSSC SVC WITHOUT PROCESSING THE    @G24LB18*/ 00118000
*   /* REQUEST. OTHERWISE, PROCESS IT NORMALLY.              @G24LB18*/ 00119000
*   /*                                                               */ 00120000
*   /*****************************************************************/ 00121000
*                                                                  0141 00122000
*   IF OPCODE=SELECT THEN           /* IS IT A SELECT REQ    @G24LB18*/ 00123000
@RF00136 CLI   OPCODE(PARMPTR),X'89'                               0141 00124000
         BNE   @RF00141                                            0141 00125000
*     DO;                           /* YES                   @G24LB18*/ 00126000
*       IF REG3=ABTRMIO|            /* ABNORMAL TERMINATION? @G24LB18*/ 00127000
*           REG3=ABTRMNIO THEN      /*                       @G24LB18*/ 00128000
         SLR   @10,@10                                             0143 00129000
         IC    @10,ABTRMIO                                         0143 00130000
         CR    REG3,@10                                            0143 00131000
         BE    @RT00143                                            0143 00132000
         SLR   @10,@10                                             0143 00133000
         IC    @10,ABTRMNIO                                        0143 00134000
         CR    REG3,@10                                            0143 00135000
         BE    @RT00143                                            0143 00136000
*         GO TO EXIT;               /* YES - EXIT            @G24LB18*/ 00137000
*       CALL ALLOCSEL;              /* CALL SELECT RTN       @G24LB18*/ 00138000
         BAL   @14,ALLOCSEL                                        0145 00139000
*       GO TO EXIT;                 /* EXIT                  @G24LB18*/ 00140000
         B     EXIT                                                0146 00141000
*     END;                          /* WITH RETURN CODE      @G24LB18*/ 00142000
*                                                                  0148 00143000
*   /*****************************************************************/ 00144000
*   /*                                                               */ 00145000
*   /* IF THE OPCODE PASSED IN THE PARAMETER LIST IS         @G24LB18*/ 00146000
*   /* UNDEFINED (NOT ONE OF THE EXPECTED OPCODES),          @G24LB18*/ 00147000
*   /* RETURN TO THE MSSC SVC WITH A RETURN CODE OF 4 AND A  @G24LB18*/ 00148000
*   /* REASON CODE OF 217 (INVALID REQUEST OPCODE).          @G24LB18*/ 00149000
*   /*                                                               */ 00150000
*   /*****************************************************************/ 00151000
*                                                                  0148 00152000
*   REASONCD=INVALOP;               /* SET REASON CODE=217   @G24LB18*/ 00153000
@RF00141 MVC   REASONCD(4),@CF00065                                0148 00154000
*   RETURNCD=FOUR;                  /* SET RETURN CODE=4     @G24LB18*/ 00155000
*                                                                  0149 00156000
         MVC   RETURNCD(4),@CF00109                                0149 00157000
*   /*****************************************************************/ 00158000
*   /*                                                               */ 00159000
*   /* RETURN TO THE MSSC SVC WITH RETURN CODE IN REG 15 AND @G24LB18*/ 00160000
*   /* REASON CODE IN REG 0.                                 @G24LB18*/ 00161000
*   /*                                                               */ 00162000
*   /*****************************************************************/ 00163000
*                                                                  0150 00164000
*EXIT:                                                             0150 00165000
*   RFY                                                            0150 00166000
*     REG2 RSTD;                    /* RESTRICT REG 2        @G24LB18*/ 00167000
EXIT     DS    0H                                                  0151 00168000
*   REG2=REASONCD;                  /* SAVE REASON CODE      @G24LB18*/ 00169000
         L     REG2,REASONCD                                       0151 00170000
*   REG3=RETURNCD;                  /* SAVE RETURN CODE      @G24LB18*/ 00171000
         L     REG3,RETURNCD                                       0152 00172000
*   RFY                                                            0153 00173000
*     REG2 UNRSTD;                  /* UNRESTRICT REG 2      @G24LB18*/ 00174000
*   GENERATE;                                                      0154 00175000
*                                                                  0154 00176000
*                                                          /*@G24LB18*/ 00177000
* FREE AUTOMATIC STORAGE                                                00178000
         L     @13,4(,@13)             GET SAVEAREA ADDR       @G24LB18 00179000
         L     @00,@SIZDATD            GET STORAGE SIZE        @G24LB18 00180000
         LR    @01,@11                 GET STORAGE ADDR        @G24LB18 00181000
         FREEMAIN R,LV=(0),A=(1)                                        00182000
* RESTORE REGISTERS                                                     00183000
         LR    @15,@03                 RESTORE RETURN CODE     @G24LB18 00184000
         LR    @00,@02                 RESTORE REASON CODE     @G24LB18 00185000
         L     @14,12(@13)             RESTORE RETURN ADDR     @G24LB18 00186000
         LM    @01,@12,24(@13)         RESTORE CALLER'S REGS   @G24LB18 00187000
* RETURN TO MSSC SVC                                                    00188000
         BR    @14                     RETURN TO MSSC SVC      @G24LB18 00189000
*   /*****************************************************************/ 00190000
*   /*                                                               */ 00191000
*   /* END OF MAINLINE PROCESSING FOR ICBVSC00. THE          @G24LB18*/ 00192000
*   /* REMAINDER OF THE MODULE CONSISTS OF INTERNAL          @G24LB18*/ 00193000
*   /* PROCEDURES FOR ICBVSC00.                              @G24LB18*/ 00194000
