*                                                                  0333 01056000
*   /*****************************************************************/ 01057000
*   /*                                                               */ 01058000
*   /* DETERMINE THE NUMBER OF VOL SERS IN THE JFCB. PASS    @G24LB18*/ 01059000
*   /* EACH VOL SER TO THE UNIT PROCESSOR (ICBVUP01) TO      @G24LB18*/ 01060000
*   /* DETERMINE WHICH UCB IT IS MOUNTED ON.                 @G24LB18*/ 01061000
*   /*                                                               */ 01062000
*   /*****************************************************************/ 01063000
*                                                                  0333 01064000
*   SAVERET2=REG14;                 /* SAVE RETURN ADDR      @G24LB18*/ 01065000
         ST    REG14,SAVERET2                                      0333 01066000
*   NVOL=JFCBNVOL;                  /* GET NO. OF VOL SERS   @G24LB18*/ 01067000
         L     @10,JFCBPTR                                         0334 01068000
         SLR   NVOL,NVOL                                           0334 01069000
