*     NVOL=FIVE;                    /* SET NO. OF VOLSERS=5  @G24LB18*/ 01075000
         LR    NVOL,@10                                            0336 01076000
*   UNITP=''B;                      /* CLEAR UNITP PARM LIST @G24LB18*/ 01077000
@RF00335 LA    @10,UNITRPL                                         0337 01078000
         XC    UNITP(17,@10),UNITP(@10)                            0337 01079000
*   UNIFMTD=ON;                     /* SET FUNCTION FLAG     @G24LB18*/ 01080000
         OI    UNIFMTD(@10),B'10000000'                            0338 01081000
*   INDEX=ONE;                      /* INITIALIZE INDEX=1    @G24LB18*/ 01082000
         MVI   INDEX,X'01'                                         0339 01083000
*   DO WHILE NVOL>ZERO;             /* DO FOR EACH VOL SER   @G24LB18*/ 01084000
         B     @DE00340                                            0340 01085000
@DL00340 DS    0H                                                  0341 01086000
*     UNIVOL=JFCAVOLS(INDEX);       /* MOVE IN VOL SER       @G24LB18*/ 01087000
         LA    @10,UNITRPL                                         0341 01088000
         SLR   @01,@01                                             0341 01089000
         IC    @01,INDEX                                           0341 01090000
         MH    @01,@CH01185                                        0341 01091000
         L     @07,JFCBPTR                                         0341 01092000
         ALR   @07,@01                                             0341 01093000
         MVC   UNIVOL(6,@10),JFCAVOLS-6(@07)                       0341 01094000
*     CALL ICBVUP01(UNITP,VVICB);   /* CALL UNIT PROCESSOR   @G24LB18*/ 01095000
*                                                                  0342 01096000
