*/*                                                          @G24LB18*/ 01500000
*/* PROCEDURE NAME - GETRPLV                                 @G24LB18*/ 01501000
*/*                                                          @G24LB18*/ 01502000
*/* FUNCTION - THIS SUBROUTINE IS CALLED WHEN STORAGE IS     @G24LB18*/ 01503000
*/*            REQUIRED FOR AN RPLV. IT GETMAINS STORAGE     @G24LB18*/ 01504000
*/*            FOR THE RPLV, SETS UP ADDRESSABILITY TO IT,   @G24LB18*/ 01505000
*/*            CLEARS IT, AND INITIALIZES THE COMMON FIELDS. @G24LB18*/ 01506000
*/*            IF THE GETMAIN FAILS, THE ROUTINE ISSUES A    @G24LB18*/ 01507000
*/*            CONSOLE ERROR MESSAGE AND RETURNS A RETURN    @G24LB18*/ 01508000
*/*            CODE OF 4.                                    @G24LB18*/ 01509000
*/*                                                          @G24LB18*/ 01510000
*/* INPUT - LENGTH OF STORAGE NEEDED IN FIELD GETLEN AND     @G24LB18*/ 01511000
*/*         ASCB ADDRESS IN REG 7.                           @G24LB18*/ 01512000
*/*                                                          @G24LB18*/ 01513000
*/* OUTPUT - IF GETMAIN IS SUCCESSFUL, AN INITIALIZED RPLV   @G24LB18*/ 01514000
*/*          POINTED TO BY RPLVPTR IS RETURNED ALONG WITH    @G24LB18*/ 01515000
*/*          A ZERO RETURN CODE IN REG 15.                   @G24LB18*/ 01516000
*/*                                                          @G24LB18*/ 01517000
*/*          IF GETMAIN FAILS, CONSOLE ERROR MESSAGE ICB409I @G24LB18*/ 01518000
*/*          'NO STORAGE AVAILABLE' IS ISSUED AND RETURN     @G24LB18*/ 01519000
*/*          CODE = 4 IS RETURNED IN REG 15.                 @G24LB18*/ 01520000
*/*                                                          @G24LB18*/ 01521000
*/*********************************************************  @G24LB18*/ 01522000
*                                                                  0443 01523000
*GETRPLV:                                                          0443 01524000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/ 01525000
*                                                                  0443 01526000
GETRPLV  DS    0H                                                  0444 01527000
*   /*****************************************************************/ 01528000
*   /*                                                               */ 01529000
*   /* ISSUE GETMAIN FROM SUBPOOL 241 FOR THE RPLV. IF THE   @G24LB18*/ 01530000
*   /* GETMAIN FAILS, ISSUE CONSOLE ERROR MESSAGE ICB409I    @G24LB18*/ 01531000
*   /* 'NO STORAGE AVAILABLE', SET RETURN CODE               @G24LB18*/ 01532000
*   /* OF 4 IN REG 15, AND RETURN TO CALLER.                 @G24LB18*/ 01533000
*   /*                                                               */ 01534000
*   /*****************************************************************/ 01535000
*                                                                  0444 01536000
*   RFY                                                            0444 01537000
*     REG1 RSTD;                    /* RESTRICT REG 1        @G24LB18*/ 01538000
*   REG0=GETLEN;                    /* SET GETMAIN LENGTH    @G24LB18*/ 01539000
         LR    REG0,GETLEN                                         0445 01540000
*LGET1:                                                            0446 01541000
*   GEN(GETMAIN RC,LV=(0),SP=241);  /* GETMAIN RPLV          @G24LB18*/ 01542000
LGET1    GETMAIN RC,LV=(0),SP=241                                       01543000
*   IF RETCODE^=ZERO THEN           /* STORAGE GOTTEN?       @G24LB18*/ 01544000
         LTR   RETCODE,RETCODE                                     0447 01545000
         BZ    @RF00447                                            0447 01546000
*     DO;                           /* NO                    @G24LB18*/ 01547000
*       REG1=ADDR(MSG1);            /* POINT TO ERROR MSG    @G24LB18*/ 01548000
         LA    REG1,MSG1                                           0449 01549000
*LWTO2:                                                            0450 01550000
*       GEN(WTO MF=(E,(REG1)));     /* SEND ERROR MESSAGE    @G24LB18*/ 01551000
LWTO2    WTO MF=(E,(REG1))                                              01552000
*       RETCODE=FOUR;               /* SET RETURN CODE = 4   @G24LB18*/ 01553000
         LA    RETCODE,4                                           0451 01554000
*       RETURN;                     /* RETURN TO CALLER      @G24LB18*/ 01555000
@EL00008 DS    0H                                                  0452 01556000
@EF00008 DS    0H                                                  0452 01557000
@ER00008 BR    @14                                                 0452 01558000
*     END;                          /*                       @G24LB18*/ 01559000
*                                                                  0454 01560000
*   /*****************************************************************/ 01561000
*   /*                                                               */ 01562000
*   /* SET UP ADDRESSABILITY TO THE RPLV, CLEAR IT, AND      @G24LB18*/ 01563000
*   /* INITIALIZE THE COMMON FIELDS IN IT. SET RETURN CODE   @G24LB18*/ 01564000
*   /* OF 0 IN REG 15 AND RETURN TO CALLER.                  @G24LB18*/ 01565000
*   /*                                                               */ 01566000
*   /*****************************************************************/ 01567000
*                                                                  0454 01568000
*   RPLVPTR=REG1;                   /* BASE RPLV             @G24LB18*/ 01569000
@RF00447 LR    RPLVPTR,REG1                                        0454 01570000
*   RPLV=RPLV&&RPLV;                /* CLEAR RPLV BASE       @G24LB18*/ 01571000
         XC    RPLV(132,RPLVPTR),RPLV(RPLVPTR)                     0455 01572000
*   RPLVTLEN=GETLEN;                /* SET RPLV LENGTH       @G24LB18*/ 01573000
         STH   GETLEN,RPLVTLEN(,RPLVPTR)                           0456 01574000
*   RPLASCBP=ASCBSAVE;              /* SET ASCB ADDRESS      @G24LB18*/ 01575000
         L     @10,CVTPTR                                          0457 01576000
         L     @10,CVTTCBP(,@10)                                   0457 01577000
         L     @10,ASCBSAVE(,@10)                                  0457 01578000
         ST    @10,RPLASCBP(,RPLVPTR)                              0457 01579000
*   RETCODE=ZERO;                   /* SET RETURN CODE = 0   @G24LB18*/ 01580000
         SLR   RETCODE,RETCODE                                     0458 01581000
*   RFY                                                            0459 01582000
*     REG1 UNRSTD;                  /* UNRESTRICT REG 1      @G24LB18*/ 01583000
*   RETURN;                         /* RETURN TO CALLER      @G24LB18*/ 01584000
         B     @EL00008                                            0460 01585000
*   END GETRPLV;                    /* END GETMAIN RTN       @G24LB18*/ 01586000
*                                                                  0462 01587000
*/*********************************************************  @G24LB18*/ 01588000
*/*                                                          @G24LB18*/ 01589000
*/* PROCEDURE NAME - QUERPLV                                 @G24LB18*/ 01590000
*/*                                                          @G24LB18*/ 01591000
*/* FUNCTION - THIS SUBROUTINE IS CALLED WHEN A REQUEST IS   @G24LB18*/ 01592000
*/*            TO BE ADDED TO THE MSVC SUBTASK QUEUES SO     @G24LB18*/ 01593000
*/*            THAT THE REQUESTED FUNCTION CAN BE COMPLETED  @G24LB18*/ 01594000
*/*            BY THE MSVC SUBTASK. THE ROUTINE ADDS THE     @G24LB18*/ 01595000
*/*            RPLV TO THE MSVC HIGH PRIORITY QUEUE, WAITS   @G24LB18*/ 01596000
*/*            ON SUBTASK COMPLETION IF REQUESTED, AND       @G24LB18*/ 01597000
*/*            RETURNS TO THE CALLER.                        @G24LB18*/ 01598000
*/*                                                          @G24LB18*/ 01599000
*/* INPUT - THE RPLV AND THE VVICB.                          @G24LB18*/ 01600000
*/*                                                          @G24LB18*/ 01601000
*/* OUTPUT - THE RPLV IS ADDED TO THE MSVC SUBTASK HIGH      @G24LB18*/ 01602000
*/*          PRIORITY QUEUE.                                 @G24LB18*/ 01603000
*/*                                                          @G24LB18*/ 01604000
*/*********************************************************  @G24LB18*/ 01605000
*                                                                  0462 01606000
*QUERPLV:                                                          0462 01607000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @G24LB18*/ 01608000
QUERPLV  DS    0H                                                  0463 01609000
*                                                                  0463 01610000
*   /*****************************************************************/ 01611000
*   /*                                                               */ 01612000
*   /* SAVE THE 'WAIT' OR 'NO WAIT' INDICATOR FROM THE RPLV. @G24LB18*/ 01613000
*   /*                                                               */ 01614000
*   /*****************************************************************/ 01615000
*                                                                  0463 01616000
*   REG3=ONE;                       /* SET 'NO WAIT'         @G24LB18*/ 01617000
         LA    REG3,1                                              0463 01618000
*   IF RPLVPOST=ON THEN             /* IS WAIT REQUESTED?    @G24LB18*/ 01619000
         TM    RPLVPOST(RPLVPTR),B'01000000'                       0464 01620000
         BNO   @RF00464                                            0464 01621000
*     REG3=ZERO;                    /* YES - SET 'WAIT'      @G24LB18*/ 01622000
         SLR   REG3,REG3                                           0465 01623000
*                                                                  0466 01624000
*   /*****************************************************************/ 01625000
*   /*                                                               */ 01626000
*   /* GET THE HIGH PRIORITY QUEUE POINTER FROM THE VVICB    @G24LB18*/ 01627000
*   /* AND CHAIN THE RPLV TO THE HEAD OF THE QUEUE USING     @G24LB18*/ 01628000
*   /* COMPARE AND SWAP.                                     @G24LB18*/ 01629000
*   /*                                                               */ 01630000
*   /*****************************************************************/ 01631000
*                                                                  0466 01632000
*   RFY                                                            0466 01633000
*     REG1 RSTD;                    /* RESTRICT REG 1        @G24LB18*/ 01634000
@RF00464 DS    0H                                                  0467 01635000
*   RFY                                                            0467 01636000
*     REG2 RSTD;                    /* RESTRICT REG 2        @G24LB18*/ 01637000
*ADDRA:                                                            0468 01638000
*   REG2=ADDR(VVIQRPL1);            /* POINT TO QUEUE HEAD   @G24LB18*/ 01639000
ADDRA    L     @10,VVIPTR                                          0468 01640000
         LA    REG2,VVIQRPL1(,@10)                                 0468 01641000
*   REG1=VVIQRPL1;                  /* POINT TO 1ST RPLV     @G24LB18*/ 01642000
         L     REG1,VVIQRPL1(,@10)                                 0469 01643000
*   RPLVQPTR=REG1;                  /* SET FORWARD CHAIN PTR @G24LB18*/ 01644000
         ST    REG1,RPLVQPTR(,RPLVPTR)                             0470 01645000
*   GEN(CS    REG1,RPLVPTR,M0(REG2));/* COMPARE AND SWAP     @G24LB18*/ 01646000
         CS    REG1,RPLVPTR,M0(REG2)                                    01647000
*   BC(FOUR,ADDRA);                 /* IF FAIL, REPEAT       @G24LB18*/ 01648000
         BC    4,ADDRA                                             0472 01649000
*                                                                  0473 01650000
*   /*****************************************************************/ 01651000
*   /*                                                               */ 01652000
*   /* INITIALIZE THE VVICB FOR A CROSS MEMORY POST.         @G24LB18*/ 01653000
*   /*                                                               */ 01654000
*   /*****************************************************************/ 01655000
*                                                                  0473 01656000
*   VVIRES4=ADDR(VVIECB1);          /* SET PARM LIST ECB     @G24LB18*/ 01657000
