*                 DO;               /*                       @OZ09548*/ 00169000
*                   CALL CHKSSID;   /* DEFINED BY SSID       @YD00113*/ 00170000
         BAL   @14,CHKSSID                                         0050 00171000
*                   CALL CHECKUSE;  /* MULTIPLE USAGES       @OZ09548*/ 00172000
         BAL   @14,CHECKUSE                                        0051 00173000
*                 END;              /*                       @OZ09548*/ 00174000
*               ELSE                                               0053 00175000
*                 ;                 /*                       @YD00113*/ 00176000
@RF00048 DS    0H                                                  0054 00177000
*             END;                  /*                       @YD00113*/ 00178000
*           ELSE                                                   0055 00179000
*             ;                     /*                       @YD00113*/ 00180000
@RF00045 DS    0H                                                  0056 00181000
*         END;                      /*                       @YD00113*/ 00182000
*       ELSE                                                       0057 00183000
*         ;                         /*                       @YD00113*/ 00184000
@RF00032 DS    0H                                                  0058 00185000
*     END;                          /* DO DEVINDEX           @YD00113*/ 00186000
         LA    @10,1                                               0058 00187000
         AL    @10,DEVINDEX                                        0058 00188000
         ST    @10,DEVINDEX                                        0058 00189000
         CH    @10,@CH00254                                        0058 00190000
         BNH   @DL00031                                            0058 00191000
*   END;                            /* DO DSINDEX            @YD00113*/ 00192000
         LA    @10,1                                               0059 00193000
