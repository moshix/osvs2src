         LR    @15,@05                                             0032 00067000
         SLA   @15,6                                               0032 00068000
         ALR   @15,@10                                             0032 00069000
         MH    @15,@CH00255                                        0032 00070000
         L     @14,COMPTR2(,R4)                                    0032 00071000
         ST    @15,@TF00001                                        0032 00072000
         ALR   @15,@14                                             0032 00073000
         AL    @15,@CF00540                                        0032 00074000
         TM    USESPEC-464(@15),B'10000000'                        0032 00075000
         BNO   @RF00032                                            0032 00076000
*         DO;                       /*                       @Y30LB12*/ 00077000
*                                                                  0033 00078000
*           /*********************************************************/ 00079000
*           /*                                                       */ 00080000
*           /* *****************************************     @YD00113*/ 00081000
*           /* IF THE USE OF THE ADDRESS IS SPECIFIED        @YD00113*/ 00082000
*           /* AS 'REAL', CHECK THAT THE PHYSICAL            @YD00113*/ 00083000
*           /* DEVICE IS PRESENT IN THE CONFIGURATION,       @YD00113*/ 00084000
*           /* AND, IF THE DEVICE IS PHYSICALLY              @YD00113*/ 00085000
*           /* PRESENT, FURTHER CHECK THAT THE DEVICE        @YD00113*/ 00086000
*           /* IS SPECIFIED AS REAL ON THE DASD CONTROL      @YD00113*/ 00087000
*           /* STATEMENT.                                    @YD00113*/ 00088000
*           /* *****************************************     @YD00113*/ 00089000
*           /*                                                       */ 00090000
*           /*********************************************************/ 00091000
*                                                                  0034 00092000
*           IF USEREAL(DSINDEX,DEVINDEX)='1'B&/* REAL        @Y30LB12*/ 00093000
*               (BINTIND='0'B|(DSINDEX>6|(DEVINDEX^=1&DEVINDEX^=2&/*    00094000
*                                                            @OZ28368*/ 00095000
*               DEVINDEX^=9&DEVINDEX^=10)))/* CHECK FOR TABPTH     0034 00096000
*                                                            @OZ28368*/ 00097000
*             THEN                  /*                       @Y30LB12*/ 00098000
