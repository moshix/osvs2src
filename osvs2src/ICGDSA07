         AL    @14,@CF00540                                        0034 00100000
         TM    USEREAL-464(@14),B'00010000'                        0034 00101000
         BNO   @RF00034                                            0034 00102000
         TM    BINTIND(R4),B'00000100'                             0034 00103000
         BZ    @RT00034                                            0034 00104000
         CH    @05,@CH00227                                        0034 00105000
         BH    @RT00034                                            0034 00106000
         CH    @10,@CH00042                                        0034 00107000
         BE    @RF00034                                            0034 00108000
         CH    @10,@CH00050                                        0034 00109000
         BE    @RF00034                                            0034 00110000
         CH    @10,@CH00112                                        0034 00111000
         BE    @RF00034                                            0034 00112000
         CH    @10,@CH00480                                        0034 00113000
         BE    @RF00034                                            0034 00114000
@RT00034 DS    0H                                                  0035 00115000
*             DO;                   /*                       @Y30LB12*/ 00116000
*               CALL CHKTHERE;      /* PHYSICAL SPINDLE      @Y30LB12*/ 00117000
         BAL   @14,CHKTHERE                                        0036 00118000
*               IF ERROR1='0'B      /*                       @YD00113*/ 00119000
*                 THEN              /*                       @YD00113*/ 00120000
         TM    ERROR1,B'01000000'                                  0037 00121000
         BNZ   @RF00037                                            0037 00122000
*                 DO;               /*                       @OZ09548*/ 00123000
*                   CALL CHKREAL;   /* DEFINED AS REAL       @YD00113*/ 00124000
