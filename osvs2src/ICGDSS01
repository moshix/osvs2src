*         DO;                       /*                       @Y30LB12*/ 00165000
*                                                                  0056 00166000
*           /*********************************************************/ 00167000
*           /*                                                       */ 00168000
*           /* ***********************************           @YD00113*/ 00169000
*           /* ICG223I VALUE ERROR FOR SDG                   @YD00113*/ 00170000
*           /* ***********************************           @YD00113*/ 00171000
*           /*                                                       */ 00172000
*           /*********************************************************/ 00173000
*                                                                  0057 00174000
