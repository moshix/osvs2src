*/*                                                          @Y30LB12*/ 00058000
*/* PROCEDURE NAME - S01INIT                                 @Y30LB12*/ 00059000
*/*                                                          @Y30LB12*/ 00060000
*/* FUNCTION - THIS PROCEDURE INITIALIZES THE LOCAL WORK     @Y30LB12*/ 00061000
*/*            AREA RSGWORKA AND EXTRACTS THE SDG NUMBER     @YD00113*/ 00062000
*/*            FROM THE SUFFIX OF THE COMMAND WORD.          @Y30LB12*/ 00063000
*/*            IT THEN TESTS THE LENGTH OF THE COMMAND AND   @YD00113*/ 00064000
*/*            SETS AN ERROR FLAG IF THE LENGTH IS TOO LONG  @YD00113*/ 00065000
*/*            OR TOO SHORT.  IF A LENGTH ERROR IS DETECTED  @YD00113*/ 00066000
*/*            AN ERROR MESSAGE IS PRINTED VIA 'PUTIT2'.     @YD00113*/ 00067000
*/*            IF NO LENGTH ERROR IS FOUND, THE VALUE        @YD00113*/ 00068000
*/*            RETURNED FOR THE SDG IS TESTED.  IF THE VALUE @YD00113*/ 00069000
*/*            IS GREATER THAN 27 (DEC.), ERROR MESSAGE      @YD00113*/ 00070000
*/*            ICG223I IS PRINTED VIA 'PUTIT1'.              @YD00113*/ 00071000
*/*                                                          @Y30LB12*/ 00072000
*/********************************************************** @YD00113*/ 00073000
*                                                                  0027 00074000
*S01INIT:                                                          0027 00075000
*   PROCEDURE;                      /*                       @Y30LB12*/ 00076000
S01INIT  STM   @14,@12,@SA00002                                    0027 00077000
*   RSGMSGID='00'X;                 /* CONTROL FLAGS         @Y30LB12*/ 00078000
         MVI   RSGMSGID,X'00'                                      0028 00079000
*   HAVES='00'X;                    /* 'FOUND IT' FLAGS      @Y30LB12*/ 00080000
         MVI   HAVES,X'00'                                         0029 00081000
*   WRKRSGNO=0;                     /* INDEX TO BUILDRSG     @Y30LB12*/ 00082000
         SLR   @10,@10                                             0030 00083000
         STH   @10,WRKRSGNO                                        0030 00084000
*   WORKRSG='0000'X;                /* SDG NUMBER            @YD00113*/ 00085000
         MVC   WORKRSG(2),@CB00349                                 0031 00086000
*   DO I=1 TO 4;                    /*                       @OZ05139*/ 00087000
         LA    @10,1                                               0032 00088000
         STH   @10,I                                               0032 00089000
@DL00032 DS    0H                                                  0033 00090000
*     WKSTAMPS(I)='00'X;            /* CLEAR LRUGROUPS       @OZ05139*/ 00091000
         LA    @07,WKSTAMPS-1(@10)                                 0033 00092000
         MVI   0(@07),X'00'                                        0033 00093000
*   END;                            /*                       @OZ05139*/ 00094000
         AH    @10,@CH00042                                        0034 00095000
         STH   @10,I                                               0034 00096000
         CH    @10,@CH00068                                        0034 00097000
         BNH   @DL00032                                            0034 00098000
*   IF SCANDATA(4)^='40'X           /*                       @Y30LB12*/ 00099000
*     THEN                          /*                       @Y30LB12*/ 00100000
         L     @10,STARTCOL(,R4)                                   0035 00101000
         CLI   SCANDATA+3(@10),X'40'                               0035 00102000
         BE    @RF00035                                            0035 00103000
*     IF SCANDATA(5)^='40'X         /*                       @Y30LB12*/ 00104000
*       THEN                        /*                       @Y30LB12*/ 00105000
         CLI   SCANDATA+4(@10),X'40'                               0036 00106000
         BE    @RF00036                                            0036 00107000
*       IF SCANDATA(6)='40'X        /*                       @Y30LB12*/ 00108000
*         THEN                      /*                       @Y30LB12*/ 00109000
         CLI   SCANDATA+5(@10),X'40'                               0037 00110000
         BNE   @RF00037                                            0037 00111000
*         DO;                       /*                       @Y30LB12*/ 00112000
*           WORKRSG=SCANDATA(4:5);  /*                       @Y30LB12*/ 00113000
         MVC   WORKRSG(2),SCANDATA+3(@10)                          0039 00114000
*           CALL CONVDEC;           /* CONVERT TO BINARY     @Y30LB12*/ 00115000
         BAL   @14,CONVDEC                                         0040 00116000
*         END;                      /*                       @Y30LB12*/ 00117000
*       ELSE                        /*                       @Y30LB12*/ 00118000
*         ERROR1='1'B;              /* COMMAND TOO LONG      @Y30LB12*/ 00119000
         B     @RC00037                                            0042 00120000
@RF00037 OI    ERROR1,B'00010000'                                  0042 00121000
*     ELSE                          /*                       @Y30LB12*/ 00122000
*       DO;                         /*                       @Y30LB12*/ 00123000
         B     @RC00036                                            0043 00124000
@RF00036 DS    0H                                                  0044 00125000
*         WORKRSG(1)='F0'X;         /*                       @Y30LB12*/ 00126000
         MVI   WORKRSG,X'F0'                                       0044 00127000
*         WORKRSG(2)=SCANDATA(4);   /*                       @Y30LB12*/ 00128000
         L     @10,STARTCOL(,R4)                                   0045 00129000
         MVC   WORKRSG+1(1),SCANDATA+3(@10)                        0045 00130000
*         CALL CONVDEC;             /* CONVERT TO BINARY     @Y30LB12*/ 00131000
         BAL   @14,CONVDEC                                         0046 00132000
*       END;                        /*                       @Y30LB12*/ 00133000
*   ELSE                            /*                       @Y30LB12*/ 00134000
*     ERROR1='1'B;                  /* COMMAND TOO SHORT     @Y30LB12*/ 00135000
         B     @RC00035                                            0048 00136000
@RF00035 OI    ERROR1,B'00010000'                                  0048 00137000
*   IF ERROR1='1'B                  /* LENGTH ERROR          @Y30LB12*/ 00138000
*     THEN                          /*                       @Y30LB12*/ 00139000
@RC00035 TM    ERROR1,B'00010000'                                  0049 00140000
         BNO   @RF00049                                            0049 00141000
*     DO;                           /*                       @Y30LB12*/ 00142000
*                                                                  0050 00143000
*       /*************************************************************/ 00144000
*       /*                                                           */ 00145000
*       /* *****************************************         @YD00113*/ 00146000
*       /* ICG204I INVALID COMMAND WORD                      @Y30LB12*/ 00147000
*       /* *****************************************         @YD00113*/ 00148000
*       /*                                                           */ 00149000
*       /*************************************************************/ 00150000
*                                                                  0051 00151000
*       MSGNO=ICG204I;              /* ID FOR ICGDS204       @Y30LB12*/ 00152000
         MVC   MSGNO(2,R4),ICG204I                                 0051 00153000
*       CALL PUTIT2;                /* TO MESSAGE WRITER     @Y30LB12*/ 00154000
         BAL   @14,PUTIT2                                          0052 00155000
*     END;                          /*                       @Y30LB12*/ 00156000
*   ELSE                            /*                       @Y30LB12*/ 00157000
*     DO;                           /*                       @Y30LB12*/ 00158000
         B     @RC00049                                            0054 00159000
@RF00049 DS    0H                                                  0055 00160000
