*           CALL PUTIT1;            /* TO MESSAGE WRITER     @Y30LB12*/ 00179000
         BAL   @14,PUTIT1                                          0059 00180000
*         END;                      /*                       @Y30LB12*/ 00181000
*       ELSE                        /*                       @Y30LB12*/ 00182000
*         WRKRSGNO=WORKRSG+1;       /*                       @Y30LB12*/ 00183000
         B     @RC00055                                            0061 00184000
@RF00055 LA    @10,1                                               0061 00185000
