*/*                                                          @Y30LB12*/ 00197000
*/* PROCEDURE NAME - S01SCAN                                 @Y30LB12*/ 00198000
*/*                                                          @Y30LB12*/ 00199000
*/* FUNCTION - SCAN KEYWORDS AND CALL PROPER ROUTINE TO      @Y30LB12*/ 00200000
*/*            HANDLE PARAMETERS.                            @Y30LB12*/ 00201000
*/*                                                          @Y30LB12*/ 00202000
*/********************************************************** @YD00113*/ 00203000
*                                                                  0064 00204000
*S01SCAN:                                                          0064 00205000
*   PROCEDURE;                      /*                       @Y30LB12*/ 00206000
S01SCAN  STM   @14,@12,@SA00003                                    0064 00207000
*   DO WHILE(LASTPARM^='1'B);       /* TILL NO MORE DATA     @Y30LB12*/ 00208000
         B     @DE00065                                            0065 00209000
@DL00065 DS    0H                                                  0066 00210000
*     CALL READS99;                 /* EXTRACT KEYWORD       @Y30LB12*/ 00211000
         BAL   @14,READS99                                         0066 00212000
*     KNOWN='0'B;                   /* RESET REC. FLAG       @Y30LB12*/ 00213000
         NI    KNOWN,B'01111111'                                   0067 00214000
*     IF LASTPARM='1'B              /* IF LAST OF DATA       @Y30LB12*/ 00215000
*       THEN                        /*                       @Y30LB12*/ 00216000
         TM    LASTPARM(R4),B'00001000'                            0068 00217000
         BO    @RT00068                                            0068 00218000
*       RETURN;                     /* RET. TO MAINLINE      @Y30LB12*/ 00219000
*     ELSE                                                         0070 00220000
*       ;                           /*                       @Y30LB12*/ 00221000
*                                                                  0070 00222000
*     /***************************************************************/ 00223000
*     /*                                                             */ 00224000
*     /* ***********************************************     @YD00113*/ 00225000
*     /* IF THE FIELD JUST EXTRACTED IS NOT A KEYWORD,       @Y30LB12*/ 00226000
*     /* WRITE MESSAGE ICG209I EXPECTED, BUT DID NOT         @Y30LB12*/ 00227000
*     /* FIND, KEYWORD', AND RETURN TO MAINLINE              @Y30LB12*/ 00228000
*     /* ***********************************************     @YD00113*/ 00229000
*     /*                                                             */ 00230000
*     /***************************************************************/ 00231000
*                                                                  0071 00232000
*     IF KEYBIT^='1'B               /* SET BY ICGDSS99       @Y30LB12*/ 00233000
*       THEN                        /*                       @Y30LB12*/ 00234000
         TM    KEYBIT(R4),B'01000000'                              0071 00235000
         BO    @RF00071                                            0071 00236000
*       DO;                         /*                       @Y30LB12*/ 00237000
*         MSGNO=ICG209I;            /* ID FOR ICGDS209       @Y30LB12*/ 00238000
         MVC   MSGNO(2,R4),ICG209I                                 0073 00239000
*         CALL PUTIT2;              /* TO MESSAGE WRITER     @Y30LB12*/ 00240000
         BAL   @14,PUTIT2                                          0074 00241000
*         RETURN;                   /* RET. TO MAINLINE      @Y30LB12*/ 00242000
@EL00003 DS    0H                                                  0075 00243000
@EF00003 DS    0H                                                  0075 00244000
@ER00003 LM    @14,@12,@SA00003                                    0075 00245000
         BR    @14                                                 0075 00246000
*       END;                        /*                       @Y30LB12*/ 00247000
*     ELSE                                                         0077 00248000
*       ;                           /*                       @Y30LB12*/ 00249000
*                                                                  0077 00250000
@RF00071 DS    0H                                                  0078 00251000
*     /***************************************************************/ 00252000
*     /*                                                             */ 00253000
*     /* ***********************************************     @YD00113*/ 00254000
*     /* IF THE FIELD LAST SCANNED WAS THE KEYWORD           @Y30LB12*/ 00255000
*     /* ACTPAGE, SET THE 'KNOWN' FLAG ON, AND CALL          @YD00140*/ 00256000
*     /* 'S01THOLD' TO GET THE PARAMETERS                    @YD00113*/ 00257000
*     /* ***********************************************     @YD00113*/ 00258000
*     /*                                                             */ 00259000
*     /***************************************************************/ 00260000
*                                                                  0078 00261000
*     IF SCANDATA(1:8)='ACTPAGE='   /*                       @YD00140*/ 00262000
*       THEN                        /*                       @Y30LB12*/ 00263000
         L     @10,STARTCOL(,R4)                                   0078 00264000
         CLC   SCANDATA(8,@10),@CC00363                            0078 00265000
         BNE   @RF00078                                            0078 00266000
*       DO;                         /*                       @Y30LB12*/ 00267000
*         KNOWN='1'B;               /* RECOGNIZED KEYWD      @Y30LB12*/ 00268000
         OI    KNOWN,B'10000000'                                   0080 00269000
*         CALL S01THOLD;            /* TO GET PARAMETERS     @Y30LB12*/ 00270000
         BAL   @14,S01THOLD                                        0081 00271000
*       END;                        /*                       @Y30LB12*/ 00272000
*     ELSE                                                         0083 00273000
*       ;                           /*                       @Y30LB12*/ 00274000
*                                                                  0083 00275000
@RF00078 DS    0H                                                  0084 00276000
*     /***************************************************************/ 00277000
*     /*                                                             */ 00278000
*     /* ***********************************************     @YD00113*/ 00279000
*     /* IF THE FIELD LAST SCANNED WAS THE KEYWORD           @Y30LB12*/ 00280000
*     /* LRUGROUP, SET 'KNOWN' FLAG ON AND CALL              @YD00113*/ 00281000
*     /* 'S01STAMP' TO GET THE PARAMETERS                    @YD00113*/ 00282000
*     /* ***********************************************     @YD00113*/ 00283000
*     /*                                                             */ 00284000
*     /***************************************************************/ 00285000
*                                                                  0084 00286000
*     IF SCANDATA(1:9)='LRUGROUP='  /*                       @Y30LB12*/ 00287000
*       THEN                        /*                       @Y30LB12*/ 00288000
         L     @10,STARTCOL(,R4)                                   0084 00289000
         CLC   SCANDATA(9,@10),@CC00365                            0084 00290000
         BNE   @RF00084                                            0084 00291000
*       DO;                         /*                       @Y30LB12*/ 00292000
*         KNOWN='1'B;               /* RECOGNIZED KEYWD      @Y30LB12*/ 00293000
         OI    KNOWN,B'10000000'                                   0086 00294000
*         CALL S01STAMP;            /* TO GET PARAMETERS     @Y30LB12*/ 00295000
         BAL   @14,S01STAMP                                        0087 00296000
*       END;                        /*                       @Y30LB12*/ 00297000
*     ELSE                                                         0089 00298000
*       ;                           /*                       @Y30LB12*/ 00299000
*                                                                  0089 00300000
@RF00084 DS    0H                                                  0090 00301000
*     /***************************************************************/ 00302000
*     /*                                                             */ 00303000
*     /* ***********************************************     @YD00113*/ 00304000
*     /* THE KEYWORD WAS NOT RECOGNIZED AS EITHER            @Y30LB12*/ 00305000
*     /* OF THE ABOVE, SET 'KNOWN' FLAGG TO ZERO (OFF)       @YD00113*/ 00306000
*     /* AND CALL 'UNKNOWN' FOR ERROR MESSAGE AND            @YD00113*/ 00307000
*     /* SET UP FOR NEXT KEYWORD.                            @YD00113*/ 00308000
*     /* ***********************************************     @YD00113*/ 00309000
*     /*                                                             */ 00310000
*     /***************************************************************/ 00311000
*                                                                  0090 00312000
*     IF KNOWN='0'B                 /* UNKNOWN KEYWORD       @Y30LB12*/ 00313000
*       THEN                        /*                       @Y30LB12*/ 00314000
         TM    KNOWN,B'10000000'                                   0090 00315000
         BNZ   @RF00090                                            0090 00316000
*       CALL UNKNOWN;               /* TO WRITE MSG, ETC     @Y30LB12*/ 00317000
         BAL   @14,UNKNOWN                                         0091 00318000
*     ELSE                                                         0092 00319000
*       ;                           /*                       @Y30LB12*/ 00320000
@RF00090 DS    0H                                                  0093 00321000
*   END;                            /* END OF DO WHILE       @Y30LB12*/ 00322000
@DE00065 TM    LASTPARM(R4),B'00001000'                            0093 00323000
         BNO   @DL00065                                            0093 00324000
*   END S01SCAN;                    /* RET. TO MAINLINE      @Y30LB12*/ 00325000
         B     @EL00003                                            0094 00326000
*                                                                  0095 00327000
*/********************************************************** @YD00113*/ 00328000
*/*                                                          @Y30LB12*/ 00329000
*/* PROCEDURE NAME - S01THOLD                                @Y30LB12*/ 00330000
*/*                                                          @Y30LB12*/ 00331000
*/* FUNCTION - EXTRACT TWO PARAMETERS, THE LOWER AND UPPER   @Y30LB12*/ 00332000
*/*            THRESHOLD VALUES FOR THE THE LRU MECHANISM,   @Y30LB12*/ 00333000
*/*            FROM THE CONTROL STATEMENT.                   @Y30LB12*/ 00334000
*/*            THE ACTUAL EXTRACTION AND FORMATTING OF THE   @Y30LB12*/ 00335000
*/*            DATA IS PERFORMED IN THE CALLED PROCEDURE     @Y30LB12*/ 00336000
*/*            CHECKDEC.  IF AN ERROR IS FOUND IN THE DATA,  @Y30LB12*/ 00337000
*/*            EITHER THE ERROR MESSAGE IS WRITTEN IN        @Y30LB12*/ 00338000
*/*            CHECKDEC OR THE FLAG NOTPARM IS SET ON.       @Y30LB12*/ 00339000
*/*                                                          @Y30LB12*/ 00340000
*/********************************************************** @YD00113*/ 00341000
*                                                                  0095 00342000
*S01THOLD:                                                         0095 00343000
*   PROCEDURE;                      /*                       @Y30LB12*/ 00344000
S01THOLD STM   @14,@12,@SA00004                                    0095 00345000
*   NOTPARM='0'B;                   /* CONTROL FLAG OFF      @Y30LB12*/ 00346000
         NI    NOTPARM,B'11011111'                                 0096 00347000
*   PLUGIN='ACTPAGE ';              /* FOR MESSAGE TEXT      @YD00140*/ 00348000
*                                                                  0097 00349000
         MVC   PLUGIN(8),@CC00372                                  0097 00350000
*   /*****************************************************************/ 00351000
*   /*                                                               */ 00352000
*   /* **********************************************        @YD00113*/ 00353000
*   /* TEST FOR DUPLICATE KEYWORD 'ACTPAGE'. IF NO           @YD00140*/ 00354000
*   /* DUPLICATION, SET FLAG 'HAVTHOLD' ON. OTHERWISE        @YD00113*/ 00355000
*   /* ISSUE ERROR MESSAGE ICG219I VIA 'PUTIT1'              @YD00113*/ 00356000
*   /* **********************************************        @YD00113*/ 00357000
*   /*                                                               */ 00358000
*   /*****************************************************************/ 00359000
*                                                                  0098 00360000
*   IF HAVTHOLD^='1'B               /*                       @Y30LB12*/ 00361000
*     THEN                          /*                       @Y30LB12*/ 00362000
         TM    HAVTHOLD,B'10000000'                                0098 00363000
         BO    @RF00098                                            0098 00364000
*     HAVTHOLD='1'B;                /*                       @Y30LB12*/ 00365000
         OI    HAVTHOLD,B'10000000'                                0099 00366000
*   ELSE                            /*                       @Y30LB12*/ 00367000
*     DO;                           /*                       @Y30LB12*/ 00368000
*                                                                  0100 00369000
         B     @RC00098                                            0100 00370000
@RF00098 DS    0H                                                  0101 00371000
*       /*************************************************************/ 00372000
*       /*                                                           */ 00373000
*       /* *****************************************         @YD00113*/ 00374000
*       /* ICG219I DUPLICATE KEYWORD ACTPAGE                 @YD00140*/ 00375000
*       /* *****************************************         @YD00113*/ 00376000
*       /*                                                           */ 00377000
*       /*************************************************************/ 00378000
*                                                                  0101 00379000
*       OUTBUF2(1:27)=ICGDS219;     /*                       @Y30LB12*/ 00380000
         MVC   OUTBUF2(27,R4),ICGDS219                             0101 00381000
*       OUTBUF2(28:34)='ACTPAGE';   /*                       @YD00140*/ 00382000
         MVC   OUTBUF2+27(7,R4),@CC00373                           0102 00383000
*       CALL PUTIT1;                /* TO MESSAGE WRITER     @Y30LB12*/ 00384000
         BAL   @14,PUTIT1                                          0103 00385000
*     END;                          /*                       @Y30LB12*/ 00386000
*   CALL CHECKDEC;                  /* GET FIRST VALUE       @Y30LB12*/ 00387000
@RC00098 BAL   @14,CHECKDEC                                        0105 00388000
*   IF NOTPARM^='1'B                /* IS A PARAMETER        @Y30LB12*/ 00389000
*     THEN                          /*                       @Y30LB12*/ 00390000
         TM    NOTPARM,B'00100000'                                 0106 00391000
         BO    @RF00106                                            0106 00392000
*     DO;                           /*                       @Y30LB12*/ 00393000
*       WRKLOWER=WRKLOWUP;          /* SAVE LOW VALUE        @Y30LB12*/ 00394000
         MVC   WRKLOWER(1),WRKLOWUP                                0108 00395000
*       CALL CHECKDEC;              /* GET OTHER VALUE       @Y30LB12*/ 00396000
*                                                                  0109 00397000
         BAL   @14,CHECKDEC                                        0109 00398000
*       /*************************************************************/ 00399000
*       /*                                                           */ 00400000
*       /* *****************************************         @YD00113*/ 00401000
*       /* TEST 'NOTPARM' FLAG FROM 'CHECKDEC' TO            @YD00113*/ 00402000
*       /* DETERMINE IF VALUE WAS THE LOW VALUE              @YD00113*/ 00403000
*       /* PARAMETER. IF IT WAS, REISSUE 'CHECKDEC'          @YD00113*/ 00404000
*       /* TO GET THE HIGH VALUE. THE SAME TEST              @YD00113*/ 00405000
*       /* IS MADE. IF BOTH VALUES ARE OK THEY ARE           @YD00113*/ 00406000
*       /* ARE SAVED. IF EITHER VALUE IS NOT OK,             @YD00113*/ 00407000
