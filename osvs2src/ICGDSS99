*         NEXTCOL=NEXTCOL+1;        /* BUMP TO NEXT COL      @Y30LB12*/ 00283000
         AL    @10,NEXTCOL(,R4)                                    0085 00284000
         ST    @10,NEXTCOL(,R4)                                    0085 00285000
*         IF NEXTCOL=R6             /* IF NEXT IS COL 73     @Y30LB12*/ 00286000
*           THEN                    /* OVERRAN CONT COL,     @Y30LB12*/ 00287000
         CR    @10,R6                                              0086 00288000
         BNE   @RF00086                                            0086 00289000
*           CALL ICGDS203;          /* WRITE ERROR MSG       @Y30LB12*/ 00290000
         BAL   @14,ICGDS203                                        0087 00291000
