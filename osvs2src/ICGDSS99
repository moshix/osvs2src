         TM    S99FLAG,B'10000000'                                 0075 00243000
         BNO   @RF00075                                            0075 00244000
@RT00075 DS    0H                                                  0076 00245000
*       DO;                         /*                       @Y30LB12*/ 00246000
*         S99FLAG='0'B;             /* TO EXIT LOOP          @Y30LB12*/ 00247000
         NI    S99FLAG,B'01111111'                                 0077 00248000
*         NEXTCOL=NEXTCOL+1;        /* NEXT SCAN START       @Y30LB12*/ 00249000
         LA    @10,1                                               0078 00250000
         AL    @10,NEXTCOL(,R4)                                    0078 00251000
         ST    @10,NEXTCOL(,R4)                                    0078 00252000
*         PARMBIT='1'B;             /* SET PARAMETER BIT     @Y30LB12*/ 00253000
         OI    PARMBIT(R4),B'00100000'                             0079 00254000
*       END;                        /*                       @Y30LB12*/ 00255000
*     ELSE                                                         0081 00256000
*       ;                           /*                       @Y30LB12*/ 00257000
*                                                                  0081 00258000
@RF00075 DS    0H                                                  0082 00259000
*     /***************************************************************/ 00260000
*     /*                                                             */ 00261000
*     /* ***********************************************     @YD00113*/ 00262000
*     /* IF COLUMN DID NOT CONTAIN AN EQUAL SIGN, A          @Y30LB12*/ 00263000
*     /* COMMA, OR A BLANK, IT CONTAINED A CHARACTER         @Y30LB12*/ 00264000
*     /* OF KEYWORD OR PARAMETER. BUMP NEXTCOL AND           @Y30LB12*/ 00265000
*     /* SCANLEN AND CONTINUE LOOKING FOR A DELIMITER.       @Y30LB12*/ 00266000
*     /* ERROR IF INTO COLUMN 73 BEFORE DELIMITER IS         @Y30LB12*/ 00267000
*     /* FOUND                                               @Y30LB12*/ 00268000
*     /* ***********************************************     @YD00113*/ 00269000
*     /*                                                             */ 00270000
*     /***************************************************************/ 00271000
*                                                                  0082 00272000
*     IF S99FLAG='1'B               /*                       @Y30LB12*/ 00273000
*       THEN                        /*                       @Y30LB12*/ 00274000
         TM    S99FLAG,B'10000000'                                 0082 00275000
         BNO   @RF00082                                            0082 00276000
*       DO;                         /*                       @Y30LB12*/ 00277000
*         SCANLEN=SCANLEN+1;        /* BUMP LENGTH           @Y30LB12*/ 00278000
         LA    @10,1                                               0084 00279000
