*/* PROCEDURE NAME - READIT                                  @Y30LB12*/ 00308000
*/*                                                          @Y30LB12*/ 00309000
*/* FUNCTION - READ THE CONTINUATION OF THE CONTROL          @Y30LB12*/ 00310000
*/*            STATEMENT AND POSITION TO THE FIRST           @Y30LB12*/ 00311000
*/*            NON-BLANK CHARACTER AFTER POSITION 1.         @Y30LB12*/ 00312000
*/*            SET STARTCOL AND NEXTCOL EQUAL TO THAT        @Y30LB12*/ 00313000
*/*            COLUMN AND RETURN TO THE MAINLINE.            @Y30LB12*/ 00314000
*/*                                                          @Y30LB12*/ 00315000
*/*           IF COLUMN 1 CONTAINS AN ASTERISK, THE          @Y30LB12*/ 00316000
*/*           STATEMENT CONTAINS COMMENTS ONLY AND SHOULD    @Y30LB12*/ 00317000
*/*           JUST BE READ AND PRINTED AND THE NEXT          @Y30LB12*/ 00318000
*/*           RECORD READ.                                   @Y30LB12*/ 00319000
*/*                                                          @Y30LB12*/ 00320000
*/*           IF COLUMN 1 CONTAINS ANY OTHER NON-BLANK       @Y30LB12*/ 00321000
*/*           CHARACTER, TREAT THE RECORD AS AN ERROR AND    @Y30LB12*/ 00322000
*/*           TERMINATE SCAN OF THIS CONTROL STATEMENT.      @Y30LB12*/ 00323000
*/*                                                          @Y30LB12*/ 00324000
*/***********************************************************@Y30LB12*/ 00325000
*                                                                  0092 00326000
*READIT:                                                           0092 00327000
*   PROCEDURE;                      /*                       @Y30LB12*/ 00328000
READIT   STM   @14,@12,@SA00003                                    0092 00329000
*   S99READ='1'B;                   /* CONTROL FLAG          @Y30LB12*/ 00330000
         OI    S99READ,B'01000000'                                 0093 00331000
*   DO WHILE(S99READ='1'B);         /* CONTROL LOOP          @Y30LB12*/ 00332000
         B     @DE00094                                            0094 00333000
@DL00094 DS    0H                                                  0095 00334000
*     R1=ADDR(READDCB);             /* ADDRESS OF DCB        @Y30LB12*/ 00335000
         LA    R1,READDCB(,R4)                                     0095 00336000
*     R0=ADDR(INBUF);               /* ADDRESS OF BUFFER     @Y30LB12*/ 00337000
         LA    R0,INBUF(,R4)                                       0096 00338000
*     GEN(GET   (1),(0));           /* READ A RECORD         @Y30LB12*/ 00339000
         GET   (1),(0)                                                  00340000
*     PRNTCARD='1'B;                /* SET PRINT RECORD      @Y30LB12*/ 00341000
         OI    PRNTCARD(R4),B'00010000'                            0098 00342000
*     GEN(LINK  EP=ICGDSMSG);       /* GO PRINT IT           @Y30LB12*/ 00343000
         LINK  EP=ICGDSMSG                                              00344000
*     PRNTCARD='0'B;                /* PRINT RECORD OFF      @Y30LB12*/ 00345000
         NI    PRNTCARD(R4),B'11101111'                            0100 00346000
*     NEXTCOL=ADDR(INBUF);          /* SCANBUF OVERLAY       @Y30LB12*/ 00347000
         LA    @10,INBUF(,R4)                                      0101 00348000
         ST    @10,NEXTCOL(,R4)                                    0101 00349000
*     IF INBUF(1)^='*'              /* IF COMMENTS,          @Y30LB12*/ 00350000
*       THEN                        /* READ AGAIN            @Y30LB12*/ 00351000
         CLI   INBUF(R4),C'*'                                      0102 00352000
         BE    @RF00102                                            0102 00353000
*       S99READ='0'B;               /* CONTROL FLAG OFF      @Y30LB12*/ 00354000
         NI    S99READ,B'10111111'                                 0103 00355000
*     ELSE                                                         0104 00356000
*       ;                           /* NOT A COMMENT         @Y30LB12*/ 00357000
@RF00102 DS    0H                                                  0105 00358000
*   END;                            /* END OF DO WHILE       @Y30LB12*/ 00359000
@DE00094 TM    S99READ,B'01000000'                                 0105 00360000
         BO    @DL00094                                            0105 00361000
*   IF INBUF(1)='40'X               /* IF CONT RECORD,       @Y30LB12*/ 00362000
*     THEN                          /* CONTAINS A LABEL      @Y30LB12*/ 00363000
         CLI   INBUF(R4),X'40'                                     0106 00364000
         BNE   @RF00106                                            0106 00365000
*     DO;                           /*                       @Y30LB12*/ 00366000
*       DO I=1 TO 72 WHILE(SCANBUF(I)='40'X);/*              @Y30LB12*/ 00367000
         LA    I,1                                                 0108 00368000
@DL00108 L     @10,NEXTCOL(,R4)                                    0108 00369000
         ALR   @10,I                                               0108 00370000
         AL    @10,@CF00663                                        0108 00371000
         CLI   SCANBUF(@10),X'40'                                  0108 00372000
         BNE   @DC00108                                            0108 00373000
*       END;                        /*                       @Y30LB12*/ 00374000
         AH    I,@CH00042                                          0109 00375000
         CH    I,@CH00272                                          0109 00376000
         BNH   @DL00108                                            0109 00377000
@DC00108 DS    0H                                                  0110 00378000
*       IF I^=73                    /*                       @Y30LB12*/ 00379000
*         THEN                      /*                       @Y30LB12*/ 00380000
         CH    I,@CH00640                                          0110 00381000
         BE    @RF00110                                            0110 00382000
*         DO;                       /*                       @Y30LB12*/ 00383000
*           STARTCOL=ADDR(INBUF)-1+I;/*                      @Y30LB12*/ 00384000
         LA    @10,INBUF(,R4)                                      0112 00385000
         BCTR  @10,0                                               0112 00386000
