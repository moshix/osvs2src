*/* PROCEDURE NAME - DOCMD                                   @Y30LB12*/ 00579000
*/*                                                          @Y30LB12*/ 00580000
*/* FUNCTION - AT THIS POINT, SCANDATA IS POSITIONED TO      @Y30LB12*/ 00581000
*/*            POINT AT THE FIRST CHARACTER OF COMMAND WORD  @Y30LB12*/ 00582000
*/*            AND SCANBUF IS POSITIONED TO POINT AT THE     @Y30LB12*/ 00583000
*/*            FIRST CHARACTER OF KEYWORD OR COLUMN 73,      @Y30LB12*/ 00584000
*/*            WHICHEVER OCCURRED FIRST.                     @Y30LB12*/ 00585000
*/*                                                          @Y30LB12*/ 00586000
*/*            WE WILL NOW CHECK THE COMMAND WORD AGAINST    @Y30LB12*/ 00587000
*/*            KNOWN COMMAND WORDS AND LINK TO THE ROUTINE   @Y30LB12*/ 00588000
*/*            WHICH HANDLES THE MATCHING COMMAND WORD.      @Y30LB12*/ 00589000
*/*                                                          @Y30LB12*/ 00590000
*/********************************************************** @YD00113*/ 00591000
*                                                                  0139 00592000
*DOCMD:                                                            0139 00593000
*   PROCEDURE;                      /*                       @Y30LB12*/ 00594000
DOCMD    STM   @14,@12,@SA00006                                    0139 00595000
*   KNOWN='0'B;                     /* MATCH FLAG OFF        @Y30LB12*/ 00596000
         NI    KNOWN,B'01111111'                                   0140 00597000
*   IF SCANDATA(1:5)='DASD '        /*                       @Y30LB12*/ 00598000
*     THEN                          /*                       @Y30LB12*/ 00599000
         L     @10,STARTCOL(,R4)                                   0141 00600000
         CLC   SCANDATA(5,@10),@CC00666                            0141 00601000
         BNE   @RF00141                                            0141 00602000
*     DO;                           /*                       @Y30LB12*/ 00603000
*       KNOWN='1'B;                 /* MATCH FLAG ON         @Y30LB12*/ 00604000
         OI    KNOWN,B'10000000'                                   0143 00605000
*       GEN(LINK  EP=ICGDSS09);     /*                       @Y30LB12*/ 00606000
         LINK  EP=ICGDSS09                                              00607000
*     END;                          /*                       @Y30LB12*/ 00608000
*   ELSE                                                           0146 00609000
*     ;                             /*                       @Y30LB12*/ 00610000
@RF00141 DS    0H                                                  0147 00611000
*   IF SCANDATA(1:3)='SDG'&KNOWN='0'B/*                      @Y30LB12*/ 00612000
*     THEN                          /*                       @Y30LB12*/ 00613000
