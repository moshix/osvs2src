@DC00131 DS    0H                                                  0133 00554000
*   IF L=73                         /* IF COL 72, THEN       @Y30LB12*/ 00555000
*     THEN                          /* NO KEYWORD FOUND      @Y30LB12*/ 00556000
         CLC   L(4),@CF00650                                       0133 00557000
         BNE   @RF00133                                            0133 00558000
*     LASTPARM='1'B;                /* STOP SCAN IN          @Y30LB12*/ 00559000
         OI    LASTPARM(R4),B'00001000'                            0134 00560000
*   ELSE                                                           0135 00561000
*     ;                             /* CALLED PROCEDURES     @Y30LB12*/ 00562000
@RF00133 DS    0H                                                  0136 00563000
*   NEXTCOL=ADDR(INBUF)+L-1;        /* START OF KEYWORD      @Y30LB12*/ 00564000
         LA    @10,INBUF(,R4)                                      0136 00565000
         AL    @10,L                                               0136 00566000
         BCTR  @10,0                                               0136 00567000
         ST    @10,NEXTCOL(,R4)                                    0136 00568000
*   CALL DOCMD;                     /* EXAMINE CMD WORD      @Y30LB12*/ 00569000
         BAL   @14,DOCMD                                           0137 00570000
*   END FINDKEY;                    /* RET. TO STARTCMD      @Y30LB12*/ 00571000
@EL00005 DS    0H                                                  0138 00572000
@EF00005 DS    0H                                                  0138 00573000
@ER00005 LM    @14,@12,@SA00005                                    0138 00574000
