         ST    @05,STARTCOL(,R4)                                   0119 00478000
*   DO K=J TO 72 WHILE(SCANBUF(K)^='40'X);/*                 @Y30LB12*/ 00479000
         B     @DE00120                                            0120 00480000
@DL00120 L     @05,NEXTCOL(,R4)                                    0120 00481000
         ALR   @05,@10                                             0120 00482000
         AL    @05,@CF00684                                        0120 00483000
         CLI   SCANBUF(@05),X'40'                                  0120 00484000
         BE    @DC00120                                            0120 00485000
*   END;                            /* WAS BLANK OR 72       @Y30LB12*/ 00486000
         AH    @10,@CH00042                                        0121 00487000
@DE00120 ST    @10,K                                               0121 00488000
         CH    @10,@CH00272                                        0121 00489000
         BNH   @DL00120                                            0121 00490000
@DC00120 DS    0H                                                  0122 00491000
*   IF K^=73                        /* IF NOT 72, THEN       @Y30LB12*/ 00492000
*     THEN                          /* END OF CMD WORD       @Y30LB12*/ 00493000
         CLC   K(4),@CF00650                                       0122 00494000
         BE    @RF00122                                            0122 00495000
*     CALL FINDKEY;                 /* FIND START OF KEY     @Y30LB12*/ 00496000
