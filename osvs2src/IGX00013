*      2 CVTBTERM PTR(31),          /* ADDRESS OF ENTRY POINT OF THE    00392000
*                                      ABTERM ROUTINE                */ 00393000
*      2 CVTDATE CHAR(4) BDY(WORD), /* CURRENT DATE IN PACKED DECIMAL*/ 00394000
*      2 CVTMSLT PTR(31),           /* ADDRESS OF THE MASTER COMMON     00395000
*                                      AREA IN MASTER SCHEDULER    0075 00396000
*                                      RESIDENT DATA AREA. NOTE - USE   00397000
*                                      CVTMSER INSTEAD TO ADDRESS  0075 00398000
*                                      MASTER SCHEDULER RESIDENT DATA   00399000
*                                      AREA.                         */ 00400000
*      2 CVTZDTAB PTR(31),          /* ADDRESS OF I/O DEVICE       0075 00401000
*                                      CHARACTERISTIC TABLE          */ 00402000
*      2 CVTXITP PTR(31),           /* ADDRESS OF ERROR INTERPRETER     00403000
*                                      ROUTINE                       */ 00404000
*      2 CVTDAR PTR(31),            /* SAME AS CVTDARA BELOW (OS/VS1)   00405000
*                                      MDC029                        */ 00406000
*       3 CVTFLGS1 BIT(8),          /* FLAG BYTE (OS/VS1) MDC030     */ 00407000
*        4 CVTDMPLK BIT(1),         /* SVC DUMP IS IN PROGRESS     0075 00408000
*                                      (OS/VS1) MDC030               */ 00409000
*        4 CVTRSV01 BIT(1),         /* RESERVED                      */ 00410000
