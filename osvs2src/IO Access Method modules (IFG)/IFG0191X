         ST    @10,CAXACB(,@12)                                    0367 00636000
*       IF ACBSCRA=ON|              /* IF SYS BUILD REQUEST  @Y30SSDW*/ 00637000
*           ACBUCRA=ON THEN         /* OR USER CRA BLD REQ   @Y30SSDW*/ 00638000
         TM    ACBSCRA(RPARC),B'00001100'                          0368 00639000
         BZ    @RF00368                                            0368 00640000
*         DO;                       /* SET CRA FIELDS IN CAX @Y30SSDW*/ 00641000
*           CAXCCA=R11;             /* SET ADDR OF THE CCA   @Y30SSDW*/ 00642000
         ST    R11,CAXCCA(,@12)                                    0370 00643000
*           RFY                                                    0371 00644000
*             R11 UNRSTD;           /*                       @Y30SSDW*/ 00645000
*           CAXCRACB=DXUACBAD;      /* SET CRA ACB ADDRESS   @Y30SSDW*/ 00646000
         ST    @10,CAXCRACB(,@12)                                  0372 00647000
*           IF ACBUCRA=ON THEN      /* USER CRA REQUEST      @Y30SSDW*/ 00648000
         TM    ACBUCRA(RPARC),B'00000100'                          0373 00649000
         BNO   @RF00373                                            0373 00650000
*             DO;                   /* SET VALUES IN AMCBS   @Y30SSDW*/ 00651000
*               CBSCRTCB=ZERO;      /* CLEAR TCB ADDRESS     @Y30SSDW*/ 00652000
         L     @09,CVTPTR                                          0375 00653000
         L     @09,CVTCBSP(,@09)                                   0375 00654000
         SLR   @07,@07                                             0375 00655000
         ST    @07,CBSCRTCB(,@09)                                  0375 00656000
*               CBSCRACA=ACBUAPTR;  /* POINT TO UCRA CAXWA CHAIN   0376 00657000
*                                                            @Y30SSDW*/ 00658000
         ST    @12,CBSCRACA(,@09)                                  0376 00659000
*             END;                  /*                       @Y30SSDW*/ 00660000
*         END;                      /*                       @Y30SSDW*/ 00661000
@RF00373 DS    0H                                                  0379 00662000
*       IF ACBSDS=ON THEN           /* SYSTEM DATA SET       @Z40WSRL*/ 00663000
@RF00368 TM    ACBSDS(RPARC),B'00000010'                           0379 00664000
         BNO   @RF00379                                            0379 00665000
*         DO;                       /*                       @Z40WSRL*/ 00666000
*           R15=R15+LENGTH(IGGCAXWA);/* POINT TO DUMMY TIOT  @Z40WSRL*/ 00667000
         AH    R15,@CH00176                                        0381 00668000
*           R15->TIOT1=TCBTIO->TIOT1;/* COPY TIOT            @Z40WSRL*/ 00669000
         L     @12,CVTPTR                                          0382 00670000
         L     @12,CVTTCBP(,@12)                                   0382 00671000
         L     @12,TCBPTR(,@12)                                    0382 00672000
         L     @12,TCBTIO(,@12)                                    0382 00673000
         MVC   TIOT1(24,R15),TIOT1(@12)                            0382 00674000
*           R15=R15+LENGTH(TIOT1);  /* POINT TO TIOT ENTRY   @Z40WSRL*/ 00675000
         AH    R15,@CH00110                                        0383 00676000
*           R15->TIOENTRY=DXTIOTAD->TIOENTRY;/* COPY TIOT ENTRY    0384 00677000
*                                                            @Z40WSRL*/ 00678000
         L     @12,DXTIOTAD(,RCORE)                                0384 00679000
         MVC   TIOENTRY(40,R15),TIOENTRY(@12)                      0384 00680000
*           R15->TIOTFEND=R15->TIOTFEND&&R15->TIOTFEND;/* ZEROS    0385 00681000
*                                                            @Z40WSRL*/ 00682000
         XC    TIOTFEND(4,R15),TIOTFEND(R15)                       0385 00683000
*         END;                      /*                       @Z40WSRL*/ 00684000
*     END;                          /*                       @Z40WSRL*/ 00685000
*   RFY                                                            0388 00686000
