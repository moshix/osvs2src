*       CAXID=CAXCD;                /* SET CAXWA ID                  */ 00628000
         L     @12,ACBUAPTR(,RPARC)                                0365 00629000
         MVI   CAXID(@12),X'CA'                                    0365 00630000
*       CAXF2CA=ON;                 /* SET FREE CAXWA IF ERROR       */ 00631000
         OI    CAXF2CA(@12),B'00000100'                            0366 00632000
