         MVC   JFCBVOLS(6,@15),CAXVOLID(@12)                       0311 00469000
*     END;                          /*                       @Y30SSDW*/ 00470000
*   ELSE                            /* NOT CRA BUILD         @Y30SSDW*/ 00471000
*     DO;                           /*                       @Y30SSDW*/ 00472000
         B     @RC00308                                            0313 00473000
@RF00308 DS    0H                                                  0314 00474000
*       JFCBDSNM(1:44)=MCNAME;      /* SET UP SPECIAL NAME           */ 00475000
         LA    @15,100                                             0314 00476000
         ALR   @15,RCORE                                           0314 00477000
         MVC   JFCBDSNM(44,@15),@CC04511                           0314 00478000
*       JFCBVOLS(1:6)=UCBVOLI;      /* SET VOL SER NO.       @Y30SSDW*/ 00479000
         L     @12,ACBUAPTR(,RPARC)                                0315 00480000
         L     @12,CAXUCB(,@12)                                    0315 00481000
         MVC   JFCBVOLS(6,@15),UCBVOLI(@12)                        0315 00482000
*     END;                          /*                       @Y30SSDW*/ 00483000
*   JFCCAT=ON;                      /* SET CATLG'D DATA SET          */ 00484000
@RC00308 DS    0H                                                  0318 00485000
*   JFCNWRIT=ON;                    /* IND. DON'T RE-WRITE JFCB      */ 00486000
         LA    @15,100                                             0318 00487000
