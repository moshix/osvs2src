*   GEN(GETMAIN EC,MF=(E,(1)));     /* ISSUE GETMAIN                 */ 00545000
         GETMAIN EC,MF=(E,(1))                                          00546000
*   RFY                                                            0336 00547000
*     R15 RSTD;                                                    0336 00548000
*   R1=ADDR(MESSAGEA);              /* RESET PTR               Y02020*/ 00549000
         LR    R1,RCORE                                            0337 00550000
*   IF R15^=ZERO THEN               /* ERROR                         */ 00551000
         LTR   R15,R15                                             0338 00552000
         BZ    @RF00338                                            0338 00553000
*     DO;                           /* YES                           */ 00554000
*       DXECB(1:1)=NCER;            /* NO CORE ERROR           Y02020*/ 00555000
         MVI   DXECB(RCORE),X'11'                                  0340 00556000
*       CALL CAPXCTL;               /* GET OUT                 Y02020*/ 00557000
