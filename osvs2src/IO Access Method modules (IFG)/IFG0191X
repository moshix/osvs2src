*   JFCSHARE=ON;                    /* SET SHARED BIT                */ 00490000
         OI    JFCSHARE(@15),B'00001000'                           0319 00491000
*   JFCORGAM=ON;                    /* SET AM BIT                    */ 00492000
         OI    JFCORGAM(@15),B'00001000'                           0320 00493000
*   JFCBNVOL='01'X;                 /* SET NO. VOLS = 1              */ 00494000
         MVI   JFCBNVOL(@15),X'01'                                 0321 00495000
