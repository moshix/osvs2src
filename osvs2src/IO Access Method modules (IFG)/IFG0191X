@ER00003 BR    @14                                                 0389 00692000
@PB00003 DS    0H                                                  0390 00693000
*   GEN(EJECT);                     /*                       @Y30SSDW*/ 00694000
         EJECT                                                          00695000
*                                                                  0391 00696000
*/********************************************************************/ 00697000
*/*      INTERNAL PROCEDURE TO GET THE JFCB                          */ 00698000
*/*           IF CALLER IS SCHEDULER, THEN JFCB HAS BEEN READ        */ 00699000
*/*              AND IS IN SCHEDULER'S WORKAREA.                     */ 00700000
*/*           ELSE - MUST GETMAIN FOR QUEUE MANAGER WORKAREA         */ 00701000
*/*              AND READ THE JFCB.                                  */ 00702000
*/*           INPUT - TIOT PTR IN CAXWA                              */ 00703000
*/*                 - ACB PTR IN RPARC                               */ 00704000
*/*                 - JFCB TTR IN TIOT                               */ 00705000
*/*                 - OFFSET TO TIOT ENTRY IN ACB                    */ 00706000
*/********************************************************************/ 00707000
*                                                                  0391 00708000
*GETJFCB:                                                          0391 00709000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0391 00710000
         B     @PB00004                                            0391 00711000
GETJFCB  DS    0H                                                  0392 00712000
*   IF CAXSCR^=ON THEN              /* NOT INVOKED BY SCHED          */ 00713000
         L     @12,ACBUAPTR(,RPARC)                                0392 00714000
         TM    CAXSCR(@12),B'00000001'                             0392 00715000
         BO    @RF00392                                            0392 00716000
*     DO;                           /* YES                           */ 00717000
*       RFY                                                        0394 00718000
*        (R14,                                                     0394 00719000
*         R10) RSTD;                /*                       @Y30SSDW*/ 00720000
*       R10=R14;                    /* SAVE RETURN REG               */ 00721000
         LR    R10,R14                                             0395 00722000
*/********************************************************************/ 00723000
*/*                   JFCB  PASSED  BY  CALLER                       */ 00724000
*/********************************************************************/ 00725000
*                                                                  0396 00726000
*       IF ACBUJFCB^=ZERO THEN      /* CK IF JFCB PASSED     @Y30LSPS*/ 00727000
         ICM   @15,15,ACBUJFCB(RPARC)                              0396 00728000
         BZ    @RF00396                                            0396 00729000
*         DO;                       /* JFCB PASSED CODE      @Y30LSPS*/ 00730000
*           GEN(TESTAUTH FCTN=1);   /* IS CALLER APF         @Y30LSPS*/ 00731000
         TESTAUTH FCTN=1                                                00732000
*           RFY                                                    0399 00733000
*             R15 RSTD;             /*                       @Y30SSDW*/ 00734000
*           IF R15^=ZERO THEN       /* ERR IF NOT APF        @Y30LSPS*/ 00735000
         LTR   R15,R15                                             0400 00736000
         BZ    @RF00400                                            0400 00737000
*             DO;                   /* QUIT WITH ERR         @Y30LSPS*/ 00738000
*               RFY                                                0402 00739000
*                 R15 UNRSTD;       /*                       @Y30LSDW*/ 00740000
*               DXECB(1:1)=IOER;    /* INDICATE ERROR        @Y30LSPS*/ 00741000
         MVI   DXECB(RCORE),X'41'                                  0403 00742000
*               CAXF2IOE=ON;        /* MUST FREE CAXWA       @Y30LSPS*/ 00743000
         L     @15,ACBUAPTR(,RPARC)                                0404 00744000
         OI    CAXF2IOE(@15),B'00010000'                           0404 00745000
*               CALL CAPERR;        /* GOTO ERR CLEANUP      @Y30LSPS*/ 00746000
         BAL   @14,CAPERR                                          0405 00747000
*             END;                  /* END IF NOT APF OK     @Y30LSPS*/ 00748000
*           ELSE                    /* IS APF AUTHORIZED     @Y30LSPS*/ 00749000
*             DO;                   /* OK TO COPY JFCB       @Y30LSPS*/ 00750000
         B     @RC00400                                            0407 00751000
@RF00400 DS    0H                                                  0408 00752000
*               DXJBF=OPENJFCB;     /* COPY PASSED JFCB      @Y30LSPS*/ 00753000
         L     @15,ACBUJFCB(,RPARC)                                0408 00754000
         MVC   DXJBF(176,RCORE),OPENJFCB(@15)                      0408 00755000
*               DXECB(1:1)=NOER;    /* SET NO ERROR          @Y30LSPS*/ 00756000
         MVI   DXECB(RCORE),X'7F'                                  0409 00757000
*             END;                  /* END IF OK OPENJ       @Y30LSPS*/ 00758000
*         END;                      /* END IF OPENJ          @Y30LSPS*/ 00759000
*/********************************************************************/ 00760000
*/*             NO  JFCB  PASSED  -  MUST  READ  JFCB                */ 00761000
*/********************************************************************/ 00762000
*                                                                  0412 00763000
*       ELSE                        /* NO JFCB PASSED        @Y30LSPS*/ 00764000
*         DO;                       /* MUST RDJFCB           @Y30LSPS*/ 00765000
         B     @RC00396                                            0412 00766000
@RF00396 DS    0H                                                  0413 00767000
*           RFY                                                    0413 00768000
*             R1 RSTD;                                             0413 00769000
*           R1=ADDR(MESSAGEA);      /* USE O/C/EOV W.A.              */ 00770000
         LR    R1,RCORE                                            0414 00771000
*           SMLEN=LENGTH(SCHWA);    /* LENGTH OF W A.                */ 00772000
