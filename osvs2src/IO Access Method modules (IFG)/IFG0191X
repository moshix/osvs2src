         TITLE 'IFG0191X - VSAM CATALOG OPEN ACB PROCESSOR, LOAD 1     *00001000
                        '                                               00002000
IFG0191X CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IFG0191X  78.117'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @03,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@03                                         0001 00012000
RBPRFXLN EQU 64                                                         00013000
RBPRFLNA EQU 32                                                         00014000
PRBLEN   EQU 136                                                        00015000
SIRBLEN  EQU 200                                                        00016000
TIRBLEN  EQU 136                                                        00017000
IRBLEN   EQU 128                                                        00018000
SVRBLEN  EQU 224                                                        00019000
          STM   R14,R3,DXREG9(RCORE)   SAVE REGS             @OZ29185   00020000
          TESTAUTH FCTN=1,STATE=YES,KEY=YES,BRANCH=YES       @OZ29185   00021000
          L     R14,DXREG9(,RCORE)    RESTORE REG 14         @OZ29185   00022000
          LM    R0,R3,DXREGB(RCORE)   RESTORE OTHER REGS     @OZ29185   00023000
*   IF R15^=ZERO THEN               /* CALLER AUTHORIZED?    @OZ29185*/ 00024000
         LTR   R15,R15                                             0187 00025000
         BZ    @RF00187                                            0187 00026000
*     DO;                                                          0188 00027000
*       RFY                                                        0189 00028000
*         R15 UNRSTD;                                              0189 00029000
*       DXECB(1:1)=IOER;            /* SET ERROR OPEN WA     @OZ29185*/ 00030000
         MVI   DXECB(RCORE),X'41'                                  0190 00031000
*       CALL CAPERR;                /* CALL ERROR EXIT RTN   @OZ29185*/ 00032000
         BAL   @14,CAPERR                                          0191 00033000
*     END;                                                         0192 00034000
*   ELSE                                                           0193 00035000
*     ;                                                            0193 00036000
*                                                                  0193 00037000
@RF00187 DS    0H                                                  0194 00038000
*/********************************************************************/ 00039000
*/*         SWITCH TO KEY ZERO AND SAVE CALLER'S KEY           Y02020*/ 00040000
*/********************************************************************/ 00041000
*                                                                  0194 00042000
*   RFY                                                            0194 00043000
*     RKEYSAVE RSTD;                /*                         Y02020*/ 00044000
*   GEN(MODESET EXTKEY=ZERO,SAVEKEY=(2));/*                    Y02020*/ 00045000
         MODESET EXTKEY=ZERO,SAVEKEY=(2)                                00046000
*   KEYSAVE=RKEYSAVE;               /* SAVE KEY OF CALLER      Y02020*/ 00047000
         STC   RKEYSAVE,KEYSAVE(,RCORE)                            0196 00048000
*   RFY                                                            0197 00049000
*     RKEYSAVE UNRSTD;              /*                         Y02020*/ 00050000
*   RFY                                                            0198 00051000
*    (RCORE,                                                       0198 00052000
*     RES,                                                         0198 00053000
*     RWTG,                                                        0198 00054000
*     RPARC,                                                       0198 00055000
*     RWTGC,                                                       0198 00056000
*     R11) RSTD;                    /*                       @Z40WSRL*/ 00057000
*   RESPECIFY                                                      0199 00058000
*     INFMJFCB BASED(ADDR(DXJBF));  /* JFCB BASE               Y02020*/ 00059000
*   RESPECIFY                                                      0200 00060000
*     IGGCAXWA BASED(ACBUAPTR);     /* CAXWA BASE              Y02020*/ 00061000
*   RPARC=DXPDCBAD;                 /* GET ACB ADDR            Y02020*/ 00062000
         L     RPARC,DXPDCBAD(,RCORE)                              0201 00063000
*   DXREGE=RES;                     /* SAVE RET ADDR           Y02020*/ 00064000
         ST    RES,DXREGE(,RCORE)                                  0202 00065000
*   RESPECIFY                                                      0203 00066000
*     RES UNRSTD;                   /*                         Y02020*/ 00067000
*   RESPECIFY                                                      0204 00068000
*     R1 RESTRICTED;                /*                       @Z40WSRL*/ 00069000
*   R1=XRBLNK;                      /* GET CALLER RB         @Z40WSRL*/ 00070000
         L     @12,CVTPTR                                          0205 00071000
         L     @12,CVTTCBP(,@12)                                   0205 00072000
         L     @12,TCBPTR(,@12)                                    0205 00073000
         L     @12,TCBRBP(,@12)                                    0205 00074000
         L     R1,XRBLNK(,@12)                                     0205 00075000
*   RESPECIFY                                                      0206 00076000
*     RBBASIC BASED(R1);            /* BASE RB ON REG 1      @Z40WSRL*/ 00077000
*   IF ACBSDS=ON&                   /* SYSTEM DATA SET AND   @Z40WSRL*/ 00078000
*       (RBRPSWPK^=BITZERO|         /* NOT KEY ZERO OR       @Z40WSRL*/ 00079000
*       RBRPSWPS=ON) THEN           /* NOT SUPERVISOR STATE  @Z40WSRL*/ 00080000
         TM    ACBSDS(RPARC),B'00000010'                           0207 00081000
         BNO   @RF00207                                            0207 00082000
         LA    @12,16                                              0207 00083000
         ALR   @12,R1                                              0207 00084000
         TM    RBRPSWPK(@12),B'11110000'                           0207 00085000
         BNZ   @RT00207                                            0207 00086000
         TM    RBRPSWPS(@12),B'00000001'                           0207 00087000
         BNO   @RF00207                                            0207 00088000
@RT00207 DS    0H                                                  0208 00089000
*     DO;                           /* UNAUTHORIZED CALLER   @Z40WSRL*/ 00090000
*       DXECB(1:1)=IOER;            /* SET ERROR             @Z40WSRL*/ 00091000
         MVI   DXECB(RCORE),X'41'                                  0209 00092000
*       CALL CAPXCTL;               /* QUIT WITH ERROR       @Z40WSRL*/ 00093000
         BAL   @14,CAPXCTL                                         0210 00094000
*     END;                          /*                       @Z40WSRL*/ 00095000
*   RESPECIFY                                                      0212 00096000
*     RBBASIC BASED(TCBRBP);        /* REBASE RB ON TCB PTR  @Z40WSRL*/ 00097000
@RF00207 DS    0H                                                  0213 00098000
*   RESPECIFY                                                      0213 00099000
*     R1 UNRESTRICTED;              /*                       @Z40WSRL*/ 00100000
*/********************************************************************/ 00101000
*/*              PROCESSING  FOR  EXISTENT  DD  ENTRY                */ 00102000
*/********************************************************************/ 00103000
*                                                                  0214 00104000
*   IF ACBSCRA=OFF&                 /* NOT SYSTEM CRA REQUEST      0214 00105000
*                                                            @Y30SSDW*/ 00106000
*       DXTIOTAD^=ZERO THEN         /* TIOT ADDR PASSED FROM 'OPEN'     00107000
*                                                            @Y30SSDW*/ 00108000
         TM    ACBSCRA(RPARC),B'00001000'                          0214 00109000
         BNZ   @RF00214                                            0214 00110000
         SLR   @12,@12                                             0214 00111000
         C     @12,DXTIOTAD(,RCORE)                                0214 00112000
         BE    @RF00214                                            0214 00113000
*     DO;                           /*                       @Y30SSDW*/ 00114000
*       IF ACBUAPTR^=ZERO&          /* IS THERE A PTR        @Y30SSDW*/ 00115000
*           ACBUCRA=OFF THEN        /* NOT USER CRA REQUEST  @Y30SSDW*/ 00116000
         L     @10,ACBUAPTR(,RPARC)                                0216 00117000
         CR    @10,@12                                             0216 00118000
         BE    @RF00216                                            0216 00119000
         TM    ACBUCRA(RPARC),B'00000100'                          0216 00120000
         BNZ   @RF00216                                            0216 00121000
*         DO;                       /* YES                     Y02020*/ 00122000
*           IF CAXID^=CAXCD THEN    /* IS IT A CAXWA           Y02020*/ 00123000
         CLI   CAXID(@10),202                                      0218 00124000
         BE    @RF00218                                            0218 00125000
*             DO;                   /* NOT A CAXWA             Y02020*/ 00126000
*                                                                  0219 00127000
*               /*****************************************************/ 00128000
*               /*                                                   */ 00129000
*               /* IF THIS IS THE SCHEDULER CALLING, THE CALLER IS   */ 00130000
*               /* KEY ZERO                                          */ 00131000
*               /*                                                   */ 00132000
*               /*****************************************************/ 00133000
*                                                                  0220 00134000
*               RFY                                                0220 00135000
*                 R1 RSTD;          /* NEED R1 FOR THIS CODE         */ 00136000
*               R1=XRBLNK;          /* GET CALLER RB         @Y30LSPS*/ 00137000
         L     @12,CVTPTR                                          0221 00138000
         L     @12,CVTTCBP(,@12)                                   0221 00139000
         L     @12,TCBPTR(,@12)                                    0221 00140000
         L     @12,TCBRBP(,@12)                                    0221 00141000
         L     R1,XRBLNK(,@12)                                     0221 00142000
*               RESPECIFY                                          0222 00143000
*                 RBBASIC BASED(R1);/* BASE RB ON REG 1      @Z40WSRL*/ 00144000
*               IF RBRPSWPK=BITZERO|/* CHK IF KEY 0          @Y30LSPS*/ 00145000
*                   RBRPSWPS=OFF THEN/* CK SUPERVR           @Y30LSPS*/ 00146000
         LA    @12,16                                              0223 00147000
         ALR   @12,R1                                              0223 00148000
         TM    RBRPSWPK(@12),B'11110000'                           0223 00149000
         BZ    @RT00223                                            0223 00150000
         TM    RBRPSWPS(@12),B'00000001'                           0223 00151000
         BNZ   @RF00223                                            0223 00152000
@RT00223 DS    0H                                                  0224 00153000
*                 DO;               /* CALLER IS OK          @Y30LSPS*/ 00154000
*                   CALL BLDCAXWA;  /* BUILD A CAXWA         @Y30LSPS*/ 00155000
         BAL   @14,BLDCAXWA                                        0225 00156000
*                   CAXSCR=ON;      /* SET INVOKED BY SCHED FLG      */ 00157000
         L     @12,ACBUAPTR(,RPARC)                                0226 00158000
         OI    CAXSCR(@12),B'00000001'                             0226 00159000
*                 END;              /* END IF CALLER OK      @Y30LSPS*/ 00160000
*               ELSE                /* CALLER NOT KEY 0      @Y30LSPS*/ 00161000
*                 DO;               /* MUST QUIT WITH ER     @Y30LSPS*/ 00162000
         B     @RC00223                                            0228 00163000
@RF00223 DS    0H                                                  0229 00164000
*                   DXECB(1:1)=IOER;/* SET ERROR             @Y30LSPS*/ 00165000
         MVI   DXECB(RCORE),X'41'                                  0229 00166000
*                   CALL CAPXCTL;   /* QUIT WITH ERROR       @Y30LSPS*/ 00167000
         BAL   @14,CAPXCTL                                         0230 00168000
*                 END;              /* END IF CALLER BAD     @Y30LSPS*/ 00169000
*               RESPECIFY                                          0232 00170000
*                 RBBASIC BASED(TCBRBP);/* REBASE RB ON TCB PTR    0232 00171000
*                                                            @Z40WSRL*/ 00172000
@RC00223 DS    0H                                                  0233 00173000
*               RESPECIFY                                          0233 00174000
*                 R1 UNRESTRICTED;  /*                       @Z40WSRL*/ 00175000
*             END;                  /*                       @Z40WSRL*/ 00176000
*         END;                                                     0235 00177000
*       ELSE                        /* NO POINTER EXISTS IN ACBUAPTR */ 00178000
*                                                                  0236 00179000
*         /***********************************************************/ 00180000
*         /*                                                         */ 00181000
*         /* CALL BUILD CAXWA ROUTINE                          Y02020*/ 00182000
*         /*                                                         */ 00183000
*         /***********************************************************/ 00184000
*                                                                  0236 00185000
*         CALL BLDCAXWA;            /* BUILD A CAXWA                 */ 00186000
         B     @RC00216                                            0236 00187000
@RF00216 BAL   @14,BLDCAXWA                                        0236 00188000
*       CAXOPN=ON;                  /* SET OPEN IN CONTROL   @Y30SSDW*/ 00189000
@RC00216 L     @12,ACBUAPTR(,RPARC)                                0237 00190000
         OI    CAXOPN(@12),B'01000000'                             0237 00191000
*       CAXATIOT=TCBTIO;            /* PUT TIOT PTR IN CAXWA   Y02020*/ 00192000
         L     @10,CVTPTR                                          0238 00193000
         L     @10,CVTTCBP(,@10)                                   0238 00194000
         L     @10,TCBPTR(,@10)                                    0238 00195000
         L     @10,TCBTIO(,@10)                                    0238 00196000
         ST    @10,CAXATIOT(,@12)                                  0238 00197000
*       CAXUCB=TIOEFSRT;            /* PUT UCB PTR IN CAXWA    Y02020*/ 00198000
         L     @09,DXTIOTAD(,RCORE)                                0239 00199000
         SLR   @07,@07                                             0239 00200000
         ICM   @07,7,TIOEFSRT(@09)                                 0239 00201000
         ST    @07,CAXUCB(,@12)                                    0239 00202000
*       ACBTIOT=DXTIOTAD-CAXATIOT;  /* SET TIOT OFFSET         Y02020*/ 00203000
*                                                                  0240 00204000
         SLR   @09,@10                                             0240 00205000
         STH   @09,ACBTIOT(,RPARC)                                 0240 00206000
*       /*************************************************************/ 00207000
*       /*                                                           */ 00208000
*       /* CALL GETJFCB ROUTINE                                Y02020*/ 00209000
*       /*                                                           */ 00210000
*       /*************************************************************/ 00211000
*                                                                  0241 00212000
*       CALL GETJFCB;               /* OBTAIN THE JFCB         Y02020*/ 00213000
         BAL   @14,GETJFCB                                         0241 00214000
*       IF ACBSDS=ON THEN           /* IF SYSTEM DATA SET    @Z40WSRL*/ 00215000
         TM    ACBSDS(RPARC),B'00000010'                           0242 00216000
         BNO   @RF00242                                            0242 00217000
*         CAXATIOT=ACBUAPTR+LENGTH(IGGCAXWA);/* PT CAXWA TO DUMMY  0243 00218000
*                                      TIOT                  @Z40WSRL*/ 00219000
         L     @12,ACBUAPTR(,RPARC)                                0243 00220000
         LA    @10,132                                             0243 00221000
         ALR   @10,@12                                             0243 00222000
         ST    @10,CAXATIOT(,@12)                                  0243 00223000
*     END;                          /* END 'DO' FOR EXISTENT DD ENTRY*/ 00224000
*/********************************************************************/ 00225000
*/*                   DD  ENTRY  DOES  NOT  EXIST                    */ 00226000
*/********************************************************************/ 00227000
*                                                                  0245 00228000
*   ELSE                            /* PROCESSING FOR NO DD ENTRY    */ 00229000
*     DO;                           /*                         Y02020*/ 00230000
*                                                                  0245 00231000
         B     @RC00214                                            0245 00232000
@RF00214 DS    0H                                                  0246 00233000
*       /*************************************************************/ 00234000
*       /*                                                           */ 00235000
*       /* TEST IF OPENING MASTER CATALOG                      Y02020*/ 00236000
*       /*                                                           */ 00237000
*       /*************************************************************/ 00238000
*                                                                  0246 00239000
*       IF ACBSCRA=ON|              /* SYSTEM CRA REQUEST OR @Y30SSDW*/ 00240000
*           (CAXID=CAXCD&CAXMCT=ON) THEN/* MCAT OPEN         @Y30SSDW*/ 00241000
         TM    ACBSCRA(RPARC),B'00001000'                          0246 00242000
         BO    @RT00246                                            0246 00243000
         L     @12,ACBUAPTR(,RPARC)                                0246 00244000
         CLI   CAXID(@12),202                                      0246 00245000
         BNE   @RF00246                                            0246 00246000
         TM    CAXMCT(@12),B'00000100'                             0246 00247000
         BNO   @RF00246                                            0246 00248000
@RT00246 DS    0H                                                  0247 00249000
*         DO;                       /*                         Y02020*/ 00250000
*           IF ACBSCRA=ON&          /* SYSTEM CRA REQUEST    @Y30SSDW*/ 00251000
*               CAXBLD=ON THEN      /* BUILD REQUEST         @Y30SSDW*/ 00252000
         TM    ACBSCRA(RPARC),B'00001000'                          0248 00253000
         BNO   @RF00248                                            0248 00254000
         L     @12,ACBUAPTR(,RPARC)                                0248 00255000
         TM    CAXBLD(@12),B'10000000'                             0248 00256000
         BNO   @RF00248                                            0248 00257000
*             DO;                   /*                       @Y30SSDW*/ 00258000
*               CAXATIOT=TCBTIO;    /* SET ADDR OF TIOT      @Y30SSDW*/ 00259000
         L     @10,CVTPTR                                          0250 00260000
         L     @10,CVTTCBP(,@10)                                   0250 00261000
         L     @10,TCBPTR(,@10)                                    0250 00262000
         L     @10,TCBTIO(,@10)                                    0250 00263000
         ST    @10,CAXATIOT(,@12)                                  0250 00264000
*               ACBTIOT=DXTIOTAD-CAXATIOT;/* SET TIOT OFFSET @Y30SSDW*/ 00265000
         LCR   @10,@10                                             0251 00266000
         AL    @10,DXTIOTAD(,RCORE)                                0251 00267000
         STH   @10,ACBTIOT(,RPARC)                                 0251 00268000
*             END;                  /*                       @Y30SSDW*/ 00269000
*           ELSE                    /* NOT SYSTEM CRA OR NOT BUILD 0253 00270000
*                                      REQUEST               @Y30SSDW*/ 00271000
*             DO;                   /*                       @Y30SSDW*/ 00272000
         B     @RC00248                                            0253 00273000
@RF00248 DS    0H                                                  0254 00274000
*               IF ACBSCRA=ON&      /* SYSTEM CRA REQUEST    @Y30SSDW*/ 00275000
*                   CAXRATEP^=ZERO THEN/* RECOVERY AREA TIOT ENTRY 0254 00276000
*                                      POINTER EXISTS        @Y30SSDW*/ 00277000
         TM    ACBSCRA(RPARC),B'00001000'                          0254 00278000
         BNO   @RF00254                                            0254 00279000
         L     @12,ACBUAPTR(,RPARC)                                0254 00280000
         L     @12,CAXRATEP(,@12)                                  0254 00281000
         LTR   @12,@12                                             0254 00282000
         BZ    @RF00254                                            0254 00283000
*                 DO;               /*                       @Y30SSDW*/ 00284000
*                   DXTIOTAD=CAXRATEP;/* SET ENTRY ADDR      @Y30SSDW*/ 00285000
         ST    @12,DXTIOTAD(,RCORE)                                0256 00286000
*                   ACBTIOT=CAXRATEP-TCBTIO;/* SET TIOT OFFSET IN ACB   00287000
*                                                            @Y30SSDW*/ 00288000
         L     @10,CVTPTR                                          0257 00289000
         L     @10,CVTTCBP(,@10)                                   0257 00290000
         L     @10,TCBPTR(,@10)                                    0257 00291000
         SL    @12,TCBTIO(,@10)                                    0257 00292000
         STH   @12,ACBTIOT(,RPARC)                                 0257 00293000
*                 END;              /*                       @Y30SSDW*/ 00294000
*               ELSE                /* NO TIOT ENTRY PTR     @Y30SSDW*/ 00295000
*                 DO;               /*                       @Y30SSDW*/ 00296000
*                                                                  0259 00297000
         B     @RC00254                                            0259 00298000
@RF00254 DS    0H                                                  0260 00299000
*                   /*************************************************/ 00300000
*                   /*                                               */ 00301000
*                   /* MASTER CATALOG OPEN                     Y02020*/ 00302000
*                   /*                                               */ 00303000
*                   /*************************************************/ 00304000
*                                                                  0260 00305000
*                   DXTIOTAD=CAXATIOT+LENGTH(TIOT1);/* PUT     Y02020   00306000
*                                      ALT TIOT                Y02020   00307000
*                                      ENTRY PTR IN            Y02020   00308000
*                                      OPEN WORKAREA           Y02020*/ 00309000
         LA    @12,24                                              0260 00310000
         L     @10,ACBUAPTR(,RPARC)                                0260 00311000
         L     @10,CAXATIOT(,@10)                                  0260 00312000
         ALR   @10,@12                                             0260 00313000
         ST    @10,DXTIOTAD(,RCORE)                                0260 00314000
*                   ACBTIOT=LENGTH(TIOT1);/* OFFSET IN ACB     Y02020*/ 00315000
         STH   @12,ACBTIOT(,RPARC)                                 0261 00316000
*                 END;              /*                       @Y30SSDW*/ 00317000
*             END;                  /*                       @Y30SSDW*/ 00318000
*                                                                  0263 00319000
@RC00254 DS    0H                                                  0264 00320000
*           /*********************************************************/ 00321000
*           /*                                                       */ 00322000
*           /* CALL BUILD JFCB ROUTINE                         Y02020*/ 00323000
*           /*                                                       */ 00324000
*           /*********************************************************/ 00325000
*                                                                  0264 00326000
*           CALL BLDJFCB;           /*                         Y02020*/ 00327000
@RC00248 BAL   @14,BLDJFCB                                         0264 00328000
*         END;                      /* END 'DO' FOR SCRA OR MCAT     */ 00329000
*       ELSE                        /* NOT SCRA OR MCAT OPEN   Y02020*/ 00330000
*         DO;                       /*                         Y02020*/ 00331000
*                                                                  0266 00332000
         B     @RC00246                                            0266 00333000
@RF00246 DS    0H                                                  0267 00334000
*           /*********************************************************/ 00335000
*           /*                                                       */ 00336000
*           /* SET NO DDNAME ERROR                             Y02020*/ 00337000
*           /*                                                       */ 00338000
*           /*********************************************************/ 00339000
*                                                                  0267 00340000
*           DXECB(1:1)=VSONODDN;    /* NO DDNAME               Y02020*/ 00341000
         MVI   DXECB(RCORE),X'0C'                                  0267 00342000
*           CALL CAPERR;            /* ERROR                   Y02020*/ 00343000
         BAL   @14,CAPERR                                          0268 00344000
*         END;                      /*                         Y02020*/ 00345000
*     END;                          /*                         Y02020*/ 00346000
@RC00246 DS    0H                                                  0271 00347000
*   CAXOPN=ON;                      /* SET OPEN IN CONTROL     Y02020*/ 00348000
@RC00214 L     @12,ACBUAPTR(,RPARC)                                0271 00349000
         OI    CAXOPN(@12),B'01000000'                             0271 00350000
*   ACBINFL=CAPFLG;                 /* MOVE INDICATOR FLG      Y02020*/ 00351000
         MVC   ACBINFL(1,RPARC),CAPFLG(RPARC)                      0272 00352000
*   ACBERFL=X0;                     /* ZERO ERROR FLAGS        Y02020*/ 00353000
         MVI   ACBERFL(RPARC),X'00'                                0273 00354000
*/********************************************************************/ 00355000
*/*             NOT CATALOG BUILD AND NOT CRA REQUEST                */ 00356000
*/********************************************************************/ 00357000
*                                                                  0274 00358000
*   IF CAXBLD^=ON&                  /* NOT A BUILD REQUEST?  @Y30SSDW*/ 00359000
*       ACBSCRA^=ON&                /* AND NOT SYS CRA REQ   @Y30SSDW*/ 00360000
*       ACBUCRA^=ON THEN            /* AND NOT USER CRA      @Y30SSDW*/ 00361000
         TM    CAXBLD(@12),B'10000000'                             0274 00362000
         BO    @RF00274                                            0274 00363000
         TM    ACBSCRA(RPARC),B'00001100'                          0274 00364000
         BNZ   @RF00274                                            0274 00365000
*     DO;                           /* NOT BUILD               Y02020*/ 00366000
*                                                                  0275 00367000
*       /*************************************************************/ 00368000
*       /*                                                           */ 00369000
*       /* SAVE DSNAME IN CAXWA AND INITIALIZE                 Y02020*/ 00370000
*       /* ACB PROCESSING INDICATORS.                          Y02020*/ 00371000
*       /*                                                           */ 00372000
*       /*************************************************************/ 00373000
*                                                                  0276 00374000
*       CAXCNAM=JFCBDSNM;           /* MOVE DSN FROM JFCB INTO CAXWA */ 00375000
         MVC   CAXCNAM(44,@12),JFCBDSNM+100(RCORE)                 0276 00376000
*       ACBMACR1=X0;                /* CLEAR MACRF                   */ 00377000
         MVI   ACBMACR1(RPARC),X'00'                               0277 00378000
*       ACBKEY=ON;                  /* SET KEYED PROC. IND.          */ 00379000
*       ACBADR=ON;                  /* SET ADDRESSED PROC. IND.      */ 00380000
*       ACBDIR=ON;                  /* SET DIRECT                    */ 00381000
*       ACBIN=ON;                   /* SET GET/READ                  */ 00382000
*       ACBOUT=ON;                  /* SET PUT/WRITE                 */ 00383000
         OI    ACBKEY(RPARC),B'11001110'                           0282 00384000
*     END;                                                         0283 00385000
*   ELSE                                                           0284 00386000
*     ;                             /* BUILD CASE OR CRA REQUEST     */ 00387000
@RF00274 DS    0H                                                  0285 00388000
*   ACBAMETH='11'X;                 /* SET VSAM IND                  */ 00389000
         MVI   ACBAMETH(RPARC),X'11'                               0285 00390000
*   ACBCNV=ON;                      /* IND CI PROCESSING     @Y30LSPS*/ 00391000
         OI    ACBCNV(RPARC),B'00100000'                           0286 00392000
*   CAXOPEWA=RCORE;                 /* SAVE OPEN W.A. PTR            */ 00393000
         L     @12,ACBUAPTR(,RPARC)                                0287 00394000
         ST    RCORE,CAXOPEWA(,@12)                                0287 00395000
*   IF ACBVVIC=OFF THEN             /* CHECK IF SPECIAL VVIC         */ 00396000
         TM    ACBVVIC(RPARC),B'00000010'                          0288 00397000
         BNZ   @RF00288                                            0288 00398000
*     DO;                           /* PUT SPECIAL NAME IF JFCB      */ 00399000
*/********************************************************************/ 00400000
*/*         MUST PLACE SPECIAL NAME IN JFCBDSNM IN ORDER TO          */ 00401000
*/*         INTERCEPT CATALOG REQUESTS                               */ 00402000
*/*          'VSAM-CATLG-OPEN-ACB=' AL4(PTRACB)                      */ 00403000
*/********************************************************************/ 00404000
*                                                                  0290 00405000
*       IF CAXBLD=ON&               /* CATALOG BUILD AND     @Y30SSDW*/ 00406000
*           ACBSCRA=OFF THEN        /* NOT SYS CRA BUILD     @Y30SSDW*/ 00407000
         TM    CAXBLD(@12),B'10000000'                             0290 00408000
         BNO   @RF00290                                            0290 00409000
         TM    ACBSCRA(RPARC),B'00001000'                          0290 00410000
         BNZ   @RF00290                                            0290 00411000
*         DO;                       /* SET SPECIAL INDICS    @Y30SSSB*/ 00412000
*           JFCBDSNM(1:20)=COINCN;  /* SET SPECIAL NAME              */ 00413000
         LA    @12,100                                             0292 00414000
         ALR   @12,RCORE                                           0292 00415000
         MVC   JFCBDSNM(20,@12),@CC04516                           0292 00416000
*           JFCBDSNM(21:24)=RPARC;  /* ACB PTR                       */ 00417000
         ST    RPARC,JFCBDSNM+20(,@12)                             0293 00418000
*         END;                      /* END SPECIAL NAME FOR CATL     */ 00419000
*     END;                          /* END OF BUILD SPECIALS @Y30SSSB*/ 00420000
*   ELSE                                                           0296 00421000
*     ;                             /* SPECIAL VVIC REQUEST  @Y30SSDW*/ 00422000
@RF00288 DS    0H                                                  0297 00423000
*   IF ACBUCRA=ON THEN              /* USER CRA REQUEST      @Y30SSDW*/ 00424000
@RC00288 TM    ACBUCRA(RPARC),B'00000100'                          0297 00425000
         BNO   @RF00297                                            0297 00426000
*     CAXVOLID=JFCBVOLS(1:6);       /* SET VOL SER AS NAME   @Y30SSDW*/ 00427000
         L     @12,ACBUAPTR(,RPARC)                                0298 00428000
         MVC   CAXVOLID(6,@12),JFCBVOLS+100(RCORE)                 0298 00429000
*   ELSE                                                           0299 00430000
*     ;                             /* NOT USER CRA REQUEST  @Y30SSDW*/ 00431000
@RF00297 DS    0H                                                  0300 00432000
*   RESPECIFY                                                      0300 00433000
*     R15 RSTD;                     /*                         Y02020*/ 00434000
*   R15=ZERO;                       /* IND NO ERROR            Y02020*/ 00435000
         SLR   R15,R15                                             0301 00436000
*   RESPECIFY                                                      0302 00437000
*     R15 UNRSTD;                   /*                       @Y30SSDW*/ 00438000
*   CALL CAPXCTL;                   /* CALL XCTL RTN                 */ 00439000
         BAL   @14,CAPXCTL                                         0303 00440000
*   GEN(EJECT);                     /*                       @Y30SSDW*/ 00441000
         EJECT                                                          00442000
*                                                                  0305 00443000
*/********************************************************************/ 00444000
*/*      INTERNAL PROCEDURE TO BUILD A JFCB:                         */ 00445000
*/*      CALLED IF CATALOG TO BE OPENED IS A MASTER CATALOG          */ 00446000
*/********************************************************************/ 00447000
*                                                                  0305 00448000
*BLDJFCB:                                                          0305 00449000
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0305 00450000
         B     @PB00002                                            0305 00451000
BLDJFCB  DS    0H                                                  0306 00452000
*   RFY                                                            0306 00453000
*     R14 RSTD;                     /*                         Y02020*/ 00454000
*   DXJBF=DXJBF&&DXJBF;             /* CLEAR JFCB AREA               */ 00455000
         XC    DXJBF(176,RCORE),DXJBF(RCORE)                       0307 00456000
*   IF ACBSCRA=ON THEN              /* CRA BUILD/OPEN REQ    @Y30SSDW*/ 00457000
         TM    ACBSCRA(RPARC),B'00001000'                          0308 00458000
         BNO   @RF00308                                            0308 00459000
*     DO;                           /*                       @Y30SSDW*/ 00460000
*       JFCBDSNM=CAXVOLID;          /* SET VOLID AS NAME     @Y30SSDW*/ 00461000
         LA    @15,100                                             0310 00462000
         ALR   @15,RCORE                                           0310 00463000
