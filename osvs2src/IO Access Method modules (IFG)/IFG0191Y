*       RPARC=DXPDCBAD;             /* RESTORE ACB ADDRESS   @Y30SSDW*/ 00435000
         L     RPARC,DXPDCBAD(,RCORE)                              0279 00436000
*     END;                          /* END OF DO FOR CRA REQ @Y30SSDW*/ 00437000
*/********************************************************************/ 00438000
*/*            NOT CRA REQUEST, SET UP DATA SPACE EXTENTS            */ 00439000
*/*            FOR VTOC TO READ FORMAT 1 DSCB                        */ 00440000
*/********************************************************************/ 00441000
*                                                                  0281 00442000
*   ELSE                            /* NOT CRA REQUEST       @Y30SSDW*/ 00443000
*     DO;                           /*                       @Y30SSDW*/ 00444000
