*   RFY                                                            0403 00800000
*     R1 UNRSTD;                                                   0403 00801000
*                                                                  0403 00802000
*/********************************************************************/ 00803000
*/*             VERIFY  READ  OPERATION  WAS  SUCCESSFUL             */ 00804000
*/********************************************************************/ 00805000
*                                                                  0404 00806000
*   IF DRWECMP^='7F'X THEN          /* I/O ERROR                     */ 00807000
         CLI   DRWECMP(R12),127                                    0404 00808000
         BE    @RF00404                                            0404 00809000
*     DO;                                                          0405 00810000
*       CAXF2IOE=ON;                /* SET I/O ERROR SW              */ 00811000
         L     @09,ACBUAPTR(,RPARC)                                0406 00812000
         OI    CAXF2IOE(@09),B'00010000'                           0406 00813000
*       CALL CAPERR;                /* CALL ERROR ROUTINE            */ 00814000
         BAL   @14,CAPERR                                          0407 00815000
*     END;                                                         0408 00816000
*   ELSE                                                           0409 00817000
*     ;                             /* I/O SUCCESSFUL                */ 00818000
@RF00404 DS    0H                                                  0410 00819000
*/********************************************************************/ 00820000
*/*                  REQUEST  TO  OPEN  THE  CRA                     */ 00821000
*/********************************************************************/ 00822000
*                                                                  0410 00823000
*   RFY                                                            0410 00824000
*     CATRCD BASED(ADDR(DRWRIA));                                  0410 00825000
*   RFY                                                            0411 00826000
*     TNAM BASED(ADDR(DRWRIA)+LENGTH(CATRCD));                     0411 00827000
*   IF ACBSCRA=ON|                  /* SYSTEM CRA OPEN REQ   @Y30SSDW*/ 00828000
*       ACBUCRA=ON THEN             /* OR USER CRA OPEN REQ  @Y30SSDW*/ 00829000
         TM    ACBSCRA(RPARC),B'00001100'                          0412 00830000
         BZ    @RF00412                                            0412 00831000
*     DO;                           /*                       @Y30SSDW*/ 00832000
*       IF RCDID^='D' THEN          /* ERROR IF NOT DATA REC @Y30SSDW*/ 00833000
         CLI   RCDID+204(R12),C'D'                                 0414 00834000
         BE    @RF00414                                            0414 00835000
*         DO;                       /*                       @Y30SSDW*/ 00836000
*           CAXF2IOE=ON;            /* SET I/O ERROR FLAG    @Y30SSDW*/ 00837000
         L     @09,ACBUAPTR(,RPARC)                                0416 00838000
         OI    CAXF2IOE(@09),B'00010000'                           0416 00839000
*           CALL CAPERR;            /* CALL ERROR PROCEDURE  @Y30SSDW*/ 00840000
