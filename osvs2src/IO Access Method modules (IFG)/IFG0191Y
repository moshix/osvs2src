*               CALL CAPERR;        /* CALL ERROR ROUTINE            */ 00884000
         BAL   @14,CAPERR                                          0433 00885000
*             END;                                                 0434 00886000
*           ELSE                                                   0435 00887000
*             ;                     /* DD NAMES ARE EQUAL            */ 00888000
@RF00430 DS    0H                                                  0436 00889000
*         END;                                                     0436 00890000
*       ELSE                                                       0437 00891000
*         ;                         /* MASTER CATALOG OPEN REQUEST   */ 00892000
@RF00428 DS    0H                                                  0438 00893000
*       CAXCNAM=TNAM;               /* MOVE DSNAME INTO CAXWA        */ 00894000
         L     @09,ACBUAPTR(,RPARC)                                0438 00895000
