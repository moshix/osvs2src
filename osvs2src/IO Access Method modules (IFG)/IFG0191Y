         ALR   @09,@07                                             0385 00753000
         STH   @09,DRWSEEK+3(,R12)                                 0385 00754000
*           DRWSEEK(6:7)=ZERO;      /* SET HEAD ZERO         @XA03929*/ 00755000
         SLR   @09,@09                                             0386 00756000
         STH   @09,DRWSEEK+5(,R12)                                 0386 00757000
*         END;                      /* END OF DO             @XA03929*/ 00758000
*       ELSE                                                       0388 00759000
*         ;                         /* NOT END OF CYL        @XA03929*/ 00760000
@RF00383 DS    0H                                                  0389 00761000
*       DRWECC=DRWSEEK(4:7);        /* SET END CCHH          @Y30SSDW*/ 00762000
         MVC   DRWECC(4,R12),DRWSEEK+3(R12)                        0389 00763000
*     END;                          /* END OF NON-CRA        @Y30SSDW*/ 00764000
*/********************************************************************/ 00765000
*/*                  PREPARE  THE  READ  DATA  CCW                   */ 00766000
*/********************************************************************/ 00767000
*                                                                  0391 00768000
*   RFY                                                            0391 00769000
*     R9 RSTD;                                                     0391 00770000
@RC00259 DS    0H                                                  0392 00771000
*   R9=ADDR(DRWCCW3);               /* GET READ DATA CCW             */ 00772000
         LA    R9,DRWCCW3(,R12)                                    0392 00773000
*   CCWCMD='06'X;                   /* SET READ DATA CMD             */ 00774000
