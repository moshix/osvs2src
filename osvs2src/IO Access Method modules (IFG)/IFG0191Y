         TITLE 'IFG0191Y - VSAM CATALOG OPEN ACB PROCESSOR, LOAD 2     *00001000
                        '                                               00002000
IFG0191Y CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IFG0191Y  78.135'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @03,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@03                                         0001 00012000
         MODESET EXTKEY=ZERO,SAVEKEY=(2)                                00013000
*   KEYSAVE=RKEYSAVE;               /* SAVE KEY OF CALLER      Y02020*/ 00014000
         STC   RKEYSAVE,KEYSAVE(,RCORE)                            0135 00015000
*   RFY                                                            0136 00016000
*     RKEYSAVE UNRSTD;              /*                         Y02020*/ 00017000
*   RFY                                                            0137 00018000
*    (RCORE,                                                       0137 00019000
*     RES,                                                         0137 00020000
*     RWTG,                                                        0137 00021000
*     RPARC,                                                       0137 00022000
*     RWTGC) RSTD;                  /*                         Y02020*/ 00023000
*   RESPECIFY                                                      0138 00024000
*     INFMJFCB BASED(ADDR(DXJBF));  /*                         Y02020*/ 00025000
*   RPARC=DXPDCBAD;                 /* GET ACB ADDR            Y02020*/ 00026000
         L     RPARC,DXPDCBAD(,RCORE)                              0139 00027000
*   DXREGE=RES;                     /* SAVE RETURN ADDR        Y02020*/ 00028000
         ST    RES,DXREGE(,RCORE)                                  0140 00029000
*   RFY                                                            0141 00030000
*     RES UNRSTD;                   /*                         Y02020*/ 00031000
*/********************************************************************/ 00032000
*/*     OBTAIN MAIN STORAGE SPACE FOR DSCB READ WORKAREA (DRWA)      */ 00033000
*/********************************************************************/ 00034000
*                                                                  0142 00035000
*   RFY                                                            0142 00036000
*     R1 RSTD;                                                     0142 00037000
*   RFY                                                            0143 00038000
*     SMWA BASED(R1);                                              0143 00039000
*   R1=RCORE;                       /* USE OPEN WORKAREA       Y02020*/ 00040000
         LR    R1,RCORE                                            0144 00041000
*   SMLEN=LENGTH(IGGMDRWA);         /* SIZE OF DRWA                  */ 00042000
         MVC   SMLEN(4,R1),@CF04136                                0145 00043000
*   SMADR=ADDR(SMCOR);              /* SET ADDR OF CORE PTR          */ 00044000
         LA    @12,SMCOR(,R1)                                      0146 00045000
         ST    @12,SMADR(,R1)                                      0146 00046000
*   SMSPL=USUBPOOL;                 /* SET SUBPOOL ID.               */ 00047000
         MVI   SMSPL(R1),X'FC'                                     0147 00048000
*   GEN(GETMAIN EC,MF=(E,(1)));     /* ISSUE GETMAIN                 */ 00049000
         GETMAIN EC,MF=(E,(1))                                          00050000
*   R1=RCORE;                       /* RESET WORKAREA PTR      Y02020*/ 00051000
         LR    R1,RCORE                                            0149 00052000
*   RFY                                                            0150 00053000
*     R15 RSTD;                                                    0150 00054000
*   IF R15^=ZERO THEN               /* GETMAIN FAILED                */ 00055000
         LTR   R15,R15                                             0151 00056000
         BZ    @RF00151                                            0151 00057000
*     DO;                                                          0152 00058000
*       RFY                                                        0153 00059000
*         R15 UNRSTD;                                              0153 00060000
*       ACBERFLG=RCINSSWA;          /* SET ERROR CODE                */ 00061000
         MVI   ACBERFLG(RPARC),X'A4'                               0154 00062000
*       CAXF2NCR=ON;                /* SET NO CORE SW                */ 00063000
         L     @12,ACBUAPTR(,RPARC)                                0155 00064000
         OI    CAXF2NCR(@12),B'00100000'                           0155 00065000
*       CALL CAPERR;                /* CALL ERROR PROC               */ 00066000
         BAL   @14,CAPERR                                          0156 00067000
*     END;                                                         0157 00068000
*   ELSE                                                           0158 00069000
*     ;                             /* GETMAIN SUCCESSFUL            */ 00070000
@RF00151 DS    0H                                                  0159 00071000
*   CAXDRWP=SMCOR;                  /* GET DRWA CORE ADDR &CLEAR     */ 00072000
         L     @12,ACBUAPTR(,RPARC)                                0159 00073000
         L     @09,SMCOR(,R1)                                      0159 00074000
         ST    @09,CAXDRWP(,@12)                                   0159 00075000
*   RFY                                                            0160 00076000
*     R1 UNRSTD;                                                   0160 00077000
*   RFY                                                            0161 00078000
*     R12 RSTD;                     /*                         Y02020*/ 00079000
*   R12=CAXDRWP;                    /* POINT TO DSCB READ AREA Y02020*/ 00080000
         LR    R12,@09                                             0162 00081000
*   RFY                                                            0163 00082000
*     IGGMDRWA BASED(R12);          /*                         Y02020*/ 00083000
*/********************************************************************/ 00084000
*/*                  CLEAR  DSCB  READ  WORKAREA                     */ 00085000
*/********************************************************************/ 00086000
*                                                                  0164 00087000
*   IGGMDRWA(1:256)=IGGMDRWA(1:256)&&IGGMDRWA(1:256);              0164 00088000
         XC    IGGMDRWA(256,R12),IGGMDRWA(R12)                     0164 00089000
*   IGGMDRWA(257:512)=IGGMDRWA(257:512)&&IGGMDRWA(257:512);        0165 00090000
         XC    IGGMDRWA+256(256,R12),IGGMDRWA+256(R12)             0165 00091000
*   IGGMDRWA(513:LENGTH(IGGMDRWA))=IGGMDRWA(513:LENGTH(IGGMDRWA))&&     00092000
*       IGGMDRWA(513:LENGTH(IGGMDRWA));                            0166 00093000
*                                                                  0166 00094000
         XC    IGGMDRWA+512(204,R12),IGGMDRWA+512(R12)             0166 00095000
*/********************************************************************/ 00096000
*/*    BUILD VOLUME LIST FOR THE VOLUME MOUNT AND VERIFY ROUTINE     */ 00097000
*/*    IDA0192V.  LOAD MODULE IDA0192V AND BRANCH TO IT.  DELETE     */ 00098000
*/*    THE MODULE, AND CALL 'CAPERR' IF IDA0192V ENCOUNTERED AN      */ 00099000
*/*    ERROR.  IF NO ERRORS, SET THE UCB ADDRESS IN THE CAXWA AND    */ 00100000
*/*    CONTINUE PROCESSING THE OPEN REQUEST.                         */ 00101000
*/********************************************************************/ 00102000
*                                                                  0167 00103000
*   RFY                                                            0167 00104000
*     IDAVVOLL BASED(CAXDRWP);                                     0167 00105000
*   VVOLLID=ON;                     /* SET VOL LIST INDICATOR        */ 00106000
*   VVOLLCAT=ON;                    /* SET CATLG INDICATOR           */ 00107000
         OI    VVOLLID(@09),B'01000100'                            0169 00108000
*   VVOLLNBR=ONE;                   /* SET NO. OF VOLUMES            */ 00109000
         MVC   VVOLLNBR(2,@09),@CH00034                            0170 00110000
*   VVOLLACB=ZERO;                  /* ZERO ACB PTR,NO TMSP CHK      */ 00111000
         SLR   @07,@07                                             0171 00112000
         ST    @07,VVOLLACB(,@09)                                  0171 00113000
*   VVOLLVSR(1)=JFCBVOLS(1:6);      /* MOVE VOL SER                  */ 00114000
         MVC   VVOLLVSR(6,@09),JFCBVOLS+100(RCORE)                 0172 00115000
*   IF ACBSCRA=ON&                  /* SYSTEM CRA REQUEST    @Y30SSDW*/ 00116000
*       CAXRATEP^=ZERO THEN         /* RECOVERY AREA TIOT POINTER  0173 00117000
*                                                            @Y30SSDW*/ 00118000
         TM    ACBSCRA(RPARC),B'00001000'                          0173 00119000
         BNO   @RF00173                                            0173 00120000
         L     @05,ACBUAPTR(,RPARC)                                0173 00121000
         L     @05,CAXRATEP(,@05)                                  0173 00122000
         CR    @05,@07                                             0173 00123000
         BE    @RF00173                                            0173 00124000
*     VVOLLTAB=CAXRATEP;            /* SET PTR TO TIOT ENTRY @Y30SSDW*/ 00125000
         ST    @05,VVOLLTAB(,@09)                                  0174 00126000
*   ELSE                            /* NOT SCRA REQUEST      @Y30SSDW*/ 00127000
*     VVOLLTAB=CAXATIOT+ACBTIOT;    /* SET PTR TO TIOT ENTRY         */ 00128000
         B     @RC00173                                            0175 00129000
@RF00173 L     @09,ACBUAPTR(,RPARC)                                0175 00130000
         L     @07,CAXDRWP(,@09)                                   0175 00131000
         LH    @05,ACBTIOT(,RPARC)                                 0175 00132000
         AL    @05,CAXATIOT(,@09)                                  0175 00133000
         ST    @05,VVOLLTAB(,@07)                                  0175 00134000
*/********************************************************************/ 00135000
*/*       LOAD  AND  CALL  MODULE  IDA0192V  TO  MOUNT  VOLUME       */ 00136000
*/********************************************************************/ 00137000
*                                                                  0176 00138000
*   RFY                                                            0176 00139000
*    (R0,                                                          0176 00140000
*     R1) RSTD;                                                    0176 00141000
@RC00173 DS    0H                                                  0177 00142000
*   R1=CVTSVDCB;                    /* GET SVCLIB DCB ADDR           */ 00143000
         L     @09,CVTPTR                                          0177 00144000
         L     R1,CVTSVDCB(,@09)                                   0177 00145000
*   DRWDSCB(1:8)=M192V;             /* SET MODULE NAME               */ 00146000
         MVC   DRWDSCB(8,R12),@CC04068                             0178 00147000
*   R0=ADDR(DRWDSCB);               /* SET ADDR OF NAME              */ 00148000
         LA    R0,DRWDSCB(,R12)                                    0179 00149000
*   GEN(LOAD EPLOC=(0),DCB=(1));                                   0180 00150000
         LOAD EPLOC=(0),DCB=(1)                                         00151000
*   RFY                                                            0181 00152000
*    (R13,                                                         0181 00153000
*     R14,                                                         0181 00154000
*     R15) RSTD;                                                   0181 00155000
*   R13=ADDR(DRWDSCB);              /* USE AS SAVE AREA              */ 00156000
         LA    R13,DRWDSCB(,R12)                                   0182 00157000
*   R15=R0;                         /* ENTRY POINT ADDR OF 192V      */ 00158000
         LR    R15,R0                                              0183 00159000
*   R1=ADDR(IDAVVOLL);              /* ADDR OF LIST                  */ 00160000
         L     @09,ACBUAPTR(,RPARC)                                0184 00161000
         L     R1,CAXDRWP(,@09)                                    0184 00162000
*   GEN(BALR R14,R15);              /* BR TO IDA0192V                */ 00163000
         BALR R14,R15                                                   00164000
*/********************************************************************/ 00165000
*/*              CHECK  FOR  VOLUME  MOUNT  ERRORS                   */ 00166000
*/********************************************************************/ 00167000
*                                                                  0186 00168000
*   IF R15^=ZERO|VVOLLMNT(1)^=ON THEN/* ERROR                        */ 00169000
         LTR   R15,R15                                             0186 00170000
         BNZ   @RT00186                                            0186 00171000
         L     @09,ACBUAPTR(,RPARC)                                0186 00172000
         L     @09,CAXDRWP(,@09)                                   0186 00173000
         TM    VVOLLMNT(@09),B'10000000'                           0186 00174000
         BO    @RF00186                                            0186 00175000
@RT00186 DS    0H                                                  0187 00176000
*     DO;                                                          0187 00177000
*       R0=ADDR(DRWDSCB);           /* ADDR OF NAME                  */ 00178000
         LA    R0,DRWDSCB(,R12)                                    0188 00179000
*       DRWDSCB(1:8)=M192V;         /* MOD NAME IN                   */ 00180000
         MVC   DRWDSCB(8,R12),@CC04068                             0189 00181000
*       GEN(DELETE EPLOC=(0));      /* DELETE 192V                   */ 00182000
         DELETE EPLOC=(0)                                               00183000
*       CALL CAPERR;                /* ERROR EXIT                    */ 00184000
         BAL   @14,CAPERR                                          0191 00185000
*     END;                          /* END OF DO FOR ERROR           */ 00186000
*   ELSE                                                           0193 00187000
*     ;                             /* NO ERROR                      */ 00188000
@RF00186 DS    0H                                                  0194 00189000
*   IF ACBSCRA=ON THEN              /* SYSTEM CRA REQUEST    @Y30SSDW*/ 00190000
         TM    ACBSCRA(RPARC),B'00001000'                          0194 00191000
         BNO   @RF00194                                            0194 00192000
*     DO;                           /*                       @Y30SSDW*/ 00193000
*       ACBTIOT=LENGTH(TIOT1);      /* SET TIOT OFFSET       @Y30SSDW*/ 00194000
         LA    @09,24                                              0196 00195000
         STH   @09,ACBTIOT(,RPARC)                                 0196 00196000
*       RFY                                                        0197 00197000
*         TIOT1 BASED(CAXATIOT);    /*                       @Y30SSDW*/ 00198000
*       TIOEFSRT=VVOLLUCB(1);       /* SET UCB ADDRESS       @Y30SSDW*/ 00199000
         L     @07,ACBUAPTR(,RPARC)                                0198 00200000
         AL    @09,CAXATIOT(,@07)                                  0198 00201000
         L     @07,CAXDRWP(,@07)                                   0198 00202000
         MVC   TIOEFSRT(3,@09),VVOLLUCB+1(@07)                     0198 00203000
*       RFY                                                        0199 00204000
*         TIOT1 BASED(TIOTPTR);     /*                       @Y30SSDW*/ 00205000
*     END;                          /*                       @Y30SSDW*/ 00206000
*   ELSE                                                           0201 00207000
*     ;                             /* NOT SCRA REQUEST      @Y30SSDW*/ 00208000
@RF00194 DS    0H                                                  0202 00209000
*   R0=ADDR(DRWDSCB);               /* ADDR OF NAME                  */ 00210000
         LA    R0,DRWDSCB(,R12)                                    0202 00211000
*   DRWDSCB(1:8)=M192V;             /* MOD NAME FOR DELETE           */ 00212000
         MVC   DRWDSCB(8,R12),@CC04068                             0203 00213000
*   GEN(DELETE EPLOC=(0));          /* REMOVE VOLUME MOUNT MODULE    */ 00214000
         DELETE EPLOC=(0)                                               00215000
*   CAXUCB=VVOLLUCB(1);             /* UCB ADDR INTO CAXWA           */ 00216000
         L     @09,ACBUAPTR(,RPARC)                                0205 00217000
         L     @07,CAXDRWP(,@09)                                   0205 00218000
         L     @07,VVOLLUCB(,@07)                                  0205 00219000
         ST    @07,CAXUCB(,@09)                                    0205 00220000
*   IGGMDRWA(1:256)=IGGMDRWA(1:256)&&IGGMDRWA(1:256);/* CLEAR CORE 0206 00221000
*                                      USED FOR VOLUME MOUNT         */ 00222000
         XC    IGGMDRWA(256,R12),IGGMDRWA(R12)                     0206 00223000
*   RFY                                                            0207 00224000
*    (R0,                                                          0207 00225000
*     R1,                                                          0207 00226000
*     R13,                                                         0207 00227000
*     R14,                                                         0207 00228000
*     R15) UNRSTD;                                                 0207 00229000
*                                                                  0207 00230000
*/********************************************************************/ 00231000
*/*       BUILD  DATA  EXTENT  BLOCK  (DEB)  TO  READ  F4  DSCB      */ 00232000
*/********************************************************************/ 00233000
*                                                                  0208 00234000
*   DRWDEXT='01'X;                  /* NUM OF EXTS = 1               */ 00235000
         MVI   DRWDEXT(R12),X'01'                                  0208 00236000
*   DRWDDCB=ADDR(DRWDCB);           /* DCB PTR                       */ 00237000
         LA    @09,DRWDCB(,R12)                                    0209 00238000
         STCM  @09,7,DRWDDCB(R12)                                  0209 00239000
*   DRWDID='0F'X;                   /* SET DEB ID                    */ 00240000
         MVI   DRWDID(R12),X'0F'                                   0210 00241000
*   DRWDAPP=CVTXAPG;                /* IOS APP.TAB ADDR              */ 00242000
         L     @09,CVTPTR                                          0211 00243000
         MVC   DRWDAPP(3,R12),CVTXAPG+1(@09)                       0211 00244000
*   DRWDEXS='04'X;                  /* EXT SCALE                     */ 00245000
         MVI   DRWDEXS(R12),X'04'                                  0212 00246000
*   DRWDMOD=X0;                     /* SET FILE MASK                 */ 00247000
         MVI   DRWDMOD(R12),X'00'                                  0213 00248000
*   DRWECC='FFFFFFFF'X;             /* SET TO END OF PACK            */ 00249000
         MVC   DRWECC(4,R12),@CB04098                              0214 00250000
*   DRWNTK='7FFF'X;                 /* NO. TRKS                      */ 00251000
         MVC   DRWNTK(2,R12),@CB04099                              0215 00252000
*   DRWDEBP=ADDR(DRWDEB);           /* DCB DEB PTR                   */ 00253000
         ST    R12,DRWDEBP(,R12)                                   0216 00254000
*   DRWDUCB=CAXUCB;                 /* UCB ADDR INTO DEB             */ 00255000
         STCM  @07,7,DRWDUCB(R12)                                  0217 00256000
*/********************************************************************/ 00257000
*/*          BUILD  I/O  BLOCK  (IOB)  TO  READ  F4  DSCB            */ 00258000
*/********************************************************************/ 00259000
*                                                                  0218 00260000
*IOBBLD:                                                           0218 00261000
*   DRWIECB=ADDR(DRWECB);           /* ECB PTR INTO IOB              */ 00262000
IOBBLD   LA    @09,DRWECB(,R12)                                    0218 00263000
         ST    @09,DRWIECB(,R12)                                   0218 00264000
*   DRWSCP=ADDR(DRWCCW1);           /* START OF CCW'S                */ 00265000
         LA    @09,DRWCCW1(,R12)                                   0219 00266000
         ST    @09,DRWSCP(,R12)                                    0219 00267000
*   DRWIDCB=ADDR(DRWDCB);           /* DCB PTR INTO IOB              */ 00268000
         LA    @09,DRWDCB(,R12)                                    0220 00269000
         ST    @09,DRWIDCB(,R12)                                   0220 00270000
*/********************************************************************/ 00271000
*/*      GET TTR OF VTOC - (THE CONTENTS OF REGISTERS 9-13           */ 00272000
*/*      ARE DESTROYED BY THE CONVERT ROUTINE.)                      */ 00273000
*/********************************************************************/ 00274000
*                                                                  0221 00275000
*   RFY                                                            0221 00276000
*     UCBOB BASED(DRWDUCB);         /* TEMP BASE                     */ 00277000
*   RFY                                                            0222 00278000
*    (R0,                                                          0222 00279000
*     R1,                                                          0222 00280000
*     R9,                                                          0222 00281000
*     R10,                                                         0222 00282000
*     R11,                                                         0222 00283000
*     R13,                                                         0222 00284000
*     R15) RSTD;                    /*                         Y02020*/ 00285000
*   R0=UCBVTOC;                     /* TTR OF VTOC IN REG 0          */ 00286000
         ICM   @14,7,DRWDUCB(R12)                                  0223 00287000
         L     R0,UCBVTOC(,@14)                                    0223 00288000
*   R15=CVTPCNVT;                   /* ADDR OF TTR CONVERT RTN       */ 00289000
         L     @07,CVTPTR                                          0224 00290000
         L     R15,CVTPCNVT(,@07)                                  0224 00291000
*   R2=ADDR(DRWSEEK);               /* ADDR OF RESULT FIELD          */ 00292000
         LA    R2,DRWSEEK(,R12)                                    0225 00293000
*   R1=ADDR(DRWDEB);                /* ADDR OF DEB                   */ 00294000
         LR    R1,R12                                              0226 00295000
*   DXREGC=R12;                     /* SAVE REGISTER 12        Y02020*/ 00296000
         ST    R12,DXREGC(,RCORE)                                  0227 00297000
*   RFY                                                            0228 00298000
*     UCBOB BASED(CAXUCB);          /* RESET BASE                    */ 00299000
*   RFY                                                            0229 00300000
*    (R0,                                                          0229 00301000
*     R1,                                                          0229 00302000
*     R9,                                                          0229 00303000
*     R10,                                                         0229 00304000
*     R11,                                                         0229 00305000
*     R13,                                                         0229 00306000
*     R15) UNRSTD;                  /*                         Y02020*/ 00307000
*   GEN(BALR R14,R15);              /* BR TO TTR CONVERT ROUTINE     */ 00308000
         BALR R14,R15                                                   00309000
*   R12=DXREGC;                     /* RESTORE REG 12          Y02020*/ 00310000
         L     R12,DXREGC(,RCORE)                                  0231 00311000
*   RPARC=DXPDCBAD;                 /* RESTORE REG 2           Y02020*/ 00312000
         L     RPARC,DXPDCBAD(,RCORE)                              0232 00313000
*/********************************************************************/ 00314000
*/*        BUILD  CHANNEL  PROGRAM  TO  READ  FORMAT  4  DSCB        */ 00315000
*/********************************************************************/ 00316000
*                                                                  0233 00317000
*   RFY                                                            0233 00318000
*     R9 RSTD;                                                     0233 00319000
*   RFY                                                            0234 00320000
*     WCCW BASED(R9);                                              0234 00321000
*   R9=DRWSCP;                      /* ADDR OF CCW1            Y02020*/ 00322000
         L     @07,DRWSCP(,R12)                                    0235 00323000
         LR    R9,@07                                              0235 00324000
*   CCWCMD='31'X;                   /* SEARCH ID EQUAL CMD           */ 00325000
         MVI   CCWCMD(R9),X'31'                                    0236 00326000
*   CCWDTA=ADDR(DRWSEEK)+3;         /* ARGUEMENT                     */ 00327000
         LA    @05,DRWSEEK(,R12)                                   0237 00328000
         AH    @05,@CH00032                                        0237 00329000
         STCM  @05,7,CCWDTA(R9)                                    0237 00330000
*   CCWFLG='40'X;                   /* COMMAND CHAINING IND.         */ 00331000
         MVI   CCWFLG(R9),X'40'                                    0238 00332000
*   CCWBTE=BYTECNT5;                /* SET BYTE COUNT                */ 00333000
         MVC   CCWBTE(2,R9),@CH00051                               0239 00334000
*   R9=R9+LENGTH(WCCW);             /* ADJUST TO NEXT CCW            */ 00335000
         LA    @05,8                                               0240 00336000
         ALR   R9,@05                                              0240 00337000
*   CCWCMD='08'X;                   /* TIC                           */ 00338000
         MVI   CCWCMD(R9),X'08'                                    0241 00339000
*   CCWDTA=DRWSCP;                  /* CCW1                    Y02020*/ 00340000
         STCM  @07,7,CCWDTA(R9)                                    0242 00341000
*   R9=R9+LENGTH(WCCW);             /* ADJUST TO CCW3                */ 00342000
         ALR   R9,@05                                              0243 00343000
*   CCWCMD='06'X;                   /* READ DATA F4 DSCB             */ 00344000
         MVI   CCWCMD(R9),X'06'                                    0244 00345000
*   CCWDTA=ADDR(DRWDSCB);           /* BUFFER AREA ADDR              */ 00346000
         LA    @07,DRWDSCB(,R12)                                   0245 00347000
         STCM  @07,7,CCWDTA(R9)                                    0245 00348000
*   CCWBTE=LENGTH(IECSDSL4);        /* BYTE COUNT                    */ 00349000
         MVC   CCWBTE(2,R9),@CH00148                               0246 00350000
*/********************************************************************/ 00351000
*/*           EXCP  AND  WAIT  TO  READ  FORMAT  4  DSCB             */ 00352000
*/********************************************************************/ 00353000
*                                                                  0247 00354000
*   RFY                                                            0247 00355000
*     R1 RSTD;                                                     0247 00356000
*   R1=ADDR(DRWIOB);                /* ADDR OF IOB                   */ 00357000
         LA    R1,DRWIOB(,R12)                                     0248 00358000
*   GEN(EXCP (1));                  /* ISSUE EXCP COMMAND            */ 00359000
         EXCP (1)                                                       00360000
*   R1=ADDR(DRWECB);                /* ECB ADDR                      */ 00361000
         LA    R1,DRWECB(,R12)                                     0250 00362000
*   GEN(WAIT ECB=(1));              /* ISSUE WAIT COMMAND            */ 00363000
         WAIT ECB=(1)                                                   00364000
*   RFY                                                            0252 00365000
*     R1 UNRSTD;                                                   0252 00366000
*   IF DRWECMP^='7F'X THEN          /* I/O ERROR                     */ 00367000
         CLI   DRWECMP(R12),127                                    0253 00368000
         BE    @RF00253                                            0253 00369000
*     DO;                                                          0254 00370000
*       CAXF2IOE=ON;                /* SET I/O ERROR SW              */ 00371000
         L     @07,ACBUAPTR(,RPARC)                                0255 00372000
         OI    CAXF2IOE(@07),B'00010000'                           0255 00373000
*       CALL CAPERR;                /* CALL ERROR PROC               */ 00374000
         BAL   @14,CAPERR                                          0256 00375000
*     END;                                                         0257 00376000
*   ELSE                                                           0258 00377000
*     ;                             /* NO I/O ERROR                  */ 00378000
@RF00253 DS    0H                                                  0259 00379000
*/********************************************************************/ 00380000
*/*        IF CRA REQUEST, PREPARE TO READ CI 0 ON THE VOLUME        */ 00381000
*/********************************************************************/ 00382000
*                                                                  0259 00383000
*   IF ACBSCRA=ON|                  /* SYSTEM CRA REQUEST    @Y30SSDW*/ 00384000
*       ACBUCRA=ON THEN             /* OR USER CRA REQUEST   @Y30SSDW*/ 00385000
