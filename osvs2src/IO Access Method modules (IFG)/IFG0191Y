*   CCWFLG=X0;                      /* RESET FLGS                    */ 00779000
         MVI   CCWFLG(R9),X'00'                                    0395 00780000
*   CCWBTE=LENGTH(DRWRIA);          /* RECORD LENGTH                 */ 00781000
         MVC   CCWBTE(2,R9),@CH01071                               0396 00782000
*   RFY                                                            0397 00783000
*     R9 UNRSTD;                                                   0397 00784000
*                                                                  0397 00785000
*/********************************************************************/ 00786000
*/*          EXCP  AND  WAIT  TO  READ  A  512-BYTE  RECORD          */ 00787000
*/********************************************************************/ 00788000
*                                                                  0398 00789000
*   RFY                                                            0398 00790000
*     R1 RSTD;                                                     0398 00791000
*   R1=ADDR(DRWIOB);                /* IOB PTR                       */ 00792000
         LA    R1,DRWIOB(,R12)                                     0399 00793000
*   GEN(EXCP (1));                  /* EXCP                          */ 00794000
