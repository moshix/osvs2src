*       SLL(R0,ONEBYTE);            /* LEFT JUSTIFY 3-BYTE TTR IN  0272 00417000
*                                      REGISTER ZERO         @Y30SSDW*/ 00418000
         SLL   R0,8                                                0272 00419000
*       R1=ADDR(DRWDEB);            /* ADDRESS OF DEB        @Y30SSDW*/ 00420000
