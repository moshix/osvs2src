*         END;                      /*                       @Y30SSDW*/ 00842000
*       ELSE                        /* DATA RECORD FOUND     @Y30SSDW*/ 00843000
*         CAXRACTS=RCDRACTS;        /* SET CRA CREATION TIMESTAMP IN    00844000
*                                      CAXWA FROM DATA RECORD      0419 00845000
*                                                            @Y30SSDW*/ 00846000
         B     @RC00414                                            0419 00847000
@RF00414 L     @09,ACBUAPTR(,RPARC)                                0419 00848000
