         XC    DXATEXC1,DXATEXC1  *     ZERO AUDIT TRAIL       @Y021139 22100002
         XC    DXATEXC2,DXATEXC2  *     ZERO AUDIT TRAIL       @Y021139 22150002
*                                                                       22200002
         MVC   RSTATRTN,IDARMAIN        NOTE IFG2A IN CONTROL  @Y30DSDT 22210003
*        RSTATOPT=IDARLOAD; /* NOTE ENTRY TO IFG0192A.       @Y30DSDT*/ 22250003
         MVI   RSTATOPT,IDARLOAD * NOTE ENTRY TO IFG0192A      @Y30DSDT 22300003
*                                                                       22310002
         L     R9,WTGPREFX *      GET ADDRESS OF PREFIX.       @Y021139 22320002
         USING IECPREFX,R9 *      BASE REGISTER FOR PREFIX.    @Y021139 22330002
         L     R9,IECRRPRM *      ADDRESS OF RRPL IN R9.       @Y021139 22340002
         USING IECRRPL,R9 *       ADDRESSABILITY IS R9.        @Y021139 22342002
*                                                                       22344002
*        RRFAMSTR = ON;   /* NOTE VSAM STRING IN CONTROL.    @Y021139*/ 22346002
         OI    RRFLAGS1,RRFAMSTR  VSAM IS IN CONTROL.          @Y021139 22348002
*                                                                       22348402
         L     R0,SPLNG                 GET SUB-POOL & LENGTH           22350090
*                                       AND MODULE ID LENGTH    Z30SSRL 22400003
         GETMAIN R,LV=(R0)              GETMAIN FOR SAVEAREA            22800090
         LR    RSAVE,R1                 POINT TO SAVEAREA               23400090
         XC    SVAREA(SVAREASZ),SVAREA  CLEAR SAVEAREA                  23450090
         LA    R11,SVAREASZ             LOAD SAVE AREA SIZE     Z30SSRL 23500003
         ST    R11,SAVESIZE             STORE THE SIZE          Z30SSRL 23550003
         MVC   SVAREAID,MODULEID        MOVE IN MOD ID & DATE   Z30SSRL 23600003
         ST    RSAVE,DXCCW7             CHAIN WITH FORCORE      Z30SSRL 23650003
         L     R10,CVTPTR               POINT TO CVT                    24000090
*                                                                       24600090
         CLC   OPSVC,WTGMODNM+OFSSVC    TEST IF WTG INDICATES OPEN      25200090
         BNE   TESTCL                   BRANCH IF NO                    25800090
         CLC   IIOPENID,WTGMODID        TEST FOR ISAM INTERFACE OPEN    26400090
         BE    IIOPEN                   BRANCH IF YES                   27000090
         MVC   MODNAM,OPENNM            MOVE OPEN MODULE NAME           27600090
*                                                                       27610002
*        RSTATOPT=IDAROPEN; /* NOTE OPTION IS VSAM OPEN.     @Y021139*/ 27620002
         MVI   RSTATOPT,IDAROPEN * THIS IS A VSAM OPEN.        @YM38159 27630002
*                                                                       27640002
         L     R11,CVTSVDCB             POINT TO SVCLIB DCB             27650090
         LOAD  EPLOC=MODNAM,DCB=(R11)   LOAD VSAM OPEN                  28200090
         LR    R15,R0                   SET ENTRY POINT IN REG 15       28800090
         BALR  RET,R15                  BRANCH TO VSAM OPEN             29400090
         DELETE EPLOC=MODNAM            DELETE VSAM OPEN                30000090
         MODESET EXTKEY=DATAMGT         OBTAIN KEY FIVE        @Y021139 30600002
*        THE FOLLOWING IECRES IS USED TO MOVE THE COPIED ACB   @Y021139 30650002
*        BACK TO THE USER AREA.                                @Y021139 30660002
         IECRES INIT,DCBCOPY=FRWKAR,STM=(0,14,WTGPREFX)        @Y021139 30700002
         L     R1,DXDEBXAD              POINT TO DEBXTN        @Y021139 30800002
         LH    R0,DEBXLNGH-DEBXTN(R1)   LOAD LENGTH OF DEBXTN  @Y021139 30900002
         ICM   R0,8,MDEBPOOL            INSERT DEBXTN SUBPOOL  @Y021139 31000002
         FREEMAIN R,LV=(R0),A=(R1)      FREEMAIN MODEL DEBXTN  @Y021139 31100002
         SR    R1,R1                    R1=0                   @Y021139 31200002
         ST    R1,DXDEBXAD              DXDEBXAD=0             @Y021139 31300002
         MODESET EXTKEY=ZERO            OBTAIN KEY ZERO        @Y021139 31400002
