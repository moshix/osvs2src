*     GEN(GTRACE ID=4095,PAGEIN=YES,MF=(E,(1)));/* GTRACE      YM7364*/ 01130000
         GTRACE ID=4095,PAGEIN=YES,MF=(E,(1))                           01131000
*     R9=R9-256;                    /* DECREMENT DATA LNG COUNTER    */ 01132000
         LA    @11,256                                             0505 01133000
         SLR   R9,@11                                              0505 01134000
*     RC=RC+256;                    /* INCREMENT DATA PTR            */ 01135000
         ALR   RC,@11                                              0506 01136000
*   END;                            /* CHK FOR END OF LOOP           */ 01137000
@DE00499 SLR   @11,@11                                             0507 01138000
         CR    RF,@11                                              0507 01139000
         BNE   @DC00499                                            0507 01140000
