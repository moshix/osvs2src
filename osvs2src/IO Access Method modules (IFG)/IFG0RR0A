*     ELSE                          /* DATA LNG < 256                */ 01124000
*       GTRACLNG=R9;                /* SET DATA LNG < 256            */ 01125000
         B     @RC00500                                            0502 01126000
@RF00500 STH   R9,GTRACLNG(,RD)                                    0502 01127000
*     GTRACDAP=RC;                  /* SET DATA PTR FIELD            */ 01128000
