         BE    @RF00919                                            0919 02277000
*               DO;                 /* NOT INDICATED THEN            */ 02278000
*                 RC=RC+44;         /* INCR PAST DSNAME              */ 02279000
         LA    RC,44(,RC)                                          0921 02280000
*                 IF RC->DXDSCB(1)^='1' THEN/* IF NOT A FMT 1        */ 02281000
         CLI   DXDSCB(RC),C'1'                                     0922 02282000
         BNE   @RT00922                                            0922 02283000
*                   GOTO DMCTOP1;   /* DSCB THEN GO CHK CHAIN        */ 02284000
*               END;                                               0924 02285000
*             IF RC^=RC->DSCCORE&   /* IF CHN PT ^= NEXT PT    YM7071*/ 02286000
