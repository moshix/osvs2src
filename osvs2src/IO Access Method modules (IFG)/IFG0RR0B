@ER00017 LM    @14,@12,12(@13)                                     0896 02150000
         BR    @14                                                 0896 02151000
*/********************************************************************/ 02152000
*/*                    DECREMENT DSAB OPEN COUNT                     */ 02153000
*/********************************************************************/ 02154000
*                                                                  0897 02155000
*DSABCALL:                                                         0897 02156000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0897 02157000
DSABCALL DS    0H                                                  0898 02158000
*   EVAL(ADDR(RA->TRRSAV2))->SARETRY=RET;/* SAVE RETURN ADDR         */ 02159000
         LA    @15,TRRSAV2(,RA)                                    0898 02160000
         ST    RET,SARETRY(,@15)                                   0898 02161000
*   DSABPTR=DXDSABAD;               /* GET CURRENT DSAB              */ 02162000
         L     DSABPTR,DXDSABAD(,RCORE)                            0899 02163000
*   IF DSABPTR=0 THEN               /* IF THERE IS NO DSAB THEN      */ 02164000
         LTR   DSABPTR,DSABPTR                                     0900 02165000
         BZ    @RT00900                                            0900 02166000
*     GOTO DSABC070;                /* GO RETURN TO CALLER           */ 02167000
*DSABC010:                          /* GET DSAB OPEN COUNT           */ 02168000
*   GEN REFS(DSABOPCT,DSABFCHN,DSABTIOT,DSABBCHN,TIOEDDNM,DXTIOTAD,     02169000
*       DXDSABAD);                                                 0902 02170000
DSABC010 DS    0H                                                  0902 02171000
         L     RD,DSABOPCT-2(,DSABPTR)  DSABOPCT 2ND HALFWORD           02172000
DSABC020 MODESET EXTKEY=SCHED           ASSUME SCHEDULER KEY            02173000
         LR    RET,RD                   SAVE FOR CS IN RD               02174000
         SRDL  RET,16                   SHIFT OPEN CT TO RF             02175000
         SRA   RF,16                    PROPOGATE SIGN FOR LTR          02176000
         LTR   RF,RF                    MAKE SURE ITS POSITIVE          02177000
         BNP   DSABC030                 BRANCH IF NO                    02178000
         BCTR  RF,R0                    DECREMENT BY ONE                02179000
         SLL   RF,16                    SHIFT TO HIGH 16 BITS           02180000
         SLDL  RET,16                   UPDATED FULLWORD IN RET         02181000
         CS    RD,RET,DSABOPCT-2(DSABPTR) TRY TO SNEAK IT IN            02182000
         BNE   DSABC020                 BR IF HAVE TO TRY AGAIN         02183000
DSABC030 MODESET EXTKEY=DATAMGT         DATA MANAGEMENT KEY             02184000
*        DECREMENT COUNT IN ALL DSABS WHOSE TIOT                        02185000
*        ENTRIES HAVE BLANK DDNAMES                                     02186000
         L     RC,DSABFCHN(,DSABPTR)    NEXT DSAB                       02187000
         LTR   RC,RC                    IS THERE A NEXT DSAB            02188000
         BZ    DSABC040                 BRANCH IF NO                    02189000
         L     TIOTPTR,DSABTIOT(,DSABPTR) GET TIOT ENTRY                02190000
         CLI   TIOEDDNM-24(TIOTPTR),C' '            CONCATENATION       02191000
         BE    DSABC010                 GET NEXT DSAB                   02192000
DSABC040 L     TIOTPTR,DXTIOTAD(,RCORE) RESTORE TIOT ADDRESS            02193000
         L     DSABPTR,DXDSABAD(,RCORE) GET CURRENT DSAB                02194000
DSABC050 CLI   TIOEDDNM-24(TIOTPTR),C' ' IS THIS TIOT ENTRY             02195000
*                                       CONCATENATED                    02196000
         BNE   DSABC070                 BR IF NO-DONT BACK UP           02197000
         L     DSABPTR,DSABBCHN(,DSABPTR) GET PREVIOUS DSAB             02198000
         L     TIOTPTR,DSABTIOT(,DSABPTR) GET TIOT ADDRESS              02199000
         L     RET,DSABOPCT-2(,DSABPTR) GET OPEN COUNT                  02200000
DSABC060 LR    RD,RET                   SAVE FOR CS                     02201000
         SRDL  RET,16                   SHIFT COUNT TO RF               02202000
         SRA   RF,16                    PROPOGATE SIGN BIT              02203000
         LTR   RF,RF                    MAKE SURE ITS POSITIVE          02204000
         BNP   DSABC050                 BRANCH IF NO TO KEEP            02205000
*                                       BACKING UP DSAB CHAIN           02206000
         BCTR  RF,R0                    DECREMENT OPEN COUNT            02207000
         SLL   RF,16                    SHIFT TO HIGH 16 BITS           02208000
         SLDL  RET,16                   UPDATED FULLWORD IN RET         02209000
         MODESET EXTKEY=SCHED           SCHEDULER KEY FOR DSAB          02210000
         CS    RD,RET,DSABOPCT-2(DSABPTR) TRY TO SNEAK IT IN            02211000
         MODESET EXTKEY=DATAMGT         RETURN TO DATA MGT KEY          02212000
         BNE   DSABC060                 BR IF NEED TO TRY AGAIN         02213000
         B     DSABC050                 KEEP BACKING UP CHAIN           02214000
*DSABC070:                                                         0903 02215000
*   RET=(ADDR(RA->TRRSAV2))->SARETRY;/* RESTORE RETURN ADDR          */ 02216000
DSABC070 LA    @15,TRRSAV2(,RA)                                    0903 02217000
         L     RET,SARETRY(,@15)                                   0903 02218000
*   END DSABCALL;                   /* RETURN TO CALLER              */ 02219000
@EL00018 DS    0H                                                  0904 02220000
@EF00018 DS    0H                                                  0904 02221000
@ER00018 BR    @14                                                 0904 02222000
@PB00018 DS    0H                                                  0905 02223000
*   GEN(EJECT);                                                    0905 02224000
         EJECT                                                          02225000
