         B     RRQCHK20                                            1060 02678000
*                   END;            /*                         YM7233*/ 02679000
*               END;                /*                         YM7233*/ 02680000
@RF01057 DS    0H                                                  1062 02681000
@DE01055 L     @00,DEBDEBB-1(,RF)                                  1062 02682000
         ICM   @00,8,@CB00081                                      1062 02683000
         LTR   @00,@00                                             1062 02684000
         BNZ   @DL01055                                            1062 02685000
*RRQCHK20:                          /* RELEASE LOCAL LOCK      YM7233*/ 02686000
*               STM(DEBPTR,RET,TRRSAV2);/* SAVE SETLOCK REGS   YM7233*/ 02687000
