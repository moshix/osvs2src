         LR    DEBPTR,R9                                           0863 02047000
*           RFY                                                    0864 02048000
*             R1 RSTD;              /* RESTRICT REG                  */ 02049000
*DCHNDEB1:                                                         0865 02050000
*           R1=DEBTCBAD+4;          /* INIT TO FIND 1ST DEB          */ 02051000
DCHNDEB1 L     @15,DEBTCBAD(,DEBPTR)                               0865 02052000
         LA    @15,4(,@15)                                         0865 02053000
         LR    R1,@15                                              0865 02054000
*           DO WHILE DEBPTR^=R1->DEBDEBB;/* SEARCH DEB CHAIN         */ 02055000
         B     @DE00866                                            0866 02056000
@DL00866 DS    0H                                                  0867 02057000
*             IF R1->DEBDEBB=0 THEN /* IF DEB NOT FOUND ON CHAIN     */ 02058000
         L     @15,DEBDEBB-1(,R1)                                  0867 02059000
         LA    @15,0(,@15)                                         0867 02060000
         LTR   @15,@15                                             0867 02061000
         BZ    @RT00867                                            0867 02062000
*               GOTO DCHNDEB2;      /* GO PURGE DEB FROM DEB TABLE   */ 02063000
*             R1=R1->DEBDEBB;       /* GET NEXT DEB AND CONTINUE     */ 02064000
         LR    R1,@15                                              0869 02065000
*           END;                    /* WITH SEARCH                   */ 02066000
@DE00866 L     @15,DEBDEBB-1(,R1)                                  0870 02067000
         LA    @15,0(,@15)                                         0870 02068000
         CR    DEBPTR,@15                                          0870 02069000
         BNE   @DL00866                                            0870 02070000
*           RC=DEBPTR;              /* DEB HAS BEEN FOUND ON CHAIN   */ 02071000
         LR    RC,DEBPTR                                           0871 02072000
*           RD=DEBDEBAD;            /* INIT TO DECHAIN DEB           */ 02073000
         L     RD,DEBDEBAD(,DEBPTR)                                0872 02074000
*           GEN REFS(DEBAMLNG,DEBBASIC,DEBDEBAD,DCHNDEB1);         0873 02075000
         ICM   RD,8,DEBAMLNG-DEBBASIC(R1)   PRIME HI-ORDER BYTE         02076000
         ICM   RC,8,DEBAMLNG-DEBBASIC(R1)   PRIME HI-ORDER BYTE         02077000
         CS    RC,RD,DEBDEBAD-DEBBASIC(R1)  DECHAIN DEB                 02078000
         BNZ   DCHNDEB1                     BR IF NOT SUCCESSFUL        02079000
*           RFY                                                    0874 02080000
*             R1 UNRSTD;            /* MAKE REG UNRESTRICTED         */ 02081000
*DCHNDEB2:                          /* RELEASE LOCK, PURGE DEB       */ 02082000
*           GEN;                                                   0875 02083000
DCHNDEB2 DS    0H                                                  0875 02084000
DCHNRRRR SETLOCK RELEASE,TYPE=LOCAL,    ENABLE                         *02085000
               RELATED=(LOCAL,IFG0RR0B(DCHNOOOO))                       02086000
*         END;                                                     0876 02087000
*       GEN(MODESET EXTKEY=DATAMGT);/* SET DATA MGT KEY        YM7871*/ 02088000
@RF00861 DS    0H                                                  0877 02089000
         MODESET EXTKEY=DATAMGT                                         02090000
*       RD=RA;                      /* RESTORE SAVE AREA PTR         */ 02091000
         LR    RD,RA                                               0878 02092000
*     END;                                                         0879 02093000
*   END DCHNDEB;                    /* RETURN TO CALLER              */ 02094000
@EL00016 DS    0H                                                  0880 02095000
@EF00016 DS    0H                                                  0880 02096000
