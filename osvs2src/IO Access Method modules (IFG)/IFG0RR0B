*       IF JFCBELNM(2)^=BLANK THEN  /* IF A GENERATION DATA SET      */ 03151000
         CLI   JFCBELNM+101(RCORE),C' '                            1217 03152000
         BE    @RF01217                                            1217 03153000
*         DO;                       /* THEN                          */ 03154000
*           RET=RET+9;              /* PT TO GENERATION NO. IN DSN 1219 03155000
*                                      WHICH IS OF FORM GNNNNVNN     */ 03156000
         LA    RET,9(,RET)                                         1219 03157000
