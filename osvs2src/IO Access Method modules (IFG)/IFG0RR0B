*     END;                                                         1085 02753000
*   ELSE                            /* NOT A VTAM ACB                */ 02754000
*     DO;                                                          1086 02755000
         B     @RC01080                                            1086 02756000
@RF01080 DS    0H                                                  1087 02757000
*       DCBOFLWR=OFF;               /* RESET WRITE BIT               */ 02758000
*       DCBOFLRB=OFF;               /* RESET READ BACKWARD BIT       */ 02759000
*       DCBOFEOV=OFF;               /* RESET EOV CONCATENATION BIT   */ 02760000
*       DCBOFOPN=OFF;               /* RESET OPEN BIT                */ 02761000
*       DCBOFTM=OFF;                /* RESET TAPEMARK READ BIT       */ 02762000
*       DCBOFIOF=OFF;               /* RESET BUSY BIT                */ 02763000
*       DCBOFUEX=ON;                /* RESET LOCK BIT          YM7853*/ 02764000
         OI    DCBOFUEX+40(R2),B'00000010'                         1093 02765000
