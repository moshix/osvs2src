*   GEN(EJECT);                                                    1171 03011000
         EJECT                                                          03012000
*                                                                  1172 03013000
*/********************************************************************/ 03014000
*/*                           WRITE TAPEMARK                         */ 03015000
*/********************************************************************/ 03016000
*                                                                  1172 03017000
*TAPEMARK:                                                         1172 03018000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1172 03019000
         B     @PB00027                                            1172 03020000
TAPEMARK DS    0H                                                  1173 03021000
*   DXCCW(1:16)=WTMCCW;             /* SET UP CCW TO WRITE TM        */ 03022000
         MVC   DXCCW(16,RCORE),WTMCCW                              1173 03023000
*   DXDEBUCB=DEBSUCBA;              /* UCB ADDR TO WORK AREA DEB     */ 03024000
         L     @15,DEBSUCBA(,DEBPTR)                               1174 03025000
         ST    @15,DXDEBUCB(,RCORE)                                1174 03026000
*   GEN(MODESET EXTKEY=SUPR);       /* SET UCB KEY             YM7871*/ 03027000
         MODESET EXTKEY=SUPR                                            03028000
*   UCBREW=OFF;                     /* INSURE REWIND BIT OFF         */ 03029000
         NI    UCBREW(UCBPTR),B'11101111'                          1176 03030000
*   GEN(MODESET EXTKEY=DATAMGT);    /* SET DATA MGT KEY        YM7871*/ 03031000
         MODESET EXTKEY=DATAMGT                                         03032000
*   RFY                                                            1178 03033000
*     R1 RSTD;                      /* RESTRICT REG            YM4679*/ 03034000
*   R1=ADDR(DXIOB);                 /* GET IOB ADDR            YM4679*/ 03035000
         LA    R1,DXIOB(,RCORE)                                    1179 03036000
*   GEN(EXCP  (1));                 /* INIT I/O OPERATION      YM4679*/ 03037000
         EXCP  (1)                                                      03038000
*   R1=ADDR(DXECB);                 /* GET ECB ADDR            YM4679*/ 03039000
         LA    R1,DXECB(,RCORE)                                    1181 03040000
*   GEN(WAIT  ,ECB=(1));            /* WAIT TO COMPLETE I/O    YM4679*/ 03041000
         WAIT  ,ECB=(1)                                                 03042000
*   RFY                                                            1183 03043000
*     R1 UNRSTD;                    /* MAKE REG UNRSTD         YM4679*/ 03044000
*   IF DXECB(1)='7F'X THEN          /* IF I/O COMPLETED OK           */ 03045000
         CLI   DXECB(RCORE),X'7F'                                  1184 03046000
         BNE   @RF01184                                            1184 03047000
*     RC=0;                         /* SET RETURN CODE OF ZERO       */ 03048000
         SLR   RC,RC                                               1185 03049000
*   ELSE                            /* OTHERWISE                     */ 03050000
*     RC=1;                         /* SET NON-ZERO RETURN CODE      */ 03051000
         B     @RC01184                                            1186 03052000
@RF01184 LA    RC,1                                                1186 03053000
*   END TAPEMARK;                   /* RETURN TO CALLER              */ 03054000
@EL00027 DS    0H                                                  1187 03055000
@EF00027 DS    0H                                                  1187 03056000
@ER00027 BR    @14                                                 1187 03057000
@PB00027 DS    0H                                                  1188 03058000
*   GEN(EJECT);                                                    1188 03059000
         EJECT                                                          03060000
*                                                                  1189 03061000
*/********************************************************************/ 03062000
*/*             DETERMINE IF TAPEMARK SHOULD BE WRITTEN              */ 03063000
*/*             AFTER DATA FOR AN SL/AL DATA SET                     */ 03064000
*/********************************************************************/ 03065000
*                                                                  1189 03066000
*DATM:                                                             1189 03067000
*   PROC OPTIONS(NOSAVEAREA);                                      1189 03068000
         B     @PB00028                                            1189 03069000
DATM     STM   @14,@12,12(@13)                                     1189 03070000
*   IF DXATDATM=OFF THEN            /* TAPEMARK WRITTEN AFTER DATA   */ 03071000
         TM    DXATDATM(RCORE),B'00100000'                         1190 03072000
         BNZ   @RF01190                                            1190 03073000
*     DO;                           /* NO, THEN                      */ 03074000
*       CALL TAPEMARK;              /* GO WRITE A TAPEMARK           */ 03075000
         BAL   @14,TAPEMARK                                        1192 03076000
*       IF RC=0 THEN                /* TAPEMARK WRITTEN OK           */ 03077000
         SLR   @15,@15                                             1193 03078000
         CR    RC,@15                                              1193 03079000
         BNE   @RF01193                                            1193 03080000
*         DO;                       /* YES, THEN                     */ 03081000
*           IF SDWACOMP^=0 THEN     /* DOES A CRSA EXIST             */ 03082000
         C     @15,SDWACOMP(,SDWAPTR)                              1195 03083000
         BE    @RF01195                                            1195 03084000
*             CRSTMAD=ON;           /* YES, THEN SET FLAG            */ 03085000
         L     @15,DXATCOM4(,RCORE)                                1196 03086000
         OI    CRSTMAD(@15),B'10000000'                            1196 03087000
*         END;                      /* GO RETURN                     */ 03088000
*       ELSE                        /* ERROR WRITING TAPEMARK        */ 03089000
*         RC=1;                     /* SET ERROR RETURN CODE         */ 03090000
         B     @RC01193                                            1198 03091000
@RF01193 LA    RC,1                                                1198 03092000
*     END;                          /* GO RETURN                     */ 03093000
*   ELSE                            /* TAPEMARK ALREADY WRITTEN      */ 03094000
*     RC=0;                         /* SET RETURN CODE               */ 03095000
         B     @RC01190                                            1200 03096000
@RF01190 SLR   RC,RC                                               1200 03097000
*   END DATM;                       /* RETURN TO CALLER              */ 03098000
@EL00028 DS    0H                                                  1201 03099000
@EF00028 DS    0H                                                  1201 03100000
@ER00028 LM    @14,@12,12(@13)                                     1201 03101000
         BR    @14                                                 1201 03102000
@PB00028 DS    0H                                                  1202 03103000
*   GEN(EJECT);                                                    1202 03104000
         EJECT                                                          03105000
*                                                                  1203 03106000
*/********************************************************************/ 03107000
*/*                  CONSTRUCT END-OF-FILE LABEL 1                   */ 03108000
*/********************************************************************/ 03109000
*                                                                  1203 03110000
*TRL1:                                                             1203 03111000
*   PROC OPTIONS(NOSAVEAREA);                                      1203 03112000
         B     @PB00029                                            1203 03113000
TRL1     STM   @14,@12,12(@13)                                     1203 03114000
*   IF DXATTRL1=OFF THEN            /* IF TRLR LBL 1 NOT WRITTEN     */ 03115000
         TM    DXATTRL1(RCORE),B'00010000'                         1204 03116000
         BNZ   @RF01204                                            1204 03117000
*     DO;                           /* THEN BUILD AND WRITE IT       */ 03118000
*       FL1LABI=EOF;                /* SET FILE LABEL ID TO EOF      */ 03119000
         MVC   FL1LABI(3,RCORE),EOF                                1206 03120000
*       FL1NO=CHAR1;                /* SET FILE LABEL NUMBER TO 1    */ 03121000
         MVI   FL1NO(RCORE),C'1'                                   1207 03122000
*/*             DETERMINE 17 LEAST SIGNIFICANT NON-BLANK             */ 03123000
*/*             CHARACTERS IN DATA SET NAME IN JFCB                  */ 03124000
*                                                                  1208 03125000
*       R9=ADDR(JFCBDSNM);          /* PT TO JFCB DSNAME             */ 03126000
         LA    R9,100(,RCORE)                                      1208 03127000
*       RET=R9+27;                  /* GET ADDR OF 1ST POSSIBLE    1209 03128000
*                                      SIGNIFICANT CHARACTER         */ 03129000
         LA    RET,27(,R9)                                         1209 03130000
*       DO WHILE(RET+16)->JFCBDSNM(1)=BLANK;/* FIND NON-BLANK CHAR   */ 03131000
         B     @DE01210                                            1210 03132000
@DL01210 DS    0H                                                  1211 03133000
*         RET=RET-1;                /* DECR POINTER                  */ 03134000
         BCTR  RET,0                                               1211 03135000
*       END;                        /* CONTINUE SEARCH               */ 03136000
@DE01210 CLI   JFCBDSNM+16(RET),C' '                               1212 03137000
         BE    @DL01210                                            1212 03138000
*       IF RET^<R9 THEN             /* IF LEFT PT WITHIN FIELD THEN  */ 03139000
         CR    RET,R9                                              1213 03140000
         BL    @RF01213                                            1213 03141000
*         R9=RET;                   /* USE LAST 17 LEAST SIG CHARS   */ 03142000
         LR    R9,RET                                              1214 03143000
*       FL1ID=R9->JFCBDSNM;         /* MOVE DSNAME INTO FILE ID      */ 03144000
@RF01213 MVC   FL1ID(17,RCORE),JFCBDSNM(R9)                        1215 03145000
*/*        GENERATION NUMBER AND VERSION NUMBER OF GENERATION        */ 03146000
*                                                                  1216 03147000
*       FL1GNO(1:45)='';            /* BLANK FIELDS                  */ 03148000
