*           DXCCW11(1)='00'X;       /* ZERO TEMP SAVE AREA           */ 01778000
         MVI   DXCCW11(RCORE),X'00'                                0797 01779000
*           IF DCBMFECP=ON&         /* IF EXCP AND                   */ 01780000
*               DCBMFFE=OFF THEN    /* FOUNDATION BLK EXT EXISTS     */ 01781000
         TM    DCBMFECP+40(R2),B'10000000'                         0798 01782000
         BNO   @RF00798                                            0798 01783000
         TM    DCBMFFE+40(R2),B'01000000'                          0798 01784000
         BZ    @RT00798                                            0798 01785000
*             GOTO CFN04500;        /* THEN GO BEGIN RESTORE         */ 01786000
*           IF DCBDSGIS=ON|         /* IF ISAM OR                    */ 01787000
*               DCBDSGDA=ON|        /* DIRECT OR                     */ 01788000
*               DCBDSGPO=ON THEN    /* PARTITIONED THEN              */ 01789000
@RF00798 LA    @15,16                                              0800 01790000
         ALR   @15,R2                                              0800 01791000
         TM    DCBDSGIS(@15),B'10100010'                           0800 01792000
         BNZ   @RT00800                                            0800 01793000
*             GOTO CFN04400;        /* GO SAVE HIARCHY FLAGS         */ 01794000
*           IF DCBDSGPS=OFF THEN    /* IF NOT PHYSICAL SEQUENTIAL    */ 01795000
         TM    DCBDSGPS(@15),B'01000000'                           0802 01796000
         BZ    @RT00802                                            0802 01797000
*             GOTO CFN04300;        /* THEN GO CHK FOR GRAPHICS      */ 01798000
*           IF TIOEQNAM=ON THEN     /* IF TCAM PS THEN               */ 01799000
