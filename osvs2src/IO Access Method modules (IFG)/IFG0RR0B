*   RD=RA;                          /* RESTORE SAVE AREA PTR         */ 02143000
@RC00889 DS    0H                                                  0895 02144000
@RC00884 LR    RD,RA                                               0895 02145000
*   END DECRDSAB;                                                  0896 02146000
*                                                                  0896 02147000
