         TM    DCBDSGGS(@15),B'10000000'                           0809 01814000
         BO    @RT00809                                            0809 01815000
*             GOTO CFN04500;        /* GO BEGIN RESTORE              */ 01816000
*           IF DCBDSGTX=ON|         /* IF TCAM LINE GROUP OR         */ 01817000
*               DCBDSGTQ=ON THEN    /* TCAM MESSAGE QUEUE THEN       */ 01818000
         TM    DCBDSGTX(@15),B'01100000'                           0811 01819000
         BNZ   @RT00811                                            0811 01820000
*             GOTO CFN05100;        /* GO ZERO MERGED FIELDS         */ 01821000
*           IF DCBMFRD=ON|          /* IF READ OR                    */ 01822000
*               DCBMFWRT=ON THEN    /* WRITE THEN                    */ 01823000
         LA    @15,40                                              0813 01824000
         ALR   @15,R2                                              0813 01825000
         TM    DCBMFRD(@15),B'00100000'                            0813 01826000
         BO    @RT00813                                            0813 01827000
         TM    DCBMFWRT(@15),B'00100000'                           0813 01828000
         BNO   @RF00813                                            0813 01829000
@RT00813 DS    0H                                                  0814 01830000
*CFN04400:                                                         0814 01831000
*             DO;                   /* DO THE FOLLOWING              */ 01832000
CFN04400 DS    0H                                                  0815 01833000
*               DXCCW11(1)=DCBHIARC;/* SAVE HIARCHY FLAGS            */ 01834000
         MVC   DXCCW11(1,RCORE),DCBHIARC+28(R2)                    0815 01835000
*               DXCCW11(1)=DXCCW11(1)&(DCBH0X+DCBH1X);/* CLEAR ALL 0816 01836000
*                                      BITS EXCEPT HIARCHY FLAGS     */ 01837000
         SLR   @15,@15                                             0816 01838000
         IC    @15,DXCCW11(,RCORE)                                 0816 01839000
         N     @15,@CF04414                                        0816 01840000
         STC   @15,DXCCW11(,RCORE)                                 0816 01841000
*             END;                                                 0817 01842000
*CFN04500:                          /* BYPASS SAVING HIARCHY FLAGS   */ 01843000
*           RFY                                                    0818 01844000
*             R1 RSTD;              /* RESTRICT REG                  */ 01845000
@RF00813 DS    0H                                                  0818 01846000
CFN04500 DS    0H                                                  0819 01847000
*           R1=1;                   /* INIT REG WITH 1               */ 01848000
         LA    R1,1                                                0819 01849000
*           RC=ADDR(DADSP);         /* PT TO DCB DISPLACEMENT TBL    */ 01850000
         LA    RC,DADSP                                            0820 01851000
*           RFY                                                    0821 01852000
*             R7 RSTD;              /* RESTRICT REG                  */ 01853000
*           R7=ADDR(DALGH);         /* PT TO DCB FIELD LENGTH TBL    */ 01854000
         LA    R7,DALGH                                            0822 01855000
*           RET=0;                  /* CLEAR REG                     */ 01856000
         SLR   RET,RET                                             0823 01857000
*           RFY                                                    0824 01858000
*             RF RSTD;              /* RESTRICT REG                  */ 01859000
*           RF=0;                   /* CLEAR REG                     */ 01860000
         SLR   RF,RF                                               0825 01861000
*CFN04700:                          /* CHK FOR ALTERED DCB FIELD     */ 01862000
*           R1=R1&R6;               /* NEXT DCB FIELD CHGD     YM5934*/ 01863000
CFN04700 NR    R1,R6                                               0826 01864000
*           BC(8,CFN04900);         /* NO, IT WAS NOT CHANGED        */ 01865000
         BC    8,CFN04900                                          0827 01866000
*           RFY                                                    0828 01867000
*            (R1,                                                  0828 01868000
*             RF) UNRSTD;           /* MAKE REGS UNRESTRICTED        */ 01869000
*           GEN REFS(DCBRECFM) FLOWS(CFN05100);                    0829 01870000
*                                                            @ZA05430*/ 01871000
         IC    RET,0(,RC)               INSERT DCB FIELD DISPLACEMENT   01872000
         IC    RF,0(,R7)                INSERT DCB FIELD LENGTH-1       01873000
         CLI   0(RC),X'24'              IS THIS FIELD RECFM             01874000
         BE    CFN05000                 BR IF IT IS RECFM               01875000
CFN04800 EQU   *                                                        01876000
         AR    RET,R2                   LOCATION OF FIELD IN DCB        01877000
         EX    RF,CFN05700              EXECUTE MOVE INSTRUCTION        01878000
*                                                                       01879000
*        FIELD  IN  DCB  HAS  BEEN  RETURNED  TO ITS                    01880000
*                       FORMER  STATUS                                  01881000
CFN04900 EQU   *                                                        01882000
         SRL   R6,1                     SHIFT MASK               YM5934 01883000
         SR    RET,RET                  CLEAR RET                       01884000
         SR    RF,RF                    AND RF                          01885000
         LA    RC,1(,RC)                GET NEXT DISP PARAMETER         01886000
         LA    R7,1(,R7)                GET NEXT LENGTH PARAMETER       01887000
         LA    R1,1                     REINITIALIZE R1 WITH ONE        01888000
         CLI   0(RC),0                  END OF RESTORE TABLE            01889000
         BE    CFN05800                 BRANCH IF THE END               01890000
         B     CFN04700                 CHECK NEXT FIELD                01891000
CFN05000 EQU   *                                                        01892000
         TM    DCBRECFM+28(R2),1        INDICATIOR ON IN RECFM   YM4679 01893000
         BZ    CFN04800                 BR IF INDICATOR BIT OFF         01894000
         MVI   DCBRECFM+28(R2),1        RESET RECFM WITH BIT ON  YM5329 01895000
         B     CFN04900                 BR TO LOOK AT NEXT FIELD        01896000
*                                                                       01897000
* FOR TCAM WITH DSORG=TX OR DSORG=TQ, BUFMAX, PCI, RESERVE, BUFSIZE,    01898000
* AND THRESH FIELDS MAY HAVE BEEN MERGED FROM THE JFCB WITH A UNIQUE    01899000
* MERGE. THE MERGED FIELDS WILL BE RESTORED TO ZERO.                    01900000
*                                                                       01901000
CFN05100 EQU   *                                                        01902000
         LA    R1,8                     THRESH BIT MASK                 01903000
         NR    R1,R6                    WAS FIELD MERGED         YM5934 01904000
         BZ    CFN05200                 BRANCH IF NO                    01905000
         MVI   32(R2),0                 RESTORE FIELD TO ZERO           01906000
CFN05200 LA    R1,256                   BUFMAX BIT MASK                 01907000
         NR    R1,R6                    WAS FIELD MERGED         YM5934 01908000
         BZ    CFN05300                 BRANCH IF NO                    01909000
         MVI   28(R2),0                 RESTORE FIELD TO ZERO           01910000
CFN05300 LA    R1,1                     BUFSIZE BIT MASK                01911000
         SLL   R1,12                    SHIFT MASK                      01912000
         NR    R1,R6                    WAS FIELD MERGED         YM5934 01913000
         BZ    CFN05400                 BRANCH IF NO                    01914000
         SR    R7,R7                    ZERO OUT R7                     01915000
         STH   R7,54(,R2)               RESTORE FIELD TO ZERO           01916000
CFN05400 LA    R1,1                     RESERVE BIT MASK                01917000
         SLL   R1,19                    SHIFT MASK                      01918000
         NR    R1,R6                    WAS FIELD MERGED         YM5934 01919000
         BZ    CFN05500                 BRANCH IF NO                    01920000
         ST    R7,56(,R2)               RESTORE FIELD TO ZERO           01921000
CFN05500 LA    R1,1                     PCI BIT MASK                    01922000
         SLL   R1,21                    SHIFT MASK                      01923000
         NR    R1,R6                    WAS FIELD MERGED         YM5934 01924000
         BZ    CFN05600                 BRANCH IF NO                    01925000
         MVI   25(R2),0                 RESTORE FIELD TO ZERO           01926000
CFN05600 EQU   *                                                        01927000
         B     CFN04500                 CHECK OTHER FIELDS              01928000
CFN05700 MVC   0(1,RET),DANULL          MOVE ZEROS TO DCB FIELD         01929000
*                                                                       01930000
*                                                                       01931000
CFN05800 EQU   *                                                        01932000
*           RFY                                                    0830 01933000
*            (R6,                                                  0830 01934000
*             R7) UNRSTD;           /* MAKE REG UNRESTRICTED         */ 01935000
*           IF RDEB->MODMASK(9)=ON THEN/* IF WRITE CHK BIT SET THEN  */ 01936000
         TM    MODMASK+1(RDEB),B'10000000'                         0831 01937000
         BNO   @RF00831                                            0831 01938000
*             DCBOPTW=OFF;          /* RESET WRITE CHK SWITCH        */ 01939000
         NI    DCBOPTW+52(R2),B'01111111'                          0832 01940000
*           IF RDEB->MODMASK(1)=ON THEN/* IF DCBUSASI BIT SET THEN   */ 01941000
@RF00831 TM    MODMASK(RDEB),B'10000000'                           0833 01942000
         BNO   @RF00833                                            0833 01943000
*             DCBUSASI=DCBUSASI&('FF'X-DCBUSAON);/* RESET BIT        */ 01944000
         LA    @15,80                                              0834 01945000
         ALR   @15,R2                                              0834 01946000
         SLR   @06,@06                                             0834 01947000
         IC    @06,DCBUSASI(,@15)                                  0834 01948000
         N     @06,@CF04415                                        0834 01949000
