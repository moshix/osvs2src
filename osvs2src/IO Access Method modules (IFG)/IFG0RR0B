         L     R1,36(RC,RDEB)           GET ADDR OF TOTALING SAVE AREA  01680000
         LA    R1,0(,R1)                CLEAR HI-ORDER BYTE             01681000
*       IF R1^=0 THEN               /* IF A SAVE AREA ADDR EXISTS    */ 01682000
         LTR   R1,R1                                               0769 01683000
         BZ    @RF00769                                            0769 01684000
*         DO;                       /* THEN                          */ 01685000
*           R2=DXUKEY;              /* GET USER KEY            YM7871*/ 01686000
         SLR   R2,R2                                               0771 01687000
         IC    R2,DXUKEY(,RCORE)                                   0771 01688000
*           GEN;                                                   0772 01689000
         MODESET KEYADDR=(2),WORKREG=2  ENTER USER KEY           YM7871 01690000
         L     RC,TOTSVLNG-TOTSAVWA(,R1)  GET LNG OF TOTALING AREA      01691000
