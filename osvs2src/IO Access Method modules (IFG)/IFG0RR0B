         BE    @RF00358                                            0358 00321000
         CLI   DEBAMLNG(DEBPTR),8                                  0358 00322000
         BL    @RF00358                                            0358 00323000
*                       DO;         /* THEN                  @ZA05430*/ 00324000
*                         RFY                                      0360 00325000
*                           RET RSTD;/* RESTRICT REGISTER 14 @ZA05430*/ 00326000
*                         RF=DEBEXSCL;/* POINT DEB EXTENT SCALE    0361 00327000
*                                                            @ZA05430*/ 00328000
         SLR   RF,RF                                               0361 00329000
         IC    RF,DEBEXSCL(,DEBPTR)                                0361 00330000
*                         RET=DEBNMEXT;/* POINT NUMBER OF EXTENTS  0362 00331000
*                                                            @ZA05430*/ 00332000
         SLR   RET,RET                                             0362 00333000
         IC    RET,DEBNMEXT(,DEBPTR)                               0362 00334000
*                         SLL(RET,RF);/* MULTIPLY SCALE TIMES EXTENTS   00335000
*                                                            @ZA05430*/ 00336000
         SLL   RET,0(RF)                                           0363 00337000
*                         RFY                                      0364 00338000
*                           DEBACS01 BASED(ADDR(DEBBASND)+RET);/*  0364 00339000
*                                      POINT LAST EXT        @ZA05430*/ 00340000
*                         IF DCBDSGPO=ON&/* IF BPAM AND      @ZA18498*/ 00341000
*                             DEBDSNAM(1)^=0 THEN/* MEMNAME THERE  0365 00342000
*                                                            @ZA05430*/ 00343000
         TM    DCBDSGPO+16(R2),B'00000010'                         0365 00344000
         BNO   @RF00365                                            0365 00345000
         LA    @01,DEBBASND(,DEBPTR)                               0365 00346000
         ALR   @01,RET                                             0365 00347000
         CLI   DEBDSNAM(@01),0                                     0365 00348000
         BE    @RF00365                                            0365 00349000
*                           DO;     /* THEN                  @ZA05430*/ 00350000
*                             DEBDSNAM=TEMPNAME;/* CHANGE NAME     0367 00351000
*                                                            @ZA05430*/ 00352000
         MVC   DEBDSNAM(8,@01),TEMPNAME                            0367 00353000
*                             RFY                                  0368 00354000
*                               DEBACS01 BASED(ADDR(DEBBASND)+RET+4);   00355000
*                                   /* PNT LAST EXT          @ZA05430*/ 00356000
*                           END;    /*                       @ZA05430*/ 00357000
