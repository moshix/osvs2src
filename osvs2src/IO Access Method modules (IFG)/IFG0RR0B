*                                                              YM7871*/ 00801000
@RF00507 DS    0H                                                  0514 00802000
         MODESET EXTKEY=DATAMGT                                         00803000
*                     END;                                         0515 00804000
*                 END;                                             0516 00805000
*               ELSE                /* DCB COPY EXISTS               */ 00806000
*                 SARETRY=ADDR(SELTFIN);/* RESET RETURN ADDR         */ 00807000
