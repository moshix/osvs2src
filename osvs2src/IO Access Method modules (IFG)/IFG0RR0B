         BAL   @14,DECRDMCT                                        0354 00290000
*                   END;            /*                         YM7099*/ 00291000
@RF00353 DS    0H                                                  0356 00292000
*                 IF DXATNVOL=OFF THEN/* NEW VOL SW NOW OFF    YM7099*/ 00293000
@RF00350 TM    DXATNVOL(RCORE),B'01000000'                         0356 00294000
         BNZ   @RF00356                                            0356 00295000
*                   DO;             /* THEN                    YM7099*/ 00296000
*                     IF RRFUNCTN=RRFEOV&/* IF EOV ENTRY AND @ZA05430*/ 00297000
*                         DEBACCS(2:4)='111'B&/* OUTPUT DEB  @ZA05430*/ 00298000
*                         DEBDISP(1:2)='01'B&/* OLD AND      @ZA05430*/ 00299000
*                         DCBCNSTO^=ON&/* NOT MEM STOW AND   @ZA05430*/ 00300000
*                         DCBCNQSM^=ON&/* NOT QSAM AND       @ZA05430*/ 00301000
*                         DCBOPTT^=ON&/* NOT USER TOTAL AND  @ZA05430*/ 00302000
*                         DCBMACF2^='28'X&/* NOT BDAM CREATE @ZA05430*/ 00303000
*                         DEBAMLNG>=8 THEN/* MEMNAME LONG ENOUGH   0358 00304000
*                                                            @ZA05430*/ 00305000
         CLI   RRFUNCTN(RRRF),X'37'                                0358 00306000
         BNE   @RF00358                                            0358 00307000
         TM    DEBACCS(DEBPTR),B'00000111'                         0358 00308000
         BNO   @RF00358                                            0358 00309000
         TM    DEBDISP(DEBPTR),B'01000000'                         0358 00310000
         BNO   @RF00358                                            0358 00311000
         TM    DEBDISP(DEBPTR),B'10000000'                         0358 00312000
         BNZ   @RF00358                                            0358 00313000
         LA    @01,52                                              0358 00314000
         ALR   @01,R2                                              0358 00315000
         TM    DCBCNSTO(@01),B'10000001'                           0358 00316000
         BNZ   @RF00358                                            0358 00317000
         TM    DCBOPTT(@01),B'00000010'                            0358 00318000
         BO    @RF00358                                            0358 00319000
