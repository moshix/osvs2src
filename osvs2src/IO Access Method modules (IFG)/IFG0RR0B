*/*       DELETE ACCESS METHOD SUBROUTINES AND I/O APPENDAGES        */ 01547000
*/********************************************************************/ 01548000
*                                                                  0730 01549000
*CHKSUBNM:                                                         0730 01550000
*   PROC OPTIONS(NOSAVEAREA);                                      0730 01551000
         B     @PB00012                                            0730 01552000
CHKSUBNM STM   @14,@12,12(@13)                                     0730 01553000
*   RFY                                                            0731 01554000
*     R0 RSTD;                      /* RESTRICT REG                  */ 01555000
*   RFY                                                            0732 01556000
*    (RA,                                                          0732 01557000
*     RET) UNRSTD;                  /* MAKE REGS UNRESTRICTED        */ 01558000
*   IF DEBPTR^=0&                   /* IF A DEB ADDR EXISTS AND      */ 01559000
*       DXATCLOS<4 THEN             /* A.M. SUBRTNS & I/O APPENDAGES    01560000
*                                      NOT YET DELETED THEN    YM5934*/ 01561000
         SLR   @01,@01                                             0733 01562000
         CR    DEBPTR,@01                                          0733 01563000
         BE    @RF00733                                            0733 01564000
         CLI   DXATCLOS(RCORE),4                                   0733 01565000
         BNL   @RF00733                                            0733 01566000
*     DO;                           /* DETERMINE THE OFFSET OF THE 0734 01567000
*                                      SUBROUTINE SECTION FROM THE 0734 01568000
*                                      BEGINNING OF THE DEB          */ 01569000
*       RC=DEBNMSUB;                /* PICK UP NO. OF SUBROUTINES    */ 01570000
         SLR   RC,RC                                               0735 01571000
         IC    RC,DEBNMSUB(,DEBPTR)                                0735 01572000
*       IF RC^=0 THEN               /* IF SUBROUTINES EXIST THEN     */ 01573000
         CR    RC,@01                                              0736 01574000
         BE    @RF00736                                            0736 01575000
*         DO;                                                      0737 01576000
*           R9=DEBNMEXT;            /* PICK UP NO. OF EXTENTS        */ 01577000
         SLR   R9,R9                                               0738 01578000
         IC    R9,DEBNMEXT(,DEBPTR)                                0738 01579000
*           R9=R9*4;                /* MULTIPLY BY FOUR              */ 01580000
         SLL   R9,2                                                0739 01581000
*           IF DEBEXSCL='04'X THEN  /* IF EXTENT TYPE IS D.A. THEN   */ 01582000
         CLI   DEBEXSCL(DEBPTR),X'04'                              0740 01583000
         BNE   @RF00740                                            0740 01584000
*             R9=R9*4;              /* MULTIPLY BY FOUR AGAIN        */ 01585000
         SLL   R9,2                                                0741 01586000
*           R9=R9+LENGTH(DEBBASIC); /* ADD DEB BASIC SECTION SIZE    */ 01587000
@RF00740 LA    R9,32(,R9)                                          0742 01588000
*           R0=DEBAMLNG;            /* PICK UP A.M.SECTION SIZE      */ 01589000
         SLR   R0,R0                                               0743 01590000
         IC    R0,DEBAMLNG(,DEBPTR)                                0743 01591000
*           IF DCBMFECP=OFF THEN    /* IF NOT EXCP THEN              */ 01592000
         TM    DCBMFECP+40(R2),B'10000000'                         0744 01593000
         BNZ   @RF00744                                            0744 01594000
*             IF DCBDSGDA=ON THEN   /* IF DSORG = BDAM THEN          */ 01595000
         TM    DCBDSGDA+16(R2),B'00100000'                         0745 01596000
         BNO   @RF00745                                            0745 01597000
*               R0=R0*4;            /* CALCULATE A.M. SECTION SIZE   */ 01598000
         SLL   R0,2                                                0746 01599000
*           DEBSBPTR=R9+R0+DEBPTR;  /* SUBRTN SECTION OFFSET         */ 01600000
@RF00745 DS    0H                                                  0747 01601000
@RF00744 LR    @01,R9                                              0747 01602000
         ALR   @01,R0                                              0747 01603000
         ALR   @01,DEBPTR                                          0747 01604000
         LR    DEBSBPTR,@01                                        0747 01605000
*           DO WHILE RC^=0;         /* DO WHILE SUBRTN COUNT ^= 0    */ 01606000
         B     @DE00748                                            0748 01607000
@DL00748 DS    0H                                                  0749 01608000
*             TRRWTGP->WTGMODNM(1:6)=IGG;/* SET UP FOR DELETE        */ 01609000
         L     @01,TRRWTGP(,RD)                                    0749 01610000
         MVC   WTGMODNM(6,@01),IGG                                 0749 01611000
*             TRRWTGP->WTGMODID=DEBSUBID;/* MOVE IN SUBRTN ID        */ 01612000
         MVC   WTGMODID(2,@01),DEBSUBID(DEBSBPTR)                  0750 01613000
*             R0=TRRWTGP;           /* SET SUBRTN NAME PTR           */ 01614000
         LR    R0,@01                                              0751 01615000
*             GEN(DELETE EPLOC=(0));/* DELETE SUBROUTINE             */ 01616000
         DELETE EPLOC=(0)                                               01617000
*             DEBSBPTR=DEBSBPTR+2;  /* INCREMENT TO NEXT ID          */ 01618000
         LA    DEBSBPTR,2(,DEBSBPTR)                               0753 01619000
*             RC=RC-1;              /* DECREMENT ID COUNTER          */ 01620000
         BCTR  RC,0                                                0754 01621000
*           END;                                                   0755 01622000
@DE00748 LTR   RC,RC                                               0755 01623000
         BNZ   @DL00748                                            0755 01624000
*         END;                                                     0756 01625000
*     END;                                                         0757 01626000
*   RFY                                                            0758 01627000
*     R0 UNRSTD;                    /* MAKE REG UNRESTRICTED         */ 01628000
*   RFY                                                            0759 01629000
*    (RA,                                                          0759 01630000
*     RET) RSTD;                    /* RESTRICT REGS                 */ 01631000
*   END CHKSUBNM;                   /* RETURN TO CALLER              */ 01632000
@EL00012 DS    0H                                                  0760 01633000
@EF00012 DS    0H                                                  0760 01634000
@ER00012 LM    @14,@12,12(@13)                                     0760 01635000
         BR    @14                                                 0760 01636000
