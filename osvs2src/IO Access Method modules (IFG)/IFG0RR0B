*             END;                                                 1068 02700000
*           IF R1^=0 THEN           /* IF RRQ TO BE FREED      YM7233*/ 02701000
@RF01047 LTR   R1,R1                                               1069 02702000
         BZ    @RF01069                                            1069 02703000
*             DO;                   /* FREE RRQ                YM7233*/ 02704000
