         AL    @01,@CF00129                                        0370 00368000
         CLI   DEBDSNAM(@01),0                                     0370 00369000
         BE    @RF00370                                            0370 00370000
*                             DO;   /* THEN                  @ZA05430*/ 00371000
*                               DEBDSNAM=TEMPNAME;/* CHANGE NAME   0372 00372000
*                                                            @ZA05430*/ 00373000
         MVC   DEBDSNAM(8,@01),TEMPNAME                            0372 00374000
*                               RFY                                0373 00375000
*                                 DEBACS01 BASED(DEBEXPTR);/* REPOINT   00376000
