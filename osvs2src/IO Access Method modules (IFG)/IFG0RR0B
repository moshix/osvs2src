         TITLE 'IFG0RR0B -- FORCE CLOSE PROCESSING && EXECUTOR INTERFAC*00001000
               CE        '                                              00002000
IFG0RR0B CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IFG0RR0B  78.146'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @03,0                                               0001 00010000
@PSTART  LA    @05,4095(,@03)                                      0001 00011000
         USING @PSTART,@03                                         0001 00012000
         USING @PSTART+4095,@05                                    0001 00013000
START0B  DS    0H                                                  0267 00014000
*   RET=ADDR(GETCRSA);              /* REG 14 RETRY ADDR             */ 00015000
         LA    RET,GETCRSA                                         0270 00016000
*   RR1RETRY=ADDR(RETRY0B);         /* SET RETRY ROUTINE ADDR        */ 00017000
         LA    @15,RETRY0B                                         0271 00018000
         ST    @15,RR1RETRY(,RRRF)                                 0271 00019000
*   GEN REFS(IECREGSV,IECEXTPR,TRRWTG,CVT,CVTDMSR);                0272 00020000
*        IECRES LOAD,EXTPR=(RD),MODNM=RR0B,BRANCH=NO  OPTIONAL TRACE    00021000
         LR    15,RD                    GET PTR TO EXT PREFIX           00022000
         STM   14,12,IECREGSV-IECEXTPR+12(15) SAVE REGISTERS            00023000
         LA    1,IECEXTPR-IECPREFX(0,0) BACK UP REGISTER 15 TO          00024000
         SR    15,1                     POINT TO START OF PREFIX        00025000
         MVC   TRRWTG(12,13),RR0B       MOVE MODULE NAME & EPA          00026000
         LA    6,TRRWTG(,13)            LOAD PTR TO WTG MOD NAME        00027000
         L     5,CVTPTR                 GET ADDRESS OF CVT              00028000
         L     5,CVTDMSR-CVT(,5)        ADDRESS OF RESIDENT RTN         00029000
         BAL   14,24(,5)                LOAD AND RETURN                 00030000
         LM    14,12,44(15)             RESTORE REGISTERS               00031000
*   GEN(EJECT);                                                    0273 00032000
         EJECT                                                          00033000
*                                                                  0274 00034000
*/********************************************************************/ 00035000
*/*               BUILD COMPONENT RECOVERY STATUS AREA               */ 00036000
*/********************************************************************/ 00037000
*                                                                  0274 00038000
*GETCRSA:                           /* OBTAIN AND INITIALIZE CRSA    */ 00039000
*   SARETRY=ADDR(CHKFIN1);          /* SET SAVED RETRY ADDR          */ 00040000
GETCRSA  LA    @15,CHKFIN1                                         0274 00041000
         ST    @15,SARETRY(,RD)                                    0274 00042000
*   R2=IECDCBNO;                    /* GET NUMBER OF DCBS            */ 00043000
         L     @15,WTGPREFX(,RWTG)                                 0275 00044000
         SLR   R2,R2                                               0275 00045000
         IC    R2,IECDCBNO(,@15)                                   0275 00046000
*   R2=(R2*LENGTH(CRSIECNT))+LENGTH(IECCRSA);/* CALC CRSA CORE       */ 00047000
         LR    @15,R2                                              0276 00048000
         MH    @15,@CH00188                                        0276 00049000
         LA    @15,20(,@15)                                        0276 00050000
         LR    R2,@15                                              0276 00051000
*   RFY                                                            0277 00052000
*     RF RSTD;                      /* RESTRICT REG                  */ 00053000
*   RF=ADDR(TRRSAV2);               /* PT TO SAVE AREA FOR IECRES    */ 00054000
         LA    RF,TRRSAV2(,RD)                                     0278 00055000
*   GEN(IECRES GET,PREFIX=NO,LV=(R2),STM=(2,14,0(RF)))             0279 00056000
*   REFS(CVT,CVTDMSR);              /* GET CRSA CORE                 */ 00057000
         IECRES GET,PREFIX=NO,LV=(R2),STM=(2,14,0(RF))                  00058000
*   RFY                                                            0280 00059000
*     RF UNRSTD;                    /* MAKE REG UNRESTRICTED         */ 00060000
*   RFY                                                            0281 00061000
*     R1 RSTD;                      /* RESTRICT REG                  */ 00062000
*   IF SDWACOMP^=0 THEN             /* IF OLD CRSA PTR EXISTS THEN   */ 00063000
         L     @15,SDWACOMP(,SDWAPTR)                              0282 00064000
         LTR   @15,@15                                             0282 00065000
         BZ    @RF00282                                            0282 00066000
*     R1->CRSIECBS=SDWACOMP;        /* SAVE IT IN NEW CRSA           */ 00067000
         ST    @15,CRSIECBS(,R1)                                   0283 00068000
*   GEN(MODESET EXTKEY=ZERO);       /* KEY ZERO                YM7871*/ 00069000
@RF00282 DS    0H                                                  0284 00070000
         MODESET EXTKEY=ZERO                                            00071000
*   SDWACOMP=R1;                    /* PUT NEW CRSA PTR IN RTCA      */ 00072000
         ST    R1,SDWACOMP(,SDWAPTR)                               0285 00073000
*   GEN(MODESET EXTKEY=DATAMGT);    /* DATA MANAGEMENT KEY     YM7871*/ 00074000
         MODESET EXTKEY=DATAMGT                                         00075000
*   CRSIECID=OCEID;                 /* INIT CRSA COMPONENT ID        */ 00076000
         L     @15,SDWACOMP(,SDWAPTR)                              0287 00077000
         MVC   CRSIECID(3,@15),OCEID                               0287 00078000
*   CRSIECMM(8)=ON;                 /* INIT CRSA FORMAT ID           */ 00079000
         OI    CRSIECMM(@15),B'00000001'                           0288 00080000
*   CRSIECLN=R2;                    /* INIT CRSA LENGTH              */ 00081000
         STH   R2,CRSIECLN(,@15)                                   0289 00082000
*   CRSIECSP=230;                   /* INIT CRSA SUBPOOL NUMBER      */ 00083000
         MVI   CRSIECSP(@15),X'E6'                                 0290 00084000
*   CRSIECKY=FIVE;                  /* INIT CRSA KEY                 */ 00085000
         MVI   CRSIECKY(@15),X'05'                                 0291 00086000
*   CRSIECFN=RRFUNCTN;              /* INIT CRSA FUNCTION ID         */ 00087000
         MVC   CRSIECFN(1,@15),RRFUNCTN(RRRF)                      0292 00088000
*   R2=IECDCBNO;                    /* GET NUMBER OF DCBS            */ 00089000
         L     @15,WTGPREFX(,RWTG)                                 0293 00090000
         SLR   R2,R2                                               0293 00091000
         IC    R2,IECDCBNO(,@15)                                   0293 00092000
*   R1=R1+(LENGTH(IECCRSA)-LENGTH(CRSIECNT));/* SET UP SO THAT THE 0294 00093000
*                                      FOLLOWING DO-WHILE LOOP WILL     00094000
*                                      PICK UP 1ST CRSA ENTRY        */ 00095000
         LA    R1,8(,R1)                                           0294 00096000
*   RWTGC=RWTG+(LENGTH(WTG)-LENGTH(WTGENTRY));/* SET UP SO THAT THE     00097000
*                                      FOLLOWING DO-WHILE LOOP WILL     00098000
*                                      PICK UP 1ST WTG ENTRY         */ 00099000
         LA    RWTGC,24(,RWTG)                                     0295 00100000
*   DO WHILE R2^=0;                 /* FOR EACH DCB,                 */ 00101000
         B     @DE00296                                            0296 00102000
@DL00296 DS    0H                                                  0297 00103000
*     R1=R1+LENGTH(CRSIECNT);       /* PT TO ITS CRSA ENTRY,         */ 00104000
         LA    R1,12(,R1)                                          0297 00105000
*     RWTGC=RWTGC+LENGTH(WTGENTRY); /* PT TO ITS WTG TBL ENTRY,      */ 00106000
         LA    RWTGC,8(,RWTGC)                                     0298 00107000
*     RCORE=WTGCORE;                /* GET ITS WORK AREA ADDRESS.    */ 00108000
         L     RCORE,WTGCORE-1(,RWTGC)                             0299 00109000
         LA    RCORE,0(,RCORE)                                     0299 00110000
*     IF RCORE^=0 THEN              /* IF A WORK AREA PTR EXISTS,    */ 00111000
         LTR   RCORE,RCORE                                         0300 00112000
         BZ    @RF00300                                            0300 00113000
*       DXATCOM4=R1;                /* SAVE THE CRSA ENTRY PTR.      */ 00114000
         ST    R1,DXATCOM4(,RCORE)                                 0301 00115000
*     R2=R2-1;                      /* DECREMENT DCB COUNT           */ 00116000
@RF00300 BCTR  R2,0                                                0302 00117000
*   END;                            /* END OF CRSA ENTRY LOOP        */ 00118000
@DE00296 LTR   R2,R2                                               0303 00119000
         BNZ   @DL00296                                            0303 00120000
*   R1->CRSLAST=ON;                 /* INDICATE LAST CRSA ENTRY      */ 00121000
         OI    CRSLAST(R1),B'10000000'                             0304 00122000
*   RFY                                                            0305 00123000
*     R1 UNRSTD;                    /* MAKE REG UNRESTRICTED         */ 00124000
*   GEN(EJECT);                                                    0306 00125000
         EJECT                                                          00126000
*                                                                  0307 00127000
*/********************************************************************/ 00128000
*/*         CHECK FOR COMPLETION OF FIRST MODULE OF FUNCTION         */ 00129000
*/********************************************************************/ 00130000
*                                                                  0307 00131000
*CHKFIN1:                           /* CHK FOR 1ST LOAD COMPLETION   */ 00132000
*   SARETRY=RR0E(9:12);             /* SET RETRY PTR TO IFG0RR0E     */ 00133000
CHKFIN1  MVC   SARETRY(4,RD),RR0E+8                                0307 00134000
*   IF RRFFIN1=OFF THEN             /* IF 1ST MOD NOT COMPLETE YM7099*/ 00135000
         TM    RRFFIN1(RRRF),B'10000000'                           0308 00136000
         BNZ   @RF00308                                            0308 00137000
*     DO;                           /* THEN                          */ 00138000
*       RFY                                                        0310 00139000
*         RF RSTD;                  /* RESTRICT REG                  */ 00140000
*       RF=RR0E(9:12);              /* GET PTR TO CLEAN UP MODULE    */ 00141000
         ICM   RF,15,RR0E+8                                        0311 00142000
*       GEN(BR    RF);              /* BRANCH TO CLEAN UP MODULE     */ 00143000
         BR    RF                                                       00144000
*       RFY                                                        0313 00145000
*         RF UNRSTD;                /* MAKE REG UNRESTRICTED         */ 00146000
*     END;                                                         0314 00147000
*   ELSE                            /* 1ST LOAD COMPLETED            */ 00148000
*     TRRRWTGC=RWTG+(LENGTH(WTG)-LENGTH(WTGENTRY));/* SET UP SO THAT    00149000
*                                      THE FOLLOWING DO-WHILE LOOP 0315 00150000
*                                      WILL PICK UP 1ST WTG TBL ENTRY   00151000
*                                      ON INITIAL ENTRY TO THE SELECT   00152000
*                                      SUBROUTINE                    */ 00153000
         B     @RC00308                                            0315 00154000
@RF00308 LA    @15,24                                              0315 00155000
         ALR   @15,RWTG                                            0315 00156000
         ST    @15,TRRRWTGC(,RD)                                   0315 00157000
*/********************************************************************/ 00158000
*/*                    START OF FORCE CLOSE LOOP                     */ 00159000
*/********************************************************************/ 00160000
*                                                                  0316 00161000
*FCLOOP:                            /* START OF FORCE CLOSE LOOP     */ 00162000
*   DO WHILE RRFFINFC=OFF;          /* ENTER FORCE CLOSE LOOP WHILE     00163000
*                                      FINAL PASS NOT YET MADE       */ 00164000
@RC00308 DS    0H                                                  0316 00165000
FCLOOP   B     @DE00316                                            0316 00166000
@DL00316 DS    0H                                                  0317 00167000
*SELTCALL:                          /* DCB/ACB SELECTION             */ 00168000
*     CALL SELECT;                  /* SELECT DCB TO FORCE CLOSE     */ 00169000
SELTCALL BAL   @14,SELECT                                          0317 00170000
*     GOTO SELTCALL;                /* GO MAKE ANOTHER SELECTION     */ 00171000
         B     SELTCALL                                            0318 00172000
*SELTFIN:                           /* DCB/ACB SELECTION DONE        */ 00173000
*     RWTGC=TRRRWTGC;               /* GET CURRENT WTG ENTRY PTR     */ 00174000
SELTFIN  L     RWTGC,TRRRWTGC(,RD)                                 0319 00175000
*     IF RWTGC>=(IECRRPRM-8) THEN   /* IF WTG ENTRY PTR IS NOT       */ 00176000
         L     @15,WTGPREFX(,RWTG)                                 0320 00177000
         L     @15,IECRRPRM(,@15)                                  0320 00178000
         SL    @15,@CF00069                                        0320 00179000
         CR    RWTGC,@15                                           0320 00180000
         BL    @RF00320                                            0320 00181000
*       DO;                         /* VALID THEN                    */ 00182000
*         RRFFINFC=ON;              /* SET FINAL PASS INDICATOR      */ 00183000
         OI    RRFFINFC(RRRF),B'00100000'                          0322 00184000
*         GOTO POSTFC;              /* GO TO END OF FORCE CLOSE      */ 00185000
         B     POSTFC                                              0323 00186000
*       END;                                                       0324 00187000
*     IF DCBMFECP=OFF&ACBDORGA=ON&(ACBAMETH='60'X|ACBDDNM=VTAMDDNM)     00188000
*       THEN                        /* IF VTAM THEN          @ZA03175*/ 00189000
@RF00320 TM    DCBMFECP+40(R2),B'10000000'                         0325 00190000
         BNZ   @RF00325                                            0325 00191000
         TM    ACBDORGA(R2),B'00001000'                            0325 00192000
         BNO   @RF00325                                            0325 00193000
         CLI   ACBAMETH(R2),96                                     0325 00194000
         BE    @RT00325                                            0325 00195000
         CLC   ACBDDNM(8,R2),VTAMDDNM                              0325 00196000
         BE    @RT00325                                            0325 00197000
*       GOTO SKIPCHK;               /* SKIP DEBCHK           @ZA03175*/ 00198000
*     IF IDAMB=OFF THEN             /* IF AMB IS IN PROCESS    YM4652*/ 00199000
@RF00325 TM    IDAMB(RD),B'10000000'                               0327 00200000
         BNZ   @RF00327                                            0327 00201000
*       DO;                         /* THEN                          */ 00202000
*         DEBPTR=0;                 /* INITIALIZE DEB PTR TO ZERO    */ 00203000
         SLR   DEBPTR,DEBPTR                                       0329 00204000
*VALCALL:                                                          0330 00205000
*         CALL DEBVALCK;            /* GO FIND VALID DEB PTR         */ 00206000
VALCALL  BAL   @14,DEBVALCK                                        0330 00207000
*       END;                                                       0331 00208000
*SKIPCHK:                                                          0332 00209000
*     TRRWORK3=0;                   /* INIT FOR EXEC RETURN CODE     */ 00210000
@RF00327 DS    0H                                                  0332 00211000
SKIPCHK  SLR   @15,@15                                             0332 00212000
         ST    @15,TRRWORK3(,RD)                                   0332 00213000
*     SARETRY=ADDR(POSTFCX);        /* SET SAVED RETRY PTR           */ 00214000
         LA    @01,POSTFCX                                         0333 00215000
         ST    @01,SARETRY(,RD)                                    0333 00216000
*     IF DXATCOM4^=0 THEN           /* IF A CRSA EXISTS THEN         */ 00217000
         L     @01,DXATCOM4(,RCORE)                                0334 00218000
         CR    @01,@15                                             0334 00219000
         BE    @RF00334                                            0334 00220000
*       CRSFCAT=ON;                 /* SHOW FORCE CLOSE ATTEMPTED    */ 00221000
         OI    CRSFCAT(@01),B'01000000'                            0335 00222000
*     RFY                                                          0336 00223000
*       RF RSTD;                    /* RESTRICT REG                  */ 00224000
@RF00334 DS    0H                                                  0337 00225000
*     IF RRFUNCTN^=RRFEOV&          /* IF NOT AN EOV ENTRY AND       */ 00226000
*         RRFUNCTN^=RRFFEOV&        /* NOT A FEOV ENTRY AND          */ 00227000
*         RRFUNCTN^=RRFTCLOS THEN   /* NOT A TCLOSE ENTRY THEN       */ 00228000
         CLI   RRFUNCTN(RRRF),X'37'                                0337 00229000
         BE    @RF00337                                            0337 00230000
         CLI   RRFUNCTN(RRRF),X'1F'                                0337 00231000
         BE    @RF00337                                            0337 00232000
         CLI   RRFUNCTN(RRRF),X'17'                                0337 00233000
         BE    @RF00337                                            0337 00234000
*CTRCALL:                                                          0338 00235000
*       CALL CTRLCHK;               /* CHK IF EXEC NEEDS CONTROL     */ 00236000
CTRCALL  BAL   @14,CTRLCHK                                         0338 00237000
*     ELSE                          /* EOV OR FEOV OR TCLOSE ENTRY   */ 00238000
*       DO;                                                        0339 00239000
         B     @RC00337                                            0339 00240000
@RF00337 DS    0H                                                  0340 00241000
*         IF DCBMFECP=OFF&ACBDORGA=ON&(ACBAMETH='60'X|ACBDDNM=VTAMDDNM) 00242000
*           THEN                    /* IF VTAM THEN          @ZA03175*/ 00243000
         TM    DCBMFECP+40(R2),B'10000000'                         0340 00244000
         BNZ   @RF00340                                            0340 00245000
         TM    ACBDORGA(R2),B'00001000'                            0340 00246000
         BNO   @RF00340                                            0340 00247000
         CLI   ACBAMETH(R2),96                                     0340 00248000
         BE    @RT00340                                            0340 00249000
         CLC   ACBDDNM(8,R2),VTAMDDNM                              0340 00250000
         BE    @RT00340                                            0340 00251000
*           GOTO POSTFCX;           /* SKIP EOV PROCESS      @ZA03175*/ 00252000
*         IF IDAMB=ON THEN          /* IF AMB IS IN PROCESS    YM4652*/ 00253000
@RF00340 TM    IDAMB(RD),B'10000000'                               0342 00254000
         BNO   @RF00342                                            0342 00255000
*           DO;                                                    0343 00256000
*             RF=ADDR(VSAMX);       /* PT TO VSAM RECOVERY EXEC      */ 00257000
         LA    RF,VSAMX                                            0344 00258000
*             GOTO EOVCALLX;        /* GO CALL VSAM EXECUTOR         */ 00259000
         B     EOVCALLX                                            0345 00260000
*           END;                                                   0346 00261000
*         ELSE                      /* NOT AN AMB                    */ 00262000
*           DO;                                                    0347 00263000
@RF00342 DS    0H                                                  0348 00264000
*             IF DXATFC=OFF&        /* IF FORCE CLOSE NOT INDIC    0348 00265000
*                                                              YM7099*/ 00266000
*                 DEBPTR^=0 THEN    /* A DEB PTR EXISTS THEN   YM7099*/ 00267000
         TM    DXATFC(RCORE),B'10000000'                           0348 00268000
         BNZ   @RF00348                                            0348 00269000
         LTR   DEBPTR,DEBPTR                                       0348 00270000
         BZ    @RF00348                                            0348 00271000
*               DO;                                                0349 00272000
*                 IF DXATNVOL=ON&   /* IF ON NEW VOLUME AND    YM7099*/ 00273000
*                     UCB3DACC=ON THEN/* DEV IS DIRECT ACCESS  YM7099*/ 00274000
