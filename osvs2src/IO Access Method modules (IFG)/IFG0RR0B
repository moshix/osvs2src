         IC    @14,LIMIT(,RD)                                      0479 00688000
         CR    @15,@14                                             0479 00689000
         BNH   @DL00472                                            0479 00690000
*/********************************************************************/ 00691000
*/*                     END OF FORCE CLOSE LOOP                      */ 00692000
*/********************************************************************/ 00693000
*                                                                  0480 00694000
*POSTFC:                            /* END OF FORCE CLOSE LOOP       */ 00695000
*   END;                            /* FORCE CLOSE COMPLETED         */ 00696000
POSTFC   DS    0H                                                  0480 00697000
@DE00316 TM    RRFFINFC(RRRF),B'00100000'                          0480 00698000
