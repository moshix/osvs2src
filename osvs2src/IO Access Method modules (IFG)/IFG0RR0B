         STM   0,14,IECREGSV-IECEXTPR(15) SAVE REGISTERS                01140000
         LA    1,IECEXTPR-IECPREFX(0,0) BACK UP REGISTER 15 TO          01141000
         SR    15,1                     POINT TO START OF PREFIX        01142000
         MVC   WTGMODNM(12,6),0(RC)     MOVE MODULE NAME AND EPA        01143000
         LA    6,WTGMODNM(,6)           LOAD PTR TO WTG MODULE NAME     01144000
         L     5,CVTPTR                 LOAD ADDRESS OF CVT             01145000
         L     5,CVTDMSR-CVT(,5)        LOAD ADDRESS OF IFG019RA        01146000
         BAL   14,24(,5)                LOAD AND RETURN                 01147000
         LM    0,14,IECREGSV-IECPREFX(15) RESTORE REGISTERS             01148000
*       IF RC->FCXNAME=HASPCIX THEN /* IF HASP CI EXEC WAS CHOSEN    */ 01149000
         CLC   FCXNAME(12,RC),HASPCIX                              0607 01150000
         BNE   @RF00607                                            0607 01151000
*         SPDCBMSK=HEXZERO4;        /* THEN CLEAR SPOOLED DCB      0608 01152000
*                                      MODIFICATION MASK AREA        */ 01153000
         MVC   SPDCBMSK(4,RD),HEXZERO4                             0608 01154000
*       RC=WTGMODEP;                /* PICK UP EXECUTOR EPA          */ 01155000
@RF00607 L     RC,WTGMODEP(,RWTG)                                  0609 01156000
*       RWTG=RA;                    /* RESTORE ORIGINAL WTG TBL PT   */ 01157000
         LR    RWTG,RA                                             0610 01158000
*       R5=RR1WRKAR;                /* PT TO WORK AREA FOR EXEC      */ 01159000
         L     R5,RR1WRKAR(,RRRF)                                  0611 01160000
