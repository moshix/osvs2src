*                                                                  0173 00184000
@RF00171 DS    0H                                                  0173 00185000
         IECRES FREE,PREFIX=FIRST,A=(RD) FREE O/C/E/D RECOVERY WORKAREA 00186000
         BR    RA                       BRANCH TO RTM RETURN LOGIC      00187000
*/********************************************************************/ 00188000
*/*       RETURN TO RTM VIA BR 14 TO CONTINUE THE TERMINATION        */ 00189000
*/********************************************************************/ 00190000
*                                                                  0174 00191000
*PERCBR:                                                           0174 00192000
*   RFY                                                            0174 00193000
*     RF RSTD;                                                     0174 00194000
PERCBR   DS    0H                                                  0175 00195000
*   RF=0;                           /* SET RETURN CODE 0 IN REG 15   */ 00196000
         SLR   RF,RF                                               0175 00197000
*   RET=R9;                         /* GET SAVED RTM RETURN PTR      */ 00198000
         LR    RET,R9                                              0176 00199000
*   GEN(BR   RET);                  /* RETURN TO RTM                 */ 00200000
         BR   RET                                                       00201000
*   RFY                                                            0178 00202000
*     RF UNRSTD;                                                   0178 00203000
*                                                                  0178 00204000
*/********************************************************************/ 00205000
*/*       RETURN TO RTM VIA SETRP TO CONTINUE THE TERMINATION        */ 00206000
*/********************************************************************/ 00207000
*                                                                  0179 00208000
*PERCSET:                                                          0179 00209000
*   RD=R9;                          /* GET SAVED RTM SAVE AREA PTR   */ 00210000
PERCSET  LR    RD,R9                                               0179 00211000
*   SDWAPTR=R8;                     /* GET SAVED RTCA PTR            */ 00212000
         LR    SDWAPTR,R8                                          0180 00213000
*   GEN(MODESET EXTKEY=SUPR);       /* SET SUPERVISOR KEY            */ 00214000
         MODESET EXTKEY=SUPR                                            00215000
*   DO;                             /* SETRP WKAREA(SDWAPTR)REGS(14,1   00216000
*                                      2)RC(0)                       */ 00217000
*     RESPECIFY                                                    0183 00218000
*      (GPR00P,                                                    0183 00219000
*       GPR01P,                                                    0183 00220000
*       GPR14P,                                                    0183 00221000
*       GPR15P) RSTD;                                              0183 00222000
*     GPR01P=SDWAPTR;                                              0184 00223000
         LR    GPR01P,SDWAPTR                                      0184 00224000
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */ 00225000
         MVI   SDWARCDE(GPR01P),X'00'                              0185 00226000
*     GEN(RETURN (14,12));                                         0186 00227000
