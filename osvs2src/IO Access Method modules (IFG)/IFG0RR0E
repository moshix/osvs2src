*           RA=ADDR(PERCSET);       /* PT TO SETRP PERCOLATE LOGIC   */ 00154000
         LA    RA,PERCSET                                          0164 00155000
*           R8=SDWAPTR;             /* SAVE RTCA PTR IN REGISTER     */ 00156000
         LR    R8,SDWAPTR                                          0165 00157000
*         END;                                                     0166 00158000
*     END;                                                         0167 00159000
*   ELSE                            /* AN RTCA DOES NOT EXIST        */ 00160000
*     RA=ADDR(PERCBR);              /* PT TO BRANCH PERCOLATE LOGIC  */ 00161000
         B     @RC00156                                            0168 00162000
@RF00156 LA    RA,PERCBR                                           0168 00163000
*   RFY                                                            0169 00164000
*     R9 RSTD;                                                     0169 00165000
@RC00156 DS    0H                                                  0170 00166000
*   R9=SACALLER;                    /* PICK UP RTM RETURN ADDRESS IF    00167000
*                                      AN RTCA DOES NOT EXIST; PICK     00168000
*                                      UP RTM SAVE AREA PTR IF AN  0170 00169000
*                                      RTCA DOES EXIST.              */ 00170000
         L     R9,SACALLER(,RD)                                    0170 00171000
*/********************************************************************/ 00172000
*/*    FREE THE RECOVERY ROUTINE WORK AREA AND BRANCH ON REGISTER    */ 00173000
*/*    RA TO THE APPROPRIATE LOGIC FOR RETURNING CONTROL TO RTM      */ 00174000
*/********************************************************************/ 00175000
*                                                                  0171 00176000
*TRRWAFRE:                                                         0171 00177000
*   IF WADUMP=ON THEN               /* TEST IF DUMP RQSTD    @ZA01557*/ 00178000
TRRWAFRE TM    WADUMP(RD),B'00100000'                              0171 00179000
         BNO   @RF00171                                            0171 00180000
*     CALL RRESDUMP;                /* CALL SDUMP            @ZA01557*/ 00181000
         BAL   @14,RRESDUMP                                        0172 00182000
