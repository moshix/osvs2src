*   GEN(MODESET EXTKEY=SUPR);       /* SET SUPERVISOR KEY            */ 00253000
         MODESET EXTKEY=SUPR                                            00254000
*   DO;                             /* SETRP RETADDR(RETLAB)RETREGS(Y   00255000
*                                      ES)WKAREA(SDWAPTR)DUMP(NO)REGS   00256000
*                                      (14,12)FRESDWA(YES)RC(4)      */ 00257000
