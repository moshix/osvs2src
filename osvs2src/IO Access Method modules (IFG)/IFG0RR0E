         TITLE 'IFG0RR0E -- RESOURCE CLEANUP && FINAL RECOVERY ROUTINE *00001000
                FUNCTIONS'                                              00002000
IFG0RR0E CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IFG0RR0E  78.250'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @03,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@03                                         0001 00012000
JSCBDISP EQU 188                                                        00013000
JSCBAOS1 EQU  72                                                        00014000
JSCBAOS2 EQU 192                                                        00015000
START0E  DS    0H                                                  0131 00016000
*        IECRES LOAD,EXTPR=(RD),MODNM=RR0E,BRANCH=NO  OPTIONAL TRACE    00017000
         LR    15,RD                    GET PTR TO EXT PREFIX           00018000
         STM   14,12,IECREGSV-IECEXTPR+12(15) SAVE REGISTERS            00019000
         LA    1,32                     BACK UP REGISTER 15 TO          00020000
         SR    15,1                     POINT TO START OF PREFIX        00021000
         MVC   TRRWTG(12,13),RR0E       MOVE MODULE NAME & EPA          00022000
         LA    6,TRRWTG(,13)            LOAD PTR TO WTG MOD NAME        00023000
         L     5,CVTPTR                 GET ADDRESS OF CVT              00024000
         L     5,CVTDMSR-CVT(,5)        ADDRESS OF RESIDENT RTN         00025000
         BAL   14,24(5)                 LOAD AND RETURN                 00026000
         LM    14,12,44(15)             RESTORE REGISTERS               00027000
*   IF RRFDADSM=ON THEN             /* IF A DADSM ENTRY THEN         */ 00028000
         TM    RRFDADSM(RRRF),B'10000000'                          0133 00029000
         BNO   @RF00133                                            0133 00030000
*     DO;                                                          0134 00031000
*       LIMIT=DIM(A);               /* SET UP TO CALL THE DADSM      */ 00032000
         MVI   LIMIT(RD),X'05'                                     0135 00033000
*       ARYPTR=ADDR(A);             /* FINAL CLEAN UP SUBROUTINES    */ 00034000
         LA    @15,A                                               0136 00035000
         ST    @15,ARYPTR(,RD)                                     0136 00036000
*     END;                                                         0137 00037000
*   ELSE                            /* OPEN/CLOSE/EOV ENTRY          */ 00038000
*     DO;                                                          0138 00039000
*                                                                  0138 00040000
         B     @RC00133                                            0138 00041000
@RF00133 DS    0H                                                  0139 00042000
*       /*************************************************************/ 00043000
*       /*                                                           */ 00044000
*       /* CHECK FOR VALID MAINLINE RETRY CONDITIONS                 */ 00045000
*       /*                                                           */ 00046000
*       /*************************************************************/ 00047000
*                                                                  0139 00048000
*       IF RRMLRTRY^=0 THEN         /* IF A MAINLINE RETRY ZA00141   */ 00049000
         L     @15,RRMLRTRY(,RRRF)                                 0139 00050000
         LTR   @15,@15                                             0139 00051000
         BZ    @RF00139                                            0139 00052000
*         DO;                       /* ADDR IS PROVIDED THEN         */ 00053000
*           IF RRFNRTCA=ON|         /* IF NO RTCA EXISTS OR          */ 00054000
*               RRFTERM=ON|         /* A SPECIAL ENTRY OR            */ 00055000
*               SDWAPERC=ON|        /* A PERCOLATION ENTRY OR        */ 00056000
*               SDWAPCHK=ON|        /* A PROGRAM CHK ERROR OR        */ 00057000
*               SDWAMCHK=ON|        /* A MACHINE CHK ERROR OR        */ 00058000
*               SDWARKEY=ON|        /* A RESTART KEY ENTRY           */ 00059000
*               SDWASVCD=OFF|       /* SVC D NOT ISSUED OR           */ 00060000
*               SDWACLUP=ON|        /* RETRY NOT ALLOWED OR          */ 00061000
*               SDWANRBE=ON THEN    /* RB ASSOCIATED WITH THIS ESTAE    00062000
*                                      EXIT WAS NOT IN CONTROL AT  0141 00063000
*                                      TIME OF ERROR THEN            */ 00064000
         TM    RRFNRTCA(RRRF),B'01001000'                          0141 00065000
         BNZ   @RT00141                                            0141 00066000
         TM    SDWAPERC(SDWAPTR),B'00010000'                       0141 00067000
         BO    @RT00141                                            0141 00068000
         TM    SDWAPCHK(SDWAPTR),B'11100000'                       0141 00069000
         BNZ   @RT00141                                            0141 00070000
         TM    SDWASVCD(SDWAPTR),B'00010000'                       0141 00071000
         BNO   @RT00141                                            0141 00072000
         TM    SDWACLUP(SDWAPTR),B'11000000'                       0141 00073000
         BZ    @RF00141                                            0141 00074000
@RT00141 DS    0H                                                  0142 00075000
*             RRMLRTRY=0;           /* SET RETRY ADDR TO ZERO --- NO    00076000
*                                      RETRY WILL BE ATTEMPTED       */ 00077000
         SLR   @15,@15                                             0142 00078000
         ST    @15,RRMLRTRY(,RRRF)                                 0142 00079000
*         END;                      /* ZA00141                       */ 00080000
@RF00141 DS    0H                                                  0144 00081000
*       LIMIT=DIM(B);               /* SET UP TO CALL OCE ZA00141    */ 00082000
@RF00139 MVI   LIMIT(RD),X'02'                                     0144 00083000
*       ARYPTR=ADDR(B);             /* FINAL CLEAN UP SUBROUTINES    */ 00084000
         LA    @15,B                                               0145 00085000
         ST    @15,ARYPTR(,RD)                                     0145 00086000
*     END;                                                         0146 00087000
*                                                                  0146 00088000
*/********************************************************************/ 00089000
*/*                CALL RESOURCE CLEANUP SUBROUTINES                 */ 00090000
*/********************************************************************/ 00091000
*                                                                  0147 00092000
*SUBCALL:                                                          0147 00093000
*   SARETRY=ADDR(RETCALL);          /* SET SAVED RETRY ADDRESS       */ 00094000
@RC00133 DS    0H                                                  0147 00095000
SUBCALL  LA    @15,RETCALL                                         0147 00096000
         ST    @15,SARETRY(,RD)                                    0147 00097000
*   RR1RETRY=ADDR(RETRY0E);         /* SET UP RETRY ADDR             */ 00098000
         LA    @15,RETRY0E                                         0148 00099000
         ST    @15,RR1RETRY(,RRRF)                                 0148 00100000
*   DO I=1 TO LIMIT;                /* DO LOOP TO CALL SUBROUTINES   */ 00101000
         LA    @15,1                                               0149 00102000
         B     @DE00149                                            0149 00103000
@DL00149 DS    0H                                                  0150 00104000
*     RFY                                                          0150 00105000
*       RF RSTD;                                                   0150 00106000
*     RF=DUMARY(I);                                                0151 00107000
         LH    @12,I(,RD)                                          0151 00108000
         SLA   @12,2                                               0151 00109000
         L     @11,ARYPTR(,RD)                                     0151 00110000
         AL    @11,@CF03103                                        0151 00111000
         L     RF,DUMARY(@12,@11)                                  0151 00112000
*     CALL SUB;                     /* CALL SUBROUTINES              */ 00113000
         BALR  @14,@15                                             0152 00114000
*     RFY                                                          0153 00115000
*       RF UNRSTD;                                                 0153 00116000
*RETCALL:                           /* RETURN FROM SUBROUTINE        */ 00117000
*   END;                                                           0154 00118000
RETCALL  LA    @15,1                                               0154 00119000
         AH    @15,I(,RD)                                          0154 00120000
@DE00149 STH   @15,I(,RD)                                          0154 00121000
         SLR   @12,@12                                             0154 00122000
         IC    @12,LIMIT(,RD)                                      0154 00123000
         CR    @15,@12                                             0154 00124000
         BNH   @DL00149                                            0154 00125000
*   RR1RETRY=0;                     /* ZERO 1ST LEVEL RETRY ADDR     */ 00126000
         SLR   @15,@15                                             0155 00127000
         ST    @15,RR1RETRY(,RRRF)                                 0155 00128000
*/********************************************************************/ 00129000
*/* SAVE ADDRESSES FOR RETURNING TO RTM SINCE ONLY REGISTERS EIGHT,  */ 00130000
*/* NINE AND TEN ARE PRESERVED ACROSS FREEING THE RECOVERY WORKAREA. */ 00131000
*/********************************************************************/ 00132000
*                                                                  0156 00133000
*   IF RRFNRTCA=OFF THEN            /* IF AN RTCA EXISTS THEN        */ 00134000
         TM    RRFNRTCA(RRRF),B'01000000'                          0156 00135000
         BNZ   @RF00156                                            0156 00136000
*     DO;                                                          0157 00137000
*       IF RRFTERM=OFF&             /* IF NOT A SPECIAL ENTRY & A    */ 00138000
*           RRMLRTRY^=0 THEN        /* MAINLINE RETRY PTR EXISTS THEN*/ 00139000
