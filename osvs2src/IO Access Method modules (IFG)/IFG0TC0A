*IFG0TC0M:                          /*                         YM7870*/ 02146000
*   PROC OPTIONS(NOSAVEAREA);                                      0823 02147000
         B     @PB00017                                            0823 02148000
IFG0TC0M STM   @14,@12,12(@13)                                     0823 02149000
*   MSGBUF=IEC999I;                 /* SETUP MSG             @ZA01557*/ 02150000
         MVC   MSGBUF(70,RCORE),IEC999I                            0824 02151000
*   RFY                                                            0825 02152000
*     R9 RSTD;                                                     0825 02153000
*   R9=TCBTIO;                      /* GET TIOT PTR          @ZA01557*/ 02154000
         L     @11,WARMPL(,RCORE)                                  0826 02155000
         L     @11,RMPLTCBA(,@11)                                  0826 02156000
         L     R9,TCBTIO(,@11)                                     0826 02157000
*   IF R9^=0 THEN                   /* CHECK FOR TIOT        @ZA01557*/ 02158000
