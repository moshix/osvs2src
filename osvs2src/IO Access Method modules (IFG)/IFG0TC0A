*IFG0TC0I:                                                         0819 02118000
*   PROC OPTIONS(NOSAVEAREA);                                      0819 02119000
         B     @PB00016                                            0819 02120000
IFG0TC0I STM   @14,@12,12(@13)                                     0819 02121000
*   GEN REFS(IECREGSV,IECEXTPR,WTGMODNM,CVT,CVTDMSR);              0820 02122000
*        IECRES LOAD,EXTPR=(RPREFX),MODNM=CATVSMOD,BRANCH=DIRECT        02123000
         LR    15,RPREFX                GET PTR TO EXT PREFIX  @ZA01557 02124000
         STM   0,14,IECREGSV-IECEXTPR(15) SAVE REGISTERS         Y02080 02125000
         LA    1,32                     BACK UP REGISTER 15 TO   Y02080 02126000
         SR    15,1                     POINT TO START OF PREFIX Y02080 02127000
         MVC   WTGMODNM(12,6),CATVSMOD  MOVE MODULE NAME & EPA   Y02080 02128000
         LA    6,WTGMODNM(,6)           LOAD PTR TO WTG MOD NAME Y02080 02129000
         L     5,CVTPTR                 GET ADDRESS OF CVT       Y02080 02130000
         L     5,CVTDMSR-CVT(,5)        ADDRESS OF RESIDENT RTN  Y02080 02131000
         B     20(,5)                   LOAD & BRANCH DIRECT     Y02080 02132000
