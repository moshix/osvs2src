         MVC   TCMODNM(8,RCORE),TC0AWTGM(RPREFX)                   0833 02172000
*   TCSUBNM=TCSUBNM&&TCSUBNM;       /* ZERO FOR COMPARE      @ZA01557*/ 02173000
         XC    TCSUBNM(8,RCORE),TCSUBNM(RCORE)                     0834 02174000
