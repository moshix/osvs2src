@RF00145 DS    0H                                                  0147 00336000
EQUIP1   CLI   IOSCOD(IOSBPTR),X'7E'                               0147 00337000
         BE    @RT00147                                            0147 00338000
*     GO TO EQUIP2;                                                0148 00339000
*   IF CCWRST^>0 THEN               /* INVALID CCW ADDR, PERM ERR    */ 00340000
         LTR   CCWRST,CCWRST                                       0149 00341000
         BNP   @RT00149                                            0149 00342000
*     GO TO ERRWTO;                                                0150 00343000
*   IF CMDB6=ON&                    /* READ CMD, TEST SENSE BYTE 1   */ 00344000
*       CMDB7=OFF THEN                                             0151 00345000
         TM    CMDB6(CCWRST),B'00000010'                           0151 00346000
         BNO   @RF00151                                            0151 00347000
