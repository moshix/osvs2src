ERCORR   DS    0H                                                  0176 00370000
*   IOSERR=OFF;                                                    0176 00371000
*                                                                  0176 00372000
         NI    IOSEX(IOSBPTR),B'11011011'                          0176 00373000
*   /*****************************************************************/ 00374000
*   /*                                                               */ 00375000
*   /* XCTL TO STATISTICS UPDATE ROUTINE                             */ 00376000
*   /*                                                               */ 00377000
*   /*****************************************************************/ 00378000
*                                                                  0177 00379000
*   GEN REFS(CVTPTR,CVTXTLER,STATID)SETS(IDREG,XCTLREG)EXIT NOSEQFLOW;  00380000
         LH    IDREG,STATID         ID OF STAT UPDATE ROUTINE           00381000
         L     XCTLREG,CVTXTLER(,CVTPTR) ADDR OF XCTL ROUTINE           00382000
         BR    XCTLREG              TO XCTL TO STAT UPDATE RTN          00383000
*   GEN(EJECT);                                                    0178 00384000
         EJECT                                                          00385000
*TSTSTAT:                                                          0179 00386000
*   IF UCBTBYT4=RD2501|             /* IF 2501 OR 2520, USE SECOND 0179 00387000
*                                      STATUS TABLE                  */ 00388000
*       UCBTBYT4=RDP2520 THEN                                      0179 00389000
TSTSTAT  CLI   UCBTBYT4(UCBPTR),X'04'                              0179 00390000
         BE    @RT00179                                            0179 00391000
         CLI   UCBTBYT4(UCBPTR),X'05'                              0179 00392000
         BE    @RT00179                                            0179 00393000
*     GO TO TSTSTAT2;                                              0180 00394000
*                                                                  0180 00395000
*   /*****************************************************************/ 00396000
*   /*                                                               */ 00397000
*   /* LINK TO THE RESIDENT ERROR INTERPRETER ROUTINE TO INTERROGATE */ 00398000
*   /* THE CSW STATUS BYTES FOR THE 2540.                            */ 00399000
*   /*                                                               */ 00400000
*   /*****************************************************************/ 00401000
*                                                                  0181 00402000
*   GEN REFS(CVTPTR,CVTXITP)FLOWS(PERMERR,ERCORR)SETS(SAVREG,XCTLREG);  00403000
         LR    SAVREG,BASREG        SAVE REG 15                         00404000
         L     BASREG,CVTXITP(,CVTPTR) ADDR OF ERR INTERPRETER RTN      00405000
STATBASE BALR  XCTLREG,BASREG       TO INTERPRET 2540 STATUS BYTESS     00406000
         DC    X'1A'                IF PROGRAM CHECK,                   00407000
         DC    AL1(ERSTAT1-STATBASE-2) THEN GO TO PERMERR               00408000
         DC    X'1B'                IF PROTECTION CHECK,                00409000
         DC    AL1(ERSTAT1-STATBASE-4) THEN GO TO PERMERR               00410000
         DC    X'17'                IF UNIT EXCEPTION,                  00411000
         DC    AL1(ERSTAT1-STATBASE-6) THEN GO TO PERMERR               00412000
         DC    X'19'                IF INCORRECT LENGTH,                00413000
         DC    AL1(ERSTAT1-STATBASE-8) THEN GO TO PERMERR               00414000
