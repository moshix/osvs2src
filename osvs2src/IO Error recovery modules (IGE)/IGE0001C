*                                      AND ISSUE 'I/O ERROR' MSG     */ 00328000
OVERRUN  TM    IOOVRN(EWAPTR),B'00000100'                          0163 00329000
         BO    @RT00163                                            0163 00330000
*     GO TO LOGOUT;                                                0164 00331000
*   ELSE                                                           0165 00332000
*     IOOVRN=ON;                    /* SET OVERRUN FLAG              */ 00333000
         OI    IOOVRN(EWAPTR),B'00000100'                          0165 00334000
