         NI    IORSTRT(EWAPTR),B'01111111'                         0171 00354000
*       IOSTATUS=IOSTATUS&&IOSTATUS;/* CLEAR CSW STATUS IN IOSB      */ 00355000
         XC    IOSTATUS(2,IOSBPTR),IOSTATUS(IOSBPTR)               0172 00356000
