*   EWAPTR=IOSERP;                  /* PTR FOR WORK AREA Y02990      */ 00016000
         L     EWAPTR,IOSERP(,IOSBPTR)                             0108 00017000
*   IOSERR='1'B;                    /* IOSB ERROR FLAG ON Y02990     */ 00018000
*   IOSEX='1'B;                     /* SET IOSB EXCEPTION Y02990     */ 00019000
         OI    IOSERR(IOSBPTR),B'00100100'                         0110 00020000
*   CVTREG=CVTPTR;                  /* INITIALIZE CVT REGISTER       */ 00021000
         L     CVTREG,CVTPTR                                       0111 00022000
*   IF IOSACHN='11'B                /* TEST FOR MIX CHAIN Y02990     */ 00023000
*     THEN                          /* YES, RECOVERY NOT POSSIBLE    */ 00024000
         TM    IOSACHN(IOSBPTR),B'11000000'                        0112 00025000
         BO    @RT00112                                            0112 00026000
*     GO TO EXIT22;                 /* PERMANENT ERROR- NO LOGOUT    */ 00027000
*   ELSE                            /* NO - CONTINUE                 */ 00028000
*     CCWREG=IOSCSWCA-EIGHT;        /* BACKUP ONE CCW Y02990         */ 00029000
