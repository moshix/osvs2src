*       GO TO RSTART;               /* SET RESTART ADDRESS           */ 00051000
         B     RSTART                                              0123 00052000
*     END;                                                         0124 00053000
*   IF WKREG=IOSRST                 /* ERROR ON RESTART? Y02990      */ 00054000
*     THEN                          /* YES - FLAGS ALREADY SET       */ 00055000
@RF00120 C     WKREG,IOSRST(,IOSBPTR)                              0125 00056000
         BE    @RT00125                                            0125 00057000
*     GO TO STATUS;                 /* CHECK STATUS BYTE             */ 00058000
