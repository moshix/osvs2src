         SL    CCWREG,@CF00183                                     0114 00032000
*   IF IOSCCHN='0'B                 /* TEST OF CMND CHAIN Y02990     */ 00033000
*     THEN                          /* NO - MUST BE DATA OR NONE     */ 00034000
         TM    IOSCCHN(IOSBPTR),B'01000000'                        0115 00035000
         BZ    @RT00115                                            0115 00036000
*     GO TO STATUS;                 /* CHECK STATUS BYTE             */ 00037000
*   RFY                                                            0117 00038000
*     WKREG RSTD;                   /* Y02990                        */ 00039000
*   WKREG=CCWREG;                   /* Y02990                        */ 00040000
