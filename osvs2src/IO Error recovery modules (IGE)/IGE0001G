*   GEN(LRA WKREG,0(WKREG));        /* POINT TO REAL ADDR Y02990     */ 00042000
         LRA WKREG,0(WKREG)                                             00043000
*   IF IOSENT='0'B                  /* PREVIOUS ENTRY? Y02990        */ 00044000
*     THEN                          /* NO -FIRST TIME THROUGH        */ 00045000
         TM    IOSENT(EWAPTR),B'00000100'                          0120 00046000
         BNZ   @RF00120                                            0120 00047000
*     DO;                                                          0121 00048000
*       IOSENT='1'B;                /* SET ENTRY BIT Y02990          */ 00049000
