*     IF PREV=YES THEN              /* IF BEEN HERE BEFORE, GIVE UP. */ 00391000
         TM    PREV(EWAPTR),B'10000000'                         02S0278 00392000
         BO    @RT00278                                         02S0278 00393000
*       GOTO LOGMSG;                                            02S0279 00394000
*     ELSE                                                      02S0280 00395000
*       DO;                                                     02S0280 00396000
*         PREV=YES;                 /* SET HERE NOW.                 */ 00397000
