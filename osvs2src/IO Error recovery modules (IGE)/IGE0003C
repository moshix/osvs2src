         MVC   MYCSW(8,APIOPTR),SAVEVCSW(EWAPTR)                01S0398 00784000
*       GO TO PROGERR;              /* EXIT STAGE LEFT.              */ 00785000
         B     PROGERR                                          01S0399 00786000
*     END;                                                      01S0400 00787000
*   ELSE                                                        01S0401 00788000
*     GOTO OK;                      /* IF NO, JUST EXIT BECAUSE ERROR   00789000
*                                      HAS CLEARED UP.               */ 00790000
