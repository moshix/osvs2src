*           PREV=YES;               /* INDICATE THAT IT TRIED ONCE.  */ 00379000
         OI    PREV(EWAPTR),B'10000000'                         02S0272 00380000
*           GO TO DOITOVER;         /* GO TRY AGAIN.                 */ 00381000
