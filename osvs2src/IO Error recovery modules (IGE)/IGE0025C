         BH    @RT00549                                            0549 01245000
*     GOTO TWOLIN;                  /* THEN WE NEED A 2 LINE MSG     */ 01246000
*   WKREG2=WKREG;                   /* INTO TWO REGS                 */ 01247000
         LR    WKREG2,WKREG                                        0551 01248000
*   GEN REFS(IOSEN,EWASNS);                                        0552 01249000
