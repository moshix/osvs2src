         TITLE 'IGE0301H:  VS2 FORMAT SENSE MODULE - LOAD 1            *00001000
                        '                                               00002000
IGE0301H CSECT                                                          00003000
@MAINENT DS   0H                                              @Z40LB02  00004000
         USING *,REGF                                         @Z40LB02  00005000
         B     @PROLOG                                        @Z40LB02  00006000
         DC    C'IGE0301H &SYSDATE'                           @Z40LB02  00007000
         DROP  REGF                                           @Z40LB02  00008000
         DS    0H                                             @Z40LB02  00009000
@PROLOG  BALR  BASEREG,0                                      @Z40LB02  00010000
@PSTART  DS    0H                                             @Z40LB02  00011000
         USING @DATD,REGC                                     @Z40LB02  00012000
         USING @PSTART,BASEREG                                @Z40LB02  00013000
*   CVTPTR=CVTLOC;                  /* SETUP CVT POINTER     @Z40LB02*/ 00014000
         L     CVTPTR,CVTLOC                                       0188 00015000
*   IOCOMPTR=CVTIXAVL;              /* SETUP IOCOM PTR       @Z40LB02*/ 00016000
         L     IOCOMPTR,CVTIXAVL(,CVTPTR)                          0189 00017000
*   EWAPTR=REGD;                    /* SAVE REG 13           @Z40LB02*/ 00018000
*                                                                  0190 00019000
         LR    EWAPTR,REGD                                         0190 00020000
*   /*****************************************************************/ 00021000
*   /*                                                               */ 00022000
*   /* GET LOCAL LOCK TO PROTECT FRR STACK FOR CORE MGR      @Z40LB02*/ 00023000
*   /* REGISTERS 11,12,13,14 ARE DESTROYED BY SETLOCK        @Z40LB02*/ 00024000
*   /* REG 13 MUST BE SAVE BEFORE SETLOCK IS ISSUED          @Z40LB02*/ 00025000
*   /* SO THAT IOS CORE MANAGER WILL HAVE A VALID            @Z40LB02*/ 00026000
*   /* SAVEAREA ADDRESS IN REG 13.                           @Z40LB02*/ 00027000
*   /*                                                               */ 00028000
*   /*****************************************************************/ 00029000
*                                                                  0191 00030000
*   GEN REFS(PSALITA);                                             0191 00031000
*                                                                  0191 00032000
*                                                         /*@Z40LB02*/  00033000
GETLOCK SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        **00034000
               RELATED=(IECVSMGR,IGE0301H(FREELOCK))                    00035000
*   /*****************************************************************/ 00036000
*   /*                                                               */ 00037000
*   /* SET UP FOR AND GO TO STORAGE MANGER TO GET 160 BYTE   @Z40LB02*/ 00038000
*   /* BLOCK IN WHICH TO BUILD MESSAGE                       @Z40LB02*/ 00039000
*   /* REG 13 MUST POINT TO IOS SAVE AREA AS SPECIFIED       @Z40LB02*/ 00040000
*   /* IN REG 13 UPON ENTRY TO THIS MODULE                   @Z40LB02*/ 00041000
*   /* REGB-1ST 4 BITS MAY BE 1000,2000,3000, OR 4000        @Z40LB02*/ 00042000
*   /* THIS NUMBER REPRESENT THE NUMBER OF BLOCKS            @Z40LB02*/ 00043000
*   /* DESIRED FROM THE CORE MANAGER. BITS 4 AND 5           @Z40LB02*/ 00044000
*   /* WILL CONTAIN THE ASID(FILLED IN BY THIS MODULE).      @Z40LB02*/ 00045000
*   /* BITS 6 AND 7 WILL ALWAYS CONTAIN AN INDES OF 20.      @Z40LB02*/ 00046000
*   /*                                                               */ 00047000
*   /*****************************************************************/ 00048000
*                                                                  0192 00049000
*   REGD=EWAPTR;                    /* RESTORE REG 13        @Z40LB02*/ 00050000
         LR    REGD,EWAPTR                                         0192 00051000
*   REGB='10000020'X;               /* R11=# BLKS & ASID     @Z40LB02*/ 00052000
         L     REGB,@CF02080                                       0193 00053000
*   REGF=IOCORMGT;                  /* ADDR OF IOS           @Z40LB02   00054000
*                                      STORAGE MANAGER       @Z40LB02*/ 00055000
         L     REGF,IOCORMGT(,IOCOMPTR)                            0194 00056000
*   GEN REFS(IOSASID);                                             0195 00057000
        ICM   REGB,6,IOSASID(IOSBPTR) GET ASID IN REG11                 00058000
        BALR  BALRREG,REGF            GO GET STORAGE                    00059000
*   FSWAPTR=REGB;                   /* SET UP POINTER TO     @Z40LB02   00060000
*                                      STORAGE AREA          @Z40LB02*/ 00061000
         LR    FSWAPTR,REGB                                        0196 00062000
*   EWAPTR=REGD;                    /* SAVE REG 13           @Z40LB02*/ 00063000
*                                                                  0197 00064000
         LR    EWAPTR,REGD                                         0197 00065000
*   /*****************************************************************/ 00066000
*   /*                                                               */ 00067000
*   /* FREE LOCAL LOCK JUST GOTTEN FOR STORAGE MANAGER SINCE @Z40LB02*/ 00068000
*   /* NO SVC'S CAN BE ISSUED WHILE IT IS HELD.              @Z40LB02*/ 00069000
*   /* REGISTER 13 MUST BE SAVED BEFORE RELEASING LOCAL      @Z40LB02*/ 00070000
*   /* LOCK                                                  @Z40LB02*/ 00071000
*   /*                                                               */ 00072000
*   /*****************************************************************/ 00073000
*                                                                  0198 00074000
*   GEN REFS(PSALITA);                                             0198 00075000
*                                                         /*@Z40LB02*/  00076000
FREELOCK SETLOCK RELEASE,TYPE=LOCAL,                                  **00077000
               RELATED=(IECVSMGR,IGE0301H(GETLOCK))                     00078000
         LA    REGC,DATASTOR(,FSWAPTR)   DATA AREA ADDR       @Z40LB02  00079000
*   REGD=EWAPTR;                    /* RESTORE REG 13        @Z40LB02*/ 00080000
         LR    REGD,EWAPTR                                         0199 00081000
*   RFY                                                            0200 00082000
*    (REGB,                                                        0200 00083000
*     BALRREG) UNRSTD;                                             0200 00084000
*   SAVER1=IOSBPTR;                                                0201 00085000
         ST    IOSBPTR,SAVER1(,FSWAPTR)                            0201 00086000
*   SAVR12=REGC;                    /* SAVE REG 12           @Z40LB02*/ 00087000
         ST    REGC,SAVR12(,FSWAPTR)                               0202 00088000
*   UCBPTR=IOSUCB;                  /* PRIMARY UCB           @Z40LB02*/ 00089000
         L     UCBPTR,IOSUCB(,IOSBPTR)                             0203 00090000
*   EWAPTR=IOSERP;                  /* EWA POINTER           @Z40LB02*/ 00091000
         L     EWAPTR,IOSERP(,IOSBPTR)                             0204 00092000
*   SNSPTR=ADDR(EWASNS);            /* ADDR OF SENSE DATA    @Z40LB02*/ 00093000
         LA    SNSPTR,EWASNS(,EWAPTR)                              0205 00094000
*   ICBPTR=CVTICB;                  /* SET UP POINTER TO     @Z40LB02   00095000
*                                      MSSC CONTL BLOCK      @Z40LB02*/ 00096000
         L     @14,CVTEXT2(,CVTPTR)                                0206 00097000
         L     @14,CVTICB(,@14)                                    0206 00098000
         LR    ICBPTR,@14                                          0206 00099000
*   SRBPTR=IOSSRB;                  /* SETUP SRB POINTER     @Z40LB02*/ 00100000
         MVC   SRBPTR(4,FSWAPTR),IOSSRB(IOSBPTR)                   0207 00101000
*   VVIPTR=ICBVVICB;                /* MSVC POINTER          @Z40LB02*/ 00102000
         MVC   VVIPTR(4,FSWAPTR),ICBVVICB(@14)                     0208 00103000
*   FSEXIT=OFF;                     /* INIT EXIT SWITCH      @Z40LB02*/ 00104000
         NI    FSEXIT(FSWAPTR),B'01111111'                         0209 00105000
*   MSGNUM=C0;                      /* INIT MSG INDEX        @Z40LB02*/ 00106000
         MVI   MSGNUM(FSWAPTR),X'00'                               0210 00107000
*   MSGBLT=OFF;                     /* INIT MSG SWITCH       @Z40LB02*/ 00108000
         NI    MSGBLT(FSWAPTR),B'10111111'                         0211 00109000
*   MSGNUM=C0;                      /* INIT MSG INDEX        @Z40LB02*/ 00110000
         MVI   MSGNUM(FSWAPTR),X'00'                               0212 00111000
*   CHAR2INP=MASKSSID&EWOSSID;      /* MASK FOR SSID VALUE   @Z40LB02*/ 00112000
         MVC   CHAR2INP(2,FSWAPTR),MASKSSID                        0213 00113000
         NC    CHAR2INP(2,FSWAPTR),EWOSSID(SNSPTR)                 0213 00114000
*   RFY                                                            0214 00115000
*    (EWAPTR,                                                      0214 00116000
*     ICBPTR) UNRSTD;                                              0214 00117000
*                                                                  0214 00118000
*   /*****************************************************************/ 00119000
*   /*                                                               */ 00120000
*   /* THE FOLLOWING CODE WILL CHECK THE SENSE DATA TO FIND  @Z40LB02*/ 00121000
*   /* WHICH FORMAT(0 OR 3) WAS SPECIFIED. BASED ON THE      @ZA09552*/ 00122000
*   /* FORMAT SPECIFICATION, OTHER SPECIFIC FLDS OF THE SNSE @Z40LB02*/ 00123000
*   /* DATA WILL BE CHECKED TO DETERMINE MESSAGE CODE,       @ZA09552*/ 00124000
*   /* SUU, TERMINATION CODE, ETC. AFTER DETERMINATION       @Z40LB02*/ 00125000
*   /* HAS BEEN MADE THE APPROPRIATE ROUTINE WILL BE CALLED  @Z40LB02*/ 00126000
*   /* TO PROCESS DATA FOR THE MESSAGE.                      @Z40LB02*/ 00127000
*   /* FORMAT 4 AND 5 ARE CHECKED FOR SUU ONLY.              @ZA09552*/ 00128000
*   /*                                                               */ 00129000
*   /*****************************************************************/ 00130000
*                                                                  0215 00131000
*                                                                  0215 00132000
*   /*****************************************************************/ 00133000
*   /*                                                               */ 00134000
*   /* CHECK FOR MSS UNIT UNUSABLE CONDITION-ICB503E MSG     @Z40LB02*/ 00135000
*   /*                                                               */ 00136000
*   /*****************************************************************/ 00137000
*                                                                  0215 00138000
*   IF(SUU=ON&FSEXIT=OFF) THEN      /* CHECK FOR SUU         @Z40LB02*/ 00139000
         TM    SUU(SNSPTR),B'01000000'                             0215 00140000
         BNO   @RF00215                                            0215 00141000
         TM    FSEXIT(FSWAPTR),B'10000000'                         0215 00142000
         BNZ   @RF00215                                            0215 00143000
*     IF(EWOMSGCD^=MSG09|EWOSNS12^=C8) THEN/*                @ZA29152*/ 00144000
         CLI   EWOMSGCD(SNSPTR),X'09'                              0216 00145000
         BNE   @RT00216                                            0216 00146000
         CLI   EWOSNS12(SNSPTR),8                                  0216 00147000
         BE    @RF00216                                            0216 00148000
@RT00216 DS    0H                                                  0217 00149000
*       DO;                                                        0217 00150000
*         MSGNUM=C4;                /* SET MESSAGE INDEX     @Z40LB02*/ 00151000
         MVI   MSGNUM(FSWAPTR),X'04'                               0218 00152000
*         FSEXIT=ON;                /* SET EXIT SWITCH       @Z40LB02*/ 00153000
         OI    FSEXIT(FSWAPTR),B'10000000'                         0219 00154000
*       END;                                                       0220 00155000
*                                                                  0220 00156000
*   /*****************************************************************/ 00157000
*   /*                                                               */ 00158000
*   /* CHECK FOR FORMAT 0 MSG CODE 07 MESSAGES               @Z40LB02*/ 00159000
*   /*                                                               */ 00160000
*   /*****************************************************************/ 00161000
*                                                                  0221 00162000
*   IF(FORMAT0=ON&FSEXIT=OFF) THEN  /* WAS IT FORMAT 0?      @Z40LB02*/ 00163000
@RF00216 DS    0H                                                  0221 00164000
@RF00215 TM    FORMAT0(SNSPTR),B'10000000'                         0221 00165000
         BNO   @RF00221                                            0221 00166000
         TM    FSEXIT(FSWAPTR),B'10000000'                         0221 00167000
         BNZ   @RF00221                                            0221 00168000
*     DO;                                                          0222 00169000
*       IF EWOMSGCD=MSG07 THEN      /* MESSAGE 07?           @Z40LB02*/ 00170000
         CLI   EWOMSGCD(SNSPTR),X'07'                              0223 00171000
         BNE   @RF00223                                            0223 00172000
*         DO;                                                      0224 00173000
*           IF EWOSNS12=TERMC02 THEN                               0225 00174000
*                                                                  0225 00175000
         CLI   EWOSNS12(SNSPTR),X'02'                              0225 00176000
         BNE   @RF00225                                            0225 00177000
*             /*******************************************************/ 00178000
*             /*                                                     */ 00179000
*             /* IF TERM CODE 02 THEN STRIPES AVAIL ZERO-ICB501E MSG */ 00180000
*             /*                                             @ZA16133*/ 00181000
*             /*                                                     */ 00182000
*             /*******************************************************/ 00183000
*                                                                  0226 00184000
*             DO;                                                  0226 00185000
*               FSEXIT=ON;          /* SET EXIT SWITCH       @Z40LB02*/ 00186000
         OI    FSEXIT(FSWAPTR),B'10000000'                         0227 00187000
*               MSGNUM=C2;          /* SET MESSAGE NUMBER    @Z40LB02*/ 00188000
         MVI   MSGNUM(FSWAPTR),X'02'                               0228 00189000
*             END;                                                 0229 00190000
*           ELSE                                                   0230 00191000
*             IF EWOSNS12=TERMC03 THEN                             0230 00192000
*                                                                  0230 00193000
         B     @RC00225                                            0230 00194000
@RF00225 CLI   EWOSNS12(SNSPTR),X'03'                              0230 00195000
         BNE   @RF00230                                            0230 00196000
*               /*****************************************************/ 00197000
*               /*                                                   */ 00198000
*               /* IF TERM CODE 03 THEN CART LOAD FAILURE-ICB502E MSG*/ 00199000
*               /*                                           @Z40LB02*/ 00200000
*               /*                                                   */ 00201000
*               /*****************************************************/ 00202000
*                                                                  0231 00203000
*               DO;                                                0231 00204000
*                 FSEXIT=ON;        /* SET EXIT SWITCH       @Z40LB02*/ 00205000
         OI    FSEXIT(FSWAPTR),B'10000000'                         0232 00206000
*                 MSGNUM=C3;        /* SET MESSAGE NUMBER    @Z40LB02*/ 00207000
         MVI   MSGNUM(FSWAPTR),X'03'                               0233 00208000
*               END;                                               0234 00209000
*             ELSE                                                 0235 00210000
*               IF EWOSNS12=TERMC01|EWOSNS12=TERMC04 THEN          0235 00211000
*                                                                  0235 00212000
         B     @RC00230                                            0235 00213000
@RF00230 CLI   EWOSNS12(SNSPTR),X'01'                              0235 00214000
         BE    @RT00235                                            0235 00215000
         CLI   EWOSNS12(SNSPTR),X'04'                              0235 00216000
