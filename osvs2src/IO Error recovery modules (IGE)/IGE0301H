*                     /***********************************************/ 00417000
*                                                                  0280 00418000
*                     DO;                                          0280 00419000
*                       FSEXIT=ON;  /* SET EXIT SWITCH       @Z40LB02*/ 00420000
         OI    FSEXIT(FSWAPTR),B'10000000'                         0281 00421000
*                       MSGNUM=C16; /* SET MESSAGE NUMBER    @ZA16133*/ 00422000
         MVI   MSGNUM(FSWAPTR),X'10'                               0282 00423000
*                     END;                                         0283 00424000
*                   ELSE                                           0284 00425000
*                     IF EWOSNS12=TERMC25 THEN                     0284 00426000
*                                                                  0284 00427000
         B     @RC00279                                            0284 00428000
@RF00279 CLI   EWOSNS12(SNSPTR),X'25'                              0284 00429000
         BNE   @RF00284                                            0284 00430000
*                       /*********************************************/ 00431000
*                       /*                                           */ 00432000
*                       /* IF TERM CODE 25 THEN UNTHREADABLE         */ 00433000
*                       /* CART-ICB517E MSG                  @ZA16133*/ 00434000
*                       /*                                           */ 00435000
*                       /*********************************************/ 00436000
*                                                                  0285 00437000
*                       DO;                                        0285 00438000
*                         FSEXIT=ON;/* SET EXIT SWITCH       @ZA16133*/ 00439000
