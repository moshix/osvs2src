*       END;                        /* WTOP MF(E,FMTWT01)    @Z40LB02*/ 02250000
*       IOSBPTR=SAVER1;                                            0743 02251000
         L     IOSBPTR,SAVER1(,FSWAPTR)                            0743 02252000
*       IF MSGNUM=C9 THEN           /* MESSAGE # FOUR?       @Z40LB02*/ 02253000
*                                                                  0744 02254000
         CLI   MSGNUM(FSWAPTR),9                                   0744 02255000
         BNE   @RF00744                                            0744 02256000
*         /***********************************************************/ 02257000
*         /*                                                         */ 02258000
*         /* IF MESSAGE INDEX WAS 9, THEN BUILD PART II PERMANENT    */ 02259000
*         /*                                                 @ZA16133*/ 02260000
*         /* WRITE ERROR(ICB509E).                           @ZA16133*/ 02261000
*         /*                                                         */ 02262000
*         /***********************************************************/ 02263000
*                                                                  0745 02264000
*         DO;                       /*                       @Z40LB02*/ 02265000
*           FMTWT03(38:79)='';      /* BLANKS ZA30102                */ 02266000
         MVI   FMTWT03+37(FSWAPTR),C' '                            0746 02267000
         MVC   FMTWT03+38(41,FSWAPTR),FMTWT03+37(FSWAPTR)          0746 02268000
*           MSF509=MSF;             /* STATIC MSF TXT        @Z40LB02*/ 02269000
         MVC   MSF509(4,FSWAPTR),MSF                               0747 02270000
*           MSFVAL9=MSFVAL;         /* MSF VALUE             @Z40LB02*/ 02271000
         MVC   MSFVAL9(1,FSWAPTR),MSFVAL(FSWAPTR)                  0748 02272000
*           XYZ509=XYZ;             /* XYZ STATIC TXT        @Z40LB02*/ 02273000
         MVC   XYZ509(6,FSWAPTR),XYZ                               0749 02274000
*           XVAL9A=XVAL;            /* X VALUE               @Z40LB02*/ 02275000
         MVC   XVAL9A(3,FSWAPTR),XVAL(FSWAPTR)                     0750 02276000
*           YVAL9A=YVAL;            /* LOAD Y VALUE          @Z40LB02*/ 02277000
         MVC   YVAL9A(2,FSWAPTR),YVAL(FSWAPTR)                     0751 02278000
*           ZVAL9A=ZVAL;            /* LOAD Z VALUE          @Z40LB02*/ 02279000
         MVC   ZVAL9A(1,FSWAPTR),ZVAL(FSWAPTR)                     0752 02280000
*           CANO=MS509A;            /* CASE STATIC TEXT      @ZA30102*/ 02281000
         MVC   CANO(9,FSWAPTR),MS509A                              0753 02282000
*           CANR=EWOSNS12|HIMASK;   /* PRINTABLE CASE NR     @ZA30102*/ 02283000
         MVC   CANR(1,FSWAPTR),EWOSNS12(SNSPTR)                    0754 02284000
         OI    CANR(FSWAPTR),X'F0'                                 0754 02285000
*           END509A=MSGEND;         /* TRAILING BLANK        @Z40LB02*/ 02286000
         MVC   END509A(1,FSWAPTR),MSGEND                           0755 02287000
*           DO;                     /* WTOP MF(E,FMTWT01)            */ 02288000
*             RFY                                                  0757 02289000
*               GPR01P RSTD;        /* RESTRICT REG 1                */ 02290000
*             GPR01P=ADDR(FMTWT01); /* ADDRESS CONTROL PROGRAM LIST  */ 02291000
         LA    GPR01P,FMTWT01(,FSWAPTR)                            0758 02292000
*             SVC(35);              /* ISSUE WTO SVC                 */ 02293000
         SVC   35                                                  0759 02294000
*             RFY                                                  0760 02295000
*               GPR01P UNRSTD;      /* RELEASE REG 1                 */ 02296000
*           END;                    /* WTOP MF(E,FMTWT01)    @Z40LB02*/ 02297000
*         END;                      /*                       @Z40LB02*/ 02298000
*       IOSBPTR=SAVER1;             /*                       @Z40LB02*/ 02299000
@RF00744 L     IOSBPTR,SAVER1(,FSWAPTR)                            0763 02300000
*       IOSMSG=OFF;                 /* TURN OFF MSG BIT      @Z40LB02*/ 02301000
         NI    IOSMSG(IOSBPTR),B'11111011'                         0764 02302000
*     END;                                                         0765 02303000
*   BALRREG=SAVE4678;               /* RESTORE REGISTER 14   @Z40LB02*/ 02304000
@RF00735 LR    BALRREG,SAVE4678                                    0766 02305000
*   RETURN;                                                        0767 02306000
@EL00004 DS    0H                                                  0767 02307000
@EF00004 DS    0H                                                  0767 02308000
@ER00004 BR    @14                                                 0767 02309000
*   END FSMS4678;                   /*                       @Z40LB02*/ 02310000
*                                                                  0769 02311000
*/********************************************************************/ 02312000
*/*  MODULE NAME - FSCLNUP                                           */ 02313000
*/*                                                                  */ 02314000
*/*  DESCRIPTIVE NAME - FORMAT SENSE CLEAN UP ROUTINE                */ 02315000
*/*                                                                  */ 02316000
*/*  FUNCTION - THIS SUBROUTINE WILL FREE THE 160 BYTE BLOCK OF      */ 02317000
*/*             STORAGE USED TO BUILD THE MESSAGE.  IN ORDER TO      */ 02318000
*/*             ACCOMPLISH THIS, THE LOCAL LOCK IS OBTAINED TO       */ 02319000
*/*             PROTECT THE FRR STACK.  THE LOCK IS RELEASED AFTER   */ 02320000
*/*             STORAGE AREA IS FREED.                               */ 02321000
*/********************************************************************/ 02322000
*                                                                  0769 02323000
*FSCLNUP:                                                          0769 02324000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0769 02325000
FSCLNUP  DS    0H                                                  0770 02326000
*   RFY                                                            0770 02327000
*    (REG6,                                                        0770 02328000
*     REG0,                                                        0770 02329000
*     REGF,                                                        0770 02330000
*     BALRREG,                                                     0770 02331000
*     REGB) RSTD;                                                  0770 02332000
*   SAVCLUP=BALRREG;                /* SAVE REGISTER 14      @Z40LB02*/ 02333000
         LR    SAVCLUP,BALRREG                                     0771 02334000
*   GEN REFS(PSALITA);                                             0772 02335000
GETLK1 SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                         **02336000
               RELATED=(IECVSMGR,IGE0301H(FREELK1))                     02337000
*   REGD=EWAPTR;                    /* RESTORE REG 13        @Z40LB02*/ 02338000
*                                                                  0773 02339000
         LR    REGD,EWAPTR                                         0773 02340000
*   /*****************************************************************/ 02341000
*   /*                                                               */ 02342000
*   /* NOW THAT THE LOCAL LOCK IS ENGAGED WE CAN FREE THE    @Z40LB02*/ 02343000
*   /* IOS STORAGE AREA USED TO BUILD THE MESSAGE.           @Z40LB02*/ 02344000
*   /* IT IS NECESSARY TO ZERO THE FIRST WORD OF THE 160     @ZA09517*/ 02345000
*   /* BYTE AREA BEING RELEASED.                             @ZA09517*/ 02346000
*   /* REG1=ADDR OF BLOCK TO BE FREED                        @Z40LB02*/ 02347000
*   /* REG11=00000020 TO INDICATE 160 BYTE BLOCK             @Z40LB02*/ 02348000
*   /* REG13=ADDR OF SAVE AREA                               @Z40LB02*/ 02349000
*   /*                                                               */ 02350000
*   /*****************************************************************/ 02351000
*                                                                  0774 02352000
*   FMTSNWA1(1:4)=FMTSNWA1(1:4)&&FMTSNWA1(1:4);/* ZERO WORD  @ZA09517*/ 02353000
         XC    FMTSNWA1(4,FSWAPTR),FMTSNWA1(FSWAPTR)               0774 02354000
*   REGB='00000020'X;                                              0775 02355000
         LA    REGB,32                                             0775 02356000
*   REG0=IOSBPTR;                   /* SAVE REG1, IOSB ADDR@Z40LB02  */ 02357000
         LR    REG0,IOSBPTR                                        0776 02358000
*   IOSBPTR=FSWAPTR;                /* REG1 HAS ADDR OF      @Z40LB02   02359000
*                                      STORAGE AREA          @Z40LB02*/ 02360000
         LR    IOSBPTR,FSWAPTR                                     0777 02361000
*   REGF=IOCORMGT;                  /* ADDR OF STORAGE MGR   @Z40LB02*/ 02362000
         L     REGF,IOCORMGT(,IOCOMPTR)                            0778 02363000
*   GEN(BAL  BALRREG,4(REGF));      /* FREE STORAGE          @Z40LB02*/ 02364000
         BAL  BALRREG,4(REGF)                                           02365000
*   IOSBPTR=REG0;                   /* RESTORE REG 1         @Z40LB02*/ 02366000
         LR    IOSBPTR,REG0                                        0780 02367000
*   EWAPTR=REGD;                    /* RESTORE REG 13        @Z40LB02*/ 02368000
*                                                                  0781 02369000
         LR    EWAPTR,REGD                                         0781 02370000
*   /*****************************************************************/ 02371000
*   /*                                                               */ 02372000
*   /* FREE LOCAL LOCK GOTTEN FOR STORAGE MANAGER            @Z40LB02*/ 02373000
*   /*                                                               */ 02374000
*   /*****************************************************************/ 02375000
*                                                                  0782 02376000
*   GEN REFS(PSALITA);                                             0782 02377000
FREELK1 SETLOCK RELEASE,TYPE=LOCAL,                                   **02378000
               RELATED=(IECVSMGR,IGE0301H(GETLK1))                      02379000
*   REGC=SAVR12;                                                   0783 02380000
         L     REGC,SAVR12(,FSWAPTR)                               0783 02381000
*   BALRREG=SAVCLUP;                /* RESTORE REGISTER 14   @Z40LB02*/ 02382000
         LR    BALRREG,SAVCLUP                                     0784 02383000
*   RETURN;                                                        0785 02384000
@EL00005 DS    0H                                                  0785 02385000
@EF00005 DS    0H                                                  0785 02386000
@ER00005 BR    @14                                                 0785 02387000
*   END FSCLNUP;                    /*                       @Z40LB02*/ 02388000
@PB00005 DS    0H                                                  0786 02389000
*   END IGE0301H                                                   0787 02390000
*                                                                  0787 02391000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02392000
*/*%INCLUDE SYSLIB  (IEZSENSE)                                       */ 02393000
*/*%INCLUDE SYSLIB  (IEZFMSWA)                                       */ 02394000
