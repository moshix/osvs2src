*       SVC(15);                    /* POST ERROR                    */ 00422000
         SVC   15                                                  0330 00423000
*     END;                                                         0331 00424000
*   DO;                             /* ?IGFDERR CANCEL               */ 00425000
*                                                                  0332 00426000
@RF00325 DS    0H                                                  0333 00427000
*     /***************************************************************/ 00428000
*     /*                                                             */ 00429000
*     /* IGFDERR - CANCEL RECOVERY ENVIRONMENT                       */ 00430000
*     /*                                                             */ 00431000
*     /***************************************************************/ 00432000
*                                                                  0333 00433000
*     DERFUNK=DERCANC;              /* INDICATE CANCEL               */ 00434000
