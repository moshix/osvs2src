*   DECLARE                                                        0340 00472000
*     ILIST CHAR(10) BDY(DWORD);    /* STANDARD LIST                 */ 00473000
*   DECLARE                         /* COMMON VARIABLES              */ 00474000
*     I256C CHAR(256) BASED,                                       0341 00475000
