*   /*****************************************************************/ 00406000
*   /*                                                               */ 00407000
*   /* IF DDR WAS NOT SCHEDULED, POST A PERMANENT I/O ERROR          */ 00408000
*   /*                                                               */ 00409000
*   /*****************************************************************/ 00410000
*                                                                  0325 00411000
*   IF DERLRC^=VALID THEN                                          0325 00412000
         CLI   DERLRC,0                                            0325 00413000
         BE    @RF00325                                            0325 00414000
*     DO;                           /* POST PERMANENT ERROR          */ 00415000
*       IOSERR='0'B;                /* RESET IOSERR FLAG             */ 00416000
*       IOSEX='1'B;                 /* SET IOSEX FLAG                */ 00417000
         OI    IOSEX(IOSBPTR),B'00000100'                          0328 00418000
