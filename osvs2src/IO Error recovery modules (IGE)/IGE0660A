         ST    @12,@AL00001                                        0334 00438000
         L     @15,@CV00085                                        0334 00439000
         LA    @01,@AL00001                                        0334 00440000
         BALR  @14,@15                                             0334 00441000
*   END;                            /* ?IGFDERR CANCEL               */ 00442000
*                                                                  0335 00443000
*   /*****************************************************************/ 00444000
*   /*                                                               */ 00445000
*   /* THE AUTOMATIC DATA AREA MUST NOW BE FREEMAINED                */ 00446000
*   /*                                                               */ 00447000
*   /*****************************************************************/ 00448000
*                                                                  0336 00449000
*   GENERATE;                                                      0336 00450000
   L     @00,@SIZDATD              PICK UP SPN/LENGTH                   00451000
   LR    @01,DATABASE              POINT TO AREA TO BE FREED            00452000
   FREEMAIN R,LV=(0),A=(1)         FREE WORK AREA                       00453000
*   SVC(3);                         /* EXIT FROM MODULE              */ 00454000
         SVC   3                                                   0337 00455000
*                                                                  0338 00456000
*   /*****************************************************************/ 00457000
*   /*                                                               */ 00458000
*   /* ?EPILOG - GENERATE DECLARES FOR SYSTEM MACROS                 */ 00459000
*   /*                                                               */ 00460000
*   /*****************************************************************/ 00461000
*                                                                  0338 00462000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00463000
*     GPR01P PTR(31) REG(1);                                       0338 00464000
*   DCL                                                            0339 00465000
*     1 I004 DEF(ILIST) BDY(WORD),  /* LIST FOR GETMAIN              */ 00466000
*      2 I00401 PTR(31),            /* LENGTH                        */ 00467000
