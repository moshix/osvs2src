         TITLE 'IGE0660A, DDR - ERP INTERFACE MODULE                   *00001000
                        '                                               00002000
IGE0660A CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGE0660A  77.322'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @08,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@08                                         0001 00012000
*   IOSBPTR=RIOSBIN;                /* COPY ADDRESS OF IOSB TO A SAFE   00013000
*                                      REGISTER                      */ 00014000
*                                                                  0234 00015000
         LR    IOSBPTR,RIOSBIN                                     0234 00016000
*   /*****************************************************************/ 00017000
*   /*                                                               */ 00018000
*   /* THE NORMAL PROLOG COULD NOT BE USED TO GETMAIN THE AUTOMATIC  */ 00019000
*   /* AREA SINCE GETMAIN DESTROYS REGISTER 1 (WHICH CONTAINS THE    */ 00020000
*   /* ADDRESS OF THE IOSB) AND PL/S CANNOT SAVE AND RESTORE REG 1   */ 00021000
*   /* ACROSS THE GETMAIN BECAUSE THERE IS NO USEABLE SAVE AREA ON   */ 00022000
*   /* ENTRY TO THIS MODULE - THEREFORE, THE SPACE FOR THE AUTOMATIC */ 00023000
*   /* AREA IS GETMAINED HERE                                        */ 00024000
*   /*                                                               */ 00025000
*   /*****************************************************************/ 00026000
*                                                                  0235 00027000
*   GENERATE;                                                      0235 00028000
   L     @00,@SIZDATD              PICK UP LENGTH OF AUTO AREA          00029000
   GETMAIN R,LV=(0)                GETMAIN SPACE FOR AUTOMATIC DATA     00030000
   LR    DATABASE,@01              PUT ADDRESS OF AREA INTO DATA REG    00031000
   USING @DATD,DATABASE            ESTABLISH ADDRESSABILITY TO          00032000
*                                     AUTOMATIC DATA AREA               00033000
   LR    SAVEPTR,DATABASE          POINT TO REGISTER SAVE AREA          00034000
*   EWAPTR=IOSERP;                  /* PICK UP ADDRESS OF ERWA       */ 00035000
         L     EWAPTR,IOSERP(,IOSBPTR)                             0236 00036000
*   DDRPTR=ADDR(EWDASNS)+LENGTH(EWDASNS);/* THE WORKING DDRCOM WILL     00037000
*                                      BE WITHIN THE ERWA IMMEDIATELY   00038000
*                                      FOLLOWING THE EWDASNS FIELD   */ 00039000
         LA    DDRPTR,EWDASNS(,EWAPTR)                             0237 00040000
         AL    DDRPTR,@CF01565                                     0237 00041000
*   DDRCLR=DDRCLR&&DDRCLR;          /* CLEAR THE WORKING DDRCOM    0238 00042000
*                                                            @ZA18592*/ 00043000
         XC    DDRCLR(100,DDRPTR),DDRCLR(DDRPTR)                   0238 00044000
*   DDRUIOSB=IOSBPTR;               /* PUT THE IOSB ADDRESS INTO THE    00045000
*                                      DDRCOM - THIS IS NEEDED IN  0239 00046000
*                                      CASE THE ESTAE EXIT IS ENTERED*/ 00047000
         ST    IOSBPTR,DDRUIOSB(,DDRPTR)                           0239 00048000
*   DO;                             /* ?IGFDERR INIT                 */ 00049000
*                                                                  0240 00050000
*     /***************************************************************/ 00051000
*     /*                                                             */ 00052000
*     /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT                   */ 00053000
*     /*                                                             */ 00054000
*     /***************************************************************/ 00055000
*                                                                  0241 00056000
*     DERPLIST=DERPLIST&&DERPLIST;  /* CLEAR PARAMETER LIST          */ 00057000
         XC    DERPLIST(48),DERPLIST                               0241 00058000
*     DERFUNK=DERINIT;              /* INDICATE INIT FUNCTION        */ 00059000
         MVI   DERFUNK,X'01'                                       0242 00060000
*     DERREC=ADDR(DERRECPM);        /* POINT TO REC PARMS            */ 00061000
         LA    @12,DERRECPM                                        0243 00062000
         ST    @12,DERREC                                          0243 00063000
*     DERRETRY=ADDR(IGE0660R);      /* POINT TO RETRY ROUTINE        */ 00064000
         LA    @12,IGE0660R                                        0244 00065000
         ST    @12,DERRETRY                                        0244 00066000
*     GEN SETS(DERDATD)(MVC DERDATD,@SIZDATD);/* SET UP SPN, SIZE    */ 00067000
         MVC DERDATD,@SIZDATD                                           00068000
*     DERPFX=DERRMASK;              /* SET UP RUB MASK               */ 00069000
         MVC   DERPFX(2),@CB01278                                  0246 00070000
*     DERRCODE=CODEBASE;            /* SAVE CODEREG                  */ 00071000
         ST    CODEBASE,DERRCODE                                   0247 00072000
*     DERRDATA=DATABASE;            /* SAVE DATAREG                  */ 00073000
         ST    DATABASE,DERRDATA                                   0248 00074000
*     DERRDDR=DDRPTR;               /* SAVE DDRCOM POINTER           */ 00075000
         ST    DDRPTR,DERRDDR                                      0249 00076000
*     DERRSAVE=SAVEPTR;             /* SAVE REGISTER 13              */ 00077000
         ST    SAVEPTR,DERRSAVE                                    0250 00078000
*     CALL IGFDE0(DERPLIST);        /* ISSUE ESTAE                   */ 00079000
         LA    @12,DERPLIST                                        0251 00080000
         ST    @12,@AL00001                                        0251 00081000
         L     @15,@CV00085                                        0251 00082000
         LA    @01,@AL00001                                        0251 00083000
         BALR  @14,@15                                             0251 00084000
*   END;                            /* ?IGFDERR INIT                 */ 00085000
*   DERLRC=PERMERR;                 /* INDICATE PERMANENT I/O ERROR     00086000
*                                      IS TO BE POSTED               */ 00087000
         MVI   DERLRC,X'04'                                        0253 00088000
*   UCBPTR=IOSUCB;                  /* POINT TO UCB                  */ 00089000
*                                                                  0254 00090000
         L     UCBPTR,IOSUCB(,IOSBPTR)                             0254 00091000
*   /*****************************************************************/ 00092000
*   /*                                                               */ 00093000
*   /* SEE IF IT IS POSSIBLE TO USE DDR TO CORRECT THE I/O ERROR     */ 00094000
*   /*                                                               */ 00095000
*   /*****************************************************************/ 00096000
*                                                                  0255 00097000
*   IF CVTNIP='0'B                  /* THE SYSTEM MUST BE INITIALIZED*/ 00098000
*       &CVTDDR='1'B                /* SYSTEM SWAPS MUST BE ALLOWED  */ 00099000
*       &UCBQISCE='0'B              /* THE DEVICE CANNOT ALREADY BE     00100000
*                                      INVOLVED IN A SWAP            */ 00101000
*       &IOSQISCE='0'B              /* THE I/O ERROR CANNOT BE FROM     00102000
*                                      DDR                           */ 00103000
*       &UCBDDRSW='0'B              /* THE DEVICE CANNOT ALREADY BE     00104000
*                                      INVOLVED IN A SWAP            */ 00105000
*       &IOSDVRID^=IOSOLTID         /* OLTEP CANNOT OWN THE DEVICE   */ 00106000
*       &(IOSCOD=IOSNRMC|IOSCOD=IOSMIHC)/* THE COMPLETION CODE MUST     00107000
*                                      BE A PERMANENT I/O ERROR OR A    00108000
*                                      SIMULATED PERM I/O ERROR      */ 00109000
*       &(IOSDVRID^=IOSXCPID|TCBTID^='FC'X)/* DDR CANNOT BE        0255 00110000
*                                      PROCESSING A REQUEST FOR THE     00111000
*                                      SAME DEVICE ALREADY (EXCP   0255 00112000
*                                      DRIVER AND THE DDR TASKS TCB     00113000
*                                      INDICATES I/O REQUEST FROM  0255 00114000
*                                      DDR)                          */ 00115000
*       &(UCB3DACC='1'B|(UCB3TAPE='1'B&IOSDVRID=IOSXCPID))/* THE   0255 00116000
*                                      DEVICE MUST BE DIRECT ACCESS     00117000
*                                      OR A TAPE BEING USED WITH EXCP*/ 00118000
*     THEN                          /* IF ALL THE ABOVE CONDITIONS 0255 00119000
*                                      ARE SATISFIED, DDR MAY BE USED*/ 00120000
         L     @12,CVTPTR                                          0255 00121000
         TM    CVTDDR(@12),B'00100000'                             0255 00122000
         BNO   @RF00255                                            0255 00123000
         TM    CVTNIP(@12),B'00010000'                             0255 00124000
         BNZ   @RF00255                                            0255 00125000
         TM    UCBQISCE(UCBPTR),B'00000001'                        0255 00126000
         BNZ   @RF00255                                            0255 00127000
         TM    IOSQISCE(IOSBPTR),B'00100000'                       0255 00128000
         BNZ   @RF00255                                            0255 00129000
         TM    UCBDDRSW(UCBPTR),B'00000001'                        0255 00130000
         BNZ   @RF00255                                            0255 00131000
         CLI   IOSDVRID(IOSBPTR),X'06'                             0255 00132000
         BE    @RF00255                                            0255 00133000
         CLI   IOSCOD(IOSBPTR),X'7F'                               0255 00134000
         BE    @GL00002                                            0255 00135000
         CLI   IOSCOD(IOSBPTR),X'74'                               0255 00136000
         BNE   @RF00255                                            0255 00137000
@GL00002 CLI   IOSDVRID(IOSBPTR),X'02'                             0255 00138000
         BNE   @GL00001                                            0255 00139000
         L     @12,RQEPTR(,IOSBPTR)                                0255 00140000
         L     @12,TCBPTR(,@12)                                    0255 00141000
         CLI   TCBTID(@12),X'FC'                                   0255 00142000
         BE    @RF00255                                            0255 00143000
@GL00001 TM    UCB3DACC(UCBPTR),B'00100000'                        0255 00144000
         BO    @RT00255                                            0255 00145000
         TM    UCB3TAPE(UCBPTR),B'10000000'                        0255 00146000
         BNO   @RF00255                                            0255 00147000
         CLI   IOSDVRID(IOSBPTR),X'02'                             0255 00148000
         BNE   @RF00255                                            0255 00149000
@RT00255 DS    0H                                                  0256 00150000
*     DO;                           /* ATTEMPT TO ACTIVATE DDR       */ 00151000
*                                                                  0256 00152000
*       /*************************************************************/ 00153000
*       /*                                                           */ 00154000
*       /* BUILD THE DDRCOM IN THE ERWA                              */ 00155000
*       /*                                                           */ 00156000
*       /*************************************************************/ 00157000
*                                                                  0257 00158000
*       DDRID='DDR ';               /* FILL IN THE ID FIELD          */ 00159000
         MVC   DDRID(4,DDRPTR),@CC03006                            0257 00160000
*       DDRSYS='1'B;                /* INDICATE SYSTEM SWAP          */ 00161000
         OI    DDRSYS(DDRPTR),B'01000000'                          0258 00162000
*       DDRSIRB='1'B;               /* INDICATE DDR RUNNING UNDER  0259 00163000
*                                      SIRB                          */ 00164000
         OI    DDRSIRB(DDRPTR),B'00000100'                         0259 00165000
*       DDRUASID=IOSASID;           /* PICK UP USERS ADDRESS SPACE ID*/ 00166000
         LH    @12,IOSASID(,IOSBPTR)                               0260 00167000
         N     @12,@CF03037                                        0260 00168000
         STH   @12,DDRUASID(,DDRPTR)                               0260 00169000
*       IF UCB3TAPE='1'B THEN       /* INDICATE TAPE IF APPROPRIATE  */ 00170000
         TM    UCB3TAPE(UCBPTR),B'10000000'                        0261 00171000
         BNO   @RF00261                                            0261 00172000
*         DDRMT='1'B;                                              0262 00173000
         OI    DDRMT(DDRPTR),B'00100000'                           0262 00174000
*       ELSE                        /* IF NOT TAPE, MUST BE DISK     */ 00175000
*         DDRDA='1'B;                                              0263 00176000
         B     @RC00261                                            0263 00177000
@RF00261 OI    DDRDA(DDRPTR),B'01000000'                           0263 00178000
*       DDRFMCUA=UCBNAME;           /* COPY DEVICE ADDRESS TO DDRCOM */ 00179000
@RC00261 MVC   DDRFMCUA(3,DDRPTR),UCBNAME(UCBPTR)                  0264 00180000
*       DDRFMUCB=IOSUCB;            /* FILL IN UCB ADDRESS           */ 00181000
         L     @12,IOSUCB(,IOSBPTR)                                0265 00182000
         ST    @12,DDRFMUCB(,DDRPTR)                               0265 00183000
*       IF IOSPGDPX='1'B THEN       /* SET PAGING I/O ERROR FLAG IF     00184000
*                                      APPROPRIATE                   */ 00185000
         TM    IOSPGDPX(IOSBPTR),B'00000010'                       0266 00186000
         BNO   @RF00266                                            0266 00187000
*         DDRPAGE='1'B;                                            0267 00188000
         OI    DDRPAGE(DDRPTR),B'00100000'                         0267 00189000
*       DDRASID=MASTERID;           /* DDR WILL RUN IN MASTER ADDRESS   00190000
*                                      SPACE ID=1            @YM04069*/ 00191000
@RF00266 MVC   DDRASID(2,DDRPTR),@CH00041                          0268 00192000
*       DDRUASCB=IOSSRB->SRBASCB;   /* PICK UP THE ADDRESS OF THE  0269 00193000
*                                      USERS ASCB                    */ 00194000
         L     @12,IOSSRB(,IOSBPTR)                                0269 00195000
         L     @12,SRBASCB(,@12)                                   0269 00196000
         ST    @12,DDRUASCB(,DDRPTR)                               0269 00197000
*       IF IOSDVRID=IOSXCPID THEN   /* FOR EXCP, AN RQE EXISTS       */ 00198000
         CLI   IOSDVRID(IOSBPTR),X'02'                             0270 00199000
         BNE   @RF00270                                            0270 00200000
*         DO;                       /* EXCP - FILL IN OTHER FIELDS   */ 00201000
*           DDRUTCB=RQETCB;         /* COPY USERS TCB ADDRESS        */ 00202000
         L     @12,RQEPTR(,IOSBPTR)                                0272 00203000
         L     @11,RQETCB(,@12)                                    0272 00204000
         ST    @11,DDRUTCB(,DDRPTR)                                0272 00205000
*           DDRUIOB=RQEIOB;         /* COPY USERS IOB ADDRESS        */ 00206000
         L     @11,RQEIOB(,@12)                                    0273 00207000
         ST    @11,DDRUIOB(,DDRPTR)                                0273 00208000
*           DDRUDEB=RQEDEB;         /* COPY USERS DEB ADDRESS        */ 00209000
         L     @12,RQEDEB(,@12)                                    0274 00210000
         ST    @12,DDRUDEB(,DDRPTR)                                0274 00211000
*           DDRUDCB=DEBDCBB;        /* COPY USERS DCB ADDRESS        */ 00212000
         L     @12,DEBPTR(,DDRPTR)                                 0275 00213000
         L     @11,DEBDCBB-1(,@12)                                 0275 00214000
         LA    @11,0(,@11)                                         0275 00215000
         ST    @11,DDRUDCB(,DDRPTR)                                0275 00216000
*           DDRUMODE=DEBSDVM;       /* USER MODE SET         @YM04069*/ 00217000
         MVC   DDRUMODE(1,DDRPTR),DEBSDVM(@12)                     0276 00218000
*           DDRUBCNT=DCBBLKCT;      /* USER BLOCK COUNT      @YM04069*/ 00219000
         L     @12,DCBPTR(,DDRPTR)                                 0277 00220000
         L     @12,DCBBLKCT(,@12)                                  0277 00221000
         ST    @12,DDRUBCNT(,DDRPTR)                               0277 00222000
*         END;                                                     0278 00223000
*                                                                  0278 00224000
*       /*************************************************************/ 00225000
*       /*                                                           */ 00226000
*       /* CHECK DEVICE STATUS AND SENSE TO DETERMINE WHETHER OR NOT */ 00227000
*       /* DDR CAN BE USED TO CORRECT THE ERROR - IF SO, VALIDATE THE*/ 00228000
*       /* 'FROM' DEVICE                                             */ 00229000
*       /*                                                           */ 00230000
*       /*************************************************************/ 00231000
*                                                                  0279 00232000
*       IF DDRMT='1'B               /* IS THIS A TAPE DEVICE         */ 00233000
*         THEN                      /* YES, USE TAPE TESTS           */ 00234000
@RF00270 TM    DDRMT(DDRPTR),B'00100000'                           0279 00235000
         BNO   @RF00279                                            0279 00236000
*         IF CSTAT^='000'B          /* THE ERROR MUST BE CAUSED BY 0280 00237000
*                                      THE CHANNEL                   */ 00238000
*             |(UCHK='1'B           /* OR ONE OF THE FOLLOWING       */ 00239000
*             &(EWDBUSO='1'B        /* BUS OUT PARITY CHECK          */ 00240000
*             |EWDEQU='1'B          /* EQUIPMENT CHECK               */ 00241000
*             |EWDDATA='1'B         /* DATA CHECK                    */ 00242000
*             |EWDOVRN='1'B)) THEN  /* OVERRUN                       */ 00243000
         TM    CSTAT(IOSBPTR),B'00001110'                          0280 00244000
         BNZ   @RT00280                                            0280 00245000
         TM    UCHK(IOSBPTR),B'00000010'                           0280 00246000
         BNO   @RF00280                                            0280 00247000
         TM    EWDBUSO(EWAPTR),B'00111100'                         0280 00248000
         BZ    @RF00280                                            0280 00249000
@RT00280 DS    0H                                                  0281 00250000
*           DO;                     /* IF SO, DDR MAY BE ABLE TO HELP*/ 00251000
*             DDRINV2=DDRVFM;       /* INDICATE 'FROM' VALIDATION    */ 00252000
         MVI   DDRINV2(DDRPTR),X'01'                               0282 00253000
*             CALL IGFDT1;          /* VALIDATE THE 'FROM' DEVICE    */ 00254000
         L     @15,@CV00087                                        0283 00255000
         BALR  @14,@15                                             0283 00256000
*             DERLRC=RTNCODE;       /* COPY RETURN CODE              */ 00257000
         STC   RTNCODE,DERLRC                                      0284 00258000
*           END;                                                   0285 00259000
*         ELSE                                                     0286 00260000
*           ;                       /* UNSUPPORTABLE TAPE I/O ERROR  */ 00261000
@RF00280 DS    0H                                                  0287 00262000
*       ELSE                        /* NO, USE DISK TESTS            */ 00263000
*         IF CSTAT^='000'B          /* THE ERROR MUST BE CAUSED BY 0287 00264000
*                                      THE CHANNEL                   */ 00265000
*             |(UCHK='1'B           /* OR ONE OF THE FOLLOWING       */ 00266000
*             &((EWDBUSO='1'B       /* BUS OUT PARITY CHECK          */ 00267000
*             |EWDEQU='1'B          /* EQUIPMENT CHECK               */ 00268000
*             |EWDDATA='1'B         /* DATA CHECK                    */ 00269000
*             |EWDOVRN='1'B         /* OVERRUN                       */ 00270000
*             |EWDSKCK='1'B)        /* SEEK CHECK                    */ 00271000
*             &EWDCREJ='0'B)) THEN  /* BUT NOT COMMAND REJECT        */ 00272000
         B     @RC00279                                            0287 00273000
@RF00279 TM    CSTAT(IOSBPTR),B'00001110'                          0287 00274000
         BNZ   @RT00287                                            0287 00275000
         TM    UCHK(IOSBPTR),B'00000010'                           0287 00276000
         BNO   @RF00287                                            0287 00277000
         TM    EWDBUSO(EWAPTR),B'00111101'                         0287 00278000
         BZ    @RF00287                                            0287 00279000
         TM    EWDCREJ(EWAPTR),B'10000000'                         0287 00280000
         BNZ   @RF00287                                            0287 00281000
@RT00287 DS    0H                                                  0288 00282000
*           DO;                     /* IF SO, DDR MAY BE ABLE TO HELP*/ 00283000
*             DDRINV2=DDRVFM;       /* INDICATE 'FROM' VALIDATION    */ 00284000
         MVI   DDRINV2(DDRPTR),X'01'                               0289 00285000
*             CALL IGFDD1;          /* VALIDATE THE 'FROM' DEVICE    */ 00286000
         L     @15,@CV00084                                        0290 00287000
         BALR  @14,@15                                             0290 00288000
*             DERLRC=RTNCODE;       /* COPY RETURN CODE              */ 00289000
         STC   RTNCODE,DERLRC                                      0291 00290000
*           END;                                                   0292 00291000
*       IF DERLRC=VALID THEN        /* IS DEVICE VALID FOR SWAP      */ 00292000
@RF00287 DS    0H                                                  0293 00293000
@RC00279 CLI   DERLRC,0                                            0293 00294000
         BNE   @RF00293                                            0293 00295000
*         DO;                       /* YES, SCHEDULE DDR             */ 00296000
*                                                                  0294 00297000
*           /*********************************************************/ 00298000
*           /*                                                       */ 00299000
*           /* GETMAIN SPACE FOR A SHORT DDRCOM AND AN SRB           */ 00300000
*           /*                                                       */ 00301000
*           /*********************************************************/ 00302000
*                                                                  0295 00303000
*           DO;                     /* GETMAIN (EU) LV(LENGTH(DDRSHOR   00304000
*                                      T)+LENGTH(SRBSECT))         0295 00305000
*                                      A(DERGMADR) SP(SPN)           */ 00306000
*             RESPECIFY                                            0296 00307000
*              (GPR01P) RESTRICTED;                                0296 00308000
*             I004=I004&&I004;      /* INIT LIST                     */ 00309000
         XC    I004(10),I004                                       0297 00310000
*             I00404='00'X;         /* TYPE                          */ 00311000
         MVI   I00404,X'00'                                        0298 00312000
*             I00401=LENGTH(DDRSHORT)+LENGTH(SRBSECT);/* LENGTH      */ 00313000
         MVC   I00401(4),@CF03027                                  0299 00314000
*             I00403=ADDR(DERGMADR);/* ADDR OF ADDR LIST             */ 00315000
         LA    @12,DERGMADR                                        0300 00316000
         STCM  @12,7,I00403                                        0300 00317000
*             I00405=SPN;           /* SUBPOOL VALUE                 */ 00318000
         MVI   I00405,X'F5'                                        0301 00319000
*             GPR01P=ADDR(I004);    /* REG1 POINTS TO LIST           */ 00320000
         LA    GPR01P,I004                                         0302 00321000
*             SVC(4);               /* STANDARD-FORM OF GETMAIN      */ 00322000
         SVC   4                                                   0303 00323000
*             RESPECIFY                                            0304 00324000
*              (GPR01P) UNRESTRICTED;                              0304 00325000
*           END;                    /* GETMAIN (EU) LV(LENGTH(DDRSHOR   00326000
*                                      T)+LENGTH(SRBSECT))         0305 00327000
*                                      A(DERGMADR) SP(SPN)           */ 00328000
*           DERGMLNG=LENGTH(DDRSHORT)+LENGTH(SRBSECT);/* INDICATE  0306 00329000
*                                      SIZE OF AREA GETMAINED        */ 00330000
         MVC   DERGMLNG(3),@CF03027+1                              0306 00331000
*           DERGMSPN=SPN;           /* INDICATE SUBPOOL NUMBER       */ 00332000
         MVI   DERGMSPN,X'F5'                                      0307 00333000
*           SDDRPTR=DERGMADR;       /* PICK UP ADDR OF SHORT DDRCOM  */ 00334000
*                                                                  0308 00335000
         L     SDDRPTR,DERGMADR                                    0308 00336000
*           /*********************************************************/ 00337000
*           /*                                                       */ 00338000
*           /* BUILD THE SRB REQUIRED TO SCHEDULE DDR                */ 00339000
*           /*                                                       */ 00340000
*           /*********************************************************/ 00341000
*                                                                  0309 00342000
*           SRBPTR=SDDRPTR+LENGTH(DDRSHORT);/* BUILD THE SRB RIGHT 0309 00343000
*                                      PAST THE SHORT DDRCOM         */ 00344000
         LA    SRBPTR,24                                           0309 00345000
         ALR   SRBPTR,SDDRPTR                                      0309 00346000
*           SRBSECT=SRBSECT&&SRBSECT;/* CLEAR THE SRB                */ 00347000
         XC    SRBSECT(44,SRBPTR),SRBSECT(SRBPTR)                  0310 00348000
*           SRBID='SRB ';           /* FILL IN THE ID FIELD          */ 00349000
         MVC   SRBID(4,SRBPTR),@CC03014                            0311 00350000
*           SRBASCB=ASVTENTY(DDRASID);/* FILL IN THE ADDRESS OF THE     00351000
*                                      ASCB UNDER WHICH DDR IS TO RUN*/ 00352000
         LH    @12,DDRASID(,DDRPTR)                                0312 00353000
         SLA   @12,2                                               0312 00354000
         L     @11,CVTPTR                                          0312 00355000
         L     @11,ASVTPTR(,@11)                                   0312 00356000
         L     @12,ASVTENTY-4(@12,@11)                             0312 00357000
         ST    @12,SRBASCB(,SRBPTR)                                0312 00358000
*           SRBPARM=SDDRPTR;        /* INDICATE THE SHORT DDRCOM IS A   00359000
*                                      PARAMETER TO DDR              */ 00360000
         ST    SDDRPTR,SRBPARM(,SRBPTR)                            0313 00361000
*           SRBEP=ADDR(IGFDS0);     /* POINT TO DDR SRB ROUTINE      */ 00362000
*                                                                  0314 00363000
         L     @12,@CV00086                                        0314 00364000
         ST    @12,SRBEP(,SRBPTR)                                  0314 00365000
*           /*********************************************************/ 00366000
*           /*                                                       */ 00367000
*           /* SET UP THE SHORT DDRCOM                               */ 00368000
*           /*                                                       */ 00369000
*           /*********************************************************/ 00370000
*                                                                  0315 00371000
*           SDDRPTR->DDRSHORT=DDRSHORT;/* COPY THE WORKING DDRCOM  0315 00372000
*                                      INTO THE SHORT DDRCOM         */ 00373000
         MVC   DDRSHORT(24,SDDRPTR),DDRSHORT(DDRPTR)               0315 00374000
*           SDDRPTR->DDRID='DDRS';  /* CHANGE ID FOR SHORT DDRCOM    */ 00375000
         MVC   DDRID(4,SDDRPTR),@CC03015                           0316 00376000
*           SDDRPTR->DDRSIRB='0'B;  /* INDICATE NO LONGER RUNNING  0317 00377000
*                                      UNDER SIRB                    */ 00378000
*                                                                  0317 00379000
         NI    DDRSIRB(SDDRPTR),B'11111011'                        0317 00380000
*           /*********************************************************/ 00381000
*           /*                                                       */ 00382000
*           /* SCHEDULE THE SRB TO ACTIVATE DDR                      */ 00383000
*           /*                                                       */ 00384000
*           /*********************************************************/ 00385000
*                                                                  0318 00386000
*           GENERATE REFS(SRBPTR,CVTLSMQ,SRBFLNK)                  0318 00387000
*               (SCHEDULE SRB=(SRBPTR),SCOPE=LOCAL);               0318 00388000
         SCHEDULE SRB=(SRBPTR),SCOPE=LOCAL                              00389000
*           DERGMLNG=0;             /* INDICATE THE GETMAINED AREA IS   00390000
*                                      NO LONGER OWNED BY IGE0660A   */ 00391000
         SLR   @12,@12                                             0319 00392000
         STCM  @12,7,DERGMLNG                                      0319 00393000
*           DERGMADR=0;             /* CLEAR ADDRESS                 */ 00394000
