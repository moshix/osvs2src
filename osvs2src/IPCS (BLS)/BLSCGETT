*                                      CODE                          */ 00372000
         ST    @10,DMCBRTC(,DMCBPTR)                               0124 00373000
*                     END;          /* GET REQUEST ERROR             */ 00374000
@RF00123 DS    0H                                                  0125 00375000
@RC00119 DS    0H                                                  0126 00376000
*                 END;              /* DO GET                        */ 00377000
@RC00101 DS    0H                                                  0127 00378000
*             END;                  /* SET IS OK                     */ 00379000
@RF00094 DS    0H                                                  0128 00380000
*         END;                      /* VSAM FILE                     */ 00381000
@RF00077 DS    0H                                                  0129 00382000
