*   DMCBRTC=F0C;                    /* WE ARE SUCCESSFUL UNTIL WE  0016 00036000
*                                      FAIL                          */ 00037000
*                                                                  0016 00038000
         ST    @10,DMCBRTC(,DMCBPTR)                               0016 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* FIRST DECIDE WHETHER DMCB IS OPEN                             */ 00042000
*   /*                                                               */ 00043000
*   /*****************************************************************/ 00044000
*                                                                  0017 00045000
*   IF DMCBSIN=OFF                  /* DMCB NOT OPEN FOR INPUT       */ 00046000
*       &DMCBSOUT=OFF               /* OR OUTPUT                     */ 00047000
*     THEN                                                         0017 00048000
         TM    DMCBSIN(DMCBPTR),B'11000000'                        0017 00049000
         BNZ   @RF00017                                            0017 00050000
*     DO;                                                          0018 00051000
*       DMCBRTC=INVRQ;              /* INDICATE INVALID REQUEST      */ 00052000
         MVC   DMCBRTC(4,DMCBPTR),@CF00037                         0019 00053000
*       DMCBREJ=F0C;                /* REQ REJECTED SINCE DMCB IS  0020 00054000
*                                      CLOSED                        */ 00055000
         MVI   DMCBREJ(DMCBPTR),X'00'                              0020 00056000
*     END;                                                         0021 00057000
*   ELSE                                                           0022 00058000
*     DO;                           /* DMCB IS OPEN                  */ 00059000
         B     @RC00017                                            0022 00060000
@RF00017 DS    0H                                                  0023 00061000
