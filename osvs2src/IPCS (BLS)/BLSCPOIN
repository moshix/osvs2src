*                                                                  0099 00344000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00345000
*     GPR01F FIXED(31) REG(1),                                     0099 00346000
*     GPR15F FIXED(31) REG(15),                                    0099 00347000
*     GPR01P PTR(31) REG(1),                                       0099 00348000
*     GPR02P PTR(31) REG(2),                                       0099 00349000
*     GPR03P PTR(31) REG(3),                                       0099 00350000
*     GPR08P PTR(31) REG(8);                                       0099 00351000
*   DECLARE                         /* COMMON VARIABLES              */ 00352000
*     I256C CHAR(256) BASED,                                       0100 00353000
*     I031F FIXED(31) BASED,                                       0100 00354000
*     I031P PTR(31) BASED,                                         0100 00355000
*     I015F FIXED(15) BASED,                                       0100 00356000
*     I015P PTR(15) BASED,                                         0100 00357000
*     I008P PTR(8) BASED,                                          0100 00358000
*     I001C CHAR(1) BASED;                                         0100 00359000
*   DCL                                                            0101 00360000
*     1 DASPATCH LOCAL BDY(DWORD),  /* PATCH AREA                    */ 00361000
