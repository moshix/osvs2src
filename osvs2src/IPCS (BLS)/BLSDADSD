*         IOPTLIST(8)=IOPTLIST(8)|'80000000'X;/* PLIST END MARKER    */ 00098000
         O     @10,@CF00059                                        0136 00099000
         ST    @10,IOPTLIST+28                                     0136 00100000
*         RFY                                                      0137 00101000
*          (GPR01P,                                                0137 00102000
*           GPR15P) RSTD;                                          0137 00103000
*         GPR01P=ADDR(IOPTLIST);    /* POINT REGISTER 1 TO PLIST     */ 00104000
         LA    GPR01P,IOPTLIST                                     0138 00105000
*         GPR15P=ZZ2ALOCP;          /* BLSCALOC ENTRY POINT          */ 00106000
         L     GPR15P,ZZ2ALOCP(,R9)                                0139 00107000
*         CALL GPR15P->DMCBDMGR;    /* PERFORM DATA SET ALLOCATION   */ 00108000
         BALR  @14,@15                                             0140 00109000
*         RFY                                                      0141 00110000
*          (GPR01P,                                                0141 00111000
*           GPR15P) UNRSTD;                                        0141 00112000
*       END;                        /* BLSCONAL                    0142 00113000
*                                      (ZZ2,DMCBPTR,MODEL,ACCMODE) 0142 00114000
*                                      DSN(LENGTH(FPDSDS),FPDSDS)  0142 00115000
*                                      ALLOCATE THE DSD              */ 00116000
*       RESPECIFY                                                  0143 00117000
*        (R0,                                                      0143 00118000
*         RF) RESTRICTED;                                          0143 00119000
*       ANCHOR=R0;                  /* SAVE MESSAGE CHAIN IF PRESENT */ 00120000
         ST    R0,ANCHOR                                           0144 00121000
*       RETCODE=RF;                 /* SAVE ALLOCATE RETCODE         */ 00122000
         ST    RF,RETCODE                                          0145 00123000
*       RESPECIFY                                                  0146 00124000
*        (R0,                                                      0146 00125000
*         RF) UNRESTRICTED;                                        0146 00126000
*       IF RETCODE^=0 THEN          /* TEST FOR DSD DATA SET       0147 00127000
*                                      ALLOCATION ERROR              */ 00128000
         L     @10,RETCODE                                         0147 00129000
         LTR   @10,@10                                             0147 00130000
         BZ    @RF00147                                            0147 00131000
*         DO;                       /* DSD DATA SET ALLOCATION ERROR */ 00132000
*           DO;                     /* BLSDMSG                     0149 00133000
*                                      (ZZ2,ALOCERR,ANCHOR,BLSDMSGS)    00134000
*                                      LIFO ROUTE(TERM)              */ 00135000
*             CALL BLSDMSG0(ZZ2,ALOCERR,ANCHOR,BLSDMSGS,'10010000'B,0); 00136000
