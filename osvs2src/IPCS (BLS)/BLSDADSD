         ST    @08,IOPTLIST+12                                     0131 00085000
*         IOPTLIST(5)=ADDR(DMCBCONA);/* CONDITIONAL ALLOCATE RQST    */ 00086000
         LA    @08,@CB00888                                        0132 00087000
         ST    @08,IOPTLIST+16                                     0132 00088000
*         IOPTLIST(6)='02000000'X;  /* 'DSN' KEY                     */ 00089000
         MVC   IOPTLIST+20(4),@CF00937                             0133 00090000
*         IOPTLIST(7)=ADDR(LENGTH(FPDSDS));/* 'DSN' LENGTH           */ 00091000
