BLSDADSD CSECT ,                                                   0002 00005000
@MAINENT DS    0H                                                  0002 00006000
         USING *,@15                                               0002 00007000
         B     @PROLOG                                             0002 00008000
         DC    AL1(16)                                             0002 00009000
         DC    C'BLSDADSD  78.062'                                 0002 00010000
         DROP  @15                                                      00011000
@PROLOG  STM   @14,@12,12(@13)                                     0002 00012000
         BALR  @12,0                                               0002 00013000
@PSTART  DS    0H                                                  0002 00014000
         USING @PSTART,@12                                         0002 00015000
         L     @00,@SIZDATD                                        0002 00016000
         BLSUALLR R,LV=(0)                                              00017000
         LR    @11,@01                                             0002 00018000
         USING @DATD,@11                                           0002 00019000
         ST    @13,@SA00001+4                                      0002 00020000
         LM    @00,@01,20(@13)                                     0002 00021000
         ST    @11,8(,@13)                                         0002 00022000
         LR    @13,@11                                             0002 00023000
         EJECT                                                          00024000
*                                                                  0118 00025000
*   /*****************************************************************/ 00026000
*   /*                                                               */ 00027000
*   /* INITIALIZE MODULE STATUS                                      */ 00028000
*   /*                                                               */ 00029000
*   /*****************************************************************/ 00030000
*                                                                  0118 00031000
*   MODNAME=LMODNMC;                                               0118 00032000
         MVC   MODNAME(8),@CC00934                                 0118 00033000
*   RETCODE=ZERO;                                                  0119 00034000
         SLR   @10,@10                                             0119 00035000
         ST    @10,RETCODE                                         0119 00036000
*   ANCHOR=ZERO;                                                   0120 00037000
*                                                                  0120 00038000
         ST    @10,ANCHOR                                          0120 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* INSURE THAT THE FACILITIES PARAMETER BLOCK IS INITIALIZED     */ 00042000
*   /*                                                               */ 00043000
*   /*****************************************************************/ 00044000
*                                                                  0121 00045000
*   IF ZZ1FPP=ZZZNULL THEN          /* TEST FOR UNINITIALIZED FPBLOCK*/ 00046000
         L     @10,ZZ2ZZ1P(,R9)                                    0121 00047000
         CLC   ZZ1FPP(4,@10),@CF00059                              0121 00048000
         BNE   @RF00121                                            0121 00049000
*     DO;                           /* FPBLOCK NOT PRESENT           */ 00050000
*       RETCODE=ZZZFLAGT;           /* MARK TERMINATING ERROR        */ 00051000
         MVC   RETCODE(4),@CF00050                                 0123 00052000
*     END;                          /* FPBLOCK NOT PRESENT           */ 00053000
*   ELSE                                                           0125 00054000
*     DO;                           /* FPBLOCK PRESENT               */ 00055000
*                                                                  0125 00056000
         B     @RC00121                                            0125 00057000
@RF00121 DS    0H                                                  0126 00058000
*       /*************************************************************/ 00059000
*       /*                                                           */ 00060000
*       /* ALLOCATE THE DATA SET DIRECTORY DATA SET                  */ 00061000
*       /*                                                           */ 00062000
*       /*************************************************************/ 00063000
*                                                                  0126 00064000
*       DMCBPTR=ZZ1DSDP&B0MASK;     /* INIT THE DMCB POINTER         */ 00065000
         L     @10,ZZ2ZZ1P(,R9)                                    0126 00066000
         L     @08,ZZ1DSDP(,@10)                                   0126 00067000
         N     @08,@CF00930                                        0126 00068000
         ST    @08,DMCBPTR                                         0126 00069000
*/* BLSCONAL (ZZ2,DMCBPTR,MODEL,ACCMODE) DSN(LENGTH(FPDSDS),FPDSDS)  */ 00070000
*                                                                  0127 00071000
*       DO;                         /* BLSCONAL                    0127 00072000
*                                      (ZZ2,DMCBPTR,MODEL,ACCMODE) 0127 00073000
*                                      DSN(LENGTH(FPDSDS),FPDSDS)    */ 00074000
*         IOPTLIST(1)=ADDR(ZZ2);    /* ZZ2 POINTER PARAMETER         */ 00075000
         ST    R9,IOPTLIST                                         0128 00076000
*         IOPTLIST(2)=ADDR(DMCBPTR);/* DMCB POINTER PARAMETER        */ 00077000
         LA    @08,DMCBPTR                                         0129 00078000
         ST    @08,IOPTLIST+4                                      0129 00079000
*         IOPTLIST(3)=ADDR(MODEL);  /* MODEL NAME PARAMETER          */ 00080000
         LA    @08,@CC00932                                        0130 00081000
         ST    @08,IOPTLIST+8                                      0130 00082000
*         IOPTLIST(4)=ADDR(ACCMODE);/* MAXIMUM ACCESS PARAMETER      */ 00083000
