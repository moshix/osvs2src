         ST    @10,@AL00001+12                                     0150 00143000
         LA    @10,@CB00940                                        0150 00144000
         ST    @10,@AL00001+16                                     0150 00145000
         LA    @10,@CF00043                                        0150 00146000
         ST    @10,@AL00001+20                                     0150 00147000
         L     @15,ZZ2MSG0P(,R9)                                   0150 00148000
         LA    @01,@AL00001                                        0150 00149000
         BALR  @14,@15                                             0150 00150000
*                                   /* BUILD MESSAGE                 */ 00151000
*           END;                    /* BLSDMSG                     0151 00152000
*                                      (ZZ2,ALOCERR,ANCHOR,BLSDMSGS)    00153000
*                                      LIFO ROUTE(TERM)              */ 00154000
*         END;                      /* DSD DATA SET ALLOCATION ERROR */ 00155000
*       ELSE                                                       0153 00156000
*         ZZ1DSDP=DMCBPTR;          /* ALLOCATION OK, SAVE DMCB    0153 00157000
*                                      POINTER                       */ 00158000
         B     @RC00147                                            0153 00159000
@RF00147 L     @10,ZZ2ZZ1P(,R9)                                    0153 00160000
         L     @08,DMCBPTR                                         0153 00161000
         ST    @08,ZZ1DSDP(,@10)                                   0153 00162000
*     END;                          /* FPBLOCK PRESENT               */ 00163000
@RC00147 DS    0H                                                  0155 00164000
*   RETURN CODE(RETCODE);                                          0155 00165000
@RC00121 L     @10,RETCODE                                         0155 00166000
