         ST    @10,@AL00001+12                                     0150 00142000
         LA    @10,@CB00940                                        0150 00143000
         ST    @10,@AL00001+16                                     0150 00144000
         LA    @10,@CF00043                                        0150 00145000
         ST    @10,@AL00001+20                                     0150 00146000
         L     @15,ZZ2MSG0P(,R9)                                   0150 00147000
         LA    @01,@AL00001                                        0150 00148000
         BALR  @14,@15                                             0150 00149000
*                                   /* BUILD MESSAGE                 */ 00150000
*           END;                    /* BLSDMSG                     0151 00151000
*                                      (ZZ2,ALOCERR,ANCHOR,BLSDMSGS)    00152000
*                                      LIFO ROUTE(TERM)              */ 00153000
*         END;                      /* PDR DATA SET ALLOCATION ERROR */ 00154000
*       ELSE                                                       0153 00155000
*         ZZ1PDRP=DMCBPTR;          /* ALLOCATION OK, SAVE DMCB    0153 00156000
*                                      POINTER                       */ 00157000
         B     @RC00147                                            0153 00158000
@RF00147 L     @10,ZZ2ZZ1P(,R9)                                    0153 00159000
         L     @08,DMCBPTR                                         0153 00160000
         ST    @08,ZZ1PDRP(,@10)                                   0153 00161000
*     END;                          /* FPBLOCK PRESENT               */ 00162000
@RC00147 DS    0H                                                  0155 00163000
*   RETURN CODE(RETCODE);                                          0155 00164000
@RC00121 L     @10,RETCODE                                         0155 00165000
