*         IOPTLIST(8)=ADDR(FPPDDS); /* 'DSN' TEXT                    */ 00094000
         L     @10,ZZ1FPP(,@10)                                    0135 00095000
         ST    @10,IOPTLIST+28                                     0135 00096000
*         IOPTLIST(8)=IOPTLIST(8)|'80000000'X;/* PLIST END MARKER    */ 00097000
         O     @10,@CF00059                                        0136 00098000
         ST    @10,IOPTLIST+28                                     0136 00099000
*         RFY                                                      0137 00100000
*          (GPR01P,                                                0137 00101000
*           GPR15P) RSTD;                                          0137 00102000
*         GPR01P=ADDR(IOPTLIST);    /* POINT REGISTER 1 TO PLIST     */ 00103000
         LA    GPR01P,IOPTLIST                                     0138 00104000
*         GPR15P=ZZ2ALOCP;          /* BLSCALOC ENTRY POINT          */ 00105000
         L     GPR15P,ZZ2ALOCP(,R9)                                0139 00106000
*         CALL GPR15P->DMCBDMGR;    /* PERFORM DATA SET ALLOCATION   */ 00107000
         BALR  @14,@15                                             0140 00108000
*         RFY                                                      0141 00109000
*          (GPR01P,                                                0141 00110000
*           GPR15P) UNRSTD;                                        0141 00111000
*       END;                        /* BLSCONAL                    0142 00112000
*                                      (ZZ2,DMCBPTR,MODEL,ACCMODE) 0142 00113000
*                                      DSN(LENGTH(FPPDDS),FPPDDS)  0142 00114000
*                                      ALLOCATE THE PDR              */ 00115000
*       RESPECIFY                                                  0143 00116000
*        (R0,                                                      0143 00117000
*         RF) RESTRICTED;                                          0143 00118000
*       ANCHOR=R0;                  /* SAVE MESSAGE CHAIN IF PRESENT */ 00119000
         ST    R0,ANCHOR                                           0144 00120000
*       RETCODE=RF;                 /* SAVE ALLOCATE RETCODE         */ 00121000
         ST    RF,RETCODE                                          0145 00122000
*       RESPECIFY                                                  0146 00123000
*        (R0,                                                      0146 00124000
*         RF) UNRESTRICTED;                                        0146 00125000
*       IF RETCODE^=0 THEN          /* TEST FOR PDR DATA SET       0147 00126000
*                                      ALLOCATION ERROR              */ 00127000
         L     @10,RETCODE                                         0147 00128000
         LTR   @10,@10                                             0147 00129000
         BZ    @RF00147                                            0147 00130000
*         DO;                       /* PDR DATA SET ALLOCATION ERROR */ 00131000
*           DO;                     /* BLSDMSG                     0149 00132000
*                                      (ZZ2,ALOCERR,ANCHOR,BLSDMSGS)    00133000
*                                      LIFO ROUTE(TERM)              */ 00134000
*             CALL BLSDMSG0(ZZ2,ALOCERR,ANCHOR,BLSDMSGS,'10010000'B,0); 00135000
