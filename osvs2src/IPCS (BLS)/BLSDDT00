*                 /***************************************************/ 00455000
*                 /*                                                 */ 00456000
*                 /* NONE OF THE ABOVE, MUST BE ERROR RESPONSE       */ 00457000
*                 /*                                                 */ 00458000
*                 /***************************************************/ 00459000
*                                                                  0198 00460000
*                 RETCD=4;          /* GOOD ONES GONE MUST BE ERROR  */ 00461000
         B     @RC00190                                            0198 00462000
@RF00190 LA    RETCD,4                                             0198 00463000
*         END;                      /* END OF VALIDITY CHECK         */ 00464000
*       ELSE                                                       0200 00465000
*         RETCD=4;                  /* ERROR IN MM/DD/YY STATEMENT   */ 00466000
         B     @RC00161                                            0200 00467000
@RF00161 LA    RETCD,4                                             0200 00468000
*     END;                          /* END OF THE DATE CHECK         */ 00469000
@RC00161 DS    0H                                                  0202 00470000
*   RETURN CODE(RETCD);             /* RETURN TO CALLER WITH RETURN     00471000
*                                      CODE                          */ 00472000
