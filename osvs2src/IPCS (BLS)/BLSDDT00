         C     @10,@CF00096                                        0134 00057000
         BNH   @RF00134                                            0134 00058000
         C     @10,@CF00798                                        0134 00059000
         BNL   @RF00134                                            0134 00060000
*         DO;                       /* LENGTH IS CORRECT CONTINUE  0135 00061000
*                                      WITH TEST                     */ 00062000
*                                                                  0135 00063000
*           /*********************************************************/ 00064000
*           /*                                                       */ 00065000
*           /* LENGTH IS CORRECT CHECK FOR LOCATION OF SLASHES A     */ 00066000
*           /* CHECK IS ALSO MADE TO ENSURE THAT THE CHARACTERS OTHER*/ 00067000
*           /* THAN SLASHES ARE NUMBERIC. ANY ERROR CONDITION SETS   */ 00068000
*           /* THE BAD RETURN CODE WHICH INDICATES THAT A REPROMPT IS*/ 00069000
*           /* REQUIRED.                                             */ 00070000
*           /*                                                       */ 00071000
*           /*********************************************************/ 00072000
*                                                                  0136 00073000
*           DO I=1 TO DTEIL WHILE(FLG^=2&ERR=0);/* COMMAND SCAN      */ 00074000
         LA    I,1                                                 0136 00075000
         B     @DE00136                                            0136 00076000
@DL00136 CLI   FLG,2                                               0136 00077000
         BE    @DC00136                                            0136 00078000
         CLI   ERR,0                                               0136 00079000
         BNE   @DC00136                                            0136 00080000
*             IF DTEI(1:1)='/' THEN /* IS THE CHARACTER A SLASH?     */ 00081000
