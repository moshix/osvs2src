*/*%INCLUDE SYSLIB  (BLRSERVD)                                       */ 00380000
*/*%INCLUDE SYSLIB  (BLSDSERV)                                       */ 00381000
*                                                                  0192 00382000
*       ;                                                          0192 00383000
@EL00001 L     @13,4(,@13)                                         0192 00384000
@EF00001 L     @00,@SIZDATD                                        0192 00385000
         LR    @01,@11                                             0192 00386000
         BLSUFRE2 R,LV=(0),A=(1)                                        00387000
@ER00001 LM    @14,@12,12(@13)                                     0192 00388000
         BR    @14                                                 0192 00389000
@DATA    DS    0H                                                       00390000
@CH00054 DC    H'8'                                                     00391000
@SM00812 MVC   TMEP1(0),BPRI(@09)                                       00392000
@SM00814 MVC   TMEP2(0),BPRI(@10)                                       00393000
@SM00816 MVC   TMEP3(0),BPRI(@09)                                       00394000
@DATD    DSECT                                                          00395000
         DS    0F                                                       00396000
@SA00001 DS    18F                                                      00397000
@PC00001 DS    3F                                                       00398000
BLSDTM00 CSECT                                                          00399000
         DS    0F                                                       00400000
@CF00046 DC    F'1'                                                     00401000
@CF00099 DC    F'3'                                                     00402000
@CF00175 DC    F'6'                                                     00403000
@CF00262 DC    F'23'                                                    00404000
@CF00804 DC    F'59'                                                    00405000
@DATD    DSECT                                                          00406000
         DS    0D                                                       00407000
         DS    0D                                                       00408000
@TS00001 DS    CL8                                                      00409000
TMEP1    DS    CL2                                                      00410000
TMEP2    DS    CL2                                                      00411000
TMEP3    DS    CL2                                                      00412000
TIME1    DS    CL2                                                      00413000
TIME2    DS    CL2                                                      00414000
TIME3    DS    CL2                                                      00415000
TFLG     DS    CL1                                                      00416000
TERR     DS    CL1                                                      00417000
BLSDTM00 CSECT                                                          00418000
         DS    0F                                                       00419000
@SIZDATD DC    AL1(0)                                                   00420000
         DC    AL3(@ENDDATD-@DATD)                                      00421000
         DS    0D                                                       00422000
@CC00785 DC    C'00'                                                    00423000
@DATD    DSECT                                                          00424000
         SPACE 2                                                        00425000
*********************************************************************** 00426000
* THE FOLLOWING AREA, BLRPATCH, IS RESERVED FOR PATCH APPLICATION     * 00427000
* TO OBTAIN PATCH ROOM IN THE @DATD AUTOMATIC STORAGE AREA, MODIFY    * 00428000
