*                                                                  0131 00031000
*   ZZ2PTR=ADDR(ZZ2PARM);           /* INITIALIZE ZZ2 POINTER        */ 00032000
         L     ZZ2PTR,@PC00001                                     0131 00033000
*   RETC=ZERO;                      /* INITIALIZE THE RETURN         */ 00034000
         SLR   RETC,RETC                                           0132 00035000
*   RETCDE=ZERO;                    /* CODES                         */ 00036000
         SLR   RETCDE,RETCDE                                       0133 00037000
*   MAXRETC=ZERO;                                                  0134 00038000
         SLR   MAXRETC,MAXRETC                                     0134 00039000
*   ZZ1PDRP->DMCBMSG=ZERO;          /* SET MSG AREA PTR TO 0         */ 00040000
*                                                                  0135 00041000
         SLR   @14,@14                                             0135 00042000
         L     @10,ZZ2ZZ1P(,ZZ2PTR)                                0135 00043000
