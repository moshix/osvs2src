         BNE   @RF00208                                         01S0208 00334000
*             DO;                   /* ANSWER YES                    */ 00335000
*               RETC=ZERO;          /* YES, INDICATE YES             */ 00336000
         SLR   RETC,RETC                                        01S0210 00337000
*               RETCODE=ZERO;       /* REPROMPT NOT NEEDED           */ 00338000
