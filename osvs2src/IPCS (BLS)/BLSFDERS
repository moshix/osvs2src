         TITLE 'IPCS SUBCOMMAND DELDSN - MODULE BLSFDERS               *00001000
                        '                                               00002000
* THIS MODULE IS WRITTEN FOR @G57LPAF.                                  00003000
BLSFDERS CSECT ,                                                   0002 00004000
@MAINENT DS    0H                                                  0002 00005000
         USING *,@15                                               0002 00006000
         B     @PROLOG                                             0002 00007000
         DC    AL1(16)                                             0002 00008000
         DC    C'BLSFDERS  78.065'                                 0002 00009000
         DROP  @15                                                      00010000
@PROLOG  STM   @14,@12,12(@13)                                     0002 00011000
         BALR  @12,0                                               0002 00012000
@PSTART  DS    0H                                                  0002 00013000
         USING @PSTART,@12                                         0002 00014000
         L     @00,@SIZDATD                                        0002 00015000
         BLSUALLS R,LV=(0)                                              00016000
         LR    @11,@01                                             0002 00017000
         USING @DATD,@11                                           0002 00018000
         ST    @13,@SA00001+4                                      0002 00019000
         LM    @00,@01,20(@13)                                     0002 00020000
         ST    @11,8(,@13)                                         0002 00021000
         LR    @13,@11                                             0002 00022000
         MVC   @PC00001(28),0(@01)                                 0002 00023000
         EJECT                                                          00024000
*                                                                  0138 00025000
*   /*****************************************************************/ 00026000
*   /*                                                               */ 00027000
*   /* INITIALIZE                                                    */ 00028000
*   /*                                                               */ 00029000
*   /*****************************************************************/ 00030000
*                                                                  0138 00031000
*   ZZ2PTR=ADDR(ZZ2PARM);           /* POINTER TO ZZ2 CONTROL BLOCK  */ 00032000
         L     ZZ2PTR,@PC00001                                     0138 00033000
*   RETC=ZZZFLAGI;                  /* INFORMATIONAL RETURN CODE     */ 00034000
         SLR   RETC,RETC                                           0139 00035000
*   MSGPTR=NULLPTR;                 /* NO MESSAGES ON MESSAGE CHAIN  */ 00036000
*                                                                  0140 00037000
         L     @10,@PC00001+24                                     0140 00038000
         SLR   @07,@07                                             0140 00039000
         ST    @07,MSGPTR(,@10)                                    0140 00040000
*   /*****************************************************************/ 00041000
*   /*                                                               */ 00042000
*   /* BUILD THE KEY OF THE RECORD TO BE ERASED                      */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0141 00046000
*   DSDKTYP=TYPE;                   /* LOAD THE SPECIFIED TYPE INTO     00047000
*                                      THE KEY                       */ 00048000
         LA    @10,RECKEY                                          0141 00049000
         L     @07,@PC00001+4                                      0141 00050000
         MVC   DSDKTYP(1,@10),TYPE(@07)                            0141 00051000
*   DSDKDSNM=DATASET;               /* LOAD THE SPECIFIED DATASET AND   00052000
*                                      MEMBER INTO THE KEY           */ 00053000
         L     @04,@PC00001+8                                      0142 00054000
         MVC   DSDKDSNM(52,@10),DATASET(@04)                       0142 00055000
*   IF TYPE=DSDRBTYC THEN                                          0143 00056000
         CLI   TYPE(@07),C'B'                                      0143 00057000
         BNE   @RF00143                                            0143 00058000
*     DSDKPID=DSDBRECC;             /* IF THIS IS A BASE RECORD THEN    00059000
*                                      IGNORE THE PROBLEM SPECIFIED     00060000
*                                      AND USE THE DEFINED CONSTANT  */ 00061000
         MVC   DSDKPID(8,@10),@CC00804                             0144 00062000
*   ELSE                                                           0145 00063000
*     DSDKPID=PROBLEM;              /* OTHERWISE USE THE SPECIFIED 0145 00064000
*                                      CONSTANT                      */ 00065000
*                                                                  0145 00066000
         B     @RC00143                                            0145 00067000
@RF00143 LA    @10,RECKEY                                          0145 00068000
         L     @07,@PC00001+12                                     0145 00069000
         MVC   DSDKPID(8,@10),PROBLEM(@07)                         0145 00070000
*   /*****************************************************************/ 00071000
*   /*                                                               */ 00072000
*   /* GET THE RECORD WHICH IS TO BE ERASED                          */ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*                                                                  0146 00076000
*/* BLSGET (ZZ1DSDP) UPD KEYEQ AREA(BUFFER)AREALEN(LENGTH(BUFFER))ARG(R 00077000
*CKEY)ARGLEN(LENGTH(RECKEY))                                         */ 00078000
*                                                                  0146 00079000
*   DO;                             /* BLSGET (ZZ1DSDP) UPD KEYEQ ARE   00080000
*                                      A(BUFFER)AREALEN(LENGTH(BUFFER   00081000
*                                      ))ARG(RECKEY)ARGLEN(LENGTH(REC   00082000
*                                      KEY))                         */ 00083000
@RC00143 DS    0H                                                  0147 00084000
*     RFY                                                          0147 00085000
*       GPR01P RSTD;                                               0147 00086000
*     GPR01P=ZZ1DSDP;               /* ADDRESS DMCB                  */ 00087000
         L     @10,ZZ2ZZ1P(,ZZ2PTR)                                0148 00088000
         L     GPR01P,ZZ1DSDP(,@10)                                0148 00089000
*     RFY                                                          0149 00090000
*       DMCB BASED(GPR01P);                                        0149 00091000
*     IF((GPR01P&'FFFFFFF8'X)=GPR01P)&DMCBID='DMCB'/* VALID DMCB ?   */ 00092000
*       THEN                                                       0150 00093000
         LR    @10,GPR01P                                          0150 00094000
         N     @10,@CF00984                                        0150 00095000
         CLR   @10,GPR01P                                          0150 00096000
         BNE   @RF00150                                            0150 00097000
         CLC   DMCBID(4,GPR01P),@CC00985                           0150 00098000
         BNE   @RF00150                                            0150 00099000
*       DO;                                                        0151 00100000
*         DMCBBUFP=ADDR(BUFFER);    /* PLACE BUFFER ADDRESS IN DMCB  */ 00101000
         LA    @10,BUFFER                                          0152 00102000
         ST    @10,DMCBBUFP(,GPR01P)                               0152 00103000
*         DMCBBLEN=LENGTH(BUFFER)&'7FFFFFFF'X;/* PLACE BUFFER LENGTH    00104000
*                                      IN DMCB                       */ 00105000
         MVC   DMCBBLEN(4,GPR01P),@CF01000                         0153 00106000
*         DMCBKEYP=ADDR(RECKEY);    /* PLACE KEY ADDRESS IN DMCB     */ 00107000
         LA    @10,RECKEY                                          0154 00108000
         ST    @10,DMCBKEYP(,GPR01P)                               0154 00109000
*         DMCBKEYL=(LENGTH(RECKEY))&'7FFFFFFF'X;/* PLACE KEY LENGTH     00110000
*                                      IN DMCB                       */ 00111000
         MVC   DMCBKEYL(4,GPR01P),@CF00774                         0155 00112000
*         DMCBKYD='1'B;             /* INDICATE KEYED ACCESS         */ 00113000
*         DMCBAPX='0'B;             /* INDICATE EXACT KEY MATCH      */ 00114000
*         DMCBGKY='0'B;             /* INDICATE FULL KEY SEARCH      */ 00115000
*         DMCBUPD='1'B;             /* INDICATE UPDATE OPTION        */ 00116000
         OI    DMCBKYD(GPR01P),B'10010000'                         0159 00117000
         NI    DMCBAPX(GPR01P),B'10011111'                         0159 00118000
*         DMCBRQST=DMCBGER;         /* INDICATE GET REQUEST          */ 00119000
         MVI   DMCBRQST(GPR01P),X'04'                              0160 00120000
*         CALL DMCBRI->DMCBDMGR;    /* CALL BLSCRQST                 */ 00121000
         L     @15,DMCBRI(,GPR01P)                                 0161 00122000
         BALR  @14,@15                                             0161 00123000
*       END;                                                       0162 00124000
*     ELSE                                                         0163 00125000
*       DO;                                                        0163 00126000
         B     @RC00150                                            0163 00127000
@RF00150 DS    0H                                                  0164 00128000
*         RFY                                                      0164 00129000
*          (GPR00F,                                                0164 00130000
*           GPR15F) RSTD;                                          0164 00131000
*         GPR00F=0;                 /* INDICATE NO MESSAGE CHAIN     */ 00132000
         SLR   GPR00F,GPR00F                                       0165 00133000
*         GPR15F=16;                /* INDICATE INVALID DMCB         */ 00134000
         LA    GPR15F,16                                           0166 00135000
*         RFY                                                      0167 00136000
*          (GPR00F,                                                0167 00137000
*           GPR15F);                                               0167 00138000
*       END;                                                       0168 00139000
*     RFY                                                          0169 00140000
*       DMCB;                                                      0169 00141000
@RC00150 DS    0H                                                  0170 00142000
*     RFY                                                          0170 00143000
*       GPR01P;                                                    0170 00144000
*   END;                            /* BLSGET (ZZ1DSDP) UPD KEYEQ ARE   00145000
*                                      A(BUFFER)AREALEN(LENGTH(BUFFER   00146000
*                                      ))ARG(RECKEY)ARGLEN(LENGTH(REC   00147000
*                                      KEY))                         */ 00148000
*   RESPECIFY                                                      0172 00149000
*     RF RESTRICTED;                                               0172 00150000
*   TRC=RF;                         /* SAVE THE RETURN CODE          */ 00151000
         LR    TRC,RF                                              0173 00152000
*   RESPECIFY                                                      0174 00153000
*     RF;                                                          0174 00154000
*                                                                  0174 00155000
*   /*****************************************************************/ 00156000
*   /*                                                               */ 00157000
*   /* HANDLE THE CASE IN WHICH THE GET FAILED                       */ 00158000
*   /*                                                               */ 00159000
*   /*****************************************************************/ 00160000
*                                                                  0175 00161000
*   IF TRC^=ZZZFLAGI THEN                                          0175 00162000
         SLR   @10,@10                                             0175 00163000
         CR    TRC,@10                                             0175 00164000
         BE    @RF00175                                            0175 00165000
*     DO;                           /* GET FAILED                    */ 00166000
*       IF TRC^=ZZZFLAGT THEN                                      0177 00167000
         C     TRC,@CF00065                                        0177 00168000
         BE    @RF00177                                            0177 00169000
*         DO;                       /* THERE ARE MESSAGES IN THE DMCB*/ 00170000
*           MSGPTR=ZZ1DSDP->DMCBMSG;/* GET MESSAGES FROM DMCB        */ 00171000
         L     @07,@PC00001+24                                     0179 00172000
         L     @04,ZZ2ZZ1P(,ZZ2PTR)                                0179 00173000
         L     @04,ZZ1DSDP(,@04)                                   0179 00174000
         L     @03,DMCBMSG(,@04)                                   0179 00175000
         ST    @03,MSGPTR(,@07)                                    0179 00176000
*           ZZ1DSDP->DMCBMSG=NULLPTR;/* RESET DMCB                   */ 00177000
         ST    @10,DMCBMSG(,@04)                                   0180 00178000
*         END;                      /* THERE ARE MESSAGES IN THE DMCB*/ 00179000
*       ELSE                                                       0182 00180000
*         MSGPTR=NULLPTR;           /* THERE IS NO DMCB, SO NULL   0182 00181000
*                                      MESSAGE CHAIN                 */ 00182000
         B     @RC00177                                            0182 00183000
@RF00177 L     @10,@PC00001+24                                     0182 00184000
         SLR   @07,@07                                             0182 00185000
         ST    @07,MSGPTR(,@10)                                    0182 00186000
*       IF TRC=ZZZFLAGE THEN                                       0183 00187000
@RC00177 C     TRC,@CF00062                                        0183 00188000
         BNE   @RF00183                                            0183 00189000
*         DO;                       /* RECORD NOT FOUND              */ 00190000
*           RETC=ZZZFLAGW;          /* RETURN CODE IS WARNING        */ 00191000
         LA    RETC,4                                              0185 00192000
*         END;                      /* RECORD NOT FOUND              */ 00193000
*       ELSE                                                       0187 00194000
*         DO;                       /* SEVERE ERROR                  */ 00195000
         B     @RC00183                                            0187 00196000
@RF00183 DS    0H                                                  0188 00197000
*           DO;                     /* BLSDMSG                     0188 00198000
*                                      (ZZ2,MESSAGE1,MSGPTR,BLSDMSGS)   00199000
*                                      LIFO ROUTE(TERM)              */ 00200000
*             CALL BLSDMSG0(ZZ2,MESSAGE1,MSGPTR,BLSDMSGS,'10010000'B,0) 00201000
*                 ;                 /* BUILD MESSAGE                 */ 00202000
         ST    ZZ2PTR,@AL00001                                     0189 00203000
         LA    @10,@CF00966                                        0189 00204000
         ST    @10,@AL00001+4                                      0189 00205000
         L     @10,@PC00001+24                                     0189 00206000
         ST    @10,@AL00001+8                                      0189 00207000
         L     @10,ZZ2MSGSP(,ZZ2PTR)                               0189 00208000
         ST    @10,@AL00001+12                                     0189 00209000
         LA    @10,@CB00989                                        0189 00210000
         ST    @10,@AL00001+16                                     0189 00211000
         LA    @10,@CF00059                                        0189 00212000
         ST    @10,@AL00001+20                                     0189 00213000
         L     @15,ZZ2MSG0P(,ZZ2PTR)                               0189 00214000
         LA    @01,@AL00001                                        0189 00215000
         BALR  @14,@15                                             0189 00216000
*           END;                    /* BLSDMSG                     0190 00217000
*                                      (ZZ2,MESSAGE1,MSGPTR,BLSDMSGS)   00218000
*                                      LIFO ROUTE(TERM) PRINT MESSAGE*/ 00219000
*           DO;                     /* ABEND(ACODE) DUMP USER        */ 00220000
*             RESPECIFY                                            0192 00221000
*              (GPR01F) RESTRICTED;                                0192 00222000
*             GPR01F=(ACODE)&'00000FFF'X;/* COMP CODE IN BITS 20-31  */ 00223000
         LA    GPR01F,115                                          0193 00224000
*             GPR01F=GPR01F|'80000000'X;/* DUMP/STEP/DUMPOPTS FLAG   */ 00225000
         O     GPR01F,@CF00074                                     0194 00226000
*             SVC(13);              /* ISSUE ABEND SVC               */ 00227000
         SVC   13                                                  0195 00228000
*             RESPECIFY                                            0196 00229000
*              (GPR01F) UNRESTRICTED;                              0196 00230000
*           END;                    /* ABEND(ACODE) DUMP USER ABEND  */ 00231000
*         END;                      /* SEVERE ERROR                  */ 00232000
*     END;                          /* GET FAILED                    */ 00233000
*   ELSE                                                           0200 00234000
*     DO;                           /* GET SUCCEEDED                 */ 00235000
*                                                                  0200 00236000
         B     @RC00175                                            0200 00237000
@RF00175 DS    0H                                                  0201 00238000
*       /*************************************************************/ 00239000
*       /*                                                           */ 00240000
*       /* LOAD THE RECORD PARAMETER                                 */ 00241000
*       /*                                                           */ 00242000
*       /*************************************************************/ 00243000
*                                                                  0201 00244000
*       IF RECLEN>0 THEN            /* LOAD THE RECORD PARAMETER ONLY   00245000
*                                      IF A POSITIVE RECLEN HAS BEEN    00246000
*                                      SPECIFIED                     */ 00247000
         L     @10,@PC00001+16                                     0201 00248000
         L     @10,RECLEN(,@10)                                    0201 00249000
         LTR   @10,@10                                             0201 00250000
         BNP   @RF00201                                            0201 00251000
*         DO;                       /* LOAD RECORD PARAMETER         */ 00252000
*           IF ZZ1DSDP->DMCBIRL>RECLEN THEN                        0203 00253000
