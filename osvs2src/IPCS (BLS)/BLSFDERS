         L     @07,ZZ1DSDP(,@07)                                   0203 00255000
         C     @10,DMCBIRL(,@07)                                   0203 00256000
         BNL   @RF00203                                            0203 00257000
*             RECORD(FIRST:RECLEN)=BUFFER(FIRST:RECLEN);/* THE DSD 0204 00258000
*                                      RECORD IS LONGER THAN THE   0204 00259000
*                                      RECORD PARAMETER SO LOAD AND     00260000
*                                      TRUNCATE                      */ 00261000
         L     @07,@PC00001+20                                     0204 00262000
         BCTR  @10,0                                               0204 00263000
         EX    @10,@SM01004                                        0204 00264000
*           ELSE                                                   0205 00265000
*             DO;                   /* THE RECORD PARAMETER IS LONGER   00266000
*                                      THAN THE DSD RECORD           */ 00267000
         B     @RC00203                                            0205 00268000
@RF00203 DS    0H                                                  0206 00269000
*               RECORD(FIRST:ZZ1DSDP->DMCBIRL)=BUFFER(FIRST:ZZ1DSDP->   00270000
*                   DMCBIRL);                                      0206 00271000
         L     @10,@PC00001+20                                     0206 00272000
         L     @07,ZZ2ZZ1P(,ZZ2PTR)                                0206 00273000
         L     @07,ZZ1DSDP(,@07)                                   0206 00274000
         L     @07,DMCBIRL(,@07)                                   0206 00275000
         LR    @04,@07                                             0206 00276000
         BCTR  @04,0                                               0206 00277000
         EX    @04,@SM01006                                        0206 00278000
*               DO I=ZZ1DSDP->DMCBIRL+ONE TO RECLEN;/* RIGHT JUSTIFY    00279000
*                                      WITH BLANKS                   */ 00280000
         AL    @07,@CF00054                                        0207 00281000
         LR    I,@07                                               0207 00282000
         B     @DE00207                                            0207 00283000
@DL00207 DS    0H                                                  0208 00284000
*                 RECORD(I)=BLANK;                                 0208 00285000
         L     @10,@PC00001+20                                     0208 00286000
         ALR   @10,I                                               0208 00287000
         BCTR  @10,0                                               0208 00288000
         MVI   RECORD(@10),C' '                                    0208 00289000
*               END;                /* DO LOOP                       */ 00290000
         AL    I,@CF00054                                          0209 00291000
@DE00207 L     @10,@PC00001+16                                     0209 00292000
         C     I,RECLEN(,@10)                                      0209 00293000
         BNH   @DL00207                                            0209 00294000
*             END;                  /* THE RECORD PARAMETER IS LONGER   00295000
*                                      THAN THE DSD RECORD           */ 00296000
*         END;                      /* LOAD RECORD PARAMETER         */ 00297000
*                                                                  0211 00298000
@RC00203 DS    0H                                                  0212 00299000
*       /*************************************************************/ 00300000
*       /*                                                           */ 00301000
*       /* ERASE THE DSD RECORD                                      */ 00302000
*       /*                                                           */ 00303000
*       /*************************************************************/ 00304000
*                                                                  0212 00305000
*       CALL BLSFERAS(ZZ2,ZZ1DSDP,MESSAGE2);/* ERASE THE DESIRED   0212 00306000
*                                      RECORD                        */ 00307000
@RF00201 ST    ZZ2PTR,@AL00001                                     0212 00308000
         L     @10,ZZ2ZZ1P(,ZZ2PTR)                                0212 00309000
         LA    @10,ZZ1DSDP(,@10)                                   0212 00310000
         ST    @10,@AL00001+4                                      0212 00311000
         LA    @10,@CF00968                                        0212 00312000
         ST    @10,@AL00001+8                                      0212 00313000
         L     @15,@CV00976                                        0212 00314000
         LA    @01,@AL00001                                        0212 00315000
         BALR  @14,@15                                             0212 00316000
*     END;                          /* GET SUCCEEDED                 */ 00317000
*   RETURN CODE(RETC);                                             0214 00318000
*                                                                  0214 00319000
@RC00175 L     @13,4(,@13)                                         0214 00320000
         L     @00,@SIZDATD                                        0214 00321000
         LR    @01,@11                                             0214 00322000
         BLSUFRES R,LV=(0),A=(1)                                        00323000
         LR    @15,@06                                             0214 00324000
         L     @14,12(,@13)                                        0214 00325000
         LM    @00,@12,20(@13)                                     0214 00326000
         BR    @14                                                 0214 00327000
