         MVC   CURMEM+1(7,@10),CURMEM(@10)                      01S0160 00054000
*   CALL GETDSN;                    /* CALL FOR CURRENT DSN          */ 00055000
         BAL   @14,GETDSN                                       01S0161 00056000
*   RESPECIFY                                                   01S0162 00057000
*     RF RESTRICTED;                /* SAVE THE RETURN CODE REG      */ 00058000
*   IF RETCODE=ZERO&                /* CURRENT VALUE FOUND           */ 00059000
*       ZZ1EVEP='0'B THEN           /* NO REQUEST FOR TERMINATION    */ 00060000
         LTR   RETCODE,RETCODE                                  01S0163 00061000
         BNZ   @RF00163                                         01S0163 00062000
