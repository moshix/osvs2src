*           SVC(13);                /* ISSUE ABEND SVC               */ 00411000
         SVC   13                                                  0255 00412000
*           RESPECIFY                                              0256 00413000
*            (GPR01F) UNRESTRICTED;                                0256 00414000
*         END;                      /* ABEND(100) DUMP USER YES,   0257 00415000
*                                      ABEND TO GET INTO TEST        */ 00416000
*       RETC=MAX(RETC,TWELVE);      /* BUMP RETURN CODE TO ERROR   0258 00417000
*                                      CONDITION                     */ 00418000
@RF00250 LA    @10,12                                              0258 00419000
         CR    RETC,@10                                            0258 00420000
         BNL   *+6                                                      00421000
         LR    RETC,@10                                            0258 00422000
*     END;                          /* END OPEN ERROR PROCESSING     */ 00423000
*   RETURN CODE(RETC);              /* RETURN WITH RETURN CODE       */ 00424000
