*                     ESBAFSA='1'B; /* ABSOLUTE ADDRESS VALID        */ 00494000
         OI    ESBAFSA(ESBAPTR),B'00010000'                     02S0265 00495000
*                   END;            /* ABSOLUTE STORAGE FRAME        */ 00496000
*               END;                /* FIRST STORAGE RECORD          */ 00497000
