         BNE   @RF00241                                         02S0241 00392000
*           /*********************************************************/ 00393000
*           /*                                                       */ 00394000
*           /* SUMDUMP RECORD                                        */ 00395000
*           /*                                                       */ 00396000
*           /*********************************************************/ 00397000
*                                                               02S0242 00398000
*           DO;                     /* SUMDUMP RECORD                */ 00399000
*             ESBAKEY='FF'X;        /* INDETERMINATE STORAGE KEY     */ 00400000
         MVI   ESBAKEY(ESBAPTR),X'FF'                           02S0243 00401000
*             ESBAFSS='1'B;         /* SUMDUMP DATA                  */ 00402000
         OI    ESBAFSS(ESBAPTR),B'00000001'                     02S0244 00403000
