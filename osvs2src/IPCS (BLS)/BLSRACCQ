         BNH   *+6                                                      00585000
         LR    @04,@05                                          02S0288 00586000
         ST    @04,AMTOMOVE                                     02S0288 00587000
*     RESPECIFY                     /* BEFORE MVCL                   */ 00588000
*      (R0,                         /* ->USER BUFFER                 */ 00589000
*       R1,                         /* STORAGE TO MOVE               */ 00590000
*       RE) RESTRICTED;             /* ->INPUT BUFFER                */ 00591000
*     R0=CURBUFAD;                  /* ->CURRENT BUFFER POSITION     */ 00592000
         L     R0,CURBUFAD                                      02S0290 00593000
*     R1=AMTOMOVE;                  /* STORAGE TO MOVE               */ 00594000
         LR    R1,@04                                           02S0291 00595000
*     RE=ADDR(HSD)+RAAUROF(1)+OFFINBLK;/* ->INPUT BUFFER             */ 00596000
         L     @05,OFFINBLK                                     02S0292 00597000
         LR    RE,HSDPTR                                        02S0292 00598000
         AL    RE,RAAUROF                                       02S0292 00599000
         ALR   RE,@05                                           02S0292 00600000
*     RF=R1;                        /* STORAGE TO MOVE               */ 00601000
         LR    RF,R1                                            02S0293 00602000
*     MVCL(R0,RE);                  /* FILL USER BUFFER              */ 00603000
         MVCL  R0,RE                                            02S0294 00604000
*     CURBUFAD=R0;                  /* ->CURRENT BUFFER POSITION     */ 00605000
         ST    R0,CURBUFAD                                      02S0295 00606000
*     RESPECIFY                     /* AFTER MVCL                    */ 00607000
*      (R0,                         /* ->USER BUFFER+LENGTH MOVED    */ 00608000
*       R1,                         /* 0                             */ 00609000
*       RE) UNRESTRICTED;           /* ->INPUT BUFFER+LENGTH MOVED   */ 00610000
*     AMTLEFT=AMTLEFT-AMTOMOVE;     /* REQUESTED STORAGE REMAINING   */ 00611000
         L     @14,AMTOMOVE                                     02S0297 00612000
         L     @07,AMTLEFT                                      02S0297 00613000
         SLR   @07,@14                                          02S0297 00614000
         ST    @07,AMTLEFT                                      02S0297 00615000
*     CURADDR=CURADDR+AMTOMOVE;     /* ->CURRENT STORAGE POSITION    */ 00616000
         ALR   CURADDR,@14                                      02S0298 00617000
*   END;                            /* ACCESS ALL STORAGE REQUESTED  */ 00618000
@DE00177 ICM   @14,15,AMTLEFT                                   01S0299 00619000
         BP    @DL00177                                         01S0299 00620000
