*                           ESBAFS2='1'B;/* MULTIPLE STORAGE KEYS    */ 00471000
         OI    ESBAFS2(ESBAPTR),B'01000000'                     02S0258 00472000
*                       END;        /* BOTH STORAGE KEYS APPLY       */ 00473000
*                   END;            /* INITIAL 2K OF 4K FRAME        */ 00474000
*                 ELSE              /* SECOND 2K OF 4K FRAME         */ 00475000
*                   ESBAKEY=HSDSK2; /* STORAGE KEY                   */ 00476000
         B     @RC00251                                         02S0261 00477000
@RF00251 MVC   ESBAKEY(1,ESBAPTR),HSDSK2(HSDPTR)                02S0261 00478000
*                 IF HSDSASI=HSDSASIR THEN/* ABSOLUTE STORAGE FRAME  */ 00479000
