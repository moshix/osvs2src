*                                                               02S0247 00414000
*           DO;                     /* NO SUMDUMP DATA               */ 00415000
*             IF ESBAFSC='0'B THEN  /* FIRST STORAGE RECORD          */ 00416000
*                                                               02S0248 00417000
         TM    ESBAFSC(ESBAPTR),B'10000000'                     02S0248 00418000
         BNZ   @RF00248                                         02S0248 00419000
*               /*****************************************************/ 00420000
*               /*                                                   */ 00421000
*               /* FIRST STORAGE RECORD                              */ 00422000
*               /*                                                   */ 00423000
*               /*****************************************************/ 00424000
*                                                               02S0249 00425000
*               DO;                 /* FIRST STORAGE RECORD          */ 00426000
*                 P1=CURADDR&ZZZP4OFF;/* OFFSET FROM PAGE ORIGIN     */ 00427000
         LR    P1,CURADDR                                       02S0250 00428000
         N     P1,@CF00118                                      02S0250 00429000
*                 IF P1<ZZZLENP2 THEN/* INITIAL 2K OF 4K FRAME       */ 00430000
*                                                               02S0251 00431000
         LA    @14,2048                                         02S0251 00432000
         CR    P1,@14                                           02S0251 00433000
         BNL   @RF00251                                         02S0251 00434000
*                   /*************************************************/ 00435000
*                   /*                                               */ 00436000
*                   /* INITIAL 2K OF 4K FRAME                        */ 00437000
*                   /*                                               */ 00438000
*                   /*************************************************/ 00439000
*                                                               02S0252 00440000
*                   DO;             /* INITIAL 2K OF 4K FRAME        */ 00441000
*                     ESBAKEY=HSDSK1;/* STORAGE KEY                  */ 00442000
         MVC   ESBAKEY(1,ESBAPTR),HSDSK1(HSDPTR)                02S0253 00443000
*                     IF P1+AMTLEFT>ZZZLENP2 THEN/* 2 STORAGE KEYS   */ 00444000
