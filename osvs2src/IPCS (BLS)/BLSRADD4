BLSRADD4 CSECT ,                                                   0002 00006000
@MAINENT DS    0H                                                  0002 00007000
         USING *,@15                                               0002 00008000
         B     @PROLOG                                             0002 00009000
         DC    AL1(16)                                             0002 00010000
         DC    C'BLSRADD4  78.057'                                 0002 00011000
         DROP  @15                                                      00012000
@PROLOG  STM   @14,@12,12(@13)                                     0002 00013000
         BALR  @12,0                                               0002 00014000
@PSTART  DS    0H                                                  0002 00015000
         USING @PSTART,@12                                         0002 00016000
         L     @00,@SIZDATD                                        0002 00017000
         BLSUALLS R,LV=(0)                                              00018000
         LR    @11,@01                                             0002 00019000
         USING @DATD,@11                                           0002 00020000
         ST    @13,@SA00001+4                                      0002 00021000
         LM    @00,@01,20(@13)                                     0002 00022000
         ST    @11,8(,@13)                                         0002 00023000
         LR    @13,@11                                             0002 00024000
         MVC   @PC00001(16),0(@01)                                 0002 00025000
*/********************************************************************/ 00026000
*/*                                                                  */ 00027000
*/* BLSRADD4--OVERRIDE CPU ADDRESS OR ASID                           */ 00028000
*/*                                                                  */ 00029000
*/********************************************************************/ 00030000
*                                                                  0177 00031000
*   ESBAPTR=ADDR(ESPA);             /* ->ESBA (ALIAS FOR ESPA)       */ 00032000
         L     ESBAPTR,@PC00001+4                                  0177 00033000
*   PDLPTR=ADDR(PD);                /* ->PD                          */ 00034000
         L     PDLPTR,@PC00001+8                                   0178 00035000
*   PPCPU=PDLPTR+PXAO(PXAOCPU);     /* ->PXCPU                       */ 00036000
         L     @10,@PC00001+12                                     0179 00037000
         LR    PPCPU,PDLPTR                                        0179 00038000
         AH    PPCPU,PXAO+10(,@10)                                 0179 00039000
*   RESPECIFY                                                      0180 00040000
*    (PXCPU) BASED(PPCPU);          /* ->PXCPU                       */ 00041000
*   IF PXCPUIFP='1'B THEN           /* EXPLICIT CPU ADDRESS          */ 00042000
         TM    PXCPUIFP(PPCPU),B'10000000'                         0181 00043000
         BNO   @RF00181                                            0181 00044000
*     ESBAAS1=PXCPUI;               /* EXPLICIT CPU ADDRESS          */ 00045000
         L     @10,PXCPUIP(,PPCPU)                                 0182 00046000
         L     @10,PXCPUI(,@10)                                    0182 00047000
         ST    @10,ESBAAS1(,ESBAPTR)                               0182 00048000
*   IF ESBAAST=ZZZASTCV THEN        /* S370 VIRTUAL STORAGE          */ 00049000
*                                                                  0183 00050000
@RF00181 CLC   ESBAAST(2,ESBAPTR),@CC00084                         0183 00051000
         BNE   @RF00183                                            0183 00052000
*     /***************************************************************/ 00053000
*     /*                                                             */ 00054000
*     /* S370 VIRTUAL STORAGE                                        */ 00055000
*     /*                                                             */ 00056000
*     /***************************************************************/ 00057000
*                                                                  0184 00058000
*     DO;                           /* S370 VIRTUAL STORAGE          */ 00059000
*       PPASI=PDLPTR+PXAO(PXAOASI); /* ->PXASI                       */ 00060000
         L     @10,@PC00001+12                                     0185 00061000
         LR    PPASI,PDLPTR                                        0185 00062000
         AH    PPASI,PXAO+14(,@10)                                 0185 00063000
*       RESPECIFY                                                  0186 00064000
*        (PXASI) BASED(PPASI);      /* ->PXASI                       */ 00065000
*       IF PXASIIFP='1'B THEN       /* EXPLICIT ASID                 */ 00066000
         TM    PXASIIFP(PPASI),B'10000000'                         0187 00067000
         BNO   @RF00187                                            0187 00068000
*         ESBAAS2=PXASII;           /* EXPLICIT ASID                 */ 00069000
