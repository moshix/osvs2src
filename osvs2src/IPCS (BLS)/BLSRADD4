         L     @10,PXASII(,@10)                                    0188 00071000
         ST    @10,ESBAAS2(,ESBAPTR)                               0188 00072000
*     END;                                                         0189 00073000
*   ELSE                            /* NOT S370 VIRTUAL STORAGE      */ 00074000
*     ESBAAS2=0;                    /* NO ASID                       */ 00075000
         B     @RC00183                                            0190 00076000
@RF00183 SLR   @10,@10                                             0190 00077000
         ST    @10,ESBAAS2(,ESBAPTR)                               0190 00078000
*   RETURN CODE(ZZZFLAGI);                                         0191 00079000
*                                                                  0191 00080000
