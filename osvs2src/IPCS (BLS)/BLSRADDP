@EL00001 L     @13,4(,@13)                                         0266 00422000
@EF00001 L     @00,@SIZDATD                                        0266 00423000
         LR    @01,@11                                             0266 00424000
         BLSUFRES R,LV=(0),A=(1)                                        00425000
@ER00001 LM    @14,@12,12(@13)                                     0266 00426000
         BR    @14                                                 0266 00427000
@DATA    DS    0H                                                       00428000
@DATD    DSECT                                                          00429000
         DS    0F                                                       00430000
@SA00001 DS    18F                                                      00431000
@PC00001 DS    4F                                                       00432000
@AL00001 DS    2A                                                       00433000
BLSRADDP CSECT                                                          00434000
         DS    0F                                                       00435000
@CF00052 DC    F'1'                                                     00436000
@CF00077 DC    F'2'                                                     00437000
@CF00049 DC    F'4'                                                     00438000
@CF01244 DC    F'4103'                                                  00439000
@CF00128 DC    F'16777215'                                              00440000
@CF00132 DC    F'2147483647'                                            00441000
@CF01249 DC    XL4'0000FFFF'                                            00442000
@DATD    DSECT                                                          00443000
         DS    0D                                                       00444000
BLSRADDP CSECT                                                          00445000
         DS    0F                                                       00446000
@SIZDATD DC    AL1(0)                                                   00447000
         DC    AL3(@ENDDATD-@DATD)                                      00448000
         DS    0D                                                       00449000
@CC00078 DC    C'A '                                                    00450000
@CC00080 DC    C'C '                                                    00451000
@CC00088 DC    C'H '                                                    00452000
DTU      DS    CL34                                                     00453000
         ORG   DTU                                                      00454000
DTU0     DS    0C                                                       00455000
DTUY     DC    CL1'U'                                                   00456000
@NM00089 DC    X'00'                                                    00457000
DTUD     DC    CL31'                               '                    00458000
DTUE     DC    CL1' '                                                   00459000
DTU9     DS    0C                                                       00460000
         ORG   DTU+34                                                   00461000
M1       DS    CL67                                                     00462000
         ORG   M1                                                       00463000
M1L      DC    AL2(67)                                                  00464000
@NM00113 DC    X'0000'                                                  00465000
M1T      DS    CL63                                                     00466000
         ORG   M1T                                                      00467000
M1T1     DC    CL63'BLS18054I UPPER ADDRESS BOUND EXCEEDED. SCALAR SUBSC00468000
               TITUTED     '                                            00469000
         ORG   M1+67                                                    00470000
         DS    CL1                                                      00471000
M2       DS    CL40                                                     00472000
         ORG   M2                                                       00473000
M2L      DC    AL2(40)                                                  00474000
@NM00114 DC    X'0000'                                                  00475000
M2T      DS    CL36                                                     00476000
         ORG   M2T                                                      00477000
M2T1     DC    CL36'BLS18074I ENTRY(IES) KEYWORD IGNORED'               00478000
         ORG   M2+40                                                    00479000
@DATD    DSECT                                                          00480000
         SPACE 2                                                        00481000
*********************************************************************** 00482000
* THE FOLLOWING AREA, BLRPATCH, IS RESERVED FOR PATCH APPLICATION     * 00483000
* TO OBTAIN PATCH ROOM IN THE @DATD AUTOMATIC STORAGE AREA, MODIFY    * 00484000
* CONSTANT @SIZDATD TO REFLECT THE DESIRED @DATD SIZE                 * 00485000
*********************************************************************** 00486000
         SPACE                                                          00487000
BLSRADDP CSECT                                                          00488000
         ORG                                                            00489000
         DS    0D                                                       00490000
BLRPATCH DC    CL8'ZAPAREA',(((*-BLSRADDP+19)/20+7)/8)CL8'BLSRADDP'     00491000
@DATD    DSECT                                                          00492000
         DS    0H                                                       00493000
@DATD    DSECT                                                          00494000
         SPACE 2                                                        00495000
*********************************************************************** 00496000
* ALIGN END OF DATA ON A DOUBLEWORD BOUNDARY                          * 00497000
*********************************************************************** 00498000
         SPACE                                                          00499000
         DS    0D                                                       00500000
@DATD    DSECT                                                          00501000
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA                00502000
@ENDDATD EQU   *                                                        00503000
