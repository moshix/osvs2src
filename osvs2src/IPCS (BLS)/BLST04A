*   ESBAPTR=ADDR(ESPA);             /* ->ESBA (ALIAS FOR ESPA)       */ 00048000
@RF00134 L     ESBAPTR,@PC00001+4                                  0139 00049000
*   M1B=M1S;                        /* MESSAGE M1 LITERAL TEXT       */ 00050000
         MVC   M1B(38),M1S                                         0140 00051000
*   DO;                             /* BLRCNVPC(ESBALAD+ESBADOF) FROM   00052000
