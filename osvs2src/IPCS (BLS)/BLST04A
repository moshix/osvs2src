*     TR(M1BTBA,ZZ1TRHEX);          /* ZONED HEXADECIMAL=>EBCDIC     */ 00074000
         L     @10,ZZ2ZZ1P(,R9)                                    0145 00075000
         TR    M1BTBA(6),ZZ1TRHEX(@10)                             0145 00076000
*   END;                            /* BLRCNVPC(ESBALAD+ESBADOF) FROM   00077000
*                                      (POINTER)TO(CHARACTER)SET(M1BT   00078000
*                                      BA)REMARK(ADDRESS OF        0146 00079000
*                                      STORAGE)TR(ZZ1TRHEX)          */ 00080000
*   IF ESBAFSC='1'B THEN            /* STORAGE DATA AVAILABLE        */ 00081000
         TM    ESBAFSC(ESBAPTR),B'10000000'                        0147 00082000
         BNO   @RF00147                                            0147 00083000
*     DO;                           /* BLRCNVBC(ESBAKEY) FROM(BIT)TO(   00084000
*                                      CHARACTER)SET(M1BTD)REMARK(S37   00085000
*                                      0 STORAGE KEY)TR(ZZ1TRHEX)    */ 00086000
*                                                                  0148 00087000
*       /*************************************************************/ 00088000
*       /*                                                           */ 00089000
*       /* S370 STORAGE KEY                                          */ 00090000
*       /*                                                           */ 00091000
*       /*************************************************************/ 00092000
*                                                                  0149 00093000
*       UNPK(M1BTD(1:LENGTH(M1BTD)-1),ESBAKEY);/* BIT=>ZONED       0149 00094000
