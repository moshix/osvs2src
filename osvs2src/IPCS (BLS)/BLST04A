         L     @15,BVTMPK1P(,@10)                                  0158 00129000
         LA    @01,@AL00001                                        0158 00130000
         BALR  @14,@15                                             0158 00131000
*   RETC=RF;                        /* RETURN CODE                   */ 00132000
         LR    RETC,RF                                             0159 00133000
*   IF RETC>ZZZFLAGE THEN           /* SERIOUS ERROR COMPRESSING M1  */ 00134000
         C     RETC,@CF00056                                       0160 00135000
         BH    @RT00160                                            0160 00136000
*     GO TO EXIT;                   /* COMMON EXIT PATH              */ 00137000
*   CALL BLSUPUTV(ZZ2,M1B);         /* TRANSMIT MESSAGE M1           */ 00138000
