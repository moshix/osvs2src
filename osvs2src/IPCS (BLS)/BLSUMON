*/* BLSUMON1--MONITOR INITIALIZATION                                 */ 00030000
*/*------------------------------------------------------------------*/ 00031000
*                                                               02S0089 00032000
*   DO;                             /* MONITOR INITIALIZATION        */ 00033000
*     FLAG='00'X;                   /* PROCESSING CONTROL FLAGS      */ 00034000
         MVI   FLAG,X'00'                                       02S0090 00035000
*     IOPLUPT=ADDR(UPT);            /* ADDR OF USER PROFILE TABLE    */ 00036000
         MVC   IOPLUPT(4),CPPLUPT(R9)                           02S0091 00037000
*     IOPLECT=ADDR(ECT);            /* ADDR OF ENVIRONMENT CNTRL TBL */ 00038000
