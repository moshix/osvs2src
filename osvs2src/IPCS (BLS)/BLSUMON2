BLSUMON2 CSECT ,                                                01S0002 00006000
@MAINENT DS    0H                                               01S0002 00007000
         USING *,@15                                            01S0002 00008000
         B     @PROLOG                                          01S0002 00009000
         DC    AL1(16)                                          01S0002 00010000
         DC    C'BLSUMON2  78.059'                              01S0002 00011000
         DROP  @15                                                      00012000
@PROLOG  STM   @14,@12,12(@13)                                  01S0002 00013000
         BALR  @12,0                                            01S0002 00014000
@PSTART  DS    0H                                               01S0002 00015000
         USING @PSTART,@12                                      01S0002 00016000
         L     @00,@SIZDATD                                     01S0002 00017000
         BLSUALLS R,LV=(0)                                              00018000
         LR    @11,@01                                          01S0002 00019000
         USING @DATD,@11                                        01S0002 00020000
         ST    @13,@SA00001+4                                   01S0002 00021000
         LM    @00,@01,20(@13)                                  01S0002 00022000
         ST    @11,8(,@13)                                      01S0002 00023000
         LR    @13,@11                                          01S0002 00024000
         MVC   @PC00001(8),0(@01)                               01S0002 00025000
*/*------------------------------------------------------------------*/ 00026000
*/* BLSUMON2--COMMAND SCAN INTERFACE SUBROUTINE                      */ 00027000
*/*------------------------------------------------------------------*/ 00028000
*                                                               01S0080 00029000
*   CSPLUPT=ADDR(UPT);              /* ->USER PROFILE TABLE          */ 00030000
         L     @10,@PC00001+4                                   01S0080 00031000
         MVC   CSPLUPT(4,@10),CPPLUPT(R9)                       01S0080 00032000
*   CSPLECT=ADDR(ECT);              /* ->ENVIRONMENT CONTROL TABLE   */ 00033000
         MVC   CSPLECT(4,@10),CPPLECT(R9)                       01S0081 00034000
*   CSPLECB=ADDR(ZZZZNULL);         /* NO ECB                        */ 00035000
         LA    @08,@CF00051                                     01S0082 00036000
         ST    @08,CSPLECB(,@10)                                01S0082 00037000
*   CSPLFLG=ADDR(ZZZZNULL);         /* ->COMMAND SCAN OPTIONS FLAGS  */ 00038000
         ST    @08,CSPLFLG(,@10)                                01S0083 00039000
*   RESPECIFY                                                   01S0084 00040000
*    (R1) RESTRICTED;               /* ->CSPL                        */ 00041000
*   R1=ADDR(CSPL);                  /* ->CSPL                        */ 00042000
         LR    R1,@10                                           01S0085 00043000
*   RESPECIFY                                                   01S0086 00044000
*    (RF) RESTRICTED;               /* RETURN CODE                   */ 00045000
*   CALL IKJSCAN;                   /* COMMAND SCAN TSO SERVICE RTN. */ 00046000
         L     @15,ZZ2SCANP(,R9)                                01S0087 00047000
         BALR  @14,@15                                          01S0087 00048000
*   RESPECIFY                                                   01S0088 00049000
*    (R1) UNRESTRICTED;             /* ->CSPL                        */ 00050000
*   CSOAPTR=CSPLOA;                 /* ->COMMAND SCAN OUTPUT AREA    */ 00051000
         L     @14,@PC00001+4                                   01S0089 00052000
         L     CSOAPTR,CSPLOA(,@14)                             01S0089 00053000
*   IF                              /* COMMAND SCAN ERROR            */ 00054000
*/*     ONLY REASONABLE RETURN CODE IS ZERO                          */ 00055000
*                                                               01S0090 00056000
*       RF^=ZZZFLAGI|               /* UNEXPECTED RETURN CODE        */ 00057000
*/*     QUESTION MARKS ARE NORMALLY INTERCEPTED BY PUTGET            */ 00058000
*                                                               01S0090 00059000
*       CSOAQM='1'B THEN            /* QUESTION MARK NOT INTERCEPTED */ 00060000
*                                                               01S0090 00061000
         LTR   RF,RF                                            01S0090 00062000
         BNZ   @RT00090                                         01S0090 00063000
         TM    CSOAQM(CSOAPTR),B'00100000'                      01S0090 00064000
         BNO   @RF00090                                         01S0090 00065000
@RT00090 DS    0H                                               01S0091 00066000
*     /***************************************************************/ 00067000
*     /*                                                             */ 00068000
*     /* COMMAND SCAN ERROR                                          */ 00069000
*     /*                                                             */ 00070000
*     /***************************************************************/ 00071000
*                                                               01S0091 00072000
*     DO;                           /* COMMAND SCAN ERROR            */ 00073000
*       CALL BLSUTRMV(ZZ2,M055);    /* TRANSMIT MESSAGE M055         */ 00074000
         ST    R9,@AL00001                                      01S0092 00075000
         LA    @14,M055                                         01S0092 00076000
         ST    @14,@AL00001+4                                   01S0092 00077000
         L     @15,ZZ2TRMVP(,R9)                                01S0092 00078000
         LA    @01,@AL00001                                     01S0092 00079000
         BALR  @14,@15                                          01S0092 00080000
*       RETC=ZZZFLAGT;              /* RETURN CODE                   */ 00081000
         LA    RETC,16                                          01S0093 00082000
*     END;                          /* COMMAND SCAN ERROR            */ 00083000
*   ELSE                            /* VALID COMMAND SCAN RESULTS    */ 00084000
*     IF CSOABAD='1'B THEN          /* INVALID COMMAND SYNTAX        */ 00085000
*                                                               01S0095 00086000
         B     @RC00090                                         01S0095 00087000
@RF00090 TM    CSOABAD(CSOAPTR),B'00001000'                     01S0095 00088000
         BNO   @RF00095                                         01S0095 00089000
*       /*************************************************************/ 00090000
*       /*                                                           */ 00091000
*       /* INVALID COMMAND SYNTAX                                    */ 00092000
*       /*                                                           */ 00093000
*       /*************************************************************/ 00094000
*                                                               01S0096 00095000
*       DO;                         /* INVALID COMMAND SYNTAX        */ 00096000
*         CALL BLSUTRMV(ZZ2,M056);  /* TRAMSMIT MESSAGE M056         */ 00097000
         ST    R9,@AL00001                                      01S0097 00098000
         LA    @14,M056                                         01S0097 00099000
         ST    @14,@AL00001+4                                   01S0097 00100000
         L     @15,ZZ2TRMVP(,R9)                                01S0097 00101000
         LA    @01,@AL00001                                     01S0097 00102000
         BALR  @14,@15                                          01S0097 00103000
*         RETC=ZZZFLAGS;            /* RETURN CODE                   */ 00104000
         LA    RETC,12                                          01S0098 00105000
*       END;                        /* INVALID COMMAND SYNTAX        */ 00106000
*     ELSE                          /* WELL-FORMED COMMAND           */ 00107000
*       RETC=ZZZFLAGI;              /* RETURN CODE                   */ 00108000
         B     @RC00095                                         01S0100 00109000
@RF00095 SLR   RETC,RETC                                        01S0100 00110000
*   RETURN CODE(RETC);                                          01S0101 00111000
*                                                               01S0101 00112000
@RC00095 DS    0H                                               01S0101 00113000
@RC00090 L     @13,4(,@13)                                      01S0101 00114000
         L     @00,@SIZDATD                                     01S0101 00115000
         LR    @01,@11                                          01S0101 00116000
         BLSUFRES R,LV=(0),A=(1)                                        00117000
         LR    @15,@02                                          01S0101 00118000
         L     @14,12(,@13)                                     01S0101 00119000
         LM    @00,@12,20(@13)                                  01S0101 00120000
         BR    @14                                              01S0101 00121000
*/*BLSUPEND--MVS IPCS MODULE                                         */ 00122000
*                                                               01S0102 00123000
*   DECLARE                         /* COMMON VARIABLES              */ 00124000
*     I256C CHAR(256) BASED,                                    01S0102 00125000
*     I031F FIXED(31) BASED,                                    01S0102 00126000
*     I031P PTR(31) BASED,                                      01S0102 00127000
*     I015F FIXED(15) BASED,                                    01S0102 00128000
*     I015P PTR(15) BASED,                                      01S0102 00129000
*     I008P PTR(8) BASED,                                       01S0102 00130000
*     I001C CHAR(1) BASED;                                      01S0102 00131000
*   GENERATE NODEFS NOREFS DATA;                                01S0103 00132000
*   END BLSUMON2                    /* BLRPEND(BLSUMON2)             */ 00133000
