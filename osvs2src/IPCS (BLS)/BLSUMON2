*                                                               01S0104 00147000
*       ;                                                       01S0104 00148000
@EL00001 L     @13,4(,@13)                                      01S0104 00149000
@EF00001 L     @00,@SIZDATD                                     01S0104 00150000
         LR    @01,@11                                          01S0104 00151000
         BLSUFRES R,LV=(0),A=(1)                                        00152000
@ER00001 LM    @14,@12,12(@13)                                  01S0104 00153000
         BR    @14                                              01S0104 00154000
@DATA    DS    0H                                                       00155000
@DATD    DSECT                                                          00156000
         DS    0F                                                       00157000
@SA00001 DS    18F                                                      00158000
@PC00001 DS    2F                                                       00159000
@AL00001 DS    2A                                                       00160000
BLSUMON2 CSECT                                                          00161000
         DS    0F                                                       00162000
@CF00051 DC    F'0'                                                     00163000
@DATD    DSECT                                                          00164000
         DS    0D                                                       00165000
BLSUMON2 CSECT                                                          00166000
         DS    0F                                                       00167000
@SIZDATD DC    AL1(0)                                                   00168000
         DC    AL3(@ENDDATD-@DATD)                                      00169000
         DS    0D                                                       00170000
M055     DS    CL32                                                     00171000
         ORG   M055                                                     00172000
M055L    DC    AL2(32)                                                  00173000
@NM00072 DC    X'0000'                                                  00174000
M055T    DS    CL28                                                     00175000
         ORG   M055T                                                    00176000
M055TA   DC    CL28'BLS21055I COMMAND SCAN ERROR'                       00177000
         ORG   M055+32                                                  00178000
M056     DS    CL36                                                     00179000
         ORG   M056                                                     00180000
M056L    DC    AL2(36)                                                  00181000
@NM00073 DC    X'0000'                                                  00182000
M056T    DS    CL32                                                     00183000
         ORG   M056T                                                    00184000
M056TA   DC    CL32'BLS21056I INVALID COMMAND SYNTAX'                   00185000
         ORG   M056+36                                                  00186000
@DATD    DSECT                                                          00187000
         SPACE 2                                                        00188000
*********************************************************************** 00189000
* THE FOLLOWING AREA, BLRPATCH, IS RESERVED FOR PATCH APPLICATION     * 00190000
* TO OBTAIN PATCH ROOM IN THE @DATD AUTOMATIC STORAGE AREA, MODIFY    * 00191000
