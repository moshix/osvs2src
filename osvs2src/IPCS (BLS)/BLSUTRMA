         TM    ZZ1EVEP(@14),B'01000000'                         01S0118 00036000
         BNZ   @RF00118                                         01S0118 00037000
*     /***************************************************************/ 00038000
*     /*                                                             */ 00039000
*     /* NO TERMINATION REQUEST PENDING, NOT IN FULL SCREEN MODE     */ 00040000
*     /*                                                             */ 00041000
*     /***************************************************************/ 00042000
*                                                               01S0119 00043000
*     DO;                           /* NO TERMINATION, NO FULL SCREEN*/ 00044000
*       IF MSGPT0=' ' THEN          /* SINGLE SPACE                  */ 00045000
