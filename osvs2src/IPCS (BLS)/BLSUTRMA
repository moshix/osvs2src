         LA    @01,@AL00001                                     01S0131 00106000
         BALR  @14,@15                                          01S0131 00107000
*             END;                  /* BLANK LINES                   */ 00108000
@RF00130 DS    0H                                               01S0133 00109000
*           MSGAL=MSGPL;            /* RESET ORIGINAL MSG LENGTH     */ 00110000
@RF00126 L     @14,@PC00001+4                                   01S0133 00111000
         LH    @10,MSGPL(,@14)                                  01S0133 00112000
         N     @10,@CF00783                                     01S0133 00113000
         STH   @10,MSGAL                                        01S0133 00114000
*           MSGAT1(1:MSGAL-5)=MSGPT1(1:MSGAL-5);/* TEXT              */ 00115000
         SL    @10,@CF00160                                     01S0134 00116000
         EX    @10,@SM00784                                     01S0134 00117000
*           CALL BLSUTRMV(ZZ2,MSGA);/* SEND THE REQUESTED MESSAGE    */ 00118000
