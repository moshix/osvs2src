*       ELSE                                                    01S0122 00055000
*                                                               01S0122 00056000
*         /***********************************************************/ 00057000
*         /*                                                         */ 00058000
*         /* CONSTRUCT MESSAGE BUFFER FOR PUTLINE                    */ 00059000
*         /*                                                         */ 00060000
*         /***********************************************************/ 00061000
*                                                               01S0122 00062000
*         DO;                       /* CONSTRUCT MESSAGE FOR PUTLINE */ 00063000
         B     @RC00120                                         01S0122 00064000
@RF00120 DS    0H                                               01S0123 00065000
*           MSGAL=6;                /* MESSAGE LENGTH                */ 00066000
         MVC   MSGAL(2),@CH00160                                01S0123 00067000
*           MSGAO=0;                /* OFFSET                        */ 00068000
         SLR   @14,@14                                          01S0124 00069000
         STH   @14,MSGAO                                        01S0124 00070000
*           MSGAT0=' ';             /* SINGLE SPACE                  */ 00071000
         MVI   MSGAT0,C' '                                      01S0125 00072000
*           IF MSGPT0='0'|MSGPT0='-' THEN/* DOUBLE OR TRIPLE SPACE   */ 00073000
*                                                               01S0126 00074000
         L     @14,@PC00001+4                                   01S0126 00075000
         CLI   MSGPT0(@14),C'0'                                 01S0126 00076000
         BE    @RT00126                                         01S0126 00077000
         CLI   MSGPT0(@14),C'-'                                 01S0126 00078000
         BNE   @RF00126                                         01S0126 00079000
@RT00126 DS    0H                                               01S0127 00080000
*             /*******************************************************/ 00081000
*             /*                                                     */ 00082000
*             /* SEND ONE OR TWO BLANK LINES                         */ 00083000
*             /*                                                     */ 00084000
*             /*******************************************************/ 00085000
*                                                               01S0127 00086000
*             DO;                   /* BLANK LINES                   */ 00087000
*               MSGAT1(1:1)=' ';    /* INITIALIZE MSG                */ 00088000
         MVI   MSGAT1,C' '                                      01S0128 00089000
*               CALL BLSUTRMV(ZZ2,MSGA);/* SEND ONE LINE             */ 00090000
