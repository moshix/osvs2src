         BO    @RT00290                                            0290 00529000
*     GO TO PCHKEXIT;               /* COMMON EXIT PATH              */ 00530000
*   SAAULAD=FVAL(ERRFNDX-DIM(MSGPTR));/* INSERT VALUE                */ 00531000
         LR    @14,ERRFNDX                                         0292 00532000
         SLA   @14,2                                               0292 00533000
         L     @14,FVAL-20(@14)                                    0292 00534000
         ST    @14,SAAULAD                                         0292 00535000
*   IF PCHK24=0 THEN                /* NULL LOCATOR                  */ 00536000
*                                                                  0293 00537000
