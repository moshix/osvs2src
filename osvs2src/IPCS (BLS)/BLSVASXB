*BLSRPCK1:                          /* EVALUATE A REQUIRED LOCATOR   */ 00515000
*   PROCEDURE;                                                     0287 00516000
BLSRPCK1 STM   @14,@03,@SA00002                                    0287 00517000
         STM   @05,@12,@SA00002+24                                 0287 00518000
*   DECLARE                                                        0288 00519000
*     1 PCHK POINTER DEFINED(SAAULAD),/* LOCATOR VALIDATION DATA     */ 00520000
*      2 PCHK24IG BIT(8),           /* IGNORED IN 24-BIT ADDRESS     */ 00521000
*      2 PCHK24 POINTER(24);        /* 24-BIT INTERPRETATION         */ 00522000
*   ERLF=ERRF(ERRFNDX);             /* ERROR FLAGS                   */ 00523000
