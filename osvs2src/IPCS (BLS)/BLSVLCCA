*/* BLSRPCK1--EVALUATE A REQUIRED LOCATOR                            */ 00471000
*/*------------------------------------------------------------------*/ 00472000
*                                                                  0262 00473000
*BLSRPCK1:                          /* EVALUATE A REQUIRED LOCATOR   */ 00474000
*   PROCEDURE;                                                     0262 00475000
BLSRPCK1 STM   @14,@12,@SA00002                                    0262 00476000
*   DECLARE                                                        0263 00477000
*     1 PCHK POINTER DEFINED(SAAULAD),/* LOCATOR VALIDATION DATA     */ 00478000
*      2 PCHK24IG BIT(8),           /* IGNORED IN 24-BIT ADDRESS     */ 00479000
*      2 PCHK24 POINTER(24);        /* 24-BIT INTERPRETATION         */ 00480000
*   ERLF=ERRF(ERRFNDX);             /* ERROR FLAGS                   */ 00481000
         LR    @14,SABAPTR                                         0264 00482000
         ALR   @14,ERRFNDX                                         0264 00483000
         MVC   ERLF(1),ERRF-1(@14)                                 0264 00484000
*   IF ERLFD=ZZZ1 THEN              /* LOCATOR TEST COMPLETE         */ 00485000
         TM    ERLFD,B'01000000'                                   0265 00486000
         BO    @RT00265                                            0265 00487000
*     GO TO PCHKEXIT;               /* COMMON EXIT PATH              */ 00488000
*   SAAULAD=FVAL(ERRFNDX-DIM(MSGPTR));/* INSERT VALUE                */ 00489000
         LR    @14,ERRFNDX                                         0267 00490000
         SLA   @14,2                                               0267 00491000
         L     @14,FVAL-16(@14)                                    0267 00492000
         ST    @14,SAAULAD                                         0267 00493000
*   IF PCHK24=0 THEN                /* NULL LOCATOR                  */ 00494000
*                                                                  0268 00495000
         L     @14,PCHK24-1                                        0268 00496000
         LA    @14,0(,@14)                                         0268 00497000
         LTR   @14,@14                                             0268 00498000
         BNZ   @RF00268                                            0268 00499000
*     /***************************************************************/ 00500000
*     /*                                                             */ 00501000
*     /* NULL LOCATOR                                                */ 00502000
*     /*                                                             */ 00503000
*     /***************************************************************/ 00504000
*                                                                  0269 00505000
*     DO;                           /* NULL LOCATOR                  */ 00506000
*       SABASRC=ZZZFLAGE;           /* SCAN RETURN CODE              */ 00507000
         MVI   SABASRC(SABAPTR),X'08'                              0270 00508000
*       SABASFS=ZZZ1;               /* STORAGE REQUIRED FOR        0271 00509000
*                                      MESSAGE(S)                    */ 00510000
         OI    SABASFS(SABAPTR),B'00010000'                        0271 00511000
*       ERLFB=ZZZ1;                 /* ERROR FLAG                    */ 00512000
*       ERLFN=ZZZ1;                 /* NULL LOCATOR                  */ 00513000
*       ERLFD=ZZZ1;                 /* LOCATOR TEST COMPLETE         */ 00514000
         OI    ERLFB,B'11100000'                                   0274 00515000
*       SABAGMT=ZZ2TOD;             /* REQUEST DATA BASE UPDATE      */ 00516000
         MVC   SABAGMT(8,SABAPTR),ZZ2TOD(R9)                       0275 00517000
*       GO TO PCHKEXIT;             /* COMMON EXIT PATH              */ 00518000
         B     PCHKEXIT                                            0276 00519000
*     END;                          /* NULL LOCATOR                  */ 00520000
*   IF PCHK24=0 THEN                /* NULL LOCATOR                  */ 00521000
@RF00268 L     @14,PCHK24-1                                        0278 00522000
         LA    @14,0(,@14)                                         0278 00523000
         LTR   @14,@14                                             0278 00524000
         BNZ   @RF00278                                            0278 00525000
*     ERLFN=ZZZ1;                   /* NULL LOCATOR                  */ 00526000
         OI    ERLFN,B'00100000'                                   0279 00527000
*   ELSE                            /* LOCATOR NOT NULL              */ 00528000
*                                                                  0280 00529000
*     /***************************************************************/ 00530000
*     /*                                                             */ 00531000
*     /* LOCATOR NOT NULL                                            */ 00532000
*     /*                                                             */ 00533000
*     /***************************************************************/ 00534000
*                                                                  0280 00535000
*     DO;                           /* LOCATOR NOT NULL              */ 00536000
         B     @RC00278                                            0280 00537000
@RF00278 DS    0H                                                  0281 00538000
*       PCHK24IG='00'X;             /* ELIMINATE EXTRANEOUS BITS     */ 00539000
         MVI   PCHK24IG,X'00'                                      0281 00540000
*       CALL BLSRSAG(ZZ2,SAAU,SABA);/* PROCESS THE SA RECORD         */ 00541000
         ST    R9,@AL00001                                         0282 00542000
         LA    @14,SAAU                                            0282 00543000
         ST    @14,@AL00001+4                                      0282 00544000
         ST    SABAPTR,@AL00001+8                                  0282 00545000
         L     @08,ZZ2RVTP(,R9)                                    0282 00546000
         L     @15,RVTRSAGP(,@08)                                  0282 00547000
         LA    @01,@AL00001                                        0282 00548000
         BALR  @14,@15                                             0282 00549000
*       IF SAAUSF1=ZZZ0 THEN        /* SCAN STARTED                  */ 00550000
*                                                                  0283 00551000
         TM    SAAUSF1,B'10000000'                                 0283 00552000
         BNZ   @RF00283                                            0283 00553000
*         /***********************************************************/ 00554000
*         /*                                                         */ 00555000
*         /* LOCATOR TEST INCOMPLETE                                 */ 00556000
*         /*                                                         */ 00557000
*         /***********************************************************/ 00558000
*                                                                  0284 00559000
*         DO;                       /* LOCATOR TEST INCOMPLETE       */ 00560000
*           SABASF9=ZZZ0;           /* SCAN NOT COMPLETE             */ 00561000
         NI    SABASF9(SABAPTR),B'10111111'                        0285 00562000
*           GO TO PCHKEXIT;         /* COMMON EXIT PATH              */ 00563000
         B     PCHKEXIT                                            0286 00564000
*         END;                      /* LOCATOR TEST INCOMPLETE       */ 00565000
*       IF SAAUSRC>ZZZFLAGE THEN    /* LOCATOR ERROR                 */ 00566000
*                                                                  0288 00567000
@RF00283 CLI   SAAUSRC,8                                           0288 00568000
         BNH   @RF00288                                            0288 00569000
*         /***********************************************************/ 00570000
*         /*                                                         */ 00571000
*         /* LOCATOR ERROR                                           */ 00572000
*         /*                                                         */ 00573000
*         /***********************************************************/ 00574000
*                                                                  0289 00575000
*         DO;                       /* LOCATOR ERROR                 */ 00576000
*           SABASRC=ZZZFLAGE;       /* SCAN RETURN CODE              */ 00577000
         MVI   SABASRC(SABAPTR),X'08'                              0290 00578000
*           SABASFS=ZZZ1;           /* STORAGE REQUIRED FOR        0291 00579000
*                                      MESSAGE(S)                    */ 00580000
         OI    SABASFS(SABAPTR),B'00010000'                        0291 00581000
*           ERLFB=ZZZ1;             /* LOCATOR ERROR                 */ 00582000
