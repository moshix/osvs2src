         L     @15,BVTPUTVP(,@08)                                  0256 00440000
         LA    @01,@AL00001                                        0256 00441000
         BALR  @14,@15                                             0256 00442000
*             END;                  /* DAMAGED POINTER               */ 00443000
*         END;                      /* DIAGNOSE DAMAGED POINTERS     */ 00444000
@RF00244 AL    ERRFNDX,@CF00051                                    0258 00445000
         C     ERRFNDX,@CF00048                                    0258 00446000
         BNH   @DL00243                                            0258 00447000
*     END;                          /* BLSV8                         */ 00448000
*                                                                  0259 00449000
