*                                      INTERMEDIATE                  */ 00406000
         LR    @14,FNAMENDX                                        0250 00407000
         SLA   @14,2                                               0250 00408000
         L     @14,FVAL-4(@14)                                     0250 00409000
         ST    @14,PTRPTR                                          0250 00410000
*                 UNPK(MV99BTF(1:LENGTH(MV99BTF)-1),PTRBIT);/*     0251 00411000
*                                      BIT=>ZONED HEXADECIMAL        */ 00412000
         UNPK  MV99BTF(7),PTRBIT(4)                                0251 00413000
*                 MV99BTF(LENGTH(MV99BTF):LENGTH(MV99BTF))=PTRBIT(25:32 00414000
*                     );            /* FINAL ZONED HEXADECIMAL DIGIT */ 00415000
         MVC   MV99BTF+7(1),PTRBIT+3                               0252 00416000
*                 TR(MV99BTF,ZZ1TRHEX);/* ZONED HEXADECIMAL=>EBCDIC  */ 00417000
         L     @14,ZZ2ZZ1P(,R9)                                    0253 00418000
         TR    MV99BTF(8),ZZ1TRHEX(@14)                            0253 00419000
*               END;                /* BLRCNVPC(FVAL(FNAMENDX)) FROM(   00420000
*                                      POINTER)TO(CHARACTER)SET(MV99B   00421000
*                                      TF)REMARK(CONTENTS OF DAMAGED    00422000
*                                      POINTER)TR(ZZ1TRHEX)          */ 00423000
*               CALL BLSUMPKN(ZZ2,MV99B,LENGTH(MV99BTA)+LENGTH(MV99BTB) 00424000
*                   );              /* COMPRESS MESSAGE MV99         */ 00425000
         ST    R9,@AL00001                                         0255 00426000
         LA    @14,MV99B                                           0255 00427000
         ST    @14,@AL00001+4                                      0255 00428000
         LA    @14,@CF00037                                        0255 00429000
         ST    @14,@AL00001+8                                      0255 00430000
