*         END;                      /* LOCATOR ERROR                 */ 00584000
*     END;                          /* LOCATOR NOT NULL              */ 00585000
@RF00288 DS    0H                                                  0295 00586000
*   ERLFD=ZZZ1;                     /* LOCATOR TEST COMPLETE         */ 00587000
@RC00278 OI    ERLFD,B'01000000'                                   0295 00588000
*   SABAGMT=ZZ2TOD;                 /* REQUEST DATA BASE UPDATE      */ 00589000
         MVC   SABAGMT(8,SABAPTR),ZZ2TOD(R9)                       0296 00590000
*PCHKEXIT:                          /* COMMON EXIT PATH              */ 00591000
*   ERRF(ERRFNDX)=ERLF;             /* ERROR FLAGS                   */ 00592000
