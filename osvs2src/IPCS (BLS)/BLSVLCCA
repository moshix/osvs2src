         ALR   @14,ERRFNDX                                         0297 00594000
         MVC   ERRF-1(1,@14),ERLF                                  0297 00595000
*   ERRFNDX=ERRFNDX+1;              /* ERROR INDEX                   */ 00596000
         AL    ERRFNDX,@CF00051                                    0298 00597000
*   RETURN;                                                        0299 00598000
@EL00002 DS    0H                                                  0299 00599000
@EF00002 DS    0H                                                  0299 00600000
@ER00002 LM    @14,@12,@SA00002                                    0299 00601000
         BR    @14                                                 0299 00602000
*   END BLSRPCK1;                                                  0300 00603000
*                                                                  0300 00604000
