*             /*                                                     */ 00404000
*             /*******************************************************/ 00405000
*                                                                  0269 00406000
*             DO;                   /* DAMAGED POINTER               */ 00407000
*               FNAMENDX=ERRFNDX-DIM(MSGPTR);/* FNAME/FVAL INDEX     */ 00408000
         LR    FNAMENDX,ERRFNDX                                    0270 00409000
         BCTR  FNAMENDX,0                                          0270 00410000
         BCTR  FNAMENDX,0                                          0270 00411000
*               MV99B=MV99S;        /* MESSAGE MV99 LITERAL TEXT     */ 00412000
