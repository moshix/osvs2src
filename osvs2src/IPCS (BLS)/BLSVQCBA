         ALR   @14,ERRFNDX                                         0264 00399000
         TM    ERRFB-1(@14),B'10000000'                            0264 00400000
         BNO   @RF00264                                            0264 00401000
*             /*******************************************************/ 00402000
*             /*                                                     */ 00403000
*             /* DAMAGED POINTER                                     */ 00404000
*             /*                                                     */ 00405000
*             /*******************************************************/ 00406000
*                                                                  0265 00407000
*             DO;                   /* DAMAGED POINTER               */ 00408000
*               FNAMENDX=ERRFNDX-DIM(MSGPTR);/* FNAME/FVAL INDEX     */ 00409000
         LR    FNAMENDX,ERRFNDX                                    0266 00410000
         SL    FNAMENDX,@CF00048                                   0266 00411000
*               MV99B=MV99S;        /* MESSAGE MV99 LITERAL TEXT     */ 00412000
         MVC   MV99B(59),MV99S                                     0267 00413000
*               MV99BTD=FNAME(FNAMENDX);/* NAME OF DAMAGED POINTER   */ 00414000
         LR    @14,FNAMENDX                                        0268 00415000
