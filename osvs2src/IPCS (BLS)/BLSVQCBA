         LA    @08,FNAME-7(@14)                                    0268 00417000
         MVC   MV99BTD(7),0(@08)                                   0268 00418000
*               DO;                 /* BLRCNVPC(FVAL(FNAMENDX)) FROM(   00419000
*                                      POINTER)TO(CHARACTER)SET(MV99B   00420000
*                                      TF)REMARK(CONTENTS OF DAMAGED    00421000
*                                      POINTER)TR(ZZ1TRHEX)          */ 00422000
*                                                                  0269 00423000
*                 /***************************************************/ 00424000
*                 /*                                                 */ 00425000
*                 /* CONTENTS OF DAMAGED POINTER                     */ 00426000
*                 /*                                                 */ 00427000
*                 /***************************************************/ 00428000
*                                                                  0270 00429000
*                 PTRPTR=FVAL(FNAMENDX);/* SOURCE TO BIT/POINTER   0270 00430000
