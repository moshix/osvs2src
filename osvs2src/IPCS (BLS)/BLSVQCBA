*BLSRPCK2:                          /* EVALUATE AN OPTIONAL LOCATOR  */ 00499000
*   PROCEDURE;                                                     0282 00500000
BLSRPCK2 STM   @14,@03,@SA00002                                    0282 00501000
         STM   @05,@12,@SA00002+24                                 0282 00502000
*   DECLARE                                                        0283 00503000
*     1 PCHK POINTER DEFINED(SAAULAD),/* LOCATOR VALIDATION DATA     */ 00504000
*      2 PCHK24IG BIT(8),           /* IGNORED IN 24-BIT ADDRESS     */ 00505000
*      2 PCHK24 POINTER(24);        /* 24-BIT INTERPRETATION         */ 00506000
*   ERLF=ERRF(ERRFNDX);             /* ERROR FLAGS                   */ 00507000
         LR    @14,SABAPTR                                         0284 00508000
         ALR   @14,ERRFNDX                                         0284 00509000
         MVC   ERLF(1),ERRF-1(@14)                                 0284 00510000
*   IF ERLFD=ZZZ1 THEN              /* LOCATOR TEST COMPLETE         */ 00511000
         TM    ERLFD,B'01000000'                                   0285 00512000
         BO    @RT00285                                            0285 00513000
*     GO TO PCHKEXIT;               /* COMMON EXIT PATH              */ 00514000
*   SAAULAD=FVAL(ERRFNDX-DIM(MSGPTR));/* INSERT VALUE                */ 00515000
         LR    @14,ERRFNDX                                         0287 00516000
         SLA   @14,2                                               0287 00517000
         L     @14,FVAL-20(@14)                                    0287 00518000
         ST    @14,SAAULAD                                         0287 00519000
*   IF PCHK24=0 THEN                /* NULL LOCATOR                  */ 00520000
         L     @14,PCHK24-1                                        0288 00521000
         LA    @14,0(,@14)                                         0288 00522000
