         BNZ   @RF00288                                            0288 00524000
*     ERLFN=ZZZ1;                   /* NULL LOCATOR                  */ 00525000
         OI    ERLFN,B'00100000'                                   0289 00526000
*   ELSE                            /* LOCATOR NOT NULL              */ 00527000
*                                                                  0290 00528000
*     /***************************************************************/ 00529000
*     /*                                                             */ 00530000
*     /* LOCATOR NOT NULL                                            */ 00531000
*     /*                                                             */ 00532000
*     /***************************************************************/ 00533000
*                                                                  0290 00534000
*     DO;                           /* LOCATOR NOT NULL              */ 00535000
         B     @RC00288                                            0290 00536000
@RF00288 DS    0H                                                  0291 00537000
*       PCHK24IG='00'X;             /* ELIMINATE EXTRANEOUS BITS     */ 00538000
         MVI   PCHK24IG,X'00'                                      0291 00539000
*       CALL BLSRSAG(ZZ2,SAAU,SABA);/* PROCESS THE SA RECORD         */ 00540000
