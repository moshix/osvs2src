         LA    @01,@AL00001                                        0281 00527000
         BALR  @14,@15                                             0281 00528000
*       IF SAAUSF1=ZZZ0 THEN        /* SCAN STARTED                  */ 00529000
*                                                                  0282 00530000
         TM    SAAUSF1,B'10000000'                                 0282 00531000
         BNZ   @RF00282                                            0282 00532000
*         /***********************************************************/ 00533000
*         /*                                                         */ 00534000
*         /* LOCATOR TEST INCOMPLETE                                 */ 00535000
*         /*                                                         */ 00536000
*         /***********************************************************/ 00537000
*                                                                  0283 00538000
*         DO;                       /* LOCATOR TEST INCOMPLETE       */ 00539000
*           SABASF9=ZZZ0;           /* SCAN NOT COMPLETE             */ 00540000
         NI    SABASF9(SABAPTR),B'10111111'                        0284 00541000
*           GO TO PCHKEXIT;         /* COMMON EXIT PATH              */ 00542000
         B     PCHKEXIT                                            0285 00543000
*         END;                      /* LOCATOR TEST INCOMPLETE       */ 00544000
*       IF SAAUSRC>ZZZFLAGE THEN    /* LOCATOR ERROR                 */ 00545000
*                                                                  0287 00546000
@RF00282 CLI   SAAUSRC,8                                           0287 00547000
         BNH   @RF00287                                            0287 00548000
*         /***********************************************************/ 00549000
*         /*                                                         */ 00550000
*         /* LOCATOR ERROR                                           */ 00551000
*         /*                                                         */ 00552000
*         /***********************************************************/ 00553000
*                                                                  0288 00554000
*         DO;                       /* LOCATOR ERROR                 */ 00555000
*           SABASRC=ZZZFLAGE;       /* SCAN RETURN CODE              */ 00556000
