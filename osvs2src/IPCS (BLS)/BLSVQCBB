*                                                                  0271 00478000
*BLSRPCK2:                          /* EVALUATE AN OPTIONAL LOCATOR  */ 00479000
*   PROCEDURE;                                                     0271 00480000
BLSRPCK2 STM   @14,@03,@SA00002                                    0271 00481000
         STM   @05,@12,@SA00002+24                                 0271 00482000
*   DECLARE                                                        0272 00483000
*     1 PCHK POINTER DEFINED(SAAULAD),/* LOCATOR VALIDATION DATA     */ 00484000
*      2 PCHK24IG BIT(8),           /* IGNORED IN 24-BIT ADDRESS     */ 00485000
*      2 PCHK24 POINTER(24);        /* 24-BIT INTERPRETATION         */ 00486000
*   ERLF=ERRF(ERRFNDX);             /* ERROR FLAGS                   */ 00487000
         LR    @14,SABAPTR                                         0273 00488000
         ALR   @14,ERRFNDX                                         0273 00489000
         MVC   ERLF(1),ERRF-1(@14)                                 0273 00490000
*   IF ERLFD=ZZZ1 THEN              /* LOCATOR TEST COMPLETE         */ 00491000
         TM    ERLFD,B'01000000'                                   0274 00492000
         BO    @RT00274                                            0274 00493000
*     GO TO PCHKEXIT;               /* COMMON EXIT PATH              */ 00494000
*   SAAULAD=FVAL(ERRFNDX-DIM(MSGPTR));/* INSERT VALUE                */ 00495000
         LR    @14,ERRFNDX                                         0276 00496000
         SLA   @14,2                                               0276 00497000
         L     @14,FVAL-16(@14)                                    0276 00498000
         ST    @14,SAAULAD                                         0276 00499000
*   IF PCHK24=0 THEN                /* NULL LOCATOR                  */ 00500000
         L     @14,PCHK24-1                                        0277 00501000
         LA    @14,0(,@14)                                         0277 00502000
         LTR   @14,@14                                             0277 00503000
         BNZ   @RF00277                                            0277 00504000
*     ERLFN=ZZZ1;                   /* NULL LOCATOR                  */ 00505000
         OI    ERLFN,B'00100000'                                   0278 00506000
*   ELSE                            /* LOCATOR NOT NULL              */ 00507000
*                                                                  0279 00508000
*     /***************************************************************/ 00509000
*     /*                                                             */ 00510000
*     /* LOCATOR NOT NULL                                            */ 00511000
*     /*                                                             */ 00512000
*     /***************************************************************/ 00513000
*                                                                  0279 00514000
*     DO;                           /* LOCATOR NOT NULL              */ 00515000
         B     @RC00277                                            0279 00516000
@RF00277 DS    0H                                                  0280 00517000
*       PCHK24IG='00'X;             /* ELIMINATE EXTRANEOUS BITS     */ 00518000
         MVI   PCHK24IG,X'00'                                      0280 00519000
*       CALL BLSRSAG(ZZ2,SAAU,SABA);/* PROCESS THE SA RECORD         */ 00520000
