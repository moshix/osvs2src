*     END;                          /* LOCATOR NOT NULL              */ 00564000
@RF00287 DS    0H                                                  0294 00565000
*   ERLFD=ZZZ1;                     /* LOCATOR TEST COMPLETE         */ 00566000
@RC00277 OI    ERLFD,B'01000000'                                   0294 00567000
*   SABAGMT=ZZ2TOD;                 /* REQUEST DATA BASE UPDATE      */ 00568000
         MVC   SABAGMT(8,SABAPTR),ZZ2TOD(R9)                       0295 00569000
*PCHKEXIT:                          /* COMMON EXIT PATH              */ 00570000
