         ALR   @14,ERRFNDX                                         0296 00573000
         MVC   ERRF-1(1,@14),ERLF                                  0296 00574000
*   ERRFNDX=ERRFNDX+1;              /* ERROR INDEX                   */ 00575000
         AL    ERRFNDX,@CF00051                                    0297 00576000
*   RETURN;                                                        0298 00577000
@EL00002 DS    0H                                                  0298 00578000
@EF00002 DS    0H                                                  0298 00579000
@ER00002 LM    @14,@03,@SA00002                                    0298 00580000
         LM    @05,@12,@SA00002+24                                 0298 00581000
         BR    @14                                                 0298 00582000
*   END BLSRPCK2;                                                  0299 00583000
