*                                      POINTER)TO(CHARACTER)SET(MV99B   00400000
*                                      TF)REMARK(CONTENTS OF DAMAGED    00401000
*                                      POINTER)TR(ZZ1TRHEX)          */ 00402000
*                                                                  0258 00403000
*                 /***************************************************/ 00404000
*                 /*                                                 */ 00405000
*                 /* CONTENTS OF DAMAGED POINTER                     */ 00406000
*                 /*                                                 */ 00407000
*                 /***************************************************/ 00408000
*                                                                  0259 00409000
*                 PTRPTR=FVAL(FNAMENDX);/* SOURCE TO BIT/POINTER   0259 00410000
*                                      INTERMEDIATE                  */ 00411000
         LR    @14,FNAMENDX                                        0259 00412000
         SLA   @14,2                                               0259 00413000
         L     @14,FVAL-4(@14)                                     0259 00414000
         ST    @14,PTRPTR                                          0259 00415000
*                 UNPK(MV99BTF(1:LENGTH(MV99BTF)-1),PTRBIT);/*     0260 00416000
*                                      BIT=>ZONED HEXADECIMAL        */ 00417000
         UNPK  MV99BTF(7),PTRBIT(4)                                0260 00418000
*                 MV99BTF(LENGTH(MV99BTF):LENGTH(MV99BTF))=PTRBIT(25:32 00419000
*                     );            /* FINAL ZONED HEXADECIMAL DIGIT */ 00420000
         MVC   MV99BTF+7(1),PTRBIT+3                               0261 00421000
*                 TR(MV99BTF,ZZ1TRHEX);/* ZONED HEXADECIMAL=>EBCDIC  */ 00422000
         L     @14,ZZ2ZZ1P(,R9)                                    0262 00423000
         TR    MV99BTF(8),ZZ1TRHEX(@14)                            0262 00424000
*               END;                /* BLRCNVPC(FVAL(FNAMENDX)) FROM(   00425000
*                                      POINTER)TO(CHARACTER)SET(MV99B   00426000
*                                      TF)REMARK(CONTENTS OF DAMAGED    00427000
*                                      POINTER)TR(ZZ1TRHEX)          */ 00428000
*               CALL BLSUMPKN(ZZ2,MV99B,LENGTH(MV99BTA)+LENGTH(MV99BTB) 00429000
*                   );              /* COMPRESS MESSAGE MV99         */ 00430000
