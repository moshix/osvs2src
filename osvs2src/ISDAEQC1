*               CALL FREECORE;      /* FREE ALL GOTTEN CORE. @Y30LB51*/ 00069000
         BAL   @14,FREECORE                                        0039 00070000
*             END;                  /*                       @Y30LB51*/ 00071000
*           ELSE                    /* SOMETHING WRONG,      @Y30LB51*/ 00072000
*             DO;                   /* FREE ANY DYNAMIC STORAGE.   0041 00073000
*                                                            @Y30LB51*/ 00074000
         B     @RC00035                                            0041 00075000
@RF00035 DS    0H                                                  0042 00076000
*               CALL FREECORE;      /* GO FREE IT.           @Y30LB51*/ 00077000
         BAL   @14,FREECORE                                        0042 00078000
*               EQCRETCD=NOSPACE;   /* SET RETURN CODE.      @Y30LB51*/ 00079000
         LA    EQCRETCD,4                                          0043 00080000
*             END;                  /*                       @Y30LB51*/ 00081000
*         END;                      /*                       @Y30LB51*/ 00082000
*       ELSE                        /* NO DATA IN ACCUMULATOR,     0046 00083000
*                                                            @Y30LB51*/ 00084000
