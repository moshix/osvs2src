*                         CALL IEFAB49C(ADDR(VMVRQBLK),ZERO,ZERO);      00702000
         MVC   @AL00001+4(8),@AL00328                           02S0328 00703000
         LA    @15,VMVREQBP                                     02S0328 00704000
         ST    @15,@AL00001                                     02S0328 00705000
         L     @15,@CV00147                                     02S0328 00706000
         LA    @01,@AL00001                                     02S0328 00707000
         BALR  @14,@15                                          02S0328 00708000
*                         SAVERTNC=REG15;   /*SAVE RETURN CODE       */ 00709000
         ST    REG15,SAVERTNC                                   02S0329 00710000
*                         GEN REFS(ENQCORE4);  /*DEQ                 */ 00711000
                         DEQ MF=(E,ENQCORE4)                            00712000
*                         CHNGQ = OFF;   /*SET ESTAE INDICATOR       */ 00713000
         NI    CHNGQ(EXITP),B'11011111'                         02S0331 00714000
*                         IF SAVERTNC^=SUCCESS THEN /*UNLOAD OK?     */ 00715000
         L     @15,SAVERTNC                                     02S0332 00716000
         LTR   @15,@15                                          02S0332 00717000
         BZ    @RF00332                                         02S0332 00718000
*                           MSSUMSW=ON;     /*NO,INDICATE REPROCESS  */ 00719000
         OI    MSSUMSW,B'10000000'                              02S0333 00720000
*                         ELSE              /*UNLOAD SUCCESSFUL      */ 00721000
*                           DO;                                 02S0334 00722000
         B     @RC00332                                         02S0334 00723000
@RF00332 DS    0H                                               02S0335 00724000
*                               DO; /*MODESET  KEYADDR(ZERO)*/  02S0335 00725000
*                                 SPKA (ZERO); /*SET PSW KEY*/  02S0336 00726000
         SLR   @01,@01                                          02S0336 00727000
         SPKA  0(@01)                                           02S0336 00728000
*                               END; /*MODESET  KEYADDR(ZERO)*/ 02S0337 00729000
*                             UCBRESV=OFF; /*TURN OFF RSVD BIT       */ 00730000
*                             UCBUNLD=OFF; /*TURN OFF PENDING UNLOAD */ 00731000
         L     @15,UCBUNLD-3(,UCBPTR)                           02S0339 00732000
@GL00004 LR    @14,@15                                          02S0339 00733000
         N     @14,@CF03699                                     02S0339 00734000
         CS    @15,@14,UCBUNLD-3(UCBPTR)                        02S0339 00735000
         BNE   @GL00004                                         02S0339 00736000
*                               DO; /*MODESET  EXTKEY(SCHED)*/  02S0340 00737000
*                                 SPKA (16); /*SET PSW KEY*/    02S0341 00738000
         LA    @01,16                                           02S0341 00739000
         SPKA  0(@01)                                           02S0341 00740000
*                               END; /*MODESET  EXTKEY(SCHED)*/ 02S0342 00741000
*                             WTOLIST=UNLDMSG; /*PUT MSG TEXT IN BUF */ 00742000
         L     @15,@CA01681                                     02S0343 00743000
         MVC   WTOLIST(32),UNLDMSG-UNLDMSG(@15)                 02S0343 00744000
*                             CALL SETUPWTO; /*BUILD & WRITE UNLD MSG*/ 00745000
         BAL   @14,SETUPWTO                                     02S0344 00746000
*                           END;                                02S0345 00747000
*                      END;                                     02S0346 00748000
*                    ELSE                /*ALLOCATED AFTER ENQ       */ 00749000
*                      DO;                                      02S0347 00750000
         B     @RC00321                                         02S0347 00751000
@RF00321 DS    0H                                               02S0348 00752000
*                        MSSUMSW=ON;     /*INDICATE REPROCESS        */ 00753000
         OI    MSSUMSW,B'10000000'                              02S0348 00754000
*                        GEN REFS(ENQCORE4); /*DEQ                   */ 00755000
                        DEQ MF=(E,ENQCORE4)                             00756000
*                        CHNGQ = OFF;  /*SET ESTAE INDICATOR         */ 00757000
         NI    CHNGQ(EXITP),B'11011111'                         02S0350 00758000
*                      END;                                     02S0351 00759000
*                   END;                                        02S0352 00760000
*                 ELSE;                 /*ENQ NOT SUCCESSFUL         */ 00761000
@RF00319 DS    0H                                               02S0354 00762000
*               END;                                            02S0354 00763000
*             ELSE                      /*UCB ALLOCATED              */ 00764000
*                 MSSUMSW=ON;           /*INDICATE REPROCESS         */ 00765000
         B     @RC00303                                         02S0355 00766000
@RF00303 OI    MSSUMSW,B'10000000'                              02S0355 00767000
*           END;                                                02S0356 00768000
*         ELSE;                         /*NO UNLOAD PENDING          */ 00769000
@RF00301 DS    0H                                               02S0358 00770000
