         BNO   @RF00534                                         02S0534 01416000
*        EREQTERM=ON;              /*SET BIT IN RB                   */ 01417000
         L     @08,EREQPTR                                      02S0535 01418000
         OI    EREQTERM(@08),B'10000000'                        02S0535 01419000
*      IF SIOTQNAM=ON THEN         /*IF A QNAME                      */ 01420000
@RF00534 TM    SIOTQNAM(SIOTPTR),B'00000001'                    02S0536 01421000
         BNO   @RF00536                                         02S0536 01422000
*        EREQQNM=ON;               /*SET BIT IN RB                   */ 01423000
         L     @08,EREQPTR                                      02S0537 01424000
         OI    EREQQNM(@08),B'01000000'                         02S0537 01425000
*      CALL IEFAB428 (EREQPTR,JSCBPTR,EXITPRMP); /*CALL ETIOT BLD RTN*/ 01426000
@RF00536 LA    @08,EREQPTR                                      02S0538 01427000
         ST    @08,@AL00001                                     02S0538 01428000
