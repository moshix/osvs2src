*      DO WHILE (RESCANP ^= VUPTR      /*RESCAN ALL VOLUNIT ENTRIES  */ 01853000
*        & VOLRESVE = ON );            /*UNTIL DUP VOLSER FOUND      */ 01854000
         B     @DE00626                                         03S0626 01855000
@DL00626 DS    0H                                               03S0627 01856000
*        IF RESCANP -> VOLID = VOLID THEN /*IS VOLSER A DUPLICATE ?  */ 01857000
         CLC   VOLID(6,RESCANP),VOLID(VUPTR)                    03S0627 01858000
         BNE   @RF00627                                         03S0627 01859000
*          DO;                         /*YES, VOLSER IS DUPLICATE    */ 01860000
*            VOLRESVE = OFF;           /*TURN OFF UNIQUE INDICATOR   */ 01861000
         NI    VOLRESVE(VUPTR),B'11111011'                      03S0629 01862000
*          END;                                                 03S0630 01863000
*        ELSE;                         /*MATCH FOR VOLSER NOT FOUND  */ 01864000
@RF00627 DS    0H                                               03S0632 01865000
*        RESCANP=RESCANP+LENGTH(VOLUNTAB);/*POINT TO NEXT VOLUNIT       01866000
*                                        ENTRY FOR RESCAN            */ 01867000
         AL    RESCANP,@CF03661                                 03S0632 01868000
*      END;                                                     03S0633 01869000
@DE00626 CR    RESCANP,VUPTR                                    03S0633 01870000
         BE    @DC00626                                         03S0633 01871000
         TM    VOLRESVE(VUPTR),B'00000100'                      03S0633 01872000
         BO    @DL00626                                         03S0633 01873000
@DC00626 DS    0H                                               03S0634 01874000
*      IF VOLRESVE = ON THEN           /*IS ENTRY REALLY UNIQUE ?    */ 01875000
         TM    VOLRESVE(VUPTR),B'00000100'                      03S0634 01876000
         BNO   @RF00634                                         03S0634 01877000
*        DO;                           /*YES, ENTRY IS UNIQUE        */ 01878000
*          NOENQCTR = NOENQCTR + ONE;  /*INCREMENT COUNT TO ENQ      */ 01879000
         AL    NOENQCTR,@CF00076                                03S0636 01880000
*        END;                                                   03S0637 01881000
*      ELSE;                           /*ENTRY IS NOT UNIQUE         */ 01882000
@RF00634 DS    0H                                               03S0639 01883000
*    END;                                                       03S0639 01884000
*  ELSE;                               /*NOT UNALLOC SPECIFIC VOL ENT*/ 01885000
@RF00622 DS    0H                                               03S0641 01886000
*  VUPTR = VUPTR + LENGTH(VOLUNTAB);   /*POINT TO NEXT VOLUNIT ENTRY */ 01887000
         AL    VUPTR,@CF03661                                   03S0641 01888000
*END;                                                           03S0642 01889000
         AL    VUCTR,@CF00076                                   03S0642 01890000
@DE00621 CR    VUCTR,VOLUNNO                                    03S0642 01891000
         BNH   @DL00621                                         03S0642 01892000
*IF NOENQCTR ^=ZERO THEN               /*ANY ENQS NEEDED             */ 01893000
