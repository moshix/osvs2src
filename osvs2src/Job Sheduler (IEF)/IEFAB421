         BNO   @RF00595                                         01S0595 01605000
*                      DO;             /*YES, ENQ DDR DA     @ZA32215*/ 01606000
*                        GEN REFS(ENQLIST); /*               @ZA32215*/ 01607000
                     ENQ (SYSQNAME,SYSRNME2,S,5,SYSTEM),MF=(E,ENQLIST)  01608000
*                      END;            /*END ENQ DDR DA      @ZA32215*/ 01609000
*                  END;               /*                     @Z40FPPD*/ 01610000
@RF00595 DS    0H                                               01S0600 01611000
*                CALL IEFAB430(ALCWAPTR);/*ALLOCATE TO FIXED DA DEV     01612000
*                                                            @ZA04127*/ 01613000
@RF00587 LA    @08,ALCWAPTR                                     01S0600 01614000
         ST    @08,@AL00001                                     01S0600 01615000
         L     @15,@CV00142                                     01S0600 01616000
         LA    @01,@AL00001                                     01S0600 01617000
         BALR  @14,@15                                          01S0600 01618000
*                RTNCODE=REG15;       /*SAVE RETURN CODE     @ZA04127*/ 01619000
         ST    REG15,RTNCODE                                    01S0601 01620000
*                IF RTNCODE=SUCCESS THEN /*IF OK CALL JES3      01S0602 01621000
*                                          EXIT              @ZA04127*/ 01622000
         L     @08,RTNCODE                                      01S0602 01623000
         LTR   @08,@08                                          01S0602 01624000
         BNZ   @RF00602                                         01S0602 01625000
*                  DO;                   /*                  @ZA04127*/ 01626000
*                    FLAG422=FLAG422&&FLAG422;/*CLEAR FLAGS @Y30QPPA*/  01627000
         XC    FLAG422(1),FLAG422                               01S0604 01628000
*                    IF EXTRACAL=ON THEN /*RECALL FOR OLD DD @Y30QPPA*/ 01629000
         L     @08,ALCWAPTR                                     01S0605 01630000
         L     @08,FMAPPTR(,@08)                                01S0605 01631000
         TM    EXTRACAL(@08),B'00000001'                        01S0605 01632000
         BNO   @RF00605                                         01S0605 01633000
*                      CALLAGIN = ON;     /*INDICATE RECALL  @Y30QPPA*/ 01634000
         OI    CALLAGIN,B'10000000'                             01S0606 01635000
*                    CALL IEFAB422(ALCWAPTR,STEPNUM,            01S0607 01636000
*                              FLAG422);  /*CALL JES3 RTN    @Y30QPPA*/ 01637000
@RF00605 LA    @08,ALCWAPTR                                     01S0607 01638000
         ST    @08,@AL00001                                     01S0607 01639000
         L     @08,@PC00001+32                                  01S0607 01640000
         ST    @08,@AL00001+4                                   01S0607 01641000
         LA    @08,FLAG422                                      01S0607 01642000
         ST    @08,@AL00001+8                                   01S0607 01643000
         L     @15,@CV00137                                     01S0607 01644000
         LA    @01,@AL00001                                     01S0607 01645000
         BALR  @14,@15                                          01S0607 01646000
*                    IF SU18BIT=ON THEN   /*SU 18 IN SYSTEM  @G18MP21*/ 01647000
         L     @08,CVTPTR                                       01S0608 01648000
         L     @08,CVTIHASU(,@08)                               01S0608 01649000
         TM    SU18BIT(@08),B'00100000'                         01S0608 01650000
         BNO   @RF00608                                         01S0608 01651000
*                      RTNCODE=REG15;  /*SET RETURN CODE     @G18MP21*/ 01652000
         ST    REG15,RTNCODE                                    01S0609 01653000
*                  END;                   /*                 @ZA04127*/ 01654000
@RF00608 DS    0H                                               01S0611 01655000
*                IF RTNCODE = SUCCESS &  /*IEFAB430 SUCCESSFUL       */ 01656000
*                   TPREQS ^= ZERO THEN  /*TP REQS TO ALLOCATE       */ 01657000
@RF00602 SLR   @08,@08                                          01S0611 01658000
         C     @08,RTNCODE                                      01S0611 01659000
         BNE   @RF00611                                         01S0611 01660000
         L     @01,ALCWAPTR                                     01S0611 01661000
         C     @08,TPREQS(,@01)                                 01S0611 01662000
         BE    @RF00611                                         01S0611 01663000
*                  DO;                                          01S0612 01664000
*                    CALL IEFAB425(ALCWAPTR);  /*ALLOCATE TP REQUESTS*/ 01665000
         LA    @08,ALCWAPTR                                     01S0613 01666000
         ST    @08,@AL00001                                     01S0613 01667000
         L     @15,@CV00139                                     01S0613 01668000
         LA    @01,@AL00001                                     01S0613 01669000
         BALR  @14,@15                                          01S0613 01670000
*                    RTNCODE = REG15;  /*SAVE IEFAB425 RETURN CODE   */ 01671000
         ST    REG15,RTNCODE                                    01S0614 01672000
*                  END;                                         01S0615 01673000
*                IF TOTREQS ^= ZERO & RTNCODE = SUCCESS THEN    01S0616 01674000
@RF00611 SLR   @08,@08                                          01S0616 01675000
         L     @15,ALCWAPTR                                     01S0616 01676000
         C     @08,TOTREQS(,@15)                                01S0616 01677000
         BE    @RF00616                                         01S0616 01678000
         C     @08,RTNCODE                                      01S0616 01679000
         BNE   @RF00616                                         01S0616 01680000
*                                      /*ANY OTHER REQS AND NO ERRORS*/ 01681000
*                  DO;                     /*YES,CONTINUE ALLOCATION */ 01682000
*/********************************************************************/ 01683000
*/*                                                                  */ 01684000
*/* SEGMENT NAME - DOVOLENQ                                          */ 01685000
*/*                                                                  */ 01686000
*/* DESCRIPTIVE NAME - VOLUME RESERVATION                            */ 01687000
*/*                                                                  */ 01688000
*/* FUNCTION -                                                       */ 01689000
*/*   THE PURPOSE OF THIS CODE SEGMENT IS TO ENSURE THAT ALL SPECIFIC*/ 01690000
*/*   VOLUMES REQUIRED BY THIS ALLOCATION ARE AVAILABLE FOR ITS USE. */ 01691000
*/*   ALL PERMANENTLY-RESIDENT AND RESERVED DIRECT ACCESS VOLUMES    */ 01692000
*/*   ARE ALWAYS AVAILABLE TO EACH JOB BECAUSE THEY ARE AUTOMATICALLY*/ 01693000
*/*   SHAREABLE BETWEEN ALL JOBS, HOWEVER THIS CODE SEGMENT MUST     */ 01694000
*/*   ENQ ON ALL OTHER SPECIFIC VOLUMES REQUESTED BY THIS ALLOCATION */ 01695000
*/*   IN ORDER TO DETERMINE IF THEY ARE AVAILABLE. FROM THE VOLUNIT  */ 01696000
*/*   TABLE IT CAN BE DETERMINED WHICH VOLUMES MUST BE ENQ'ED AND    */ 01697000
*/*   WHETHER THEY SHOULD BE ENQ'ED SHARED OR EXCLUSIVE.             */ 01698000
*/*                                                                  */ 01699000
*/*     .ALL TAPE VOLUMES MUST BE ENQ'ED EXCLUSIVE BECAUSE TAPE      */ 01700000
*/*      VOLUMES CANNOT BE SHARED BETWEEN DIFFERENT JOBS.            */ 01701000
*/*                                                                  */ 01702000
*/*     .BECAUSE DIRECT ACCESS DEVICES ARE INHERENTLY SHAREABLE,     */ 01703000
*/*      DIRECT ACCESS VOLUMES ARE ENQ'ED SHARED EXCEPT WHEN IT IS   */ 01704000
*/*      POSSIBLE THAT THIS JOB MAY HAVE TO REMOVE THIS VOLUME DURING*/ 01705000
*/*      ITS PROCESSING - WHICH CAN ONLY BE DUE TO ONE OF THE        */ 01706000
*/*      FOLLOWING SITUATIONS:                                       */ 01707000
*/*        1) THE VOLUME IS TO BE ALLOCATED TO THE LAST UNIT FOR A   */ 01708000
*/*           MULTI-VOLUME DATA SET REQUEST WHERE MORE               */ 01709000
*/*           VOLUMES THAN UNITS WERE REQUESTED - AND THEREFORE THE  */ 01710000
*/*           EXCESS VOLUMES ARE ALL ALLOCATED TO THE LAST UNIT.     */ 01711000
*/*                                                                  */ 01712000
*/*        2) THROUGH UNIT AFFINITY, THE VOLUME WAS REQUESTED TO BE  */ 01713000
*/*           ALLOCATED TO THE SAME UNIT AS ANOTHER VOLUME.          */ 01714000
*/*                                                                  */ 01715000
*/*   IF BIT VWAITSW  OF THE FUNCTION MAP (FUNCMAP) IS ON, THEN      */ 01716000
*/*   THE ALLOCATION WILL WAIT UNTIL THE VOLUMES REQUESTED ARE       */ 01717000
*/*   AVAILABLE. THE OPERATOR WILL BE ABLE TO CANCEL THE ALLOCATION  */ 01718000
*/*   EVEN WHILE IT IS WAITING.                                      */ 01719000
*/*   IF BIT VWAITSW  OF THE FUNCTION MAP IS NOT ON, THEN THE        */ 01720000
*/*   ALLOCATION REQUESTS WILL BE FAILED IF ALL THE REQUESTED        */ 01721000
*/*   VOLUMES ARE NOT IMMEDIATELY AVAILABLE. A VOLUME IS NOT         */ 01722000
*/*   IMMEDIATELY AVAILABLE UNDER THE FOLLOWING CONDITIONS:          */ 01723000
*/*     1) VOLUME IS REQUESTED EXCLUSIVE AND IS CURRENTLY BEING      */ 01724000
*/*        USED BY ANOTHER JOB OR THIS JOB (BECAUSE OF A PREVIOUS    */ 01725000
*/*        CALL TO ALLOCATION).                                      */ 01726000
*/*                                                                  */ 01727000
*/*     2) VOLUME IS REQUESTED SHARED BUT IS BEING USED EXCLUSIVE    */ 01728000
*/*        BY ANOTHER JOB OR THIS JOB (BECAUSE OF A PREVIOUS         */ 01729000
*/*        CALL TO ALLOCATION).                                      */ 01730000
*/*                                                                  */ 01731000
*/*   OPERATION -                                                    */ 01732000
*/*     THE VOLUNIT TABLE IS USED TO DETERMINE WHICH VOLUME SERIAL   */ 01733000
*/*     NUMBERS SHOULD BE ENQ'ED AND TO DETERMINE WHETHER EACH       */ 01734000
*/*     VOLSER SHOULD BE ENQ'ED SHARED OR EXCLUSIVE.                 */ 01735000
*/*     THE VOLUNIT TABLE IS FIRST SCANNED TO DETERMINE HOW MANY     */ 01736000
*/*     VOLSERS ARE TO BE ENQ'ED AND TO MARK THE VOLUME ENTRIES      */ 01737000
*/*     TO BE ENQ'ED.                                                */ 01738000
*/*     AFTER CORE IS OBTAINED FOR THE ENQ LIST, THE VOLUNIT TABLE   */ 01739000
*/*     IS SCANNED AGAIN TO FILL IN THE INFORMATION FOR THE ENQ LIST.*/ 01740000
*/*                                                                  */ 01741000
*/*     THE REST OF THE OPERATION IS DOCUMENTED BY THE CODE ITSELF.  */ 01742000
*/*                                                                  */ 01743000
*/* ENTRY - INLINE SUBROUTINE                                        */ 01744000
*/*                                                                  */ 01745000
*/*   PURPOSE - SEE FUNCTION                                         */ 01746000
*/*                                                                  */ 01747000
*/*   INPUT -                                                        */ 01748000
*/*     FUNCMAP - FUNCTION MAP                                       */ 01749000
*/*     ALCWAPTR - POINTER TO THE ALLOCATION WORK AREA               */ 01750000
*/*       VOLUNPTR - POINTER TO VOLUNIT TABLE                        */ 01751000
*/*                                                                  */ 01752000
*/* EXIT - INLINE SUBROUTINE                                         */ 01753000
*/*                                                                  */ 01754000
*/*   CONDITION - ALL VOLUMES AVAILABLE TO THE ALLOCATION.           */ 01755000
*/*                                                                  */ 01756000
*/*     OUTPUT -                                                     */ 01757000
*/*            VOLUMES ENQ'ED SHARED OR EXCLUSIVE                    */ 01758000
*/*                                                                  */ 01759000
*/*     RETURN CODE - (NONE)                                         */ 01760000
*/*                                                                  */ 01761000
*/*   CONDITION - ALLOCATION COULD NOT WAIT AND ALL REQUESTED        */ 01762000
*/*               VOLUMES WERE NOT IMMEDIATELY AVAILABLE.            */ 01763000
*/*                                                                  */ 01764000
*/*     OUTPUT -                                                     */ 01765000
*/*            REASON CODE (FAILRSC) SET TO 7                        */ 01766000
*/*                                                                  */ 01767000
*/*     RETURN CODE -                                                */ 01768000
*/*            RTNCODE - 4                                           */ 01769000
*/*                                                                  */ 01770000
*/*   CONDITION - OPERATOR CANCELLED ALLOCATION                      */ 01771000
*/*                                                                  */ 01772000
*/*     OUTPUT -                                                     */ 01773000
*/*            REASON CODE (FAILRSC) SET TO 5                        */ 01774000
*/*                                                                  */ 01775000
*/*     RETURN CODE                                                  */ 01776000
*/*            RTNCODE - 4                                           */ 01777000
*/*                                                                  */ 01778000
*/*   CONDITION - GETMAIN ERROR                                      */ 01779000
*/*                                                                  */ 01780000
*/*     OUTPUT -                                                     */ 01781000
*/*            REASON CODE (FAILRSC) SET TO 2                        */ 01782000
*/*                                                                  */ 01783000
*/*     RETURN CODE -                                                */ 01784000
*/*            RETCODE - 4                                           */ 01785000
*/*                                                                  */ 01786000
*/*                                                                  */ 01787000
*/********************************************************************/ 01788000
*VUPTR = VOLUNPTR;                     /*POINT TO FIRST VOLUNIT ENTRY*/ 01789000
         L     VUPTR,VOLUNPTR(,@15)                             02S0618 01790000
*VOLUNNO=VOLUNSZE/LENGTH(VOLUNTAB);    /*DETERMINE # OF VU ENTRIES   */ 01791000
         L     @00,VOLUNSZE(,@15)                               02S0619 01792000
         SRDA  @00,32                                           02S0619 01793000
         D     @00,@CF03661                                     02S0619 01794000
         LR    VOLUNNO,@01                                      02S0619 01795000
*/********************************************************************/ 01796000
*/*                                                                  */ 01797000
*/* SEGMENT NAME - CNTVENQS                                          */ 01798000
*/*                                                                  */ 01799000
*/* DESCRIPTIVE NAME - DETERMINE NUMBER OF VOLUME ENQS NEEDED        */ 01800000
*/*                                                                  */ 01801000
*/* FUNCTION -                                                       */ 01802000
*/*   THE PURPOSE OF THIS CODE SEGMENT IS TO TELL HOW MANY SPECIFIC  */ 01803000
*/*   VOLUME SERIAL NUMBERS MUST BE ENQ'ED BY THIS ALLOCATION.       */ 01804000
*/*   THIS SEGMENT ALSO INDICATES WHICH VOLSERS MUST BE ENQ'ED -     */ 01805000
*/*   BY MARKING THE APPROPRIATE ENTRIES IN THE VOLUNIT TABLE.       */ 01806000
*/*                                                                  */ 01807000
*/*   OPERATION -                                                    */ 01808000
*/*     THIS  CODE  SCANS THE VOLUNIT TABLE TO FIND EACH UNIQUE      */ 01809000
*/*     SPECIFIC VOLUME ENTRY FOR WHICH THERE IS AT LEAST ONE REQUEST*/ 01810000
*/*     WHICH HAS NOT YET BEEN ALLOCATED. ALL SUCH VOLUME ENTRIES    */ 01811000
*/*     ARE COUNTED AND MARKED (SO THEY WILL BE ENQ'ED LATER).       */ 01812000
*/*                                                                  */ 01813000
*/* ENTRY - INLINE SUBROUTINE                                        */ 01814000
*/*                                                                  */ 01815000
*/*   PURPOSE - SEE FUNCTION                                         */ 01816000
*/*                                                                  */ 01817000
*/*   INPUT -                                                        */ 01818000
*/*     ALCWAPTR - POINTER TO THE ALLOCATION WORK AREA.              */ 01819000
*/*       VOLUNPTR - POINTER TO THE VOLUNIT TABLE.                   */ 01820000
*/*                                                                  */ 01821000
*/* EXIT - NORMAL - INLINE SUBROUTINE                                */ 01822000
*/*                                                                  */ 01823000
*/*   CONDITION - VOLUNIT ENTRIES FOR VOLUME SERIAL NUMBERS TO BE    */ 01824000
*/*               ENQ'ED ARE COUNTED AND MARKED.                     */ 01825000
*/*                                                                  */ 01826000
*/*   OUTPUT -                                                       */ 01827000
*/*       NOENQCTR - NUMBER OF VOLSERS TO ENQ.                       */ 01828000
*/*       VOLRESVE - BIT IN EACH VOLUNIT TABLE ENTRY. SET TO 1 FOR   */ 01829000
*/*                  UNIQUE SPECIFIC VOLUME ENTRIES TO BE ENQ'ED.    */ 01830000
*/*                                                                  */ 01831000
*/*   RETURN CODE - (NONE)                                           */ 01832000
*/*                                                                  */ 01833000
*/********************************************************************/ 01834000
*NOENQCTR = ZERO;                      /*INITIALIZE COUNT TO ZERO    */ 01835000
         SLR   NOENQCTR,NOENQCTR                                03S0620 01836000
*DO VUCTR = ONE TO VOLUNNO;            /*SCAN ALL VOLUNIT ENTRIES    */ 01837000
