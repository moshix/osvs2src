*      VOLRESVE = ON;                  /*ASSUME UNIQUE VOLSER ENTRY  */ 01848000
         OI    VOLRESVE(VUPTR),B'00000100'                      03S0624 01849000
*      RESCANP = VOLUNPTR;             /*POINT TO FIRST VOLUNIT ENTRY*/ 01850000
