*        IF UCBALOC=OFF THEN            /*UCB STILL UNALLOCATED?     */ 00866000
         TM    UCBALOC(UCBPTR),B'00001000'                      03S0377 00867000
         BNZ   @RF00377                                         03S0377 00868000
*          DO;                          /*YES,VMV ZEROS UCB FIELDS   */ 00869000
*            IF UCB3TAPE=ON | UCB3DACC=ON THEN /*IF TAPE OR DA       */ 00870000
         TM    UCB3TAPE(UCBPTR),B'10100000'                     03S0379 00871000
         BZ    @RF00379                                         03S0379 00872000
*              DO;                                              03S0380 00873000
*                VMVREQBP=ADDR(VMVCORE);    /*POINT TO VMV REQ BLK   */ 00874000
         LA    @15,VMVCORE                                      03S0381 00875000
