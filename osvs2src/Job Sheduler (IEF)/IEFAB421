SETUPWTO STM   @14,@12,@SA00002                                 02S0954 02867000
*/********************************************************************/ 02868000
*/*                                                                  */ 02869000
*/* SEGMENT NAME = SWTOSPEC                                          */ 02870000
*/*                                                                  */ 02871000
*/* DESCRIPTIVE NAME - SETUP OFFLINE MSG AND ROUTING CODE            */ 02872000
*/*                                                                  */ 02873000
*/* FUNCTION -                                                       */ 02874000
*/*   THE PURPOSE OF THIS CODE SEGMENT IS TO PUT THE UNIT ADDRESS    */ 02875000
*/*   IN THE MESSAGE TEXT (IEF281I DDD NOW OFFLINE  OR IEF282I DD    */ 02876000
*/*   NOW UNLOADED),TO DETERMINE WHICH OPERATOR THE MESSAGE SHOULD   */ 02877000
*/*   BE ROUTED TO,AND TO ISSUE THE MESSAGE TO THE OPERATOR.         */ 02878000
*/*   THE ROUTING CODE IS DETERMINED BY THE DEVICE TYPE  OF THE      */ 02879000
*/*   UNIT MADE OFFLINE, AS SHOWN BELOW:                             */ 02880000
*/*      .IF TAPE DEVICE, ROUTING CODE IS 3                          */ 02881000
*/*      .IF DIRECT ACCESS DEVICE, ROUTING CODE IS 4                 */ 02882000
*/*      .IF UNIT RECORD DEVICE, ROUTING CODE IS 7                   */ 02883000
*/*      .IF TP DEVICE, ROUTING CODE IS 8                            */ 02884000
*/*      .IF OTHER DEVICE TYPE, DEFAULT ROUTING CODE OF 2 IS USED.   */ 02885000
*/*                                                                  */ 02886000
*/* ENTRY - SETUPWTO                                                 */ 02887000
*/*                                                                  */ 02888000
*/*   PURPOSE - SEE FUNCTION                                         */ 02889000
*/*                                                                  */ 02890000
*/*   INPUT - UCBPTR IS A POINTER TO THE UCB ASSOCIATED WITH         */ 02891000
*/*           A DEVICE JUST MADE OFFLINE OR UNLAODED.                */ 02892000
*/*                                                                  */ 02893000
*/* EXITS - RETURN TO CALLER                                         */ 02894000
*/*                                                                  */ 02895000
*/*   CONDITION - FUNCTION IS COMPLETED SUCCESSFULLY                 */ 02896000
*/*                                                                  */ 02897000
*/*   OUTPUT - WTOLIST (A COPY OF THE LIST FORM OF THE WTO           */ 02898000
*/*            MACRO) IS FILLED IN WITH THE DEVICE ADDRESS           */ 02899000
*/*            AND WITH THE CORRECT ROUTING CODE AND WTO ISSUED.     */ 02900000
*/*                                                                  */ 02901000
*/********************************************************************/ 02902000
*OFFUNIT = UCBNAME;                    /*PUT DEVICE ADDRESS IN MSG   */ 02903000
         MVC   OFFUNIT(3),UCBNAME(UCBPTR)                       02S0955 02904000
*IF UCB3TAPE = ON THEN                 /*IS THIS A TAPE DEVICE       */ 02905000
         TM    UCB3TAPE(UCBPTR),B'10000000'                     02S0956 02906000
         BNO   @RF00956                                         02S0956 02907000
*  DO;                                 /*YES TAPE DEVICE             */ 02908000
*    ROUTCDE = TAPEROUT;               /*SET TAPE ROUTING CODE       */ 02909000
         MVC   ROUTCDE(2),@CB01659                              02S0958 02910000
*  END;                                                         02S0959 02911000
*ELSE                                  /*NOT TAPE DEVICE             */ 02912000
*  DO;                                                          02S0960 02913000
         B     @RC00956                                         02S0960 02914000
@RF00956 DS    0H                                               02S0961 02915000
*    IF UCB3DACC = ON THEN             /*IS THIS A DA DEVICE         */ 02916000
         TM    UCB3DACC(UCBPTR),B'00100000'                     02S0961 02917000
         BNO   @RF00961                                         02S0961 02918000
*      DO;                             /*YES DIRECT ACCESS DEVICE    */ 02919000
*        ROUTCDE = DAROUT;             /*SET DA ROUTING CODE         */ 02920000
         MVC   ROUTCDE(2),@CB01655                              02S0963 02921000
*      END;                                                     02S0964 02922000
*    ELSE                              /*NOT DIRECT ACCESS DEVICE    */ 02923000
*      DO;                                                      02S0965 02924000
         B     @RC00961                                         02S0965 02925000
@RF00961 DS    0H                                               02S0966 02926000
*        IF UCB3UREC = ON THEN         /*IS THIS A UNIT RECORD DEVICE*/ 02927000
         TM    UCB3UREC(UCBPTR),B'00001000'                     02S0966 02928000
         BNO   @RF00966                                         02S0966 02929000
*          DO;                         /*YES UNIT RECORD DEVICE      */ 02930000
*            ROUTCDE = URECROUT;       /*SET UNIT RECORD ROUTING CODE*/ 02931000
         MVC   ROUTCDE(2),@CB01663                              02S0968 02932000
*          END;                                                 02S0969 02933000
*        ELSE                          /*NOT UNIT RECORD DEVICE      */ 02934000
*          DO;                                                  02S0970 02935000
         B     @RC00966                                         02S0970 02936000
@RF00966 DS    0H                                               02S0971 02937000
*            IF UCB3COMM = ON THEN     /*IS THIS A TP DEVICE         */ 02938000
         TM    UCB3COMM(UCBPTR),B'01000000'                     02S0971 02939000
         BNO   @RF00971                                         02S0971 02940000
*              DO;                     /*YES TELEPROCESSING DEVICE   */ 02941000
*                ROUTCDE = TPROUT;     /*SET TP ROUTING CODE         */ 02942000
         MVC   ROUTCDE(2),@CB01661                              02S0973 02943000
*              END;                                             02S0974 02944000
*            ELSE                      /*NOT TP DEVICE               */ 02945000
*              DO;                                              02S0975 02946000
         B     @RC00971                                         02S0975 02947000
@RF00971 DS    0H                                               02S0976 02948000
*                ROUTCDE = REGROUT;    /*SET DEFAULT ROUTING CODE    */ 02949000
         MVC   ROUTCDE(2),@CB01657                              02S0976 02950000
*              END;                                             02S0977 02951000
*          END;                                                 02S0978 02952000
@RC00971 DS    0H                                               02S0979 02953000
*      END;                                                     02S0979 02954000
@RC00966 DS    0H                                               02S0980 02955000
*  END;                                                         02S0980 02956000
@RC00961 DS    0H                                               02S0981 02957000
*GEN REFS(WTOLIST,REG1);               /*ISSUE UNLOAD/OFFLINE MSG    */ 02958000
@RC00956 DS    0H                                               02S0981 02959000
 WTO MF=(E,WTOLIST)                                                     02960000
*END SETUPWTO;                                                  02S0982 02961000
@EL00002 DS    0H                                               02S0982 02962000
@EF00002 DS    0H                                               02S0982 02963000
@ER00002 LM    @14,@12,@SA00002                                 02S0982 02964000
