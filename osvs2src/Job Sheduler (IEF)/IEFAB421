         L     @01,PPJSCBP(,@15)                                02S0153 00126000
         L     @15,JSCBASWA(,@01)                               02S0153 00127000
         CR    @15,@08                                          02S0153 00128000
         BE    @RT00153                                         02S0153 00129000
