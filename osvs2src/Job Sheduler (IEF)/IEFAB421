*ASWARTNP = ADDR(IEFAB4E8);            /*ADDRESS OF EXIT RTN @Z40FPPG*/ 00111000
         L     @14,@CV00136                                     02S0149 00112000
         STCM  @14,15,ASWARTNP(@15)                             02S0149 00113000
*ASWAPRMS = EXITP;                     /*PARMS FOR THIS EXIT @Z40FPPG*/ 00114000
         STCM  EXITP,15,ASWAPRMS(@15)                           02S0150 00115000
*ASWACOMF = ON;                        /*CHECK FOR PERCOLATION  02S0151 00116000
*                                                            @Z40FPPG*/ 00117000
         OI    ASWACOMF(@15),B'01000000'                        02S0151 00118000
*ISSUESTA = OFF;                       /*INIT. ESTAE SW      @Z40FPPG*/ 00119000
         NI    ISSUESTA,B'01111111'                             02S0152 00120000
*IF JSCBASWA = ZERO   |                /*NO ALLOC ESTAE IN EFFECT OR    00121000
*                                                            @Z40FPPG*/ 00122000
*   JSCBASWA -> ASWASCBP ^= TCBSTABB THEN /*ITS NOT THE CURRENT ONE     00123000
*                                                             @Z40FPPG* 00124000
