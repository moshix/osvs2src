*TESTENQ: PROC OPTIONS(NOSAVEAREA,NOSAVE);                      02S0983 02966000
TESTENQ  DS    0H                                               02S0984 02967000
*/********************************************************************/ 02968000
*/*                                                                  */ 02969000
*/* SEGMENT NAME - TESTENQ                                           */ 02970000
*/*                                                                  */ 02971000
*/* DESCRIPTIVE NAME - TEST IF EXCLUSIVE ENQ COMPLETED               */ 02972000
*/*                                                                  */ 02973000
*/* FUNCTION -                                                       */ 02974000
*/*   THIS ROUTINE CHECKS TO SEE IF AN EXCLUSIVE ENQ HAS COMPLETED.  */ 02975000
*/*   IF REG 15 INDICATES THAT THE ENQ HAS NOT YET COMPLETED, A ONE- */ 02976000
*/*   SECOND WAIT IS SET. IF THE ENQ ECB HAS NOT BEEN POSTED AFTER   */ 02977000
*/*   THE WAIT, THIS ROUTINE DEQS OFF THE RESOURCE.                  */ 02978000
*/*                                                                  */ 02979000
*/* ENTRY - CLOSED SUBROUTINE                                        */ 02980000
*/*   PURPOSE - SEE FUNCTION                                         */ 02981000
*/*   INPUT -                                                        */ 02982000
*/*     POINTER TO THE ENQ LIST TO BE USED IF A DEQ IS NECESSARY     */ 02983000
*/*     REGISTER 15 - CONTAINS THE ENQ RETURN CODE                   */ 02984000
*/*     ENQ ECB                                                      */ 02985000
*/*                                                                  */ 02986000
*/* EXIT-NORMAL - RETURNS TO CALLER                                  */ 02987000
*/*   CONDITION - ENQ COMPLETED SUCCESSFULLY                         */ 02988000
*/*   OUTPUT -                                                       */ 02989000
*/*     ENQOKSW - SET TO 1 TO INDICATE ENQ COMPLETED                 */ 02990000
*/*   RETURN CODE - NONE                                             */ 02991000
*/*                                                                  */ 02992000
*/*   CONDITION - ENQ NOT COMPLETED                                  */ 02993000
*/*   OUTPUT -                                                       */ 02994000
*/*     ENQOKSW - SET TO 0 TO INDICATE ENQ NOT COMPLETED             */ 02995000
*/*     MSSUMSW - SET TO 1 TO INDICATE TO TURN ON REPROCESS BIT      */ 02996000
*/*   RETURN CODE - NONE                                             */ 02997000
*/*                                                                  */ 02998000
*/*                                                                  */ 02999000
*/********************************************************************/ 03000000
*RFY (REG14) RSTD;                                              02S0984 03001000
*ECBOFF = OFF;                         /*TURN ECB INDICATOR OFF FOR     03002000
*                                        DEQ                         */ 03003000
         NI    ECBOFF(ENQCOREP),B'11111011'                     02S0985 03004000
*IF ENQRTNP ^= ZERO THEN               /*ENQ NOT COMPLETED           */ 03005000
         LTR   ENQRTNP,ENQRTNP                                  02S0986 03006000
         BZ    @RF00986                                         02S0986 03007000
*  DO;                                 /*WAIT 1 SECOND               */ 03008000
*    GEN REFS(TIMEWAIT);                                        02S0988 03009000
      STIMER WAIT,DINTVL=TIMEWAIT                                       03010000
*    IF ENQPOST = OFF THEN             /*ENQ STILL NOT COMPLETE      */ 03011000
         TM    ENQPOST,B'01000000'                              02S0989 03012000
         BNZ   @RF00989                                         02S0989 03013000
*      DO;                                                      02S0990 03014000
*        ENQOKSW = OFF;              /*INDICATE ENQ NOT COMPLETE   */   03015000
         NI    ENQOKSW,B'01111111'                              02S0991 03016000
*        MSSUMSW = ON;                 /*INDICATE REPROCESS          */ 03017000
         OI    MSSUMSW,B'10000000'                              02S0992 03018000
*        RFY (REG1) RSTD;                                       02S0993 03019000
*        REG1 = ENQCOREP;              /*PTR TO LIST FOR DEQ         */ 03020000
         LR    REG1,ENQCOREP                                    02S0994 03021000
*        GEN ( DEQ MF=(E,(1)));                                 02S0995 03022000
          DEQ MF=(E,(1))                                                03023000
*        RFY (REG1) UNRSTD;                                     02S0996 03024000
*      END;                                                     02S0997 03025000
*    ELSE                              /*ENQ COMPLETED AFTER WAIT    */ 03026000
*      ENQOKSW = ON;                   /*INDICATE ENQ COMPLETE       */ 03027000
         B     @RC00989                                         02S0998 03028000
@RF00989 OI    ENQOKSW,B'10000000'                              02S0998 03029000
*  END;                                                         02S0999 03030000
*ELSE                                  /*ENQ COMPLETED               */ 03031000
*  ENQOKSW = ON;                       /*INDICATE ENQ COMPLETED      */ 03032000
         B     @RC00986                                         02S1000 03033000
@RF00986 OI    ENQOKSW,B'10000000'                              02S1000 03034000
*RFY (REG14) UNRSTD;                                            02S1001 03035000
*END TESTENQ;                                                   02S1002 03036000
@EL00003 DS    0H                                               02S1002 03037000
@EF00003 DS    0H                                               02S1002 03038000
@ER00003 BR    @14                                              02S1002 03039000
*                                                               01S1003 03040000
