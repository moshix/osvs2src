*    ASWACHNP = JSCBASWA;              /*POINT BACK TO LAST  @Z40FPPG*/ 00186000
         STCM  @15,15,ASWACHNP(@08)                             02S0170 00187000
*  END;                                /*CURRENT ESTAE USED  @Z40FPPG*/ 00188000
*IF RTNCODE = ZERO THEN                /*IF NO ERROR         @Z40FPPG*/ 00189000
