         ST    @14,ALCNUCBS(,@08)                               02S0230 00396000
*    IF SU29BIT = ON THEN              /*IS SU 29 INSTALLED? @G29AN2F*/ 00397000
         L     @01,CVTIHASU(,@15)                               02S0231 00398000
         TM    SU29BIT(@01),B'00000100'                         02S0231 00399000
         BNO   @RF00231                                         02S0231 00400000
*      DO;                             /*YES, SET ALCWA FLDS @G29AN2F*/ 00401000
*        SSERBLKP = SSEBLKP;           /*SET PTR TO ERR BLK  @G29AN2F*/ 00402000
