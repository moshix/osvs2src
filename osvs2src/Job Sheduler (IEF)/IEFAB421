         TITLE 'IEFAB421 - COMMON ALLOCATION CONTROL                   *00001000
                        '                                               00002000
IEFAB421 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(24)                                          01S0001 00007000
         DC    C'IEFAB421 78124  UZ18622 '                      01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @11,0                                            01S0001 00011000
@PSTART  LA    @12,4095(,@11)                                   01S0001 00012000
         USING @PSTART,@11                                      01S0001 00013000
         USING @PSTART+4095,@12                                 01S0001 00014000
         L     @00,@SIZDATD                                     01S0001 00015000
         GSPACE   R,LV=(0)                                              00016000
         LR    @10,@01                                          01S0001 00017000
         USING @DATD,@10                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         LM    @00,@01,20(@13)                                  01S0001 00020000
         ST    @10,8(,@13)                                      01S0001 00021000
         LR    @13,@10                                          01S0001 00022000
         MVC   @PC00001(40),0(@01)                              01S0001 00023000
JSCBDISP EQU 188                                                        00024000
JSCBAOS1 EQU  72                                                        00025000
JSCBAOS2 EQU 192                                                        00026000
*REG2 = ADDR(EXITPRMS);                /*ADDR OF CORE TO CLEAR       */ 00027000
         LA    REG2,EXITPRMS                                    01S0136 00028000
*REG3 = LENGTH(PRMESTAE);              /*LENGTH OF CORE TO CLEAR     */ 00029000
         LA    REG3,296                                         01S0137 00030000
*REG4 = ZERO;                          /*CLEAR REG FOR MOVE LONG     */ 00031000
         SLR   REG4,REG4                                        01S0138 00032000
*REG5 = ZERO;                          /*CLEAR REG FOR MOVE LONG     */ 00033000
         SLR   REG5,REG5                                        01S0139 00034000
*GEN( MVCL 2,4);                                                01S0140 00035000
          MVCL 2,4                                                      00036000
*RFY (REG2,REG3,REG4,REG5) UNRSTD;                              01S0141 00037000
*EXITP = ADDR(EXITPRMS);               /*PTR TO ESTAE PARMS          */ 00038000
