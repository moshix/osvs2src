*                                                            @ZA12405*/ 02036000
@RC00676 L     VENQLSTP,VENQADDR                                02S0694 02037000
         AL    VENQLSTP,ENQPRFX                                 02S0694 02038000
*        VUPTR = VOLUNPTR;             /*POINT TO FIRST VOLUNIT ENTRY*/ 02039000
         L     @04,ALCWAPTR                                     02S0695 02040000
         L     VUPTR,VOLUNPTR(,@04)                             02S0695 02041000
*        STENQNO = NOENQCTR;           /*SAVE NUMBER OF ENQS         */ 02042000
         ST    NOENQCTR,STENQNO                                 02S0696 02043000
*        DO WHILE NOENQCTR ^= ZERO;    /*BUILD ENQ LIST FOR EACH        02044000
*                                        UNALLOC, SPECIFIC VOLUME    */ 02045000
         B     @DE00697                                         02S0697 02046000
@DL00697 DS    0H                                               02S0698 02047000
*          IF VOLRESVE = ON THEN       /*UNIQUE,SPECIFIC,UNALLOC VOL?*/ 02048000
         TM    VOLRESVE(VUPTR),B'00000100'                      02S0698 02049000
         BNO   @RF00698                                         02S0698 02050000
*            DO;                       /*YES, BUILD ENQ LIST         */ 02051000
*              VENQLIST = ENQLFORM;    /*COPY SHARE SKELETON ENQ LIST*/ 02052000
         MVC   VENQLIST(12,VENQLSTP),ENQLFORM                   02S0700 02053000
*              VENQSERP = ADDR(VOLID); /*INSERT PTR TO VOLSER        */ 02054000
         ST    VUPTR,VENQSERP(,VENQLSTP)                        02S0701 02055000
*              IF VOLNSHR = ON | VOLTAREQ=ON THEN /*IS VOLUME TO BE     02056000
*                                        NON-SHARED?                 */ 02057000
         TM    VOLNSHR(VUPTR),B'00001000'                       02S0702 02058000
         BO    @RT00702                                         02S0702 02059000
         TM    VOLTAREQ(VUPTR),B'10000000'                      02S0702 02060000
         BNO   @RF00702                                         02S0702 02061000
@RT00702 DS    0H                                               02S0703 02062000
*                DO;                   /*YES, CHANGE ENQ LIST        */ 02063000
*                  VENQSHSW = OFF;     /*TURN OFF SHARED INDICATOR   */ 02064000
         NI    VENQSHSW(VENQLSTP),B'01111111'                   02S0704 02065000
*                END;                                           02S0705 02066000
*              ELSE;                   /*NOT NON-SHAREABLE           */ 02067000
@RF00702 DS    0H                                               02S0707 02068000
*              VENQLSTP=VENQLSTP + ENQENTLN;/*POINT TO NEXT ENQ ENTRY*/ 02069000
         AL    VENQLSTP,@CF00043                                02S0707 02070000
*              NOENQCTR=NOENQCTR - ONE;/*DECREMENT COUNT OF ENQ ENTRY*/ 02071000
         BCTR  NOENQCTR,0                                       02S0708 02072000
*            END;                                               02S0709 02073000
*          ELSE;                       /*NOT UNIQUE,SPECIFIC,+UNALLOC*/ 02074000
@RF00698 DS    0H                                               02S0711 02075000
*          VUPTR=VUPTR+LENGTH(VOLUNTAB); /*PTR TO NEXT VOLUNIT ENTRY */ 02076000
         AL    VUPTR,@CF03661                                   02S0711 02077000
*        END;                                                   02S0712 02078000
@DE00697 SLR   @04,@04                                          02S0712 02079000
         CR    NOENQCTR,@04                                     02S0712 02080000
         BNE   @DL00697                                         02S0712 02081000
*        VENQLSTP=VENQADDR+ENQSPACE-ENQENTLN; /*LOCATE LAST ENQ ENTRY*/ 02082000
         L     @15,VENQADDR                                     02S0713 02083000
         LR    VENQLSTP,@15                                     02S0713 02084000
         AL    VENQLSTP,ENQSPACE                                02S0713 02085000
         SL    VENQLSTP,@CF00043                                02S0713 02086000
*        VENQLAST = VENQLAST | LAST;   /*INDICATE THIS IS LAST ENTRY */ 02087000
         OI    VENQLAST(VENQLSTP),X'80'                         02S0714 02088000
*        ENQECB = ZERO;                /*ZERO ECB                    */ 02089000
         ST    @04,ENQECB                                       02S0715 02090000
*        RFY (REG1) RSTD;              /*RESTRICT USE OF REG 1       */ 02091000
*        REG1 = VENQADDR + ENQPRFX;    /*POINT TO ENQ LIST START        02092000
*                                                            @ZA12405*/ 02093000
         AL    @15,ENQPRFX                                      02S0717 02094000
         LR    REG1,@15                                         02S0717 02095000
*        GEN REFS(REG1);               /*ENQ ON VOLUME SERIAL NUMBERS*/ 02096000
        ENQ MF=(E,(REG1))                                               02097000
*        RFY (REG1) UNRSTD;                                     02S0719 02098000
*        ENQRTNP=REG15;                                         02S0720 02099000
         LR    ENQRTNP,REG15                                    02S0720 02100000
*/********************************************************************/ 02101000
*/*                                                                  */ 02102000
*/* SEGMENT NAME - VENQRTN                                           */ 02103000
*/*                                                                  */ 02104000
*/* DESCRIPTIVE NAME - HANDLE RETURN FROM VOLUME ENQ                 */ 02105000
*/*                                                                  */ 02106000
*/* FUNCTION -                                                       */ 02107000
*/*   THE RESULTS OF THE VOLUME ENQ ARE PROCESSED AS FOLLOWS:        */ 02108000
*/*                                                                  */ 02109000
*/*       .IF THE ENQ WAS UNCONDITIONAL (+ ASYNCHRONOUS) AND         */ 02110000
*/*        WAS NOT IMMEDIATELY SATISFIED, THIS ROUTINE DEQ'S OFF     */ 02111000
*/*        THE ALLOCATION RESOURCE, INFORMS THE OPERATOR             */ 02112000
*/*        THAT THE ALLOCATION IS WAITING FOR VOLUMES,               */ 02113000
*/*        THEN WAITS FOR ENQ OR OPERATOR REPLY OF NO. WHEN          */ 02114000
*/*        POSTED, THE ALLOCATION RESOURCE IS RE-ENQ'D.              */ 02115000
*/*                                                                  */ 02116000
*/*       .IF THE ENQ WAS CONDITIONAL, THE RETURNED INFO FROM ENQ    */ 02117000
*/*        IS EXAMINED. IF A VOLUME WAS NOT IMMEDIATELY AVAILABLE,   */ 02118000
*/*        THE ALLOCATION IS FAILED. IF THE VOLUME IS ALREADY OWNED  */ 02119000
*/*        BY THIS JOB, THEN THE ALLOCATION IS FAILED UNLESS THE     */ 02120000
*/*        VOLUME IS REQUESTED SHARED AND IS CURRENTLY RESERVED      */ 02121000
*/*        SHARED (BY THIS JOB).                                     */ 02122000
*/*                                                                  */ 02123000
*/*   OPERATION -                                                    */ 02124000
*/*     NO SPECIAL ALGORITHMS ARE USED IN THIS SEGMENT. THE          */ 02125000
*/*     OPERATION IS DOCUMENTED IN THE CODE.                         */ 02126000
*/*                                                                  */ 02127000
*/* ENTRY - INLINE SUBROUTINE                                        */ 02128000
*/*                                                                  */ 02129000
*/*   PURPOSE - SEE FUNCTION                                         */ 02130000
*/*                                                                  */ 02131000
*/*   INPUT -                                                        */ 02132000
*/*      VWAITSW - WAIT/NO-WAIT INDICATOR.                           */ 02133000
*/*      ENQRTNP - 0 (SUCCESS) OR POINTER TO RETURN INFO (PROBLEM).  */ 02134000
*/*                                                                  */ 02135000
*/* EXIT - NORMAL - INLINE SUBROUTINE                                */ 02136000
*/*                                                                  */ 02137000
*/*   CONDITION - VOLUME ENQ SUCCESSFUL.                             */ 02138000
