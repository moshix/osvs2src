*    RFY (REG2,REG3) RSTD;             /*RESTRICT WORK REGS  @Z40FPPG*/ 00149000
*    REG2 = ADDR(IEFAB4ED);            /*COMMON ESTAE EXIT   @Z40FPPG*/ 00150000
         L     REG2,@CV00135                                    02S0159 00151000
*    REG3 = ASWAPTR;                   /*ASWA IS EXIT PARMS  @Z40FPPG*/ 00152000
         LR    REG3,@08                                         02S0160 00153000
*    ESTAECOR(1:ESTAELNG) = ESTAELST(1:ESTAELNG);               02S0161 00154000
         L     @08,ESTAELNG                                     02S0161 00155000
         BCTR  @08,0                                            02S0161 00156000
         EX    @08,@SM03686                                     02S0161 00157000
*                                      /*MOVE TO AUTO CORE   @Z40FPPG*/ 00158000
*    GEN REFS(ESTAECOR) SETS(REG15,TCBSTABB);                   02S0162 00159000
      ESTAE (2),PARAM=(3),MF=(E,ESTAECOR)                               00160000
*    RFY (REG2,REG3) UNRSTD;                                    02S0163 00161000
*    RTNCODE = REG15;                  /*SAVE RETURN CODE    @Z40FPPG*/ 00162000
         ST    REG15,RTNCODE                                    02S0164 00163000
*    IF RTNCODE = ZERO THEN            /*IF ESTAE SUCCESSFUL @Z40FPPG*/ 00164000
         L     @08,RTNCODE                                      02S0165 00165000
         LTR   @08,@08                                          02S0165 00166000
         BNZ   @RF00165                                         02S0165 00167000
*      ASWASCBP = TCBSTABB;            /* CURRENT SCB        @Z40FPPG*/ 00168000
         L     @08,@PC00001+20                                  02S0166 00169000
