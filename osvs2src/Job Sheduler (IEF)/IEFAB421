         CR    @14,@08                                          02S0153 00135000
         BE    @RF00153                                         02S0153 00136000
@RT00153 DS    0H                                               02S0154 00137000
*  DO;                                 /*PROCESS NEW ESTAE   @Z40FPPG*/ 00138000
*    ISSUESTA = ON;                    /*SAY ESTAE ISSUED    @Z40FPPG*/ 00139000
         OI    ISSUESTA,B'10000000'                             02S0155 00140000
*    ASWAAUTO = ADDR(AB4EDPMS);        /*1ST ASWA GIVES W/A  @Z40FPPG*/ 00141000
         L     @08,ASWAPTR                                      02S0156 00142000
         LA    @15,AB4EDPMS                                     02S0156 00143000
         STCM  @15,15,ASWAAUTO(@08)                             02S0156 00144000
*    ASWAOLDP = JSCBASWA;              /*OLD LAST IF ANY     @Z40FPPG*/ 00145000
         L     @15,@PC00001+8                                   02S0157 00146000
         L     @01,PPJSCBP(,@15)                                02S0157 00147000
