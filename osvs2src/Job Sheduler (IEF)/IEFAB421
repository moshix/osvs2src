         BNO   @RF00531                                         02S0531 01408000
*        EREQJSCT = ON;                                         02S0532 01409000
         L     @08,EREQPTR                                      02S0532 01410000
         OI    EREQJSCT(@08),B'00000100'                        02S0532 01411000
*      ELSE;                                                    02S0533 01412000
@RF00531 DS    0H                                               02S0534 01413000
*      IF SIOTTERM=ON THEN         /*IF A TERM=TS                    */ 01414000
