*   GTSTR2 = SUBPOOL; /* SET SUBPOOL*/                          02S0193 00288000
         LA    GTSTR2,230                                       02S0193 00289000
*   GENERATE CODE;                                              02S0194 00290000
     GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN                               00291000
*   GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/               02S0195 00292000
         ST    GTSTR15,GTSTRTCD                                 02S0195 00293000
*   ALCWAPTR = GTSTR1; /* ADDR OF GOTN STORG*/                  02S0196 00294000
         ST    GTSTR1,ALCWAPTR                                  02S0196 00295000
*   RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */                02S0197 00296000
