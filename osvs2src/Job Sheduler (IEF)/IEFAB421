         NI    ENQQ4(EXITP),B'01011111'                         03S0417 00949000
*                WTOLIST=OFFWTO;        /*PUT OFFLINE MSG IN MSG BUFF*/ 00950000
         L     @15,@CA01680                                     03S0418 00951000
         MVC   WTOLIST(32),OFFWTO-OFFWTO(@15)                   03S0418 00952000
