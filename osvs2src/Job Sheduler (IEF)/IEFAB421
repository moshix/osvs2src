*      EREQRPOS=ONE;               /*SET REL POS TO ONE              */ 01387000
         STH   @03,EREQRPOS(,@08)                               02S0523 01388000
*      EREQSIOT=SIOTPTR;           /*POINT TO SIOT                   */ 01389000
         ST    SIOTPTR,EREQSIOT(,@08)                           02S0524 01390000
