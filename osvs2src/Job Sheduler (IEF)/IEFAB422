*  IF SIOTALCD = OFF THEN              /*SIOT NOT ALLOCATED          */ 00074000
@RC00079 TM    SIOTALCD(SIOTPTR),B'00000010'                    01S0085 00075000
         BNZ   @RF00085                                         01S0085 00076000
*    DO;                               /*INTERFACE WITH JES3         */ 00077000
*      TOTALCTR = TOTALCTR + 1;        /*UP UNALLOCATED SIOT COUNT   */ 00078000
         LA    @14,1                                            01S0087 00079000
         AH    @14,TOTALCTR                                     01S0087 00080000
         STH   @14,TOTALCTR                                     01S0087 00081000
*/********************************************************************/ 00082000
*/*                                                                  */ 00083000
*/* SEGMENT NAME - EXTNSSOB                                          */ 00084000
*/*                                                                  */ 00085000
*/* DESCRIPTIVE NAME - BUILD FUNCTION DEPENDENT SSOB                 */ 00086000
*/*                    EXTENSION                                     */ 00087000
*/*                                                                  */ 00088000
*/* FUNCTION -                                                       */ 00089000
*/*   FOR EACH UNALLOCATED SIOT, THIS ROUTINE BUILDS THE             */ 00090000
*/*   FUNCTION DEPENDENT EXTENSION TO THE SSOB. THIS                 */ 00091000
*/*   EXTENSION, ALONG WITH THE SSOB, SERVES AS INPUT TO             */ 00092000
*/*   JES3.                                                          */ 00093000
*/*                                                                  */ 00094000
*/*   OPERATION - NO SPECIAL ALGORITHMS ARE USED                     */ 00095000
*/*                                                                  */ 00096000
*/* ENTRY - IN-LINE SUBROUTINE                                       */ 00097000
*/*   PURPOSE - BUILD FUNCTION DEPENDENT SSOB EXTENSION              */ 00098000
*/*   INPUT -                                                        */ 00099000
*/*     STEPNUM - STEP NUMBER                                        */ 00100000
*/*     SIOT -                                                       */ 00101000
*/*       SCTDDNAM - DDNAME                                          */ 00102000
*/*       SIOTOTUN - TOTAL NO. OF UNITS NEEDED FOR REQUEST           */ 00103000
*/*       SIOTCATL & SIOTGDGA - BITS INDICATING PRIVATE              */ 00104000
*/*                             CATALOG REQUEST                      */ 00105000
*/*     JFCB -                                                       */ 00106000
*/*       JFCBDSNM - DATA SET NAME                                   */ 00107000
*/*                                                                  */ 00108000
*/* EXIT-NORMAL -                                                    */ 00109000
*/*   CONDITION - SSOB FUNCTION DEPENDENT EXTENSION BUILT            */ 00110000
*/*   OUTPUT - SSOB FUNCTION DEPENDENT EXTENSION                     */ 00111000
*/*   RETURN CODES - NONE                                            */ 00112000
*/*                                                                  */ 00113000
*/* EXIT-ERROR - NONE                                                */ 00114000
*/*                                                                  */ 00115000
*/********************************************************************/ 00116000
*SSOBRETN = ZERO;                      /*CLEAR RET CODE AREA         */ 00117000
         SLR   @10,@10                                          02S0088 00118000
         ST    @10,SSOBRETN                                     02S0088 00119000
*SSCA = SSCA && SSCA;                  /*CLEAR EXTENSION             */ 00120000
         XC    SSCA(32),SSCA                                    02S0089 00121000
*SSCALEN = LENGTH(SSCA);               /*LENGTH OF SSCA TO SSCA      */ 00122000
         MVC   SSCALEN(2),@CH00169                              02S0090 00123000
*JESFLAGS = JESFLAGS && JESFLAGS;      /*CLEAR JES3 FLAGS            */ 00124000
         XC    JESFLAGS(2),JESFLAGS                             02S0091 00125000
*SSCAPFLG = ADDR(JESFLAGS);            /*PTR TO FLAGS TO SSCA        */ 00126000
         LA    @10,JESFLAGS                                     02S0092 00127000
         ST    @10,SSCAPFLG                                     02S0092 00128000
*IF TOTALCTR = TOTREQS THEN            /*LAST CALL TO JES3 FOR STEP  */ 00129000
         L     @10,@PC00001                                     02S0093 00130000
         L     @10,ALCWAPTR(,@10)                               02S0093 00131000
         C     @14,TOTREQS(,@10)                                02S0093 00132000
         BNE   @RF00093                                         02S0093 00133000
*  LASTDD = ON;                        /*INDICATE LAST DD FOR STEP   */ 00134000
         OI    LASTDD,B'01000000'                               02S0094 00135000
*IF CALLAGIN =ON THEN                  /*RECALL JES3 FOR DD          */ 00136000
@RF00093 L     @14,@PC00001+8                                   02S0095 00137000
         TM    CALLAGIN(@14),B'10000000'                        02S0095 00138000
         BNO   @RF00095                                         02S0095 00139000
*  EXTRCALL = ON;                      /*INDICATE THIS IS RECALL     */ 00140000
         OI    EXTRCALL,B'10000000'                             02S0096 00141000
*SSCAPSTN = ADDR(STEPNUM);             /*PTR TO STEP NUMBER          */ 00142000
@RF00095 L     @14,@PC00001+4                                   02S0097 00143000
         ST    @14,SSCAPSTN                                     02S0097 00144000
*SSCAPDDN = ADDR(SAVEDDNM);            /*DDNAME PTR TO SSCA          */ 00145000
         LA    @14,SAVEDDNM                                     02S0098 00146000
         ST    @14,SSCAPDDN                                     02S0098 00147000
*SSCAPRPN = ADDR(CONCATNO);            /*REL. POS. NO. TO SSCA       */ 00148000
         LA    @14,CONCATNO                                     02S0099 00149000
         ST    @14,SSCAPRPN                                     02S0099 00150000
*IF SIOTCATL = ON &                    /*PRIVATE CATLG REQ.          */ 00151000
*   SIOTGDGA = ON THEN                                          02S0100 00152000
         TM    SIOTCATL(SIOTPTR),B'01000000'                    02S0100 00153000
         BNO   @RF00100                                         02S0100 00154000
         TM    SIOTGDGA(SIOTPTR),B'01000000'                    02S0100 00155000
         BNO   @RF00100                                         02S0100 00156000
*  SSCAPDSN = ADDR(SJFCBPTR->JFCBDSNM); /*DSNAME PTR TO SSCA         */ 00157000
         L     @14,SJFCBPTR(,SIOTPTR)                           02S0101 00158000
         ST    @14,SSCAPDSN                                     02S0101 00159000
*SSCAPNUN = ADDR(SIOTOTUN);            /*PTR TO NO. UNITS            */ 00160000
@RF00100 LA    @14,SIOTOTUN(,SIOTPTR)                           02S0102 00161000
         ST    @14,SSCAPNUN                                     02S0102 00162000
*SSCAPUAR = ADDR(JES3UCBS);            /*PTR TO UCB RET AREA         */ 00163000
         LA    @14,JES3UCBS                                     02S0103 00164000
         ST    @14,SSCAPUAR                                     02S0103 00165000
*JES3UCBS = JES3UCBS && JES3UCBS;      /*CLEAR UCB RET AREA          */ 00166000
         XC    JES3UCBS(236),JES3UCBS                           02S0104 00167000
*                                                               01S0105 00168000
*       /* ? IEFSSREQ  (SSOB);*/                                01S0105 00169000
*/*                                           */                01S0105 00170000
*/*  PASS CONTROL TO JOB ENTRY SUBSYSTEM TO   */                01S0105 00171000
*/*  PROCESS REQUEST -                        */                01S0105 00172000
*/*                                           */                01S0105 00173000
*/*    INPUT - REGISTER 1 POINTS TO A ONE-    */                01S0105 00174000
*/*            WORD PARAMETER LIST WHICH      */                01S0105 00175000
*/*            POINTS TO THE SSOB             */                01S0105 00176000
*/*                                           */                01S0105 00177000
*CALL SSREQ(SSOB);               /*EXIT TO JES3                */       00178000
         LA    @14,SSOB                                         01S0105 00179000
         ST    @14,@AL00001                                     01S0105 00180000
         MVI   @AL00001,X'80'                                   01S0105 00181000
         L     @10,CVTPTR                                       01S0105 00182000
         L     @10,CVTJESCT(,@10)                               01S0105 00183000
         L     @15,JESSSREQ(,@10)                               01S0105 00184000
         LA    @01,@AL00001                                     01S0105 00185000
         BALR  @14,@15                                          01S0105 00186000
*      IF SSOBRETN = SSCAJESA THEN     /*JES3 CHOSE DEVICES          */ 00187000
         CLC   SSOBRETN(4),@CF00082                             01S0106 00188000
         BNE   @RF00106                                         01S0106 00189000
*        DO;                           /*SHOW JES3 SELECTIONS        */ 00190000
*/********************************************************************/ 00191000
*/*                                                                  */ 00192000
*/* SEGMENT NAME - MARKJES3                                          */ 00193000
*/*                                                                  */ 00194000
*/* DESCRIPTIVE NAME - INDICATE JES3 SELECTIONS                      */ 00195000
*/*                                                                  */ 00196000
*/* FUNCTION -                                                       */ 00197000
*/*   THIS ROUTINE RECEIVES CONTROL EVERY TIME JES3 SELECTS          */ 00198000
*/*   DEVICES TO BE ALLOCATED TO A REQUEST. IT PERFORMS THE          */ 00199000
*/*   FOLLOWING FUNCTIONS IF SU 18 IS NOT IN THE SYSTEM:     @G18MP21*/ 00200000
*/*     .UPDATES THE EDL SO THAT ENTRIES FOR UNSELECTED UCBS         */ 00201000
*/*      ARE MARKED INELIGIBLE AND ENTRIES FOR GROUPS WHICH          */ 00202000
*/*      CONTAIN NO SELECTED UCBS ARE MARKED INELIGIBLE.             */ 00203000
*/*     .STORES THE UCB ADDRESSES WHICH JES3 SELECTED IN             */ 00204000
*/*      THE VOLUNIT TABLE ENTRIES WHICH                             */ 00205000
*/*      ARE MARKED DEVICE REQUIRED. THIS IS DONE SO THAT            */ 00206000
*/*      IEFAB490 CAN REORDER THE TIOT IF NECESSARY.                 */ 00207000
*/*     .STORES THE GROUP ID OF THE UCB SELECTED BY JES3 IN          */ 00208000
*/*      THE VOLUNIT TABLE. THIS GROUP ID IS USED BY                 */ 00209000
*/*      GENERIC ALLOCATION WHEN BUILDING THE ALGORITHM              */ 00210000
*/*      TABLES.                                                     */ 00211000
*/*     .TURNS ON THE JES3 BIT IN THE SIOT.                          */ 00212000
*/*   IF SU 18 IS IN THE SYSTEM THE FOLLOWING FUNCTIONS ARE  @G18MP21*/ 00213000
*/*   PERFORMED:                                             @G18MP21*/ 00214000
*/*     .STORES THE ADDRESSES OF THE UCB POOLS IN THE        @G18MP21*/ 00215000
*/*      VOLUNIT TABLE                                       @G18MP21*/ 00216000
*/*     .UPDATES THE TOTAL NUMBER OF UCBS RETURNED IN THE    @G18MP21*/ 00217000
*/*      UCB POOLS FROM JES3.                                @G18MP21*/ 00218000
*/*     .TURNS ON THE JES3 BIT IN THE SIOT                   @G18MP21*/ 00219000
*/*                                                                  */ 00220000
*/*   OPERATION - NO SPECIAL ALGORITHMS ARE USED                     */ 00221000
*/*                                                                  */ 00222000
*/* ENTRY - IN-LINE SUBROUTINE                                       */ 00223000
*/*   PURPOSE - MAKE ALL UNITS NOT SELECTED BY JES3                  */ 00224000
*/*             INELIGIBLE                                           */ 00225000
*/*   INPUT -                                                        */ 00226000
*/*     SIOT -                                                       */ 00227000
*/*       SIOTEDLP - PTR TO EDL FOR REQUEST                          */ 00228000
*/*       SVOLUNAD - PTR TO 1ST VOLUNIT ENTRY FOR REQUEST            */ 00229000
*/*       SVOLUNNO - NUMBER OF VOLUNIT ENTRIES FOR REQUEST           */ 00230000
*/*       TOTUCB   - TOTAL NUMBER OF UCBS RETURNED FROM JES3 @G18MP21*/ 00231000
*/*                                                                  */ 00232000
*/* EXIT-NORMAL -                                                    */ 00233000
*/*   CONDITION - EDL,VOLUNIT TABLE, AND SIOT UPDATED                */ 00234000
*/*   OUTPUT -                                                       */ 00235000
*/*     EDLJINEL - SET TO 1 IN EDL GROUP ENTRIES WHICH DO            */ 00236000
*/*                NOT CONTAIN UCBS SELECTED BY JES3                 */ 00237000
*/*     EDLNOJES - SET TO 1 IN EDL UCB ENTRIES WHICH WERE            */ 00238000
*/*                NOT SELECTED BY JES3                              */ 00239000
*/*     VUUCBP - CONTAINS ADDRESS OF UCB SELECTED BY JES3            */ 00240000
*/*              OR THE POINTER TO THE UCB POOL FOR A SYSTEM @G18MP21*/ 00241000
*/*              WITH SU 18.                                 @G18MP21*/ 00242000
*/*     VUGRID - CONTAINS GROUP ID OF UCB SELECTED BY JES3           */ 00243000
*/*     SIOTJES3 - SET TO 1 TO INDICATE JES3 CHOSE DEVICES           */ 00244000
*/*     TOTUCB - TOTAL NUMBER OF UCBS RETURNED FROM JES3     @G18MP21*/ 00245000
*/*   RETURN CODES - NONE                                            */ 00246000
*/*                                                                  */ 00247000
*/* EXIT-ERROR - NONE                                                */ 00248000
*/*                                                                  */ 00249000
*/********************************************************************/ 00250000
*IF SU18BIT=ON THEN                    /*SU18 IN THE SYSTEM? @G18MP21*/ 00251000
         L     @14,CVTPTR                                       02S0108 00252000
         L     @14,CVTIHASU(,@14)                               02S0108 00253000
         TM    SU18BIT(@14),B'00100000'                         02S0108 00254000
         BNO   @RF00108                                         02S0108 00255000
*  DO;                                 /* YES,               @G18MP21*/ 00256000
*    VUPTR=SVOLUNAD;                   /* SET PTR TO VOLUNIT TABLE      00257000
*                                                            @G18MP21*/ 00258000
         L     VUPTR,SVOLUNAD(,SIOTPTR)                         02S0110 00259000
*    UCBCTR=ZERO;                      /*INIT COUNTER        @G18MP21*/ 00260000
         SLR   UCBCTR,UCBCTR                                    02S0111 00261000
*    DO I=1 TO SVOLUNNO;               /*SET PTRS TO UCB POOLS  02S0112 00262000
*                                                            @G18MP21*/ 00263000
         LA    I,1                                              02S0112 00264000
         B     @DE00112                                         02S0112 00265000
@DL00112 DS    0H                                               02S0113 00266000
*      IF VDEVREQD=ON THEN             /*DEVICE REQUIRED     @G18MP21*/ 00267000
         TM    VDEVREQD(VUPTR),B'00100000'                      02S0113 00268000
         BNO   @RF00113                                         02S0113 00269000
*        DO;                           /*SET UCB POOL PTR    @G18MP21*/ 00270000
*          UCBCTR=UCBCTR+ONE;          /*ENTRY IN ARRAY JES3 RETURNED   00271000
*                                                            @G18MP21*/ 00272000
         AL    UCBCTR,@CF00044                                  02S0115 00273000
*          VUUCBP =JESUCB(UCBCTR);     /*SET UCB POOL PTR    @G18MP21*/ 00274000
         LR    @14,UCBCTR                                       02S0116 00275000
         SLA   @14,2                                            02S0116 00276000
         L     @14,JESUCB-4(@14)                                02S0116 00277000
         ST    @14,VUUCBP(,VUPTR)                               02S0116 00278000
*          TOTUCB  = TOTUCB+VUPOOL#;   /*UPDATE TOTAL UCB'S  @G18MP21*/ 00279000
         L     @10,TOTUCB                                       02S0117 00280000
         AL    @10,VUPOOL#(,@14)                                02S0117 00281000
         ST    @10,TOTUCB                                       02S0117 00282000
*        END;                          /*END SET PTR UCB PLS @G18MP21*/ 00283000
*      VUPTR=VUPTR+LENGTH(VOLUNTAB);/*POINT TO NEXT VU ENTRY    02S0119 00284000
*                                                            @G18MP21*/ 00285000
@RF00113 AL    VUPTR,@CF01952                                   02S0119 00286000
*    END;                              /*END                 @G18MP21*/ 00287000
         AL    I,@CF00044                                       02S0120 00288000
@DE00112 CH    I,SVOLUNNO(,SIOTPTR)                             02S0120 00289000
         BNH   @DL00112                                         02S0120 00290000
*    SIOTJES3=ON;                      /*SIOT JES3 REQUEST   @G18MP21*/ 00291000
         OI    SIOTJES3(SIOTPTR),B'00100000'                    02S0121 00292000
* END;                                 /*END SU18 PROCESSING @G18MP21*/ 00293000
*ELSE                                  /*NON SU18 SYSTEM     @G18MP21*/ 00294000
*  DO;                                 /*PROCESS NON SU 18   @G18MP21*/ 00295000
         B     @RC00108                                         02S0123 00296000
@RF00108 DS    0H                                               02S0124 00297000
*    EDLPTR = EDLFGENP;                /*PTR 1ST GENERIC ENT         */ 00298000
         L     @14,SIOTEDLP(,SIOTPTR)                           02S0124 00299000
         L     EDLPTR,EDLFGENP(,@14)                            02S0124 00300000
*    DO WHILE(EDLPTR ^= ZERO);         /*SCAN GENERIC ENTS           */ 00301000
         B     @DE00125                                         02S0125 00302000
@DL00125 DS    0H                                               02S0126 00303000
*      EDLGNTP = EDLGRPTR;             /*PTR TO GROUP ENTRY          */ 00304000
         L     EDLGNTP,EDLGRPTR(,EDLPTR)                        02S0126 00305000
*      DO WHILE EDLGNTP ^= ZERO;       /*SCAN GROUP ENTRIES          */ 00306000
         B     @DE00127                                         02S0127 00307000
@DL00127 DS    0H                                               02S0128 00308000
*        EDLUCBLP = EDLLISTP;          /*PTR TO UCB ENTRY            */ 00309000
         L     EDLUCBLP,EDLLISTP(,EDLGNTP)                      02S0128 00310000
*        JESGRPSW = OFF;               /*ASSUME GRP INELIG.          */ 00311000
         NI    JESGRPSW,B'01111111'                             02S0129 00312000
*        DO UEDLCTR = 1 TO EDLUCBNO;   /*SCAN GROUP'S UCBS           */ 00313000
         LA    UEDLCTR,1                                        02S0130 00314000
         B     @DE00130                                         02S0130 00315000
@DL00130 DS    0H                                               02S0131 00316000
*          NOJES = ON;                 /*ASSUME NOT JES GRP          */ 00317000
         OI    NOJES,B'10000000'                                02S0131 00318000
*          DO UCBCTR = 1 TO SIOTOTUN   /*SCAN LIST OF UCBS           */ 00319000
*             WHILE NOJES = ON;        /*UNTIL MATCH FOUND           */ 00320000
         LA    UCBCTR,1                                         02S0132 00321000
         B     @DE00132                                         02S0132 00322000
@DL00132 TM    NOJES,B'10000000'                                02S0132 00323000
         BNO   @DC00132                                         02S0132 00324000
*            IF EDLUCBAD(UEDLCTR) = JESUCB(UCBCTR) THEN /*MATCH      */ 00325000
         LR    @14,UEDLCTR                                      02S0133 00326000
         SLA   @14,2                                            02S0133 00327000
         LR    @09,UCBCTR                                       02S0133 00328000
         SLA   @09,2                                            02S0133 00329000
         AL    @14,@CF01958                                     02S0133 00330000
         LH    @14,EDLUCBAD(@14,EDLUCBLP)                       02S0133 00331000
         N     @14,@CF01959                                     02S0133 00332000
         C     @14,JESUCB-4(@09)                                02S0133 00333000
         BNE   @RF00133                                         02S0133 00334000
*              DO;                                              02S0134 00335000
*                VUPTR = SVOLUNAD;     /*PTR 1ST VU FOR REQ.         */ 00336000
         L     VUPTR,SVOLUNAD(,SIOTPTR)                         02S0135 00337000
*                NOJES = OFF;          /*EXIT LOOP                   */ 00338000
         NI    NOJES,B'01111111'                                02S0136 00339000
*                JESGRPSW = ON;        /*GROUP IS ELIGIBLE           */ 00340000
         OI    JESGRPSW,B'10000000'                             02S0137 00341000
*                FOUNDSW = OFF;        /*INIT LOOP CONTROL           */ 00342000
         NI    FOUNDSW,B'01111111'                              02S0138 00343000
*                VOLUNCTR  = ZERO;     /*INIT VU ENT CTR             */ 00344000
         SLR   VOLUNCTR,VOLUNCTR                                02S0139 00345000
*                DO VUCTR = 1 TO SVOLUNNO  /*SCAN VU ENTRIES         */ 00346000
*                   WHILE FOUNDSW = OFF;                        02S0140 00347000
         LA    @14,1                                            02S0140 00348000
         B     @DE00140                                         02S0140 00349000
@DL00140 TM    FOUNDSW,B'10000000'                              02S0140 00350000
         BNZ   @DC00140                                         02S0140 00351000
*                  IF VDEVREQD = ON THEN   /*DEVICE IS REQUIRED      */ 00352000
         TM    VDEVREQD(VUPTR),B'00100000'                      02S0141 00353000
         BNO   @RF00141                                         02S0141 00354000
*                    DO;                                        02S0142 00355000
*                      VOLUNCTR = VOLUNCTR + 1; /*UP VU CTR          */ 00356000
         AL    VOLUNCTR,@CF00044                                02S0143 00357000
*                      IF VOLUNCTR = UCBCTR THEN /*RIGHT VU ENT      */ 00358000
         CR    VOLUNCTR,UCBCTR                                  02S0144 00359000
         BNE   @RF00144                                         02S0144 00360000
*                        DO;                                    02S0145 00361000
*                          FOUNDSW = ON;   /*EXIT LOOP               */ 00362000
         OI    FOUNDSW,B'10000000'                              02S0146 00363000
*                          VUUCBP = JESUCB(UCBCTR); /*UCB ADDR       */ 00364000
