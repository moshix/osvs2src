         L     @14,ALCWAPTR(,@14)                               01S0077 00044000
         L     SIOTPTR,SIOT1P(,@14)                             01S0077 00045000
*DO WHILE(SIOTPTR ^= ZERO);            /*PROCESS ALL SIOTS           */ 00046000
         B     @DE00078                                         01S0078 00047000
@DL00078 DS    0H                                               01S0079 00048000
*  IF SCTDDNAM ^= NONAME &             /*NON BLANK DDNAME AND        */ 00049000
*     ((SCTDDNAM = JOBCAT | SCTDDNAM = STEPCAT) | /*JOBCAT OR STEPCAT*/ 00050000
*        SIOTJSCT = OFF) THEN          /*NOT IN CONCATENATION        */ 00051000
         CLC   SCTDDNAM(8,SIOTPTR),NONAME                       01S0079 00052000
