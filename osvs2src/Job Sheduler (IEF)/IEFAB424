*  MSSELIG = ON;                       /* SET MSS REQUESTS ELIGIBLE     00058000
*                                                            @G24LPSJ*/ 00059000
         B     @RC00067                                         01S0071 00060000
@RF00067 OI    MSSELIG,B'10000000'                              01S0071 00061000
*DO SIOTCTR = ONE TO NUMREQS WHILE(RETNCODE=ZERO); /* PROC ALL SIOTS */ 00062000
@RC00067 LA    @10,1                                            01S0072 00063000
         B     @DE00072                                         01S0072 00064000
@DL00072 ICM   @10,3,RETNCODE                                   01S0072 00065000
         BNZ   @DC00072                                         01S0072 00066000
