         BNO   @RF00068                                         01S0068 00045000
         TM    ICBB(@08),B'00010000'                            01S0068 00046000
         BNZ   @RF00068                                         01S0068 00047000
*    MSSELIG = ON;                     /* SET MSS REQUESTS ELIGIBLE     00048000
*                                                            @G24LPSJ*/ 00049000
         OI    MSSELIG,B'10000000'                              01S0069 00050000
*  ELSE                                /* ELSE               @G24LPSJ*/ 00051000
*    MSSELIG = OFF;                    /* SET MSS REQUEST INELIGIBLE    00052000
*                                                            @G24LPSJ*/ 00053000
         B     @RC00068                                         01S0070 00054000
@RF00068 NI    MSSELIG,B'01111111'                              01S0070 00055000
*ELSE                                  /* IF NO SU 24 INSTALLED 01S0071 00056000
