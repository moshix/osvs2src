         ST    EDLUCBLP,EDLLISTP(,EDLGNTP)                      02S0245 00890000
*    EDLUCBAD(ONE) = UCBPTR;           /* UCB ADDRESS IN UCB LIST    */ 00891000
         STH   UCBPTR,EDLUCBAD(,EDLUCBLP)                       02S0246 00892000
*    IF UCB3COMM ^= ON THEN            /* NOT COMMUNICATION DEVICE   */ 00893000
         TM    UCB3COMM(UCBPTR),B'01000000'                     02S0247 00894000
         BO    @RF00247                                         02S0247 00895000
