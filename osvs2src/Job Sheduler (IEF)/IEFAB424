         ALR   @08,EDLGNTP                                      02S0240 00878000
         LR    EDLUCBLP,@08                                     02S0240 00879000
*    UCBPTR = SCTUTYPE(7:8);           /* SET BASE FOR UCB           */ 00880000
         SLR   UCBPTR,UCBPTR                                    02S0241 00881000
         ICM   UCBPTR,3,SCTUTYPE+6(SIOTPTR)                     02S0241 00882000
