*IF SU24BIT = ON THEN                  /* IF SU 24 INSTALLED @G24LPSJ*/ 00033000
         L     @01,CVTIHASU(,@08)                               01S0067 00034000
         TM    SU24BIT(@01),B'10000000'                         01S0067 00035000
         BNO   @RF00067                                         01S0067 00036000
*  IF CVTICB ^= ZERO&                  /* THEN IF ICB EXISTS @G24LPSJ*/ 00037000
*     ICBI = ON&                       /* AND MSS IS INIT.   @G24LPSJ*/ 00038000
*     ICBB = OFF THEN                  /* AND MSS NOT BROKEN @G24LPSJ*/ 00039000
         L     @08,CVTEXT2(,@08)                                01S0068 00040000
         L     @08,CVTICB(,@08)                                 01S0068 00041000
