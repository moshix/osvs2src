         ST    @06,EDLGRPNO(,EDLPTR)                            02S0242 00884000
*    EDLGRPTR = ADDR(EDLGNTRY);        /* PTR TO FIRST GROUP ENTRY   */ 00885000
         ST    EDLGNTP,EDLGRPTR(,EDLPTR)                        02S0243 00886000
