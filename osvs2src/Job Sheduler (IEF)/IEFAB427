         B     @DE00313                                         02S0313 01023000
@DL00313 DS    0H                                               02S0314 01024000
*      SIOTPTR = SARBSPTR;             /*GET PTR TO SIOT     @G29AN2F*/ 01025000
         L     SIOTPTR,SARBSPTR(,SSARBPTR)                      02S0314 01026000
*      SSDATAP = SSAGSSCM;             /*GET SUBSYSTEM DATA  @G29AN2F*/ 01027000
         L     SSDATAP,SSAGSSCM(,SSARBPTR)                      02S0315 01028000
*      SSNAMEP = SSAGSSNM;             /*PTR TO SUBSYSTEM NM @G29AN2F*/ 01029000
         L     SSNAMEP,SSAGSSNM(,SSARBPTR)                      02S0316 01030000
*      CALL BLDTIOT;                   /*BUILD DSAB & TIOT   @G29AN2F*/ 01031000
         BAL   @14,BLDTIOT                                      02S0317 01032000
*      SSARBPTR = SSAGNRBP;            /*POINT TO NEXT RB    @G29AN2F*/ 01033000
         L     SSARBPTR,SSAGNRBP(,SSARBPTR)                     02S0318 01034000
*    END;                              /*                    @G29AN2F*/ 01035000
@DE00313 SLR   @08,@08                                          02S0319 01036000
         CR    SSARBPTR,@08                                     02S0319 01037000
         BE    @DC00313                                         02S0319 01038000
         CR    RETCODE,@08                                      02S0319 01039000
         BE    @DL00313                                         02S0319 01040000
