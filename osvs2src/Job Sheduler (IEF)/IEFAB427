         CLC   SSOBRETN(4),@CF00047                             02S0460 01689000
         BNE   @RF00460                                         02S0460 01690000
*           SIOTRSNC= NOWAITCD;       /*WAITING FOR UNITS NOT ALLOWED*/ 01691000
         MVC   SIOTRSNC(2,SIOTPTR),@CH00102                     02S0461 01692000
*        ELSE                                                   02S0462 01693000
*          IF SSOBRETN = SSALCREQ THEN /*CANCEL REQUESTED            */ 01694000
         B     @RC00460                                         02S0462 01695000
@RF00460 CLC   SSOBRETN(4),@CF00109                             02S0462 01696000
         BNE   @RF00462                                         02S0462 01697000
*            SSRSNC   = CANCELCD;      /*JOB CANCELED                */ 01698000
         L     @10,@PC00001                                     02S0463 01699000
         L     @10,ALCWAPTR(,@10)                               02S0463 01700000
         L     @10,FAILRSCP(,@10)                               02S0463 01701000
         MVC   SSRSNC(2,@10),@CH00091                           02S0463 01702000
*          ELSE                                                 02S0464 01703000
*            IF SSOBRETN = SSALIDST THEN /*INVALID DESTINATION       */ 01704000
         B     @RC00462                                         02S0464 01705000
@RF00462 CLC   SSOBRETN(4),@CF00959                             02S0464 01706000
         BNE   @RF00464                                         02S0464 01707000
*               SIOTRSNC = BADDEST;      /*BAD DESTINATION REQUESTED */ 01708000
         MVC   SIOTRSNC(2,SIOTPTR),@CH00089                     02S0465 01709000
*            ELSE                                               02S0466 01710000
*              IF SSOBRETN = SSALNAUT THEN /*AUTHORIZER         02S0466 01711000
*                                            REQUESTOR OF SUBSYSTEM  */ 01712000
         B     @RC00464                                         02S0466 01713000
@RF00464 CLC   SSOBRETN(4),@CF00071                             02S0466 01714000
         BNE   @RF00466                                         02S0466 01715000
*                SIOTRSNC = BADAUTH;   /*SET DD RELATED RSN CODE     */ 01716000
         MVC   SIOTRSNC(2,SIOTPTR),@CH00087                     02S0467 01717000
*              ELSE                                             02S0468 01718000
*                SSRSNC = NOALLOC;     /*UNABLE TO ALLOC SUBSYSTEM      01719000
*                                        DATA SET.                   */ 01720000
         B     @RC00466                                         02S0468 01721000
@RF00466 L     @10,@PC00001                                     02S0468 01722000
         L     @10,ALCWAPTR(,@10)                               02S0468 01723000
         L     @10,FAILRSCP(,@10)                               02S0468 01724000
         MVC   SSRSNC(2,@10),@CH00042                           02S0468 01725000
*      END;                                                     02S0469 01726000
*    ELSE                                                       02S0470 01727000
*      SSRSNC = NOALLOC;               /*UNABLE TO ALLOCATE SUB-        01728000
*                                           SYSTEM DATA SETS         */ 01729000
         B     @RC00458                                         02S0470 01730000
@RF00458 L     @10,@PC00001                                     02S0470 01731000
         L     @10,ALCWAPTR(,@10)                               02S0470 01732000
         L     @10,FAILRSCP(,@10)                               02S0470 01733000
         MVC   SSRSNC(2,@10),@CH00042                           02S0470 01734000
*    RETCODE = ERROR;                  /*ERROR RETURN CODE           */ 01735000
@RC00458 LA    RETCODE,4                                        02S0471 01736000
*  END;                                /*                    @G29AN2F*/ 01737000
*RETURN;                               /*RETURN TO CALLER    @G29AN2F*/ 01738000
@EL00002 DS    0H                                               02S0473 01739000
@EF00002 DS    0H                                               02S0473 01740000
@ER00002 LM    @14,@09,@SA00002                                 02S0473 01741000
         LM    @11,@12,@SA00002+48                              02S0473 01742000
         BR    @14                                              02S0473 01743000
*END ALOCNGRP;                         /*END NON GROUP REQUEST  02S0474 01744000
*                                        PROCESSING          @G29AN2F*/ 01745000
         B     @EL00002                                         02S0474 01746000
*/********************************************************************/ 01747000
*/*                                                          @G29AN2F*/ 01748000
*/* SEGMENT NAME - BLDTIOT                                   @G29AN2F*/ 01749000
*/*                                                          @G29AN2F*/ 01750000
*/* DESCRIPTIVE NAME - BUILD DSAB & TIOT ENTRIES COMMON      @G29AN2F*/ 01751000
*/*                    ROUTINE                               @G29AN2F*/ 01752000
*/*                                                          @G29AN2F*/ 01753000
*/* FUNCTION -                                               @G29AN2F*/ 01754000
*/*   THE PURPOSE OF THIS ROUTINE IS TO BUILD A REQUEST      @G29AN2F*/ 01755000
*/*   BLOCK FOR INPUT TO THE BUILD DSAB/TIOT ENTRY ROUTINE   @G29AN2F*/ 01756000
*/*   IEFAB428, CALL IEFAB428 AND UPDATE THE DSAB, TIOT,     @G29AN2F*/ 01757000
*/*   AND SIOT INDICATORS TO COMPLETE THE ALLOCATION AND TO  @G29AN2F*/ 01758000
*/*   DECREMENT THE COMMON ALLOCATION COUNTERS.              @G29AN2F*/ 01759000
*/*                                                          @G29AN2F*/ 01760000
*/* ENTRY - BLDTIOT                                          @G29AN2F*/ 01761000
*/*                                                          @G29AN2F*/ 01762000
*/*   PURPOSE - SEE FUNCTION                                 @G29AN2F*/ 01763000
*/*                                                          @G29AN2F*/ 01764000
*/*   INPUT -                                                @G29AN2F*/ 01765000
*/*     SIOTPTR - POINTER TO SIOT                            @G29AN2F*/ 01766000
*/*                                                          @G29AN2F*/ 01767000
*/* EXIT - RETURN                                            @G29AN2F*/ 01768000
*/*                                                          @G29AN2F*/ 01769000
*/*  CONDITION - NO ERROR                                    @G29AN2F*/ 01770000
*/*    OUTPUT -                                              @G29AN2F*/ 01771000
*/*      DSAB CONSTRUCTED AND MARKED AS A SUBSYSTEM REQUEST  @G29AN2F*/ 01772000
*/*      TIOT ENTRY MADE AND MARKED AS A SUBSYSTEM REQUEST   @G29AN2F*/ 01773000
*/*      SIOT MARKED AS ALLOCATED                            @G29AN2F*/ 01774000
*/*      COUNT FIELDS (TOTREQ AND SUBSREQS) IN ALCWA         @G29AN2F*/ 01775000
*/*       DECREMENTED                                        @G29AN2F*/ 01776000
*/*    RETURN CODE - ZERO                                    @G29AN2F*/ 01777000
*/*                                                          @G29AN2F*/ 01778000
*/*   CONDITION - ERROR                                      @G29AN2F*/ 01779000
*/*     ERROR IN BUILDING DSAB/TIOT ENTRY                    @G29AN2F*/ 01780000
*/*     OUTPUT -                                             @G29AN2F*/ 01781000
*/*       STEP REASON CODE SET                               @G29AN2F*/ 01782000
*/*       RETCODE SET                                        @G29AN2F*/ 01783000
*/*                                                          @G29AN2F*/ 01784000
*/********************************************************************/ 01785000
*BLDTIOT: PROC;                        /*BUILD DSAB & TIOT   @G29AN2F*/ 01786000
BLDTIOT  STM   @14,@09,@SA00003                                 02S0475 01787000
         STM   @11,@12,@SA00003+48                              02S0475 01788000
*EREQPTR=ADDR(EREQCORE);               /*BUILD REQUEST BLOCK         */ 01789000
         LA    @10,EREQCORE                                     02S0476 01790000
         ST    @10,EREQPTR                                      02S0476 01791000
*ETIOTREQ =ETIOTREQ && ETIOTREQ;       /*ZERO OUT REQUEST BLOCK      */ 01792000
         XC    ETIOTREQ(42,@10),ETIOTREQ(@10)                   02S0477 01793000
*EREQBLD  =ON;                         /*INDICATE BUILD FUNCTION     */ 01794000
         OI    EREQBLD(@10),B'10000000'                         02S0478 01795000
*EREQDDNM =SCTDDNAM;                   /*PUT DDNAME IN RB            */ 01796000
         MVC   EREQDDNM(8,@10),SCTDDNAM(SIOTPTR)                02S0479 01797000
*EREQDEVS =ONE;                        /*SET NO OF DEVICES TO ONE    */ 01798000
         LA    @09,1                                            02S0480 01799000
         STH   @09,EREQDEVS(,@10)                               02S0480 01800000
*EREQRPOS =ONE;                        /*SET REL POS TO ONE          */ 01801000
         STH   @09,EREQRPOS(,@10)                               02S0481 01802000
*EREQSIOT = SIOTPTR;                   /*POINT TO SIOT               */ 01803000
         ST    SIOTPTR,EREQSIOT(,@10)                           02S0482 01804000
*EREQJSVA =SCTPJFCB;                   /*JFCB SWA VIRTUAL ADDRESS    */ 01805000
         MVC   EREQJSVA(3,@10),SCTPJFCB(SIOTPTR)                02S0483 01806000
*EREQSSVA =SIOTDSKA;                   /*SIOT SWA VIRTUAL ADDRESS    */ 01807000
         MVC   EREQSSVA(3,@10),SIOTDSKA(SIOTPTR)                02S0484 01808000
*EREQJSCB =JSCBPTR;                    /*JSCB PTR                    */ 01809000
         L     @09,@PC00001                                     02S0485 01810000
         L     @09,ALCWAPTR(,@09)                               02S0485 01811000
         MVC   EREQJSCB(4,@10),JSCBPTR(@09)                     02S0485 01812000
*IF SIOTPASS = ON THEN                 /*DATA SET DISP = PASS?       */ 01813000
         TM    SIOTPASS(SIOTPTR),B'00010000'                    02S0486 01814000
         BNO   @RF00486                                         02S0486 01815000
*  EREQPASS  =ON;                      /*SET BIT IN RB               */ 01816000
         OI    EREQPASS(@10),B'00001000'                        02S0487 01817000
*IF SIOCLUNL = ON THEN                 /*SHOULD CLOSE UNALLOC THE       01818000
*                                        D.S ?                       */ 01819000
@RF00486 TM    SIOCLUNL(SIOTPTR),B'10000000'                    02S0488 01820000
         BNO   @RF00488                                         02S0488 01821000
*  EREQUNAL = ON;                      /*YES, SET CORRESPONDING 02S0489 01822000
*                                        INDICATOR IN R.B            */ 01823000
         L     @10,EREQPTR                                      02S0489 01824000
         OI    EREQUNAL(@10),B'00010000'                        02S0489 01825000
*CALL IEFAB428 (EREQPTR,JSCBPTR,EXITPRMP); /*CALL ETIOT         02S0490 01826000
*                                                      BUILD RTN     */ 01827000
@RF00488 LA    @09,EREQPTR                                      02S0490 01828000
         ST    @09,@AL00001                                     02S0490 01829000
         L     @09,@PC00001                                     02S0490 01830000
         L     @09,ALCWAPTR(,@09)                               02S0490 01831000
         LA    @15,JSCBPTR(,@09)                                02S0490 01832000
         ST    @15,@AL00001+4                                   02S0490 01833000
         LA    @09,EXITPRMP(,@09)                               02S0490 01834000
         ST    @09,@AL00001+8                                   02S0490 01835000
         L     @15,@CV01429                                     02S0490 01836000
         LA    @01,@AL00001                                     02S0490 01837000
         BALR  @14,@15                                          02S0490 01838000
*RETCODE = REG15;                      /*SAVE RETURN CODE            */ 01839000
         LR    RETCODE,REG15                                    02S0491 01840000
*IF RETCODE = ZERO THEN                /*WAS BUILD SUCCESSFUL?       */ 01841000
         LTR   RETCODE,RETCODE                                  02S0492 01842000
         BNZ   @RF00492                                         02S0492 01843000
*  DO;                                 /*YES                         */ 01844000
*    DSABPTR = SIOTETIO;               /*POINT TO DSAB               */ 01845000
         L     DSABPTR,SIOTETIO(,SIOTPTR)                       02S0494 01846000
*    DSABSSCM = SSDATAP;               /*SET  FIELD          @G29AN2F*/ 01847000
         ST    SSDATAP,DSABSSCM(,DSABPTR)                       02S0495 01848000
*    DSABSSNM = SSNAMEP->SSNM;         /*SET NAME FLD IN DSAB@G29AN2F*/ 01849000
         MVC   DSABSSNM(4,DSABPTR),SSNM(SSNAMEP)                02S0496 01850000
*    TIOESSDS = ON;                    /*SET SUBSYS IN TIOT  @YM02438*/ 01851000
         L     @09,DSABTIOT(,DSABPTR)                           02S0497 01852000
         OI    TIOESSDS(@09),B'00000010'                        02S0497 01853000
*    SIOTALCD = ON;                    /*MARK SIOT ALLOCATED         */ 01854000
         OI    SIOTALCD(SIOTPTR),B'00000010'                    02S0498 01855000
*    SUBSREQS = SUBSREQS - ONE;        /*DECREMENT SUBSYS REQUESTS   */ 01856000
         L     @09,@PC00001                                     02S0499 01857000
         L     @09,ALCWAPTR(,@09)                               02S0499 01858000
         L     @15,SUBSREQS(,@09)                               02S0499 01859000
         BCTR  @15,0                                            02S0499 01860000
         ST    @15,SUBSREQS(,@09)                               02S0499 01861000
*    TOTREQS  = TOTREQS - ONE;         /*DECREMENT TOTAL REQUESTS  */   01862000
         L     @15,TOTREQS(,@09)                                02S0500 01863000
         BCTR  @15,0                                            02S0500 01864000
         ST    @15,TOTREQS(,@09)                                02S0500 01865000
*  END;                                                         02S0501 01866000
*ELSE                                                           02S0502 01867000
*  SSRSNC = EREQCODE;                  /*SET FAIL REASON CODE       */  01868000
         B     @RC00492                                         02S0502 01869000
@RF00492 L     @09,@PC00001                                     02S0502 01870000
         L     @09,ALCWAPTR(,@09)                               02S0502 01871000
         L     @09,FAILRSCP(,@09)                               02S0502 01872000
         L     @01,EREQPTR                                      02S0502 01873000
         MVC   SSRSNC(2,@09),EREQCODE(@01)                      02S0502 01874000
*RETURN;                               /*RETURN TO CALLER    @G29AN2F*/ 01875000
@EL00003 DS    0H                                               02S0503 01876000
@EF00003 DS    0H                                               02S0503 01877000
@ER00003 LM    @14,@09,@SA00003                                 02S0503 01878000
