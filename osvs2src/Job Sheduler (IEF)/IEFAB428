*  DO;                                  /*NO,GET CORE FOR ID LIST    */ 01066000
*    IF DSABGIDP ^= ZERO THEN           /*SHOULD ID LIST EXIST?      */ 01067000
         L     @01,DSABPTR                                      02S0359 01068000
         C     @14,DSABGIDP(,@01)                               02S0359 01069000
         BE    @RF00359                                         02S0359 01070000
*      DO;                              /*YES, GET CORE FOR IT       */ 01071000
*        GETLEN=HDRLEN+(GRIDLEN*EREQDEVS); /*LENGTH OF GROUP ID LIST */ 01072000
