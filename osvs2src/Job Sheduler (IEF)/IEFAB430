         AL    @10,PNSPCNT                                      02S0096 00210000
         ST    @10,PNSPCNT                                      02S0096 00211000
*                      ELSE;           /*NOT PRIVATE                 */ 00212000
@RF00095 DS    0H                                               02S0098 00213000
*                    END;                                       02S0098 00214000
*                END;                                           02S0099 00215000
*            END;                                               02S0100 00216000
*        END;                                                   02S0101 00217000
*      ELSE;                           /*THIS ENTRY WAS ALLOC'D      */ 00218000
@RF00073 DS    0H                                               02S0103 00219000
*      VUPTR = VUPTR + VULENGTH;       /*UPDATE TO NEXT ENTRY        */ 00220000
@RC00073 AL    VUPTR,VULENGTH                                   02S0103 00221000
*    END;                                                       02S0104 00222000
         AH    VUCTR,@CH00052                                   02S0104 00223000
@DE00072 CH    VUCTR,SVOLUNNO(,SIOTPTR)                         02S0104 00224000
         BNH   @DL00072                                         02S0104 00225000
*  ELSE;                               /*REQ WAS COMPLETELY ALLOC'D  */ 00226000
@RF00071 DS    0H                                               02S0106 00227000
