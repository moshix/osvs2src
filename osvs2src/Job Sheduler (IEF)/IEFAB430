         IC    @07,SIOPSCNT(,SIOTPTR)                           02S0092 00192000
         ALR   @10,@07                                          02S0092 00193000
         STC   @10,SIOPSCNT(,SIOTPTR)                           02S0092 00194000
*                    END;                                       02S0093 00195000
*                  ELSE                /*NOT STORAGE                 */ 00196000
*                    DO;                                        02S0094 00197000
         B     @RC00088                                         02S0094 00198000
