         BNZ   @RF00115                                         01S0115 00261000
*  DO;                                                          01S0116 00262000
*/********************************************************************/ 00263000
*/*                                                                  */ 00264000
*/* SEGMENT NAME - BLDPRLST                                          */ 00265000
*/*                                                                  */ 00266000
*/* DESCRIPTIVE NAME - BUILD PERMRES/RESERVED LIST                   */ 00267000
*/*                                                                  */ 00268000
*/* FUNCTION - THE FUNCTION OF THIS ROUTINE IS TO BUILD A LIST OF    */ 00269000
*/*   HALFWORD POINTERS TO UCBS. THESE UCBS WILL BE THOSE WHICH      */ 00270000
*/*   REPRESENT DIRECT ACCESS DEVICES WHICH ARE ONLINE, READY, NOT   */ 00271000
*/*   PENDING OFFLINE NOR PENDING UNLOAD,  NOT                       */ 00272000
*/*   IN USE BY OLTEP, AND PERMANENTLY RESIDENT OR RESERVED          */ 00273000
*/*   WITH A VOLUME SERIAL NUMBER IN THE UCB.                @ZA18146*/ 00274000
*/*   THIS LIST IS THEN USED BY THE ALLOCATION ROUTINES FOR THEIR    */ 00275000
*/*   DEVICE SELECTION. IF CORE IS NOT AVAILABLE FOR THE LIST A      */ 00276000
*/*   GETMAIN ERROR INDICATOR WILL BE RETURNED IN REG 15.            */ 00277000
*/*                                                                  */ 00278000
*/* ENTRY -  INLINE SEGMENT OF IEFAB430                              */ 00279000
*/*   PURPOSE - SEE FUNCTION                                         */ 00280000
*/*                                                                  */ 00281000
*/*   INPUT  -                                                       */ 00282000
*/*     ALCWAPTR - POINTER TO ALLOCATION WORK AREA.                  */ 00283000
*/*     EDTNUCBS - NUMBER OF ENTRIES IN IOS LOOK-UP TABLE.   @Z40FPPD*/ 00284000
*/*                USED TO DETERMINE MAXIMUM SIZE OF LIST.   @Z40FPPD*/ 00285000
*/*                                                                  */ 00286000
*/* EXITS -                                                          */ 00287000
*/*   CONDITION - PERMRES/RESERVED LIST BUILT                        */ 00288000
*/*                                                                  */ 00289000
*/*     OUTPUT-                                                      */ 00290000
*/*        PRLST - A LIST OF HALFWORD POINTERS TO UCBS WHICH ARE     */ 00291000
*/*              DIRECT ACCESS,ONLINE,READY,PERMANENTLY RESIDENT OR  */ 00292000
*/*              RESERVED.                                           */ 00293000
*/*                                                                  */ 00294000
*/*        PRLSTLEN - A FULL WORD FIELD IN PSCPARM CONTAINING THE    */ 00295000
*/*                   LENGTH OF THE PERMRES/RESERVED LIST.           */ 00296000
*/*                                                                  */ 00297000
*/*                                                                  */ 00298000
*/*        PRLSTPTR - A FULL WORD POINTER TO THE PRLIST.             */ 00299000
*/*                                                                  */ 00300000
*/*     RETURN CODE -                                                */ 00301000
*/*        ERRCODE2 - 0, INDICATING PRLIST WAS SUCCESSFULLY BUILT.   */ 00302000
*/*                                                                  */ 00303000
*/*                                                                  */ 00304000
*/*   CONDITION - NO CORE AVAILABLE TO BUILD PRLIST.                 */ 00305000
*/*                                                                  */ 00306000
*/*     OUTPUT-                                                      */ 00307000
*/*        FAILCODE - 2, INDICATING GETMAIN FAILURE                  */ 00308000
*/*                                                                  */ 00309000
*/*     RETURN CODE -                                                */ 00310000
*/*        ERRCODE2 - 4,INDICATING GETMAIN FAILURE TRYING TO GET     */ 00311000
*/*                     CORE TO BUILD PRLIST                         */ 00312000
*/*                                                                  */ 00313000
*/*                                                                  */ 00314000
*/*                                                                  */ 00315000
*/********************************************************************/ 00316000
*RFY IOSLUT BASED(IOSLUTP);                                     02S0117 00317000
*SLOTNO = ONE;                         /*INIT PRLST SLOT POINTER     */ 00318000
         LA    SLOTNO,1                                         02S0118 00319000
*PRLSTLEN =  (ALCNUCBS * TWO) + TWO;   /*CALC MAX LENGTH OF LIST PLUS   00320000
*                                        LIST END INDICATOR  @Z40FPPJ*/ 00321000
         L     @08,ALCWAPTR                                     02S0119 00322000
         L     @08,ALCNUCBS(,@08)                               02S0119 00323000
         ALR   @08,@08                                          02S0119 00324000
         AH    @08,@CH00091                                     02S0119 00325000
         ST    @08,PRLSTLEN                                     02S0119 00326000
* /* ?GETSTORG (SPID,PRLSTLEN,PRLSTPTR);*/                      02S0120 00327000
*   /* DEFINITIONS OF GETMAIN PARMS */                          02S0120 00328000
*   DCL                                                         02S0120 00329000
*     GTSTR0   FIXED(31) REG(0),/*GTMN PARM REG*/               02S0120 00330000
*     GTSTR1   FIXED(31) REG(1),/*GTMN PARM REG*/               02S0120 00331000
*     GTSTR2   PTR(31)   REG(2),/*GTMN PARM REG*/               02S0120 00332000
*     GTSTR15  FIXED(31) REG(15),/*GTMN RET COD*/               02S0120 00333000
*     GTSTRTCD FIXED(31); /* GETMAIN RTN CODE  */               02S0120 00334000
*   DCL                                                         02S0121 00335000
*     GTSTR3   FIXED(31) REG(3),/* CLEARING REG*/               02S0121 00336000
*     GTSTR4   FIXED(31) REG(4),/* CLEARING REG*/               02S0121 00337000
*     GTSTR5   FIXED(31) REG(5),/* CLEARING REG*/               02S0121 00338000
*     GTSTR6   FIXED(31) REG(6),/* CLEARING REG*/               02S0121 00339000
*     GTSTR7   FIXED(31) REG(7),/* CLEARING REG*/               02S0121 00340000
*     GTSTR8   FIXED(31) REG(8),/* CLEARING REG*/               02S0121 00341000
*     GTSTZERO FIXED(31) INIT(0),                               02S0121 00342000
*     GTST253  FIXED(31) CONSTANT(253);                         02S0121 00343000
*   RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/               02S0122 00344000
*   GTSTR0 = PRLSTLEN; /* SET AMOUNT*/                          02S0123 00345000
         LR    GTSTR0,@08                                       02S0123 00346000
*   GTSTR2 = SPID; /* SET SUBPOOL*/                             02S0124 00347000
         LA    GTSTR2,230                                       02S0124 00348000
*   GENERATE CODE;                                              02S0125 00349000
     GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN                               00350000
*   GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/               02S0126 00351000
         LR    GTSTRTCD,GTSTR15                                 02S0126 00352000
*   PRLSTPTR = GTSTR1; /* ADDR OF GOTN STORG*/                  02S0127 00353000
         ST    GTSTR1,PRLSTPTR                                  02S0127 00354000
*   RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */                02S0128 00355000
*   IF GTSTRTCD = 0 THEN /*GETMAIN SUCCESSFUL? */               02S0129 00356000
         LTR   GTSTRTCD,GTSTRTCD                                02S0129 00357000
         BNZ   @RF00129                                         02S0129 00358000
*     DO;               /* YES, CLEAR STORAGE.*/                02S0130 00359000
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0131 00360000
*                          GTSTR7,GTSTR8) RSTD;                 02S0131 00361000
*       GTSTR4 = PRLSTPTR; /*STORG PTR*/                        02S0132 00362000
         L     @03,PRLSTPTR                                     02S0132 00363000
         LR    GTSTR4,@03                                       02S0132 00364000
*       GTSTR5 = PRLSTLEN; /*STORG AMNT*/                       02S0133 00365000
         L     GTSTR5,PRLSTLEN                                  02S0133 00366000
*       GTSTR7 = '00000000'X; /*PAD CHAR & LNTH*/               02S0134 00367000
         SLR   GTSTR7,GTSTR7                                    02S0134 00368000
*       IF SPID = GTST253 THEN                                  02S0135 00369000
         B     @RF00135                                         02S0135 00370000
*      GEN                                                      02S0136 00371000
*   (MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8);           02S0136 00372000
         MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8                 00373000
*       GEN REFS(GTSTR4,GTSTR6);                                02S0137 00374000
@RF00135 DS    0H                                               02S0137 00375000
          MVCL  GTSTR4,GTSTR6 CLEAR GOTTEN STORG                        00376000
*       IF SPID = GTST253 THEN                                  02S0138 00377000
         B     @RF00138                                         02S0138 00378000
*         GEN (MODESET KEYADDR=(2));                            02S0139 00379000
