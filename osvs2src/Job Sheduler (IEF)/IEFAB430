*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0140 00381000
*                          GTSTR7,GTSTR8) UNRSTD;               02S0140 00382000
@RF00138 DS    0H                                               02S0141 00383000
*     END;       /* STORAGE CLEARING COMPLETED */               02S0141 00384000
*   ELSE                                                        02S0142 00385000
*   PRLSTPTR= 0;   /* CLEAR RETURN ADDR. */                     02S0142 00386000
         B     @RC00129                                         02S0142 00387000
