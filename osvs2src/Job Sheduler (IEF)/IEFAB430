         TM    VOLALOC(VUPTR),B'10000000'                       02S0073 00126000
         BNZ   @RF00073                                         02S0073 00127000
*        DO;                                                    02S0074 00128000
*          TOTLCNT = TOTLCNT+ONE;      /*INCREMENT TOTAL COUNT OF       00129000
*                                        UNITS LEFT TO BE ALLOC'D FOR   00130000
*                                        THIS STEP                   */ 00131000
         LA    @10,1                                            02S0075 00132000
         L     @07,TOTLCNT                                      02S0075 00133000
         ALR   @07,@10                                          02S0075 00134000
         ST    @07,TOTLCNT                                      02S0075 00135000
*          IF VOLSPEC = ON THEN        /*IS THIS A SPECIFIC REQ      */ 00136000
         TM    VOLSPEC(VUPTR),B'00100000'                       02S0076 00137000
         BNO   @RF00076                                         02S0076 00138000
*            DO;                                                02S0077 00139000
*              SPECCNT  = SPECCNT+ONE; /*UPDATE NUMBER OF UNITS 02S0078 00140000
*                                          STILL TO BE ALLOCATED TO     00141000
*                                          SPECIFICS FOR THIS STEP   */ 00142000
         AL    @10,SPECCNT                                      02S0078 00143000
         ST    @10,SPECCNT                                      02S0078 00144000
*            END;                                               02S0079 00145000
*          ELSE                        /*NOT SPECIFIC                */ 00146000
*            DO;                                                02S0080 00147000
