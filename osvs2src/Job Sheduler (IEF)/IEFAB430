@RF00076 DS    0H                                               02S0081 00149000
*              IF VOLPUB = ON THEN     /*IS THIS A PUBLIC VOL REQEST */ 00150000
         TM    VOLPUB(VUPTR),B'10000000'                        02S0081 00151000
         BNO   @RF00081                                         02S0081 00152000
*                DO;                                            02S0082 00153000
*                  PUBLCNT  = PUBLCNT+ONE; /*UPDATE NO. PUBLICS      */ 00154000
         LA    @10,1                                            02S0083 00155000
         L     @07,PUBLCNT                                      02S0083 00156000
         ALR   @07,@10                                          02S0083 00157000
         ST    @07,PUBLCNT                                      02S0083 00158000
*                  IF VOLNSHR = OFF THEN /*IF VOLUME IS SHAREABLE       00159000
*                                        INCREMENT COUNT OF     02S0084 00160000
*                                        PUBLIC AND STORAGE     02S0084 00161000
*                                        DEVICES NEEDED              */ 00162000
