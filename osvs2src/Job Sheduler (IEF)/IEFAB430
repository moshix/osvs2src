         STC   @10,SIOPSCNT(,SIOTPTR)                           02S0085 00169000
*                END;                                           02S0086 00170000
*              ELSE                    /*NOT PUBLIC                  */ 00171000
*                DO;                                            02S0087 00172000
         B     @RC00081                                         02S0087 00173000
@RF00081 DS    0H                                               02S0088 00174000
*                  IF VOLSTG = ON THEN /*IS THIS STORAGE             */ 00175000
         TM    VOLSTG(VUPTR),B'00010000'                        02S0088 00176000
