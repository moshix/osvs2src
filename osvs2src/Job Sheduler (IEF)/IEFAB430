         ST    @07,STRGCNT                                      02S0090 00184000
*                      IF VOLNSHR = OFF THEN /*IF VOL SHAREABLE      */ 00185000
         TM    VOLNSHR(VUPTR),B'00001000'                       02S0091 00186000
         BNZ   @RF00091                                         02S0091 00187000
*                        SIOPSCNT = SIOPSCNT + ONE; /*INCREMENT 02S0092 00188000
*                                        COUNT OF PUBLIC AND STORAGE    00189000
