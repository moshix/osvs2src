@DL00232 DS    0H                                               02S0233 00925000
*  IF EDLDEVT ^= SAVEDLP -> EDLDEVT THEN /*SAME DEVICE TYPE ?        */ 00926000
         CLC   EDLDEVT(4,EDLPTR),EDLDEVT(SAVEDLP)               02S0233 00927000
         BE    @RF00233                                         02S0233 00928000
*    DO;                                                        02S0234 00929000
*      EINDEVTP = ON;                  /*NO,MARK EDL ENTRY INELIGIBLE*/ 00930000
         OI    EINDEVTP(EDLPTR),B'10000000'                     02S0235 00931000
*    END;                                                       02S0236 00932000
*  ELSE;                                                        02S0237 00933000
@RF00233 DS    0H                                               02S0238 00934000
*  EDLPTR = EDLNXTNT;                  /*INCREMENT TO NEXT EDL ENTRY */ 00935000
         L     EDLPTR,EDLNXTNT(,EDLPTR)                         02S0238 00936000
*END;                                                           02S0239 00937000
