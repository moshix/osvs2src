         TITLE 'IEFAB433 - SPECIFIC VOLUME ALLOCATION CONTROL          *00001000
                        '                                               00002000
IEFAB433 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(24)                                          01S0001 00007000
         DC    C'IEFAB433 76265  UZ05402 '                      01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         MODESET EXTKEY=SCHED,SAVEKEY=(2)                               00015000
         GSPACE   R,LV=(0)                                              00016000
         LR    @11,@01                                          01S0001 00017000
         USING @DATD,@11                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         STC   @02,@OLDKEY                                      01S0001 00020000
         MODESET KEYADDR=(2)                                            00021000
         LM    @00,@02,20(@13)                                  01S0001 00022000
         ST    @11,8(,@13)                                      01S0001 00023000
         LR    @13,@11                                          01S0001 00024000
         MODESET EXTKEY=ZERO                                            00025000
         MVC   @PC00001(8),0(@01)                               01S0001 00026000
         MODESET EXTKEY=SCHED                                           00027000
*RETCODE = NOERROR;                    /*INIT LOCAL RETURN CODE VAR  */ 00028000
         SLR   RETCODE,RETCODE                                  01S0047 00029000
*SIOTPTR = SIOT1P;                     /*PTR TO FIRST SIOT           */ 00030000
         L     @10,@PC00001                                     01S0048 00031000
         L     @10,ALCWAPTR(,@10)                               01S0048 00032000
         MVC   SIOTPTR(4),SIOT1P(@10)                           01S0048 00033000
*DO SIOTCTR = ONE TO NUMREQS           /*DO UNTIL NO MORE REQ OR     */ 00034000
*  WHILE (RETCODE = NOERROR & INDRETRY=OFF); /*ERROR                 */ 00035000
         LA    SIOTCTR,1                                        01S0049 00036000
         B     @DE00049                                         01S0049 00037000
@DL00049 LTR   RETCODE,RETCODE                                  01S0049 00038000
         BNZ   @DC00049                                         01S0049 00039000
         L     @07,@PC00001                                     01S0049 00040000
         L     @07,ALCWAPTR(,@07)                               01S0049 00041000
         TM    INDRETRY(@07),B'01000000'                        01S0049 00042000
         BNZ   @DC00049                                         01S0049 00043000
*  NOSPECSW = OFF;                     /*INITIALIZE NOT SPECIFIC SW  */ 00044000
         NI    NOSPECSW,B'01111111'                             01S0050 00045000
*  ALLALCSW = ON;                      /*ASSUME ALL CAN BE ALLOC'D   */ 00046000
         OI    ALLALCSW,B'10000000'                             01S0051 00047000
*  UPEDLSW = OFF;                      /*INIT UPDATE EDL SWITCH      */ 00048000
         NI    UPEDLSW,B'01111111'                              01S0052 00049000
*  IF SIOTALCD = OFF &                 /*WAS SIOT ENTRY ALLOCATED?   */ 00050000
*     SIOTGIGN = OFF THEN              /*MUST PROCESS                */ 00051000
         L     @07,SIOTPTR                                      01S0053 00052000
         TM    SIOTALCD(@07),B'00000010'                        01S0053 00053000
         BNZ   @RF00053                                         01S0053 00054000
         TM    SIOTGIGN(@07),B'00001000'                        01S0053 00055000
         BNZ   @RF00053                                         01S0053 00056000
*    DO;                                                        01S0054 00057000
*      VUPTR = SVOLUNAD;               /*PTR TO FIRST VOLUNIT ENTRY  */ 00058000
         MVC   VUPTR(4),SVOLUNAD(@07)                           01S0055 00059000
*      DO VUCTR = ONE TO SVOLUNNO        /*DO UNTIL NO MORE VOLUNIT  */ 00060000
*        WHILE (RETCODE = NOERROR & NOSPECSW = OFF /*ENTRY OR ERROR  */ 00061000
*          & INDRETRY=OFF);                                     01S0056 00062000
         LA    VUCTR,1                                          01S0056 00063000
         B     @DE00056                                         01S0056 00064000
@DL00056 LTR   RETCODE,RETCODE                                  01S0056 00065000
         BNZ   @DC00056                                         01S0056 00066000
         TM    NOSPECSW,B'10000000'                             01S0056 00067000
         BNZ   @DC00056                                         01S0056 00068000
         L     @09,@PC00001                                     01S0056 00069000
         L     @09,ALCWAPTR(,@09)                               01S0056 00070000
         TM    INDRETRY(@09),B'01000000'                        01S0056 00071000
         BNZ   @DC00056                                         01S0056 00072000
*        IF VOLALOC = OFF & VDEVREQD=ON THEN /*ENTRY ALLOCATED? 01S0057 00073000
*                                              & DEV. REQ.           */ 00074000
         L     @09,VUPTR                                        01S0057 00075000
         TM    VDEVREQD(@09),B'00100000'                        01S0057 00076000
