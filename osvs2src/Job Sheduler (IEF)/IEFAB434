         L     @12,UCBPTR(,@12)                                 02S0085 00090000
         TM    UCB3DACC(@12),B'00100000'                        02S0085 00091000
         BNO   @RF00085                                         02S0085 00092000
*             SJFCBPTR->JFCBADSP = ON; /*NOTIFY DADSM OF PROTECT        00093000
*                                                            @G32HPPJ*/ 00094000
         L     @12,SJFCBPTR(,@11)                               02S0086 00095000
         OI    JFCBADSP(@12),B'00100000'                        02S0086 00096000
*        ELSE                                                   02S0087 00097000
*          IF (SIOTRACT = ON &         /*OR REQUEST SATISFIED   02S0087 00098000
*                                        TAPE TESTS          @G32HPPJ*/ 00099000
*             UCB3TAPE = ON) THEN      /*AND WAS ALLOCATED      02S0087 00100000
*                                        TO A TAPE DEVICE    @G32HPPJ*/ 00101000
         B     @RC00085                                         02S0087 00102000
@RF00085 L     @12,@PC00001                                     02S0087 00103000
         L     @12,SIOTPTR(,@12)                                02S0087 00104000
         TM    SIOTRACT(@12),B'00100000'                        02S0087 00105000
         BNO   @RF00087                                         02S0087 00106000
         L     @11,@PC00001+8                                   02S0087 00107000
         L     @11,UCBPTR(,@11)                                 02S0087 00108000
         TM    UCB3TAPE(@11),B'10000000'                        02S0087 00109000
         BNO   @RF00087                                         02S0087 00110000
*            SJFCBPTR->JFCBPROT = ON;  /*INDICATE TO OPEN THAT  02S0088 00111000
*                                        RACF PROTECT IS TO BE  02S0088 00112000
*                                        DONE                @G32HPPJ*/ 00113000
         L     @12,SJFCBPTR(,@12)                               02S0088 00114000
         OI    JFCBPROT(@12),B'00010000'                        02S0088 00115000
*          ELSE                        /*                    @G32HPPJ*/ 00116000
*            SIOTRSNC = PROTFAIL;      /*PROTECT FAILED FOR TAPE        00117000
*                                                            @G32HPPJ*/ 00118000
         B     @RC00087                                         02S0089 00119000
@RF00087 L     @12,@PC00001                                     02S0089 00120000
         L     @12,SIOTPTR(,@12)                                02S0089 00121000
         MVC   SIOTRSNC(2,@12),@CH00135                         02S0089 00122000
*       IF SIOTRSNC = PROTFAIL THEN    /*SET RETURN CODE     @G32HPPJ*/ 00123000
@RC00087 DS    0H                                               02S0090 00124000
@RC00085 L     @12,@PC00001                                     02S0090 00125000
