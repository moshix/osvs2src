         CLC   SIOTRSNC(2,@12),@CH00135                         02S0090 00127000
         BNE   @RF00090                                         02S0090 00128000
*          ERRWORD = FOUR;             /*SIGNIFY JOB SHOULD     02S0091 00129000
*                                        FAIL                @G32HPPJ*/ 00130000
         LA    ERRWORD,4                                        02S0091 00131000
*       ELSE;                                                   02S0092 00132000
@RF00090 DS    0H                                               02S0093 00133000
*     END;                                                      02S0093 00134000
*   ELSE                              /*RACF NOT INSTALLED   @G32HPPJ*/ 00135000
*     DO;                             /*OR NOT ACTIVE        @G32HPPJ*/ 00136000
