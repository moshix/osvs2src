*       ASWARTNP=ADDR(IEFAB4EA);    /* ADDRESS OF EXIT RTN   @Z40FPPG*/ 00193000
         L     @08,@CV00138                                     03S0090 00194000
         STCM  @08,15,ASWARTNP(ASWAPTR)                         03S0090 00195000
*       ASWAPRMS=EPRMLSTP;          /* PARMS FOR THIS EXIT   @Z40FPPG*/ 00196000
         STCM  EPRMLSTP,15,ASWAPRMS(ASWAPTR)                    03S0091 00197000
*       JSCBWRKP=JSCBACT;           /* GET ACTIVE JSCB       @Z40FPPG*/ 00198000
         ST    @14,JSCBWRKP                                     03S0092 00199000
*       RFY                                                     03S0093 00200000
*         IEZJSCB BASED(JSCBWRKP);  /*                       @Z40FPPG*/ 00201000
*       ISSUESTA=OFF;               /* INIT. ESTAE SW        @Z40FPPG*/ 00202000
         NI    ISSUESTA,B'01111111'                             03S0094 00203000
*       IF JSCBASWA=ZERO|           /* NO ALLOC ESTAE IN EFFECT OR      00204000
*                                                            @Z40FPPG*/ 00205000
