         C     @10,FSIOTPTR(,@15)                               01S0062 00031000
         BE    @RF00062                                         01S0062 00032000
*     DO;                           /* PROCESS                       */ 00033000
*                                                               02S0064 00034000
*/********************************************************************/ 00035000
*/*                                                                  */ 00036000
*/* SEGMENT NAME - HSKESTAE                                          */ 00037000
*/*                                                                  */ 00038000
*/* DESCRIPTIVE NAME - HSKP ESTAE SETUP                              */ 00039000
*/*                                                                  */ 00040000
*/* FUNCTION - OBTAIN AND INITIALIZE THE ESTAE EXIT PARMLIST.        */ 00041000
*/*            CREATE AN ESTAE ENVIRONMENT.                  @Z40FPPG*/ 00042000
*/*                                                                  */ 00043000
*/*    OPERATION - 1. ISSUE GETMAIN FOR PARMLIST                     */ 00044000
*/*                2. INITIALIZE FIELDS WITHIN PARMLIST              */ 00045000
*/*                3. CREATE ESTAE ENVIRONMENT               @Z40FPPG*/ 00046000
*/*                4. CHECK SUCCESS OF ESTAE SET UP.         @Z40FPPG*/ 00047000
*/*                5. FAIL IF ESTAE UNSUCCESSFUL AND FREE PARMLIST   */ 00048000
*/*                                                                  */ 00049000
*/* ENTRY - INLINE SEGMENT                                           */ 00050000
*/*                                                                  */ 00051000
*/*    INPUT - SEGMENT USES INPUT TO HSKP TO INITIALIZE PARMLIST     */ 00052000
*/*                                                                  */ 00053000
*/* EXIT - N/A                                                       */ 00054000
*/*                                                                  */ 00055000
*/*    CONDITION - IF SUCCESSFUL ESTAE ENVIRONMENT, RTNCODE = 0      */ 00056000
*/*                                                          @Z40FPPG*/ 00057000
*/*                IF UNSUCCESSFUL, RTNCODE=8, REASONCD=16           */ 00058000
*/*                                                                  */ 00059000
*/*    OUTPUT DATA - INITIALIZED ESTAE PARMLIST AND ESTAE            */ 00060000
*/*                  ENVIRONMENT SET UP.                     @Z40FPPG*/ 00061000
*/*                                                                  */ 00062000
*/********************************************************************/ 00063000
*                                                               02S0064 00064000
*                                                               02S0064 00065000
*/********************************************************************/ 00066000
*                                                               02S0064 00067000
*       RFY                                                     02S0064 00068000
*        (REG0,                                                 02S0064 00069000
*         REG1,                                                 02S0064 00070000
*         REG15) RSTD;                                          02S0064 00071000
*       SBPOOL=SUBPOOOL;            /* SET SUBPOOL NUMBER            */ 00072000
         MVI   SBPOOL,X'E6'                                     02S0065 00073000
*       LENTH=LENGTH(EPARMLST);     /* LENGTH OF ESTAE PARMLIST      */ 00074000
         MVC   LENTH(3),@CF01849+1                              02S0066 00075000
*       REG0=SPLEN;                 /* SET SUBPOOL+LENGTH IN REG 0   */ 00076000
         L     REG0,SPLEN                                       02S0067 00077000
*       GEN(GETMAIN R,LV=(0));      /* ESTAE PARMLIST GETMAIN        */ 00078000
