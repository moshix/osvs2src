*         DO;                       /* PROCESS NEW ESTAE     @Z40FPPG*/ 00220000
*           ISSUESTA=ON;            /* SAY ESTAE ISSUED      @Z40FPPG*/ 00221000
         OI    ISSUESTA,B'10000000'                             03S0097 00222000
*           ASWAAUTO=ADDR(AB4EDCOR);/* 1ST ASWA GIVES W/A    @Z40FPPG*/ 00223000
         LA    @14,AB4EDCOR                                     03S0098 00224000
         STCM  @14,15,ASWAAUTO(ASWAPTR)                         03S0098 00225000
*           ASWAOLDP=JSCBASWA;      /* OLD LAST IF ANY       @Z40FPPG*/ 00226000
         L     @14,JSCBWRKP                                     03S0099 00227000
         MVC   ASWAOLDP(4,ASWAPTR),JSCBASWA(@14)                03S0099 00228000
*           RFY                                                 03S0100 00229000
*            (REG2,                                             03S0100 00230000
*             REG3) RSTD;           /* RESTRICT WORK REGS    @Z40FPPG*/ 00231000
*           REG2=ADDR(IEFAB4ED);    /* COMMON ESTAE EXIT     @Z40FPPG*/ 00232000
         L     REG2,@CV00139                                    03S0101 00233000
*           REG3=ASWAPTR;           /* ASWA IS EXIT PARMS    @Z40FPPG*/ 00234000
         LR    REG3,ASWAPTR                                     03S0102 00235000
*           ESTAECOR(1:ESTAELEN)=ESTAELST(1:ESTAELEN);/* MOVE TO AUTO   00236000
*                                      CORE                  @Z40FPPG*/ 00237000
         L     @14,ESTAELEN                                     03S0103 00238000
         BCTR  @14,0                                            03S0103 00239000
         EX    @14,@SM01862                                     03S0103 00240000
*           GEN REFS(ESTAECOR) SETS(REG15,TCBSTABB);            03S0104 00241000
      ESTAE (2),PARAM=(3),RECORD=YES,TERM=YES,MF=(E,ESTAECOR)           00242000
*           RFY                                                 03S0105 00243000
*            (REG2,                                             03S0105 00244000
*             REG3) UNRSTD;                                     03S0105 00245000
*           IF REG15=ZERO THEN      /* IF ESTAE SUCCESSFUL   @Z40FPPG*/ 00246000
         LTR   REG15,REG15                                      03S0106 00247000
         BNZ   @RF00106                                         03S0106 00248000
*             ASWASCBP=TCBSTABB;    /* CURRENT SCB @         @Z40FPPG*/ 00249000
         L     @14,LOC16PTR                                     03S0107 00250000
         L     @14,TCBLISTP(,@14)                               03S0107 00251000
         L     @14,TCBPTR(,@14)                                 03S0107 00252000
         L     @14,TCBSTABB-1(,@14)                             03S0107 00253000
         LA    @14,0(,@14)                                      03S0107 00254000
         STCM  @14,15,ASWASCBP(ASWAPTR)                         03S0107 00255000
*         END;                      /* END IF ESTAE ISSUED   @Z40FPPG*/ 00256000
*       ELSE                        /* CURRENT IS ALLOC'S    @Z40FPPG*/ 00257000
*         DO;                       /* USE EXISTING ESTAE    @Z40FPPG*/ 00258000
         B     @RC00095                                         03S0109 00259000
@RF00095 DS    0H                                               03S0110 00260000
*           ASWASCBP=JSCBASWA->ASWASCBP;/* SCB @ FROM LAST      03S0110 00261000
*                                      ASWA@Z40FPPG                  */ 00262000
         L     @14,JSCBWRKP                                     03S0110 00263000
         L     @14,JSCBASWA(,@14)                               03S0110 00264000
         MVC   ASWASCBP(4,ASWAPTR),ASWASCBP(@14)                03S0110 00265000
*           ASWACHNP=JSCBASWA;      /* POINT BACK TO LAST    @Z40FPPG*/ 00266000
         STCM  @14,15,ASWACHNP(ASWAPTR)                         03S0111 00267000
*         END;                      /* CURRENT ESTAE USED    @Z40FPPG*/ 00268000
*       IF REG15=ZERO THEN          /* IF NO ERROR           @Z40FPPG*/ 00269000
@RC00095 SLR   @14,@14                                          03S0113 00270000
         CR    REG15,@14                                        03S0113 00271000
         BNE   @RF00113                                         03S0113 00272000
*         DO;                       /* PUT ASWA ONTO CHAIN   @Z40FPPG*/ 00273000
*           DO;                     /* MODESET KEYADDR(ZERO)         */ 00274000
*             SPKA(ZERO);           /* SET PSW KEY                   */ 00275000
         SPKA  0(@14)                                           03S0116 00276000
*           END;                    /* MODESET KEYADDR(ZERO) KEY TO     00277000
*                                      UPDATE JSCB           @Z40FPPG*/ 00278000
*           JSCBASWA=ASWAPTR;       /* NEW LAST ASWA PTR     @Z40FPPG*/ 00279000
         L     @14,JSCBWRKP                                     03S0118 00280000
         ST    ASWAPTR,JSCBASWA(,@14)                           03S0118 00281000
*           DO;                     /* MODESET EXTKEY(SCHED)         */ 00282000
*             SPKA(16);             /* SET PSW KEY                   */ 00283000
         LA    @14,16                                           03S0120 00284000
         SPKA  0(@14)                                           03S0120 00285000
*           END;                    /* MODESET EXTKEY(SCHED) BACK TO    00286000
*                                      SCHED KEY             @Z40FPPG*/ 00287000
*         END;                      /* END IF NO ERROR       @Z40FPPG*/ 00288000
*       RFY                                                     03S0123 00289000
*         IEZJSCB BASED(JSCBPTR);   /*                       @Z40FPPG*/ 00290000
@RF00113 DS    0H                                               03S0124 00291000
*       IF REG15^=0 THEN            /* ESTAE ENVIRONMENT NOT SET     */ 00292000
         LTR   REG15,REG15                                      02S0124 00293000
         BZ    @RF00124                                         02S0124 00294000
*         DO;                       /* - FAILING SITUATION           */ 00295000
*           RFY                                                 02S0126 00296000
*            (REG0,                                             02S0126 00297000
*             REG1) RSTD;                                       02S0126 00298000
*           SBPOOL=SUBPOOOL;        /* SET SUBPOOL NUMBER            */ 00299000
         MVI   SBPOOL,X'E6'                                     02S0127 00300000
*           LENTH=LENGTH(EPARMLST); /* LENGTH OF ESTAE PARMLIST      */ 00301000
         MVC   LENTH(3),@CF01849+1                              02S0128 00302000
*           REG0=SPLEN;             /* SUBPOOL+LENGTH TO REG 0       */ 00303000
         L     REG0,SPLEN                                       02S0129 00304000
*           REG1=EPRMLSTP;          /* PTR TO PARMLIST               */ 00305000
         LR    REG1,EPRMLSTP                                    02S0130 00306000
*           GEN(FREEMAIN R,LV=(0),A=(1));                       02S0131 00307000
         FREEMAIN R,LV=(0),A=(1)                                        00308000
*           RFY                                                 02S0132 00309000
*            (REG0,                                             02S0132 00310000
*             REG1) UNRSTD;                                     02S0132 00311000
*           RTNCODE=STEPERR;        /* 8 - ESTABLISH ESTAE           */ 00312000
         MVC   RTNCODE(4),@CF00074                              02S0133 00313000
*           REASONCD=ESTAEERR;      /* 16 - ENVIRONMENT FAILED       */ 00314000
         L     @14,@PC00001+36                                  02S0134 00315000
         MVC   REASONCD(2,@14),@CH00055                         02S0134 00316000
