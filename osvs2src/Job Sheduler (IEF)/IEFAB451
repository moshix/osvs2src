         LA    @08,HWCRI(,@15)                                  02S0182 00571000
         ST    @08,HWCRIP(,@15)                                 02S0182 00572000
*             END;                                              02S0183 00573000
*           ELSE                    /* GETMAIN FAILURE               */ 00574000
*             DO;                                               02S0184 00575000
         B     @RC00164                                         02S0184 00576000
@RF00164 DS    0H                                               02S0185 00577000
*               RTNCODE=FAILURE;    /* CALLER'S RETURN CODE INFO     */ 00578000
         MVC   RTNCODE(4),@CF00113                              02S0185 00579000
*               REASONCD=GETERROR;  /* 2 - CALLER'S REASON CODE      */ 00580000
         L     @08,@PC00001+36                                  02S0186 00581000
         MVC   REASONCD(2,@08),@CH00125                         02S0186 00582000
*             END;                                              02S0187 00583000
*           IF RTNCODE=SUCCESS THEN /* SUCCESSFUL HSKP INITIALIZATION*/ 00584000
@RC00164 L     @08,RTNCODE                                      01S0188 00585000
         LTR   @08,@08                                          01S0188 00586000
         BNZ   @RF00188                                         01S0188 00587000
*             DO;                                               01S0189 00588000
*               CALL IEFAB452(HWCNTRLP,/* DD PROCESSING CONTROL -    */ 00589000
*                   HWDATAP,HWRTNP,HWFSIOTP);/* PROCESS ALL REQUESTS    00590000
*                                      OF THIS STEP                  */ 00591000
         LA    @08,HWCNTRLP                                     01S0190 00592000
         ST    @08,@AL00001                                     01S0190 00593000
         LA    @08,HWDATAP                                      01S0190 00594000
         ST    @08,@AL00001+4                                   01S0190 00595000
         LA    @08,HWRTNP                                       01S0190 00596000
         ST    @08,@AL00001+8                                   01S0190 00597000
         L     @08,HWDATAP                                      01S0190 00598000
         LA    @08,HWFSIOTP(,@08)                               01S0190 00599000
         ST    @08,@AL00001+12                                  01S0190 00600000
         L     @15,@CV00137                                     01S0190 00601000
         LA    @01,@AL00001                                     01S0190 00602000
         BALR  @14,@15                                          01S0190 00603000
*               IF HWRTCD=DDERR THEN/* DD RELATED ERROR - 4          */ 00604000
         LA    @08,4                                            01S0191 00605000
         L     @15,HWRTNP                                       01S0191 00606000
         CH    @08,HWRTCD(,@15)                                 01S0191 00607000
         BNE   @RF00191                                         01S0191 00608000
*                 DO;               /* PUT REASON CODE IN SIOT OF    */ 00609000
*                   HWCSIOTP->SIOTRSNC=HWRSNCD;/* FAILING REQUEST    */ 00610000
         L     @01,HWDATAP                                      01S0193 00611000
         L     @01,HWCSIOTP(,@01)                               01S0193 00612000
         LH    @00,HWRSNCD(,@15)                                01S0193 00613000
         STH   @00,SIOTRSNC(,@01)                               01S0193 00614000
*                   RTNCODE=FAILURE;/* FAILING RETURN CODE - 4       */ 00615000
         ST    @08,RTNCODE                                      01S0194 00616000
*                 END;                                          01S0195 00617000
*               ELSE                /* NOT DD RELATED ERROR -4       */ 00618000
*                 IF HWRTCD=STEPERR THEN/* STEP RELATED ERROR - 8    */ 00619000
         B     @RC00191                                         01S0196 00620000
@RF00191 L     @08,HWRTNP                                       01S0196 00621000
         CLC   HWRTCD(2,@08),@CH00074                           01S0196 00622000
         BNE   @RF00196                                         01S0196 00623000
*                   DO;             /* PUT REASON CODE IN AREA       */ 00624000
*                     REASONCD=HWRSNCD;/* PROVIDED BY CALLER OF HSKP */ 00625000
         L     @15,@PC00001+36                                  01S0198 00626000
