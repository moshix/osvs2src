         TITLE 'IEFAB452 - DD PROCESSING CONTROL ROUTINE               *00001000
                        '                                               00002000
IEFAB452 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFAB452  73.325'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         MODESET EXTKEY=SCHED,SAVEKEY=(2)                               00015000
         GSPACE  R,LV=(0)                                               00016000
         LR    @11,@01                                          01S0001 00017000
         USING @DATD,@11                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         STC   @02,@OLDKEY                                      01S0001 00020000
         MODESET KEYADDR=(2)                                            00021000
         LM    @00,@02,20(@13)                                  01S0001 00022000
         ST    @11,8(,@13)                                      01S0001 00023000
         LR    @13,@11                                          01S0001 00024000
         MODESET EXTKEY=ZERO                                            00025000
         MVC   @PC00001(16),0(@01)                              01S0001 00026000
         MODESET EXTKEY=SCHED                                           00027000
JSCBDISP EQU 188                                                        00028000
JSCBAOS1 EQU  72                                                        00029000
JSCBAOS2 EQU 192                                                        00030000
*                                                                    */ 00031000
*   DATAPREG=HWDATAP;               /* MAP HSKP DATA AND RETURN      */ 00032000
         L     @10,@PC00001+4                                   01S0037 00033000
         L     DATAPREG,HWDATAP(,@10)                           01S0037 00034000
*   RTNPREG=HWRTNP;                 /* AREAS FOR PROCESSING SPEED    */ 00035000
         L     @10,@PC00001+8                                   01S0038 00036000
         L     RTNPREG,HWRTNP(,@10)                             01S0038 00037000
*   RFY                                                         01S0039 00038000
*     HWDATA BASED(DATAPREG);                                   01S0039 00039000
*   RFY                                                         01S0040 00040000
*     HWRETURN BASED(RTNPREG);                                  01S0040 00041000
*   IF HWNOSCAT=OFF&                /* STEPCAT PROCESSING ALLOWED    */ 00042000
*       SCTPCAT^=0 THEN             /* STEP-CATALOGS ARE INDICATED   */ 00043000
         L     @10,@PC00001                                     01S0041 00044000
         L     @10,HWCNTRLP(,@10)                               01S0041 00045000
         TM    HWNOSCAT(@10),B'00000001'                        01S0041 00046000
         BNZ   @RF00041                                         01S0041 00047000
         SLR   @10,@10                                          01S0041 00048000
         L     @02,HWSCTP(,DATAPREG)                            01S0041 00049000
         CL    @10,SCTPCAT(,@02)                                01S0041 00050000
         BE    @RF00041                                         01S0041 00051000
*     DO;                                                       01S0042 00052000
*                                                               02S0043 00053000
*/********************************************************************/ 00054000
*/* SEGMENT NAME - CATSPEC                                           */ 00055000
*/*                                                                  */ 00056000
*/* DESCRIPTIVE NAME - STEPCAT PROCESSING                            */ 00057000
*/*                                                                  */ 00058000
*/* FUNCTION    - CHECK THE INFORMATION OF THE STEPCAT DD(S) AND     */ 00059000
*/*               INITIATE HOUSEKEEPING PROCESSING OF THEM.          */ 00060000
*/*                                                                  */ 00061000
*/*   OPERATION-1.THIS COULD BE A CHECKPOINT RESTART, SO ALL         */ 00062000
*/*               JOB/STEPCATS MUST BE PROPER (NOT A DUPLICATE       */ 00063000
*/*               REQUEST FOR A PREVIOUS CATALOG). IF NOT PROPER     */ 00064000
*/*               THE REQUEST IS NOT SYNTAX CHECKED.                 */ 00065000
*/*             2.THE SIOT WILL BE CHECKED, INSURING THAT NO VOLUME  */ 00066000
*/*               OR UNIT INFORMATION WAS SPECIFIED, NO 'DEFER'      */ 00067000
*/*               MOUNTING WAS REQUESTED, AND THE DISPOSITION IS     */ 00068000
*/*               OLD AND 'KEEP' IS SPECIFIED OR IMPLIED.            */ 00069000
*/*             3.IEFAB454 ( DD FUNCTION CONTROL ) IS THEN CALLED    */ 00070000
*/*               TO PROCESS THE DD.                                 */ 00071000
*/*             4.THE PCCB ROUTINE (IEFAB4EF) WILL THEN BE CALLED TO */ 00072000
*/*               CREATE A PCCB FOR THE CATALOG IF THERE IS NO ERROR */ 00073000
*/*               INDICATED BY HOUSEKEEPING, AND THE DATA SET EXISTS */ 00074000
*/*               ON ONLY ONE DEVICE AND VOLUME AND IS NOT A GDG.    */ 00075000
*/*               IF THE PCCB ALREADY EXISTED, THE 'STEPCAT' AND     */ 00076000
*/*               'CATALOGUED DATA SET' BITS ARE TURNED OFF IN THE   */ 00077000
*/*               SIOT, BECAUSE ONLY ONE STEPCAT DD CAN REPRESENT    */ 00078000
*/*               ONE CATALOG DATA SET. THE DD WILL BE PROCESSED     */ 00079000
*/*               WITH AND AS A NORMAL DD IN THE JOBSTREAM.          */ 00080000
*/*                                                                  */ 00081000
*/*             5.RETURN AND REASON CODES ARE SET INTO THE HOUSE-    */ 00082000
*/*               KEEPING WORKAREA IF ANY OF THE ABOVE ERRORS EXIST. */ 00083000
*/*                                                                  */ 00084000
*/* ENTRY - IF ANY STEPCATS EXIST                                    */ 00085000
*/*                                                                  */ 00086000
*/* INPUT DATA - THE NUMBER OF STEPCATS IN THE STEP AND A POINTER TO */ 00087000
*/*              THE FIRST STEPCAT'S SIOT.                           */ 00088000
*/*                                                                  */ 00089000
*/* EXIT - INLINE IN ALL CASES                                       */ 00090000
*/*                                                                  */ 00091000
*/*   OUTPUT DATA - PROCESSED STEPCAT SIOT,JFCBS.                    */ 00092000
*/*               ERROR REASON CODES                                 */ 00093000
*/*               JCL STATEMENT ERROR - 75                           */ 00094000
*/*               CATALOG RETURN INFORMATION ERROR - 76              */ 00095000
*/*                                                                  */ 00096000
*/*   RETURN CODES - NO ERROR - 0                                    */ 00097000
*/*                - ERROR    - 4                                    */ 00098000
*/*                                                                  */ 00099000
*/********************************************************************/ 00100000
*                                                               02S0043 00101000
*       SWACTLS=0;                  /* CALL SWA INTERFACE TO READ    */ 00102000
         STH   @10,SWACTLS                                      02S0043 00103000
*       SWREAD=ON;                  /* LOCATE THE FIRST STEPCAT SIOT,*/ 00104000
         OI    SWREAD,B'00100000'                               02S0044 00105000
*       IF HWLEPARP=0 THEN          /* CREATING A NEW EPA IF NEEDED, */ 00106000
         C     @10,HWLEPARP(,DATAPREG)                          02S0045 00107000
         BNE   @RF00045                                         02S0045 00108000
*         SWNEW=ON;                                             02S0046 00109000
         OI    SWNEW,B'00001000'                                02S0046 00110000
*       ELSE                        /* ELSE, USING THE OLD HSKP EPA  */ 00111000
*         SWMOD=ON;                                             02S0047 00112000
         B     @RC00045                                         02S0047 00113000
@RF00045 OI    SWMOD,B'00000010'                                02S0047 00114000
*       CALL IEFAB4F7(SWACTLS,HWLEPARP,0,ADDR(SCTPCAT),ADDR(SCAT1),0,1) 00115000
*           ;                       /* SWA INTERFACE                 */ 00116000
@RC00045 LA    @10,SWACTLS                                      02S0048 00117000
         ST    @10,@AL00001                                     02S0048 00118000
         LA    @10,HWLEPARP(,DATAPREG)                          02S0048 00119000
         ST    @10,@AL00001+4                                   02S0048 00120000
         LA    @10,@CF00528                                     02S0048 00121000
         ST    @10,@AL00001+8                                   02S0048 00122000
         L     @10,HWSCTP(,DATAPREG)                            02S0048 00123000
         LA    @10,SCTPCAT(,@10)                                02S0048 00124000
         ST    @10,@TF00001                                     02S0048 00125000
         LA    @10,@TF00001                                     02S0048 00126000
         ST    @10,@AL00001+12                                  02S0048 00127000
         LA    @10,SCAT1                                        02S0048 00128000
         ST    @10,@TF00002                                     02S0048 00129000
         LA    @10,@TF00002                                     02S0048 00130000
         ST    @10,@AL00001+16                                  02S0048 00131000
         LA    @10,@CF00528                                     02S0048 00132000
         ST    @10,@AL00001+20                                  02S0048 00133000
         LA    @10,@CF00043                                     02S0048 00134000
         ST    @10,@AL00001+24                                  02S0048 00135000
         L     @15,@CV00085                                     02S0048 00136000
         LA    @01,@AL00001                                     02S0048 00137000
         BALR  @14,@15                                          02S0048 00138000
*       SIOTPTR=SCAT1;              /* FIRST STEPCAT TO PROCESS      */ 00139000
         L     SIOTPTR,SCAT1                                    02S0049 00140000
*       DO STEPCATS=1 TO SCTCATCT WHILE/* NUMBER OF STEPCATS IN SCT  */ 00141000
*             (HWRTCD=0);           /* AND NO ERRORS DETECTED        */ 00142000
         LA    STEPCATS,1                                       02S0050 00143000
         B     @DE00050                                         02S0050 00144000
@DL00050 SLR   @10,@10                                          02S0050 00145000
         CH    @10,HWRTCD(,RTNPREG)                             02S0050 00146000
         BNE   @DC00050                                         02S0050 00147000
*         HWCSIOTP=SIOTPTR;         /* SIOTPTR TO HSKP WORKAREA      */ 00148000
         ST    SIOTPTR,HWCSIOTP(,DATAPREG)                      02S0051 00149000
*         DISPTEMP=SCTSDISP&NODISP; /* FOR CHECKING SCHEDULER AND    */ 00150000
         LA    DISPTEMP,31                                      02S0052 00151000
         SLR   @15,@15                                          02S0052 00152000
         IC    @15,SCTSDISP(,SIOTPTR)                           02S0052 00153000
         NR    DISPTEMP,@15                                     02S0052 00154000
*         ALTDTEMP=SIOTALTD&NOALTDSP;/* ALTERNATE DISPOSITIONS       */ 00155000
         LA    ALTDTEMP,15                                      02S0053 00156000
         SLR   @15,@15                                          02S0053 00157000
         IC    @15,SIOTALTD(,SIOTPTR)                           02S0053 00158000
         NR    ALTDTEMP,@15                                     02S0053 00159000
*         IF SIOTJSCT=ON THEN       /* MAKE CHECKS ONLY IF THIS IS A    00160000
*                                      PROPER JOB/STEPCAT. IT MAY BE    00161000
*                                      A RESTART WITH BIT TURNED OFF.*/ 00162000
         TM    SIOTJSCT(SIOTPTR),B'00000001'                    02S0054 00163000
         BNO   @RF00054                                         02S0054 00164000
*           IF SCTUTYPE=BLANK&SCTUNAFF=OFF&/* NO UNIT INFORMATION AND*/ 00165000
*               SCTVOLCT=0&         /* NO VOLUME INFO, NOT A SUB-    */ 00166000
*               SIOTSSDS=OFF&SCTDEFER=OFF&/* SYSTEM DS,OR DEFER 02S0055 00167000
*                                      MOUNT,                        */ 00168000
*               SCTVOLAF=OFF&SCTSOLD=ON&/* NO VOL=REF= , AND    02S0055 00169000
*                                      DISP=OLD,                     */ 00170000
*               (SIOTKEEP=ON|DISPTEMP=0)&/* DISPOSITIONS ARE KEEP,   */ 00171000
*               (SIOTAKEP=ON|ALTDTEMP=0)&/* (OR WILL DEFAULT TO 02S0055 00172000
*                                      KEEP),                        */ 00173000
*               SCTDUMMY=OFF&       /* VSAM 'DUMMY' ILLEGAL,         */ 00174000
*               SCTSGDGS=OFF THEN   /* AND NOT A GDG SINGLE, THEN IT */ 00175000
         CLC   SCTUTYPE(8,SIOTPTR),@CC00050                     02S0055 00176000
         BNE   @RF00055                                         02S0055 00177000
         TM    SCTUNAFF(SIOTPTR),B'00000010'                    02S0055 00178000
         BNZ   @RF00055                                         02S0055 00179000
         CLI   SCTVOLCT(SIOTPTR),0                              02S0055 00180000
         BNE   @RF00055                                         02S0055 00181000
         TM    SIOTSSDS(SIOTPTR),B'10000000'                    02S0055 00182000
         BNZ   @RF00055                                         02S0055 00183000
         TM    SCTDEFER(SIOTPTR),B'00100010'                    02S0055 00184000
         BNZ   @RF00055                                         02S0055 00185000
         TM    SCTSOLD(SIOTPTR),B'00000001'                     02S0055 00186000
         BNO   @RF00055                                         02S0055 00187000
         TM    SIOTKEEP(SIOTPTR),B'00001000'                    02S0055 00188000
         BO    @GL00004                                         02S0055 00189000
         CR    DISPTEMP,@10                                     02S0055 00190000
         BNE   @RF00055                                         02S0055 00191000
@GL00004 TM    SIOTAKEP(SIOTPTR),B'00001000'                    02S0055 00192000
         BO    @GL00003                                         02S0055 00193000
         CH    ALTDTEMP,@CH00528                                02S0055 00194000
         BNE   @RF00055                                         02S0055 00195000
@GL00003 TM    SCTDUMMY(SIOTPTR),B'10000000'                    02S0055 00196000
         BNZ   @RF00055                                         02S0055 00197000
         TM    SCTSGDGS(SIOTPTR),B'10000000'                    02S0055 00198000
         BNZ   @RF00055                                         02S0055 00199000
*             DO;                   /* MAY BE PROCESSED AS A STEPCAT.*/ 00200000
*               DDNUMSAV=JSCDDNUM;  /* NO SIOTS (DDS) MAY BE CREATED */ 00201000
         L     @10,HWJSCBP(,DATAPREG)                           02S0057 00202000
         LH    DDNUMSAV,JSCDDNUM(,@10)                          02S0057 00203000
*               CALL IEFAB453(HWCNTRLP,HWDATAP,HWRTNP,HWCSIOTP);        00204000
         L     @10,@PC00001                                     02S0058 00205000
         ST    @10,@AL00001                                     02S0058 00206000
         L     @10,@PC00001+4                                   02S0058 00207000
         ST    @10,@AL00001+4                                   02S0058 00208000
         L     @10,@PC00001+8                                   02S0058 00209000
         ST    @10,@AL00001+8                                   02S0058 00210000
         LA    @10,HWCSIOTP(,DATAPREG)                          02S0058 00211000
         ST    @10,@AL00001+12                                  02S0058 00212000
         L     @15,@CV00077                                     02S0058 00213000
         LA    @01,@AL00001                                     02S0058 00214000
         BALR  @14,@15                                          02S0058 00215000
*               IF HWRTCD=0 THEN    /* NO ERROR IN DD PREPARATION    */ 00216000
         ICM   @10,3,HWRTCD(RTNPREG)                            02S0059 00217000
         BNZ   @RF00059                                         02S0059 00218000
*                 CALL IEFAB454(HWCNTRLP,HWDATAP,HWRTNP,HWCSIOTP,       00219000
*                     HWCJFCBP);    /* DD FUNCTION CONTROL           */ 00220000
         L     @10,@PC00001                                     02S0060 00221000
         ST    @10,@AL00001                                     02S0060 00222000
         L     @10,@PC00001+4                                   02S0060 00223000
         ST    @10,@AL00001+4                                   02S0060 00224000
         L     @10,@PC00001+8                                   02S0060 00225000
         ST    @10,@AL00001+8                                   02S0060 00226000
         LA    @10,HWCSIOTP(,DATAPREG)                          02S0060 00227000
         ST    @10,@AL00001+12                                  02S0060 00228000
         LA    @10,HWCJFCBP(,DATAPREG)                          02S0060 00229000
         ST    @10,@AL00001+16                                  02S0060 00230000
         L     @15,@CV00083                                     02S0060 00231000
         LA    @01,@AL00001                                     02S0060 00232000
         BALR  @14,@15                                          02S0060 00233000
*               IF HWRTCD=0&DDNUMSAV=JSCDDNUM&/* DS FOUND,NOT MULTI- */ 00234000
*                   SIOTGALL=OFF&JFCBNVOL=ONE THEN/* DEVICE OR GDG   */ 00235000
@RF00059 SLR   @10,@10                                          02S0061 00236000
         CH    @10,HWRTCD(,RTNPREG)                             02S0061 00237000
         BNE   @RF00061                                         02S0061 00238000
         L     @06,HWJSCBP(,DATAPREG)                           02S0061 00239000
         CH    DDNUMSAV,JSCDDNUM(,@06)                          02S0061 00240000
         BNE   @RF00061                                         02S0061 00241000
         TM    SIOTGALL(SIOTPTR),B'00100000'                    02S0061 00242000
         BNZ   @RF00061                                         02S0061 00243000
         L     @06,HWCJFCBP(,DATAPREG)                          02S0061 00244000
         CLI   JFCBNVOL(@06),1                                  02S0061 00245000
         BNE   @RF00061                                         02S0061 00246000
*                 DO;               /* HAVE A PCCB CREATED, (IF NONE */ 00247000
*                   PCCNTRLS=0;     /* EXIST FOR THIS CATALOG).      */ 00248000
         STH   @10,PCCNTRLS                                     02S0063 00249000
*                   PCCSTPCT=ON;    /* STEP CATALOG                  */ 00250000
*                   PCCMAKE=ON;     /* CREATE AND CHAIN A PCCB       */ 00251000
         OI    PCCSTPCT,B'10100000'                             02S0065 00252000
*                   IF CTGCVCC=JFCBDSNM THEN/* STEPCAT NAME NOT AN      00253000
*                                      ALIAS                         */ 00254000
         L     @02,HWCRIP(,DATAPREG)                            02S0066 00255000
         L     @02,CTGCVOL(,@02)                                02S0066 00256000
         CLC   CTGCVCC(44,@02),JFCBDSNM(@06)                    02S0066 00257000
         BNE   @RF00066                                         02S0066 00258000
*                     CTGCONP=0;    /* INDICATE 'NO ALIAS'           */ 00259000
         ST    @10,CTGCONP                                      02S0067 00260000
*                   ELSE                                        02S0068 00261000
*                     CTGCONP=ADDR(CTGCVCC);/* ADDRESS OF ALIAS NAME */ 00262000
         B     @RC00066                                         02S0068 00263000
@RF00066 L     @10,HWCRIP(,DATAPREG)                            02S0068 00264000
         L     @10,CTGCVOL(,@10)                                02S0068 00265000
         LA    @10,CTGCVCC(,@10)                                02S0068 00266000
         ST    @10,CTGCONP                                      02S0068 00267000
*                   CALL IEFAB4EF(PCCNTRLS,HWRTNP,JSCBACT,ADDR(JFCBDSNM 00268000
*                       ),CTGCONP,0,0,0,0);/* PCCB MAKE              */ 00269000
@RC00066 LA    @10,PCCNTRLS                                     02S0069 00270000
         ST    @10,@AL00001                                     02S0069 00271000
         L     @10,@PC00001+8                                   02S0069 00272000
         ST    @10,@AL00001+4                                   02S0069 00273000
         L     @10,HWJSCBP(,DATAPREG)                           02S0069 00274000
         LA    @10,JSCBACT(,@10)                                02S0069 00275000
         ST    @10,@AL00001+8                                   02S0069 00276000
         LA    @10,HWCJFCBP(,DATAPREG)                          02S0069 00277000
         ST    @10,@AL00001+12                                  02S0069 00278000
         LA    @10,CTGCONP                                      02S0069 00279000
         ST    @10,@AL00001+16                                  02S0069 00280000
         LA    @10,@CF00528                                     02S0069 00281000
         ST    @10,@AL00001+20                                  02S0069 00282000
         LA    @10,@CF00528                                     02S0069 00283000
         ST    @10,@AL00001+24                                  02S0069 00284000
         LA    @10,@CF00528                                     02S0069 00285000
         ST    @10,@AL00001+28                                  02S0069 00286000
         LA    @10,@CF00528                                     02S0069 00287000
         ST    @10,@AL00001+32                                  02S0069 00288000
         L     @15,@CV00084                                     02S0069 00289000
         LA    @01,@AL00001                                     02S0069 00290000
         BALR  @14,@15                                          02S0069 00291000
*                   IF HWRTCD=DUPCTLG THEN/* PCCB FOUND FOR THE 02S0070 00292000
*                                                             STEPCAT*/ 00293000
         CLC   HWRTCD(2,RTNPREG),@CH00054                       02S0070 00294000
         BNE   @RF00070                                         02S0070 00295000
*                     DO;           /* INDICATED BY THIS DD          */ 00296000
*                       HWRETURN=0; /* RESET 'INVALID' RETURN CODE   */ 00297000
         SLR   @10,@10                                          02S0072 00298000
         ST    @10,HWRETURN(,RTNPREG)                           02S0072 00299000
*                       SIOTJSCT=OFF;/* ONLY ONE STEPCAT DD ALLOWED  */ 00300000
         NI    SIOTJSCT(SIOTPTR),B'11111110'                    02S0073 00301000
*                       SIOTCATL=OFF;/* TO REPRESENT ONE CATALOG     */ 00302000
         NI    SIOTCATL(SIOTPTR),B'10111111'                    02S0074 00303000
*                     END;                                      02S0075 00304000
*                 END;                                          02S0076 00305000
*               ELSE                /* ERROR FROM IEFAB454 OR ERROR  */ 00306000
*                 IF HWRTCD=0 THEN  /* IN THE CATALOG INFORMATION    */ 00307000
         B     @RC00061                                         02S0077 00308000
@RF00061 ICM   @10,3,HWRTCD(RTNPREG)                            02S0077 00309000
         BNZ   @RF00077                                         02S0077 00310000
*                   DO;             /* DATA SET NOT A PROPER STEPCAT */ 00311000
*                     HWRTCD=DDERR; /* RETURN CODE - 4               */ 00312000
         MVC   HWRTCD(2,RTNPREG),@CH00054                       02S0079 00313000
*                     HWRSNCD=SCATERR;/* REASON CODE - 76            */ 00314000
         MVC   HWRSNCD(2,RTNPREG),@CH00059                      02S0080 00315000
*                   END;                                        02S0081 00316000
*             END;                                              02S0082 00317000
*           ELSE                    /* SYNTAX ERROR ON DD STATEMENT  */ 00318000
*             DO;                                               02S0083 00319000
