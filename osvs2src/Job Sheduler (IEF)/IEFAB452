@DC00050 DS    0H                                               02S0089 00338000
*     END;                          /* END STEPCAT PROCESSING GROUP  */ 00339000
*   SIOTPTR=FRSTSIOT;               /* MAP FIRST SIOT                */ 00340000
@RF00041 L     @10,@PC00001+12                                  01S0090 00341000
         L     @10,FRSTSIOT(,@10)                               01S0090 00342000
         LR    SIOTPTR,@10                                      01S0090 00343000
*   SCNSIOTP=FRSTSIOT;              /* GET 1ST SIOT IN CHAIN.        */ 00344000
         LR    SCNSIOTP,@10                                     01S0091 00345000
*   DO WHILE(SCNSIOTP^=0&SCTSTPLB=OFF);/* SCAN DDS IN THIS STEP FOR  */ 00346000
         B     @DE00092                                         01S0092 00347000
@DL00092 DS    0H                                               01S0093 00348000
*     IF SCNSIOTP->SCTDDNAM=STEPLIB THEN/* FOR A DDNAME OF STEPLIB,  */ 00349000
