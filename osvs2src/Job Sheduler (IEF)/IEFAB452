@RF00055 DS    0H                                               02S0084 00321000
*               HWRTCD=DDERR;       /* RETURN CODE - 4               */ 00322000
         MVC   HWRTCD(2,RTNPREG),@CH00054                       02S0084 00323000
*               HWRSNCD=SCATJCL;    /* REASON CODE - 75              */ 00324000
         MVC   HWRSNCD(2,RTNPREG),@CH00057                      02S0085 00325000
*             END;                                              02S0086 00326000
*         SIOTPTR=SIOTNPTR;         /* GET NEXT STEPCAT, IF ANY      */ 00327000
@RC00055 DS    0H                                               02S0087 00328000
@RF00054 L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       02S0087 00329000
*       END;                        /* STEPCAT PROCESSING LOOP  02S0088 00330000
*                                                                    */ 00331000
