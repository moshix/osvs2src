         L     @15,@CV00083                                     02S0054 00215000
         LA    @01,@AL00001                                     02S0054 00216000
         BALR  @14,@15                                          02S0054 00217000
*         IF PTABLP->SIOTALCD=ON&   /* REFERENCED DD MUST HAVE BEEN  */ 00218000
*             (PTABLP->SIO3DACC=ON| /* ALLOCATED, ON DIRECT ACCESS   */ 00219000
*             PTABLP->SIOVAMDS=ON)& /* OR VAM AND NOT A SUBSYSTEM    */ 00220000
*             PTABLP->SIOTSSDS^=ON THEN/* DATASET (SYSIN ETC.)       */ 00221000
         L     @10,PTABLP                                       02S0055 00222000
