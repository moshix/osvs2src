         LA    @10,HWIOSLTP(,DATAPREG)                          01S0095 00364000
         ST    @10,@AL00001+12                                  01S0095 00365000
         LA    @10,SCTUTYPE(,SIOTPTR)                           01S0095 00366000
         ST    @10,@AL00001+16                                  01S0095 00367000
         L     @15,@CV00080                                     01S0095 00368000
         LA    @01,@AL00001                                     01S0095 00369000
         BALR  @14,@15                                          01S0095 00370000
*     ELSE                                                      01S0096 00371000
*                                                               02S0096 00372000
*/********************************************************************/ 00373000
*/*                                                                  */ 00374000
*/* SEGMENT NAME - SRCHSIOT                                          */ 00375000
*/*                                                                  */ 00376000
*/* DESCRIPTIVE NAME - SEARCH SIOTS FOR UNIT TYPE INFORMATION        */ 00377000
*/*                                                                  */ 00378000
*/* FUNCTION - ATTEMPTS TO OBTAIN UNIT INFORMATION FROM THE CURRENT  */ 00379000
*/*            SIOT, IF THE UNIT PARAMETER WAS PREVIOUSLY            */ 00380000
*/*            CONVERTED FOR NON VIO DATA SETS (VIO DATA SETS        */ 00381000
*/*            REQUIRE UCB PTR REMAIN INTACT), OR SEARCHES   @YM0XXXX*/ 00382000
*/*            THE SIOT CHAIN FOR A UNIT AFFINITY REQUEST            */ 00383000
*/*            BY COMPARING THE 'UNIT AFFINITY TO DD' NUMBER TO THE  */ 00384000
*/*            DD NUMBER (SCTDDINO) OF THE SIOTS IN THE SIOT CHAIN.  */ 00385000
*/*            UPON LOCATING THE PROPER SIOT, IF THE UNIT TYPE FIELD */ 00386000
*/*            WAS PREVIOUSLY CONVERTED (SCTUTYPE BYTE 4 = 0),       */ 00387000
*/*            THIS FIELD IS COPIED INTO THE UNIT CONVERTION LIST OF */ 00388000
*/*            THE HSKP WORKAREA.  IF A VAM DATA SET WAS INDICATED,  */ 00389000
*/*            THAT WILL BE NOTED ALSO IN THE WORKAREA. UNIT NAME    */ 00390000
*/*            CONVERSION WILL BE CALLED FOR SUBSYSTEM DATA SETS TO  */ 00391000
*/*            CONVERT THE SYSTEM NAME 'SYSALLDA'.                   */ 00392000
*/*                                                                  */ 00393000
*/* ENTRY - INLINE SEGMENT                                           */ 00394000
*/*                                                                  */ 00395000
*/* EXITS - COMPLETION OF SIOT SEARCH AND UNIT TYPE VALIDITY CHECK   */ 00396000
*/*                                                                  */ 00397000
*/* OUTPUT DATA - UNIT CONVERTION LIST COMPLETED AND WHETHER THE     */ 00398000
*/*               UNIT TYPE WAS ORIGINALLY CONVERTED FROM THE EDT    */ 00399000
*/*               OR THE UCB LIST AND VAM ELIGIBILITY.               */ 00400000
*/*                                                                  */ 00401000
*/* RETURN CODE - ALWAYS ZERO                                        */ 00402000
*/* * * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * */ 00403000
*                                                               02S0096 00404000
*       IF SCTUNAFF=ON|             /* UNIT AFFINITY REQUEST OR      */ 00405000
*           (SIOUCNVT=0&SIOVAMDS=OFF) THEN/* NON-VIO CONVERTED  02S0096 00406000
*                                                            @YM07413*/ 00407000
         B     @RC00094                                         02S0096 00408000
@RF00094 TM    SCTUNAFF(SIOTPTR),B'00000010'                    02S0096 00409000
         BO    @RT00096                                         02S0096 00410000
         CLI   SIOUCNVT(SIOTPTR),0                              02S0096 00411000
         BNE   @RF00096                                         02S0096 00412000
         TM    SIOVAMDS(SIOTPTR),B'00100000'                    02S0096 00413000
         BNZ   @RF00096                                         02S0096 00414000
@RT00096 DS    0H                                               02S0097 00415000
*         DO;                                                   02S0097 00416000
*           IF SIOUCNVT=0 THEN      /* PREVIOUSLY CONVERTED          */ 00417000
         CLI   SIOUCNVT(SIOTPTR),0                              02S0098 00418000
         BNE   @RF00098                                         02S0098 00419000
*             PRIDDNUM=SCTDDINO;    /* THIS IS CORRECT SIOT          */ 00420000
         LH    PRIDDNUM,SCTDDINO(,SIOTPTR)                      02S0099 00421000
*           ELSE                                                02S0100 00422000
*             DO;                                               02S0100 00423000
         B     @RC00098                                         02S0100 00424000
@RF00098 DS    0H                                               02S0101 00425000
*               PRIDDNUM=SIOTUNAF;  /* SAVE PRIMARY DD NUMBER        */ 00426000
         LH    PRIDDNUM,SIOTUNAF(,SIOTPTR)                      02S0101 00427000
*               SIOTPTR=HWFSIOTP;   /* MAP FIRST SIOT                */ 00428000
         L     SIOTPTR,HWFSIOTP(,DATAPREG)                      02S0102 00429000
*             END;                                              02S0103 00430000
*           UTAFFERR=NOERROR;       /* INSURE ERROR SWITCH OFF       */ 00431000
@RC00098 NI    UTAFFERR,B'01111111'                             02S0104 00432000
*           DO WHILE(HWEDT^=ON&HWUCB^=ON&/* NOT FOUND AND            */ 00433000
*                 UTAFFERR=NOERROR& /* NO UNIT INFO ERROR            */ 00434000
*                 SIOTPTR^=0);      /* AND IS A VALID SIOT           */ 00435000
         B     @DE00105                                         02S0105 00436000
@DL00105 DS    0H                                               02S0106 00437000
*             IF PRIDDNUM=SCTDDINO THEN/* FOUND CORRECT SIOT         */ 00438000
         CH    PRIDDNUM,SCTDDINO(,SIOTPTR)                      02S0106 00439000
         BNE   @RF00106                                         02S0106 00440000
*               IF SIOTSSDS=ON THEN /* GET UNIT CONVERTED FOR        */ 00441000
         TM    SIOTSSDS(SIOTPTR),B'10000000'                    02S0107 00442000
         BNO   @RF00107                                         02S0107 00443000
*                 DO;               /* SUB-SYSTEM DATA SETS          */ 00444000
*                   PREPSIOT->SCTUNAFF=OFF;/* AND REMOVE UNIT        */ 00445000
         L     @10,@PC00001+12                                  02S0109 00446000
         L     @10,PREPSIOT(,@10)                               02S0109 00447000
         NI    SCTUNAFF(@10),B'11111101'                        02S0109 00448000
*                   PREPSIOT->SIOTUNAF=0;/* AFFINITY INFORMATION     */ 00449000
         SLR   @07,@07                                          02S0110 00450000
         STH   @07,SIOTUNAF(,@10)                               02S0110 00451000
*                   CALL IEFAB470(HWCNTRLP,HWDATAP,HWRTNP,HWIOSLTP,     00452000
*                       SSDSNAME);                              02S0111 00453000
         L     @10,@PC00001                                     02S0111 00454000
         ST    @10,@AL00001                                     02S0111 00455000
         L     @10,@PC00001+4                                   02S0111 00456000
         ST    @10,@AL00001+4                                   02S0111 00457000
         L     @10,@PC00001+8                                   02S0111 00458000
         ST    @10,@AL00001+8                                   02S0111 00459000
         LA    @10,HWIOSLTP(,DATAPREG)                          02S0111 00460000
         ST    @10,@AL00001+12                                  02S0111 00461000
         LA    @10,@CC00072                                     02S0111 00462000
         ST    @10,@AL00001+16                                  02S0111 00463000
         L     @15,@CV00080                                     02S0111 00464000
         LA    @01,@AL00001                                     02S0111 00465000
         BALR  @14,@15                                          02S0111 00466000
*                 END;                                          02S0112 00467000
*               ELSE                                            02S0113 00468000
*                 IF SIOUCNVT=0 THEN/* UNIT INFORMATION VALID        */ 00469000
         B     @RC00107                                         02S0113 00470000
@RF00107 CLI   SIOUCNVT(SIOTPTR),0                              02S0113 00471000
         BNE   @RF00113                                         02S0113 00472000
*                   DO;                                         02S0114 00473000
*                     IF SIOTDMND=OFF THEN/* 'DEMAND' REQUEST IS A   */ 00474000
         TM    SIOTDMND(SIOTPTR),B'10000000'                    02S0115 00475000
         BNZ   @RF00115                                         02S0115 00476000
*                       DO;         /* UNIT='UNIT ADDRESS'           */ 00477000
*                         HWEDT=ON; /* INDICATE FOUND IN EDT         */ 00478000
         OI    HWEDT(CTRLPREG),B'10000000'                      02S0117 00479000
*                         HWEDTLUV=SIOTDEVT;/* COPY LOOK-UP-VALUE    */ 00480000
         L     @10,HWUCLPTR(,DATAPREG)                          02S0118 00481000
         MVC   HWEDTLUV(4,@10),SIOTDEVT(SIOTPTR)                02S0118 00482000
*                       END;                                    02S0119 00483000
*                     ELSE          /* UCB DEVICE ENTRY              */ 00484000
*                       DO;                                     02S0120 00485000
         B     @RC00115                                         02S0120 00486000
@RF00115 DS    0H                                               02S0121 00487000
*                         HWUCB=ON; /* INDICATE FOUND-IN-UCB,DEVICE  */ 00488000
         OI    HWUCB(CTRLPREG),B'00000001'                      02S0121 00489000
*                         HWDEVTYP=SIOTDEVT;/* TYPE AND ADDRESS      */ 00490000
         L     @10,HWUCLPTR(,DATAPREG)                          02S0122 00491000
         MVC   HWDEVTYP(4,@10),SIOTDEVT(SIOTPTR)                02S0122 00492000
*                         HWUCBP=SIOUCBAD;/* OF UCB IN HSKP WORKAREA */ 00493000
         MVC   HWUCBP(3,@10),SIOUCBAD(SIOTPTR)                  02S0123 00494000
*                       END;                                    02S0124 00495000
*                     IF SIOVAMDS=ON THEN/* VAM DATA SET             */ 00496000
@RC00115 TM    SIOVAMDS(SIOTPTR),B'00100000'                    02S0125 00497000
         BNO   @RF00125                                         02S0125 00498000
*                       HWVAME=ON;  /* INDICATE VAM ELIGIBLE         */ 00499000
         OI    HWVAME(CTRLPREG),B'01000000'                     02S0126 00500000
*                   END;                                        02S0127 00501000
*                 ELSE              /* UNIT AFFINITY TO DD WITH      */ 00502000
*                   UTAFFERR=ON;    /* UNCONVERTED UNIT INFO         */ 00503000
         B     @RC00113                                         02S0128 00504000
@RF00113 OI    UTAFFERR,B'10000000'                             02S0128 00505000
*             ELSE                  /* NOT CORRECT SIOT              */ 00506000
*               SIOTPTR=SIOTNPTR;   /* BASE SIOT MAP ON              */ 00507000
         B     @RC00106                                         02S0129 00508000
@RF00106 L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       02S0129 00509000
*           END;                    /* NEXT SIOT POINTER             */ 00510000
@RC00106 DS    0H                                               02S0130 00511000
@DE00105 TM    HWEDT(CTRLPREG),B'10000000'                      02S0130 00512000
         BO    @DC00105                                         02S0130 00513000
         TM    HWUCB(CTRLPREG),B'00000001'                      02S0130 00514000
         BO    @DC00105                                         02S0130 00515000
         TM    UTAFFERR,B'10000000'                             02S0130 00516000
         BNZ   @DC00105                                         02S0130 00517000
         LTR   SIOTPTR,SIOTPTR                                  02S0130 00518000
         BNZ   @DL00105                                         02S0130 00519000
@DC00105 DS    0H                                               02S0131 00520000
*         END;                                                  02S0131 00521000
*   IF HWDDDONE^=COMPLETE THEN      /* IF DD PROCESSING IS NOT       */ 00522000
@RF00096 DS    0H                                               01S0132 00523000
@RC00094 DS    0H                                               01S0132 00524000
@RF00093 TM    HWDDDONE(CTRLPREG),B'00010000'                   01S0132 00525000
         BO    @RF00132                                         01S0132 00526000
*     HWDSNAME=JFCBDSNM;            /* DONE, GET DSN FROM DD FOR PASS   00527000
*                                      DATA SET PROCESSING.          */ 00528000
         MVC   HWDSNAME(44,DATAPREG),JFCBDSNM(JFCBPTR)          01S0133 00529000
*   END IEFAB453                                                01S0134 00530000
*                                                               01S0134 00531000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00532000
*/*%INCLUDE SYSUT5  (DDPREPPR)                                       */ 00533000
*/*%INCLUDE SYSUT5  (DDPREPDC)                                       */ 00534000
*/*%INCLUDE SYSLIB  (IEFZB436)                                       */ 00535000
*/*%INCLUDE SYSLIB  (IEFZB437)                                       */ 00536000
*/*%INCLUDE SYSLIB  (IEFJFCBN)                                       */ 00537000
*/*%INCLUDE SYSLIB  (IEFASIOT)                                       */ 00538000
