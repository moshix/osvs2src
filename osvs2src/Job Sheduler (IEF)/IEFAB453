         TITLE ' IEFAB453 - DD PREPARATION                             *00001000
                        '                                               00002000
IEFAB453 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFAB453  74.099'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         MODESET EXTKEY=SCHED,SAVEKEY=(2)                               00015000
         GSPACE  R,LV=(0)                                               00016000
         LR    @11,@01                                          01S0001 00017000
         USING @DATD,@11                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         STC   @02,@OLDKEY                                      01S0001 00020000
         MODESET KEYADDR=(2)                                            00021000
         LM    @00,@02,20(@13)                                  01S0001 00022000
         ST    @11,8(,@13)                                      01S0001 00023000
         LR    @13,@11                                          01S0001 00024000
         MODESET EXTKEY=ZERO                                            00025000
         MVC   @PC00001(16),0(@01)                              01S0001 00026000
         MODESET EXTKEY=SCHED                                           00027000
*                                                                    */ 00028000
*   CTRLPREG=HWCNTRLP;              /* REMAP HSKP CONTROL AND DATA   */ 00029000
         L     @10,@PC00001                                     01S0025 00030000
         L     CTRLPREG,HWCNTRLP(,@10)                          01S0025 00031000
*   DATAPREG=HWDATAP;               /* PTRS FOR PROCESSING SPEED     */ 00032000
         L     @10,@PC00001+4                                   01S0026 00033000
         L     DATAPREG,HWDATAP(,@10)                           01S0026 00034000
*   RFY                                                         01S0027 00035000
*     HWCNTRL BASED(CTRLPREG);                                  01S0027 00036000
*   RFY                                                         01S0028 00037000
*     HWDATA BASED(DATAPREG);                                   01S0028 00038000
*   SIOTPTR=PREPSIOT;               /* MAP THE CURRENT SIOT          */ 00039000
         L     @10,@PC00001+12                                  01S0029 00040000
         L     SIOTPTR,PREPSIOT(,@10)                           01S0029 00041000
*   JFCBPTR=SJFCBPTR;               /* GET JFCB POINTER FROM SIOT    */ 00042000
