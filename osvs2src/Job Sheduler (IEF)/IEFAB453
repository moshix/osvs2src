*               END;                                            02S0066 00253000
*             SWNEW=OFF;            /* READ PREVIOUS JFCB USING      */ 00254000
@RC00057 DS    0H                                               02S0068 00255000
*             SWMOD=ON;             /* SAME EPA AS FOR SIOT          */ 00256000
         OI    SWMOD,B'00000010'                                02S0068 00257000
         NI    SWNEW,B'11110111'                                02S0068 00258000
*             CALL IEFAB4F7(SWACTLS,HWLEPARP,0,ADDR(PTABLP->SCTPJFCB),  00259000
