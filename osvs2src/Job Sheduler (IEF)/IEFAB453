         BNO   @RF00055                                         02S0055 00228000
@GL00004 L     @10,PTABLP                                       02S0055 00229000
         TM    SIOTSSDS(@10),B'10000000'                        02S0055 00230000
         BO    @RF00055                                         02S0055 00231000
*           DO;                     /* TO BE A VALID PROGRAM SOURCE  */ 00232000
*             IF PTABLP->SIOVAMDS=ON THEN/* IF THE REFERENCED DS     */ 00233000
         TM    SIOVAMDS(@10),B'00100000'                        02S0057 00234000
         BNO   @RF00057                                         02S0057 00235000
*               DO;                 /* IS VIRTUAL ACCESS, COPY THE   */ 00236000
*                 SCTUTYPE=PTABLP->SCTUTYPE;/* UNIT INFORMATION      */ 00237000
         MVC   SCTUTYPE(8,SIOTPTR),SCTUTYPE(@10)                02S0059 00238000
