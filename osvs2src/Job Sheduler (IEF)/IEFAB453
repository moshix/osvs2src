         NI    HWUCB(CTRLPREG),B'11110010'                      01S0035 00052000
*                                                               02S0036 00053000
*/********************************************************************/ 00054000
*/*                                                                  */ 00055000
*/* SEGMENT NAME - FASTPATH                                          */ 00056000
*/*                                                                  */ 00057000
*/* DESCRIPTIVE NAME - JFCB HOUSEKEEPING FASTPATH DETERMINATION      */ 00058000
*/*                                                                  */ 00059000
*/* FUNCTION - DETERMINE IF TERM=TS OR QNAME=  WAS SPECIFIED ON THE  */ 00060000
*/*            DD. IF YES, INDICATE NO MORE PROCESSING IS REQUIRED   */ 00061000
*/*            FOR THE DD. OTHERWISE, DETERMINE IF THE SIOT IS FOR   */ 00062000
*/*            A DD DUMMY, OR SUB-SYSTEM DATA SET, AND INDICATE THAT */ 00063000
*/*            NO DSNAME RESOLUTION IS REQUIRED FOR THIS DD. A SUB-  */ 00064000
*/*            SYSTEM DATA SET IS HASP... SYSIN,SYSOUT,ETC.  IF A    */ 00065000
*/*            DD DUMMY HAS NO DATA SET NAME, MAKE IT 'NULLFILE'.    */ 00066000
*/*                                                                  */ 00067000
*/* ENTRY - N/A                                                      */ 00068000
*/*   PURPOSE - TO DETERMINE IF ANY OR ALL PROCESSING MAY BE         */ 00069000
*/*             ELIMINATED FOR THE SPECIFIED DD.                     */ 00070000
*/*                                                                  */ 00071000
*/*   INPUT DATA - THE CURRENT SIOT                                  */ 00072000
*/*                THE HOUSEKEEPING WORK AREA CONTROLS               */ 00073000
*/*                                                                  */ 00074000
*/* EXITS -                                                          */ 00075000
*/*   CONDITION - AFTER DETERMINING IF ANY OR ALL PROCESSING MAY BE  */ 00076000
*/*               ELIMINATED FOR THE DD                              */ 00077000
*/*                                                                  */ 00078000
*/*   OUTPUT DATA - BITS ARE SET IN THE HOUSEKEEPING CONTROL AREA TO */ 00079000
*/*                 INDICATE IF ANY(HWDSNRQD) OR ALL(HWDDONE)        */ 00080000
*/*                 PROCESSING MAY BE ELIMINATED FOR THE SPECIFIED DD*/ 00081000
*/*                                                                  */ 00082000
*/*   RETURN CODE - ALWAYS ZERO                                      */ 00083000
*/*                                                                  */ 00084000
*/********************************************************************/ 00085000
*                                                               02S0036 00086000
*   HWDDDONE=NOTCOMP;               /* RESET DD-DONE SWITCH          */ 00087000
         NI    HWDDDONE(CTRLPREG),B'11101111'                   02S0036 00088000
*   HWDSNRQD=REQUIRED;              /* RESET DSN-MAY-BE-RQD SW.      */ 00089000
         OI    HWDSNRQD(CTRLPREG),B'00100000'                   02S0037 00090000
*   IF SIOTTERM=ON|                 /* IF TSO TERMINAL(DD TERM=TS)   */ 00091000
*       SIOTQNAM=ON THEN            /* OR QNAME= ON DD STATEMENT     */ 00092000
         TM    SIOTTERM(SIOTPTR),B'01000001'                    02S0038 00093000
         BZ    @RF00038                                         02S0038 00094000
*     HWDDDONE=COMPLETE;            /* THEN THIS DD COMPLETE         */ 00095000
         OI    HWDDDONE(CTRLPREG),B'00010000'                   02S0039 00096000
*/*  DETERMINE IF DSN RESOLUTION MAY BE ELIMINATED                   */ 00097000
*                                                               02S0040 00098000
*   ELSE                                                        02S0040 00099000
*     IF SCTDUMMY=ON|               /* IF DD DUMMY OR SUB-SYSTEM     */ 00100000
*         SIOTSSDS=ON THEN          /* DATA-SET,(HASP-SYSIN,ETC).       00101000
*                                                            @YM08189*/ 00102000
         B     @RC00038                                         02S0040 00103000
@RF00038 TM    SCTDUMMY(SIOTPTR),B'10000000'                    02S0040 00104000
         BO    @RT00040                                         02S0040 00105000
         TM    SIOTSSDS(SIOTPTR),B'10000000'                    02S0040 00106000
         BNO   @RF00040                                         02S0040 00107000
@RT00040 DS    0H                                               02S0041 00108000
*       DO;                         /* THEN NO DSN RESOLUTION IS     */ 00109000
*         HWDSNRQD=NODSNRQD;        /* REQUIRED                      */ 00110000
         NI    HWDSNRQD(CTRLPREG),B'11011111'                   02S0042 00111000
*         IF SCTDUMMY=ON&           /* DD DUMMY                      */ 00112000
*             JFCBDSNM=BLANK THEN   /* NO DATA SET NAME IN JFCB      */ 00113000
         TM    SCTDUMMY(SIOTPTR),B'10000000'                    02S0043 00114000
         BNO   @RF00043                                         02S0043 00115000
         CLC   JFCBDSNM(8,JFCBPTR),@CC00052                     02S0043 00116000
         BNE   @RF00043                                         02S0043 00117000
*           JFCBDSNM=NULLFILE;      /* SUBALLOCATION DSNAME          */ 00118000
         MVI   JFCBDSNM+8(JFCBPTR),C' '                         02S0044 00119000
         MVC   JFCBDSNM+9(35,JFCBPTR),JFCBDSNM+8(JFCBPTR)       02S0044 00120000
         MVC   JFCBDSNM(8,JFCBPTR),@CC00054                     02S0044 00121000
*       END;                                                    02S0045 00122000
@RF00043 DS    0H                                               02S0046 00123000
*   IF HWDDDONE^=COMPLETE THEN      /* DD PROCESSING NOT COMPLETE    */ 00124000
@RF00040 DS    0H                                               01S0046 00125000
@RC00038 TM    HWDDDONE(CTRLPREG),B'00010000'                   01S0046 00126000
         BO    @RF00046                                         01S0046 00127000
*     IF SCTGOSTP=ON&SCTDDNAM=PGMFETCH THEN/* NEED PROGRAM FETCH        00128000
*                                                            @YM07284*/ 00129000
         L     @10,HWSCTP(,DATAPREG)                            01S0047 00130000
         TM    SCTGOSTP(@10),B'10000000'                        01S0047 00131000
         BNO   @RF00047                                         01S0047 00132000
         CLC   SCTDDNAM(8,SIOTPTR),@CC00070                     01S0047 00133000
         BNE   @RF00047                                         01S0047 00134000
*       DO;                                                     01S0048 00135000
*                                                               02S0049 00136000
*/********************************************************************/ 00137000
*/*                                                                  */ 00138000
*/* SEGMENT NAME - FETCHLIB                                          */ 00139000
*/*                                                                  */ 00140000
*/* DESCRIPTIVE NAME - PROGRAM FETCH PROCESSING                      */ 00141000
*/*                                                                  */ 00142000
*/* FUNCTION - RETREIVE VOLUME AND UNIT INFORMATION FOR PROGRAM      */ 00143000
*/*            FETCH DCB   -  DDNAME = 'PGM=*.DD'                    */ 00144000
*/*                                                                  */ 00145000
*/*   OPERATION- THE SIOT AND JFCB REPRESENTING THE PROGRAM ARE READ.*/ 00146000
*/*            REFERENCED SIOT NOT ALLOCATED (STEP NOT RUN), OR      */ 00147000
*/*            INVALID UNIT TYPE (NOT DIRECT ACCESS OR VIRTUAL       */ 00148000
*/*            ACCESS), OR THE DATASET IS A SUBSYSTEM (SYSIN ETC.),  */ 00149000
*/*            CAUSES TERMINATION WITH RETURN/REASON CODES IN THE    */ 00150000
*/*            HSKP RETURN INFORMATION AREA.                         */ 00151000
*/*            THE PROGRAM NAME IN THE SCT IS OBTAINED FROM THE      */ 00152000
*/*            ELEMENT NUMBER/MEMBER NAME FIELD OF THE READ JFCB,    */ 00153000
*/*            WHICH ALSO YIELDS THE VOLUME INFORMATION.             */ 00154000
*/*            VARIOUS FIELDS IN THE CURRENT SIOT ARE RESET OR       */ 00155000
*/*            PARTIALLY RESET FROM THEIR ORIGINAL VALUE AS          */ 00156000
*/*            INITIALIZED BY THE INTERPRETER FROM THE OLD TABLES.   */ 00157000
*/*            'HWDDDONE' THEN INDICATES  'DD PROCESSING COMPLETE'.  */ 00158000
*/*                                                                  */ 00159000
*/* ENTRY - DDNAME IS 'PGM=*.DD'                                     */ 00160000
*/*                                                                  */ 00161000
*/*   PURPOSE - PREPARE THE SIOT AND JFCB FOR DCB CREATION FOR       */ 00162000
*/*             PROGRAM FETCH.                                       */ 00163000
*/*                                                                  */ 00164000
*/*   INPUT DATA - SIOT, SCT, JFCB                                   */ 00165000
*/*                                                                  */ 00166000
*/* EXIT - IN LINE                                                   */ 00167000
*/*                                                                  */ 00168000
*/*   CONDITION - ALL                                                */ 00169000
*/*                                                                  */ 00170000
*/*   OUTPUT DATA - COMPLETED SIOT, SCT AND JFCB IF SUCCESSFUL       */ 00171000
*/*             -   REASON CODE 7 IF ERROR DETECTED:                 */ 00172000
*/*                 REFERENCED STEP NOT RUN                          */ 00173000
*/*                                                                  */ 00174000
*/*   RETURN CODES - SUCCESS  0                                      */ 00175000
*/*                - FAILURE  4                                      */ 00176000
*/*                                                                  */ 00177000
*/********************************************************************/ 00178000
*                                                               02S0049 00179000
*         SWACTLS=0;                /* READ THE FETCHLIB SIOT FROM   */ 00180000
         SLR   @10,@10                                          02S0049 00181000
         STH   @10,SWACTLS                                      02S0049 00182000
*         SWREAD=ON;                /* SWA TO CHECK UNIT TYPE        */ 00183000
         OI    SWREAD,B'00100000'                               02S0050 00184000
*         IF HWLEPARP=0 THEN        /* USE OLD EPA, IF ONE EXISTS    */ 00185000
         C     @10,HWLEPARP(,DATAPREG)                          02S0051 00186000
         BNE   @RF00051                                         02S0051 00187000
*           SWNEW=ON;               /* OR HAVE A NEW ONE CREATED     */ 00188000
         OI    SWNEW,B'00001000'                                02S0052 00189000
