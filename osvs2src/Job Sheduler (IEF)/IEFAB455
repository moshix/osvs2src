         TM    HWPDIEDL(@10),B'01000000'                        01S0072 00356000
         BNO   @RF00072                                         01S0072 00357000
*         DO;                       /* RECEIVED.POINTER TO DELETED   */ 00358000
*           HWRPDIEP=PDIEPTR;       /* PDIE FOR PDI BUILD REUSE.     */ 00359000
         L     @10,PDIEPTR                                      01S0074 00360000
