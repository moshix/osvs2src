         CH    @10,JFCBVLSQ(,@03)                               02S0127 00371000
         BNE   @RF00127                                         02S0127 00372000
*               JFCBVLSQ=DS1VOLSQ;  /* MOVE VOL SEQ FROM DSCB        */ 00373000
         MVC   JFCBVLSQ(2,@03),DS1VOLSQ(F1PTR)                  02S0128 00374000
*             IF JFCBXPDT=ZEROS THEN/* IF EXPIRATION DATE ZEROED     */ 00375000
@RF00127 L     @10,@PC00001+16                                  02S0129 00376000
         L     @10,JFCBPTR(,@10)                                02S0129 00377000
         CLC   JFCBXPDT(3,@10),ZEROS                            02S0129 00378000
         BNE   @RF00129                                         02S0129 00379000
*               JFCBXPDT=DS1EXPDT;  /* MOVE DATE FROM DSCB           */ 00380000
