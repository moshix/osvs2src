         BNO   @RF00234                                         03S0234 01012000
         TM    EPAJCTEX,B'00000100'                             03S0234 01013000
         BNO   @RF00234                                         03S0234 01014000
*                 HWEPAJCT=OFF;     /* IT, TELL HSKP CALLER.         */ 01015000
         NI    HWEPAJCT(@10),B'11101111'                        03S0235 01016000
*               IF RTCDSAV^=0 THEN  /* BAD ENQUEUE,                  */ 01017000
@RF00234 L     @10,RTCDSAV                                      03S0236 01018000
         LTR   @10,@10                                          03S0236 01019000
         BZ    @RF00236                                         03S0236 01020000
*                 DO;               /* ENQUEUE FAILED, FAIL JOB.     */ 01021000
*                   HWRTCD=DDERR;   /* INDICATE DD ERROR FOR         */ 01022000
         LA    @10,4                                            03S0238 01023000
         STH   @10,HWRTCD(,RTNPREG)                             03S0238 01024000
*                   IF ERRORCD=NOCHNG THEN/* INTEGRITY VIOLATION.    */ 01025000
         CLC   ERRORCD(2),@CH00094                              03S0239 01026000
         BNE   @RF00239                                         03S0239 01027000
*                     HWRSNCD=RSN4; /* DIFFERENT REASON CODE FOR     */ 01028000
         STH   @10,HWRSNCD(,RTNPREG)                            03S0240 01029000
*                   ELSE            /* DYNAMIC. BATCH GETS SAME      */ 01030000
*                     HWRSNCD=RSN3; /* MSG. FOR RSN. CODE 3 OR 4.    */ 01031000
         B     @RC00239                                         03S0241 01032000
@RF00239 MVC   HWRSNCD(2,RTNPREG),@CH00064                      03S0241 01033000
*                 END;              /* END ENQUEUE FAILURE.          */ 01034000
*               ELSE                                            03S0243 01035000
*                 IF DSNQENTP^=0&SIOTGDSN=NO THEN/* ENQ OK GET  03S0243 01036000
*                                                            @YM07288*/ 01037000
