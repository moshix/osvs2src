@RF00227 L     @10,CVTPTR                                       03S0229 00967000
         L     @10,CVTTCBP(,@10)                                03S0229 00968000
         L     @10,TCBPTRS+4(,@10)                              03S0229 00969000
         ST    @10,ACTTCBP                                      03S0229 00970000
*               RTCDSAV=0;          /* NORMAL RETURN, CHECK LATER.   */ 00971000
@RC00227 SLR   @10,@10                                          03S0230 00972000
         ST    @10,RTCDSAV                                      03S0230 00973000
*               STEPNMBR=0;         /* STEP NO.OF 0, LEAVE ENQUED       00974000
*                                      UNTIL JOB UNALLOC.    @YM07288*/ 00975000
         MVI   STEPNMBR,X'00'                                   03S0231 00976000
*               DSNQENTP=0;         /* CLEAR ENTRY PTR       @YM07288*/ 00977000
         ST    @10,DSNQENTP                                     03S0232 00978000
*               CALL IEFAB4DC       /* GET DSENQ ENTRY AND ENQ DSN.     00979000
*                                      RETURN DSENQ SVAB.            */ 00980000
*                   (DSRRFMAP,HWJCTP,DSNPARM,HWLEPAP,ACTTCBP,STEPNMBR,  00981000
*                   RETPTRS1,0,RTCDSAV,HWDSENQP,0);             03S0233 00982000
         LA    @10,DSRRFMAP                                     03S0233 00983000
         ST    @10,@AL00001                                     03S0233 00984000
         ST    DATAPREG,@AL00001+4                              03S0233 00985000
