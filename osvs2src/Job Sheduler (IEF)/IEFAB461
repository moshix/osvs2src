         L     @06,JFCBPTR(,@07)                                02S0197 00820000
         ST    @08,@TF00001                                     02S0197 00821000
         ALR   @08,@06                                          02S0197 00822000
         AL    @08,@CF00886                                     02S0197 00823000
         MVC   JFCBDSNM(1,@08),LFTPAREN                         02S0197 00824000
*               JFCBDSNM(HWDSNLN+1:HWDSNLN+ELNMSZ-1)=JFCBELNM;/*        00825000
*                                      APPEND THE GENERATION NUMBER     00826000
*                                      TO THE DSN                    */ 00827000
