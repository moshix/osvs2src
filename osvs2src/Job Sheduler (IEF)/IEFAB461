         ICM   @15,3,HWRTCD(@10)                                02S0157 00564000
         BNZ   @RF00157                                         02S0157 00565000
*                 DO;               /* LOCATE SUCCESSFUL             */ 00566000
*                   DO SEARCH=MAXDSNLN TO 1 BY-1 WHILE(GDGDSNWA(SEARCH) 00567000
*                         =BLANK);                              02S0159 00568000
         LA    SEARCH,44                                        02S0159 00569000
