         L     @06,HWCNTRLP(,@08)                               02S0200 00843000
         STH   @10,HWJLCTLS(,@06)                               02S0200 00844000
*               HWSYSLOC=SYSLOCAT;  /* ISSUE SYSTEM LOCATE           */ 00845000
*               HWGDGSLC=SPECIAL;   /* GDG BASE LEVEL PROVIDED       */ 00846000
         OI    HWSYSLOC(@06),B'00100100'                        02S0202 00847000
*               HWDOMOD=UPDATE;     /* CHANGE 'MOD' DISPOSITION TO      00848000
*                                      'NEW' IF NECESSARY            */ 00849000
         OI    HWDOMOD(@06),B'10000000'                         02S0203 00850000
*               CALL IEFAB469(HWCNTRLP,HWDATAP,HWRTNP,HWCSIOTP,JFCBPTR, 00851000
*                   ADDR(JFCBDSNM));/* LOCATE REQUESTED DATA SET     */ 00852000
         ST    @08,@AL00001                                     02S0204 00853000
