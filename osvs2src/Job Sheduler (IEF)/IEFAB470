         TM    LUVVAM(LUVPTR),B'10000000'                       01S0057 00059000
         BNO   @RF00057                                         01S0057 00060000
*           HWVAME=ON;              /* INDICATE IT IN THE HSKPWA     */ 00061000
         OI    HWVAME(CTRLPREG),B'01000000'                     01S0058 00062000
*         IF LUVGENNO=ONE           /* ONE GENERIC IN EDT    @YA00060*/ 00063000
*             &HWNOVRID=OFF THEN    /* OK TO OVERRIDE,       @YA00060*/ 00064000
@RF00057 CLC   LUVGENNO(4,LUVPTR),@CF00064                      01S0059 00065000
