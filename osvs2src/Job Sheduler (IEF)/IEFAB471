*TESTBIT: PROC;                                                 02S0442 01523000
TESTBIT  STM   @14,@12,@SA00002                                 02S0442 01524000
*/********************************************************************/ 01525000
*/*                                                                  */ 01526000
*/* SEGMENT NAME - TESTBIT                                           */ 01527000
*/*                                                                  */ 01528000
*/* DESCRIPTIVE NAME - TEST BIT IN GROUP MASK                        */ 01529000
*/*                                                                  */ 01530000
*/* FUNCTION -                                                       */ 01531000
*/*      THIS SUBROUTINE DETERMINES IF A PARTICULAR BIT IN A         */ 01532000
*/*      GROUP MASK IS ON OR OFF. THE GROUP MASK IS A VARIABLE       */ 01533000
*/*      LENGTH BIT MASK POINTED TO BY MASKPTR . THE ONE-ORIGIN      */ 01534000
*/*      INDEX OF THE BIT TO BE TESTED IS CONTAINED IN FIELD         */ 01535000
*/*      GROUPNO.                                                    */ 01536000
*/*                                                                  */ 01537000
*/*   OPERATION -                                                    */ 01538000
*/*      THIS SEGMENT GENERATES BAL INSTRUCTIONS.                    */ 01539000
*/*                                                                  */ 01540000
*/*      ONE IS SUBTRACTED FROM THE GROUPNO TO PRODUCE A ZERO-ORIGIN */ 01541000
*/*      INDEX. THE INDEX IS THEN DIVIDED BY EIGHT TO DETERMINE THE  */ 01542000
*/*      BYTE (AND REMAINDER)  OF THE BIT BEING TESTED (IN A SRDL    */ 01543000
*/*      INSTRUCTION, THE ODD REGISTER GETS THE BITS SHIFTED OUT OF  */ 01544000
*/*      THE EVEN REGISTER.). THE REMAINDER IS ALIGNED IN A REGISTER */ 01545000
*/*      AND IS USED TO INDICATE THE BIT POSITION IN THE BYTE. THIS  */ 01546000
*/*      BIT POSITION IS USED IN A TEST-UNDER-MASK INSTRUCTION VIA   */ 01547000
*/*      EXECUTE.                                                    */ 01548000
*/*                                                                  */ 01549000
*/*      EXAMPLE - IF GROUPNO = 10, THIS OPERATION WILL DETERMINE    */ 01550000
*/*                THE BIT IS 1 BYTE AFTER THE BEGINNING OF THE      */ 01551000
*/*                GROUP MASK AND IS THE SECOND BIT POSITION.        */ 01552000
*/*                                                                  */ 01553000
*/* ENTRY - THIS CODE IS AN INLINE SUBROUTINE WHICH IS ENTERED       */ 01554000
*/*         AT THE TOP.                                              */ 01555000
*/*                                                                  */ 01556000
*/*   PURPOSE  -  TO DETERMINE IF A PARTICULAR BIT IN A GROUP        */ 01557000
*/*               MASK IS ON OR OFF.                                 */ 01558000
*/*                                                                  */ 01559000
*/*   INPUT -                                                        */ 01560000
*/*       MASKPTR - PTR TO GROUP MASK                                */ 01561000
*/*       GROUPNO - 1-ORIGIN INDEX INTO MASK OF THE BIT TO BE TESTED */ 01562000
*/*       BITRTN - BIT FOR RESULTS OF TEST TO BE RETURNED IN         */ 01563000
*/*                                                                  */ 01564000
*/* EXIT - NORMAL - THIS CODE IS AN INLINE SUBROUTINE WHICH          */ 01565000
*/*                 EXITS FROM THE BOTTOM.                           */ 01566000
*/*                                                                  */ 01567000
*/*   CONDITION - BIT TESTED AND RESULTS INDICATED.                  */ 01568000
*/*                                                                  */ 01569000
*/*   OUTPUT -                                                       */ 01570000
*/*       BITRTN - SET TO 1 IF BIT WAS ON                            */ 01571000
*/*              - SET TO 0 IF BIT WAS OFF                           */ 01572000
*/*                                                                  */ 01573000
*/*   RETURN CODES - (NONE)                                          */ 01574000
*/*                                                                  */ 01575000
*/********************************************************************/ 01576000
*BITRTN = OFFVALUE;                    /*ASSUME BIT IS OFF           */ 01577000
         NI    BITRTN,B'01111111'                               02S0443 01578000
*RFY (EVENREG,ODDREG,MASKREG) RSTD;    /*RESTRICT USE OF REGS 3,4+5  */ 01579000
*GEN REFS(EVENREG,GROUPNO,MASKPTR,ODDREG,MASKREG,TMINST)        02S0445 01580000
*    FLOWS(BITOFF);                                             02S0445 01581000
        L     EVENREG,GROUPNO         PUT BIT INDEX INTO REG            01582000
        BCTR  EVENREG,0               DECREMENT INDEX FOR ZERO ORIGIN   01583000
        SRDL  EVENREG,3               DIVIDE INDEX BY 8.SAVE REMAINDER  01584000
        A     EVENREG,MASKPTR         POINT TO CORRECT BYTE             01585000
        SRL   ODDREG,29               ALIGN REMAINDER IS REG            01586000
        LA    MASKREG,X'80'           INSERT 0-BIT MASK                 01587000
        SRL   MASKREG,0(ODDREG)       POSITION MASK BIT                 01588000
        EX    MASKREG,TMINST          TEST IF BIT IS ON                 01589000
        BNO   BITOFF                  BRANCH IF BIT NOT ON              01590000
*RFY (EVENREG,ODDREG,MASKREG) UNRSTD;  /*UNRESTRICT REGS 3,4, AND 5  */ 01591000
*BITRTN = ONVALUE;                     /*INDICATE BIT WAS ON         */ 01592000
         OI    BITRTN,B'10000000'                               02S0447 01593000
*BITOFF:;                                                       02S0448 01594000
*RETURN;                                                        02S0449 01595000
@EL00002 DS    0H                                               02S0449 01596000
@EF00002 DS    0H                                               02S0449 01597000
@ER00002 LM    @14,@12,@SA00002                                 02S0449 01598000
         BR    @14                                              02S0449 01599000
*END TESTBIT;                                                   02S0450 01600000
         B     @EL00002                                         02S0450 01601000
*END IEFAB471                          /*END OF GENERIC-ALLOC-CONTROL*/ 01602000
