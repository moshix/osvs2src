*RFY GMTENTRY BASED(LUVGMTP);          /*RETURN NORMAL GRP MASK BASE */ 01002000
*/********************************************************************/ 01003000
*/*                                                                  */ 01004000
*/* SEGMENT NAME - MGENMND                                           */ 01005000
*/*                                                                  */ 01006000
*/* DESCRIPTIVE NAME - ELIMINATE GENERICS FOR MUG DEMANDS            */ 01007000
*/*                                                                  */ 01008000
*/* FUNCTION - THIS SEGMENT DETERMINES IF THERE ARE ANY MULTI-UNIT,  */ 01009000
*/*            MULTI-GENERIC GROUPS WITH DEMAND REQS - IF THERE ARE, */ 01010000
*/*            IEFAB481 IS CALLED FOR EACH REQ IN THE MUG GROUP TO   */ 01011000
*/*            MARK ALL OTHER GENERICS,EXCEPT THE ONE FOR THE        */ 01012000
*/*            DEMANDED UNIT, AS INELIGIBLE IN BOTH THE EDL AND THE  */ 01013000
*/*            ALGORITHM GROUP LIST ENTRIES.                         */ 01014000
*/*                                                                  */ 01015000
*/*            IT IS AN ERROR IF THERE ARE TWO DEMAND REQS IN THE    */ 01016000
*/*            SAME MUG GROUP AND THEY DEMAND UNITS FROM DIFFERENT   */ 01017000
*/*            DEVICE TYPES.                                         */ 01018000
*/*                                                                  */ 01019000
*/*   OPERATION - (DOCUMENTED BY THE CODE)                           */ 01020000
*/*                                                                  */ 01021000
*/* ENTRY - (INLINE SUBROUTINE)                                      */ 01022000
*/*                                                                  */ 01023000
*/*   INPUT -                                                        */ 01024000
*/*       SIOT1P - PTR TO THE FIRST SIOT IN THE CHAIN                */ 01025000
*/*        SIOTAFID - MUG GROUP ID -- NONE IF ZERO                   */ 01026000
*/*        SIOTDMND - 1 INDICATES A DEMAND REQ                       */ 01027000
*/*        SIOTEDLP - PTR TO THE ELIGIBLE DEVICES LIST               */ 01028000
*/*                                                                  */ 01029000
*/* EXIT - NORMAL - (INLINE SUBROUTINE)                              */ 01030000
*/*                                                                  */ 01031000
*/*   CONDITION - FUNCTION COMPLETED. NO ERRORS                      */ 01032000
*/*                                                                  */ 01033000
*/*   OUTPUT - (NONE EXCEPT EDL'S AND ALGORITHM TABLES MAY BE        */ 01034000
*/*            UPDATED BY IEFAB481 -- SEE IEFAB481'S SPEC )          */ 01035000
*/*                                                                  */ 01036000
*/*   RETURN CODES - (NONE)                                          */ 01037000
*/*                                                                  */ 01038000
*/* EXIT - ERROR - (INLINE SUBROUTINE)                               */ 01039000
*/*                                                                  */ 01040000
*/*   CONDITION - 2 DEMAND REQS IN SAME MUG GROUP WANTED DIFFERENT   */ 01041000
*/*               GENERIC DEVICE TYPES                               */ 01042000
*/*                                                                  */ 01043000
*/*   OUTPUT - SIOTRSNC=20                                           */ 01044000
*/*                                                                  */ 01045000
*/*   RETURN CODES - RTNCODE=4                                       */ 01046000
*/*                                                                  */ 01047000
*/********************************************************************/ 01048000
*SIOTPTR = SIOT1P;                     /*POINT TO FIRST SIOT         */ 01049000
         L     @10,@PC00001                                     02S0312 01050000
         L     @10,ALCWAPTR(,@10)                               02S0312 01051000
         L     SIOTPTR,SIOT1P(,@10)                             02S0312 01052000
*DO WHILE (SIOTPTR^=ZERO & RTNCODE=SUCCESS); /*SCAN ALL SIOTS        */ 01053000
         B     @DE00313                                         02S0313 01054000
@DL00313 DS    0H                                               02S0314 01055000
