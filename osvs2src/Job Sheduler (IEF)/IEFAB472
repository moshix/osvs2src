*TESTBIT: PROC;                                                 02S0481 01676000
TESTBIT  STM   @14,@12,@SA00002                                 02S0481 01677000
*/********************************************************************/ 01678000
*/*                                                                  */ 01679000
*/* SEGMENT NAME - TESTBIT                                           */ 01680000
*/*                                                                  */ 01681000
*/* DESCRIPTIVE NAME - TEST BIT IN GROUP MASK                        */ 01682000
*/*                                                                  */ 01683000
*/* FUNCTION -                                                       */ 01684000
*/*      THIS SUBROUTINE DETERMINES IF A PARTICULAR BIT IN A         */ 01685000
*/*      GROUP MASK IS ON OR OFF. THE GROUP MASK IS A VARIABLE       */ 01686000
*/*      LENGTH BIT MASK POINTED TO BY MASKPTR . THE ONE-ORIGIN      */ 01687000
*/*      INDEX OF THE BIT TO BE TESTED IS CONTAINED IN FIELD         */ 01688000
*/*      GROUPNO.                                                    */ 01689000
*/*                                                                  */ 01690000
*/*   OPERATION -                                                    */ 01691000
*/*      THIS SEGMENT GENERATES BAL INSTRUCTIONS                     */ 01692000
*/*                                                                  */ 01693000
*/*      ONE IS SUBTRACTED FROM THE GROUPNO TO PRODUCE A ZERO-ORIGIN */ 01694000
*/*      INDEX. THE INDEX IS THEN DIVIDED BY EIGHT TO DETERMINE THE  */ 01695000
*/*      BYTE (AND REMAINDER)  OF THE BIT BEING TESTED (IN A SRDL    */ 01696000
*/*      INSTRUCTION, THE ODD REGISTER GETS THE BITS SHIFTED OUT OF  */ 01697000
*/*      THE EVEN REGISTER.). THE REMAINDER IS ALIGNED IN A REGISTER */ 01698000
*/*      AND IS USED TO INDICATE THE BIT POSITION IN THE BYTE. THIS  */ 01699000
*/*      BIT POSITION IS USED IN A TEST-UNDER-MASK INSTRUCTION VIA   */ 01700000
*/*      EXECUTE.                                                    */ 01701000
*/*                                                                  */ 01702000
*/*      EXAMPLE - IF GROUPNO = 10, THIS OPERATION WILL DETERMINE    */ 01703000
*/*                THE BIT IS 1 BYTE AFTER THE BEGINNING OF THE      */ 01704000
*/*                GROUP MASK AND IS THE SECOND BIT POSITION.        */ 01705000
*/*                                                                  */ 01706000
*/* ENTRY - THIS CODE IS AN INLINE SUBROUTINE WHICH IS ENTERED       */ 01707000
*/*         AT THE TOP.                                              */ 01708000
*/*                                                                  */ 01709000
*/*   PURPOSE  -  TO DETERMINE IF A PARTICULAR BIT IN A GROUP        */ 01710000
*/*               MASK IS ON OR OFF.                                 */ 01711000
*/*                                                                  */ 01712000
*/*   INPUT -                                                        */ 01713000
*/*       MASKPTR - PTR TO GROUP MASK                                */ 01714000
*/*       GROUPNO - 1-ORIGIN INDEX INTO MASK OF THE BIT TO BE TESTED */ 01715000
*/*       BITRTN - BIT FOR RESULTS OF TEST TO BE RETURNED IN         */ 01716000
*/*                                                                  */ 01717000
*/* EXIT - NORMAL - THIS CODE IS AN INLINE SUBROUTINE WHICH          */ 01718000
*/*                 EXITS FROM THE BOTTOM.                           */ 01719000
*/*                                                                  */ 01720000
*/*   CONDITION - BIT TESTED AND RESULTS INDICATED.                  */ 01721000
*/*                                                                  */ 01722000
*/*   OUTPUT -                                                       */ 01723000
*/*       BITRTN - SET TO 1 IF BIT WAS ON                            */ 01724000
*/*              - SET TO 0 IF BIT WAS OFF                           */ 01725000
*/*                                                                  */ 01726000
*/*   RETURN CODES - (NONE)                                          */ 01727000
*/*                                                                  */ 01728000
*/********************************************************************/ 01729000
*BITRTN = OFFVALUE;                    /*ASSUME BIT IS OFF           */ 01730000
         NI    BITRTN,B'01111111'                               02S0482 01731000
*RFY (EVENREG,ODDREG,MASKREG) RSTD;    /*RESTRICT USE OF REGS 3,4+5  */ 01732000
*GEN REFS(EVENREG,GROUPNO,MASKPTR,ODDREG,MASKREG,TMINST)        02S0484 01733000
*    FLOWS(BITOFF);                                             02S0484 01734000
        L     EVENREG,GROUPNO         PUT BIT INDEX INTO REG            01735000
        BCTR  EVENREG,0               DECREMENT INDEX FOR ZERO ORIGIN   01736000
        SRDL  EVENREG,3               DIVIDE INDEX BY 8.SAVE REMAINDER  01737000
        A     EVENREG,MASKPTR         POINT TO CORRECT BYTE             01738000
        SRL   ODDREG,29               ALIGN REMAINDER IS REG            01739000
        LA    MASKREG,X'80'           INSERT 0-BIT MASK                 01740000
        SRL   MASKREG,0(ODDREG)       POSITION MASK BIT                 01741000
        EX    MASKREG,TMINST          TEST IF BIT IS ON                 01742000
        BNO   BITOFF                  BRANCH IF BIT NOT ON              01743000
*RFY (EVENREG,ODDREG,MASKREG) UNRSTD;  /*UNRESTRICT REGS 3,4, AND 5  */ 01744000
*BITRTN = ONVALUE;                     /*INDICATE BIT WAS ON         */ 01745000
         OI    BITRTN,B'10000000'                               02S0486 01746000
*BITOFF:;                                                       02S0487 01747000
*RETURN;                                                        02S0488 01748000
@EL00002 DS    0H                                               02S0488 01749000
@EF00002 DS    0H                                               02S0488 01750000
@ER00002 LM    @14,@12,@SA00002                                 02S0488 01751000
         BR    @14                                              02S0488 01752000
*END TESTBIT;                                                   02S0489 01753000
         B     @EL00002                                         02S0489 01754000
*END IEFAB472                                                   01S0490 01755000
