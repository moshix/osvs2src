@RC00100 DS    0H                                               02S0104 00194000
*            END;                                               02S0104 00195000
@RC00097 DS    0H                                               02S0105 00196000
*          TRIEDENS = TRIEDENS | TESTBITS; /*UPDATE DENSITIES TRIED  */ 00197000
@RC00086 OC    TRIEDENS(1),TESTBITS                             02S0105 00198000
