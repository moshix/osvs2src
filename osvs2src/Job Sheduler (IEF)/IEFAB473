         LR    @13,@09                                          01S0001 00021000
         MVC   @PC00001(8),0(@01)                               01S0001 00022000
*RETCODE = SUCCESS;                    /*INITIALIZE RETURN CODE TO 0 */ 00023000
         SLR   RETCODE,RETCODE                                  01S0069 00024000
*UCBPTR = UCBLSTEN(ONE);               /*PTR TO UCB                  */ 00025000
