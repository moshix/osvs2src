         TITLE 'IEFAB473 - AVR CONTROL                                 *00001000
                        '                                               00002000
IEFAB473 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(24)                                          01S0001 00007000
         DC    C'IEFAB473 77278  UZ14747 '                      01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @10,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@10                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         GSPACE   R,LV=(0)                                              00015000
         LR    @09,@01                                          01S0001 00016000
         USING @DATD,@09                                        01S0001 00017000
