         LA    @12,UCBPTR                                       02S0106 00201000
         ST    @12,@AL00001                                     02S0106 00202000
         LA    @12,TAPESTAT                                     02S0106 00203000
         ST    @12,@AL00001+4                                   02S0106 00204000
         LA    @12,RECORDP                                      02S0106 00205000
         ST    @12,@AL00001+8                                   02S0106 00206000
         L     @15,@CV01567                                     02S0106 00207000
         LA    @01,@AL00001                                     02S0106 00208000
         BALR  @14,@15                                          02S0106 00209000
*          RETCODE = REG15;            /*PICK UP RETURN CODE         */ 00210000
         LR    RETCODE,REG15                                    02S0107 00211000
*        END;                                                   02S0108 00212000
@DE00085 LTR   RETCODE,RETCODE                                  02S0108 00213000
         BZ    @DC00085                                         02S0108 00214000
         CLI   TRIEDENS,B'11100000'                             02S0108 00215000
         BNE   @DL00085                                         02S0108 00216000
@DC00085 DS    0H                                               02S0109 00217000
*      END;                                                     02S0109 00218000
*    ELSE                              /*MUST BE 9 TRK               */ 00219000
*      DO;                                                      02S0110 00220000
         B     @RC00082                                         02S0110 00221000
@RF00082 DS    0H                                               02S0111 00222000
*        TAPEBITS = NINETRK;           /*INDICATE 9 TRK              */ 00223000
         MVI   TAPEBITS,B'10000000'                             02S0111 00224000
