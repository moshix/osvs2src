         TITLE 'IEFAB475 - ALLOCATE WITHIN A GENERIC                   *00001000
                        '                                               00002000
IEFAB475 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFAB475  75.090'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         GSPACE   R,LV=(0)                                              00015000
         LR    @11,@01                                          01S0001 00016000
         USING @DATD,@11                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @11,8(,@13)                                      01S0001 00020000
         LR    @13,@11                                          01S0001 00021000
         MVC   @PC00001(12),0(@01)                              01S0001 00022000
*RTNCODE=SUCCESS;                      /*ASSUME NO ERRORS            */ 00023000
         SLR   RTNCODE,RTNCODE                                  01S0065 00024000
*RFY CVRRQLST BASED(VOLALGTP);                                  01S0066 00025000
*DOTYPES = ZERO;                       /*INITIALIZE SWITCHES         */ 00026000
         MVI   DOTYPES,X'00'                                    01S0067 00027000
*SPDEMAND = OFF;                       /*ASSUME NO SPEC DEMANDS      */ 00028000
         L     @10,@PC00001+8                                   01S0068 00029000
         NI    SPDEMAND(@10),B'11011111'                        01S0068 00030000
*/********************************************************************/ 00031000
*/*                                                                  */ 00032000
*/* SEGMENT NAME - FLAGDMAN                                          */ 00033000
*/*                                                                  */ 00034000
*/* DESCRIPTIVE NAME - FLAG ALL REQS EXCEPT DEMANDS ELIGIBLE FOR     */ 00035000
*/*                    THIS GENERIC                                  */ 00036000
*/*                                                                  */ 00037000
*/* FUNCTION -                                                       */ 00038000
*/*     THIS ROUTINE MARKS ALL SIOTS EXCEPT DEMAND REQUESTS WHICH    */ 00039000
*/*     ARE ELIGIBLE FOR THE GENERIC DEVICE TYPE JUST LOCKED -- SO   */ 00040000
*/*     THAT MODULE IEFAB479 (ALLOCATE DEMAND REQS) WILL ONLY TRY    */ 00041000
*/*     TO ALLOCATE DEMAND REQUESTS ELIGIBLE FOR THIS GENERIC        */ 00042000
*/*     (I.E. THE UNMARKED SIOTS).                                   */ 00043000
*/*                                                                  */ 00044000
*/*   OPERATION - THE OPERATION IS DOCUMENTED BY THE CODE ITSELF.    */ 00045000
*/*                                                                  */ 00046000
*/* ENTRY - (INLINE SUBROUTINE)                                      */ 00047000
*/*                                                                  */ 00048000
*/*   PURPOSE - (SEE FUNCTION)                                       */ 00049000
*/*                                                                  */ 00050000
*/*   INPUT - ALCWAPTR - PTR TO THE ALLOCATION WORK AREA (ALCWA)     */ 00051000
*/*            - SIOT1P - PTR TO THE FIRST SIOT IN THE CHAIN         */ 00052000
*/*            - LGENLOCK - ID OF THE GENERIC JUST LOCKED            */ 00053000
*/*                                                                  */ 00054000
*/* EXIT - NORMAL - (INLINE SUBROUTINE)                              */ 00055000
*/*                                                                  */ 00056000
*/*   CONDITION - FUNCTION COMPLETED                                 */ 00057000
*/*                                                                  */ 00058000
*/*   OUTPUT - SIOTGIGN - SET TO 0 FOR DEMAND REQS ELIGIBLE TO THE   */ 00059000
*/*                       GENERIC JUST LOCKED.                       */ 00060000
*/*                     - TO 1 FOR ALL OTHER REQS                    */ 00061000
*/*          - DODEMAND - SET TO 1 IF THERE ARE ANY DEMAND REQS      */ 00062000
*/*          - SPDEMAND - SET TO 1 IF THERE ARE ANY SPECIFIC         */ 00063000
*/*                       VOLUME DEMAND REQUESTS                     */ 00064000
*/*                                                                  */ 00065000
*/*   RETURN CODE - (NONE)                                           */ 00066000
*/*                                                                  */ 00067000
*/********************************************************************/ 00068000
*SIOTPTR = SIOT1P;                     /*POINT TO FIRST SIOT IN CHAIN*/ 00069000
         L     @10,@PC00001                                     02S0069 00070000
         L     @10,ALCWAPTR(,@10)                               02S0069 00071000
         L     SIOTPTR,SIOT1P(,@10)                             02S0069 00072000
*DO WHILE SIOTPTR ^= ZERO;             /*SCAN ALL SIOTS              */ 00073000
         B     @DE00070                                         02S0070 00074000
@DL00070 DS    0H                                               02S0071 00075000
*  SIOTGIGN = ON;                      /*ASSUME GENERIC NOT NEEDED   */ 00076000
         OI    SIOTGIGN(SIOTPTR),B'00001000'                    02S0071 00077000
*  IF SIOTALCD = OFF THEN              /*IS SIOT FULLY ALLOCATED ?   */ 00078000
         TM    SIOTALCD(SIOTPTR),B'00000010'                    02S0072 00079000
         BNZ   @RF00072                                         02S0072 00080000
*    DO;                               /*NO, SIOT IS NOT FULLY ALLOCD*/ 00081000
*      EDLPTR = EDLFGENP;              /*POINT TO 1ST GENERIC IN EDL */ 00082000
         L     @10,SIOTEDLP(,SIOTPTR)                           02S0074 00083000
         L     EDLPTR,EDLFGENP(,@10)                            02S0074 00084000
*        IF EINDEVTP = OFF             /*IS GENERIC FOUND ELIGIBLE.. */ 00085000
*            & EDLDEVT = LGENLOCK                               02S0075 00086000
*            & SIOTDMND = ON THEN      /*FOR A DEMAND REQUEST ?      */ 00087000
         TM    EINDEVTP(EDLPTR),B'10000000'                     02S0075 00088000
         BNZ   @RF00075                                         02S0075 00089000
         L     @10,@PC00001                                     02S0075 00090000
         L     @10,ALCWAPTR(,@10)                               02S0075 00091000
         CLC   EDLDEVT(4,EDLPTR),LGENLOCK(@10)                  02S0075 00092000
         BNE   @RF00075                                         02S0075 00093000
         TM    SIOTDMND(SIOTPTR),B'10000000'                    02S0075 00094000
         BNO   @RF00075                                         02S0075 00095000
*          DO;                         /*YES,..                      */ 00096000
*            SIOTGIGN = OFF;           /*TURN OFF IGNORE INDICATOR   */ 00097000
         NI    SIOTGIGN(SIOTPTR),B'11110111'                    02S0077 00098000
*            DODEMAND = ON;            /*TURN ON DEMAND SWITCH       */ 00099000
         OI    DODEMAND,B'10000000'                             02S0078 00100000
*            IF (SVOLUNAD->VOLSPEC=ON  /*SPECIFIC VOLUME REQ?        */ 00101000
*              & EDLDEVT(3)=DATYPE)                             02S0079 00102000
*              | (EDLDEVT (3)=TAPETYPE) THEN /*OR TAPE REQ ?         */ 00103000
         L     @10,SVOLUNAD(,SIOTPTR)                           02S0079 00104000
         TM    VOLSPEC(@10),B'00100000'                         02S0079 00105000
         BNO   @GL00003                                         02S0079 00106000
         CLI   EDLDEVT+2(EDLPTR),X'20'                          02S0079 00107000
         BE    @RT00079                                         02S0079 00108000
@GL00003 CLI   EDLDEVT+2(EDLPTR),X'80'                          02S0079 00109000
         BNE   @RF00079                                         02S0079 00110000
@RT00079 DS    0H                                               02S0080 00111000
*               SPDEMAND = ON;         /*YES, INDICATE TO IEFAB479   */ 00112000
         L     @10,@PC00001+8                                   02S0080 00113000
         OI    SPDEMAND(@10),B'00100000'                        02S0080 00114000
*          END;                                                 02S0081 00115000
*        ELSE;                         /*GEN NOT FOUND FOR DEMAND REQ*/ 00116000
@RF00075 DS    0H                                               02S0083 00117000
*    END;                                                       02S0083 00118000
*  ELSE;                               /*SIOT IS FULLY ALLOCATED     */ 00119000
@RF00072 DS    0H                                               02S0085 00120000
*  SIOTPTR = SIOTNPTR;                 /*POINT TO NEXT SIOT IN CHAIN */ 00121000
@RC00072 L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       02S0085 00122000
*END;                                                           02S0086 00123000
@DE00070 LTR   SIOTPTR,SIOTPTR                                  02S0086 00124000
         BNZ   @DL00070                                         02S0086 00125000
*IF DODEMAND = ON THEN                 /*ARE THERE ANY DEMAND REQS ? */ 00126000
         TM    DODEMAND,B'10000000'                             01S0087 00127000
         BNO   @RF00087                                         01S0087 00128000
*  DO;                                 /*YES, PROCESS                */ 00129000
*    CALL IEFAB479(ALCWAPTR,FLAGBITS); /*ALLOCATE DEMAND REQUESTS    */ 00130000
         L     @10,@PC00001                                     01S0089 00131000
         ST    @10,@AL00001                                     01S0089 00132000
         L     @10,@PC00001+8                                   01S0089 00133000
         ST    @10,@AL00001+4                                   01S0089 00134000
         L     @15,@CV00119                                     01S0089 00135000
         LA    @01,@AL00001                                     01S0089 00136000
         BALR  @14,@15                                          01S0089 00137000
*    RTNCODE=REG15;                    /*SAVE THE RETURN CODES       */ 00138000
         LR    RTNCODE,REG15                                    01S0090 00139000
*  END;                                                         01S0091 00140000
*ELSE;                                 /*NO DEMAND REQUESTS          */ 00141000
@RF00087 DS    0H                                               01S0093 00142000
*/********************************************************************/ 00143000
*/*                                                                  */ 00144000
*/* SEGMENT NAME - FLAGIGEN                                          */ 00145000
*/*                                                                  */ 00146000
*/* DESCRIPTIVE NAME - FLAG REQS NOT ELIGIBLE TO THIS GENERIC        */ 00147000
*/*                                                                  */ 00148000
*/* FUNCTION -                                                       */ 00149000
*/*     THIS ROUTINE MARKS THOSE UNALLOCATED SIOTS WHICH ARE         */ 00150000
*/*     NOT ELIGIBLE FOR THIS GENERIC DEVICE TYPE BEING PROCESSED    */ 00151000
*/*     SO THAT NO ATTEMPTS WILL BE MADE TO ALLOCATE THESE REQUESTS  */ 00152000
*/*     AT THIS TIME.                                                */ 00153000
*/*     THE UNALLOCATED REQUESTS WHICH ARE ELIGIBLE TO THIS GENERIC  */ 00154000
*/*     ARE CATEGORIZED TO DETERMINE WHICH OF THE FOLLOWING          */ 00155000
*/*     ALLOCATION PROCESSES ARE NECESSARY:                          */ 00156000
*/*         (1) ALLOCATION OF SPECIFIC VOLUME REQS TO MOUNTED        */ 00157000
*/*             VOLUMES (NOTE: THE NUMBER OF THESE REQUESTS ARE      */ 00158000
*/*             ALSO DETERMINED).                                    */ 00159000
*/*         (2) ALLOCATION OF SCRATCH VOLUME REQS TO MOUNTED VOLUMES */ 00160000
*/*         (3) ALLOCATION OF ANY REMAINING REQS VIA AN ALGORITHM    */ 00161000
*/*             IF ANY TAPE OR DA REQUESTS REMAIN AND        @ZA03536*/ 00162000
*/*             VOLUME MOUNTING IS NOT ALLOWED, THE REQUEST  @ZA03536*/ 00163000
*/*             IS FAILED.                                   @ZA03536*/ 00164000
*/*                                                                  */ 00165000
*/*   OPERATION - THE OPERATION IS DOCUMENTED BY THE CODE ITSELF.    */ 00166000
*/*                                                                  */ 00167000
*/* ENTRY - (INLINE SUBROUTINE)                                      */ 00168000
*/*                                                                  */ 00169000
*/*   PURPOSE - (SEE FUNCTION)                                       */ 00170000
*/*                                                                  */ 00171000
*/*   INPUT - SIOT1P - PTR TO FIRST SIOT IN THE CHAIN                */ 00172000
*/*         - ALCWAPTR - PTR TO ALLOCATION WORK AREA (ALCWA)         */ 00173000
*/*         - LGENLOCK - ID OF GENERIC BEING PROCESSED               */ 00174000
*/*         - DOTYPES - SET OF SWITCHES                              */ 00175000
*/*                                                                  */ 00176000
*/* EXIT - NORMAL - (INLINE SUBROUTINE)                              */ 00177000
*/*                                                                  */ 00178000
*/*   CONDITION - FUNCTION COMPLETED                                 */ 00179000
*/*                                                                  */ 00180000
*/*   OUTPUT - SIOTGIGN - SIOT-INELIGIBLE-FOR-GENERIC FLAG           */ 00181000
*/*            DOTYPES                                               */ 00182000
*/*              DOSPMNT - SET TO 1 IF THERE ARE ELIGIBLE SPEC VOL RQ*/ 00183000
*/*              DOSCRMNT - SET TO 1 IF ELIGIBLE SCRATCH VOLUME REQ  */ 00184000
*/*              DOALG - SET TO 1 IF ALGORITHM NEEDED                */ 00185000
*/*              SPDOALG - LEFT AS 0                                 */ 00186000
*/*                                                                  */ 00187000
*/*   RETURN CODE - (NONE)                                           */ 00188000
*/*                                                                  */ 00189000
*/* EXIT - ERROR - (INLINE SUBROUTINE)                       @ZA03536*/ 00190000
*/*                                                          @ZA03536*/ 00191000
*/*   CONDITION - ALGORITHM PROCESSING NEEDED FOR TAPE OR    @ZA03536*/ 00192000
*/*               DA BUT VOLUME MOUNTING NOT ALLOWED.        @ZA03536*/ 00193000
*/*                                                          @ZA03536*/ 00194000
*/*   OUTPUT - SIOTRSNC SET TO 56 (UNABLE TO MOUNT)          @ZA03536*/ 00195000
*/*                                                          @ZA03536*/ 00196000
*/*   RETURN CODE - RTNCODE SET TO 4                         @ZA03536*/ 00197000
*/*                                                                  */ 00198000
*/********************************************************************/ 00199000
*SIOTPTR = SIOT1P;                     /*POINT TO FIRST SIOT         */ 00200000
         L     @10,@PC00001                                     02S0093 00201000
         L     @10,ALCWAPTR(,@10)                               02S0093 00202000
         L     SIOTPTR,SIOT1P(,@10)                             02S0093 00203000
*DO WHILE (SIOTPTR ^= ZERO             /*SCAN ALL SIOTS      @ZA03536*/ 00204000
*          & RTNCODE = SUCCESS);       /*UNTIL FAILURE       @ZA03536*/ 00205000
         B     @DE00094                                         02S0094 00206000
@DL00094 DS    0H                                               02S0095 00207000
*  IF SIOTALCD = NO THEN               /*IS SIOT FULLY ALLOCATED ?   */ 00208000
         TM    SIOTALCD(SIOTPTR),B'00000010'                    02S0095 00209000
         BNZ   @RF00095                                         02S0095 00210000
*    DO;                               /*NO SIOT IS NOT ALLOCATED    */ 00211000
*      SIOTGIGN = ON;                  /*ASSUME GENERIC NOT ELIGIBLE */ 00212000
         OI    SIOTGIGN(SIOTPTR),B'00001000'                    02S0097 00213000
*      EDLPTR = EDLFGENP;              /*POINT TO 1ST GENERIC IN EDL */ 00214000
         L     @10,SIOTEDLP(,SIOTPTR)                           02S0098 00215000
         L     EDLPTR,EDLFGENP(,@10)                            02S0098 00216000
*      DO  GENSCTR = ONE TO EDLGENNO   /*SCAN ALL GENERIC ENTRIES... */ 00217000
*           WHILE (SIOTGIGN = ON       /*UNTIL GENERIC FOUND @ZA03536*/ 00218000
*                 & RTNCODE = SUCCESS);/*OR UNTIL FAILURE    @ZA03536*/ 00219000
         LA    GENSCTR,1                                        02S0099 00220000
         B     @DE00099                                         02S0099 00221000
@DL00099 TM    SIOTGIGN(SIOTPTR),B'00001000'                    02S0099 00222000
         BNO   @DC00099                                         02S0099 00223000
         LTR   RTNCODE,RTNCODE                                  02S0099 00224000
         BNZ   @DC00099                                         02S0099 00225000
*        IF EINDEVTP = OFF & EDLDEVT=LGENLOCK THEN /*IS GEN ELIGIBLE?*/ 00226000
         TM    EINDEVTP(EDLPTR),B'10000000'                     02S0100 00227000
         BNZ   @RF00100                                         02S0100 00228000
         L     @10,@PC00001                                     02S0100 00229000
         L     @10,ALCWAPTR(,@10)                               02S0100 00230000
         CLC   EDLDEVT(4,EDLPTR),LGENLOCK(@10)                  02S0100 00231000
         BNE   @RF00100                                         02S0100 00232000
*          DO;                         /*YES THIS GENERIC IS ELIGIBLE*/ 00233000
*            SIOTGIGN = OFF;           /*TURN OFF IGNORE INDICATOR   */ 00234000
         NI    SIOTGIGN(SIOTPTR),B'11110111'                    02S0102 00235000
*            IF   SIOTDMND = ON THEN;  /*IGNORE A DEMAND REQ         */ 00236000
         TM    SIOTDMND(SIOTPTR),B'10000000'                    02S0103 00237000
         BO    @RT00103                                         02S0103 00238000
*            ELSE                      /*REQ IS NOT A DEMAND REQ     */ 00239000
*              DO;                                              02S0105 00240000
*                VUPTR = SVOLUNAD;     /*POINT TO FIRST VOLUNIT ENTRY*/ 00241000
         L     VUPTR,SVOLUNAD(,SIOTPTR)                         02S0106 00242000
*                DO VUCTR = ONE TO SVOLUNNO /*SCAN ALL VU ENT@ZA03536*/ 00243000
*                   WHILE (RTNCODE = SUCCESS);/*UNTIL FAILURE@ZA03536*/ 00244000
         LA    VUCTR,1                                          02S0107 00245000
         B     @DE00107                                         02S0107 00246000
@DL00107 LTR   RTNCODE,RTNCODE                                  02S0107 00247000
         BNZ   @DC00107                                         02S0107 00248000
*                  IF VOLALOC=OFF THEN /*IS VOLUNIT ENTRY ALLOCATED? */ 00249000
         TM    VOLALOC(VUPTR),B'10000000'                       02S0108 00250000
         BNZ   @RF00108                                         02S0108 00251000
*                   DO;                /*NO,VU ENTRY IS NOT ALLOCATED*/ 00252000
*                     IF (EDLDEVT(3)=DATYPE  /*IS GENERIC D.A. ?     */ 00253000
*                        |   EDLDEVT(3)=TAPETYPE) /*OR TAPE ? AND    */ 00254000
*                        & ((VOLNSHR=OFF)                       02S0110 00255000
*                        |   (VOLNSHR=ON &VOLTAREQ=ON))         02S0110 00256000
*                        & VOLPRV=OFF                           02S0110 00257000
*                        & VOLSPEC=OFF                          02S0110 00258000
*                        & VOLPUB=ON THEN /*IS THIS A SCRATCH REQ ?  */ 00259000
         CLI   EDLDEVT+2(EDLPTR),X'20'                          02S0110 00260000
         BE    @GL00008                                         02S0110 00261000
         CLI   EDLDEVT+2(EDLPTR),X'80'                          02S0110 00262000
         BNE   @RF00110                                         02S0110 00263000
@GL00008 TM    VOLNSHR(VUPTR),B'00001000'                       02S0110 00264000
         BZ    @GL00007                                         02S0110 00265000
         TM    VOLNSHR(VUPTR),B'00001000'                       02S0110 00266000
         BNO   @RF00110                                         02S0110 00267000
         TM    VOLTAREQ(VUPTR),B'10000000'                      02S0110 00268000
         BNO   @RF00110                                         02S0110 00269000
@GL00007 TM    VOLPUB(VUPTR),B'10000000'                        02S0110 00270000
         BNO   @RF00110                                         02S0110 00271000
         TM    VOLPRV(VUPTR),B'01100000'                        02S0110 00272000
         BNZ   @RF00110                                         02S0110 00273000
*                      DO;             /*YES,THIS IS A SCRATCH REQ   */ 00274000
*                        DOSCRMNT =ON; /*TURN ON SCR/MNT SWITCH      */ 00275000
         OI    DOSCRMNT,B'00001000'                             02S0112 00276000
*                        IF EDLDEVT(3)=TAPETYPE THEN /*TAPE REQ ?    */ 00277000
         CLI   EDLDEVT+2(EDLPTR),X'80'                          02S0113 00278000
         BNE   @RF00113                                         02S0113 00279000
*                          DO;         /*YES, MAY ALLOC LIKE SPECIFIC*/ 00280000
*                            DOSPMNT = ON;                      02S0115 00281000
         OI    DOSPMNT,B'01000000'                              02S0115 00282000
*                          END;                                 02S0116 00283000
*                        ELSE;         /*NOT TAPE REQ                */ 00284000
@RF00113 DS    0H                                               02S0118 00285000
*                      END;                                     02S0118 00286000
*                    ELSE              /*NOT A SCRATCH REQ           */ 00287000
*                      DO;                                      02S0119 00288000
         B     @RC00110                                         02S0119 00289000
@RF00110 DS    0H                                               02S0120 00290000
*                        IF CVRIGNOR=OFF                        02S0120 00291000
*                           &CVRDUPLI=OFF THEN/*SHOULD REQ BE PROC'D?*/ 00292000
         L     @10,VOLALGTP(,VUPTR)                             02S0120 00293000
         TM    CVRIGNOR(@10),B'00000010'                        02S0120 00294000
         BNZ   @RF00120                                         02S0120 00295000
         TM    CVRDUPLI(@10),B'10000000'                        02S0120 00296000
         BNZ   @RF00120                                         02S0120 00297000
*                          DO;         /*YES...                      */ 00298000
*                            CVRSKFLG = OFF; /*TURN OFF SKIP SWITCH  */ 00299000
         NI    CVRSKFLG(@10),B'01111111'                        02S0122 00300000
*                          END;                                 02S0123 00301000
*                        ELSE;         /*REQ NOT TO BE PROCESSED     */ 00302000
@RF00120 DS    0H                                               02S0125 00303000
*                        IF EDLDEVT(3)=TAPETYPE /*IS GENERIC TAPE ?     00304000
*                                                            @YM03490*/ 00305000
*                           | (EDLDEVT(3)=DATYPE /*OR D.A. AND A     */ 00306000
*                           & VOLSPEC=ON) THEN /*SPEC VOLUME REQUEST?*/ 00307000
         CLI   EDLDEVT+2(EDLPTR),X'80'                          02S0125 00308000
         BE    @RT00125                                         02S0125 00309000
         CLI   EDLDEVT+2(EDLPTR),X'20'                          02S0125 00310000
         BNE   @RF00125                                         02S0125 00311000
         TM    VOLSPEC(VUPTR),B'00100000'                       02S0125 00312000
         BNO   @RF00125                                         02S0125 00313000
@RT00125 DS    0H                                               02S0126 00314000
*                          DO;         /*YES...                      */ 00315000
*                            DOSPMNT=YES; /*TURN ON SPEC/MNT SWITCH  */ 00316000
         OI    DOSPMNT,B'01000000'                              02S0127 00317000
*                          END;                                 02S0128 00318000
*                        ELSE          /*NOT A SPECIFIC VOL REQUEST  */ 00319000
*                          DO;                                  02S0129 00320000
         B     @RC00125                                         02S0129 00321000
@RF00125 DS    0H                                               02S0130 00322000
*                          IF CVRSKFLG = OFF THEN /*REG NOT  @ZA03536   00323000
*                                        MARK SKIP           @ZA03536*/ 00324000
         L     @10,VOLALGTP(,VUPTR)                             02S0130 00325000
         TM    CVRSKFLG(@10),B'10000000'                        02S0130 00326000
         BNZ   @RF00130                                         02S0130 00327000
*                            IF (EDLDEVT(3) ^= DATYPE/*NOT DA@ZA03536*/ 00328000
*                               &EDLDEVT(3) ^= TAPETYPE) /*     02S0131 00329000
*                                        NOT TAPE            @ZA03536*/ 00330000
*                               | VOLMNTSW = ON THEN /*VOL      02S0131 00331000
*                                        MOUNTING ALLOWED    @ZA03536*/ 00332000
         CLI   EDLDEVT+2(EDLPTR),X'20'                          02S0131 00333000
         BE    @GL00015                                         02S0131 00334000
         CLI   EDLDEVT+2(EDLPTR),X'80'                          02S0131 00335000
         BNE   @RT00131                                         02S0131 00336000
@GL00015 L     @10,@PC00001                                     02S0131 00337000
         L     @10,ALCWAPTR(,@10)                               02S0131 00338000
         L     @10,FMAPPTR(,@10)                                02S0131 00339000
         TM    VOLMNTSW(@10),B'10000000'                        02S0131 00340000
         BNO   @RF00131                                         02S0131 00341000
@RT00131 DS    0H                                               02S0132 00342000
*                              DOALG = ON;/*ALGORITHM NEEDED @ZA03536*/ 00343000
         OI    DOALG,B'00010000'                                02S0132 00344000
*                            ELSE      /*TAPE OR DA AND         02S0133 00345000
*                                        MOUNTING NOT ALLOWED@ZA03536*/ 00346000
*                              DO;     /*FAIL THE REQUEST    @ZA03536*/ 00347000
         B     @RC00131                                         02S0133 00348000
@RF00131 DS    0H                                               02S0134 00349000
*                                RTNCODE = FAILURE;/*FAIL IT @ZA03536*/ 00350000
         LA    RTNCODE,4                                        02S0134 00351000
*                                SIOTRSNC = NOMOUNT;/*CANT MOUNT        00352000
*                                                            @ZA03536*/ 00353000
         MVC   SIOTRSNC(2,SIOTPTR),@CH00103                     02S0135 00354000
*                              END;    /*END FAIL THE REQ    @ZA03536*/ 00355000
*                          END;                                 02S0137 00356000
*                      END;                                     02S0138 00357000
*                   END;                                        02S0139 00358000
*                  ELSE;               /*ENTRY IS ALLOCATED          */ 00359000
@RF00108 DS    0H                                               02S0141 00360000
*                  VUPTR=VUPTR+LENGTH(VOLUNTAB);                02S0141 00361000
@RC00108 AH    VUPTR,@CH00979                                   02S0141 00362000
*                END;                                           02S0142 00363000
         AH    VUCTR,@CH00040                                   02S0142 00364000
@DE00107 CH    VUCTR,SVOLUNNO(,SIOTPTR)                         02S0142 00365000
         BNH   @DL00107                                         02S0142 00366000
@DC00107 DS    0H                                               02S0143 00367000
*              END;                                             02S0143 00368000
*          END;                                                 02S0144 00369000
*        ELSE;                         /*THIS GENERIC NOT ELIGIBLE   */ 00370000
@RF00100 DS    0H                                               02S0146 00371000
*        EDLPTR = EDLNXTNT;            /*POINT TO NEXT GENERIC IN EDL*/ 00372000
@RC00100 L     EDLPTR,EDLNXTNT(,EDLPTR)                         02S0146 00373000
*      END;                                                     02S0147 00374000
