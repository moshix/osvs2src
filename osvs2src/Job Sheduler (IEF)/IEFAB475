         BNZ   @DL00290                                         02S0306 01105000
*RETURN CODE(RTNCODE);                 /*RETURN TO CALLER            */ 01106000
         L     @13,4(,@13)                                      01S0307 01107000
         L     @00,@SIZDATD                                     01S0307 01108000
         LR    @01,@11                                          01S0307 01109000
         FSPACE   R,LV=(0),A=(1)                                        01110000
         LR    @15,@06                                          01S0307 01111000
         L     @14,12(,@13)                                     01S0307 01112000
         LM    @00,@12,20(@13)                                  01S0307 01113000
         BR    @14                                              01S0307 01114000
*TESTBIT: PROC;                                                 02S0308 01115000
TESTBIT  STM   @14,@12,@SA00002                                 02S0308 01116000
*/********************************************************************/ 01117000
*/*                                                                  */ 01118000
*/* SEGMENT NAME - TESTBIT                                           */ 01119000
*/*                                                                  */ 01120000
*/* DESCRIPTIVE NAME - TEST BIT IN GROUP MASK                        */ 01121000
*/*                                                                  */ 01122000
*/* FUNCTION -                                                       */ 01123000
*/*      THIS SUBROUTINE DETERMINES IF A PARTICULAR BIT IN A         */ 01124000
*/*      GROUP MASK IS ON OR OFF. THE GROUP MASK IS A VARIABLE       */ 01125000
*/*      LENGTH BIT MASK POINTED TO BY MASKPTR . THE ONE-ORIGIN      */ 01126000
*/*      INDEX OF THE BIT TO BE TESTED IS CONTAINED IN FIELD         */ 01127000
*/*      GROUPNO.                                                    */ 01128000
*/*                                                                  */ 01129000
*/*   OPERATION -                                                    */ 01130000
*/*      THIS SEGMENT GENERATES BAL INSTRUCTIONS                     */ 01131000
*/*                                                                  */ 01132000
*/*      ONE IS SUBTRACTED FROM THE GROUPNO TO PRODUCE A ZERO-ORIGIN */ 01133000
*/*      INDEX. THE INDEX IS THEN DIVIDED BY EIGHT TO DETERMINE THE  */ 01134000
*/*      BYTE (AND REMAINDER)  OF THE BIT BEING TESTED (IN A SRDL    */ 01135000
*/*      INSTRUCTION, THE ODD REGISTER GETS THE BITS SHIFTED OUT OF  */ 01136000
*/*      THE EVEN REGISTER). THE REMAINDER IS ALIGNED IN A REGISTER  */ 01137000
*/*      AND IS USED TO INDICATE THE BIT POSITION IN THE BYTE. THIS  */ 01138000
*/*      BIT POSITION IS USED IN A TEST-UNDER-MASK INSTRUCTION VIA   */ 01139000
*/*      EXECUTE.                                                    */ 01140000
*/*                                                                  */ 01141000
*/*      EXAMPLE - IF GROUPNO = 10, THIS OPERATION WILL DETERMINE    */ 01142000
*/*                THE BIT IS 1 BYTE AFTER THE BEGINNING OF THE      */ 01143000
*/*                GROUP MASK AND IS THE SECOND BIT POSITION.        */ 01144000
*/*                                                                  */ 01145000
*/* ENTRY - THIS CODE IS AN INLINE SUBROUTINE WHICH IS ENTERED       */ 01146000
*/*         AT THE TOP.                                              */ 01147000
*/*                                                                  */ 01148000
*/*   PURPOSE  -  TO DETERMINE IF A PARTICULAR BIT IN A GROUP        */ 01149000
*/*               MASK IS ON OR OFF.                                 */ 01150000
*/*                                                                  */ 01151000
*/*   INPUT -                                                        */ 01152000
*/*       MASKPTR - PTR TO GROUP MASK                                */ 01153000
*/*       GROUPNO - 1-ORIGIN INDEX INTO MASK OF THE BIT TO BE TESTED */ 01154000
*/*       BITRTN - BIT FOR RESULTS OF TEST TO BE RETURNED IN         */ 01155000
*/*                                                                  */ 01156000
*/* EXIT - NORMAL - THIS CODE IS AN INTERNAL PROCEDURE WHICH         */ 01157000
*/*                 EXITS FROM THE BOTTOM.                           */ 01158000
*/*                                                                  */ 01159000
*/*   CONDITION - BIT TESTED AND RESULTS INDICATED.                  */ 01160000
*/*                                                                  */ 01161000
*/*   OUTPUT -                                                       */ 01162000
*/*       BITRTN - SET TO 1 IF BIT WAS ON                            */ 01163000
*/*              - SET TO 0 IF BIT WAS OFF                           */ 01164000
*/*                                                                  */ 01165000
*/*   RETURN CODES - (NONE)                                          */ 01166000
*/*                                                                  */ 01167000
*/********************************************************************/ 01168000
*BITRTN = OFFVALUE;                    /*ASSUME BIT IS OFF           */ 01169000
         NI    BITRTN,B'01111111'                               02S0309 01170000
*RFY (EVENREG,ODDREG,MASKREG) RSTD;    /*RESTRICT USE OF REGS 3,4+5  */ 01171000
*GEN REFS(EVENREG,GROUPNO,MASKPTR,ODDREG,MASKREG,TMINST)        02S0311 01172000
*    FLOWS(BITOFF);                                             02S0311 01173000
        L     EVENREG,GROUPNO         PUT BIT INDEX INTO REG            01174000
        BCTR  EVENREG,0               DECREMENT INDEX FOR ZERO ORIGIN   01175000
        SRDL  EVENREG,3               DIVIDE INDEX BY 8.SAVE REMAINDER  01176000
        A     EVENREG,MASKPTR         POINT TO CORRECT BYTE             01177000
        SRL   ODDREG,29               ALIGN REMAINDER IN REG            01178000
        LA    MASKREG,X'80'           INSERT 0-BIT MASK                 01179000
        SRL   MASKREG,0(ODDREG)       POSITION MASK BIT                 01180000
        EX    MASKREG,TMINST          TEST IF BIT IS ON                 01181000
        BNO   BITOFF                  BRANCH IF BIT NOT ON              01182000
*RFY (EVENREG,ODDREG,MASKREG) UNRSTD;  /*UNRESTRICT REGS 3,4, AND 5  */ 01183000
*BITRTN = ONVALUE;                     /*INDICATE BIT WAS ON         */ 01184000
         OI    BITRTN,B'10000000'                               02S0313 01185000
*BITOFF:;                              /*BITOFF                      */ 01186000
*RETURN;                               /*RETURN TO CALLER            */ 01187000
@EL00002 DS    0H                                               02S0315 01188000
@EF00002 DS    0H                                               02S0315 01189000
@ER00002 LM    @14,@12,@SA00002                                 02S0315 01190000
         BR    @14                                              02S0315 01191000
*END TESTBIT;                                                   02S0316 01192000
         B     @EL00002                                         02S0316 01193000
*END IEFAB475                          /*END ALLOC-WITHIN-A-GENERIC  */ 01194000
