         BNZ   @RF00038                                         02S0038 00141000
         ICM   @14,3,CVRALLCT(AREQPTR)                          02S0038 00142000
         BNP   @RF00038                                         02S0038 00143000
*    DO;                               /*YES, DETERMINE IF REARRANGE */ 00144000
*      GRPFOUND = OFF;                 /*GROUP TO UNALLOC NOT FOUND  */ 00145000
         NI    GRPFOUND,B'01111111'                             02S0040 00146000
*      AGRPPTR = CVRGRPPT;             /*POINT TO FIRST GROUP LIST NT*/ 00147000
         L     AGRPPTR,CVRGRPPT(,AREQPTR)                       02S0041 00148000
*      DO AGRPCTR = ONE TO CVRNGRP     /*SCAN ALL GROUP LIST ENTRIES */ 00149000
*          WHILE GRPFOUND = OFF;                                02S0042 00150000
         LA    AGRPCTR,1                                        02S0042 00151000
         B     @DE00042                                         02S0042 00152000
@DL00042 TM    GRPFOUND,B'10000000'                             02S0042 00153000
         BNZ   @DC00042                                         02S0042 00154000
*        IF CVRGRPAL > ZERO            /*MORE ALLOC'D THAN ALG COVER */ 00155000
*           & CVRGALL < CVRGRPAL THEN                           02S0043 00156000
