         ST    @12,DEVNO                                        02S0186 00516000
*  SCANVUP = SPTR->SVOLUNAD;           /*POINT TO FIRST VU FOR SIOT  */ 00517000
         MVC   SCANVUP(4),SVOLUNAD(SPTR)                        02S0187 00518000
*  DO SCAVUCTR = ONE TO SPTR->SVOLUNNO /*SCAN ALL THE VOLUNIT ENTRIES*/ 00519000
*    WHILE RTNCODE = SUCCESS;                                   02S0188 00520000
         LA    @12,1                                            02S0188 00521000
