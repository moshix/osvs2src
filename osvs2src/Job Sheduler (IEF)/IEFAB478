*    ELSE;                             /*ERROR BUILDING ALC UCB LIST */ 01380000
@RF00306 DS    0H                                               03S0441 01381000
*  END;                                                         03S0441 01382000
*ELSE;                                 /*ONLY ONE UCB CANDIDATE      */ 01383000
@RF00302 DS    0H                                               03S0443 01384000
* DO; /*MODESET  EXTKEY(SCHED)*/                                03S0443 01385000
*   SPKA (16); /*SET PSW KEY*/                                  03S0444 01386000
         LA    @08,16                                           03S0444 01387000
         SPKA  0(@08)                                           03S0444 01388000
* END; /*MODESET  EXTKEY(SCHED)*/                               03S0445 01389000
*IF RTNCODE = SUCCESS &                /*NO ERRORS AND       @ZA13405*/ 01390000
*   (MSSDEV = OFF |                    /*MSS UNITS OR        @ZA13405*/ 01391000
*    MSSMATCH = ON |                   /*A MATCH FOUND OR    @ZA13405*/ 01392000
*    MSSRECUR = OFF) THEN              /*ALL GROUPS PROCESSED@ZA13405*/ 01393000
         LTR   RTNCODE,RTNCODE                                  03S0446 01394000
         BNZ   @RF00446                                         03S0446 01395000
         TM    MSSMATCH,B'00100000'                             03S0446 01396000
         BNZ   @RT00446                                         03S0446 01397000
         TM    MSSDEV,B'00011000'                               03S0446 01398000
         BO    @RF00446                                         03S0446 01399000
@RT00446 DS    0H                                               03S0447 01400000
*  DO;                                 /*NO, CONTINUE PROCESSING     */ 01401000
*    UCBPTR = UCBCAND(SLOTNO);         /*POINT TO UCB TO BE ALLOCATED*/ 01402000
         LR    @08,SLOTNO                                       03S0448 01403000
         SLA   @08,2                                            03S0448 01404000
         L     @01,UCBCLSTP(,OPTPARMP)                          03S0448 01405000
         L     @08,UCBCAND-4(@08,@01)                           03S0448 01406000
         ST    @08,UCBPTR                                       03S0448 01407000
*/********************************************************************/ 01408000
*/*                                                                  */ 01409000
*/* SEGMENT NAME - GARUPARM                                          */ 01410000
*/*                                                                  */ 01411000
*/* DESCRIPTIVE NAME - SET UP THE INPUT PARAMETERS TO THE ARU ROUTINE*/ 01412000
*/*                                                                  */ 01413000
*/* FUNCTION -                                                       */ 01414000
*/*     THIS ROUTINE IS CALLED TO COMPLETE THE INFORMATION NEEDED    */ 01415000
*/*     TO INVOKE THE ALLOCATE-REQUEST-TO-UNIT ROUTINE (IEFAB434).   */ 01416000
*/*                                                                  */ 01417000
*/*     A POINTER TO A RETURN INFORMATION AREA IS SET UP AND THE     */ 01418000
*/*     FUNCTION MAP FOR THE ARU ROUTINE IS ALSO SET UP.             */ 01419000
*/*                                                                  */ 01420000
*/*   OPERATION - THE OPERATION IS DOCUMENTED BY THE CODE ITSELF.    */ 01421000
*/*                                                                  */ 01422000
*/* ENTRY - (INLINE SUBROUTINE)                                      */ 01423000
*/*                                                                  */ 01424000
*/*   PURPOSE - (SEE FUNCTION)                                       */ 01425000
*/*                                                                  */ 01426000
*/*   INPUT - SIOTPTR - PTR TO THE SIOT BEING PROCESSED              */ 01427000
*/*         - VUPTR   - PTR TO THE VOLUNIT BEING PROCESSED           */ 01428000
*/*         - UCBPTR  - PTR TO THE UNIT CONTROL BLOCK TO BE ALLOC'D  */ 01429000
*/*         - VOLSW   - VOLUME SWITCHING PERMISSION BIT              */ 01430000
*/*                                                                  */ 01431000
*/* EXIT - NORMAL - (INLINE SUBROUTINE)                              */ 01432000
*/*                                                                  */ 01433000
*/*   CONDITION - THE FUNCTION HAS COMPLETED WITHOUT AN ERROR        */ 01434000
*/*                                                                  */ 01435000
*/*   OUTPUT - UCBPARMP - PTR TO RETURN INFO AREA FOR IEFAB434       */ 01436000
*/*          - UVOLID   - VOLID FROM THE VOLUNIT ENTRY               */ 01437000
*/*          - ARUFMAP  - IEFAB434 FUNCTION MAP                      */ 01438000
*/*          - SIOTDEVS - NUMBER OF UNITS NEEDED FOR ETIOT ENTRY     */ 01439000
*/*                                                                  */ 01440000
*/*   RETURN CODES - (NONE)                                          */ 01441000
*/*                                                                  */ 01442000
*/********************************************************************/ 01443000
*UCBPARMP = ADDR(UCBPARMS);            /*POINT TO VOLUME INFO AREA   */ 01444000
         LA    @15,UCBPARMS                                     04S0449 01445000
         ST    @15,UCBPARMP                                     04S0449 01446000
*UVOLID = VOLID;                       /*PASS VOLID FROM VU ENTRY    */ 01447000
         L     @01,VUPTR                                        04S0450 01448000
         MVC   UVOLID(6),VOLID(@01)                             04S0450 01449000
*ARUFMAP = INITFMAP;                   /*COPY STANDARD FUNCTION MAP  */ 01450000
         MVC   ARUFMAP(4),INITFMAP                              04S0451 01451000
*IF SIOTETIO = ZERO THEN               /*DOES AN ETIOT ALREADY EXIST?*/ 01452000
         L     @15,SIOTPTR                                      04S0452 01453000
         ICM   @14,15,SIOTETIO(@15)                             04S0452 01454000
         BNZ   @RF00452                                         04S0452 01455000
*  DO;                                 /*NO, CHANGE THE FUNCTION MAP */ 01456000
*    ARUETUPD = OFF;                   /*INDICATE ETIOT NEEDED       */ 01457000
         NI    ARUETUPD,B'01111111'                             04S0454 01458000
*    DEVSSIOT = SIOTOTUN;              /*SET NUMBER OF UNITS NEEDED  */ 01459000
         SLR   @14,@14                                          04S0455 01460000
         IC    @14,SIOTOTUN(,@15)                               04S0455 01461000
         STH   @14,DEVSSIOT                                     04S0455 01462000
*  END;                                                         04S0456 01463000
*ELSE;                                 /*KEEP STANDARD FUNCTION MAP  */ 01464000
@RF00452 DS    0H                                               04S0458 01465000
*IF UCB3DACC = ON THEN                 /*IS UNIT DIRECT-ACCESS ?     */ 01466000
         L     @01,UCBPTR                                       04S0458 01467000
         TM    UCB3DACC(@01),B'00100000'                        04S0458 01468000
         BNO   @RF00458                                         04S0458 01469000
* DO;                                  /*YES, CHANGE THE FUNCTION MAP*/ 01470000
*   ARUSECTU = ON;                     /*INDICATE UPDATE THE USE CNT */ 01471000
         OI    ARUSECTU,B'01000000'                             04S0460 01472000
* END;                                                          04S0461 01473000
*ELSE;                                 /*UNIT NOT DIRECT ACCESS      */ 01474000
@RF00458 DS    0H                                               04S0463 01475000
*IF VOLSW = ON THEN                    /*CAN VOLUME SWITCHING BE DONE*/ 01476000
         L     @15,@PC00001+4                                   04S0463 01477000
         TM    VOLSW(@15),B'01000000'                           04S0463 01478000
         BNO   @RF00463                                         04S0463 01479000
*  DO;                                 /*YES, CHANGE THE FUNCTION MAP*/ 01480000
*    ARUVOLSW = ON;                    /*INDICATE VOL SWITCHING IS OK*/ 01481000
         OI    ARUVOLSW,B'00000010'                             04S0465 01482000
*  END;                                                         04S0466 01483000
*ELSE                                  /*VOL SWITCHING NOT OK        */ 01484000
*IF TRTRYVOL = ON                                               04S0467 01485000
*  & (UCB3TAPE = ON                                             04S0467 01486000
*  | (UCB3DACC = ON                                             04S0467 01487000
*  & SIOTRTRY = ON)) THEN              /*RECOVERY VOL SW CONDITIONS ?*/ 01488000
         B     @RC00463                                         04S0467 01489000
@RF00463 L     @15,@PC00001+4                                   04S0467 01490000
         TM    TRTRYVOL(@15),B'00001000'                        04S0467 01491000
         BNO   @RF00467                                         04S0467 01492000
         L     @15,UCBPTR                                       04S0467 01493000
         TM    UCB3TAPE(@15),B'10000000'                        04S0467 01494000
         BO    @RT00467                                         04S0467 01495000
         TM    UCB3DACC(@15),B'00100000'                        04S0467 01496000
         BNO   @RF00467                                         04S0467 01497000
         L     @01,SIOTPTR                                      04S0467 01498000
         TM    SIOTRTRY(@01),B'00000001'                        04S0467 01499000
         BNO   @RF00467                                         04S0467 01500000
@RT00467 DS    0H                                               04S0468 01501000
*  DO;                                 /*YES, INDICATE VOL SW OK     */ 01502000
*    ARUVOLSW = ON;                                             04S0469 01503000
         OI    ARUVOLSW,B'00000010'                             04S0469 01504000
*  END;                                                         04S0470 01505000
*ELSE;                                 /*NO VOLUME SWITCHING         */ 01506000
@RF00467 DS    0H                                               04S0472 01507000
*IF (VOLSPEC = ON                      /*SPECIFIC VOL REQ ?          */ 01508000
*  & UCB3DACC = ON)                                             04S0472 01509000
*  | (UCB3TAPE = ON) THEN              /*OR TAPE REQ ?               */ 01510000
@RC00463 L     @01,VUPTR                                        04S0472 01511000
         TM    VOLSPEC(@01),B'00100000'                         04S0472 01512000
         BNO   @GL00040                                         04S0472 01513000
         L     @01,UCBPTR                                       04S0472 01514000
         TM    UCB3DACC(@01),B'00100000'                        04S0472 01515000
         BO    @RT00472                                         04S0472 01516000
@GL00040 L     @01,UCBPTR                                       04S0472 01517000
         TM    UCB3TAPE(@01),B'10000000'                        04S0472 01518000
         BNO   @RF00472                                         04S0472 01519000
@RT00472 DS    0H                                               04S0473 01520000
*  DO;                                 /*YES, 434 SHOULD VALIDITY CHK*/ 01521000
*    ARUVCHEK = ON;                                             04S0474 01522000
         OI    ARUVCHEK,B'00010000'                             04S0474 01523000
*  END;                                                         04S0475 01524000
*ELSE;                                 /*NOT A SPEC VOL REQ          */ 01525000
@RF00472 DS    0H                                               04S0477 01526000
*    CALL IEFAB434                     /*ALLOC REQUEST TO UNIT       */ 01527000
*             (SIOTPTR,VUPTR,UCBPTR,ARUFMAP,ETIODATA,           03S0477 01528000
*              ALCWAPTR,ARUGRPID,UCBPARMP);                     03S0477 01529000
         LA    @15,SIOTPTR                                      03S0477 01530000
         ST    @15,@AL00001                                     03S0477 01531000
         LA    @15,VUPTR                                        03S0477 01532000
         ST    @15,@AL00001+4                                   03S0477 01533000
         LA    @15,UCBPTR                                       03S0477 01534000
         ST    @15,@AL00001+8                                   03S0477 01535000
         LA    @15,ARUFMAP                                      03S0477 01536000
         ST    @15,@AL00001+12                                  03S0477 01537000
         LA    @15,ETIODATA                                     03S0477 01538000
         ST    @15,@AL00001+16                                  03S0477 01539000
         L     @15,@PC00001                                     03S0477 01540000
         ST    @15,@AL00001+20                                  03S0477 01541000
         LA    @15,ARUGRPID                                     03S0477 01542000
         ST    @15,@AL00001+24                                  03S0477 01543000
         LA    @15,UCBPARMP                                     03S0477 01544000
         ST    @15,@AL00001+28                                  03S0477 01545000
         L     @15,@CV00208                                     03S0477 01546000
         LA    @01,@AL00001                                     03S0477 01547000
         BALR  @14,@15                                          03S0477 01548000
*/********************************************************************/ 01549000
*/*                                                                  */ 01550000
*/* SEGMENT NAME - GARURTN                                           */ 01551000
*/*                                                                  */ 01552000
*/* DESCRIPTIVE NAME - PROCESS RETURN FROM ARU MODULE (IEFAB434)     */ 01553000
*/*                                                                  */ 01554000
*/* FUNCTION -                                                       */ 01555000
*/*     THIS ROUTINE PROCESSES THE RETURN FROM MODULE IEFAB434       */ 01556000
*/*     WHICH WAS INVOKED TO ALLOCATE A PARTICULAR VOLUNIT ENTRY     */ 01557000
*/*     TO A CHOSEN DEVICE.                                          */ 01558000
*/*                                                                  */ 01559000
*/*     THIS ROUTINE DOES THE FOLLOWING PROCESSING:                  */ 01560000
*/*                                                                  */ 01561000
*/*       .IF THE VOLUNIT ENTRY WAS SUCCESSFULLY ALLOCATED, THE      */ 01562000
*/*        VOLUNIT ENTRY IS MARKED ALLOCATED AND THE NUMBER OF       */ 01563000
*/*        STILL UNALLOCATED VOLUNIT ENTRIES IS DECREASED BY ONE.    */ 01564000
*/*        IF THE RETURN INFORMATION FROM IEFAB434 INDICATES A       */ 01565000
*/*        VOLUME MUST BE MOUNTED FOR THIS REQUEST, A VM+V REQUEST   */ 01566000
*/*        BLOCK IS BUILT TO DESCRIBE THE VOLUME MOUNT NEEDED.       */ 01567000
*/*        (NOTE: VM+V WILL BE REQUESTED TO HAVE THE OPERATOR MOUNT  */ 01568000
*/*        THIS VOLUME AFTER ALL THE SIOTS HAVE BEEN ALLOCATED       */ 01569000
*/*        - SEE MODULE IEFAB492 FOR DETAILS.)                       */ 01570000
*/*                                                                  */ 01571000
*/*       .IF A NON-FAILING 'ERROR' CONDITION WAS DETECTED BY        */ 01572000
*/*        MODULE IEFAB434, THEN INDICATORS OF THIS CONDITIONS ARE   */ 01573000
*/*        SET. THE POSSIBLE NON-FAILING ERRORS ARE AS FOLLOWS:      */ 01574000
*/*          (1)THE REQUESTED VOLUME IS LOCATED ON ANOTHER DEVICE,   */ 01575000
*/*             BUT THE VOLUME CANNOT BE MOVED TO THE DEVICE IT IS   */ 01576000
*/*             TO BE ALLOCATED ON BECAUSE VOLUME SWITCHING IS NOT   */ 01577000
*/*             ALLOWED. THE ALLOCATION PROCESSING WILL HAVE TO      */ 01578000
*/*             BE 'RETRIED' WITH THIS REQUEST LOCKING UP THE        */ 01579000
*/*             ENTIRE GENERIC DEVICE TYPE SO THAT VOLUME SWITCHING  */ 01580000
*/*             WILL BE ALLOWED.                                     */ 01581000
*/*                                                                  */ 01582000
*/*          (2)THE REQUEST COULD NOT BE ALLOCATED BECAUSE THE       */ 01583000
*/*             REQUESTED VOLUME IS ALLOCATED ON ANOTHER DEVICE.     */ 01584000
*/*             VOLUME SWITCHING IS ALLOWED BUT FIRST THIS           */ 01585000
*/*             ALLOCATION MUST WAIT FOR THE VOLUME TO BE            */ 01586000
*/*             UNALLOCATED.                                         */ 01587000
*/*                                                                  */ 01588000
*/*       .AN ERROR RTNCODE IS SET IF MODULE IEFAB434 DETECTED       */ 01589000
*/*        A FAILING ERROR CONDITION.                                */ 01590000
*/*                                                                  */ 01591000
*/*   OPERATION - THE OPERATION IS DOCUMENTED BY THE CODE ITSELF.    */ 01592000
*/*                                                                  */ 01593000
*/* ENTRY - (INLINE SUBROUTINE)                                      */ 01594000
*/*                                                                  */ 01595000
*/*   PURPOSE - (SEE FUNCTION)                                       */ 01596000
*/*                                                                  */ 01597000
*/*   INPUT - REGISTER 15 - RETURN CODE FROM IEFAB434                */ 01598000
*/*         - UCBPARMP - PTR TO INFORMATION ON WHETHER A VOLUME      */ 01599000
*/*                      MOUNT IS NEEDED.                            */ 01600000
*/*                                                                  */ 01601000
*/* EXIT - NORMAL - (INLINE SUBROUTINE)                              */ 01602000
*/*                                                                  */ 01603000
*/*   CONDITION - THE VOLUNIT ENTRY WAS SUCCESSFULLY ALLOCATED       */ 01604000
*/*                                                                  */ 01605000
*/*   OUTPUT - VOLALOC=1                                             */ 01606000
*/*         - TOTVOLUN DECREMENTED                                   */ 01607000
*/*         - SIOTVMVP POINTS TO A VM+V MOUNT REQUEST, IF ANY        */ 01608000
*/*                                                                  */ 01609000
*/*   RETURN CODES - RTNCODE SET TO 0                                */ 01610000
*/*                                                                  */ 01611000
*/*   CONDITION - A NON-FAILING ERROR OCCURED                        */ 01612000
*/*                                                                  */ 01613000
*/*   OUTPUT - NONE FROM THIS SEGMENT                                */ 01614000
*/*                                                                  */ 01615000
*/*   RETURN CODES - RTNCODE SET TO 0                                */ 01616000
*/*                                                                  */ 01617000
*/* EXIT - ERROR - (INLINE SUBROUTINE)                               */ 01618000
*/*                                                                  */ 01619000
*/*   CONDITION - A FAILING ERROR OCCURED                            */ 01620000
*/*                                                                  */ 01621000
*/*   OUTPUT - NONE FROM THIS SEGMENT                                */ 01622000
*/*                                                                  */ 01623000
*/*   RETURN CODES - RTNCODE SET TO 4                                */ 01624000
*/*                                                                  */ 01625000
*/********************************************************************/ 01626000
*RFY REG15 RSTD;                                                04S0478 01627000
*IF REG15 = SUCCESS | REG15=RC40 THEN  /*ANY ERROR ?                 */ 01628000
         LTR   REG15,REG15                                      04S0479 01629000
         BZ    @RT00479                                         04S0479 01630000
         CH    REG15,@CH00191                                   04S0479 01631000
         BNE   @RF00479                                         04S0479 01632000
@RT00479 DS    0H                                               04S0480 01633000
*  DO;                                 /*NO, INDICATE ALLOC DONE OK  */ 01634000
*    RTNCODE = SUCCESS;                                         04S0481 01635000
         SLR   RTNCODE,RTNCODE                                  04S0481 01636000
*    VOLALOC = ON;                     /*SET SWITCH                  */ 01637000
         L     @14,VUPTR                                        04S0482 01638000
         OI    VOLALOC(@14),B'10000000'                         04S0482 01639000
*    TOTVOLUN = TOTVOLUN - ONE;        /*DECREMENT CT OF UNALLOC VU'S*/ 01640000
         L     @14,@PC00001                                     04S0483 01641000
         L     @14,ALCWAPTR(,@14)                               04S0483 01642000
         L     @02,TOTVOLUN(,@14)                               04S0483 01643000
         BCTR  @02,0                                            04S0483 01644000
         ST    @02,TOTVOLUN(,@14)                               04S0483 01645000
*  END;                                                         04S0484 01646000
*ELSE                                  /*ARU ERROR, PROCESS          */ 01647000
*  IF REG15 = RC20                                              04S0485 01648000
*    | REG15 = RC24                                             04S0485 01649000
*    | REG15 = RC28 THEN               /*IF RETCODES=20,24 OR 28 THEN*/ 01650000
         B     @RC00479                                         04S0485 01651000
@RF00479 CH    REG15,@CH00186                                   04S0485 01652000
         BE    @RT00485                                         04S0485 01653000
         CH    REG15,@CH00102                                   04S0485 01654000
         BE    @RT00485                                         04S0485 01655000
         CH    REG15,@CH00189                                   04S0485 01656000
         BNE   @RF00485                                         04S0485 01657000
@RT00485 DS    0H                                               04S0486 01658000
*  DO;                                 /*NON-FAILING ERROR           */ 01659000
*    RTNCODE = SUCCESS;                                         04S0487 01660000
         SLR   RTNCODE,RTNCODE                                  04S0487 01661000
*  END;                                                         04S0488 01662000
*ELSE                                  /*FAILURE                     */ 01663000
*  DO;                                                          04S0489 01664000
         B     @RC00485                                         04S0489 01665000
@RF00485 DS    0H                                               04S0490 01666000
*    RTNCODE = FAILURE;                /*SET AN ERROR RETURN CODE    */ 01667000
         LA    RTNCODE,4                                        04S0490 01668000
*  END;                                                         04S0491 01669000
*RFY REG15 UNRSTD;                                              04S0492 01670000
@RC00485 DS    0H                                               04S0492 01671000
@RC00479 DS    0H                                               04S0493 01672000
*  END;                                                         03S0493 01673000
*ELSE;                                 /*ERROR                       */ 01674000
@RF00446 DS    0H                                               03S0495 01675000
*      END;                                                     02S0495 01676000
*    ELSE                              /*NO CANDIDATE UCBS IN GROUP  */ 01677000
*      DO;                                                      02S0496 01678000
         B     @RC00300                                         02S0496 01679000
@RF00300 DS    0H                                               02S0497 01680000
*         DO; /*MODESET  EXTKEY(SCHED)*/                        02S0497 01681000
*           SPKA (16); /*SET PSW KEY*/                          02S0498 01682000
         LA    @02,16                                           02S0498 01683000
         SPKA  0(@02)                                           02S0498 01684000
*         END; /*MODESET  EXTKEY(SCHED)*/                       02S0499 01685000
*      END;                                                     02S0500 01686000
*    RFY(REG0,REG1) RSTD;                                       02S0501 01687000
@RC00300 DS    0H                                               02S0502 01688000
*    IF MSSDEV = OFF |                 /*NOT AN MSS UNIT     @ZA13405*/ 01689000
*       MSSDEV = ON &                  /*OR AN MSS UNIT AND  @ZA13405*/ 01690000
*       (MSSMATCH = ON |               /*A MATCH FOUND OR    @ZA13405*/ 01691000
*        MSSRECUR = OFF) THEN          /*ALL GROUPS PROCESSED@ZA13405*/ 01692000
         TM    MSSDEV,B'00001000'                               02S0502 01693000
         BZ    @RT00502                                         02S0502 01694000
         TM    MSSDEV,B'00001000'                               02S0502 01695000
         BNO   @RF00502                                         02S0502 01696000
         TM    MSSMATCH,B'00100000'                             02S0502 01697000
         BNZ   @RT00502                                         02S0502 01698000
         TM    MSSRECUR,B'00010000'                             02S0502 01699000
         BO    @RF00502                                         02S0502 01700000
@RT00502 DS    0H                                               02S0503 01701000
*      DO;                             /*FREE CANDIDATE LIST    02S0503 01702000
*                                        AND OPT. LIST       @ZA13405*/ 01703000
*        REG0 = UCBCLEN;               /*LENGTH OF UCB LIST GETMAIN  */ 01704000
         L     REG0,UCBCLEN                                     02S0504 01705000
*        REG1 = UCLSTP;                /*PTR TO UCB LIST GETMAIN     */ 01706000
         L     REG1,UCLSTP                                      02S0505 01707000
*        REG0 = REG0 | BYTE253;        /*SET SUBPOOL ID              */ 01708000
         O     REG0,BYTE253                                     02S0506 01709000
*        GEN(FREEMAIN R,LV=(0),A=(1)); /*FREE UCB CANDIDATE LIST     */ 01710000
         FREEMAIN R,LV=(0),A=(1)                                        01711000
*        RFY(REG0,REG1) UNRSTD;                                 02S0508 01712000
*        RFY (REG0,REG1) RSTD;                                  02S0509 01713000
*        REG0 = LENGTH(OPTPARMS);                               02S0510 01714000
         LA    REG0,20                                          02S0510 01715000
*        REG0 = REG0 | BYTE253;        /*SET SUBPOOL ID              */ 01716000
         O     REG0,BYTE253                                     02S0511 01717000
*        REG1 = OPTPARMP;                                       02S0512 01718000
         LR    REG1,OPTPARMP                                    02S0512 01719000
*        GEN (FREEMAIN R,LV=(0),A=(1));                         02S0513 01720000
         FREEMAIN R,LV=(0),A=(1)                                        01721000
*        RFY (REG0,REG1) UNRSTD;                                02S0514 01722000
*      END;                            /*END FREEING LISTS   @ZA13405*/ 01723000
*  END;                                                         02S0516 01724000
*ELSE                                  /*GETMAIN ERROR               */ 01725000
*  REASONCD = GETMNERR;                /*SET GETMAIN REASON CODE     */ 01726000
         B     @RC00277                                         02S0517 01727000
@RF00277 L     @02,@PC00001                                     02S0517 01728000
         L     @02,ALCWAPTR(,@02)                               02S0517 01729000
         L     @02,FAILRSCP(,@02)                               02S0517 01730000
         MVC   REASONCD(2,@02),@CH00044                         02S0517 01731000
*END GENAPREP;                         /*END GENAPREP PROC   @ZA13405*/ 01732000
@EL00002 DS    0H                                               02S0518 01733000
@EF00002 DS    0H                                               02S0518 01734000
@ER00002 LM    @14,@02,@SA00002                                 02S0518 01735000
         LM    @04,@09,@SA00002+20                              02S0518 01736000
         LM    @11,@12,@SA00002+44                              02S0518 01737000
         BR    @14                                              02S0518 01738000
*END IEFAB478                          /*END OF ALLOC-FROM-ALGS-GROUP*/ 01739000
