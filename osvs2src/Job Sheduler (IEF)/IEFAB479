*/* FUNCTION -                                                       */ 01518000
*/*   THE PURPOSE OF THIS CODE SEGMENT IS TO SCAN THE VOLUNIT        */ 01519000
*/*   TABLE WHENEVER IT IS DETERMINED THAT A REQUEST NEEDS RECOVERY  */ 01520000
*/*   PROCESSING. IF ANY ENTRIES IN THE VOLUNIT TABLE HAVE THE SAME  */ 01521000
*/*   VOLUNTID,OR ARE ASKING FOR THE SAME UNIT,THESE ENTRIES ARE     */ 01522000
*/*   ALSO MARKED FOR RECOVERY. AN ENTRY IS MARKED FOR RECOVERY BY   */ 01523000
*/*   TURNING ON THE VUDNALOC BIT AND PUTTING THE POINTER TO THE     */ 01524000
*/*   UCB IN FIELD CVRWUCBP OF THE ALGORITHM TABLES.                 */ 01525000
*/*                                                                  */ 01526000
*/* OPERATION -                                                      */ 01527000
*/*   THE OPERATION IS DOCUMENTED BY THE CODE.                       */ 01528000
*/*                                                                  */ 01529000
*/* ENTRY - INLINE SUBROUTINE                                        */ 01530000
*/*                                                                  */ 01531000
*/*   PURPOSE - SEE FUNCTION                                         */ 01532000
*/*                                                                  */ 01533000
*/*   INPUT - SIOTPTR - ADDRESS OF SIOT FOR THE REQUEST              */ 01534000
*/*                                                                  */ 01535000
*/* EXIT - NORMAL - INLINE SUBROUTINE                                */ 01536000
*/*                                                                  */ 01537000
*/*   CONDITION - ALL VOLUNIT ENTRIES WITH THE SAME VOLUNTID OR WHOSE*/ 01538000
*/*               SIOTS REPRESENT DEMAND REQUESTS AND ARE REQUESTING */ 01539000
*/*               THIS UNIT ARE MARKED FOR RECOVERY.                 */ 01540000
*/*                                                                  */ 01541000
*/*   OUTPUT - VUDNALOC BIT OF ALL SUCH REQUESTS TURNED ON,POINTER   */ 01542000
*/*            TO UCB PUT IN ALGORITHM TABLES.                       */ 01543000
*/*                                                                  */ 01544000
*/*   RETURN CODE - 0 IN RTNCODE                                     */ 01545000
*/*                                                                  */ 01546000
*/* EXIT ERROR - NONE                                                */ 01547000
*/*                                                                  */ 01548000
*/********************************************************************/ 01549000
*SAVSIOTP=SIOTPTR;                      /*SAVE SIOTPTR               */ 01550000
         L     SAVSIOTP,SIOTPTR                                 02S0412 01551000
*SAVUNTID=VOLUNTID;                     /*SAVE VOLUNITID             */ 01552000
         L     @10,VUPTR                                        02S0413 01553000
         LH    SAVUNTID,VOLUNTID(,@10)                          02S0413 01554000
*IF VUDMNDOF = OFF & VUDMNDAL = OFF THEN                        02S0414 01555000
         TM    VUDMNDOF(@10),B'00110000'                        02S0414 01556000
         BNZ   @RF00414                                         02S0414 01557000
*  DO;                                                          02S0415 01558000
*    VUDMUNIQ = ON;                     /*INDICATE FIRST REQ FOR UNIT*/ 01559000
         OI    VUDMUNIQ(@10),B'00000100'                        02S0416 01560000
*  END;                                                         02S0417 01561000
*VUPTR=VOLUNPTR;                        /*POINT TO FIRST VOLUN ENTRY */ 01562000
@RF00414 L     @10,@PC00001                                     02S0418 01563000
         L     @10,ALCWAPTR(,@10)                               02S0418 01564000
         MVC   VUPTR(4),VOLUNPTR(@10)                           02S0418 01565000
*DO VUCTR=ONE TO VOLUNSZE/LENGTH(VOLUNTAB); /*SCAN VOLUN TABLE       */ 01566000
         LA    VUCTR,1                                          02S0419 01567000
         B     @DE00419                                         02S0419 01568000
@DL00419 DS    0H                                               02S0420 01569000
*  SIOTPTR=VOLSIOTP;                                            02S0420 01570000
         L     @10,VUPTR                                        02S0420 01571000
         L     @05,VOLSIOTP(,@10)                               02S0420 01572000
         ST    @05,SIOTPTR                                      02S0420 01573000
*  IF VOLUNTID=SAVUNTID | (SIOTDMND=ON  /*SAME UNITID AS CURRENT REQ */ 01574000
*     & SIOTGIGN=OFF & SAVSIOTP->SIOUCBAD=SIOUCBAD) THEN        02S0421 01575000
         CH    SAVUNTID,VOLUNTID(,@10)                          02S0421 01576000
         BE    @RT00421                                         02S0421 01577000
         TM    SIOTDMND(@05),B'10000000'                        02S0421 01578000
         BNO   @RF00421                                         02S0421 01579000
         TM    SIOTGIGN(@05),B'00001000'                        02S0421 01580000
         BNZ   @RF00421                                         02S0421 01581000
         CLC   SIOUCBAD(3,SAVSIOTP),SIOUCBAD(@05)               02S0421 01582000
         BNE   @RF00421                                         02S0421 01583000
@RT00421 DS    0H                                               02S0422 01584000
*    DO;                                /*OR SAME UNIT               */ 01585000
*      VUDNALOC=ON;                     /*MARK VU FOR RECOVERY       */ 01586000
         L     @10,VUPTR                                        02S0423 01587000
         OI    VUDNALOC(@10),B'00001000'                        02S0423 01588000
*      VOLUNTID=SAVUNTID;               /*ASSIGN SAME UNIT ID        */ 01589000
         STH   SAVUNTID,VOLUNTID(,@10)                          02S0424 01590000
*      VOLALGTP->CVRWUCBP=UCBPTR;       /*PUT UCB PTR IN ALG TABLE   */ 01591000
         L     @05,UCBPTR                                       02S0425 01592000
         L     @01,VOLALGTP(,@10)                               02S0425 01593000
         STH   @05,CVRWUCBP(,@01)                               02S0425 01594000
*      WAITIGEN=ON;                     /*INDICATE ALB TABLE UPDATED */ 01595000
         L     @15,@PC00001                                     02S0426 01596000
         L     @01,ALCWAPTR(,@15)                               02S0426 01597000
         OI    WAITIGEN(@01),B'01000000'                        02S0426 01598000
*      IF UCBONLI = OFF THEN            /*IS UCB OFFLINE ?           */ 01599000
         TM    UCBONLI(@05),B'10000000'                         02S0427 01600000
         BNZ   @RF00427                                         02S0427 01601000
*        VUDMNDOF = ON;                 /*YES, INDICATE              */ 01602000
         OI    VUDMNDOF(@10),B'00100000'                        02S0428 01603000
*      ELSE                             /*INDICATE UCB IS ALLOC'D    */ 01604000
*        VUDMNDAL = ON;                                         02S0429 01605000
         B     @RC00427                                         02S0429 01606000
@RF00427 L     @10,VUPTR                                        02S0429 01607000
         OI    VUDMNDAL(@10),B'00010000'                        02S0429 01608000
*    END;                                                       02S0430 01609000
@RC00427 DS    0H                                               02S0431 01610000
*  VUPTR=VUPTR+LENGTH(VOLUNTAB);        /*POINT TO NEXT ENTRY        */ 01611000
@RF00421 LA    @10,28                                           02S0431 01612000
         AL    @10,VUPTR                                        02S0431 01613000
         ST    @10,VUPTR                                        02S0431 01614000
*END;                                                           02S0432 01615000
         AH    VUCTR,@CH00040                                   02S0432 01616000
@DE00419 L     @10,@PC00001                                     02S0432 01617000
         L     @10,ALCWAPTR(,@10)                               02S0432 01618000
         L     @00,VOLUNSZE(,@10)                               02S0432 01619000
         SRDA  @00,32                                           02S0432 01620000
         D     @00,@CF00137                                     02S0432 01621000
         CR    VUCTR,@01                                        02S0432 01622000
         BNH   @DL00419                                         02S0432 01623000
*SIOTPTR=SAVSIOTP;                                              02S0433 01624000
         ST    SAVSIOTP,SIOTPTR                                 02S0433 01625000
*VUPTR=SVOLUNAD;                                                02S0434 01626000
         L     @10,SIOTPTR                                      02S0434 01627000
         MVC   VUPTR(4),SVOLUNAD(@10)                           02S0434 01628000
*END MARKVUS;                                                   02S0435 01629000
@EL00002 DS    0H                                               02S0435 01630000
@EF00002 DS    0H                                               02S0435 01631000
@ER00002 LM    @14,@12,12(@13)                                  02S0435 01632000
         BR    @14                                              02S0435 01633000
*END IEFAB479                           /*END ALLOCATE DEMAND REQS   */ 01634000
