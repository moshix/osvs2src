         TM    OFFDEVSW(@10),B'00000100'                        02S0073 00159000
         BNZ   @RF00073                                         02S0073 00160000
*              DO;                      /*NO                         */ 00161000
*               RTNCODE=FAILURE ;       /*SET ERROR RETURN CODE      */ 00162000
         LA    RTNCODE,4                                        02S0075 00163000
*               SIOTRSNC=NOFFLINS;      /*SET SIOT REASON CODE       */ 00164000
         L     @10,SIOTPTR                                      02S0076 00165000
         MVC   SIOTRSNC(2,@10),@CH00124                         02S0076 00166000
*              END;                                             02S0077 00167000
*            ELSE                      /*CAN CONSIDER OFFLINES       */ 00168000
*              IF VOLMNTSW=OFF &       /*CAN VOLUMES BE MOUNTED?     */ 00169000
*                 (UCB3DACC = ON |     /*DIRECT ACCESS ?             */ 00170000
*                  UCB3TAPE = ON) THEN /*TAPE ?                      */ 00171000
         B     @RC00073                                         02S0078 00172000
@RF00073 L     @10,@PC00001                                     02S0078 00173000
