         ST    @02,UCBPTR                                       02S0065 00138000
*      IF UCBNALOC=ON THEN              /*IS UCB USEABLE?            */ 00139000
         TM    UCBNALOC(@02),B'00000100'                        02S0066 00140000
         BNO   @RF00066                                         02S0066 00141000
*        DO;                            /*NO                         */ 00142000
*          RTNCODE=FAILURE;             /*SET ERROR RETURN CODE      */ 00143000
         LA    RTNCODE,4                                        02S0068 00144000
*          SIOTRSNC=NALOCON;            /*SET SIOT REASON CODE       */ 00145000
         MVC   SIOTRSNC(2,@10),@CH00120                         02S0069 00146000
*        END;                                                   02S0070 00147000
*      ELSE                             /*UCB IS USEABLE             */ 00148000
*        IF UCBONLI=OFF THEN            /*IS UCB ONLINE?             */ 00149000
