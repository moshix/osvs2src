*                END;                                           02S0082 00187000
*              ELSE                    /*CAN MOUNT VOLUMES           */ 00188000
*                CALL MARKVUS;         /*MARK VU ENTRY FOR RECOVERY  */ 00189000
         B     @RC00078                                         02S0083 00190000
@RF00078 BAL   @14,MARKVUS                                      02S0083 00191000
*          END;                                                 02S0084 00192000
*        ELSE                          /*UCB IS ONLINE               */ 00193000
*          DO;                                                  02S0085 00194000
         B     @RC00071                                         02S0085 00195000
@RF00071 DS    0H                                               02S0086 00196000
*            IF UCB3TAPE=OFF & UCB3DACC=OFF THEN /*NON-TAPE/DA?      */ 00197000
         L     @10,UCBPTR                                       02S0086 00198000
         TM    UCB3TAPE(@10),B'10100000'                        02S0086 00199000
         BNZ   @RF00086                                         02S0086 00200000
*              DO;                    /*YES                          */ 00201000
*                IF UCBSYSR=ON THEN   /*PRIMARY CONSOLE?             */ 00202000
         TM    UCBSYSR(@10),B'00000010'                         02S0088 00203000
         BNO   @RF00088                                         02S0088 00204000
*                  DO;                /*YES                          */ 00205000
*                    RTNCODE=FAILURE; /*SET RETURN CODE              */ 00206000
