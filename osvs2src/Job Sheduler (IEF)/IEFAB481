         TITLE 'IEFAB481 - ELIMINATE GROUPS FROM EDL/ALG TABLES        *00001000
                        '                                               00002000
IEFAB481 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFAB481  74.211'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         MODESET EXTKEY=SCHED,SAVEKEY=(2)                               00015000
         GSPACE   R,LV=(0)                                              00016000
         LR    @11,@01                                          01S0001 00017000
         USING @DATD,@11                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         STC   @02,@OLDKEY                                      01S0001 00020000
         MODESET KEYADDR=(2)                                            00021000
         LM    @00,@02,20(@13)                                  01S0001 00022000
         ST    @11,8(,@13)                                      01S0001 00023000
         LR    @13,@11                                          01S0001 00024000
         MODESET EXTKEY=ZERO                                            00025000
         MVC   @PC00001(8),0(@01)                               01S0001 00026000
         MODESET EXTKEY=SCHED                                           00027000
*SIOTAFID = ZERO;                      /*TURN OFF MULTI INDICATOR    */ 00028000
         L     @10,@PC00001                                     01S0023 00029000
         L     @10,SIOTPTR(,@10)                                01S0023 00030000
         SLR   @03,@03                                          01S0023 00031000
         STH   @03,SIOTAFID(,@10)                               01S0023 00032000
*EDLPTR = EDLFGENP;                    /*POINT TO FIRST GEN IN EDL   */ 00033000
         L     @10,SIOTEDLP(,@10)                               01S0024 00034000
         L     EDLPTR,EDLFGENP(,@10)                            01S0024 00035000
*DO GENCTR = ONE TO EDLGENNO;          /*SCAN ALL GENS IN THE EDL    */ 00036000
         LA    GENCTR,1                                         01S0025 00037000
         B     @DE00025                                         01S0025 00038000
@DL00025 DS    0H                                               01S0026 00039000
*  IF EDLDEVT = DEVTYPE THEN           /*IS THIS THE GEN ALLOC'D ?   */ 00040000
         L     @10,@PC00001+4                                   01S0026 00041000
         CLC   EDLDEVT(4,EDLPTR),DEVTYPE(@10)                   01S0026 00042000
         BNE   @RF00026                                         01S0026 00043000
*    DO;                               /*YES, UPDATE THE ALG TABLES  */ 00044000
*      VUPTR = SVOLUNAD;               /*POINT TO SIOTS FIRST VOLUNIT*/ 00045000
         L     @10,@PC00001                                     01S0028 00046000
         L     @10,SIOTPTR(,@10)                                01S0028 00047000
         L     VUPTR,SVOLUNAD(,@10)                             01S0028 00048000
*      DO VUCTR = ONE TO SVOLUNNO;     /*PROCESS ALL VOLUNIT ENTRIES */ 00049000
         LA    VUCTR,1                                          01S0029 00050000
         B     @DE00029                                         01S0029 00051000
@DL00029 DS    0H                                               01S0030 00052000
*        GRPLISTP = CVRGRPPT;          /*POINT TO FIRST ALG GROUP ENT*/ 00053000
         L     @10,VOLALGTP(,VUPTR)                             01S0030 00054000
         L     GRPLISTP,CVRGRPPT(,@10)                          01S0030 00055000
*        ECOUNT = ZERO;                /*INITIALIZE COUNT TO 0       */ 00056000
         SLR   ECOUNT,ECOUNT                                    01S0031 00057000
*        DO GRPCTR = ONE TO CVRNGRP;   /*SCAN ALL ALG GROUP ENTRIES  */ 00058000
         LA    GRPCTR,1                                         01S0032 00059000
         B     @DE00032                                         01S0032 00060000
@DL00032 DS    0H                                               01S0033 00061000
*          IF EDLGRID = CVRGRPID THEN  /*IS GROUP THE FIRST ELIGIBLE?*/ 00062000
         L     @10,EDLGRPTR(,EDLPTR)                            01S0033 00063000
         L     @10,EDLGRID(,@10)                                01S0033 00064000
         CH    @10,CVRGRPID(,GRPLISTP)                          01S0033 00065000
         BNE   @RF00033                                         01S0033 00066000
*            DO;                       /*YES, ..                     */ 00067000
*              ECOUNT = EDLGRPNO;      /*SET COUNT OF ELIGIBLE GROUPS*/ 00068000
         L     ECOUNT,EDLGRPNO(,EDLPTR)                         01S0035 00069000
*            END;                                               01S0036 00070000
*          ELSE;                       /*GROUP IS NOT THE FIRST ELIG */ 00071000
@RF00033 DS    0H                                               01S0038 00072000
*          IF ECOUNT > ZERO THEN       /*IS THIS AN ELIGIBLE GROUP ? */ 00073000
         LTR   ECOUNT,ECOUNT                                    01S0038 00074000
         BNP   @RF00038                                         01S0038 00075000
*            DO;                       /*YES, ..                     */ 00076000
*              ECOUNT = ECOUNT - ONE;  /*DECREMENT THE COUNT OF ELIG */ 00077000
         BCTR  ECOUNT,0                                         01S0040 00078000
*            END;                                               01S0041 00079000
*          ELSE                        /*GROUP IS NOT IN GEN ALLOC'D */ 00080000
*            DO;                                                01S0042 00081000
         B     @RC00038                                         01S0042 00082000
@RF00038 DS    0H                                               01S0043 00083000
*              CVRGSKP = ON;           /*MARK THE GROUP INELIGIBLE   */ 00084000
         OI    CVRGSKP(GRPLISTP),B'10000000'                    01S0043 00085000
*            END;                                               01S0044 00086000
*          GRPLISTP=GRPLISTP+LENGTH(CVRGPLST); /*PT TO NEXT GROUP ENT*/ 00087000
@RC00038 AH    GRPLISTP,@CH00088                                01S0045 00088000
*        END;                                                   01S0046 00089000
         AH    GRPCTR,@CH00050                                  01S0046 00090000
@DE00032 L     @10,VOLALGTP(,VUPTR)                             01S0046 00091000
         CH    GRPCTR,CVRNGRP(,@10)                             01S0046 00092000
         BNH   @DL00032                                         01S0046 00093000
*        VUPTR = VUPTR + LENGTH(VOLUNTAB); /*PT TO NEXT VOLUNIT ENTRY*/ 00094000
         AH    VUPTR,@CH00367                                   01S0047 00095000
*      END;                                                     01S0048 00096000
         AH    VUCTR,@CH00050                                   01S0048 00097000
@DE00029 L     @10,@PC00001                                     01S0048 00098000
         L     @10,SIOTPTR(,@10)                                01S0048 00099000
         CH    VUCTR,SVOLUNNO(,@10)                             01S0048 00100000
         BNH   @DL00029                                         01S0048 00101000
*    END;                                                       01S0049 00102000
*  ELSE                                /*NOT THE GENERIC ALLOCATED   */ 00103000
*    DO;                                                        01S0050 00104000
         B     @RC00026                                         01S0050 00105000
@RF00026 DS    0H                                               01S0051 00106000
*      EINDEVTP = ON;                  /*MARK GEN INELIGIBLE IN EDL  */ 00107000
         OI    EINDEVTP(EDLPTR),B'10000000'                     01S0051 00108000
*    END;                                                       01S0052 00109000
*  EDLPTR = EDLNXTNT;                  /*PTR TO NEXT GEN IN THE EDL  */ 00110000
@RC00026 L     EDLPTR,EDLNXTNT(,EDLPTR)                         01S0053 00111000
*END;                                                           01S0054 00112000
         AH    GENCTR,@CH00050                                  01S0054 00113000
@DE00025 L     @10,@PC00001                                     01S0054 00114000
         L     @10,SIOTPTR(,@10)                                01S0054 00115000
         L     @10,SIOTEDLP(,@10)                               01S0054 00116000
         C     GENCTR,EDLGENNO(,@10)                            01S0054 00117000
         BNH   @DL00025                                         01S0054 00118000
*RETURN CODE (ZERO);                   /*RETURN TO CALLER            */ 00119000
         IC    @02,@OLDKEY                                      01S0055 00120000
         L     @13,4(,@13)                                      01S0055 00121000
         L     @00,@SIZDATD                                     01S0055 00122000
         LR    @01,@11                                          01S0055 00123000
         FSPACE   R,LV=(0),A=(1)                                        00124000
         MODESET KEYADDR=(2)                                            00125000
         SLR   @15,@15                                          01S0055 00126000
         L     @14,12(,@13)                                     01S0055 00127000
         LM    @00,@12,20(@13)                                  01S0055 00128000
         BR    @14                                              01S0055 00129000
*END IEFAB481                                                   01S0056 00130000
