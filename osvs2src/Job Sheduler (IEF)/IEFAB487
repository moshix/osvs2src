         SLR   @15,@15                                          02S0225 00655000
         ICM   @15,3,CVRWUCBP(RQLSTPTR)                         02S0225 00656000
         LTR   @15,@15                                          02S0225 00657000
         BZ    @RF00225                                         02S0225 00658000
         TM    VUVLUNIQ(VUPTR),B'00000010'                      02S0225 00659000
         BNO   @RF00225                                         02S0225 00660000
*            DO;                       /*SET UP FOR MESSAGE SUBRTN   */ 00661000
*              UCBPTR = CVRWUCBP;      /*SET PTR TO UCB FROM REQ LIST*/ 00662000
         LR    UCBPTR,@15                                       02S0227 00663000
*              MSGOFFS = VOLWAIT;      /*SET OFFSET TO MESSAGE ENTRY */ 00664000
         LA    MSGOFFS,2                                        02S0228 00665000
*              CALL BLDSWMSG;          /*BUILD/ISSUE SPECIFIC WAIT      00666000
*                                        FOR VOLUME MESSAGE          */ 00667000
         BAL   @14,BLDSWMSG                                     02S0229 00668000
*            END;                                               02S0230 00669000
*          IF SIOTDMND = YES &         /*DEMAND REQUEST              */ 00670000
*              VUDMNDAL = YES &        /*REQUIRED UNIT IS ALLOCATED  */ 00671000
*              VUDMUNIQ = YES THEN     /*UNIQUE REQUEST FOR UNIT     */ 00672000
@RF00225 TM    SIOTDMND(SIOTPTR),B'10000000'                    02S0231 00673000
         BNO   @RF00231                                         02S0231 00674000
         TM    VUDMNDAL(VUPTR),B'00010100'                      02S0231 00675000
         BNO   @RF00231                                         02S0231 00676000
*            DO;                       /*SET UP FOR MSG SUBROUTINE   */ 00677000
*              UCBPTR = SIOUCBAD;      /*SET PTR TO UCB FROM SIOT    */ 00678000
         SLR   UCBPTR,UCBPTR                                    02S0233 00679000
         ICM   UCBPTR,7,SIOUCBAD(SIOTPTR)                       02S0233 00680000
*              MSGOFFS = UNITWAIT;     /*SET INDEX TO MSG TEXT ENTRY */ 00681000
