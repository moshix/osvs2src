@DC00334 DS    0H                                               02S0337 01064000
*RETURN;                                                        02S0337 01065000
@EL00002 DS    0H                                               02S0337 01066000
@EF00002 DS    0H                                               02S0337 01067000
@ER00002 LM    @14,@12,12(@13)                                  02S0337 01068000
         BR    @14                                              02S0337 01069000
*END CONVERT;                                                   02S0338 01070000
*/********************************************************************/ 01071000
*/*                                                                  */ 01072000
*/* SEGMENT NAME - BLDSWMSG                                          */ 01073000
*/*                                                                  */ 01074000
*/* DESCRIPTIVE NAME - BUILD SPECIFIC WAIT MESSAGE                   */ 01075000
*/*                                                                  */ 01076000
*/* FUNCTION -                                                       */ 01077000
*/*   THIS ROUTINE BUILDS AND ISSUES THE SPECIFIC WAIT MESSAGE FOR   */ 01078000
*/*   ALLOCATION RECOVERY PROCESSING. EITHER THE 'WAIT FOR UNIT'     */ 01079000
*/*   MESSAGE OR THE 'WAIT FOR VOLUME ON UNIT' MESSAGE IS ISSUED TO  */ 01080000
*/*   THE SYSTEM OPERATOR (AND INDICATED DEVICE POOLS) ON EACH CALL  */ 01081000
*/*   TO THIS ROUTINE.                                               */ 01082000
*/*                                                                  */ 01083000
*/*   OPERATION -                                                    */ 01084000
*/*     THE APPROPRIATE MESSAGE IS BUILT IN TWO PARTS. THE FIRST,    */ 01085000
*/*     THE ID PORTION, IS RETRIEVED FROM THE MESSAGE MODULE AND     */ 01086000
*/*     THE DDNAME INSERTED (AS WELL AS THE RELATIVE POSITION, IF    */ 01087000
*/*     NECESSARY). THIS PORTION OF THE MESSAGE IS PASSED TO IEFAB4F3*/ 01088000
*/*     TO REMOVE ANY EMBEDDED BLANKS IN THE MESSAGE.                */ 01089000
*/*                                                                  */ 01090000
*/*     THE VARIABLE PART OF THE MESSAGE IS RETREIVED USING THE      */ 01091000
*/*     INPUT OFFSET VALUE AS AN INDEX INTO THE OFFSET TABLE OF      */ 01092000
*/*     THE MESSAGE MODULE. THIS TEXT IS MOVED INTO THE BUFFER       */ 01093000
*/*     IMMEDIATELY FOLLOWING THE ID TEXT. THE VARIABLE INFORMATION, */ 01094000
*/*     UNIT NAME AND/OR VOLSER IS THEN INSERTED. IT SHOULD BE  NOTED*/ 01095000
*/*     THAT THE FULL MESSAGE CANNOT BE COMPRESSED BECAUSE EMBEDDED  */ 01096000
*/*     BLANKS ARE VALID IN A VOLSER.                                */ 01097000
*/*                                                                  */ 01098000
*/* ENTRY - BLDSWMSG (INTERNAL PROC)                                 */ 01099000
*/*   PURPOSE - SEE FUNCTION                                         */ 01100000
*/*   INPUT -                                                        */ 01101000
*/*     MSGOFFS - OFFSET INDEX                                       */ 01102000
*/*     UCBPTR - PTR TO UCB OF REQUIRED UNIT                         */ 01103000
*/*     LCLFLGS - LOCAL FLAGS AREA INDICATING DEVICE POOLS TO        */ 01104000
*/*               RECEIVE MSG                                        */ 01105000
*/* EXIT -                                                           */ 01106000
*/*   CONDITION - MESSAGE ISSUED                                     */ 01107000
*/*     OUTPUT -                                                     */ 01108000
*/*       MESSAGE IEF488I ISSUED                                     */ 01109000
*/*       SWAITSW SET TO INDICATE A SPECIFIC WAIT MSG HAS BEEN ISSUED*/ 01110000
*/*     RETURN CODES - NONE                                          */ 01111000
*/*                                                                  */ 01112000
*/********************************************************************/ 01113000
*BLDSWMSG:                             /*BUILD SPECIFIC WAIT MSG     */ 01114000
*  PROC;                                                        02S0339 01115000
BLDSWMSG STM   @14,@12,@SA00003                                 02S0339 01116000
*SWAITSW = ON;                         /*INDICATE MESSAGE ISSUED     */ 01117000
         OI    SWAITSW,B'10000000'                              02S0340 01118000
*IF UCB3DACC = YES THEN                /*D.A. UNIT REQUIRED          */ 01119000
         TM    UCB3DACC(UCBPTR),B'00100000'                     02S0341 01120000
         BNO   @RF00341                                         02S0341 01121000
*  LCLFLGD = ON;                       /*ROUTE MSG TO D.A. POOL      */ 01122000
         OI    LCLFLGD,B'00010000'                              02S0342 01123000
*ELSE                                  /*NOT D.A. UNIT               */ 01124000
*  IF UCB3TAPE = YES THEN              /*TAPE UNIT REQUIRED          */ 01125000
         B     @RC00341                                         02S0343 01126000
@RF00341 TM    UCB3TAPE(UCBPTR),B'10000000'                     02S0343 01127000
         BNO   @RF00343                                         02S0343 01128000
*    LCLFLGC = ON;                     /*ROUTE TO TAPE POOL          */ 01129000
         OI    LCLFLGC,B'00100000'                              02S0344 01130000
*  ELSE                                /*NOT TAPE OR D.A. UNIT       */ 01131000
*    IF UCB3UREC = YES THEN            /*UNIT RCD DEVICE REQUIRED    */ 01132000
         B     @RC00343                                         02S0345 01133000
@RF00343 TM    UCB3UREC(UCBPTR),B'00001000'                     02S0345 01134000
         BNO   @RF00345                                         02S0345 01135000
*      LCLFLGG = ON;                   /*ROUTE MSG TO UNIT REC POOL  */ 01136000
         OI    LCLFLGG,B'00000010'                              02S0346 01137000
*RPLYRTEC = RPLYRTEC | LCLFLGS;        /*SET IN REPLY MSG DATA AREA  */ 01138000
@RF00345 DS    0H                                               02S0347 01139000
@RC00343 DS    0H                                               02S0347 01140000
@RC00341 OC    RPLYRTEC(1),LCLFLGS                              02S0347 01141000
*LCLFLGB = ON;                         /*ROUTE MSG TO MASTER CONSOLE */ 01142000
         OI    LCLFLGB,B'01000000'                              02S0348 01143000
*WPLTXT = '';                          /*CLEAR TXT AREA OF WTO P-LIST*/ 01144000
         MVI   WPLTXT(WPLPTR),C' '                              02S0349 01145000
         MVC   WPLTXT+1(125,WPLPTR),WPLTXT(WPLPTR)              02S0349 01146000
*TXTLNTH = SWIDMSGL;                   /*GET LENGTH OF BEG'NG MSG TXT*/ 01147000
         L     @05,@CA00158                                     02S0350 01148000
         LH    @15,SWIDMSGL-SWMSGID(,@05)                       02S0350 01149000
         ST    @15,TXTLNTH                                      02S0350 01150000
*SWMID(1:TXTLNTH) = SWIDMTXT(1:TXTLNTH); /*MOVE TEXT TO BUFFER       */ 01151000
         LA    @14,4                                            02S0351 01152000
         ALR   @14,WPLPTR                                       02S0351 01153000
         BCTR  @15,0                                            02S0351 01154000
         EX    @15,@SM01293                                     02S0351 01155000
*MSGJNAME = ALCJOBN;                   /*INSERT JOB NAME             */ 01156000
         L     @05,@PC00001                                     02S0352 01157000
         L     @05,ALCWAPTR(,@05)                               02S0352 01158000
         L     @05,TIOTHDRP(,@05)                               02S0352 01159000
         MVC   MSGJNAME(8,@14),ALCJOBN(@05)                     02S0352 01160000
*SWMSGDDN = SAVDDNM;                   /*INSERT DD NAME IN BUFFER    */ 01161000
         MVC   SWMSGDDN(8,@14),SAVDDNM                          02S0353 01162000
*IF CCATCTR ^= ZERO THEN               /*DD IS CONCAT'N GROUP MEMBER */ 01163000
         ICM   @05,15,CCATCTR                                   02S0354 01164000
         BZ    @RF00354                                         02S0354 01165000
*  DO;                                                          02S0355 01166000
*    CALL CONVERT(CCATCTR,MSGRPOS);    /*CONVERT RELATIVE POSITION   */ 01167000
         LA    @05,CCATCTR                                      02S0356 01168000
         ST    @05,@AL00001                                     02S0356 01169000
         LA    @05,MSGRPOS                                      02S0356 01170000
         ST    @05,@AL00001+4                                   02S0356 01171000
         LA    @01,@AL00001                                     02S0356 01172000
         BAL   @14,CONVERT                                      02S0356 01173000
*    SWMRPOS = MSGRPOS(2:4);           /*MOVE EBCDIC VALUE TO BUFFER */ 01174000
         MVC   SWMRPOS+4(3,WPLPTR),MSGRPOS+1                    02S0357 01175000
*  END;                                                         02S0358 01176000
*ELSE                                                           02S0359 01177000
*  SWMSGCCN = '';                      /*BLANK OUT REL POS'N FIELD   */ 01178000
         B     @RC00354                                         02S0359 01179000
@RF00354 MVI   SWMSGCCN+4(WPLPTR),C' '                          02S0359 01180000
         MVC   SWMSGCCN+5(3,WPLPTR),SWMSGCCN+4(WPLPTR)          02S0359 01181000
*CALL IEFAB4F3(ADDR(SWMID),TXTLNTH,MSGLNTH); /*REMOVE EMBEDDED BLANKS   01182000
*                                        FROM THIS PART OF MESSAGE   */ 01183000
@RC00354 LA    @15,4                                            02S0360 01184000
         ALR   @15,WPLPTR                                       02S0360 01185000
         ST    @15,@TF00001                                     02S0360 01186000
         LA    @15,@TF00001                                     02S0360 01187000
         ST    @15,@AL00001                                     02S0360 01188000
         LA    @15,TXTLNTH                                      02S0360 01189000
         ST    @15,@AL00001+4                                   02S0360 01190000
         LA    @15,MSGLNTH                                      02S0360 01191000
         ST    @15,@AL00001+8                                   02S0360 01192000
         L     @15,@CV00165                                     02S0360 01193000
         LA    @01,@AL00001                                     02S0360 01194000
         BALR  @14,@15                                          02S0360 01195000
*TXTPTR = ADDR(SWMSGOPS)               /*PT TO VARIABLE TEXT ENTRY   */ 01196000
*    + SPWTOFFS(MSGOFFS);                                       02S0361 01197000
         L     TXTPTR,@CA00168                                  02S0361 01198000
         LR    @15,MSGOFFS                                      02S0361 01199000
         SLA   @15,2                                            02S0361 01200000
         L     @14,@CA00161                                     02S0361 01201000
         LA    @01,0(@15,@14)                                   02S0361 01202000
         AL    @01,@CF01283                                     02S0361 01203000
         AH    TXTPTR,SPWTOFFS-SWMSGOFS(,@01)                   02S0361 01204000
*TXTLNTH = SPWMTXTL(MSGOFFS);          /*GET LENGTH OF REMAINING TEXT*/ 01205000
         BCTR  @14,0                                            02S0362 01206000
         BCTR  @14,0                                            02S0362 01207000
         LH    @15,SPWMTXTL-SWMSGOFS-2(@15,@14)                 02S0362 01208000
         ST    @15,TXTLNTH                                      02S0362 01209000
*SWMVAR(1:TXTLNTH) =                   /*MOVE TEXT TO BUFFER         */ 01210000
*    MSGTXT(1:TXTLNTH);                                         02S0363 01211000
         LA    @14,WPLTXT(,WPLPTR)                              02S0363 01212000
         AL    @14,MSGLNTH                                      02S0363 01213000
         LR    @09,@15                                          02S0363 01214000
         BCTR  @09,0                                            02S0363 01215000
         EX    @09,@SM01295                                     02S0363 01216000
*SWMUNIT = UCBNAME;                    /*MOVE UNIT NAME TO BUFFER    */ 01217000
         ALR   @15,TXTPTR                                       02S0364 01218000
         SLR   @09,@09                                          02S0364 01219000
         IC    @09,VAROFS2(,@15)                                02S0364 01220000
         ALR   @09,@14                                          02S0364 01221000
         MVC   SWMUNIT(3,@09),UCBNAME(UCBPTR)                   02S0364 01222000
*IF MSGOFFS = VOLWAIT THEN             /*'WAIT FOR VOL ON UNIT' MSG  */ 01223000
         CH    MSGOFFS,@CH00078                                 02S0365 01224000
         BNE   @RF00365                                         02S0365 01225000
*  SWMVOL = UCBVOLI;                   /*MOVE IN VOLSER              */ 01226000
         SLR   @06,@06                                          02S0366 01227000
         IC    @06,VAROFS1(,@15)                                02S0366 01228000
         ALR   @14,@06                                          02S0366 01229000
         MVC   SWMVOL(6,@14),UCBVOLI(UCBPTR)                    02S0366 01230000
*MSGLNTH = MSGLNTH + TXTLNTH;          /*GET TOTAL MESSAGE LENGTH    */ 01231000
@RF00365 L     @05,MSGLNTH                                      02S0367 01232000
         AL    @05,TXTLNTH                                      02S0367 01233000
         ST    @05,MSGLNTH                                      02S0367 01234000
*WPLLGH = MSGLNTH + FOUR;              /*SET LENGTH OF MSG IN P-LIST */ 01235000
         LA    @15,4                                            02S0368 01236000
         ALR   @15,@05                                          02S0368 01237000
         STH   @15,WPLLGH(,WPLPTR)                              02S0368 01238000
*WPLMCSF = WPLMCSF && WPLMCSF;         /*CLEAR MCS FLAGS AREA        */ 01239000
         XC    WPLMCSF(2,WPLPTR),WPLMCSF(WPLPTR)                02S0369 01240000
*WPLMCSFA = ON;                        /*INIC PRESENCE OF RTE/DESC      01241000
*                                        CODES                       */ 01242000
         OI    WPLMCSFA(WPLPTR),B'10000000'                     02S0370 01243000
*WPLFLGS = WPLFLGS && WPLFLGS;         /*CLEAR RTE/DESC CODES AREA   */ 01244000
         LA    @15,WPLTXT(,WPLPTR)                              02S0371 01245000
         ALR   @15,@05                                          02S0371 01246000
         XC    WPLFLGS(8,@15),WPLFLGS(@15)                      02S0371 01247000
*WPLDESCF = ON;                        /*SET JOB STATUS DESC'TR CODE */ 01248000
         OI    WPLDESCF(@15),B'00000100'                        02S0372 01249000
*WPLROUT1 = LCLFLGS;                   /*SET RT'NG CODES FROM LOCAL     01250000
*                                        AREA                        */ 01251000
         MVC   WPLROUT1(1,@15),LCLFLGS                          02S0373 01252000
*RFY (REG0) RSTD;                                               02S0374 01253000
*REG0 = ZERO;                          /*CLEAR R0 FOR WTO            */ 01254000
         SLR   REG0,REG0                                        02S0375 01255000
*GEN(WTO MF=(E,WTOL));                 /*ISSUE WTO                   */ 01256000
         WTO MF=(E,WTOL)                                                01257000
*RFY (REG0) UNRSTD;                                             02S0377 01258000
*RETURN;                               /*RETURN TO CALLER            */ 01259000
@EL00003 DS    0H                                               02S0378 01260000
@EF00003 DS    0H                                               02S0378 01261000
@ER00003 LM    @14,@12,@SA00003                                 02S0378 01262000
         BR    @14                                              02S0378 01263000
*END BLDSWMSG;                                                  02S0379 01264000
