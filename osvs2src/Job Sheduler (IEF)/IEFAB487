         TITLE 'IEFAB487 - ALLOCATION RECOVERY OPERATOR INTERFACE      *00001000
                        '                                               00002000
IEFAB487 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFAB487  78.124'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  LA    @11,4095(,@12)                                   01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         USING @PSTART+4095,@11                                 01S0001 00014000
         L     @00,@SIZDATD                                     01S0001 00015000
         MODESET EXTKEY=SCHED,SAVEKEY=(2)                               00016000
         GSPACE   R,LV=(0)                                              00017000
         LR    @10,@01                                          01S0001 00018000
         USING @DATD,@10                                        01S0001 00019000
         ST    @13,@SA00001+4                                   01S0001 00020000
         STC   @02,@OLDKEY                                      01S0001 00021000
         MODESET KEYADDR=(2)                                            00022000
         LM    @00,@02,20(@13)                                  01S0001 00023000
         ST    @10,8(,@13)                                      01S0001 00024000
         LR    @13,@10                                          01S0001 00025000
         MODESET EXTKEY=ZERO                                            00026000
         MVC   @PC00001(4),0(@01)                               01S0001 00027000
         MODESET EXTKEY=SCHED                                           00028000
*RETCODE = ZERO;                       /*INIT RETURN CODE AREA       */ 00029000
         SLR   RETCODE,RETCODE                                  01S0089 00030000
*COMPRTEC = ZERO;                      /*INIT COMPOSITE RT'NG CODES  */ 00031000
         MVI   COMPRTEC,X'00'                                   01S0090 00032000
*RPLYDATA = RPLYDATA && RPLYDATA;      /*CLEAR REPLY RTN'S DATA AREA */ 00033000
         XC    RPLYDATA(8),RPLYDATA                             01S0091 00034000
*WPLPTR = ADDR(WTOL);                  /*SET PTR TO WTO P-LIST       */ 00035000
         LA    WPLPTR,WTOL                                      01S0092 00036000
*/********************************************************************/ 00037000
*/*                                                                  */ 00038000
*/* SEGMENT NAME - RHDRMSGS                                          */ 00039000
*/*                                                                  */ 00040000
*/* DESCRIPTIVE NAME - RECOVERY HEADER MESSAGES                      */ 00041000
*/*                                                                  */ 00042000
*/* FUNCTION -                                                       */ 00043000
*/*   THIS ROUTINE BUILDS AND ISSUES THE RECOVERY HEADER MESSAGE     */ 00044000
*/*   (IEF244I) TO THE SYSTEM OPERATOR. THE MESSAGE INFORMS THE      */ 00045000
*/*   OPERATOR OF THE RECOVERY STATUS BY INDICATING THE TOTAL NUMBER */ 00046000
*/*   OF UNITS NEEDED TO SATISFY THIS ALLOCATION. THE MINIMUM NUMBER */ 00047000
*/*   OF ALLOCATED AND/OR OFFLINE UNITS WHICH WILL BE NEEDED IS      */ 00048000
*/*   CALCULATED (IF POSSIBLE - SEE OPERATION) AND INSERTED INTO     */ 00049000
*/*   THE SECOND LINE OF THE HEADER MESSAGE.                         */ 00050000
*/*                                                                  */ 00051000
*/*   OPERATION -                                                    */ 00052000
*/*     .DETERMINE UNITS NEEDED                                      */ 00053000
*/*      FOR EACH UNALLOCATED VOLUNIT ENTRY THE ROUTINE WILL:        */ 00054000
*/*      A) INCREMENT THE TOTAL NUMER OF UNITS NEEDED IF 1) THE ENTRY*/ 00055000
*/*         REQUIRES A DEVICE AND IS COVERED BY THE ALGORITHM, OR 2) */ 00056000
*/*         THE ENTRY REPRESENTS A DEMAND REQUEST, OR 3) THE ENTRY   */ 00057000
*/*         DOES NOT REQUIRE A DEVICE BUT SPECIFIES A VOLUME WHICH   */ 00058000
*/*         IS CURRENTLY ALLOCATED TO ANOTHER JOB.                   */ 00059000
*/*      B) INCREMENT THE NUMBER OF ALLOCATED UNITS NEEDED IF 1) THE */ 00060000
*/*         ENTRY REQUIRES A DEVICE, IS COVERED BY THE ALGORITHM     */ 00061000
*/*         AND THERE ARE ONLY ALLOCATED UNITS AVAILABLE IN THE      */ 00062000
*/*         GROUPS ELIGIBLE TO THIS ENTRY, OR 2) THE DEMAND ENTRY    */ 00063000
*/*         INDICATES A NEED FOR AN ALLOCATED UNIT (VUDMNDAL=1), OR  */ 00064000
*/*         3) (SAME AS 3 F OR A)).                                  */ 00065000
*/*      C) INCREMENT THE NUMBER OF OFFLINE UNITS NEEDED IF 1) THE   */ 00066000
*/*         ENTRY REQUIRES A DEVICE, IS COVERED BY THE ALGORITHM     */ 00067000
*/*         AND THERE ARE ONLY OFFLINE UNITS AVAILABLE IN THE GROUPS */ 00068000
*/*         ELIGIBLE TO THIS ENTRY, OR 2) THE UNIQUE DEMAND ENTRY    */ 00069000
*/*         INDICATES A NEED FOR AN OFFLINE UNIT (VUDMNDOF=1).       */ 00070000
*/*      NOTE THAT FOR THE VOLUNIT ENTRIES COVERED BY THE ALGORITHM, */ 00071000
*/*      THE PRESENCE OF BOTH ALLOCATED AND OFFLINE UNITS IN THE     */ 00072000
*/*      ELIGIBLE GROUPS IS CONSIDERED TO BE AN INDERTERMINATE       */ 00073000
*/*      CONDITION SO NEITHER COUNT IS UPDATED.                      */ 00074000
*/*                                                                  */ 00075000
*/*     .BUILD AND ISSUE MESSAGE LINE(S)                             */ 00076000
*/*      THE FIRST LINE OF THE HEADER MESSAGE IS ALWAYS BUILT. IF    */ 00077000
*/*      THERE IS ANY DATA FOR THE SECOND LINE THE NECESSARY TEXT    */ 00078000
*/*      IS ADDED TO THE MESSAGE BUFFER. THE MESSAGE IS ISSUED TO    */ 00079000
*/*      THE SYSTEM OPERATOR VIA THE MLWTO INTERFACE. THE MESSAGE    */ 00080000
*/*      IS ALSO ROUTED TO THE DA, TAPE AND/OR UNIT RECORD POOLS     */ 00081000
*/*      AS REQUIRED.                                                */ 00082000
*/*                                                                  */ 00083000
*/* ENTRY - IN-LINE SEGMENT                                          */ 00084000
*/*   PURPOSE - ISSUE RECOVERY HEADER MESSAGE LINE(S)                */ 00085000
*/*   INPUT -                                                        */ 00086000
*/*     PTR TO START OF VOLUNIT TABLE. EACH ENTRY CONTAINS --        */ 00087000
*/*       VOLALOC - IF OFF, ENTRY HAS NOT BEEN ALLOCATED             */ 00088000
*/*       VDEVREQD - IF ON, ENTRY REQUIRES A DEVICE                  */ 00089000
*/*       VOLSIOTP - PTR TO SIOT  (SIOTDMND INDICATES DEMAND REQUEST)*/ 00090000
*/*       VOLALGTP - PTR TO REQUEST LIST ENTRY                       */ 00091000
*/*     REQUEST LIST ENTRY --                                        */ 00092000
*/*       CVRSKFLG - IF OFF, ALGORITHM CAN PROCESS REQ ENTRY         */ 00093000
*/*       CVRNGRP - NUMBER OF ELIGIBLE GROUPS                        */ 00094000
*/*       CVRGRPPT - PTR TO FIRST GROUP LIST ENTRY FOR REQ ENTRY     */ 00095000
*/*     GROUP LIST ENTRY --                                          */ 00096000
*/*       CVRGALL - IF NON-NEGATIVE, POSSIBLE SOLUTION IN THIS GROUP */ 00097000
*/*       CVRGSKP - IF OFF, GROUP CONSIDERED BY ALGORITHM            */ 00098000
*/*       CVRGCPTR - PTR TO GROUP COUNT ENTRY                        */ 00099000
*/*     GROUP COUNT ENTRY --                                         */ 00100000
*/*       GRPALLCT - NUMBER ALLOCATED UNITS IN GROUP                 */ 00101000
*/*       GRPOFFLN - NUMBER OFFLINE UNITS IN GROUP                   */ 00102000
*/*                                                                  */ 00103000
*/* EXITS -                                                          */ 00104000
*/*   CONDITION - FUNCTION COMPLETED                                 */ 00105000
*/*     OUTPUT -                                                     */ 00106000
*/*       RECOVERY HEADER MESSAGE LINE(S) ISSUED                     */ 00107000
*/*       RETURN CODES - NONE                                        */ 00108000
*/********************************************************************/ 00109000
*LCLFLGS = LCLFLGS && LCLFLGS;         /*CLEAR LOCAL WTO FLAGS AREA  */ 00110000
         XC    LCLFLGS(1),LCLFLGS                               02S0093 00111000
*LCLFLGB = ON;                         /*SET RTE CDE-MASTER CONSOLE  */ 00112000
         OI    LCLFLGB,B'01000000'                              02S0094 00113000
*/********************************************************************/ 00114000
*/*                                                                  */ 00115000
*/* SEGMENT NAME - GUNITCTS                                          */ 00116000
*/*                                                                  */ 00117000
*/* DESCRIPTIVE NAME - GET UNIT COUNTS                               */ 00118000
*/*                                                                  */ 00119000
*/* FUNCTION -                                                       */ 00120000
*/*   THIS ROUTINE CALCULATES THE TOTAL NUMBER OF UNITS NEEDED TO    */ 00121000
*/*   COMPLETE THIS ALLOCATION. IN ADDITION, THE ROUTINE ATTEMPTS    */ 00122000
*/*   TO DETERMINE THE MINIMUM NUMBER OF ALLOCATED AND/OR            */ 00123000
*/*   OFFLINE UNITS NEEDED.                                          */ 00124000
*/*                                                                  */ 00125000
*/*   OPERATION -                                                    */ 00126000
*/*     AS DESCRIBED IN 'RHDRSPEC', THE ROUTINE USES INFORMATION     */ 00127000
*/*     FROM THE REQUEST LIST ENTRY, THE GROUP LIST ENTRIES AND THE  */ 00128000
*/*     GROUP COUNT ENTRY ASSOCIATED WITH EACH UNALLOCATED VOLUNIT   */ 00129000
*/*     TABLE ENTRY (AS WELL AS THE VOLUNIT ENTRY ITSELF) TO CALCU-  */ 00130000
*/*     LATE THE REQUIRED DATA.                                      */ 00131000
*/*                                                                  */ 00132000
*/* ENTRY - IN-LINE SEGMENT                                          */ 00133000
*/*   PURPOSE - (SEE FUNCTION)                                       */ 00134000
*/*   INPUT -                                                        */ 00135000
*/*     VOLUNIT TABLE                                                */ 00136000
*/*       REQUEST LIST ENTRY                                         */ 00137000
*/*         GROUP LIST ENTRY                                         */ 00138000
*/*           GROUP COUNT ENTRY                                      */ 00139000
*/*     (FOR SPECIFIC FIELDS SEE 'RHDRSPEC')                         */ 00140000
*/*                                                                  */ 00141000
*/* EXITS -                                                          */ 00142000
*/*   CONDITION - FUNCTION COMPLETED                                 */ 00143000
*/*     OUTPUT -                                                     */ 00144000
*/*       TOTUNITS - TOTAL NUMBER OF UNITS NEEDED                    */ 00145000
*/*       ALCUNITS - NUMBER OF ALLOCATED UNITS NEEDED                */ 00146000
*/*       OFLUNITS - NUMBER OF OFFLINE UNITS NEEDED                  */ 00147000
*/*       LCLFLGS - DEVICE POOL ROUTING CODES                        */ 00148000
*/*     RETURN CODES - NONE                                          */ 00149000
*/********************************************************************/ 00150000
*ALCUNITS = ZERO;                      /*INIT ALLOC'D UNITS COUNTER  */ 00151000
         SLR   @09,@09                                          03S0095 00152000
         ST    @09,ALCUNITS                                     03S0095 00153000
*OFLUNITS = ZERO;                      /*INIT OFFLINE UNITS COUNTER  */ 00154000
         ST    @09,OFLUNITS                                     03S0096 00155000
*TOTUNITS = ZERO;                      /*INIT TOTAL UNITS COUNTER    */ 00156000
         ST    @09,TOTUNITS                                     03S0097 00157000
*PRVTOTAL = ZERO;                      /*INIT PREVIOUS TOTAL FIELD   */ 00158000
         SLR   PRVTOTAL,PRVTOTAL                                03S0098 00159000
*VUPTR = VOLUNPTR;                     /*SET PTR TO FIRST VU ENTRY   */ 00160000
         L     @09,@PC00001                                     03S0099 00161000
         L     @09,ALCWAPTR(,@09)                               03S0099 00162000
         L     VUPTR,VOLUNPTR(,@09)                             03S0099 00163000
*DO VUCTR = 1 TO VOLUNSZE/LENGTH(VOLUNTAB); /*SCAN VOLUNIT TABLE     */ 00164000
         LA    VUCTR,1                                          03S0100 00165000
         B     @DE00100                                         03S0100 00166000
@DL00100 DS    0H                                               03S0101 00167000
*  IF VOLALOC = NO &                   /*ENTRY NOT ALLOCATED         */ 00168000
*     VOLALGTP ^= ZERO THEN            /*HAS REQ LIST ENTRY          */ 00169000
         TM    VOLALOC(VUPTR),B'10000000'                       03S0101 00170000
         BNZ   @RF00101                                         03S0101 00171000
         L     @06,VOLALGTP(,VUPTR)                             03S0101 00172000
         LTR   @06,@06                                          03S0101 00173000
         BZ    @RF00101                                         03S0101 00174000
*    DO;                                                        03S0102 00175000
*      RQLSTPTR = VOLALGTP;            /*POINT TO REQUEST LIST ENTRY */ 00176000
         LR    RQLSTPTR,@06                                     03S0103 00177000
*      IF CVRSKFLG = NO THEN           /*ALLOCATABLE BY ALGORITHM    */ 00178000
         TM    CVRSKFLG(RQLSTPTR),B'10000000'                   03S0104 00179000
         BNZ   @RF00104                                         03S0104 00180000
*        IF VURCVYPR = NO THEN         /*AND NOT ALREADY PROC'D      */ 00181000
         TM    VURCVYPR(VUPTR),B'00000001'                      03S0105 00182000
         BNZ   @RF00105                                         03S0105 00183000
*          DO;                                                  03S0106 00184000
*            TOTUNITS = TOTUNITS + ONE;/*INCREMENT TOTAL UNITS COUNT */ 00185000
         LA    @06,1                                            03S0107 00186000
         L     @15,TOTUNITS                                     03S0107 00187000
         ALR   @15,@06                                          03S0107 00188000
         ST    @15,TOTUNITS                                     03S0107 00189000
*            GPLSTPTR = CVRGRPPT;      /*POINT TO GROUP LIST ENTRY   */ 00190000
         L     GPLSTPTR,CVRGRPPT(,RQLSTPTR)                     03S0108 00191000
*            ALCGRPSW = OFF;           /*ASSUME NO GROUP HAS ALOC'DS */ 00192000
*            OFLGRPSW = OFF;           /*ASSUME NO GROUP HAS OFFLINES*/ 00193000
         NI    ALCGRPSW,B'00111111'                             03S0110 00194000
*            DO GRPCTR = 1 TO CVRNGRP; /*CHECK ALL ELIGIBLE GROUPS   */ 00195000
         B     @DE00111                                         03S0111 00196000
@DL00111 DS    0H                                               03S0112 00197000
*              IF CVRGSKP = NO &       /*THIS GROUP CAN COVER REQUEST*/ 00198000
*                  CVRGALL => ZERO THEN                         03S0112 00199000
         TM    CVRGSKP(GPLSTPTR),B'10000000'                    03S0112 00200000
         BNZ   @RF00112                                         03S0112 00201000
         SLR   @06,@06                                          03S0112 00202000
         CH    @06,CVRGALL(,GPLSTPTR)                           03S0112 00203000
         BH    @RF00112                                         03S0112 00204000
*                DO;                                            03S0113 00205000
*                  GPCNTPTR = CVRGCPTR;/*POINT TO GROUP COUNT ENTRY  */ 00206000
         L     GPCNTPTR,CVRGCPTR(,GPLSTPTR)                     03S0114 00207000
*                  IF GRPALLCT > ZERO THEN /*GROUP CONTAINS ALLOC'D     00208000
*                                            UNITS                   */ 00209000
         SLR   @15,@15                                          03S0115 00210000
         CH    @15,GRPALLCT(,GPCNTPTR)                          03S0115 00211000
         BNL   @RF00115                                         03S0115 00212000
*                    ALCGRPSW = ON;    /*SET INDICATOR               */ 00213000
         OI    ALCGRPSW,B'10000000'                             03S0116 00214000
*                  IF GRPOFFLN > ZERO THEN /*GROUP HAS OFFLINE UNITS */ 00215000
@RF00115 ICM   @15,3,GRPOFFLN(GPCNTPTR)                         03S0117 00216000
         BNP   @RF00117                                         03S0117 00217000
*                    OFLGRPSW = ON;    /*SET INDICATOR               */ 00218000
         OI    OFLGRPSW,B'01000000'                             03S0118 00219000
*                END;                                           03S0119 00220000
*              ELSE;                   /*REQ NOT COVERED BY GROUP    */ 00221000
@RF00112 DS    0H                                               03S0121 00222000
*              GPLSTPTR = GPLSTPTR + LENGTH(CVRGPLST); /*POINT TO       00223000
*                                        NEXT GROUP LIST ENTRY       */ 00224000
@RC00112 AH    GPLSTPTR,@CH00065                                03S0121 00225000
*            END;                                               03S0122 00226000
         LA    @06,1                                            03S0122 00227000
         AL    @06,GRPCTR                                       03S0122 00228000
@DE00111 ST    @06,GRPCTR                                       03S0122 00229000
         CH    @06,CVRNGRP(,RQLSTPTR)                           03S0122 00230000
         BNH   @DL00111                                         03S0122 00231000
*            IF ALCGRPSW = YES &       /*ONLY ALLOC'DS IN ELIG GROUPS*/ 00232000
*                OFLGRPSW = NO THEN                             03S0123 00233000
         TM    ALCGRPSW,B'10000000'                             03S0123 00234000
         BNO   @RF00123                                         03S0123 00235000
         TM    OFLGRPSW,B'01000000'                             03S0123 00236000
         BNZ   @RF00123                                         03S0123 00237000
*              ALCUNITS = ALCUNITS + ONE;/*INCREMENT COUNT OF ALLOC'D*/ 00238000
         LA    @06,1                                            03S0124 00239000
         AL    @06,ALCUNITS                                     03S0124 00240000
         ST    @06,ALCUNITS                                     03S0124 00241000
*            IF ALCGRPSW = NO &        /*ONLY OFFLINES IN ELIG GROUPS*/ 00242000
*                OFLGRPSW = YES THEN                            03S0125 00243000
@RF00123 TM    OFLGRPSW,B'01000000'                             03S0125 00244000
         BNO   @RF00125                                         03S0125 00245000
         TM    ALCGRPSW,B'10000000'                             03S0125 00246000
         BNZ   @RF00125                                         03S0125 00247000
*              OFLUNITS = OFLUNITS + ONE;/*INCREMENT COUNT OF OFFLINE*/ 00248000
         LA    @06,1                                            03S0126 00249000
         AL    @06,OFLUNITS                                     03S0126 00250000
         ST    @06,OFLUNITS                                     03S0126 00251000
*          END;                                                 03S0127 00252000
*        ELSE;                         /*ENTRY ALREADY PROCESSED     */ 00253000
@RF00105 DS    0H                                               03S0129 00254000
*      ELSE                            /*ENTRY SKIPPED BY ALGORITHM  */ 00255000
*        IF VOLSIOTP->SIOTDMND = YES & /*DEMAND REQUEST         03S0129 00256000
*                                                                    */ 00257000
*            VUDMUNIQ = YES THEN       /*UNIQUE REQUEST FOR UNIT     */ 00258000
         B     @RC00104                                         03S0129 00259000
@RF00104 L     @06,VOLSIOTP(,VUPTR)                             03S0129 00260000
         TM    SIOTDMND(@06),B'10000000'                        03S0129 00261000
         BNO   @RF00129                                         03S0129 00262000
         TM    VUDMUNIQ(VUPTR),B'00000100'                      03S0129 00263000
         BNO   @RF00129                                         03S0129 00264000
*          DO;                                                  03S0130 00265000
*            TOTUNITS = TOTUNITS + ONE;/*INCREMENT TOTAL UNITS COUNT */ 00266000
         LA    @06,1                                            03S0131 00267000
         L     @03,TOTUNITS                                     03S0131 00268000
         ALR   @03,@06                                          03S0131 00269000
         ST    @03,TOTUNITS                                     03S0131 00270000
*            IF VUDMNDAL = YES THEN    /*ALLOCATED UNIT REQUIRED     */ 00271000
         TM    VUDMNDAL(VUPTR),B'00010000'                      03S0132 00272000
         BNO   @RF00132                                         03S0132 00273000
*              ALCUNITS = ALCUNITS + ONE;/*INCREMENT COUNT OF ALLOC'D*/ 00274000
         AL    @06,ALCUNITS                                     03S0133 00275000
         ST    @06,ALCUNITS                                     03S0133 00276000
*            ELSE                                               03S0134 00277000
*              IF VUDMNDOF = YES THEN  /*OFFLINE UNIT REQUIRED       */ 00278000
         B     @RC00132                                         03S0134 00279000
@RF00132 TM    VUDMNDOF(VUPTR),B'00100000'                      03S0134 00280000
         BNO   @RF00134                                         03S0134 00281000
*                 OFLUNITS = OFLUNITS + ONE; /*INCREMENT COUNT OF       00282000
*                                        OFFLINES                    */ 00283000
         LA    @06,1                                            03S0135 00284000
         AL    @06,OFLUNITS                                     03S0135 00285000
         ST    @06,OFLUNITS                                     03S0135 00286000
*          END;                                                 03S0136 00287000
*        ELSE;                         /*NOT DEMAND REQUEST          */ 00288000
@RF00129 DS    0H                                               03S0138 00289000
*      IF CVRWUCBP ^= ZERO &           /*REQUEST NEEDS ALLOC'D VOLUME*/ 00290000
*         VUVLUNIQ = YES THEN          /*UNIQUE REQ FOR THIS VOLUME  */ 00291000
@RC00129 DS    0H                                               03S0138 00292000
@RC00104 ICM   @06,3,CVRWUCBP(RQLSTPTR)                         03S0138 00293000
         BZ    @RF00138                                         03S0138 00294000
         TM    VUVLUNIQ(VUPTR),B'00000010'                      03S0138 00295000
         BNO   @RF00138                                         03S0138 00296000
*        DO;                                                    03S0139 00297000
*          TOTUNITS = TOTUNITS + ONE;  /*INCREMENT TOTAL UNITS NEEDED*/ 00298000
         LA    @06,1                                            03S0140 00299000
         L     @05,TOTUNITS                                     03S0140 00300000
         ALR   @05,@06                                          03S0140 00301000
         ST    @05,TOTUNITS                                     03S0140 00302000
*          ALCUNITS = ALCUNITS + ONE;  /*INCREMENT COUNT OF ALLOC'DS */ 00303000
         AL    @06,ALCUNITS                                     03S0141 00304000
         ST    @06,ALCUNITS                                     03S0141 00305000
*        END;                                                   03S0142 00306000
*      IF PRVTOTAL < TOTUNITS THEN     /*TOTUNITS WAS INCREMENTED    */ 00307000
@RF00138 C     PRVTOTAL,TOTUNITS                                03S0143 00308000
         BNL   @RF00143                                         03S0143 00309000
*        DO;                           /*UPDATE LCL RTE CODES        */ 00310000
*          IF VOLDAREQ = YES THEN      /*D.A. REQUEST                */ 00311000
         TM    VOLDAREQ(VUPTR),B'00100000'                      03S0145 00312000
         BNO   @RF00145                                         03S0145 00313000
*            LCLFLGD = ON;             /*ROUTE TO D.A. POOL          */ 00314000
         OI    LCLFLGD,B'00010000'                              03S0146 00315000
*          ELSE                        /*NOT D.A. REQUEST            */ 00316000
*            IF VOLTAREQ = YES THEN    /*TAPE REQUEST                */ 00317000
         B     @RC00145                                         03S0147 00318000
@RF00145 TM    VOLTAREQ(VUPTR),B'10000000'                      03S0147 00319000
         BNO   @RF00147                                         03S0147 00320000
*              LCLFLGC = ON;           /*ROUTE TO TAPE POOL          */ 00321000
         OI    LCLFLGC,B'00100000'                              03S0148 00322000
*            ELSE                      /*NOT D.A. OR TAPE            */ 00323000
*              IF VOLURREQ = YES THEN  /*UNIT RECORD REQ             */ 00324000
         B     @RC00147                                         03S0149 00325000
@RF00147 TM    VOLURREQ(VUPTR),B'00001000'                      03S0149 00326000
         BNO   @RF00149                                         03S0149 00327000
*                LCLFLGG = ON;         /*ROUTE TO UNIT RCD POOL      */ 00328000
         OI    LCLFLGG,B'00000010'                              03S0150 00329000
*        END;                                                   03S0151 00330000
*    END;                              /*NO PRC'NG NEEDED FOR VU     */ 00331000
*  ELSE;                                                        03S0153 00332000
@RF00101 DS    0H                                               03S0154 00333000
*  PRVTOTAL = TOTUNITS;                /*SAVE CURRENT TOTAL          */ 00334000
