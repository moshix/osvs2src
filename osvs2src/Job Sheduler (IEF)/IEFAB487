         AH    VUCTR,@CH00069                                   02S0241 00694000
@DE00221 CH    VUCTR,SVOLUNNO(,SIOTPTR)                         02S0241 00695000
         BNH   @DL00221                                         02S0241 00696000
*  ELSE;                               /*REQUEST HAS BEEN ALLOCATED  */ 00697000
@RF00220 DS    0H                                               02S0243 00698000
*  SIOTPTR = SIOTNPTR;                 /*PT TO NEXT SIOT             */ 00699000
         L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       02S0243 00700000
*END;                                                           02S0244 00701000
@DE00211 LTR   SIOTPTR,SIOTPTR                                  02S0244 00702000
         BNZ   @DL00211                                         02S0244 00703000
*    IF SWAITSW = ON THEN              /*MUST WAIT FOR SPECIFIC UNITS*/ 00704000
         TM    SWAITSW,B'10000000'                              01S0245 00705000
         BNO   @RF00245                                         01S0245 00706000
*      DO;                                                      01S0246 00707000
*        COMPRTEC = COMPRTEC | RPLYRTEC;/*SET DEV POOL RTE CODES USED*/ 00708000
         OC    COMPRTEC(1),RPLYRTEC                             01S0247 00709000
*        RPLYOPT = WAITCNCL;           /*SET WAIT/CANCEL OPT CODE    */ 00710000
         MVI   RPLYOPT,X'01'                                    01S0248 00711000
*        CALL IEFAB488(ALCWAPTR,RPLYDATA); /*GIVE OPTS/PROCESS REPLY */ 00712000
         L     @09,@PC00001                                     01S0249 00713000
