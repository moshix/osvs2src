         LA    @14,VDEVRB                                       02S0238 00666000
         ST    @14,VDEVRBAD                                     02S0238 00667000
*        GEN REFS(LINKPLST,VDEVNM,VDEVRBAD) /*INVOK VDEV TO CHECK UCB*/ 00668000
*            (LINK EPLOC=VDEVNM,MF=(E,VDEVRBAD),SF=(E,LINKPLST));       00669000
