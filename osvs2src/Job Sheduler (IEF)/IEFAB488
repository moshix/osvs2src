*/*     INVLRSW - SET ON (1)                                         */ 00573000
*/* EXIT -                                                           */ 00574000
*/*   CONDITION - UCB FOUND                                          */ 00575000
*/*     OUTPUT -                                                     */ 00576000
*/*       INVLRSW = 0                                                */ 00577000
*/*       UCBPTR - PTR TO UCB FOR SPECIFIED DEVICE                   */ 00578000
*/*     RETURN CODE - NONE                                           */ 00579000
*/*   CONDITION - UCB NOT FOUND                                      */ 00580000
*/*     OUTPUT                                                       */ 00581000
*/*       INVLRSW = 1 (SAME AS ON ENTRY)                             */ 00582000
*/*     RETURN CODE - NONE                                           */ 00583000
*/********************************************************************/ 00584000
*UCBFOUND = OFF;                       /*ASSUME UCB NOT IN SYSTEM    */ 00585000
         NI    UCBFOUND,B'11110111'                             03S0213 00586000
*DO LUTCTR = 1 BY 1                                             03S0214 00587000
*    WHILE(IOSLUT(LUTCTR) ^= LUTEND &  /*SCAN IOS LOOK-UP-TABLE ...  */ 00588000
*        UCBFOUND = OFF);              /*... FOR SPECIFIED DEVICE    */ 00589000
         LA    LUTCTR,1                                         03S0214 00590000
         B     @DE00214                                         03S0214 00591000
@DL00214 DS    0H                                               03S0215 00592000
*  IF IOSLUT(LUTCTR) ^= ZERO THEN      /*ENTRY IS NON-ZERO           */ 00593000
         LR    @12,LUTCTR                                       03S0215 00594000
         ALR   @12,@12                                          03S0215 00595000
         L     @11,@PC00001                                     03S0215 00596000
         L     @11,ALCWAPTR(,@11)                               03S0215 00597000
         L     @11,IOSLUTP(,@11)                                03S0215 00598000
         ALR   @11,@12                                          03S0215 00599000
         BCTR  @11,0                                            03S0215 00600000
         BCTR  @11,0                                            03S0215 00601000
         SLR   @12,@12                                          03S0215 00602000
         ICM   @12,3,IOSLUT(@11)                                03S0215 00603000
         LTR   @12,@12                                          03S0215 00604000
         BZ    @RF00215                                         03S0215 00605000
*    DO;                               /*SEE IF THIS REQUESTED DEVICE*/ 00606000
*      UCBPTR = IOSLUT(LUTCTR);        /*SET PTR TO UCB FROM L-U-T   */ 00607000
         LR    UCBPTR,@12                                       03S0217 00608000
*      IF RPLYBUFR(1:3) = UCBNAME      /*THIS IS UNIT REQ'D  @YM07289*/ 00609000
*         & RPLYBUFR(4:6)=BLANKS THEN  /*THREE DIGIT REPLY   @YM07289*/ 00610000
         CLC   RPLYBUFR(3),UCBNAME(UCBPTR)                      03S0218 00611000
         BNE   @RF00218                                         03S0218 00612000
         CLC   RPLYBUFR+3(3),@CC00161                           03S0218 00613000
         BNE   @RF00218                                         03S0218 00614000
*        DO;                           /*CAN TERMINATE SCAN          */ 00615000
*          UCBFOUND = ON;              /*INDICATE UCB FOUND          */ 00616000
*          INVLRSW = OFF;              /*INDICATE VALID REPLY RECV'D */ 00617000
         OI    UCBFOUND,B'00001000'                             03S0221 00618000
