         LR    GPR15P,GPR11P                                    02S0354 01083000
*               GPR00P = GPR12P; /* SAVE REG. 12 IN REG. 0. */  02S0355 01084000
         LR    GPR00P,GPR12P                                    02S0355 01085000
*               GPR01P = GPR13P; /* SAVE REG. 13 IN REG. 1. */  02S0356 01086000
         LR    GPR01P,GPR13P                                    02S0356 01087000
*               GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE TABLE. * 01088000
         L     GPR13P,PSALITA                                   02S0357 01089000
*               GEN(LM GPR11P,GPR13P,552(GPR13P)); /* LOAD LOCKWORD, CL 01090000
