*RFY(REG0,REG1) RSTD;                  /*RSV REGS FOR SYSTEM MACROS  */ 00180000
*REG0 = ZERO;                          /*CLEAR REG0 FOR WTO          */ 00181000
         SLR   REG0,REG0                                        02S0136 00182000
*GEN(WTOR MF=(E,WTORL));               /*ISSUE REPLY OPTIONS MESSAGE */ 00183000
         WTOR MF=(E,WTORL)                                              00184000
*MSGID = REG1;                         /*SAVE MSG ID FOR DOM         */ 00185000
         ST    REG1,MSGID                                       02S0138 00186000
*REG1 = ADDR(RPLYECB);                 /*SET ADDR OF ECB FOR WAIT    */ 00187000
         LA    REG1,RPLYECB                                     02S0139 00188000
*GEN(WAIT ECB=(1),LONG=YES);           /*WAIT FOR REPLY      @YM05089*/ 00189000
         WAIT ECB=(1),LONG=YES                                          00190000
*RFY (REG0,REG1) UNRSTD;               /*RELEASE REGS                */ 00191000
*                                                               02S0141 00192000
* DO; /* DOM MSG(MSGID(2:4))*/                                  02S0142 00193000
*   RFY (GPR00F,GPR01F) RSTD; /*RESTRICT REGISTERS*/            02S0143 00194000
*   GPR01F = MSGID(2:4); /* MOVE IN MESSAGE ID */               02S0144 00195000
         SLR   GPR01F,GPR01F                                    02S0144 00196000
         ICM   GPR01F,7,MSGID+1                                 02S0144 00197000
*   GPR00F = 0; /* SHOW MSG SPECIFIED*/                         02S0145 00198000
         SLR   GPR00F,GPR00F                                    02S0145 00199000
*   SVC (87); /* ISSUE DOM SVC */                               02S0146 00200000
         SVC   87                                               02S0146 00201000
*   RFY (GPR00F,GPR01F) UNRSTD; /* RELEASE REGISTERS */         02S0147 00202000
* END; /* DOM MSG(MSGID(2:4))*/                 /*DELETE THE MESSAGE    00203000
*      */                                                       02S0148 00204000
*ENQCORE(1:ENQLNTH)=ENQLIST(1:ENQLNTH); /*REINITIALIZE ENQ PARM 02S0149 00205000
*                                         LIST               @ZA32215*/ 00206000
