      ENQ (,DDRDAMIN,,5),MF=(E,ENQCORE)                       @ZA32215  00228000
*  END;                                /*END ENQ DDR DA      @ZA32215*/ 00229000
*RPLYBUFR = RPLYBUFR | FOLDRPLY;       /*FOLD REPLY TO UPPER CASE    */ 00230000
@RF00155 OC    RPLYBUFR(6),@CB00184                             02S0159 00231000
*  IF RPLYBUFR = CANCEL THEN           /*REPLY WAS 'CANCEL'          */ 00232000
         CLC   RPLYBUFR(6),@CC00165                             01S0160 00233000
         BNE   @RF00160                                         01S0160 00234000
