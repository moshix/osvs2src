*GEN REFS(ENQCORE);                    /*SERIALIZE UNLOAD,ETC@YM04401*/ 00210000
  ENQ (,ALLOCMIN,,8),MF=(E,ENQCORE)                           @YM04401  00211000
*IF DDRTPUR = ON THEN                  /*ANY TAPE|UR REQUESTS@ZA32215*/ 00212000
         L     @12,@PC00001                                     02S0151 00213000
         L     @12,ALCWAPTR(,@12)                               02S0151 00214000
         TM    DDRTPUR(@12),B'00000100'                         02S0151 00215000
         BNO   @RF00151                                         02S0151 00216000
