*               GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN. */     01093000
         BALR GPR14P,GPR13P                                             01094000
*               GPR11P = GPR15P; /* RESTORE REG. 11. */         02S0360 01095000
         LR    GPR11P,GPR15P                                    02S0360 01096000
*               GPR15P = GPR13P; /* SAVE RETURN CODE. */        02S0361 01097000
         LR    GPR15P,GPR13P                                    02S0361 01098000
*               GPR12P = GPR00P; /* RESTORE REG. 12 */          02S0362 01099000
         LR    GPR12P,GPR00P                                    02S0362 01100000
*               GPR13P = GPR01P; /* RESTORE REG. 13. */         02S0363 01101000
         LR    GPR13P,GPR01P                                    02S0363 01102000
*               RFY(GPR15P,GPR00P,GPR01P) UNRSTD;               02S0364 01103000
*               RFY (GPR11P,GPR12P,GPR13P,GPR14P) UNRSTD;       02S0365 01104000
*             END; /* SETLOCK (RELEASE) TYPE(CMS) REGS(USE) RELATED(UCB 01105000
*IEFAB488(PRCDEV)) */                                           02S0366 01106000
