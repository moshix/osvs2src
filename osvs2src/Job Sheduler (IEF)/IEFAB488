*        END;                                                   03S0222 00620000
*      ELSE;                           /*NO UNIT NAME MATCH WITH RPLY*/ 00621000
@RF00218 DS    0H                                               03S0224 00622000
*    END;                                                       03S0224 00623000
*  ELSE;                               /*ZERO L-U-T ENTRY            */ 00624000
@RF00215 DS    0H                                               03S0226 00625000
*END;                                                           03S0226 00626000
         AH    LUTCTR,@CH00043                                  03S0226 00627000
@DE00214 LR    @12,LUTCTR                                       03S0226 00628000
         ALR   @12,@12                                          03S0226 00629000
         L     @11,@PC00001                                     03S0226 00630000
         L     @11,ALCWAPTR(,@11)                               03S0226 00631000
         L     @11,IOSLUTP(,@11)                                03S0226 00632000
         ALR   @11,@12                                          03S0226 00633000
         BCTR  @11,0                                            03S0226 00634000
         BCTR  @11,0                                            03S0226 00635000
         CLC   IOSLUT(2,@11),LUTEND                             03S0226 00636000
         BE    @DC00214                                         03S0226 00637000
         TM    UCBFOUND,B'00001000'                             03S0226 00638000
         BZ    @DL00214                                         03S0226 00639000
@DC00214 DS    0H                                               03S0227 00640000
*IF INVLRSW = NO THEN                  /*DEVICE REPLIED WAS VALID    */ 00641000
         TM    INVLRSW,B'10000000'                              02S0227 00642000
         BNZ   @RF00227                                         02S0227 00643000
*  DO;                                 /*SEE IF ELIGIBLE TO USE      */ 00644000
*    INVLRSW = ON;                     /*ASSUME DEV NOT USEABLE/ELIG */ 00645000
         OI    INVLRSW,B'10000000'                              02S0229 00646000
*    VDEVRCDE = ZERO;                  /*CLEAR RETURN CODE AREA      */ 00647000
         SLR   VDEVRCDE,VDEVRCDE                                02S0230 00648000
*    IF UCBONLI = NO THEN              /*DEVICE IS OFFLINE           */ 00649000
         TM    UCBONLI(UCBPTR),B'10000000'                      02S0231 00650000
         BNZ   @RF00231                                         02S0231 00651000
*      DO;                             /*SEE IF IT IS USEABLE        */ 00652000
*        VDEVRB = VDEVRB && VDEVRB;    /*CLEAR DEVICE SUBRTN PARMS   */ 00653000
         XC    VDEVRB(8),VDEVRB                                 02S0233 00654000
*        VDEVUCBP = UCBPTR;            /*SET PTR TO UCB IN PARM AREA */ 00655000
         ST    UCBPTR,VDEVUCBP                                  02S0234 00656000
