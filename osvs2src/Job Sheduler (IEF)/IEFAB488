         BNE   @RF00248                                         02S0248 00697000
*            DO;                                                02S0249 00698000
*              VURCVYPR = ON;          /*INDICATE ENTRY PROCESSED    */ 00699000
         OI    VURCVYPR(@12),B'00000001'                        02S0250 00700000
*              INVLRSW = OFF;          /*INDICATE VALID REPLY RECV'D */ 00701000
         NI    INVLRSW,B'01111111'                              02S0251 00702000
*            END;                                               02S0252 00703000
*          ELSE;                       /*NOT VALID REPLY-DEV NOT ELG */ 00704000
@RF00248 DS    0H                                               02S0254 00705000
*        ELSE                          /*NOT DEMAND REQUEST          */ 00706000
*          DO;                                                  02S0254 00707000
         B     @RC00247                                         02S0254 00708000
@RF00247 DS    0H                                               02S0255 00709000
*/********************************************************************/ 00710000
*/*                                                                  */ 00711000
*/* SEGMENT NAME - DMNDNEED                                          */ 00712000
*/*                                                                  */ 00713000
*/* DESCRIPTIVE NAME - TEST IF UCB IS DEMANDED                       */ 00714000
*/*                                                                  */ 00715000
*/* FUNCTION - THIS ROUTINE DETERMINES IF THE INPUT UCB IS           */ 00716000
*/*            NEEDED BY A DEMAND REQUEST (E.G. UNIT=190).           */ 00717000
*/*                                                                  */ 00718000
*/*   OPERATION - THE SIOT CHAIN IS SCANNED TO LOCATE EACH           */ 00719000
*/*               DEMAND REQUEST.                                    */ 00720000
*/*                                                                  */ 00721000
*/* ENTRY - (INLINE SUBROUTINE)                                      */ 00722000
*/*                                                                  */ 00723000
*/*   INPUT -                                                        */ 00724000
*/*      SIOT1P - PTR TO THE FIRST SIOT IN THE CHAIN                 */ 00725000
*/*        SIOTDMND - 1 FOR DEMAND REQUESTS                          */ 00726000
*/*        SIOUCBAD - FOR DEMAND REQUEST, ADDRESS OF THE             */ 00727000
*/*                   DEMANDED UCB                                   */ 00728000
*/*        SIOTNPTR - PTR TO THE NEXT SIOT                           */ 00729000
*/*      UCBPTR - PTR TO UCB                                         */ 00730000
*/*                                                                  */ 00731000
*/* EXIT - NORMAL - INLINE SUBROUTINE                                */ 00732000
*/*                                                                  */ 00733000
*/*   CONDITIONS - FUNCTION COMPLETED                                */ 00734000
*/*                                                                  */ 00735000
*/*   OUTPUT -                                                       */ 00736000
*/*       USEUCB - 1 = NO DEMAND REQ NEEDS THIS UCB                  */ 00737000
*/*              - 0 = A DEMAND REQ NEEDS THIS UCB                   */ 00738000
*/*                                                                  */ 00739000
*/********************************************************************/ 00740000
*USEUCB = ON;                          /*ASSUME UCB NOT DEMANDED     */ 00741000
         OI    USEUCB,B'00000100'                               03S0255 00742000
*RFY INDMSIOT BASED(SPTR);                                      03S0256 00743000
