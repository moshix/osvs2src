         TITLE 'IEFAB488 - RECOVERY REPLY OPTIONS PROCESSOR            *00001000
                        '                                               00002000
IEFAB488 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(24)                                          01S0001 00007000
         DC    C'IEFAB488 78124  UZ18622 '                      01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @10,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@10                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         GSPACE   R,LV=(0)                                              00015000
         LR    @09,@01                                          01S0001 00016000
         USING @DATD,@09                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @09,8(,@13)                                      01S0001 00020000
         LR    @13,@09                                          01S0001 00021000
         MVC   @PC00001(8),0(@01)                               01S0001 00022000
*                                                               01S0105 00023000
*                                                               01S0105 00024000
* /*                   END OF TCB                                    */ 00025000
*RETCODE = ZERO;                       /*INIT RETURN CODE AREA       */ 00026000
         SLR   RETCODE,RETCODE                                  01S0106 00027000
*INTNLSWS = ZERO;                      /*INIT INTERNAL SWITCHES      */ 00028000
         MVI   INTNLSWS,X'00'                                   01S0107 00029000
*INVLRSW = ON;                         /*ASSUME OPTIONS MESSAGE MUST    00030000
*                                        BE REISSUED                 */ 00031000
         OI    INVLRSW,B'10000000'                              01S0108 00032000
*DO WHILE(INVLRSW = ON);               /*REPEAT 'TIL VALID REPLY        00033000
