*TURNBIT1: PROC;                                                02S0479 01500000
TURNBIT1 STM   @14,@12,12(@13)                                  02S0479 01501000
*/*******************************************************************/  01502000
*/*                                                                  */ 01503000
*/* SEGMENT NAME - TURNBIT1                                          */ 01504000
*/*                                                                  */ 01505000
*/* DESCRIPTIVE NAME - TURN ON A BIT IN A GROUP MASK                 */ 01506000
*/*                                                                  */ 01507000
*/* FUNCTION -                                                       */ 01508000
*/*     GIVEN A GROUP ID, THIS ROUTINE TURNS ON THE GROUP MASK       */ 01509000
*/*     BIT ASSOCIATED WITH THAT GROUP ID.                           */ 01510000
*/*                                                                  */ 01511000
*/*   OPERATION - (THE OPERATION IS DOCUMENTED BY THE CODE ITSELF)   */ 01512000
*/*                                                                  */ 01513000
*/* ENTRY - TURNBIT1 (INTERNAL PROC)                                 */ 01514000
*/*                                                                  */ 01515000
*/*   PURPOSE - (SEE FUNCTION)                                       */ 01516000
*/*                                                                  */ 01517000
*/*   INPUT -                                                        */ 01518000
*/*     THEGRPID - GROUP ID                                          */ 01519000
*/*     MASKPTR - PTR TO THE GROUP MASK (GRPMSK)                     */ 01520000
*/*                                                                  */ 01521000
*/* EXIT - NORMAL - RETURN TO CALLER (INTERNAL PROC)                 */ 01522000
*/*                                                                  */ 01523000
*/*   CONDITIONS - BIT TURNED ON IN GROUP MASK                       */ 01524000
*/*                                                                  */ 01525000
*/*   OUTPUT -                                                       */ 01526000
*/*      BIT TURNED ON IN GROUP MASK                                 */ 01527000
*/*                                                                  */ 01528000
*/*   RETURN CODES - (NONE)                                          */ 01529000
*/*                                                                  */ 01530000
*/* EXIT - ERROR - (NONE)                                            */ 01531000
*/*                                                                  */ 01532000
*/********************************************************************/ 01533000
*GRMSKBYT = THEGRPID/LENGTH(GRPMSK);   /*CALCULATE BYTE IN GROUP MASK*/ 01534000
         LA    @12,8                                            02S0480 01535000
         LR    @00,THEGRPID                                     02S0480 01536000
         SRDA  @00,32                                           02S0480 01537000
         DR    @00,@12                                          02S0480 01538000
         LR    GRMSKBYT,@01                                     02S0480 01539000
*GRMSKBIT = THEGRPID//LENGTH(GRPMSK);  /*CALCULATE BIT POSITION OF ID*/ 01540000
         LR    @00,THEGRPID                                     02S0481 01541000
         SRDA  @00,32                                           02S0481 01542000
         D     @00,@CF00041                                     02S0481 01543000
         LR    GRMSKBIT,@00                                     02S0481 01544000
*IF GRMSKBIT = ZERO THEN               /*GROUP ID MULTIPLE OF 8  ?   */ 01545000
         CH    GRMSKBIT,@CH00167                                02S0482 01546000
         BNE   @RF00482                                         02S0482 01547000
*  GRMSKBIT = LENGTH(GRPMSK);          /*YES, SET BIT POSITION = 8   */ 01548000
         LR    GRMSKBIT,@12                                     02S0483 01549000
*ELSE                                  /*GROUP ID NOT MULTIPLE OF 8  */ 01550000
*  GRMSKBYT = GRMSKBYT + ONE;          /*INCREMENT BYTE OF GROUP ID  */ 01551000
         B     @RC00482                                         02S0484 01552000
@RF00482 LA    GRMSKBYT,1(,GRMSKBYT)                            02S0484 01553000
*GRPMSK(GRMSKBYT) =                    /*TURN ON BIT IN GROUP MASK   */ 01554000
*    GRPMSK(GRMSKBYT) | MASKTAB(GRMSKBIT);                      02S0485 01555000
@RC00482 LR    @12,MASKPTR                                      02S0485 01556000
         ALR   @12,GRMSKBYT                                     02S0485 01557000
         BCTR  @12,0                                            02S0485 01558000
         LA    @11,MASKTAB-1(GRMSKBIT)                          02S0485 01559000
         OC    GRPMSK(1,@12),0(@11)                             02S0485 01560000
*END TURNBIT1;                                                  02S0486 01561000
