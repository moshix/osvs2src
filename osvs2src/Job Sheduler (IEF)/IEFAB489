         L     @14,SVOLUNAD(,@15)                               02S0079 00073000
         ST    @14,VUPTR                                        02S0079 00074000
*  IF SIOTDMND = YES &                 /*UNALLOCATED DEMAND REQUEST  */ 00075000
*      SIOTALCD = NO &                                          02S0080 00076000
*      VUDMNDOF = YES THEN             /*NEEDED OFFLINE UNIT         */ 00077000
         TM    SIOTDMND(@15),B'10000000'                        02S0080 00078000
         BNO   @RF00080                                         02S0080 00079000
         TM    SIOTALCD(@15),B'00000010'                        02S0080 00080000
         BNZ   @RF00080                                         02S0080 00081000
         TM    VUDMNDOF(@14),B'00100000'                        02S0080 00082000
         BNO   @RF00080                                         02S0080 00083000
*    DO;                                                        02S0081 00084000
*      EDLPTR = EDLFGENP;              /*POINT TO EDL GENERIC ENTRY  */ 00085000
         L     @01,SIOTEDLP(,@15)                               02S0082 00086000
         L     EDLPTR,EDLFGENP(,@01)                            02S0082 00087000
*      EDLGNTP = EDLGRPTR;             /*GET PTR TO GROUP ENTRY      */ 00088000
         L     @08,EDLGRPTR(,EDLPTR)                            02S0083 00089000
         ST    @08,EDLGNTP                                      02S0083 00090000
*      ARUGRPID = EDLGRID;             /*SET ID OF GROUP BEING PROC'D*/ 00091000
         MVC   ARUGRPID(4),EDLGRID(@08)                         02S0084 00092000
*      RQLSTPTR = VOLALGTP;            /*SET PTR TO REQ LIST ENTRY   */ 00093000
         MVC   RQLSTPTR(4),VOLALGTP(@14)                        02S0085 00094000
*      CALL ALOCREQ;                   /*TRY TO ALLOCATE THIS REQUEST*/ 00095000
         BAL   @14,ALOCREQ                                      02S0086 00096000
*      IF VOLALOC = YES &              /*ENTRY WAS ALLOCATED         */ 00097000
*          SIOTALCD = NO THEN                                   02S0087 00098000
         L     @01,VUPTR                                        02S0087 00099000
         TM    VOLALOC(@01),B'10000000'                         02S0087 00100000
         BNO   @RF00087                                         02S0087 00101000
         L     @15,SIOTPTR                                      02S0087 00102000
         TM    SIOTALCD(@15),B'00000010'                        02S0087 00103000
         BNZ   @RF00087                                         02S0087 00104000
*        DO;                           /*INDIC REQUEST WAS ALLOCATED */ 00105000
*          SIOTALCD = ON;              /*SET INDICATOR IN SIOT       */ 00106000
         OI    SIOTALCD(@15),B'00000010'                        02S0089 00107000
*          TOTREQS = TOTREQS - ONE;    /*DECR. REQS LEFT TO ALLOC    */ 00108000
         L     @15,@PC00001                                     02S0090 00109000
         L     @15,ALCWAPTR(,@15)                               02S0090 00110000
         L     @14,TOTREQS(,@15)                                02S0090 00111000
         BCTR  @14,0                                            02S0090 00112000
         ST    @14,TOTREQS(,@15)                                02S0090 00113000
*        END;                                                   02S0091 00114000
*      ELSE;                           /*ENTRY WASN'T ALLOCATED      */ 00115000
@RF00087 DS    0H                                               02S0093 00116000
*    END;                                                       02S0093 00117000
*  ELSE;                               /*NOT UNALLOC'D DEMAND REQUEST*/ 00118000
@RF00080 DS    0H                                               02S0095 00119000
*  SIOTPTR = SIOTNPTR;                 /*POINT TO NEXT SIOT          */ 00120000
         L     @01,SIOTPTR                                      02S0095 00121000
         L     @15,SIOTNPTR(,@01)                               02S0095 00122000
         ST    @15,SIOTPTR                                      02S0095 00123000
*END;                                                           02S0096 00124000
@DE00078 SLR   @15,@15                                          02S0096 00125000
         C     @15,SIOTPTR                                      02S0096 00126000
         BE    @DC00078                                         02S0096 00127000
         C     @15,RETCODE                                      02S0096 00128000
