         L     @01,SIOTPTR                                      05S0473 01793000
         L     AVUPTR,SVOLUNAD(,@01)                            05S0473 01794000
*DO AVUCTR = 1 TO SVOLUNNO             /*GET POS'N FOR THIS ENTRY    */ 01795000
*   WHILE(AVUPTR ^= VUPTR);            /*SCAN TO ENTRY BEING PROC'D  */ 01796000
         LR    AVUCTR,@03                                       05S0474 01797000
         B     @DE00474                                         05S0474 01798000
