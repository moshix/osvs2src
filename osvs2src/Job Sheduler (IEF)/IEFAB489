*          CVRALLCT = ONE;             /*SET NO. UNITS ALLOCATED     */ 02011000
         STH   @09,CVRALLCT(,@15)                               03S0514 02012000
*          CALL IEFAB480(ALGPARMP);    /*INVOKE COVER/REDUCE         */ 02013000
         ST    @14,@AL00001                                     03S0515 02014000
         L     @15,@CV00152                                     03S0515 02015000
         LA    @01,@AL00001                                     03S0515 02016000
         BALR  @14,@15                                          03S0515 02017000
*          IF REG15 ^= ZERO THEN       /*ALG ERROR                   */ 02018000
         LTR   REG15,REG15                                      03S0516 02019000
         BZ    @RF00516                                         03S0516 02020000
*            DO;                                                03S0517 02021000
*              RETCODE = FAILURE;                               03S0518 02022000
