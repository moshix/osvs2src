*      IF UCBONLI = NO THEN            /*UNIT IS OFFLINE             */ 00243000
         L     @09,UCBPTR                                       01S0106 00244000
         TM    UCBONLI(@09),B'10000000'                         01S0106 00245000
         BNZ   @RF00106                                         01S0106 00246000
*        DO;                                                    01S0107 00247000
*/********************************************************************/ 00248000
*/*                                                                  */ 00249000
*/* SEGMENT NAME - DMNDNEED                                          */ 00250000
*/*                                                                  */ 00251000
*/* DESCRIPTIVE NAME - TEST IF UCB IS DEMANDED                       */ 00252000
*/*                                                                  */ 00253000
*/* FUNCTION - THIS ROUTINE DETERMINES IF THE INPUT UCB IS           */ 00254000
*/*            NEEDED BY A DEMAND REQUEST (E.G. UNIT=190).           */ 00255000
*/*                                                                  */ 00256000
*/*   OPERATION - THE SIOT CHAIN IS SCANNED TO LOCATE EACH           */ 00257000
*/*               DEMAND REQUEST.                                    */ 00258000
*/*                                                                  */ 00259000
*/* ENTRY - (INLINE SUBROUTINE)                                      */ 00260000
*/*                                                                  */ 00261000
*/*   INPUT -                                                        */ 00262000
*/*      SIOT1P - PTR TO THE FIRST SIOT IN THE CHAIN                 */ 00263000
*/*        SIOTDMND - 1 FOR DEMAND REQUESTS                          */ 00264000
*/*        SIOUCBAD - FOR DEMAND REQUEST, ADDRESS OF THE             */ 00265000
*/*                   DEMANDED UCB                                   */ 00266000
*/*        SIOTNPTR - PTR TO THE NEXT SIOT                           */ 00267000
*/*      UCBPTR - PTR TO UCB                                         */ 00268000
*/*                                                                  */ 00269000
*/* EXIT - NORMAL - INLINE SUBROUTINE                                */ 00270000
*/*                                                                  */ 00271000
*/*   CONDITIONS - FUNCTION COMPLETED                                */ 00272000
*/*                                                                  */ 00273000
*/*   OUTPUT -                                                       */ 00274000
*/*       USEUCB - 1 = NO DEMAND REQ NEEDS THIS UCB                  */ 00275000
*/*              - 0 = A DEMAND REQ NEEDS THIS UCB                   */ 00276000
*/*                                                                  */ 00277000
*/********************************************************************/ 00278000
*USEUCB = ON;                          /*ASSUME UCB NOT DEMANDED     */ 00279000
         OI    USEUCB,B'00000100'                               02S0108 00280000
*RFY INDMSIOT BASED(SPTR);                                      02S0109 00281000
*SPTR = SIOT1P;                        /*POINT TO THE FIRST SIOT     */ 00282000
         L     @09,@PC00001                                     02S0110 00283000
         L     @09,ALCWAPTR(,@09)                               02S0110 00284000
         L     SPTR,SIOT1P(,@09)                                02S0110 00285000
*DO WHILE (SPTR ^= ZERO                /*SCAN ALL SIOTS ..           */ 00286000
*    & USEUCB = ON);                   /*..UNTIL UCB FOUND DEMANDED  */ 00287000
         B     @DE00111                                         02S0111 00288000
@DL00111 DS    0H                                               02S0112 00289000
*  IF SIOTDMND = ON                    /*DEMAND REQ FOR THIS UCB ?   */ 00290000
*      & SIOUCBAD = UCBPTR THEN                                 02S0112 00291000
         TM    SIOTDMND(SPTR),B'10000000'                       02S0112 00292000
         BNO   @RF00112                                         02S0112 00293000
         SLR   @09,@09                                          02S0112 00294000
         ICM   @09,7,SIOUCBAD(SPTR)                             02S0112 00295000
         C     @09,UCBPTR                                       02S0112 00296000
         BNE   @RF00112                                         02S0112 00297000
*    DO;                               /*YES,INDICATE UCB IS DEMANDED*/ 00298000
*      USEUCB = OFF;                                            02S0114 00299000
         NI    USEUCB,B'11111011'                               02S0114 00300000
*    END;                                                       02S0115 00301000
*  ELSE;                               /*NOT DEMAND REQ FOR THIS UCB */ 00302000
@RF00112 DS    0H                                               02S0117 00303000
*  SPTR = SIOTNPTR;                    /*POINT TO THE NEXT SIOT      */ 00304000
         L     SPTR,SIOTNPTR(,SPTR)                             02S0117 00305000
*END;                                                           02S0118 00306000
@DE00111 LTR   SPTR,SPTR                                        02S0118 00307000
         BZ    @DC00111                                         02S0118 00308000
         TM    USEUCB,B'00000100'                               02S0118 00309000
         BO    @DL00111                                         02S0118 00310000
@DC00111 DS    0H                                               02S0119 00311000
*RFY INDMSIOT BASED(SIOTPTR);                                   02S0119 00312000
*          IF USEUCB = YES THEN        /*UNIT USEABLE BY NON-DMNDS   */ 00313000
         TM    USEUCB,B'00000100'                               01S0120 00314000
         BNO   @RF00120                                         01S0120 00315000
*            OFFLNCT = OFFLNCT + ONE;  /*INCREMENT OFFLINES COUNT    */ 00316000
         AH    OFFLNCT,@CH00066                                 01S0121 00317000
*        END;                                                   01S0122 00318000
*      ELSE;                           /*UNIT IS ONLINE              */ 00319000
@RF00106 DS    0H                                               01S0124 00320000
*  END;                                                         01S0124 00321000
@RC00106 DS    0H                                               01S0124 00322000
@RF00105 AH    DEVCTR,@CH00066                                  01S0124 00323000
@DE00103 C     DEVCTR,GRPUCBNO(,GRPPTR)                         01S0124 00324000
         BNH   @DL00103                                         01S0124 00325000
*  IF OFFLNCT < GRPOFFLN THEN          /*FEWER OFFLINE UNITS IN GROUP*/ 00326000
         LH    @09,GRPOFFLN(,GPCNTPTR)                          01S0125 00327000
         CR    OFFLNCT,@09                                      01S0125 00328000
         BNL   @RF00125                                         01S0125 00329000
*    DO;                               /*UPDATE COUNT TABLE ENTRY &     00330000
*                                        SEE IF CAN ALLOCATE WITH ANY   00331000
*                                        OF THE UNITS NOW ONLINE     */ 00332000
*      NEWONS = GRPOFFLN - OFFLNCT;    /*NUMBER OF NEW OFFLINE  01S0127 00333000
*                                        DEVICES             @ZA10805*/ 00334000
         SLR   @09,OFFLNCT                                      01S0127 00335000
         ST    @09,NEWONS                                       01S0127 00336000
*      GRPAVAIL = GRPAVAIL+(GRPOFFLN - OFFLNCT);/*UPDATE NO. UNITS      00337000
*                                        AVAILABLE                   */ 00338000
         AH    @09,GRPAVAIL(,GPCNTPTR)                          01S0128 00339000
         STH   @09,GRPAVAIL(,GPCNTPTR)                          01S0128 00340000
*      GRPOFFLN = OFFLNCT;             /*PUT NEW COUNT IN ALG TABLE  */ 00341000
         STH   OFFLNCT,GRPOFFLN(,GPCNTPTR)                      01S0129 00342000
*/********************************************************************/ 00343000
*/*                                                                  */ 00344000
*/* SEGMENT NAME - CHKNDMND                                          */ 00345000
*/*                                                                  */ 00346000
*/* DESCRIPTIVE NAME - CHECK FOR NON-DEMAND REQUESTS TO ALLOCATE     */ 00347000
*/*                                                                  */ 00348000
*/* FUNCTION -                                                       */ 00349000
*/*   THIS ROUTINE DETERMINES IF ANY OF THE UNALLOCATED VOLUNIT      */ 00350000
*/*   ENTRIES CAN BE ALLOCATED FROM THE DEVICE GROUP CURRENTLY       */ 00351000
*/*   BEING PROCESSED. FOR ANY ENTRIES FOUND THE 'ALOCREQ' SUB-      */ 00352000
*/*   ROUTINE IS INVOKED TO ATTEMPT TO ALLOCATE A UNIT FOR THE       */ 00353000
*/*   VU ENTRY.                                                      */ 00354000
*/*                                                                  */ 00355000
*/*   OPERATION -                                                    */ 00356000
*/*     THE ROUTINE WILL SCAN THE VOLUNIT TABLE, STARTING WITH       */ 00357000
*/*     THE ENTRY POINTED TO BY THE INPUT PARAMETER 'FVUPTR'.        */ 00358000
*/*     FOR EACH UNALLOCATED ENTRY WHICH REQUIRES A DEVICE THE       */ 00359000
*/*     ROUTINE WILL DETERMINE IF THE GROUP BEING PROCESSED IS       */ 00360000
*/*     ELIGIBLE TO THE REQUEST. IF IT IS, THE POINTER TO THE        */ 00361000
*/*     CORRESPONDING EDL GROUP ENTRY IS OBTAINED AND THE 'ALOCREQ'  */ 00362000
*/*     SUBROUTINE IS INVOKED.                                       */ 00363000
*/*     IF THE ENTRY IS SUCCESSFULLY ALLOCATED, SEGMENT 'CHKSIOT'    */ 00364000
*/*     WILL DETERMINE IF THIS REQUEST IS NOW COMPLETELY ALLOCATED.  */ 00365000
*/*                                                                  */ 00366000
*/* ENTRY -                                                          */ 00367000
*/*   PURPOSE - SEE FUNCTION                                         */ 00368000
*/*   INPUT -                                                        */ 00369000
*/*     FVUPTR - PTR TO FIRST VOLUNIT ENTRY TO SCAN                  */ 00370000
*/*     GRPIDENT - ID OF GROUP BEING PROCESSED                       */ 00371000
*/*                                                                  */ 00372000
*/* EXITS -                                                          */ 00373000
*/*   CONDITION - NO ENTRIES TO ALLOCATE                             */ 00374000
*/*     OUTPUT - NONE                                                */ 00375000
*/*     RETURN CODES -                                               */ 00376000
*/*       RETCODE = ZERO                                             */ 00377000
*/*   CONDITION - AT LEAST ONE VOLUNIT ENTRY ALLOCATED SUCCESSFULLY  */ 00378000
*/*     OUTPUT -                                                     */ 00379000
*/*       VOLALOC - ON FOR EACH ENTRY ALLOCATED                      */ 00380000
*/*       SIOTALCD - ON IF ENTIRE REQUEST ALLOCATED                  */ 00381000
*/*       TOTREQS - DECREMENTED FOR EACH COMPLETELY ALLOCATED REQUEST*/ 00382000
