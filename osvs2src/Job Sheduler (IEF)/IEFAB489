         BNE   @RF00498                                         05S0498 01970000
*        SIOTRTRY = ON;                /*MUST RETRY THIS REQUEST     */ 01971000
         L     @14,SIOTPTR                                      05S0499 01972000
         OI    SIOTRTRY(@14),B'00000001'                        05S0499 01973000
*    END;                                                       05S0500 01974000
*  ELSE                                /*FAILURE                     */ 01975000
*    DO;                                                        05S0501 01976000
         B     @RC00494                                         05S0501 01977000
@RF00494 DS    0H                                               05S0502 01978000
*      RETCODE = FAILURE;              /*SET AN ERROR RETURN CODE    */ 01979000
         MVC   RETCODE(4),@CF00138                              05S0502 01980000
*    END;                                                       05S0503 01981000
*RFY REG15 UNRSTD;                                              05S0504 01982000
@RC00494 DS    0H                                               05S0504 01983000
@RC00487 DS    0H                                               05S0505 01984000
*  END;                                                         04S0505 01985000
*ELSE;                                 /*ERROR                       */ 01986000
@RF00457 DS    0H                                               04S0507 01987000
*    END;                                                       03S0507 01988000
*  ELSE                                /*REQ IS WAITING FOR VOL      */ 01989000
