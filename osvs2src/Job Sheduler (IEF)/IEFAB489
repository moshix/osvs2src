@RT00480 DS    0H                                               05S0481 01830000
*  DO;                                 /*YES, 434 SHOULD VALIDITY CHK*/ 01831000
*    ARUVCHEK = ON;                                             05S0482 01832000
         OI    ARUVCHEK,B'00010000'                             05S0482 01833000
*  END;                                /*NOT A SPEC VOL REQ          */ 01834000
*ELSE;                                                          05S0484 01835000
@RF00480 DS    0H                                               05S0485 01836000
*    CALL IEFAB434 (SIOTPTR,VUPTR,UCBPTR,ARUFMAP,ETIODATA,      04S0485 01837000
*        ALCWAPTR,ARUGRPID,UCBPARMP);  /*ALLOC REQUEST TO UNIT       */ 01838000
         LA    @15,SIOTPTR                                      04S0485 01839000
