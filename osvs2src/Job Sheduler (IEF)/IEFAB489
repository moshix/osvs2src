         L     @15,@PC00001                                     03S0519 02025000
         L     @01,ALCWAPTR(,@15)                               03S0519 02026000
         L     @01,FAILRSCP(,@01)                               03S0519 02027000
         MVC   RSNCODE(2,@01),@CH00073                          03S0519 02028000
*            END;                                               03S0520 02029000
*          ELSE;                       /*NO ERROR IN ALGORITHM       */ 02030000
@RF00516 DS    0H                                               03S0522 02031000
*        END;                                                   03S0522 02032000
*      ELSE;                           /*REQ NOT CONSIDERED BY ALG   */ 02033000
@RF00510 DS    0H                                               03S0524 02034000
*    END;                                                       03S0524 02035000
*  ELSE;                               /*NOT ENOUGH UNITS IN GROUP      02036000
*                                        TO COVER                    */ 02037000
@RF00417 DS    0H                                               03S0526 02038000
*          END;                                                 02S0526 02039000
*        ELSE;                         /*NO CANDIDATES TO ALLOCATE   */ 02040000
@RF00395 DS    0H                                               02S0528 02041000
*        RFY (REG0,REG1) RSTD;                                  02S0528 02042000
