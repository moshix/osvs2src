*  IF AVUPTR -> VDEVREQD = YES THEN    /*COUNT DEVICE REQ'D ENTRIES  */ 01801000
         TM    VDEVREQD(AVUPTR),B'00100000'                     05S0475 01802000
         BNO   @RF00475                                         05S0475 01803000
*    RELDEVNO = RELDEVNO + ONE;                                 05S0476 01804000
         LH    @05,RELDEVNO                                     05S0476 01805000
         LA    @05,1(,@05)                                      05S0476 01806000
