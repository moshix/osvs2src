         MVC   @TS00001(4),DSABGRID-4(@06)                      02S0534 01704000
         MVC   DSABGRID(4,@05),@TS00001                         02S0534 01705000
*            TIOUCBP(RELDEVNO) = SAVUCBP;                       02S0535 01706000
         AL    @15,@TF00001                                     02S0535 01707000
         STCM  SAVUCBP,7,TIOUCBP-4(@15)                         02S0535 01708000
*            DSABGRID(RELDEVNO) = SAVGRPID;                     02S0536 01709000
         L     @08,@TF00001                                     02S0536 01710000
         ST    SAVGRPID,DSABGRID-4(@08,@05)                     02S0536 01711000
*          END;                                                 02S0537 01712000
*        ELSE;                         /*DEV ENTRY IS DUAL-DENSITY   */ 01713000
@RF00528 DS    0H                                               02S0539 01714000
*      END;                                                     02S0539 01715000
         AL    RELDEVNO,@CF00071                                02S0539 01716000
@DE00526 L     @06,DSABTIOT(,DSABPTR)                           02S0539 01717000
         SLR   @15,@15                                          02S0539 01718000
         IC    @15,TIOEWTCT(,@06)                               02S0539 01719000
         CR    RELDEVNO,@15                                     02S0539 01720000
         BNH   @DL00526                                         02S0539 01721000
@DC00526 DS    0H                                               02S0540 01722000
*    END;                                                       02S0540 01723000
*  ELSE;                               /*DONT PROCESS TIOT ENTRY     */ 01724000
@RF00523 DS    0H                                               02S0542 01725000
*  DSABPTR = DSABFCHN;                 /*POINT TO THE NEXT DSAB      */ 01726000
         L     DSABPTR,DSABFCHN(,DSABPTR)                       02S0542 01727000
*END;                                                           02S0543 01728000
@DE00521 LTR   DSABPTR,DSABPTR                                  02S0543 01729000
         BNZ   @DL00521                                         02S0543 01730000
*      END;                                                     01S0544 01731000
*    ELSE;                             /*NO TIOT ENTRIES TO CHANGE   */ 01732000
@RF00517 DS    0H                                               01S0546 01733000
*    IF RTNCODE = SUCCESS THEN         /*ANY ERRORS  ?               */ 01734000
         LTR   RTNCODE,RTNCODE                                  01S0546 01735000
         BNZ   @RF00546                                         01S0546 01736000
*      DO;                             /*NO ERRORS                   */ 01737000
