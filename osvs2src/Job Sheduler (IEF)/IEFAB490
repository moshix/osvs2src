         TM    DUMSW,B'00000011'                                02S0457 01412000
         BNO   @RF00457                                         02S0457 01413000
*                DO;                   /*YES -               @ZA03895*/ 01414000
*                  RTNCODE = FAILURE;  /*INDICATE ERROR      @ZA03895*/ 01415000
         LA    RTNCODE,4                                        02S0459 01416000
*                  SIOTRSNC = ERRCD18; /*'INVALID DUMMY' CODE@ZA03895*/ 01417000
         MVC   SIOTRSNC(2,SIOTPTR),@CH00160                     02S0460 01418000
*                  SIOTALCD = OFF;     /*SIOT NOT FULLY ALLOC@ZA03895*/ 01419000
         NI    SIOTALCD(SIOTPTR),B'11111101'                    02S0461 01420000
*                END;                  /*                    @ZA03895*/ 01421000
*              ELSE                    /*NO ERROR -          @ZA03895*/ 01422000
*                DO;                   /*                    @ZA03895*/ 01423000
         B     @RC00457                                         02S0463 01424000
@RF00457 DS    0H                                               02S0464 01425000
*                  IF ADSP = ON  & NOADSP = ON THEN             02S0464 01426000
         TM    ADSP,B'11000000'                                 02S0464 01427000
         BNO   @RF00464                                         02S0464 01428000
*                                      /*IF ADSP IS ON + OFF @Z40RPPE*/ 01429000
*                    DO;               /*                    @Z40RPPE*/ 01430000
*                      RTNCODE = FAILURE; /*SET RETURN CODE  @Z40RPPE*/ 01431000
         LA    RTNCODE,4                                        02S0466 01432000
*                      SIOTRSNC = ERRCD13; /*SET 'ADSP ON/OFF' CODE     01433000
*                                                            @Z40RPPE*/ 01434000
         MVC   SIOTRSNC(2,SIOTPTR),@CH00107                     02S0467 01435000
*                      SIOTALCD = OFF; /*SIOT NOT FULLY ALOC.@Z40RPPE*/ 01436000
         NI    SIOTALCD(SIOTPTR),B'11111101'                    02S0468 01437000
*                    END;              /*                    @Z40RPPE*/ 01438000
*                  ELSE                /*                    @Z40RPPE*/ 01439000
*                    DO;               /*                    @Z40RPPE*/ 01440000
         B     @RC00464                                         02S0470 01441000
@RF00464 DS    0H                                               02S0471 01442000
*                      IF NEWSW = ON   /*END OF NEW ISAM CONCAT ?    */ 01443000
*                        & (SIOTNPTR = ZERO |                   02S0471 01444000
*                        SIOTNPTR->SIOTCCAT = OFF) THEN         02S0471 01445000
