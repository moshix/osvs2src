*IF INPUTRC=SUCCESS &                                           01S0110 00039000
*   RTNCODE=SUCCESS &                                           01S0110 00040000
*   (REQUSW=ON | RETRYSW=ON) THEN/*                             01S0110 00041000
*                                        IS ALLOC TO BE REDONE       */ 00042000
