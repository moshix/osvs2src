@DL00476 LTR   RTNCODE,RTNCODE                                  03S0476 01510000
         BNZ   @DC00476                                         03S0476 01511000
*  IF TIOUCBP(ONE) ^= ZERO THEN        /*NULLFILE REQ        @ZA03895*/ 01512000
         L     @09,SIOTETIO(,SPTR)                              03S0477 01513000
         L     @09,DSABTIOT(,@09)                               03S0477 01514000
         SLR   @02,@02                                          03S0477 01515000
         ICM   @02,7,TIOUCBP(@09)                               03S0477 01516000
         LTR   @02,@02                                          03S0477 01517000
         BZ    @RF00477                                         03S0477 01518000
*    DO;                               /*NO--                @ZA03895*/ 01519000
*      CURDSABP = ISAMDSAB(ISAMCTR);   /*COPY PTR TO THIS DSAB       */ 01520000
         LR    @09,ISAMCTR                                      03S0479 01521000
         SLA   @09,2                                            03S0479 01522000
         L     @09,ISAMDSAB-4(@09)                              03S0479 01523000
         ST    @09,CURDSABP                                     03S0479 01524000
*      SAVUPTR = TIOUCBP(ONE);         /*PTR TO UCB TO GET SPACE FROM*/ 01525000
         ST    @02,SAVUPTR                                      03S0480 01526000
*      IF JFCBNVOL = ZERO THEN         /*NON-SPECIFIC REQUEST        */ 01527000
         L     @09,SJFCBPTR(,SPTR)                              03S0481 01528000
         CLI   JFCBNVOL(@09),0                                  03S0481 01529000
         BNE   @RF00481                                         03S0481 01530000
*        DO;                                                    03S0482 01531000
*          JFCBNVOL = ONE;             /*INDICATE 1 VOL IN JFCB      */ 01532000
         MVI   JFCBNVOL(@09),X'01'                              03S0483 01533000
*          JFCBVOLS(1:6) = SAVUPTR->UCBVOLI; /*VOLSER TO JFCB        */ 01534000
         MVC   JFCBVOLS(6,@09),UCBVOLI(@02)                     03S0484 01535000
*        END;                                                   03S0485 01536000
*      RFY (REG0,REG1,REG2) RSTD;                               03S0486 01537000
@RF00481 DS    0H                                               03S0487 01538000
*      REG0 = SJFCBPTR;                /*PTR TO THIS JFCB            */ 01539000
         L     REG0,SJFCBPTR(,SPTR)                             03S0487 01540000
*      REG1 = ADDR(SAVUPTR);           /*PTR TO UCB PTR FOR DADSM    */ 01541000
         LA    REG1,SAVUPTR                                     03S0488 01542000
*      REG2 = ADDR(ISAMPLST);          /*PTR TO LIST OF DDAB PTRS    */ 01543000
         LA    REG2,ISAMPLST                                    03S0489 01544000
*      GEN (SVC 32);                   /*REQUEST SPACE FROM DADSM    */ 01545000
         SVC 32                                                         01546000
*      RFY (REG0,REG1,REG2) UNRSTD;                             03S0491 01547000
*      RTNCODE = REG15;                /*SAVE THE RETURN CODE        */ 01548000
         LR    RTNCODE,REG15                                    03S0492 01549000
*      IF RTNCODE ^= SUCCESS THEN      /*WAS DADSM SUCCESSFUL ?      */ 01550000
         LTR   RTNCODE,RTNCODE                                  03S0493 01551000
         BZ    @RF00493                                         03S0493 01552000
*        DO;                                                    03S0494 01553000
*          SIOTRSNC=ERORCODE(RTNCODE/FOUR); /*CALC REASON CODE       */ 01554000
         LR    @00,RTNCODE                                      03S0495 01555000
         SRDA  @00,32                                           03S0495 01556000
         D     @00,@CF00068                                     03S0495 01557000
         ALR   @01,@01                                          03S0495 01558000
         LH    @09,ERORCODE-2(@01)                              03S0495 01559000
         STH   @09,SIOTRSNC(,SPTR)                              03S0495 01560000
*          SIOTALCD = OFF;                                      03S0496 01561000
         NI    SIOTALCD(SPTR),B'11111101'                       03S0496 01562000
*        END;                                                   03S0497 01563000
*      ELSE                            /*DADSM WAS SUCCESSFUL        */ 01564000
*        SIODADSM = ON;                /*INDICATE DADSM SUCCESSFUL   */ 01565000
         B     @RC00493                                         03S0498 01566000
@RF00493 OI    SIODADSM(SPTR),B'00000100'                       03S0498 01567000
*      SPTR = SIOTNPTR;                /*POINT TO NEXT SIOT          */ 01568000
@RC00493 L     SPTR,SIOTNPTR(,SPTR)                             03S0499 01569000
*    END;                              /*                    @ZA03895*/ 01570000
*END;                                                           03S0501 01571000
@RF00477 AL    ISAMCTR,@CF00071                                 03S0501 01572000
@DE00476 C     ISAMCTR,ISAMNUM                                  03S0501 01573000
         BNH   @DL00476                                         03S0501 01574000
@DC00476 DS    0H                                               03S0502 01575000
*RFY INDMSIOT BASED(SIOTPTR);                                   03S0502 01576000
