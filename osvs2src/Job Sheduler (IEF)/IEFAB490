         TITLE 'IEFAB490 - FINISHUP AND CLEANUP ALLOC                  *00001000
                        '                                               00002000
IEFAB490 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFAB490  77.020'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  LA    @11,4095(,@12)                                   01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         USING @PSTART+4095,@11                                 01S0001 00014000
         L     @00,@SIZDATD                                     01S0001 00015000
         GSPACE   R,LV=(0)                                              00016000
         LR    @10,@01                                          01S0001 00017000
         USING @DATD,@10                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         LM    @00,@01,20(@13)                                  01S0001 00020000
         ST    @10,8(,@13)                                      01S0001 00021000
         LR    @13,@10                                          01S0001 00022000
         MVC   @PC00001(12),0(@01)                              01S0001 00023000
JSCBDISP EQU 188                                                        00024000
JSCBAOS1 EQU  72                                                        00025000
JSCBAOS2 EQU 192                                                        00026000
*                              CATALOG ALLOCATION ISSUE CALL    01S0106 00027000
*                              IEFAB4F5                      @Z40FPPJ*/ 00028000
*RTNCODE = SUCCESS;                    /*INITIALIZE RETURN CODE      */ 00029000
         SLR   RTNCODE,RTNCODE                                  01S0107 00030000
*CLENUPFM = INDICATE;                  /*COPY INDICATORS FROM ALCWA  */ 00031000
