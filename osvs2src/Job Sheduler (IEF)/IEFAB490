         L     @14,TIOUCBP-5(@06,@15)                           02S0527 01684000
         LA    @14,0(,@14)                                      02S0527 01685000
         LR    UCBPTR,@14                                       02S0527 01686000
*        IF UCB2OPT2=OFF & UCB2OPT3=OFF THEN /*DUAL DENSITY ?        */ 01687000
         TM    UCB2OPT2(UCBPTR),B'00110000'                     02S0528 01688000
         BNZ   @RF00528                                         02S0528 01689000
*          DO;                         /*NO,SWITCH WITH 1ST DEV TRY  */ 01690000
*            DUALCHG = ON;             /*INDICATE SWITCH             */ 01691000
         OI    DUALCHG,B'10000000'                              02S0530 01692000
*            SAVUCBP = TIOUCBP(1);     /*SAVE 1ST DEVICE ENTRY       */ 01693000
         SLR   SAVUCBP,SAVUCBP                                  02S0531 01694000
         ICM   SAVUCBP,7,TIOUCBP(@15)                           02S0531 01695000
*            SAVGRPID = DSABGRID(1);   /*SAVE 1ST GROUP ID           */ 01696000
         L     @05,DSABGIDP(,DSABPTR)                           02S0532 01697000
         L     SAVGRPID,DSABGRID(,@05)                          02S0532 01698000
*            TIOUCBP(1) = TIOUCBP(RELDEVNO);                    02S0533 01699000
         STCM  @14,7,TIOUCBP(@15)                               02S0533 01700000
*            DSABGRID(1) = DSABGRID(RELDEVNO);                  02S0534 01701000
         ST    @06,@TF00001                                     02S0534 01702000
