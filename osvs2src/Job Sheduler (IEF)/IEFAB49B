*                         GPR15P = GPR13P; /* SAVE RETURN CODE. */      01031000
         LR    GPR15P,GPR13P                                    01S0383 01032000
*                         GPR12P = GPR00P; /* RESTORE REG. 12 */        01033000
         LR    GPR12P,GPR00P                                    01S0384 01034000
*                         GPR13P = GPR01P; /* RESTORE REG. 13. */       01035000
         LR    GPR13P,GPR01P                                    01S0385 01036000
*                         RFY(GPR15P,GPR00P,GPR01P) UNRSTD;     01S0386 01037000
*                         RFY (GPR11P,GPR12P,GPR13P,GPR14P) UNRSTD;     01038000
*                       END; /* SETLOCK (RELEASE) TYPE(LOCAL) REGS(USE) 01039000
*RELATED(LOCAL LOCK,IEFAB49B(IEFAB49B)) */                      01S0388 01040000
*                       DO; /*MODESET  EXTKEY(SCHED)*/          01S0389 01041000
*                         SPKA (16); /*SET PSW KEY*/            01S0390 01042000
         LA    @12,16                                           01S0390 01043000
         SPKA  0(@12)                                           01S0390 01044000
*                       END; /*MODESET  EXTKEY(SCHED)*/ /*ISSUE MODESET 01045000
*FOR                                              SCHED KEY           @ 01046000
*MO6008*/                                                       01S0391 01047000
*                  END;                /*                    @YM06008*/ 01048000
*                END;                                           01S0393 01049000
*              ELSE;                   /*VOLUME NOT YET MOUNTED      */ 01050000
@RF00069 DS    0H                                               01S0395 01051000
*            END;                                               01S0395 01052000
*          ELSE                                                 01S0396 01053000
*            DECRCNT = ON;             /*DECR COUNT & CLEAR VER PEND */ 01054000
         B     @RC00067                                         01S0396 01055000
@RF00067 OI    DECRCNT,B'10000000'                              01S0396 01056000
*          IF DECRCNT = ON THEN        /*SUCCESSFUL VERIFY ?         */ 01057000
@RC00067 TM    DECRCNT,B'10000000'                              01S0397 01058000
         BNO   @RF00397                                         01S0397 01059000
*            DO;                       /*YES                         */ 01060000
*              MTVRYVFY(DEVCTR) = ON; /*SET VERIFY COMPLETED         */ 01061000
         LR    @12,DEVCTR                                       01S0399 01062000
         MH    @12,@CH01603                                     01S0399 01063000
         L     @11,MTDEVPTR(,MTENTPTR)                          01S0399 01064000
         ALR   @11,@12                                          01S0399 01065000
         AL    @11,@CF01615                                     01S0399 01066000
         OI    MTVRYVFY(@11),B'00100000'                        01S0399 01067000
*              MVNUMWTS = MVNUMWTS - ONE;/*DECREMENT NUM. WAITS TO DO*/ 01068000
         L     @12,@PC00001                                     01S0400 01069000
         L     @12,MVCAPTR(,@12)                                01S0400 01070000
         L     @12,MVCAXPTR(,@12)                               01S0400 01071000
         LH    @11,MVNUMWTS(,@12)                               01S0400 01072000
         BCTR  @11,0                                            01S0400 01073000
         STH   @11,MVNUMWTS(,@12)                               01S0400 01074000
*              DECRCNT = OFF;          /*INDIC NO DECREMENT REQ'D       01075000
*                                                            @YM05454*/ 01076000
         NI    DECRCNT,B'01111111'                              01S0401 01077000
*            END;                                               01S0402 01078000
*          ELSE;                       /*NO DECREMENT NEEDED         */ 01079000
@RF00397 DS    0H                                               01S0404 01080000
*        END;                                                   01S0404 01081000
*      ELSE;                           /*NO VERIFY NEEDED            */ 01082000
@RF00064 DS    0H                                               01S0406 01083000
*    END;                                                       01S0406 01084000
         AH    DEVCTR,@CH00100                                  01S0406 01085000
@DE00063 C     DEVCTR,NODEVENT                                  01S0406 01086000
         BNH   @DL00063                                         01S0406 01087000
@DC00063 DS    0H                                               01S0407 01088000
*    MTENTPTR = MTNXTPTR;              /*POINT TO NEXT MOUNT ENTRY   */ 01089000
         L     MTENTPTR,MTNXTPTR(,MTENTPTR)                     01S0407 01090000
*  END;                                                         01S0408 01091000
@DE00061 SLR   @12,@12                                          01S0408 01092000
         CR    MTENTPTR,@12                                     01S0408 01093000
         BE    @DC00061                                         01S0408 01094000
         CR    RETCODE,@12                                      01S0408 01095000
         BE    @DL00061                                         01S0408 01096000
@DC00061 DS    0H                                               01S0409 01097000
*RETURN CODE(RETCODE);                                          01S0409 01098000
         L     @13,4(,@13)                                      01S0409 01099000
         L     @00,@SIZDATD                                     01S0409 01100000
         LR    @01,@09                                          01S0409 01101000
         FSPACE   R,LV=(0),A=(1)                                        01102000
         LR    @15,@08                                          01S0409 01103000
         L     @14,12(,@13)                                     01S0409 01104000
         LM    @00,@12,20(@13)                                  01S0409 01105000
         BR    @14                                              01S0409 01106000
*/********************************************************************/ 01107000
*/*                                                                  */ 01108000
*/*  SEGMENT NAME - VMVENQDQ                                         */ 01109000
*/*                                                                  */ 01110000
*/* DESCRIPTIVE NAME - VM&V ENQ/DEQ ROUTINE                          */ 01111000
*/*                                                                  */ 01112000
*/* FUNCTION - THIS ROUTINE ENQ'S ON OR DEQ'S FROM THE VM&V RESOURCE.*/ 01113000
*/*   THE VM&V RESOURCE IS THE ADDRESS OF THE UCB TO BE      @ZA07519*/ 01114000
*/*   VERIFIED. THIS PREVENTS TWO OR MORE VERIFY'S FROM              */ 01115000
*/*   VERIFING THE SAME VOLUME SERIAL.                               */ 01116000
*/*   THE ADDRESS OF THE UCB IS SET UP IN A FIELD,           @ZA07519*/ 01117000
*/*   ENQRNAME, AND USED AS THE MINOR NAME FOR BOTH ENQ AND  @ZA07519*/ 01118000
*/*   DEQ.  THE UNIT NAME CAN NOT BE USED DIRECTLY FROM THE  @ZA07519*/ 01119000
*/*   UCB AND SET UP AT BOTH ENQ AND DEQ BECAUSE DDR COULD   @ZA07519*/ 01120000
*/*   OCCUR AFTER ENQ AND BEFORE DEQ. DDR SWAPS THE EBCDIC   @ZA07519*/ 01121000
*/*   UNIT NAME IN THE UCB'S AND SO THE MINOR NAME AT DEQ    @ZA07519*/ 01122000
*/*   WOULD NOT MATCH THE MINOR NAME AT ENQ CAUSING A 138    @ZA07519*/ 01123000
*/*   ABEND.  THE ADDRESS OF THE UCB REMAINS UNCHANGED BY    @ZA07519*/ 01124000
*/*   DDR AND REPRESENTS THE SAME RESOURCE ACROSS JOBS.      @ZA07519*/ 01125000
*/*                                                                  */ 01126000
*/* ENTRY - INLINE PROCEDURE.                                        */ 01127000
*/*                                                                  */ 01128000
*/*   PURPOSE - SEE FUNCTION.                                        */ 01129000
*/*   INPUT -                                                        */ 01130000
*/*     ENQDQSW - ONE BYTE OF INDICATORS                             */ 01131000
*/*               BIT 0 = 1, ENQ ON VM&V RESOURCE                    */ 01132000
*/*               BIT 1 = 1, DEQ FROM VM&V RESOURCE                  */ 01133000
*/*     UCBNAME - PRINTABLE UCB ADDRESS,IN UCB                       */ 01134000
*/* EXIT -                                                           */ 01135000
*/*                                                                  */ 01136000
*/*   CONDITION - VM&V RESOURCE SUCCESSFULLY OBTAINED OR RELEASED    */ 01137000
*/*     OUTPUT - VM&V RESOURCE OBTAINED OR RELEASED.                 */ 01138000
*/*                                                                  */ 01139000
*/*   RETCODE -                                                      */ 01140000
*/*       NONE                                                       */ 01141000
*/*                                                                  */ 01142000
*/*                                                                  */ 01143000
*/********************************************************************/ 01144000
*VMVENQDQ: PROC;                                                02S0410 01145000
VMVENQDQ STM   @14,@12,@SA00002                                 02S0410 01146000
*IF DOENQ = ON THEN                    /*ENQ REQUESTED ?             */ 01147000
         TM    DOENQ,B'10000000'                                02S0411 01148000
         BNO   @RF00411                                         02S0411 01149000
*  DO;                                                          02S0412 01150000
*    ENQCORE(1:ENQLTH) = ENQLST(1:ENQLTH); /*MOVE LIST TO DSECT      */ 01151000
         L     @12,ENQLTH                                       02S0413 01152000
         BCTR  @12,0                                            02S0413 01153000
         EX    @12,@SM01627                                     02S0413 01154000
*    RFY (REG1,REG2) RSTD;                                      02S0414 01155000
*    ENQRNAME = UCBPTR;                /*SET UP MINOR NAME   @ZA07519*/ 01156000
         MVC   ENQRNAME(4),UCBPTR                               02S0415 01157000
*    REG2 = ADDR(ENQRNAME);            /*ADDR OF MINOR       @ZA07519*/ 01158000
         LA    REG2,ENQRNAME                                    02S0416 01159000
*    GEN REFS(ENQCORE);                                         02S0417 01160000
    ENQ (RESNAME,(2),E,4,SYSTEM),MF=(E,ENQCORE)               ZA07519   01161000
*    RFY(REG1,REG2) UNRSTD;                                     02S0418 01162000
*  END;                                                         02S0419 01163000
*ELSE;                                 /*ENQ NOT REQUESTED           */ 01164000
@RF00411 DS    0H                                               02S0421 01165000
*IF DODEQ = ON THEN                    /*DEQ REQUESTED ?             */ 01166000
         TM    DODEQ,B'01000000'                                02S0421 01167000
         BNO   @RF00421                                         02S0421 01168000
*  DO;                                                          02S0422 01169000
*    DEQCORE(1:DEQLTH) = DEQLST(1:DEQLTH);/*MOVE LIST TO DSECT       */ 01170000
         L     @12,DEQLTH                                       02S0423 01171000
         BCTR  @12,0                                            02S0423 01172000
         EX    @12,@SM01629                                     02S0423 01173000
*    RFY (REG1,REG2) RSTD;                                      02S0424 01174000
*    REG2 = ADDR(ENQRNAME);            /*ADDR OF MINOR       @ZA07519*/ 01175000
         LA    REG2,ENQRNAME                                    02S0425 01176000
*    GEN REFS(DEQCORE);                                         02S0426 01177000
    DEQ (RESNAME,(2),4,SYSTEM),MF=(E,DEQCORE)                 ZA07519   01178000
*    RFY (REG1,REG2) UNRSTD;                                    02S0427 01179000
*  END;                                                         02S0428 01180000
*ELSE;                                                          02S0429 01181000
@RF00421 DS    0H                                               02S0430 01182000
*END;                                                           02S0430 01183000
@EL00002 DS    0H                                               02S0430 01184000
@EF00002 DS    0H                                               02S0430 01185000
@ER00002 LM    @14,@12,@SA00002                                 02S0430 01186000
         BR    @14                                              02S0430 01187000
*END IEFAB49B                                                   01S0431 01188000
