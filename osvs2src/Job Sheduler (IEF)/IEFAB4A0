         B     @RC00210                                         02S0216 00600000
@RF00210 DS    0H                                               02S0217 00601000
*/********************************************************************/ 00602000
*/*                                                                  */ 00603000
*/* SEGMENT NAME - BILDDSRL                                          */ 00604000
*/*                                                                  */ 00605000
*/* DESCRIPTIVE NAME - BUILD DATA SET RELEASE LIST                   */ 00606000
*/*                                                                  */ 00607000
*/* FUNCTION -                                                       */ 00608000
*/*   THIS ROUTINE WILL BUILD A LIST OF POINTERS TO THE DATA SET     */ 00609000
*/*   NAMES FOR THE DATA SETS WHICH MAY BE RELEASED. THE DATA SET    */ 00610000
*/*   RELEASE ROUTINE WILL RELEASE (DEQ) THOSE IN THE LIST WHICH     */ 00611000
*/*   ARE NO LONGER NEEDED BY THIS JOB.                              */ 00612000
*/*                                                                  */ 00613000
*/*   OPERATION -                                                    */ 00614000
*/*     .CORE FOR THE DATA SET RELEASE LIST IS OBTAINED. THE AMOUNT  */ 00615000
*/*      OF CORE REQUESTED IS DETERMINED FROM THE NUMBER OF REQUESTS */ 00616000
*/*      FOR NON-VAM DATA SETS FOR WHICH DATA SET RELEASE WAS        */ 00617000
*/*      REQUESTED.   IF NO CORE IS AVAILABLE FOR THE LIST THE       */ 00618000
*/*      DATA SET RELEASE FUNCTION IS BYPASSED.                      */ 00619000
*/*     .IF CORE IS REQUESTED AND OBTAINED, THE LIST IS BUILT. EACH  */ 00620000
*/*      LIST ENTRY WILL CONTAIN A POINTER TO THE DATA SET NAME AND  */ 00621000
*/*      THE LENGTH OF THAT DATA SET NAME. (NOTE THAT THE DATA SET   */ 00622000
*/*      NAME CAN CONTAIN EMBEDDED BLANKS. THUS TO DETERMINE THE     */ 00623000
*/*      NUMBER OF NON-BLANK CHARACTERS IT IS NECESSARY TO SCAN THE  */ 00624000
*/*      NAME FROM RIGHT TO LEFT.)                                   */ 00625000
*/*                                                                  */ 00626000
*/*     .THE DATA SET RELEASE ROUTINE, IEFAB4A6, IS INVOKED TO       */ 00627000
*/*      RELEASE THOSE  DATA SETS WHICH ARE NO LONGER NEEDED BY THIS */ 00628000
*/*      JOB. ON RETURN FROM DATA SET RELEASE THE CORE FOR THE DATA  */ 00629000
*/*      SET RELEASE LIST IS FREED.                                  */ 00630000
*/*                                                                  */ 00631000
*/* ENTRY - IN-LINE SEGMENT                                          */ 00632000
*/*   PURPOSE - SEE FUNCTION                                         */ 00633000
*/*   INPUT -                                                        */ 00634000
*/*    FRBPTR - POINTER TO FIRST REQUEST BLOCK ON THE CHAIN          */ 00635000
*/*       CMSIOPTR - PTR FROM REQUEST BLOCK TO SIOT                  */ 00636000
*/*         SJFCBPTR - PTR FROM SIOT TO JFCB                         */ 00637000
*/*           JFCBDSNM - DATA SET NAME                               */ 00638000
*/*         SIOVAMDS - VAM DATA SET INDICATOR                        */ 00639000
*/*       CMNODISP - NO PROCESSING REQUIRED FOR REQUEST              */ 00640000
*/*       CMNODSR - NO DATA SET RELEASE REQUIRED FOR REQUEST         */ 00641000
*/*     NQTABSVA - SVA OF DATA SET ENQUEUE TABLE                     */ 00642000
*/*     NUMENTS - NUMBER OF NON-TEMPORARY DATA SETS FOR WHICH DATA   */ 00643000
*/*       SET RELEASE WAS REQUESTED                                  */ 00644000
*/*     STEPNUM - CURRENT STEP NUMBER                                */ 00645000
*/*     JSCBTCBP - PTR TO INITIATOR'S TCB                            */ 00646000
*/*                                                                  */ 00647000
*/* EXIT -                                                           */ 00648000
*/*   CONDITION - DATA SET RELEASE PERFORMED,IF NECESSARY            */ 00649000
*/*     OUTPUT -                                                     */ 00650000
*/*       DATA SET NAMES AVAILABLE FOR USE                           */ 00651000
*/*       RSNCODE - SAME AS ON ENTRY                                 */ 00652000
*/*     RETURN CODES -                                               */ 00653000
*/*       RETCODE = 0                                                */ 00654000
*/*                                                                  */ 00655000
*/*   CONDITION - NO CORE AVAILABLE FOR DATA SET RELEASE LIST        */ 00656000
*/*     OUTPUT -                                                     */ 00657000
*/*       RSNCODE = 2                                                */ 00658000
*/*     RETURN CODES -                                               */ 00659000
*/*       RETCODE = 4                                                */ 00660000
*/********************************************************************/ 00661000
*DSRLSZE = NUMENTS * LENGTH(DSRLENT)   /*CALC CORE FOR LIST          */ 00662000
*    + LENGTH(DSRLHDR);                                         03S0217 00663000
         L     @15,NUMENTS                                      03S0217 00664000
         SLA   @15,3                                            03S0217 00665000
         AL    @15,@CF00185                                     03S0217 00666000
         ST    @15,DSRLSZE                                      03S0217 00667000
* /* ?GETSTORG (SPID,DSRLSZE,DSRLPTR);*/                        03S0218 00668000
*   /* DEFINITIONS OF GETMAIN PARMS */                          03S0218 00669000
*   DCL                                                         03S0218 00670000
*     GTSTR0   FIXED(31) REG(0),/*GTMN PARM REG*/               03S0218 00671000
*     GTSTR1   FIXED(31) REG(1),/*GTMN PARM REG*/               03S0218 00672000
*     GTSTR2   PTR(31)   REG(2),/*GTMN PARM REG*/               03S0218 00673000
*     GTSTR15  FIXED(31) REG(15),/*GTMN RET COD*/               03S0218 00674000
*     GTSTRTCD FIXED(31); /* GETMAIN RTN CODE  */               03S0218 00675000
*   DCL                                                         03S0219 00676000
*     GTSTR3   FIXED(31) REG(3),/* CLEARING REG*/               03S0219 00677000
*     GTSTR4   FIXED(31) REG(4),/* CLEARING REG*/               03S0219 00678000
*     GTSTR5   FIXED(31) REG(5),/* CLEARING REG*/               03S0219 00679000
*     GTSTR6   FIXED(31) REG(6),/* CLEARING REG*/               03S0219 00680000
*     GTSTR7   FIXED(31) REG(7),/* CLEARING REG*/               03S0219 00681000
*     GTSTR8   FIXED(31) REG(8),/* CLEARING REG*/               03S0219 00682000
*     GTSTZERO FIXED(31) INIT(0),                               03S0219 00683000
*     GTST253  FIXED(31) CONSTANT(253);                         03S0219 00684000
*   RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/               03S0220 00685000
*   GTSTR0 = DSRLSZE; /* SET AMOUNT*/                           03S0221 00686000
         LR    GTSTR0,@15                                       03S0221 00687000
*   GTSTR2 = SPID; /* SET SUBPOOL*/                             03S0222 00688000
         LA    GTSTR2,230                                       03S0222 00689000
*   GENERATE CODE;                                              03S0223 00690000
