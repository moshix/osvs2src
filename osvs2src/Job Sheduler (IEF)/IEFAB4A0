*            SCTDUMMY = OFF &          /*NOT A DUMMY         @ZA25163*/ 01064000
*                                                               02S0330 01065000
*            (SIO3DACC = YES |                                  02S0330 01066000
*            SIO3TAPE = YES) THEN      /*NEED SERS FROM UCBS TOO     */ 01067000
         L     @05,SIOTETIO(,@08)                               02S0330 01068000
         LTR   @05,@05                                          02S0330 01069000
         BZ    @RF00330                                         02S0330 01070000
         TM    SCTDUMMY(@08),B'10000000'                        02S0330 01071000
         BNZ   @RF00330                                         02S0330 01072000
         TM    SIO3DACC(@08),B'10100000'                        02S0330 01073000
         BZ    @RF00330                                         02S0330 01074000
*          VOLSEND = VOLSEND + TIOEWTCT; /*ADD IN #VOLS FROM UCB'S   */ 01075000
         L     @08,DSABTIOT(,@05)                               02S0331 01076000
         SLR   @05,@05                                          02S0331 01077000
         IC    @05,TIOEWTCT(,@08)                               02S0331 01078000
         ALR   VOLSEND,@05                                      02S0331 01079000
*        SERCNTR = ONE;                /*INIT INPUT VOL SER INDEX    */ 01080000
@RF00330 LA    @03,1                                            02S0332 01081000
         LR    SERCNTR,@03                                      02S0332 01082000
*        NUMSERS = ONE;                /*INIT # SERS TO GO IN LIST   */ 01083000
         LR    NUMSERS,@03                                      02S0333 01084000
*        DEVCTR = ONE;                 /*INIT TIOT DEVICE ENTRY INDEX*/ 01085000
         LR    DEVCTR,@03                                       02S0334 01086000
*        JVOLSEND = FIVE;              /*SET MAX NO. SERS IN JFCB    */ 01087000
         MVC   JVOLSEND(4),@CF00193                             02S0335 01088000
*        JFCBXPTR = SIOTJFX;           /*SET PTR TO FIRST JFCBX      */ 01089000
         L     @03,CMSIOPTR(,CMRBPTR)                           02S0336 01090000
         L     JFCBXPTR,SIOTJFX(,@03)                           02S0336 01091000
*        VLSERPTR = ADDR(JFCBVOLS);    /*SET PTR TO INPUT VOL SERS   */ 01092000
         L     VLSERPTR,SJFCBPTR(,@03)                          02S0337 01093000
         LA    VLSERPTR,JFCBVOLS(,VLSERPTR)                     02S0337 01094000
*        DO ENTCTR = LISTCTR BY 1      /*FILL IN LIST                */ 01095000
*            WHILE(NUMSERS ^> VOLSEND); /*FILL FROM CURRENT INPUT       01096000
*                                        SOURCE(S)                   */ 01097000
         L     ENTCTR,LISTCTR                                   02S0338 01098000
         B     @DE00338                                         02S0338 01099000
@DL00338 DS    0H                                               02S0339 01100000
*          VRLSERNO(ENTCTR) = INVLSERS(SERCNTR); /*MOVE SER TO LIST  */ 01101000
         L     @15,VRLSTPTR                                     02S0339 01102000
         LR    @14,ENTCTR                                       02S0339 01103000
         SLA   @14,3                                            02S0339 01104000
         ST    @14,@TF00001                                     02S0339 01105000
         ALR   @14,@15                                          02S0339 01106000
         LR    @01,SERCNTR                                      02S0339 01107000
         MH    @01,@CH00102                                     02S0339 01108000
         ALR   @01,VLSERPTR                                     02S0339 01109000
         AL    @01,@CF02689                                     02S0339 01110000
         MVC   VRLSERNO-8(6,@14),INVLSERS(@01)                  02S0339 01111000
*          IF CMCKAVOL = YES THEN      /*NEED TO CHECK ALL ENTRIES   */ 01112000
         TM    CMCKAVOL(CMRBPTR),B'00100000'                    02S0340 01113000
         BNO   @RF00340                                         02S0340 01114000
*            VRLCHKMA(ENTCTR) = ON;    /*SET INDICATOR               */ 01115000
         AL    @15,@TF00001                                     02S0341 01116000
         OI    VRLCHKMA-8(@15),B'00100000'                      02S0341 01117000
*          ELSE                        /*DONT CHECK ALL ENTRIES      */ 01118000
*            IF CMCKDAVL = YES &       /*CHECK ONLY D.A. VOLUMES     */ 01119000
*                SIO3DACC = YES THEN   /*REQUEST WAS FOR D.A. VOLUME */ 01120000
         B     @RC00340                                         02S0342 01121000
@RF00340 TM    CMCKDAVL(CMRBPTR),B'00010000'                    02S0342 01122000
         BNO   @RF00342                                         02S0342 01123000
         L     @01,CMSIOPTR(,CMRBPTR)                           02S0342 01124000
         TM    SIO3DACC(@01),B'00100000'                        02S0342 01125000
         BNO   @RF00342                                         02S0342 01126000
*              VRLCHKMA(ENTCTR) = ON;  /*SET INDICATOR FOR D.A. VOLS */ 01127000
         LR    @15,ENTCTR                                       02S0343 01128000
         SLA   @15,3                                            02S0343 01129000
         L     @01,VRLSTPTR                                     02S0343 01130000
         ALR   @01,@15                                          02S0343 01131000
         OI    VRLCHKMA-8(@01),B'00100000'                      02S0343 01132000
*/********************************************************************/ 01133000
*/*                                                                  */ 01134000
*/* SEGMENT NAME - GETVLSER                                          */ 01135000
*/*                                                                  */ 01136000
*/* DECRIPTIVE NAME - GET A VOLUME SERIAL                            */ 01137000
*/*                                                                  */ 01138000
*/* FUNCTION -                                                       */ 01139000
*/*   THIS ROUTINE WILL OBTAIN THE NEXT VOLUME SERIAL                */ 01140000
*/*   FROM THE JFCB, THEN FROM THE JFCBX(S), AND FROM THE UCB(S)     */ 01141000
*/*   FOR A GIVEN REQUEST.                                           */ 01142000
*/*                                                                  */ 01143000
*/* ENTRY - IN-LINE SEGMENT                                          */ 01144000
*/*   PURPOSE - TO OBTAIN NEXT VOLSER TO USE IN A SCAN               */ 01145000
*/*   INPUT -                                                        */ 01146000
*/*     DEVCTR - CURRENT INDEX INTO TIOT DEVICE ENTRY                */ 01147000
*/*     JFCBXPTR - PTR TO FIRST JFCBX                                */ 01148000
*/*     JVOLSEND - CONTROLS SWITCH FROM JFCB TO JFCBX                */ 01149000
*/*      AND FROM JFCBX TO EACH SUCCEEDING ONE (INTIALLY EQUALS 5)   */ 01150000
*/*     NUMSERS - NUMBER OF VOL SER BEING PROCESSED                  */ 01151000
*/*     SERCNTR - CURRENT INDEX INTO VOL SER BUFFER                  */ 01152000
*/*     VOLSEND - TOTAL NUMBER OF VOL SERS TO PROCESS                */ 01153000
*/*     VLSERPTR - PTR TO CURRENT SOURCE OF VOL SERS                 */ 01154000
*/*      (POINTS TO JFCBVOLS ON INITIAL ENTRY)                       */ 01155000
*/* EXIT -                                                           */ 01156000
*/*   CONDITION - NEXT VOL SER OBTAINED (IF ANY)                     */ 01157000
*/*     OUTPUT -                                                     */ 01158000
*/*       VLSERPTR - PTR TO SOURCE OF NEXT VOLSER                    */ 01159000
*/*       SERCNTR - INDEX TO NEXT VOL SER                            */ 01160000
*/*       NUMSER - ADJUSTED FOR NO. ZERO ENTRIES IN TIOT DEVICE ENTRY*/ 01161000
*/*       DEVCTR - INDEX TO NEXT UCB ADDR IN TIOT DEVICE ENTRY       */ 01162000
*/*     RETURN CODE - NONE                                           */ 01163000
*/*                                                                  */ 01164000
*/********************************************************************/ 01165000
*IF NUMSERS < JFCBNVOL THEN            /*MORE SERS IN JFCB/JFCBX     */ 01166000
@RF00342 DS    0H                                               03S0344 01167000
@RC00340 L     @01,CMSIOPTR(,CMRBPTR)                           03S0344 01168000
         L     @01,SJFCBPTR(,@01)                               03S0344 01169000
         SLR   @00,@00                                          03S0344 01170000
         IC    @00,JFCBNVOL(,@01)                               03S0344 01171000
         CR    NUMSERS,@00                                      03S0344 01172000
         BNL   @RF00344                                         03S0344 01173000
*  DO;                                                          03S0345 01174000
*    IF NUMSERS = JVOLSEND THEN        /*NEED TO CHANGE BLOCKS       */ 01175000
         L     @15,JVOLSEND                                     03S0346 01176000
         CR    NUMSERS,@15                                      03S0346 01177000
         BNE   @RF00346                                         03S0346 01178000
*      DO;                                                      03S0347 01179000
*        SERCNTR = ONE;                /*RESET VOL SER INDEX         */ 01180000
         LA    SERCNTR,1                                        03S0348 01181000
*        JVOLSEND = JVOLSEND + FIFTEEN; /*SET UPPER LIMIT            */ 01182000
         AL    @15,@CF00055                                     03S0349 01183000
         ST    @15,JVOLSEND                                     03S0349 01184000
*        IF NUMSERS ^= FIVE THEN       /*END OF FIRST JFCBX          */ 01185000
         C     NUMSERS,@CF00193                                 03S0350 01186000
         BE    @RF00350                                         03S0350 01187000
*          JFCBXPTR = JFCBXNXT;        /*POINT TO NEXT JFCBX         */ 01188000
         L     JFCBXPTR,JFCBXNXT(,JFCBXPTR)                     03S0351 01189000
*        VLSERPTR = ADDR(JFCBXVOL);    /*SET PTR TO INPUT SERS       */ 01190000
@RF00350 LA    VLSERPTR,JFCBXVOL(,JFCBXPTR)                     03S0352 01191000
*      END;                                                     03S0353 01192000
*    ELSE                              /*NO BLOCK CHANGE             */ 01193000
*      SERCNTR = SERCNTR + ONE;        /*INCREMENT VOL SER BUFF INDEX*/ 01194000
         B     @RC00346                                         03S0354 01195000
@RF00346 AL    SERCNTR,@CF00091                                 03S0354 01196000
*  END;                                                         03S0355 01197000
*ELSE                                  /*NO MORE SERS IN JFCB/JFCBX  */ 01198000
*  IF NUMSERS ^> VOLSEND THEN          /*MORE SERIALS TO GET         */ 01199000
         B     @RC00344                                         03S0356 01200000
@RF00344 CR    NUMSERS,VOLSEND                                  03S0356 01201000
         BH    @RF00356                                         03S0356 01202000
*    DO;                               /*GET SERIAL FROM NEXT UCB    */ 01203000
*      UCBENTSW = OFF;                 /*INIT EXIT SWITCH            */ 01204000
         NI    UCBENTSW,B'01111111'                             03S0358 01205000
*      DO DENTCTR= DEVCTR TO TIOEWTCT  /*CHECK TIOT DEV ENTS         */ 01206000
*          WHILE(UCBENTSW = OFF);      /*EXIT WHEN FIND NON-0 ENT    */ 01207000
         LR    @15,DEVCTR                                       03S0359 01208000
         B     @DE00359                                         03S0359 01209000
@DL00359 TM    UCBENTSW,B'10000000'                             03S0359 01210000
         BNZ   @DC00359                                         03S0359 01211000
*        UCBPTR =TIOUCBP(DENTCTR);     /*SET PTR TO UCB              */ 01212000
         SLA   @15,2                                            03S0360 01213000
         L     @01,CMSIOPTR(,CMRBPTR)                           03S0360 01214000
         L     @01,SIOTETIO(,@01)                               03S0360 01215000
         L     @01,DSABTIOT(,@01)                               03S0360 01216000
         L     @15,TIOUCBP-5(@15,@01)                           03S0360 01217000
         LA    @15,0(,@15)                                      03S0360 01218000
         ST    @15,UCBPTR                                       03S0360 01219000
*       IF UCBPTR ^= ZERO THEN         /*DEVICE WAS ALLOCATED        */ 01220000
         LTR   @15,@15                                          03S0361 01221000
         BZ    @RF00361                                         03S0361 01222000
*         DO;                                                   03S0362 01223000
*           UCBENTSW = ON;             /*SET SWITCH TO EXIT LOOP     */ 01224000
         OI    UCBENTSW,B'10000000'                             03S0363 01225000
*           VLSERPTR = ADDR(UCBVOLI);  /*SET PTR TO INPUT SER        */ 01226000
         LA    VLSERPTR,UCBVOLI(,@15)                           03S0364 01227000
*           SERCNTR = ONE;             /*SET INDEX INTO INPUT SER    */ 01228000
         LA    @15,1                                            03S0365 01229000
         LR    SERCNTR,@15                                      03S0365 01230000
*           DEVCTR = DEVCTR + ONE;     /*UPDATE DEV. ENTRY INDEX     */ 01231000
         ALR   DEVCTR,@15                                       03S0366 01232000
*         END;                                                  03S0367 01233000
*       ELSE                           /*ZERO TIOT DEVICE ENTRY      */ 01234000
*         NUMSERS = NUMSERS + ONE;     /*ADJUST # INPUT SERS         */ 01235000
         B     @RC00361                                         03S0368 01236000
@RF00361 AL    NUMSERS,@CF00091                                 03S0368 01237000
*      END;                                                     03S0369 01238000
@RC00361 LA    @15,1                                            03S0369 01239000
         AL    @15,DENTCTR                                      03S0369 01240000
@DE00359 ST    @15,DENTCTR                                      03S0369 01241000
         L     @14,CMSIOPTR(,CMRBPTR)                           03S0369 01242000
         L     @14,SIOTETIO(,@14)                               03S0369 01243000
         L     @14,DSABTIOT(,@14)                               03S0369 01244000
         SLR   @01,@01                                          03S0369 01245000
         IC    @01,TIOEWTCT(,@14)                               03S0369 01246000
         CR    @15,@01                                          03S0369 01247000
         BNH   @DL00359                                         03S0369 01248000
@DC00359 DS    0H                                               03S0370 01249000
*   END;                                                        03S0370 01250000
* ELSE;                                /*NO MORE SERS FOR THIS PASS  */ 01251000
@RF00356 DS    0H                                               03S0372 01252000
*          NUMSERS = NUMSERS + ONE;    /*INCREMENT # SERS TO OBTAIN  */ 01253000
@RC00344 AL    NUMSERS,@CF00091                                 02S0372 01254000
*        END;                                                   02S0373 01255000
         AL    ENTCTR,@CF00091                                  02S0373 01256000
@DE00338 CR    NUMSERS,VOLSEND                                  02S0373 01257000
         BNH   @DL00338                                         02S0373 01258000
*        LISTCTR = ENTCTR;             /*ADJUST ENTRY INDEX START    */ 01259000
         ST    ENTCTR,LISTCTR                                   02S0374 01260000
*        CMRBPTR = CMNRBPTR;           /*POINT TO NEXT REQ BLOCK     */ 01261000
         L     CMRBPTR,CMNRBPTR(,CMRBPTR)                       02S0375 01262000
*      END;                                                     02S0376 01263000
@DE00328 LTR   CMRBPTR,CMRBPTR                                  02S0376 01264000
         BNZ   @DL00328                                         02S0376 01265000
*    END;                                                       02S0377 01266000
*  ELSE;                               /*NO VOLS TO RELEASE          */ 01267000
@RF00322 DS    0H                                               02S0379 01268000
*ELSE                                  /*GETMAIN ERROR               */ 01269000
*  RSNCODE(VRLGMERR) = VRLGMERR;                                02S0379 01270000
         B     @RC00321                                         02S0379 01271000
@RF00321 L     @15,@PC00001+28                                  02S0379 01272000
         MVC   RSNCODE+4(2,@15),@CH00183                        02S0379 01273000
*RFY INDMSIOT BASED(SIOTPTR);                                   02S0380 01274000
@RC00321 DS    0H                                               02S0381 01275000
*RFY INFMJFCB BASED(JFCBPTR);                                   02S0381 01276000
*RFY DSAB BASED(DSABPTR);                                       02S0382 01277000
*        CMESVOLB = OFF;               /*RESET ESTAE FUNCTION BIT    */ 01278000
         L     @01,CMESPAP                                      01S0383 01279000
         NI    CMESVOLB(@01),B'11101111'                        01S0383 01280000
*      END;                                                     01S0384 01281000
*    IF CMUNUNIT = YES THEN            /*UNIT UNALLOCATION REQUESTED?*/ 01282000
@RF00278 TM    CMUNUNIT(CFMAPTR),B'00000100'                    01S0385 01283000
         BNO   @RF00385                                         01S0385 01284000
*      DO;                             /*SET UP INTERFACE            */ 01285000
*        UNALUFMP = ZERO;              /*INIT FUNCTION MAP           */ 01286000
         MVI   UNALUFMP,X'00'                                   01S0387 01287000
*        IF CMNOPOST = YES THEN        /*POST FUNCTION NOT REQUIRED? */ 01288000
         TM    CMNOPOST(CFMAPTR),B'10000000'                    01S0388 01289000
         BNO   @RF00388                                         01S0388 01290000
*          UNALNPST = ON;              /*YES,SET INDIC IN FCN MAP    */ 01291000
         OI    UNALNPST,B'10000000'                             01S0389 01292000
*        IF CMNOVUT = YES THEN         /*VUT SET-UP BYPASSED         */ 01293000
@RF00388 TM    CMNOVUT(CFMAPTR),B'01000000'                     01S0390 01294000
         BNO   @RF00390                                         01S0390 01295000
*          UNALNVUT = ON;              /*SET IN UNIT UNALOC'N FCN MAP*/ 01296000
         OI    UNALNVUT,B'01000000'                             01S0391 01297000
*        IF CMCHATTR = YES THEN        /*CHANGE ATTRIBUTES IN UCB?   */ 01298000
@RF00390 TM    CMCHATTR(CFMAPTR),B'00100000'                    01S0392 01299000
         BNO   @RF00392                                         01S0392 01300000
*          UNALCHAT = ON;              /*YES,SET FCN MAP INDICATOR   */ 01301000
         OI    UNALCHAT,B'00100000'                             01S0393 01302000
*        CMESUNAL = ON;                /*UNIT UNALLOC ESTAE BIT ON   */ 01303000
@RF00392 L     @01,CMESPAP                                      01S0394 01304000
         OI    CMESUNAL(@01),B'00001000'                        01S0394 01305000
*        CALL IEFAB4A4                 /*UNIT UNALLOCATION           */ 01306000
*            (UNALUFMP,FRBPTR,JSCDSABQ,VUTSVA,CMESPAP,NAMESPTR);        01307000
         LA    @15,UNALUFMP                                     01S0395 01308000
         ST    @15,@AL00001                                     01S0395 01309000
         LA    @15,FRBPTR                                       01S0395 01310000
         ST    @15,@AL00001+4                                   01S0395 01311000
         L     @15,@PC00001+8                                   01S0395 01312000
         L     @01,JSCBPTR(,@15)                                01S0395 01313000
         LA    @15,JSCDSABQ(,@01)                               01S0395 01314000
         ST    @15,@AL00001+8                                   01S0395 01315000
         L     @15,@PC00001+24                                  01S0395 01316000
         ST    @15,@AL00001+12                                  01S0395 01317000
         LA    @15,CMESPAP                                      01S0395 01318000
         ST    @15,@AL00001+16                                  01S0395 01319000
         L     @15,@PC00001+32                                  01S0395 01320000
         ST    @15,@AL00001+20                                  01S0395 01321000
         L     @15,@CV00171                                     01S0395 01322000
         LA    @01,@AL00001                                     01S0395 01323000
         BALR  @14,@15                                          01S0395 01324000
*                                      /*                    @ZD03010*/ 01325000
*        CMESUNAL = OFF;               /*RESET ESTAE FUNCTION BIT    */ 01326000
         L     @01,CMESPAP                                      01S0396 01327000
         NI    CMESUNAL(@01),B'11110111'                        01S0396 01328000
*      END;                                                     01S0397 01329000
*    IF CMVLRLSE = YES             /*VOL RELEASE REQUESTED       */     01330000
*       & (CMGENDEQ = YES |        /*GENERIC DEQ         @YM07247*/     01331000
*        VRLSTPTR ^= ZERO)         /*VOL RLSE LIST EXISTS       01S0398 01332000
*                                                      @YM07247*/ THEN/ 01333000
*                                        VOLUME RELEASE TO BE DONE   */ 01334000
@RF00385 TM    CMVLRLSE(CFMAPTR),B'00000010'                    01S0398 01335000
         BNO   @RF00398                                         01S0398 01336000
         TM    CMGENDEQ(CFMAPTR),B'00010000'                    01S0398 01337000
         BO    @RT00398                                         01S0398 01338000
         L     @15,VRLSTPTR                                     01S0398 01339000
         LTR   @15,@15                                          01S0398 01340000
         BZ    @RF00398                                         01S0398 01341000
@RT00398 DS    0H                                               01S0399 01342000
*      DO;                             /*DO VOL RELEASE              */ 01343000
*        VOLRFMAP = ZERO;              /*INIT FUNCTION MAP           */ 01344000
         MVI   VOLRFMAP,X'00'                                   01S0400 01345000
*        IF CMGENDEQ = YES THEN        /*GENERIC DEQ REQUIRED?       */ 01346000
         TM    CMGENDEQ(CFMAPTR),B'00010000'                    01S0401 01347000
         BNO   @RF00401                                         01S0401 01348000
*          VLRGNDEQ = ON;              /*YES,SET INDIC IN FCN MAP    */ 01349000
         OI    VLRGNDEQ,B'10000000'                             01S0402 01350000
*        IF JSCBTCBP = ZERO THEN       /*NO TCB PTR IN JSCB  @ZA12405*/ 01351000
