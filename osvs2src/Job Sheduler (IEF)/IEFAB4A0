@RF00171 TM    CMALSTCT(CFMAPTR),B'01000000'                    01S0173 00329000
         BNO   @RF00173                                         01S0173 00330000
*          DSPSCATA = ON;              /* YES, TELL IEFAB4A2 @ZA06981*/ 00331000
         OI    DSPSCATA,B'01000000'                             01S0174 00332000
*        CMESDISP = ON;                /*DISP PROC ESTAE BIT ON      */ 00333000
@RF00173 L     @01,CMESPAP                                      01S0175 00334000
         OI    CMESDISP(@01),B'10000000'                        01S0175 00335000
*        CALL IEFAB4A2                 /*DISP PROCESSING CONTROL     */ 00336000
*            (DSPFMAP,FRBPTR,JSCBPTR,CMESPAP,CATALDSN,RSNCODE(CATERR)); 00337000
         LA    @15,DSPFMAP                                      01S0176 00338000
