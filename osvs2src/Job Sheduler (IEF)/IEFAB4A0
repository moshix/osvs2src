         L     @15,JSCBTCBP(,@01)                               02S0209 00567000
         ST    @15,DQTCBP                                       02S0209 00568000
*    IF CMDSRALL = YES THEN            /*DEQ ALL DSNS IN STEP?       */ 00569000
@RC00207 TM    CMDSRALL(CFMAPTR),B'10000000'                    02S0210 00570000
         BNO   @RF00210                                         02S0210 00571000
*      DO;                                                      02S0211 00572000
*        DSRLSTPN = ON;                /*SET DSR FUNC MAP IND        */ 00573000
