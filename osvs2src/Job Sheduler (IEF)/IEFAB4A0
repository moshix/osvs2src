         L     @15,JSCBTCBP(,@01)                               01S0405 01367000
         ST    @15,DQTCBP                                       01S0405 01368000
*        CMESVREL = ON;                /*VOL RELEASE ESTAE BIT ON    */ 01369000
@RC00403 L     @01,CMESPAP                                      01S0406 01370000
         OI    CMESVREL(@01),B'00000001'                        01S0406 01371000
*        CALL IEFAB4A8                 /*VOLUME RELEASE              */ 01372000
*            (VOLRFMAP,VRLSTPTR,DQTCBP,FSIOTPTR, /*          @YM03580*/ 01373000
*             RSNCODE(VRLGMERR));                               01S0407 01374000
         LA    @15,VOLRFMAP                                     01S0407 01375000
         ST    @15,@AL00001                                     01S0407 01376000
         LA    @15,VRLSTPTR                                     01S0407 01377000
         ST    @15,@AL00001+4                                   01S0407 01378000
         LA    @15,DQTCBP                                       01S0407 01379000
         ST    @15,@AL00001+8                                   01S0407 01380000
         L     @15,@PC00001+12                                  01S0407 01381000
         ST    @15,@AL00001+12                                  01S0407 01382000
         L     @15,@PC00001+28                                  01S0407 01383000
         LA    @15,RSNCODE+4(,@15)                              01S0407 01384000
         ST    @15,@AL00001+16                                  01S0407 01385000
         L     @15,@CV00173                                     01S0407 01386000
         LA    @01,@AL00001                                     01S0407 01387000
         BALR  @14,@15                                          01S0407 01388000
*        IF REG15 ^= ZERO THEN         /*ERROR IN VOL RELEASE        */ 01389000
         LTR   REG15,REG15                                      01S0408 01390000
         BZ    @RF00408                                         01S0408 01391000
*          RETCODE = REG15;            /*SAVE RETURN CODE            */ 01392000
         STH   REG15,RETCODE                                    01S0409 01393000
*        CMESVREL = OFF;               /*RESET ESTAE FUNCTION BIT    */ 01394000
@RF00408 L     @01,CMESPAP                                      01S0410 01395000
         NI    CMESVREL(@01),B'11111110'                        01S0410 01396000
*      END;                                                     01S0411 01397000
*    IF CMFDSABS = YES THEN            /*DSAB TO BE FREED ?          */ 01398000
@RF00398 TM    CMFDSABS(CFMAPTR),B'00000001'                    01S0412 01399000
         BNO   @RF00412                                         01S0412 01400000
*      DO;                             /*YES,SET INTERFACE & CALL       01401000
*                                        TIOT MANAGER                */ 01402000
*/********************************************************************/ 01403000
*/*                                                                  */ 01404000
*/* SEGMENT NAME - FDSABINT                                          */ 01405000
*/*                                                                  */ 01406000
*/* DESCRIPTIVE NAME - CREATE FREE DSAB INTERFACE                    */ 01407000
*/*                                                                  */ 01408000
*/* FUNCTION -                                                       */ 01409000
*/*   THIS ROUTINE CREATES THE INTERFACE TO THE TIOT MANAGER AND     */ 01410000
*/*   INVOKES IT TO FREE DSABS AND, IF REQUESTED, TO RELEASE TIOT    */ 01411000
*/*   DD ENTRIES.                                                    */ 01412000
*/*                                                                  */ 01413000
*/*   OPERATION -                                                    */ 01414000
*/*     THE TIOT MANAGER REQUEST BLOCK IS INITIALIZED WITH THE       */ 01415000
*/*     REQUIRED FUNCTION MAP BITS.                                  */ 01416000
*/*                                                                  */ 01417000
*/*     THE AMOUNT OF CORE REQUIRED FOR A LIST OF DSAB POINTERS IS   */ 01418000
*/*     DETERMINED AND THE CORE IS REQUESTED. IF THE CORE IS AVAIL-  */ 01419000
*/*     ABLE THE LIST IS BUILT, AND THE TIOT MANAGER IS INVOKED TO   */ 01420000
*/*     PROCESS ALL ENTRIES IN THE LIST. IF THE CORE IS NOT AVAIL-   */ 01421000
*/*     ABLE, THE TIOT MANAGER IS INVOKED ONCE FOR EACH DSAB.        */ 01422000
*/*                                                                  */ 01423000
*/*     THE ESTAE PARAMETER AREA RESOURCE MAP IS UPDATED TO INDICATE */ 01424000
*/*     THAT THE DSABS HAVE BEEN FREED.                              */ 01425000
*/*                                                                  */ 01426000
*/* ENTRY - IN-LINE SEGMENT                                          */ 01427000
*/*   PURPOSE - SEE FUNCTION                                         */ 01428000
*/*   INPUT -                                                        */ 01429000
*/*     CMRDDENT - IF ON,RELEASE TIOT DD ENTRIES                     */ 01430000
*/*     FRBPTR - PTR TO FIRST UNALLOCATION REQUEST BLOCK             */ 01431000
*/*       CMSIOPTR - PTR TO SIOT (IN REQ BLOCK)                      */ 01432000
*/*         SIOTETIO - PTR TO DSAB (IN SIOT)                         */ 01433000
*/*                                                                  */ 01434000
*/* EXIT -                                                           */ 01435000
*/*   CONDITION - DSABS FREED                                        */ 01436000
*/*     OUTPUT -                                                     */ 01437000
*/*       CORE FOR DSABS AND ASSOCIATED TABLES FREED                 */ 01438000
*/*       TIOT FREE CHAIN UPDATED IF RELEASE FUNCTION REQUESTED      */ 01439000
*/*     RETURN CODE - NONE                                           */ 01440000
*/********************************************************************/ 01441000
*TMREQBLK = TMREQBLK && TMREQBLK;      /*CLEAR TIOT MGR REQUEST BLK  */ 01442000
