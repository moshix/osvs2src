         TITLE 'IEFAB4A0 - COMMON UNALLOCATION CONTROL                 *00001000
                        '                                               00002000
IEFAB4A0 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFAB4A0  78.089'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         GSPACE   R,LV=(0)                                              00015000
         LR    @11,@01                                          01S0001 00016000
         USING @DATD,@11                                        01S0001 00017000
         ST    @13,@SA00001+4                                   01S0001 00018000
         LM    @00,@01,20(@13)                                  01S0001 00019000
         ST    @11,8(,@13)                                      01S0001 00020000
         LR    @13,@11                                          01S0001 00021000
         MVC   @PC00001(40),0(@01)                              01S0001 00022000
JSCBDISP EQU 188                                                        00023000
JSCBAOS1 EQU  72                                                        00024000
JSCBAOS2 EQU 192                                                        00025000
*RETCODE = ZERO;                       /*INITIALIZE RETURN CODE AREA */ 00026000
         SLR   @15,@15                                          01S0098 00027000
         STH   @15,RETCODE                                      01S0098 00028000
*CFMAPTR = ADDR(FCNTNMAP);             /*SET PTR TO FUNCTION MAP     */ 00029000
         L     CFMAPTR,@PC00001                                 01S0099 00030000
*/********************************************************************/ 00031000
*/*                                                                  */ 00032000
*/* SEGMENT NAME - CRESTAE                                           */ 00033000
*/*                                                                  */ 00034000
*/* DESCRIPTIVE NAME - CREATE ESTAE ENVIRONMENT                      */ 00035000
*/*                                                                  */ 00036000
*/* FUNCTION -                                                       */ 00037000
*/*   THIS ROUTINE INITIALIZES THE COMMON UNALLOCATION ESTAE         */ 00038000
*/*   PARAMETER AREA (CMESPA) AND CREATES                    @Z40FPPG*/ 00039000
*/*   A RECOVERY ENVIRONMENT.                                        */ 00040000
*/*                                                                  */ 00041000
*/* ENTRY - IN-LINE SEGMENT                                          */ 00042000
*/*   PURPOSE - CREATE ESTAE ENVIRONMENT                             */ 00043000
*/*                                                                  */ 00044000
*/*   INPUT -                                                        */ 00045000
*/*     INPUT FUNCTION MAP - CMUNFMAP                                */ 00046000
*/*     PTR TO FIRST REQ BLK - RBIPTR                                */ 00047000
*/*     PTR TO P/P JSCB - JSCBPTR                                    */ 00048000
*/*                                                                  */ 00049000
*/* EXITS -                                                          */ 00050000
*/*   CONDITION - ESTAE ENVIRONMENT CREATED                          */ 00051000
*/*     OUTPUT -                                                     */ 00052000
*/*       ESTAE PARM AREA INITIALIZED WITH --                        */ 00053000
*/*         PTR TO FIRST REQ BLOCK                                   */ 00054000
*/*         PTR TO FIRST SIOT                                        */ 00055000
*/*         DATA SET ENQUEUE TABLE SVA                               */ 00056000
*/*         STEP NUMBER                                              */ 00057000
*/*         VUT SVA                                                  */ 00058000
*/*         PTR TO P/P JSCB                                          */ 00059000
*/*         PTR TO INPUT FUNCTION MAP                                */ 00060000
*/*     RETURN CODES - 0                                             */ 00061000
*/*   CONDITION - UNABLE TO CREATE ESTAE ENVIRONMENT                 */ 00062000
*/*     OUTPUT - RSNCODE = 4                                         */ 00063000
*/*     RETURN CODES - RETURNED BY ESTAE                             */ 00064000
*/*                                                                  */ 00065000
*/********************************************************************/ 00066000
*CMESPAP = ADDR(CMESCORE);             /*PTR TO ESTAE PARM AREA      */ 00067000
         LA    @14,CMESCORE                                     02S0100 00068000
         ST    @14,CMESPAP                                      02S0100 00069000
*CMESPARM = CMESPARM && CMESPARM;      /*CLEAR ESTAE PARMAETER AREA  */ 00070000
         XC    CMESPARM(40,@14),CMESPARM(@14)                   02S0101 00071000
*CMESFMAP = ADDR(CMUNFMAP);            /*SET PTR TO INPUT FCN MAP    */ 00072000
         ST    CFMAPTR,CMESFMAP(,@14)                           02S0102 00073000
*CMESJSCB = JSCBPTR;                   /*SET PTR TO JSCB             */ 00074000
         L     @09,@PC00001+8                                   02S0103 00075000
         L     @09,JSCBPTR(,@09)                                02S0103 00076000
         ST    @09,CMESJSCB(,@14)                               02S0103 00077000
*CMESRB1P = RB1PTR;                    /*SET PTR TO FIRST REQ BLK    */ 00078000
         L     @08,@PC00001+4                                   02S0104 00079000
         L     @08,RB1PTR(,@08)                                 02S0104 00080000
         ST    @08,CMESRB1P(,@14)                               02S0104 00081000
*CMESRBNP = RB1PTR;                    /*PRIME NEXT TO BE PROC'D     */ 00082000
         ST    @08,CMESRBNP(,@14)                               02S0105 00083000
*CMESSIOP = FSIOTPTR;                  /*PTR TO 1ST SIOT TO ESTAE PRM*/ 00084000
         L     @08,@PC00001+12                                  02S0106 00085000
         L     @08,FSIOTPTR(,@08)                               02S0106 00086000
         ST    @08,CMESSIOP(,@14)                               02S0106 00087000
*CMESDSEN = NQTABSVA;                  /*DS ENQ TABLE SVA            */ 00088000
         L     @08,@PC00001+16                                  02S0107 00089000
         MVC   CMESDSEN(3,@14),NQTABSVA(@08)                    02S0107 00090000
*CMESSNUM = STEPNUM;                   /*STEP NUMBER TO ESTAE PARMS  */ 00091000
         L     @08,@PC00001+20                                  02S0108 00092000
         IC    @07,STEPNUM(,@08)                                02S0108 00093000
         STC   @07,CMESSNUM(,@14)                               02S0108 00094000
*CMESVUTP = VUTSVA;                    /*VUT SVA TO ESTAE PARMS      */ 00095000
         L     @08,@PC00001+24                                  02S0109 00096000
         MVC   CMESVUTP(3,@14),VUTSVA(@08)                      02S0109 00097000
*RFY (REG15) RSTD;                                              02S0110 00098000
*REG15 = ZERO;                         /*INIT RETURN CODE    @Z40FPPG*/ 00099000
         SLR   REG15,REG15                                      02S0111 00100000
*/********************************************************************/ 00101000
*/*                                                          @Z40FPPG*/ 00102000
*/* SEGMENT NAME - ESTAECOD                                  @Z40FPPG*/ 00103000
*/*                                                          @Z40FPPG*/ 00104000
*/* DESCRIPTIVE NAME - ESTAE CODE                            @Z40FPPG*/ 00105000
*/*                                                          @Z40FPPG*/ 00106000
*/* FUNCTION - SET UP THE APPROPRIATE ESTAE ENVIRONMENT      @Z40FPPG*/ 00107000
*/*                                                          @Z40FPPG*/ 00108000
*/* OPERATION -                                              @Z40FPPG*/ 00109000
*/*          . AN ALLOCATION STAE WORK AREA (ASWA) IS FILLED @Z40FPPG*/ 00110000
*/*            IN. THIS ALLOWS THE ALLOCATION COMMON ESTAE   @Z40FPPG*/ 00111000
*/*            EXIT (IEFAB4ED) TO ROUTE CONTROL TO THE OTHER @Z40FPPG*/ 00112000
*/*            APPROPRIATE EXIT ROUTINES FOR SPECIFIC        @Z40FPPG*/ 00113000
*/*            RECOVERY PROCESSING.                          @Z40FPPG*/ 00114000
*/*          . IF NO ALLOCATION ESTAE IS IN EFFECT, OR IF ONE@Z40FPPG*/ 00115000
*/*            IS BUT IT IS NOT THE CURRENT ESTAE,A NEW ESTAE@Z40FPPG*/ 00116000
*/*            MUST BE ISSUED, THE ASWA BUILT BECOMES THE    @Z40FPPG*/ 00117000
*/*            FIRST ONE IN A NEW CHAIN, IS THE EXIT PARM    @Z40FPPG*/ 00118000
*/*            RECEIVED BY THE COMMON ESTAE EXIT, AND MUST   @Z40FPPG*/ 00119000
*/*            PROVIDE THE AUTOMATIC CORE FOR THE DATA THAT  @Z40FPPG*/ 00120000
*/*            ROUTINE USES. THE PTR TO THE LAST ASWA OF AN  @Z40FPPG*/ 00121000
*/*            EARLIER ALLOCATION ESTAE, IF ANY, IS SAVED IN @Z40FPPG*/ 00122000
*/*            THIS ASWA. NOTE THAT THE STAE CONTROL BLOCK   @Z40FPPG*/ 00123000
*/*            ADDRESS IN THE ASWA IS NOT UPDATED UNTIL AFTER@Z40FPPG*/ 00124000
*/*            THE ESTAE IS ISSUED SO THAT IT REFLECTS THE   @Z40FPPG*/ 00125000
*/*            CURRENT ONE.                                  @Z40FPPG*/ 00126000
*/*          . IF THE CURRENT ESTAE IN EFFECT IS ALLOCATION'S@Z40FPPG*/ 00127000
*/*            IT CAN BE USED TO PROVIDE THIS ROUTINES NECES-@Z40FPPG*/ 00128000
*/*            SARY RECOVERY ENVIRONMENT. THE SCB ADDRESS    @Z40FPPG*/ 00129000
*/*            IS COPIED FROM THE MOST RECENT ASWA, TO WHICH @Z40FPPG*/ 00130000
*/*            THE CHAIN PTR OF THIS NEW ASWA WILL POINT BACK@Z40FPPG*/ 00131000
*/*          . THE ADDRESS OF THE NEW ASWA IS PLACED IN      @Z40FPPG*/ 00132000
*/*            THE JSCB AS THE LAST ONE IN THE CHAIN FOR THE @Z40FPPG*/ 00133000
*/*            CURRENT ALLOCATION ESTAE                      @Z40FPPG*/ 00134000
*/*                                                          @Z40FPPG*/ 00135000
*/* ENTRY - INLINE SUBROUTINE ENTERED AT THE TOP             @Z40FPPG*/ 00136000
*/*                                                          @Z40FPPG*/ 00137000
*/*   PURPOSE - SEE FUNCTION                                 @Z40FPPG*/ 00138000
*/*                                                          @Z40FPPG*/ 00139000
*/*   INPUT -                                                @Z40FPPG*/ 00140000
*/*        .JSCBASWA - PTR TO LAST ASWA ON CHAIN, IF ANY     @Z40FPPG*/ 00141000
*/*        .TCBSTABB - PTR TO CURRENT STAE CONTROL BLOCK     @Z40FPPG*/ 00142000
*/*        .CMESPAP - PTR TO COMMON UNALLOCATION ESTAE PARMS @Z40FPPG*/ 00143000
*/*        .REG15  - ZERO ON ENTRY                           @Z40FPPG*/ 00144000
*/*                                                          @Z40FPPG*/ 00145000
*/* EXIT - INLINE SUBROUTINE THAT EXITS AT THE BOTTOM        @Z40FPPG*/ 00146000
*/*                                                          @Z40FPPG*/ 00147000
*/*   CONDITION - ESTAE SETUP COMPLETED                      @Z40FPPG*/ 00148000
*/*                                                          @Z40FPPG*/ 00149000
*/*   OUTPUT -                                               @Z40FPPG*/ 00150000
*/*           .ASWA - FILLED IN AND PLACED ON THE CHAIN      @Z40FPPG*/ 00151000
*/*           .JSCBASWA - POINTS TO ASWA AS LAST ON CHAIN    @Z40FPPG*/ 00152000
*/*           .ISSUESTA - ON IF ESTAE ISSUED                 @Z40FPPG*/ 00153000
*/*                                                          @Z40FPPG*/ 00154000
*/*   RETURN CODES - REG15 HAS ESTAE RETURN CODE             @Z40FPPG*/ 00155000
*/*                  IF ONE WAS ISSUED. (ZERO OTHERWISE)     @Z40FPPG*/ 00156000
*/*                                                          @Z40FPPG*/ 00157000
*/********************************************************************/ 00158000
*ASWAPTR = ADDR(ASWACORE);             /*ADDRESS ASWA CORE   @Z40FPPG*/ 00159000
         LA    @08,ASWACORE                                     03S0112 00160000
         ST    @08,ASWAPTR                                      03S0112 00161000
*ASWA = ASWA && ASWA;                  /*ZERO OUT ASWA       @Z40FPPG*/ 00162000
         XC    ASWA(40,@08),ASWA(@08)                           03S0113 00163000
*ASWAID =CHARASWA;                     /*FILL IN BLOCK ID    @Z40FPPG*/ 00164000
         MVC   ASWAID(4,@08),@CC00186                           03S0114 00165000
*ASWACSCT = EXITCSCT;                  /*CSECT NAME OF EXIT  @Z40FPPG*/ 00166000
         MVC   ASWACSCT(8,@08),@CC00188                         03S0115 00167000
*ASWARTNP = ADDR(IEFAB4DE);            /*ADDRESS OF EXIT RTN @Z40FPPG*/ 00168000
         L     @07,@CV00174                                     03S0116 00169000
         STCM  @07,15,ASWARTNP(@08)                             03S0116 00170000
*ASWAPRMS = CMESPAP;                   /*PARMS FOR THIS EXIT @Z40FPPG*/ 00171000
         STCM  @14,15,ASWAPRMS(@08)                             03S0117 00172000
*ISSUESTA = OFF;                       /*INIT. ESTAE SW      @Z40FPPG*/ 00173000
         NI    ISSUESTA,B'01111111'                             03S0118 00174000
*IF JSCBASWA = ZERO   |                /*NO ALLOC ESTAE IN EFFECT OR    00175000
*                                                            @Z40FPPG*/ 00176000
*   JSCBASWA -> ASWASCBP ^= TCBSTABB THEN /*ITS NOT THE CURRENT ONE     00177000
*                                                            @Z40FPPG*/ 00178000
         SLR   @14,@14                                          03S0119 00179000
         L     @09,JSCBASWA(,@09)                               03S0119 00180000
         CR    @09,@14                                          03S0119 00181000
         BE    @RT00119                                         03S0119 00182000
         L     @14,CVTPTR                                       03S0119 00183000
         L     @14,CVTTCBP(,@14)                                03S0119 00184000
         L     @14,TCBPTR(,@14)                                 03S0119 00185000
         ICM   @08,15,ASWASCBP(@09)                             03S0119 00186000
         L     @14,TCBSTABB-1(,@14)                             03S0119 00187000
         LA    @14,0(,@14)                                      03S0119 00188000
         CR    @08,@14                                          03S0119 00189000
         BE    @RF00119                                         03S0119 00190000
@RT00119 DS    0H                                               03S0120 00191000
*  DO;                                 /*PROCESS NEW ESTAE   @Z40FPPG*/ 00192000
*    ISSUESTA = ON;                    /*SAY ESTAE ISSUED    @Z40FPPG*/ 00193000
         OI    ISSUESTA,B'10000000'                             03S0121 00194000
*    ASWAAUTO = ADDR(AB4EDPMS);        /*1ST ASWA GIVES W/A  @Z40FPPG*/ 00195000
         L     @14,ASWAPTR                                      03S0122 00196000
         LA    @09,AB4EDPMS                                     03S0122 00197000
         STCM  @09,15,ASWAAUTO(@14)                             03S0122 00198000
*    ASWAOLDP = JSCBASWA;              /*OLD LAST IF ANY     @Z40FPPG*/ 00199000
         L     @09,@PC00001+8                                   03S0123 00200000
         L     @01,JSCBPTR(,@09)                                03S0123 00201000
         MVC   ASWAOLDP(4,@14),JSCBASWA(@01)                    03S0123 00202000
*    RFY (EXITADDR,CMESPTR) RSTD;      /*RESTRICT WORK REGS  @Z40FPPG*/ 00203000
*    EXITADDR = ADDR(IEFAB4ED);        /*COMMON ESTAE EXIT   @Z40FPPG*/ 00204000
         L     EXITADDR,@CV00175                                03S0125 00205000
*    CMESPTR = ASWAPTR;                /*ASWA IS EXIT PARMS  @Z40FPPG*/ 00206000
         LR    CMESPTR,@14                                      03S0126 00207000
*    ESTAEPL(1:ESTPLLEN) = ESTPLFMT(1:ESTPLLEN);                03S0127 00208000
         L     @14,ESTPLLEN                                     03S0127 00209000
         BCTR  @14,0                                            03S0127 00210000
         EX    @14,@SM02677                                     03S0127 00211000
*                                      /*MOVE TO AUTO CORE   @Z40FPPG*/ 00212000
*    GEN REFS(ESTAEPL) SETS(REG15,TCBSTABB);                    03S0128 00213000
      ESTAE (EXITADDR),PARAM=(CMESPTR),MF=(E,ESTAEPL)                   00214000
*    RFY (EXITADDR,CMESPTR) UNRSTD;                             03S0129 00215000
*    IF REG15 = ZERO THEN              /*IF ESTAE SUCCESSFUL @Z40FPPG*/ 00216000
         LTR   REG15,REG15                                      03S0130 00217000
         BNZ   @RF00130                                         03S0130 00218000
*      ASWASCBP = TCBSTABB;            /*CURRENT SCB @       @Z40FPPG*/ 00219000
         L     @14,CVTPTR                                       03S0131 00220000
         L     @14,CVTTCBP(,@14)                                03S0131 00221000
         L     @14,TCBPTR(,@14)                                 03S0131 00222000
         L     @14,TCBSTABB-1(,@14)                             03S0131 00223000
         LA    @14,0(,@14)                                      03S0131 00224000
         L     @01,ASWAPTR                                      03S0131 00225000
         STCM  @14,15,ASWASCBP(@01)                             03S0131 00226000
*  END;                                /*END IF ESTAE ISSUED @Z40FPPG*/ 00227000
*ELSE                                  /*CURRENT IS ALLOC'S  @Z40FPPG*/ 00228000
*  DO;                                 /*USE EXISTING ESTAE  @Z40FPPG*/ 00229000
         B     @RC00119                                         03S0133 00230000
@RF00119 DS    0H                                               03S0134 00231000
*    ASWASCBP = JSCBASWA -> ASWASCBP;  /*SCB @ FROM LAST ASWA@Z40FPPG*/ 00232000
         L     @14,ASWAPTR                                      03S0134 00233000
         L     @09,@PC00001+8                                   03S0134 00234000
         L     @01,JSCBPTR(,@09)                                03S0134 00235000
         L     @09,JSCBASWA(,@01)                               03S0134 00236000
         MVC   ASWASCBP(4,@14),ASWASCBP(@09)                    03S0134 00237000
*    ASWACHNP = JSCBASWA;              /*POINT BACK TO LAST  @Z40FPPG*/ 00238000
         STCM  @09,15,ASWACHNP(@14)                             03S0135 00239000
*  END;                                /*CURRENT ESTAE USED  @Z40FPPG*/ 00240000
*IF REG15 = ZERO THEN                  /*IF NO ERROR         @Z40FPPG*/ 00241000
@RC00119 SLR   @14,@14                                          03S0137 00242000
         CR    REG15,@14                                        03S0137 00243000
         BNE   @RF00137                                         03S0137 00244000
*  DO;                                 /*PUT ASWA ONTO CHAIN @Z40FPPG*/ 00245000
*     DO; /*MODESET  KEYADDR(ZERO)*/                            03S0139 00246000
*       SPKA (ZERO); /*SET PSW KEY*/                            03S0140 00247000
         SPKA  0(@14)                                           03S0140 00248000
*     END; /*MODESET  KEYADDR(ZERO)*/           /*KEY TO UPDATE JSCB  @ 00249000
*40FPPG*/                                                       03S0141 00250000
*    JSCBASWA = ASWAPTR;               /*NEW LAST ASWA PTR   @Z40FPPG*/ 00251000
         L     @14,@PC00001+8                                   03S0142 00252000
         L     @14,JSCBPTR(,@14)                                03S0142 00253000
         L     @09,ASWAPTR                                      03S0142 00254000
         ST    @09,JSCBASWA(,@14)                               03S0142 00255000
*     DO; /*MODESET  EXTKEY(SCHED)*/                            03S0143 00256000
*       SPKA (16); /*SET PSW KEY*/                              03S0144 00257000
         LA    @14,16                                           03S0144 00258000
         SPKA  0(@14)                                           03S0144 00259000
*     END; /*MODESET  EXTKEY(SCHED)*/           /*BACK TO SCHED KEY   @ 00260000
*40FPPG*/                                                       03S0145 00261000
*  END;                                /*END IF NO ERROR     @Z40FPPG*/ 00262000
*RETCODE = REG15;                      /*SAVE ESTAE RETURN CODE      */ 00263000
@RF00137 STH   REG15,RETCODE                                    02S0147 00264000
*RFY (REG15) UNRSTD;                                            02S0148 00265000
*IF RETCODE = ZERO THEN                /*NO ERROR ISSUING ESTAE      */ 00266000
         LH    @15,RETCODE                                      01S0149 00267000
         SLR   @14,@14                                          01S0149 00268000
         CR    @15,@14                                          01S0149 00269000
         BNE   @RF00149                                         01S0149 00270000
*  DO;                                                          01S0150 00271000
*    FRBPTR = RB1PTR;                  /*SET PTR TO FIRST REQ BLOCK  */ 00272000
         L     @15,@PC00001+4                                   01S0151 00273000
         L     @15,RB1PTR(,@15)                                 01S0151 00274000
         ST    @15,FRBPTR                                       01S0151 00275000
*    IF CMDISPRC = YES THEN            /*DISP PROCESSING REQUESTED?  */ 00276000
         TM    CMDISPRC(CFMAPTR),B'00100000'                    01S0152 00277000
         BNO   @RF00152                                         01S0152 00278000
*      DO;                                                      01S0153 00279000
*        DSPFMAP = ZERO;               /*INITIALIZE FUNCTION MAP     */ 00280000
         STCM  @14,3,DSPFMAP                                    01S0154 00281000
*        IF CMDMSGS = YES THEN         /*MESSAGES TO BE ISSUED?      */ 00282000
         TM    CMDMSGS(CFMAPTR),B'10000000'                     01S0155 00283000
         BNO   @RF00155                                         01S0155 00284000
*          DSPIMSGS = ON;              /*INDICATE IN DISP FCN MAP    */ 00285000
         OI    DSPIMSGS,B'10000000'                             01S0156 00286000
*        IF CMMNSTAT = YES THEN        /*MONITOR STATUS ACTIVE?      */ 00287000
@RF00155 TM    CMMNSTAT(CFMAPTR),B'01000000'                    01S0157 00288000
         BNO   @RF00157                                         01S0157 00289000
*          DSPMSTAT = ON;              /*INDICATE IN FUNCTION MAP    */ 00290000
         OI    DSPMSTAT,B'01000000'                             01S0158 00291000
*        IF CMNERMSG = YES THEN        /*DON'T ISSUE ERROR MESSAGES  */ 00292000
@RF00157 TM    CMNERMSG(CFMAPTR),B'00000100'                    01S0159 00293000
         BNO   @RF00159                                         01S0159 00294000
*          DSPNEMSG = ON;              /*INDICATE IN FUNCTION MAP    */ 00295000
