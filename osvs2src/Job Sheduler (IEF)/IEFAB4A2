         SLR   @08,@08                                          02S0522 01981000
         ST    @08,MRETCODE                                     02S0522 01982000
*    CALL IEFAB4B0(MSGDATA,VLSTPTR,DSNPTR,MRETCODE,JSCBPTR); /*ISSUE    01983000
*                                       SYSOUT MSG.                  */ 01984000
         LA    @08,MSGDATA                                      02S0523 01985000
