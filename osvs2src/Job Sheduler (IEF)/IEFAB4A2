         MH    @03,@CH00038                                     03S0571 02256000
         L     @01,VLSTPTR                                      03S0571 02257000
         SLR   @00,@00                                          03S0571 02258000
         IC    @00,VLRTNCDE-12(@03,@01)                         03S0571 02259000
         CH    @00,@CH00304                                     03S0571 02260000
         BNE   @RF00571                                         03S0571 02261000
*        MSG0RCDE = ON;                /*SET 'PRINT 0 CODES' IN MSG  */ 02262000
         OI    MSG0RCDE,B'00010000'                             03S0572 02263000
*      ELSE                            /*DS NOT SCRATCHED            */ 02264000
*        IF VLRTNCDE(VOLCTR) = STATCDE4 |  /*STATUS CODE IS 4       */  02265000
*           VLRTNCDE(VOLCTR) = STATCDE5 |  /*......OR 5             */  02266000
*           VLRTNCDE(VOLCTR) = STATCDE6 THEN/*                  03S0573 02267000
*                                        ERR MSG TO OPERATOR         */ 02268000
