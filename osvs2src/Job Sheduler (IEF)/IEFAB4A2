         ST    @08,@AL00001+12                                  02S0523 01992000
         L     @08,@PC00001+8                                   02S0523 01993000
         ST    @08,@AL00001+16                                  02S0523 01994000
         L     @15,@CV00308                                     02S0523 01995000
         LA    @01,@AL00001                                     02S0523 01996000
         BALR  @14,@15                                          02S0523 01997000
*  END;                                                         02S0524 01998000
*          END;                                                 01S0525 01999000
*    END;                                                       01S0526 02000000
*  ELSE;                               /*DISP PROCING NOT REQUESTED  */ 02001000
@RF00093 DS    0H                                               01S0528 02002000
*  SIOTDSPD = ON;                      /*INDIC DISPOSITION PROCESSED */ 02003000
@RC00093 L     @08,SIOTPTR                                      01S0528 02004000
         OI    SIOTDSPD(@08),B'01000000'                        01S0528 02005000
*  CMRBPTR = CMNRBPTR;                 /*GET NEXT REQUEST BLOCK PTR  */ 02006000
         L     CMRBPTR,CMNRBPTR(,CMRBPTR)                       01S0529 02007000
*END;                                                           01S0530 02008000
@DE00090 SLR   @08,@08                                          01S0530 02009000
         CR    CMRBPTR,@08                                      01S0530 02010000
         BNE   @DL00090                                         01S0530 02011000
*IF INITIOSW = ON THEN                 /*DUMMY ETIOT BUILT?          */ 02012000
         TM    INITIOSW,B'00000100'                             01S0531 02013000
         BNO   @RF00531                                         01S0531 02014000
*  DO;                                                          01S0532 02015000
*    BETIOFMP = BETIOFMP && BETIOFMP;  /*CLEAR FUNC. MAP             */ 02016000
         XC    BETIOFMP(1),BETIOFMP                             01S0533 02017000
*    BETFREE = ON;                     /*REQUEST ETIOT CLEANUP       */ 02018000
         OI    BETFREE,B'00100000'                              01S0534 02019000
*    IF CATRETCD = ZERO &              /* CATALOG ALLOC ERR? @ZA06981*/ 02020000
*       DSPSCATA = ON THEN             /* ALLOCATE CATALOGS? @ZA06981*/ 02021000
         C     @08,CATRETCD                                     01S0535 02022000
         BNE   @RF00535                                         01S0535 02023000
         L     @02,@PC00001                                     01S0535 02024000
         TM    DSPSCATA(@02),B'01000000'                        01S0535 02025000
         BNO   @RF00535                                         01S0535 02026000
*      BETUNCAT = ON;                  /* SET UNALLOC ALL    @ZA06981*/ 02027000
         OI    BETUNCAT,B'00000100'                             01S0536 02028000
*    CALL IEFAB4A3                     /*CLEAN UP DUMMY ETIOT        */ 02029000
*              (BETIOFMP,JSCBPTR,ZERO,ZERO); /*              @ZA00061*/ 02030000
@RF00535 LA    @02,BETIOFMP                                     01S0537 02031000
         ST    @02,@AL00001                                     01S0537 02032000
