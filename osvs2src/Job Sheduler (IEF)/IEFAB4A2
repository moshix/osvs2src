         BALR  @14,@15                                          03S0594 02368000
*      END;                                                     02S0595 02369000
@RF00593 DS    0H                                               02S0596 02370000
*  END;                                                         02S0596 02371000
@RF00564 DS    0H                                               02S0597 02372000
*IF IRETCODE = ZERO THEN               /*NO ERRORS                   */ 02373000
@RF00541 ICM   @15,15,IRETCODE                                  02S0597 02374000
         BNZ   @RF00597                                         02S0597 02375000
*  DO;                                 /*CONTINUE PROCESSING         */ 02376000
*    IF UNCTLGDS = ON THEN             /*DATA SET TO BE UNCATALOGED  */ 02377000
         TM    UNCTLGDS,B'00100000'                             02S0599 02378000
         BNO   @RF00599                                         02S0599 02379000
*      DO;                             /*SET UP FOR UNCATLG PROCESSOR*/ 02380000
*        DISPID = UCTLGID;             /*SET DISP ID FOR UNCATLG     */ 02381000
         MVI   DISPID,X'01'                                     02S0601 02382000
*        CALL UNCPRCR;                 /*INVOKE UNCATALOG PROCESSOR  */ 02383000
         BAL   @14,UNCPRCR                                      02S0602 02384000
*        MSGID = DSPMSGID;             /*RESET MSG ID IN CASE OF ERR */ 02385000
         MVI   MSGID,X'01'                                      02S0603 02386000
*        DISPID = DELID;               /*RESET DISP ID TO DELETE     */ 02387000
         MVI   DISPID,X'03'                                     02S0604 02388000
*      END;                                                     02S0605 02389000
*    MRETCODE = ZERO;                  /*ZERO RETURN CODE FOR MSG    */ 02390000
@RF00599 SLR   @15,@15                                          02S0606 02391000
         ST    @15,MRETCODE                                     02S0606 02392000
*    MSGMSTAT = OFF;                   /*NO MN STATUS FOR DELETE MSGS*/ 02393000
         NI    MSGMSTAT,B'10111111'                             02S0607 02394000
*    IF PRCRMSGS = YES THEN            /*PROCESS MESSAGES            */ 02395000
         TM    PRCRMSGS,B'10000000'                             02S0608 02396000
         BNO   @RF00608                                         02S0608 02397000
*      CALL IEFAB4B0                   /*INVOKE MSG ROUTINE          */ 02398000
*          (MSGDATA,VLSTPTR,MSGDSNP,MRETCODE,JSCBPTR); /*    @YM07214*/ 02399000
         LA    @15,MSGDATA                                      02S0609 02400000
         ST    @15,@AL00001                                     02S0609 02401000
         LA    @15,VLSTPTR                                      02S0609 02402000
         ST    @15,@AL00001+4                                   02S0609 02403000
         LA    @15,MSGDSNP                                      02S0609 02404000
         ST    @15,@AL00001+8                                   02S0609 02405000
         LA    @15,MRETCODE                                     02S0609 02406000
         ST    @15,@AL00001+12                                  02S0609 02407000
         L     @15,@PC00001+8                                   02S0609 02408000
         ST    @15,@AL00001+16                                  02S0609 02409000
         L     @15,@CV00308                                     02S0609 02410000
         LA    @01,@AL00001                                     02S0609 02411000
         BALR  @14,@15                                          02S0609 02412000
*  END;                                                         02S0610 02413000
@RF00608 DS    0H                                               02S0611 02414000
*RFY TIOENTRY BASED(DDENTPTR);                                  02S0611 02415000
@RF00597 DS    0H                                               02S0612 02416000
*IF TAPEDS = YES &                     /*TAPE DATA SET               */ 02417000
*    NORWND = NO THEN                  /*TAPE TO BE REWOUND          */ 02418000
         TM    TAPEDS,B'00010000'                               02S0612 02419000
         BNO   @RF00612                                         02S0612 02420000
         TM    NORWND,B'01000000'                               02S0612 02421000
         BNZ   @RF00612                                         02S0612 02422000
*  DO DEVCTR = 1 TO TIOEWTCT;          /*SET REWIND INDICATOR(S)     */ 02423000
         LA    DEVCTR,1                                         02S0613 02424000
         B     @DE00613                                         02S0613 02425000
@DL00613 DS    0H                                               02S0614 02426000
*    IF TIOUCBP(DEVCTR) ^= ZERO &      /*DEVICE ALLOCATED            */ 02427000
*        TIOUCBP(DEVCTR) -> UCBREW = NO THEN /*TAPE NOT REWOUND      */ 02428000
         L     @03,DDENTPTR                                     02S0614 02429000
