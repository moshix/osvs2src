         L     @15,@CV00309                                     03S0425 01600000
         LA    @01,@AL00001                                     03S0425 01601000
         BALR  @14,@15                                          03S0425 01602000
*CMRETCDE = IRETCODE;                  /*SET INFOR'L RETURN CODE IN-    01603000
*                                        TO REQUEST BLOCK            */ 01604000
@RF00424 L     @08,IRETCODE                                     03S0426 01605000
         STH   @08,CMRETCDE(,CMRBPTR)                           03S0426 01606000
*IF IRETCODE ^= ZERO THEN              /*ERROR OCCURRED              */ 01607000
         LTR   @08,@08                                          03S0427 01608000
         BZ    @RF00427                                         03S0427 01609000
*  IF CMNODEN = OFF THEN               /*NOT DENSITY ERROR   @YM06419*/ 01610000
         TM    CMNODEN(CMRBPTR),B'00000100'                     03S0428 01611000
         BNZ   @RF00428                                         03S0428 01612000
*    IF CTLGMERR = YES THEN            /*CTLG MGMNT ERROR            */ 01613000
         TM    CTLGMERR,B'00010000'                             03S0429 01614000
         BNO   @RF00429                                         03S0429 01615000
*      CMCATERR = ON;                  /*INDIC IN REQ BLK            */ 01616000
         OI    CMCATERR(CMRBPTR),B'01000000'                    03S0430 01617000
*    ELSE                              /*NOT CATALOG MGT ERROR       */ 01618000
*      CMINTERR = ON;                  /*INDIC INTERNAL ERR IN  03S0431 01619000
*                                        REQ BLOCK                   */ 01620000
         B     @RC00429                                         03S0431 01621000
@RF00429 OI    CMINTERR(CMRBPTR),B'00010000'                    03S0431 01622000
*  ELSE;                               /*                    @YM06419*/ 01623000
@RF00428 DS    0H                                               03S0433 01624000
*  END;                                                         02S0433 01625000
@RC00428 DS    0H                                               02S0433 01626000
@RF00427 DS    0H                                               02S0434 01627000
*IF VLSTPTR ^= ZERO THEN               /*VOL LIST EXIST?             */ 01628000
@RC00310 L     @08,VLSTPTR                                      02S0434 01629000
         LTR   @08,@08                                          02S0434 01630000
         BZ    @RF00434                                         02S0434 01631000
*  DO;                                                          02S0435 01632000
*    RFY (REG0,REG1) RSTD;                                      02S0436 01633000
*    REG0 = VLSTSIZE;                                           02S0437 01634000
         L     REG0,VLSTSIZE                                    02S0437 01635000
*    REG0 = REG0 | SUBPL230;           /*FREE FROM SUBPOOL 230       */ 01636000
         O     REG0,@CF00282                                    02S0438 01637000
*    REG1 = VLSTPTR;                                            02S0439 01638000
         LR    REG1,@08                                         02S0439 01639000
*    GEN(FREEMAIN R,LV=(0),A=(1));     /*FREE VOLUME LIST            */ 01640000
         FREEMAIN R,LV=(0),A=(1)                                        01641000
*    RFY (REG0,REG1) UNRSTD;                                    02S0441 01642000
*  END;                                                         02S0442 01643000
*IF ALETIOSW = ON THEN                 /*WAS AN ETIOT ENTRY ALOCD?   */ 01644000
@RF00434 TM    ALETIOSW,B'00000010'                             02S0443 01645000
         BNO   @RF00443                                         02S0443 01646000
*  DO;                                                          02S0444 01647000
*    BETIOFMP = BETIOFMP && BETIOFMP;  /*CLEAR FUNCTION MAP          */ 01648000
         XC    BETIOFMP(1),BETIOFMP                             02S0445 01649000
*    BETUNAL = ON;                     /*UNALLOCATE ETIOT ENTRY      */ 01650000
         OI    BETUNAL,B'00010000'                              02S0446 01651000
*    CALL IEFAB4A3                     /*CALL DUMMY ETIOT HANDLER TO    01652000
*                                        UNALLOCATE ETIOT ENTRY      */ 01653000
