         NI    PARMSCRO,B'11011111'                             02S0555 02173000
         OI    PARMSCRO,B'01010000'                             02S0555 02174000
*    RFY (REG0,REG1) RSTD;                                      02S0556 02175000
@RF00554 DS    0H                                               02S0557 02176000
*    REG0 = ZERO;                      /*CLEAR REG0 FOR UCB ADDR     */ 02177000
         SLR   REG0,REG0                                        02S0557 02178000
