         BNZ   @RF00296                                         04S0296 01078000
         L     @08,@PC00001+16                                  04S0296 01079000
         ICM   @15,15,CATALDSN(@08)                             04S0296 01080000
         BZ    @RF00296                                         04S0296 01081000
*      BETALCAT = ON;                  /* INDICATE YES       @ZA06981*/ 01082000
         OI    BETALCAT,B'00001000'                             04S0297 01083000
*    CALL IEFAB4A3                     /* CALL DUMMY ETIOT BUILDER   */ 01084000
*            (BETIOFMP,JSCBPTR,CURDSABP,SIOTPTR,CATALDSN); /*@ZA06981*/ 01085000
@RF00296 LA    @08,BETIOFMP                                     04S0298 01086000
         ST    @08,@AL00001                                     04S0298 01087000
         L     @08,@PC00001+8                                   04S0298 01088000
         ST    @08,@AL00001+4                                   04S0298 01089000
         LA    @08,CURDSABP                                     04S0298 01090000
         ST    @08,@AL00001+8                                   04S0298 01091000
         LA    @08,SIOTPTR                                      04S0298 01092000
         ST    @08,@AL00001+12                                  04S0298 01093000
         L     @08,@PC00001+16                                  04S0298 01094000
         ST    @08,@AL00001+16                                  04S0298 01095000
         L     @15,@CV00307                                     04S0298 01096000
         LA    @01,@AL00001                                     04S0298 01097000
         BALR  @14,@15                                          04S0298 01098000
*    RETCODE = REG15;                  /* SAVE RETURN CODE.          */ 01099000
         LR    RETCODE,REG15                                    04S0299 01100000
*    IF RETCODE = ALCATERR THEN        /* ALLOC STEPCAT ERR  @ZA06981*/ 01101000
         CH    RETCODE,@CH00038                                 04S0300 01102000
         BNE   @RF00300                                         04S0300 01103000
*      DO;                             /* STEPCATS NOT ALLOC @ZA06981*/ 01104000
*        CATRETCD = RETCODE;           /* SET FOR SEGS AB4A2 @ZA06981*/ 01105000
         ST    RETCODE,CATRETCD                                 04S0302 01106000
*        RSNCODE = CATERR;             /* SET FOR MSG PROCES @ZA06981*/ 01107000
         L     @08,@PC00001+20                                  04S0303 01108000
         MVC   RSNCODE(2,@08),@CH00090                          04S0303 01109000
*      END;                            /*                    @ZA06981*/ 01110000
*    IF RETCODE ^= INITERR THEN        /* ETIOT INITIALIZATION ERROR?*/ 01111000
@RF00300 CH    RETCODE,@CH00163                                 04S0305 01112000
         BE    @RF00305                                         04S0305 01113000
*      INITIOSW = ON;                  /* DUMMY ETIOT HAS BEEN INIT'D*/ 01114000
         OI    INITIOSW,B'00000100'                             04S0306 01115000
*    IF RETCODE = ZERO &               /* NO ERRORS?                 */ 01116000
*       SIOVAMDS = ON THEN             /* VIO DATA SET       @ZA00061*/ 01117000
@RF00305 LTR   RETCODE,RETCODE                                  04S0307 01118000
         BNZ   @RF00307                                         04S0307 01119000
         L     @08,SIOTPTR                                      04S0307 01120000
         TM    SIOVAMDS(@08),B'00100000'                        04S0307 01121000
         BNO   @RF00307                                         04S0307 01122000
*      ALETIOSW = ON;                  /* ETIOT ENTRY SUCCESSFULLY      01123000
*                                         ALLOCATED                  */ 01124000
         OI    ALETIOSW,B'00000010'                             04S0308 01125000
*  END;                                                         04S0309 01126000
@RF00307 DS    0H                                               04S0310 01127000
*IF RETCODE = ZERO THEN                /*IS RETURN CODE 0            */ 01128000
@RF00289 LTR   RETCODE,RETCODE                                  02S0310 01129000
         BNZ   @RF00310                                         02S0310 01130000
*  DO;                                                          02S0311 01131000
*    IF DSPKEEP = YES                  /*DISP = KEEP                 */ 01132000
*        | DSPPASS = YES THEN          /*OR DISP = PASS              */ 01133000
         TM    DSPKEEP,B'00011000'                              02S0312 01134000
         BZ    @RF00312                                         02S0312 01135000
*      DO;                             /*PROCESS KEEP/PASS DISPOSIT'N*/ 01136000
*/********************************************************************/ 01137000
*/*                                                                  */ 01138000
*/* SEGMENT NAME - KEEPPRCR                                          */ 01139000
*/*                                                                  */ 01140000
*/* DESCRIPTIVE NAME - PROCESS KEEP/PASS DISPOSITION                 */ 01141000
*/*                                                                  */ 01142000
*/* FUNCTION -                                                       */ 01143000
*/*   THIS ROUTINE PERFORMS THE FOLLOWING FUNCTIONS:                 */ 01144000
*/*     .INVOKES MESSAGE ROUTINE, IEFAB4B0 TO ISSUE APPROPRIATE      */ 01145000
*/*      DISPOSITION MESSAGE.                                        */ 01146000
*/*     .IF NECESSARY, INVOKES THE RECATALOG FUNCTION OF THE         */ 01147000
*/*      CATALOG MANAGEMENT PROCESSOR TO UPDATE THE DSCB TTR OF      */ 01148000
*/*      THE DATA SET IN THE CATALOG. NO MESSAGES ARE ISSUED FOR     */ 01149000
*/*      THIS FUNCTION.                                              */ 01150000
*/*                                                                  */ 01151000
*/* ENTRY - IN-LINE SEGMENT                                          */ 01152000
*/*   PURPOSE - PROCESS KEEP/PASS DISPOSITION                        */ 01153000
*/*   INPUT -                                                        */ 01154000
*/*     PRCSFMAP - DISPOSITION PROCESSOR FUNCTION MAP                */ 01155000
*/*     MSGDATA  - INTERFACE DATA FOR MESSAGE ROUTINE                */ 01156000
*/*     VLSTPTR  - PTR TO VOLUME LIST                        @YM08111*/ 01157000
*/*     MSGDSNP - PTR TO DATA SET NAME                       @YM07214*/ 01158000
*/*     MRETCODE - RETURN CODE TO PRINT (0 FOR KEEP/PASS)            */ 01159000
*/* EXIT -                                                           */ 01160000
*/*   CONDITION - REQUIRED FUNCTIONS COMPLETED                       */ 01161000
*/*   OUTPUT -                                                       */ 01162000
*/*     KEEP/PASS MESSAGE ISSUED                                     */ 01163000
*/*     CATALOG UPDATED IF REQUIRED                                  */ 01164000
*/*   RETURN CODE - NONE                                             */ 01165000
*/********************************************************************/ 01166000
*IF PRCRMSGS = YES |                   /*MSGS ISSUED TO SYSOUT D.S.  */ 01167000
*    PRCMSTAT = YES THEN               /*MONITOR STATUS IS ACTIVE    */ 01168000
         TM    PRCRMSGS,B'11000000'                             03S0314 01169000
         BZ    @RF00314                                         03S0314 01170000
*  CALL IEFAB4B0                       /*INVOKE MESSAGE ROUTINE      */ 01171000
*      (MSGDATA,VLSTPTR,MSGDSNP,MRETCODE,JSCBPTR); /*        @YM07214*/ 01172000
         LA    @08,MSGDATA                                      03S0315 01173000
         ST    @08,@AL00001                                     03S0315 01174000
         LA    @08,VLSTPTR                                      03S0315 01175000
         ST    @08,@AL00001+4                                   03S0315 01176000
         LA    @08,MSGDSNP                                      03S0315 01177000
         ST    @08,@AL00001+8                                   03S0315 01178000
         LA    @08,MRETCODE                                     03S0315 01179000
         ST    @08,@AL00001+12                                  03S0315 01180000
         L     @08,@PC00001+8                                   03S0315 01181000
         ST    @08,@AL00001+16                                  03S0315 01182000
         L     @15,@CV00308                                     03S0315 01183000
         LA    @01,@AL00001                                     03S0315 01184000
         BALR  @14,@15                                          03S0315 01185000
*IF UDSCBTTR = YES THEN                /*DSCB TTR TO BE UPDATED      */ 01186000
@RF00314 TM    UDSCBTTR,B'00000100'                             03S0316 01187000
         BNO   @RF00316                                         03S0316 01188000
*  DO;                                 /*INVOKE RECATLG TO HANDLE    */ 01189000
*    PRCRMSGS = OFF;                   /*NO MESSAGES TO SYSOUT       */ 01190000
*    PRCMSTAT = OFF;                   /*NO MSGS TO CONSOLES/TERMINAL*/ 01191000
*    PRCREMSG = ON;                    /*NO ERROR MSGS               */ 01192000
         OI    PRCREMSG,B'00100000'                             03S0320 01193000
         NI    PRCRMSGS,B'00111111'                             03S0320 01194000
*    RCATLG = ON;                      /*INDICATE RECATLG FCN        */ 01195000
         OI    RCATLG,B'10000000'                               03S0321 01196000
*    CALL CTLGPRCR;                    /*CALL CATLG MGMNT PROCESSOR  */ 01197000
         BAL   @14,CTLGPRCR                                     03S0322 01198000
*  END;                                                         03S0323 01199000
*IRETCODE = ZERO;                      /* RESET INFO RETCODE         */ 01200000
@RF00316 SLR   @08,@08                                          03S0324 01201000
         ST    @08,IRETCODE                                     03S0324 01202000
*CTLGMERR = OFF;                       /* RESET CTLG MAN ERROR BIT   */ 01203000
         NI    CTLGMERR,B'11101111'                             03S0325 01204000
*      END;                                                     02S0326 01205000
*    ELSE                              /*DISP NOT KEEP               */ 01206000
*      IF DSPDEL = YES THEN            /*DISP = DELETE               */ 01207000
         B     @RC00312                                         02S0327 01208000
@RF00312 TM    DSPDEL,B'00000100'                               02S0327 01209000
         BNO   @RF00327                                         02S0327 01210000
*        DO;                                                    02S0328 01211000
*          IF DADS = YES THEN          /*DIRECT ACCESS DATA SET      */ 01212000
         TM    DADS,B'00001000'                                 02S0329 01213000
         BNO   @RF00329                                         02S0329 01214000
*            DO;                                                02S0330 01215000
*              IF DSPNOSMF = YES THEN  /*SMF RECORD NOT REQ'D        */ 01216000
         L     @08,@PC00001                                     02S0331 01217000
         TM    DSPNOSMF(@08),B'00000100'                        02S0331 01218000
         BNO   @RF00331                                         02S0331 01219000
*                SUPRSMF = ON;         /*SET INDIC TO SUPPRESS IT    */ 01220000
         OI    SUPRSMF,B'10000000'                              02S0332 01221000
*              IF DSPPURGE = YES THEN  /*IS DISP = PURGE ?           */ 01222000
@RF00331 L     @08,@PC00001                                     02S0333 01223000
         TM    DSPPURGE(@08),B'00001000'                        02S0333 01224000
         BNO   @RF00333                                         02S0333 01225000
*                PURGEDS = ON;         /*NEED TO PURGE DATA SET      */ 01226000
         OI    PURGEDS,B'00001000'                              02S0334 01227000
*              IF DSPTIONQ = YES THEN  /*TIOT ENQ ?                  */ 01228000
@RF00333 L     @08,@PC00001                                     02S0335 01229000
         TM    DSPTIONQ(@08),B'00010000'                        02S0335 01230000
         BNO   @RF00335                                         02S0335 01231000
*                TIOTNQED = ON;        /*TIOT IS ENQED               */ 01232000
         OI    TIOTNQED,B'00010000'                             02S0336 01233000
*              IF CMINVUCB = YES THEN  /*UCB PTR IN SIOT VALID?      */ 01234000
@RF00335 TM    CMINVUCB(CMRBPTR),B'00000100'                    02S0337 01235000
         BNO   @RF00337                                         02S0337 01236000
*                INVUCBAD = ON;        /*NO,INDICATE CAN'T USE       */ 01237000
         OI    INVUCBAD,B'10000000'                             02S0338 01238000
*              ELSE                    /*UCB PTR IN SIOT NOT VALID   */ 01239000
*                DLUCBPTR  = SIOUCBAD; /*SET PTR TO UCB FOR SCRATCH  */ 01240000
         B     @RC00337                                         02S0339 01241000
@RF00337 L     @08,SIOTPTR                                      02S0339 01242000
         SLR   @02,@02                                          02S0339 01243000
         ICM   @02,7,SIOUCBAD(@08)                              02S0339 01244000
         ST    @02,DLUCBPTR                                     02S0339 01245000
*            END;                                               02S0340 01246000
@RC00337 DS    0H                                               02S0341 01247000
*          CALL DELPRCR;               /*INVOKE DELETE PROCESSOR     */ 01248000
@RF00329 BAL   @14,DELPRCR                                      02S0341 01249000
*          IF VIRTDS = YES &           /*VIRTUAL DATA SET            */ 01250000
*             DSABPTR ^= ZERO THEN     /*DSAB EXISTS                 */ 01251000
         TM    VIRTDS,B'00000001'                               02S0342 01252000
         BNO   @RF00342                                         02S0342 01253000
         L     @08,DSABPTR                                      02S0342 01254000
         SLR   @02,@02                                          02S0342 01255000
         CR    @08,@02                                          02S0342 01256000
         BE    @RF00342                                         02S0342 01257000
*            TIOEFSRT = ZERO;          /*CLEAR UCB POINTER IN TIOT   */ 01258000
         L     @08,DSABTIOT(,@08)                               02S0343 01259000
         STCM  @02,7,TIOEFSRT(@08)                              02S0343 01260000
*        END;                                                   02S0344 01261000
*      ELSE                            /*DISP NOT = DELETE           */ 01262000
*        IF DSPCATLG  = YES THEN       /*DISP = CATLG                */ 01263000
         B     @RC00327                                         02S0345 01264000
@RF00327 TM    DSPCATLG,B'00000010'                             02S0345 01265000
         BNO   @RF00345                                         02S0345 01266000
*          DO;                                                  02S0346 01267000
*            IF TAPEDS = ON THEN       /*TAPE DATA SET ?             */ 01268000
         TM    TAPEDS,B'00010000'                               02S0347 01269000
         BNO   @RF00347                                         02S0347 01270000
*              DO;                                              02S0348 01271000
*                TAPEDENS = JFCDEN;                             02S0349 01272000
         L     @08,JFCBPTR                                      02S0349 01273000
         MVC   TAPEDENS(1),JFCDEN(@08)                          02S0349 01274000
*                IF S3400DSP = YES THEN /*DISP = 3400                */ 01275000
         L     @08,SIOTPTR                                      02S0350 01276000
         TM    S3400DSP(@08),B'01000000'                        02S0350 01277000
         BNO   @RF00350                                         02S0350 01278000
*                  CTLG3400 = ON;                               02S0351 01279000
         OI    CTLG3400,B'00100000'                             02S0351 01280000
*              END;                                             02S0352 01281000
@RF00350 DS    0H                                               02S0353 01282000
*            CALL CTLGPRCR;            /*CALL CATLG PROC             */ 01283000
@RF00347 BAL   @14,CTLGPRCR                                     02S0353 01284000
*          END;                                                 02S0354 01285000
*        ELSE                          /*DISP NOT = CATLG            */ 01286000
*          IF DSPUCTLG = YES THEN      /*IS PURGE CATLG NECESSARY ?  */ 01287000
         B     @RC00345                                         02S0355 01288000
@RF00345 TM    DSPUCTLG,B'00000001'                             02S0355 01289000
         BNO   @RF00355                                         02S0355 01290000
*            CALL UNCPRCR;             /*CALL UNCATLG PROC           */ 01291000
         BAL   @14,UNCPRCR                                      02S0356 01292000
*    CMRETCDE = IRETCODE;              /*SET INFORMATIONAL RETURN       01293000
*                                        CODE                        */ 01294000
@RF00355 DS    0H                                               02S0357 01295000
@RC00345 DS    0H                                               02S0357 01296000
@RC00327 DS    0H                                               02S0357 01297000
@RC00312 MVC   CMRETCDE(2,CMRBPTR),IRETCODE+2                   02S0357 01298000
*    IF SCRERROR = YES THEN            /*SCRATCH ERROR               */ 01299000
         TM    SCRERROR,B'00000100'                             02S0358 01300000
         BNO   @RF00358                                         02S0358 01301000
*      CMSCRERR = ON;                  /*INDICATE IN REQ BLK         */ 01302000
         OI    CMSCRERR(CMRBPTR),B'10000000'                    02S0359 01303000
*    ELSE                              /*NOT SCRATCH ERROR           */ 01304000
*      IF CTLGMERR = YES THEN          /*CTLG MGMNT ERROR            */ 01305000
         B     @RC00358                                         02S0360 01306000
@RF00358 TM    CTLGMERR,B'00010000'                             02S0360 01307000
         BNO   @RF00360                                         02S0360 01308000
*        CMCATERR = ON;                /*INDICATE IN REQ BLK         */ 01309000
         OI    CMCATERR(CMRBPTR),B'01000000'                    02S0361 01310000
*  END;                                                         02S0362 01311000
*ELSE                                  /*NO CORE FOR VOL LIST        */ 01312000
*  DO;                                 /*USE JFCB/JFCBX TO ISSUE MSGS*/ 01313000
         B     @RC00310                                         02S0363 01314000
@RF00310 DS    0H                                               02S0364 01315000
*/********************************************************************/ 01316000
*/*                                                                  */ 01317000
*/* SEGMENT NAME - NVLSTPRC                                          */ 01318000
*/*                                                                  */ 01319000
*/* DESCRIPTIVE NAME - NO VOLUME LIST PROCESSOR                      */ 01320000
*/*                                                                  */ 01321000
*/* FUNCTION -                                                       */ 01322000
*/*   THIS ROUTINE PERFORMS THE FOLLOWING FUNCTIONS:                 */ 01323000
*/*     .DETERMINE IF ANY OF THE REQUIRED PROCESSING CAN BE DONE     */ 01324000
*/*      WITHOUT A VOLUME LIST                                       */ 01325000
*/*     .INVOKE ALTERNATE MESSAGE ROUTINE TO CONSTRUCT REQUIRED      */ 01326000
*/*      MESSAGE USING JFCB/JFCBX INSTEAD OF A VOL LIST.             */ 01327000
*/*                                                                  */ 01328000
*/*   OPERATION -                                                    */ 01329000
*/*     IF THE REQUIRED FUNCTION IS EITHER 1) TO PROCESS KEEP OR     */ 01330000
*/*     PASS DISPOSITION, 2) DELETE A TAPE DATA SET, 3) TO UNCATALOG */ 01331000
*/*     A DATA SET, AND/OR 4) TO UPDATE THE DSCB TTR OF THE DATA SET,*/ 01332000
*/*     THIS ROUTINE WILL INVOKE THE APPROPRIATE PROCESSOR. THE      */ 01333000
*/*     PROCESS FUNCTION MAP WILL INDICATE THAT NO MESSAGES ARE TO   */ 01334000
*/*     BE ISSUED. THIS ROUTINE WILL DETERMINE THE MESSAGE, IF ANY,  */ 01335000
*/*     TO ISSUE. THE ALTERNATE MESSAGE ROUTINE IS INVOKED TO        */ 01336000
*/*     CONSTRUCT AND ISSUE THE MESSAGE.                             */ 01337000
*/*                                                                  */ 01338000
*/*     IF NO FUNCTIONS CAN BE PROCESSED THE NECESSARY INDICATORS    */ 01339000
*/*     ARE SET UP FOR A 'NOT (DISP) 5' MESSAGE AND THE ALTERNATE    */ 01340000
*/*     MESSAGE ROUTINE IS INVOKED TO CONSTRUCT AND ISSUE THE        */ 01341000
*/*     MESSAGE.                                             @YM08111*/ 01342000
*/*                                                                  */ 01343000
*/* ENTRY - IN-LINE SEGMENT                                          */ 01344000
*/*     PURPOSE - SEE FUNCTION                                       */ 01345000
*/*                                                                  */ 01346000
*/*   INPUT -                                                        */ 01347000
*/*     DISPRCR - INDICATES DISPOSITION TO BE PROCESSED              */ 01348000
*/*     DISPID - ID ASSOCIATED WITH NORMAL DISPOSITION               */ 01349000
*/*     MSGID - MSG # ID ASSOCIATED WITH NORMAL DISPOSITION MESSAGE  */ 01350000
*/*     MSGDSNP - DATA SET NAME                              @YM07214*/ 01351000
*/*     PRCSFMAP - PROCESS OR FUNCTION MAP                           */ 01352000
*/*     MSGDATA - DATA FOR MESSAGE ROUTINES                          */ 01353000
*/*     JFCBPTR - PTR TO JFCB OF REQUEST BEING PROCESSED             */ 01354000
*/*     SJFXPTR - PTR TO FIRST JFCBX FOR DATA SET                    */ 01355000
*/*                                                                  */ 01356000
*/* EXITS -                                                          */ 01357000
*/*   CONDITION - NECESSARY PROCESSING COMPLETED                     */ 01358000
*/*     OUTPUT -                                                     */ 01359000
*/*       DSCB TTR UPDATED                                           */ 01360000
*/*       DATA SETS KEPT,PASSED,DELETED (TAPE ONLY), OR              */ 01361000
*/*       UNCATALOGED                                                */ 01362000
*/*       DATA SET DISPOSITION MESSAGES ISSUED                       */ 01363000
*/*       INFORMATIONAL RETURN CODE SET IN REQUEST BLOCK             */ 01364000
*/*     RETURN CODE - 4 (SAME AS ON ENTRY)                           */ 01365000
*/*                                                                  */ 01366000
*/********************************************************************/ 01367000
*IF DSPUCTLG = YES |          /*CAN PROCESS CONDITIONS      */  03S0364 01368000
*    (DSPDEL = YES &                                            03S0364 01369000
*     TAPEDS = YES) |                                           03S0364 01370000
*     DSPKEEP = YES |                                           03S0364 01371000
*     DSPPASS = YES |/*                                         03S0364 01372000
*                                        ANY NORMAL PROCESSING TO DO?*/ 01373000
*     (RCATLG = YES &                  /*RECATALOG D.S.      @YM06164*/ 01374000
*      UDSCBTTR = YES) THEN            /*UPDATE DSCB TTR     @YM06164*/ 01375000
         TM    DSPUCTLG,B'00000001'                             03S0364 01376000
         BO    @RT00364                                         03S0364 01377000
         TM    DSPDEL,B'00000100'                               03S0364 01378000
         BNO   @GL00046                                         03S0364 01379000
         TM    TAPEDS,B'00010000'                               03S0364 01380000
         BO    @RT00364                                         03S0364 01381000
@GL00046 TM    DSPKEEP,B'00011000'                              03S0364 01382000
         BNZ   @RT00364                                         03S0364 01383000
         TM    RCATLG,B'10000000'                               03S0364 01384000
         BNO   @RF00364                                         03S0364 01385000
         TM    UDSCBTTR,B'00000100'                             03S0364 01386000
         BNO   @RF00364                                         03S0364 01387000
@RT00364 DS    0H                                               03S0365 01388000
*  DO;                                 /*YES,DO PROC'NG BUT NO MSGS  */ 01389000
*    PRCRMSGS = OFF;                   /*MESSAGES NOT REQUESTED      */ 01390000
*    PRCMSTAT = OFF;                   /*NO MN STATUS MESSAGES       */ 01391000
*    PRCREMSG = ON;                    /*NO ERROR MESSAGES           */ 01392000
         OI    PRCREMSG,B'00100000'                             03S0368 01393000
         NI    PRCRMSGS,B'00111111'                             03S0368 01394000
*    IF UDSCBTTR = YES THEN            /*DSCB TTR TO BE UPDATED      */ 01395000
         TM    UDSCBTTR,B'00000100'                             03S0369 01396000
         BNO   @RF00369                                         03S0369 01397000
*      DO;                             /*YES, INVOKE RECTLG FCN      */ 01398000
*        RCATLG = ON;                                           03S0371 01399000
         OI    RCATLG,B'10000000'                               03S0371 01400000
*        CALL CTLGPRCR;                /*CATALOG PROCESSOR           */ 01401000
         BAL   @14,CTLGPRCR                                     03S0372 01402000
*      END;                                                     03S0373 01403000
*    IF DSPDEL = YES & TAPEDS = YES THEN/*                      03S0374 01404000
*                                        DISP = DELETE FOR TAPE D.S. */ 01405000
@RF00369 TM    DSPDEL,B'00000100'                               03S0374 01406000
         BNO   @RF00374                                         03S0374 01407000
         TM    TAPEDS,B'00010000'                               03S0374 01408000
         BNO   @RF00374                                         03S0374 01409000
*      DO;                             /*YES,PROCESS NORMAL DISP     */ 01410000
*        CALL DELPRCR;                 /*INVOKE DELETE PROCESSOR     */ 01411000
         BAL   @14,DELPRCR                                      03S0376 01412000
*        IF UNCTLGDS = ON THEN         /*WAS DATA SET UNCATALOGED    */ 01413000
         TM    UNCTLGDS,B'00100000'                             03S0377 01414000
         BNO   @RF00377                                         03S0377 01415000
*          DO;                         /*YES,ISSUE (NOT) UNCTL'D MSG */ 01416000
*            IF CTLGMERR = NO THEN     /*ERROR ON UNCATALOG?         */ 01417000
         TM    CTLGMERR,B'00010000'                             03S0379 01418000
         BNZ   @RF00379                                         03S0379 01419000
*              DISPID = UCTLGID;       /*NO,SET ID FOR UNCTLG MSG    */ 01420000
         MVI   DISPID,X'01'                                     03S0380 01421000
*            ELSE                      /*ERROR ON UNCATALOG          */ 01422000
*              DO;                                              03S0381 01423000
         B     @RC00379                                         03S0381 01424000
@RF00379 DS    0H                                               03S0382 01425000
*                DISPID = NUCTLGID;    /*SET ID FOR NOT UNCTLG'D MSG */ 01426000
         MVI   DISPID,X'07'                                     03S0382 01427000
*                MSGID = NCMSGID;      /*CHANGE MSG # ID TO CTLG ERR.*/ 01428000
         MVI   MSGID,X'03'                                      03S0383 01429000
*                MRETCODE = CATLERRC(IRETCODE/4);/*TRANSLATE CATLG      01430000
*                                        ERROR CODE                  */ 01431000
         L     @00,IRETCODE                                     03S0384 01432000
         SRDA  @00,32                                           03S0384 01433000
         D     @00,@CF00163                                     03S0384 01434000
         SLR   @08,@08                                          03S0384 01435000
         IC    @08,CATLERRC-1(@01)                              03S0384 01436000
         ST    @08,MRETCODE                                     03S0384 01437000
*              END;                                             03S0385 01438000
*            IF NOMSGSW = NO THEN      /*NEED TO ISSUE MESSAGE       */ 01439000
