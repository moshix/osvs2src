         TITLE 'IEFAB4A2 - DISPOSITION PROCESSING CONTROL              *00001000
                        '                                               00002000
IEFAB4A2 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(24)                                          01S0001 00007000
         DC    C'IEFAB4A2 78054  UZ16857 '                      01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  LA    @11,4095(,@12)                                   01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         USING @PSTART+4095,@11                                 01S0001 00014000
         L     @00,@SIZDATD                                     01S0001 00015000
         GSPACE   R,LV=(0)                                              00016000
         LR    @10,@01                                          01S0001 00017000
         USING @DATD,@10                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         LM    @00,@01,20(@13)                                  01S0001 00020000
         ST    @10,8(,@13)                                      01S0001 00021000
         LR    @13,@10                                          01S0001 00022000
         MVC   @PC00001(24),0(@01)                              01S0001 00023000
*RETCODE = ZERO;                       /*INIT RETURN CODE FIELD      */ 00024000
         SLR   RETCODE,RETCODE                                  01S0086 00025000
*CATRETCD = ZERO;                      /*INIT STEPCAT RTNCD  @ZA06981*/ 00026000
         SLR   @08,@08                                          01S0087 00027000
         ST    @08,CATRETCD                                     01S0087 00028000
*CMRBPTR = FRBPTR;                     /*SET PTR TO FIRST REQ BLOCK  */ 00029000
         L     @02,@PC00001+4                                   01S0088 00030000
         L     CMRBPTR,FRBPTR(,@02)                             01S0088 00031000
*INITIOSW = OFF;                       /*ETIOT HASNT BEEN INITIALIZED*/ 00032000
         NI    INITIOSW,B'11111011'                             01S0089 00033000
*DO WHILE (CMRBPTR ^= ZERO);           /*PROCESS ALL REQUESTS ON CHN */ 00034000
         B     @DE00090                                         01S0090 00035000
@DL00090 DS    0H                                               01S0091 00036000
*  SIOTPTR = CMSIOPTR;                 /*SET PTR TO SIOT OF REQUEST  */ 00037000
         L     @08,CMSIOPTR(,CMRBPTR)                           01S0091 00038000
         ST    @08,SIOTPTR                                      01S0091 00039000
*  DSABPTR = SIOTETIO;                 /*SET PTR TO DSAB, IF ANY     */ 00040000
         MVC   DSABPTR(4),SIOTETIO(@08)                         01S0092 00041000
*  IF CMNODISP = NO THEN               /*DISP PROCESSING REQUESTED?  */ 00042000
         TM    CMNODISP(CMRBPTR),B'10000000'                    01S0093 00043000
         BNZ   @RF00093                                         01S0093 00044000
*    DO;                               /*YES, SEE IF REQUIRED        */ 00045000
*      IF SIOTSSDS = NO THEN           /*SUBSYSTEM DATA SET?         */ 00046000
         TM    SIOTSSDS(@08),B'10000000'                        01S0095 00047000
         BNZ   @RF00095                                         01S0095 00048000
*        DO;                           /*NO,CONTINUE SET UP & TEST   */ 00049000
*          IF SIOUCBAD ^= ZERO THEN    /*DEVICE ALLOC'D TO REQ?      */ 00050000
         SLR   @02,@02                                          01S0097 00051000
         ICM   @02,7,SIOUCBAD(@08)                              01S0097 00052000
         LTR   @02,@02                                          01S0097 00053000
         BZ    @RF00097                                         01S0097 00054000
*            DO;                       /*YES,MOVE DEV TYPE TO SIOT   */ 00055000
*              SIOTDEVT = SIOUCBAD -> UCBTYP;                   01S0099 00056000
         MVC   SIOTDEVT(4,@08),UCBTYP(@02)                      01S0099 00057000
*              IF SIO3DACC = YES THEN   /*DIRECT ACCESS DEVICE       */ 00058000
         TM    SIO3DACC(@08),B'00100000'                        01S0100 00059000
         BNO   @RF00100                                         01S0100 00060000
*                DO;                   /*TURN OFF OPTION BITS@YM05476*/ 00061000
*                  SIOUBYT2 = SIOUBYT2 & SHPOWMSK;              01S0102 00062000
         NI    SIOUBYT2(@08),X'DC'                              01S0102 00063000
*                                      /*SHARED FILE AND ALTERNATE      00064000
*                                        POWER BITS OFF      @YM05477*/ 00065000
*                  IF SIOUBYT4 = DA3340 THEN /*IF A 3340     @YM05476*/ 00066000
         CLI   SIOUBYT4(@08),X'0A'                              01S0103 00067000
         BNE   @RF00103                                         01S0103 00068000
*                    SIOUBYT2 = SIOUBYT2 & RPSMASK;             01S0104 00069000
         NI    SIOUBYT2(@08),X'EF'                              01S0104 00070000
*                                      /*RPS BIT OFF ALSO    @YM05476*/ 00071000
*                END;                  /*END IF DIRECT ACCESS@YM05476*/ 00072000
@RF00103 DS    0H                                               01S0106 00073000
*              IF SIO3TAPE = YES THEN  /*TAPE DEVICE         @YM05477*/ 00074000
@RF00100 L     @08,SIOTPTR                                      01S0106 00075000
         TM    SIO3TAPE(@08),B'10000000'                        01S0106 00076000
         BNO   @RF00106                                         01S0106 00077000
*                SIOUBYT2 = SIOUBYT2 & OPTIONS;                 01S0107 00078000
         NI    SIOUBYT2(@08),X'F4'                              01S0107 00079000
*                                      /*OPTIONS BITS OFF    @ZA04104*/ 00080000
*              IF SIO3UREC = YES THEN  /*UNIT RECORD DEVICE  @YM05476*/ 00081000
@RF00106 L     @08,SIOTPTR                                      01S0108 00082000
         TM    SIO3UREC(@08),B'00001000'                        01S0108 00083000
         BNO   @RF00108                                         01S0108 00084000
*                IF SIOUBYT4 ^= UR3211 THEN /*NOT A 3211     @YM05476*/ 00085000
         CLI   SIOUBYT4(@08),X'09'                              01S0109 00086000
         BE    @RF00109                                         01S0109 00087000
*                  SIOUBYT2 = OPTSOFF; /*ALL OPT BITS OFF    @YM05476*/ 00088000
         MVI   SIOUBYT2(@08),X'00'                              01S0110 00089000
*                ELSE                  /*3211 UNIT RECORD DEV@YM05476*/ 00090000
*                  SIOUBYT2 = SIOUBYT2 & UCSOPMSK;              01S0111 00091000
         B     @RC00109                                         01S0111 00092000
@RF00109 L     @08,SIOTPTR                                      01S0111 00093000
         NI    SIOUBYT2(@08),X'80'                              01S0111 00094000
*                                      /*ALLOW UCS BIT       @YM05476*/ 00095000
*              IF SIO3DISP = YES THEN  /*GRAPHICS DEVICE     @YM05476*/ 00096000
@RC00109 DS    0H                                               01S0112 00097000
@RF00108 L     @08,SIOTPTR                                      01S0112 00098000
         TM    SIO3DISP(@08),B'00010000'                        01S0112 00099000
         BNO   @RF00112                                         01S0112 00100000
*                SIOUBYT2 = OPTSOFF;   /*ALL OPTION BITS OFF @YM05476*/ 00101000
         MVI   SIOUBYT2(@08),X'00'                              01S0113 00102000
*            END;                                               01S0114 00103000
@RF00112 DS    0H                                               01S0115 00104000
*          JFCBPTR = SJFCBPTR;         /*SET PTR TO JFCB FOR REQUEST */ 00105000
@RF00097 L     @08,SIOTPTR                                      01S0115 00106000
         L     @02,SJFCBPTR(,@08)                               01S0115 00107000
         ST    @02,JFCBPTR                                      01S0115 00108000
*          IF JFCBNVOL ^= ZERO &        /*VOL SERS EXIST      @ZA10127* 00109000
*             ((SIO3DACC = YES &        /*D.A. DATA SET               * 00110000
*               ((SCTSNEW = YES &                               01S0116 00111000
*               SIODADSM = YES) |       /*DADSM SUCCESSFUL IF NEW     * 00112000
*               SCTSNEW = NO)) |        /*OLD D.A. DATA SET           * 00113000
*               (SIO3TAPE = YES &       /*IS A TAPE DATA SET          * 00114000
*                 JFCBVOLS(ONE)^=SCRTAPE) | /*NOT UNOPENED SCRATCH TAPE 00115000
*                                                             @YM06419* 00116000
*                 (SIO3TAPE = NO &      /*NOT TAPE AND                * 00117000
*                  SIO3DACC = NO &      /*NOT D.A. AND                * 00118000
*                  DSABPTR ^= ZERO)) THEN/*                     01S0116 00119000
*                                        SEE IF DISP PROSSING NEEDED */ 00120000
         CLI   JFCBNVOL(@02),0                                  01S0116 00121000
         BE    @RF00116                                         01S0116 00122000
         TM    SIO3DACC(@08),B'00100000'                        01S0116 00123000
         BNO   @GL00003                                         01S0116 00124000
         TM    SCTSNEW(@08),B'00000100'                         01S0116 00125000
         BNO   @GL00005                                         01S0116 00126000
         TM    SIODADSM(@08),B'00000100'                        01S0116 00127000
         BO    @RT00116                                         01S0116 00128000
@GL00005 L     @08,SIOTPTR                                      01S0116 00129000
         TM    SCTSNEW(@08),B'00000100'                         01S0116 00130000
         BZ    @RT00116                                         01S0116 00131000
@GL00003 L     @08,SIOTPTR                                      01S0116 00132000
         TM    SIO3TAPE(@08),B'10000000'                        01S0116 00133000
         BNO   @GL00002                                         01S0116 00134000
         L     @08,JFCBPTR                                      01S0116 00135000
         CLI   JFCBVOLS(@08),X'FF'                              01S0116 00136000
         BNE   @RT00116                                         01S0116 00137000
@GL00002 L     @08,SIOTPTR                                      01S0116 00138000
         TM    SIO3TAPE(@08),B'10100000'                        01S0116 00139000
         BNZ   @RF00116                                         01S0116 00140000
         ICM   @08,15,DSABPTR                                   01S0116 00141000
         BZ    @RF00116                                         01S0116 00142000
@RT00116 DS    0H                                               01S0117 00143000
*            DO;                       /*DISP PROCESSING IS REQUIRED */ 00144000
*/********************************************************************/ 00145000
*/*                                                                  */ 00146000
*/* SEGMENT NAME - PRCSDISP                                          */ 00147000
*/*                                                                  */ 00148000
*/* DESCRIPTIVE NAME - PROCESS DISPOSITIONS                          */ 00149000
*/*                                                                  */ 00150000
*/* FUNCTION -                                                       */ 00151000
*/*   THIS ROUTINE PERFORMS THE FOLLOWING FUNCTIONS:                 */ 00152000
*/*     .DETERMINE THE DISPOSITION PROCESSOR TO BE INVOKED           */ 00153000
*/*     .SET UP THE PROCESSOR INTERFACE                              */ 00154000
*/*     .INVOKE THE REQUIRED PROCESSOR                               */ 00155000
*/*                                                                  */ 00156000
*/*   OPERATION -                                                    */ 00157000
*/*     .DETERMINING THE DISPOSITION                                 */ 00158000
*/*      THE REQUEST BLOCK BEING PROCESSED INDICATES WHETHER NORMAL, */ 00159000
*/*      CONDITIONAL, OR AN OVERRIDING DISPOSITION IS TO BE USED. IF */ 00160000
*/*      THE CONDITIONAL DISPOSITION IS TO BE USED, BUT NONE WAS     */ 00161000
*/*      SPECIFIED, THE NORMAL DISPOSITION WILL BE PROCESSED. THE    */ 00162000
*/*      DISPOSITION BITS ARE MOVED FROM THE SIOT OR REQUEST BLOCK   */ 00163000
*/*      INTO A COMMON WORK BYTE, DISPRCR, FOR FUTURE USE.           */ 00164000
*/*     .SETTING UP THE INTERFACE                                    */ 00165000
*/*      THE MAJOR PART OF THE INTERFACE IS THE VOLUME LIST. IT IS   */ 00166000
*/*      THE REQUIRED INPUT TO DADSM SCRATCH AND TO CATALOG MANAGE-  */ 00167000
*/*      MENT. IF AN ETIOT DOESN'T EXIST, A DUMMY ETIOT MUST BE      */ 00168000
*/*      CREATED, IF A DIRECT ACCESS DATA SET IS TO BE DELETED,      */ 00169000
*/*      BECAUSE SCRATCH USES THE ETIOT. THE VOL                     */ 00170000
*/*      LIST IS ALSO USED BY THE NORMAL MESSAGE ROUTINE,            */ 00171000
*/*      IEFAB4B0, TO CONSTRUCT THE VOL SERS LINE(S) OF THE DISPOSI- */ 00172000
*/*      TION MESSAGES. IF CORE IS NOT AVAILABLE FOR THIS LIST THE   */ 00173000
*/*      FUNCTIONS THAT REQUIRE IT (CATLG,RECATLG AND SCRATCH) CAN   */ 00174000
*/*      NOT BE PERFORMED. FOR DIRECT ACCESS DELETE, IF A            */ 00175000
*/*      DUMMY ETIOT MUST BE BUILT, BUT A                            */ 00176000
*/*      FAILURE OCCURS BUILDING THE DUMMY ETIOT, SCRATCH            */ 00177000
*/*      CANNOT BE PERFORMED. FOR THESE DATA SETS A 'NOT             */ 00178000
*/*      (DISP) 5' MSG IS ISSUED VIA AN ALTERNATE MSG ROUTINE WHICH  */ 00179000
*/*      USES THE JFCB/JFCBX TO CONSTRUCT THE VOL SERS LINE(S) OF    */ 00180000
*/*      THE MESSAGE. (SEE NVLSTSPC FOR DETAILS OF PROCESSING        */ 00181000
*/*      DISPOSITIONS WITHOUT A VOLUME LIST.)                        */ 00182000
*/*     .INVOKING THE REQUIRED PROCESSOR                             */ 00183000
*/*      THE PROCESSOR IS DETERMINED FROM THE DISPRCR BYTE. THE      */ 00184000
*/*      PROCESSOR WILL PERFORM THE REQUIRED FUNCTION AND, IF RE-    */ 00185000
*/*      QUIRED, INVOKE THE NORMAL MESSAGE ROUTINE TO ISSUE THE      */ 00186000
*/*      APPROPRIATE DISPOSITION MESSAGES.                           */ 00187000
*/*     .CLEAN UP INTERFACE                                          */ 00188000
*/*      IF A VOLUME LIST WERE BUILT IT IS FREED. IF A DUMMY ETIOT   */ 00189000
*/*      ENTRY WERE ALLOCATED, IEFAB4A3 IS CALLED TO UNALLOCATE IT.  */ 00190000
*/*                                                                  */ 00191000
*/* ENTRY - IN-LINE SEGMENT                                          */ 00192000
*/*   PURPOSE - SEE FUNCTION                                         */ 00193000
*/*   INPUT -                                                        */ 00194000
*/*     REQUEST BLOCK BEING PROCESSED                                */ 00195000
*/*       CMRBBYT1 OF RB CONTAINS INDICATOR FOR DISP TO BE USED      */ 00196000
*/*         AND AN INDICATOR TO DETERMINE VALIDITY OF SIOUCBAD       */ 00197000
*/*       CMODISP CONTAINS OVERRIDE DISP, IF REQUIRED                */ 00198000
*/*     SIOT                                                         */ 00199000
*/*       SCTSDISP FIELD CONTAINS NORMAL DISP                        */ 00200000
*/*       SIOTALTD CONTAINS CONDITIONAL DISPOSITION                  */ 00201000
*/*       SCTUTYPE CONTAINS DEVICE TYPE ALLOCATED TO THE REQUEST     */ 00202000
*/*       SIOUCBAD CONTAINS ADDR OF A UCB ALLOCATED TO THE REQUEST   */ 00203000
*/*     JFCB                                                         */ 00204000
*/*       JFCBNVOL - NO. VOLUMES FOR DATA SET                @ZA10127*/ 00205000
*/*       JFCBVOLS - FIRST FIVE VOL SERS OF DATA SET                 */ 00206000
*/*       JFCDEN - TAPE DENSITY AT WHICH DATA SET RECORDED           */ 00207000
*/*     JFCBX(S)                                                     */ 00208000
*/*       JFCBXVOL - UP TO 15 VOL SERS PER JFCBX                     */ 00209000
*/*                                                                  */ 00210000
*/* EXIT -                                                           */ 00211000
*/*   CONDITION - DISPOSITIONS PROCESSED NORMALLY                    */ 00212000
*/*     OUTPUT -                                                     */ 00213000
*/*       DISPOSITIONS PROCESSED AND MESSAGES ISSUED                 */ 00214000
*/*       REASON CODE = 0                                            */ 00215000
*/*     RETURN CODES -                                               */ 00216000
*/*       RETCODE = 0                                                */ 00217000
*/*       CMRETCDE = RETURN CODE FROM SCRATCH/CATALOG MANAGEMENT     */ 00218000
*/*                                                                  */ 00219000
*/*   CONDITION - NO CORE FOR VOLUME LIST, OR A DUMMY ETIOT ENTRY    */ 00220000
*/*               COULDN'T BE ALLOCATED (DIRECT ACCESS DELETE ONLY)  */ 00221000
*/*     OUTPUT -                                                     */ 00222000
*/*       DISPOSITION MESSAGES ISSUED                                */ 00223000
*/*       DATA SETS KEPT,PASSED OR UNCATALOGED                       */ 00224000
*/*       TAPE DATA SETS DELETED                                     */ 00225000
*/*     RETURN CODES -                                               */ 00226000
*/*       RETCODE = 4                                                */ 00227000
*/*       CMRETCODE = 0,DISPOSITION PROCESS SUCCESSFULLY             */ 00228000
*/*                   5,DISP NOT PROCESSED BECAUSE NO VOL LIST       */ 00229000
*/*                   OTHER, CATLG MGMNT RETURN CODE (FROM UNCATLG)  */ 00230000
*/*                                                                  */ 00231000
*/********************************************************************/ 00232000
*IF CMORDISP = YES THEN                /*OVERRIDE DISP TO BE USED    */ 00233000
         TM    CMORDISP(CMRBPTR),B'00010000'                    02S0118 00234000
         BNO   @RF00118                                         02S0118 00235000
*  DISPRCR = CMODISP;                  /*MOVE FROM REQ BLOCK         */ 00236000
         MVC   DISPRCR(1),CMODISP(CMRBPTR)                      02S0119 00237000
*ELSE                                                           02S0120 00238000
*  IF CMNMDISP = ON            /*USE NORMAL DISP REQUESTED   */ 02S0120 00239000
*    | (CMCNDISP = ON          /*USE CONDL DISP REQUESTED    */ 02S0120 00240000
*       & SIOTALTD(5:8) = NOCNDISP) THEN/*                      02S0120 00241000
*                                        NORMAL REQ'D OR DEFAULTED   */ 00242000
         B     @RC00118                                         02S0120 00243000
@RF00118 TM    CMNMDISP(CMRBPTR),B'01000000'                    02S0120 00244000
         BO    @RT00120                                         02S0120 00245000
         TM    CMCNDISP(CMRBPTR),B'00100000'                    02S0120 00246000
         BNO   @RF00120                                         02S0120 00247000
         L     @08,SIOTPTR                                      02S0120 00248000
         TM    SIOTALTD(@08),B'00001111'                        02S0120 00249000
         BNZ   @RF00120                                         02S0120 00250000
@RT00120 DS    0H                                               02S0121 00251000
*    DISPRCR = SCTSDISP & NMLDMSK;     /*GET NORMAL DISP FROM SIOT   */ 00252000
         L     @08,SIOTPTR                                      02S0121 00253000
         MVC   DISPRCR(1),SCTSDISP(@08)                         02S0121 00254000
         NI    DISPRCR,B'00011111'                              02S0121 00255000
*  ELSE                                /*NORMAL DISP NOT REQ'D       */ 00256000
*    DISPRCR = SIOTALTD & CNDLDMSK;    /*GET COND'L DISP FROM SIOT   */ 00257000
         B     @RC00120                                         02S0122 00258000
@RF00120 L     @08,SIOTPTR                                      02S0122 00259000
         MVC   DISPRCR(1),SIOTALTD(@08)                         02S0122 00260000
         NI    DISPRCR,B'00001111'                              02S0122 00261000
*ALETIOSW = OFF;                       /*ETIOT ENTRY NOT YET ALLOCD     00262000
*                                        FOR THIS REQUEST.           */ 00263000
@RC00120 DS    0H                                               02S0123 00264000
@RC00118 NI    ALETIOSW,B'11111101'                             02S0123 00265000
*/********************************************************************/ 00266000
*/*                                                                  */ 00267000
*/* SEGMENT NAME - BDSPINT                                           */ 00268000
*/*                                                                  */ 00269000
*/* DESCRIPTIVE NAME - BUILD DISP PROCESSING INTERFACE               */ 00270000
*/*                                                                  */ 00271000
*/* FUNCTION -                                                       */ 00272000
*/*   THIS ROUTINE PERFORMS THE FOLLOWING FUNCTIONS:                 */ 00273000
*/*     .SETS UP INPUT FOR THE MESSAGE ROUTINE                       */ 00274000
*/*     .SETS UP INPUT FOR DISPOSITION PROCESSORS                    */ 00275000
*/*     .SETS UP INPUT FOR UNIT UNALLOCATION                         */ 00276000
*/*     .BUILDS DUMMY ETIOT IF NEEDED FOR THIS REQUEST.              */ 00277000
*/*                                                                  */ 00278000
*/*   OPERATION -                                                    */ 00279000
*/*     .INTERFACE TO MESSAGE ROUTINES                               */ 00280000
*/*      THE MESSAGE ID IS SET FOR THE NORMAL DISPOSITION MESSAGE.   */ 00281000
*/*      A DISPOSITION ID IS DETERMINED BY THE POSITION OF THE       */ 00282000
*/*      DISPOSITION BIT WITHIN THE DISPRCR BYTE. IF THE DISPOSITION */ 00283000
*/*      IS CATALOG, THE ROUTINE WILL DETERMINE IF THE CATALOG OR    */ 00284000
*/*      RECATALOG MESSAGE IS TO BE ISSUED AND WILL RESET THE DISP   */ 00285000
*/*      ID IF NECESSARY. IF AN ALIAS NAME WAS SPECIFIED,    @YM07214*/ 00286000
*/*      A POINTER TO IT IS SET UP FOR THE MSG(S).           @YM07214*/ 00287000
*/*                                                                  */ 00288000
*/*     .INTERFACE TO DISP PROCESSORS                                */ 00289000
*/*      THE NECESSARY FUNCTION MAP BITS ARE SET IN EITHER A COMMON  */ 00290000
*/*      BYTE OF THE MAP STRUCTURE, OR IN A BYTE FOR A SPECIFIC      */ 00291000
*/*      PROCESSOR. A VOLUME LIST IS BUILT WHICH CONTAINS ALL THE    */ 00292000
*/*      VOLUME SERIAL NUMBERS AND THE DEVICE TYPE OF THE DATA SET.  */ 00293000
*/*      IF AN ALIAS DSN WAS SPECIFIED, A POINTER TO IT IS   @YM07214*/ 00294000
*/*      SET UP FOR POSSIBLE USE IN RECATALOG AND UNCATALOG  @YM07214*/ 00295000
*/*      OPERATIONS, OTHERWISE, A POINTER TO THE 'REAL'      @YM07214*/ 00296000
*/*      DSN IS SET UP. THE 'REAL' DSN IS ALWAYS SET UP FOR  @YM07214*/ 00297000
*/*      POSSIBLE USE IN SCRATCH AND CATALOG OPERATIONS.     @YM07214*/ 00298000
*/*                                                                  */ 00299000
*/*     .INTERFACE TO UNIT UNALLOCATION                              */ 00300000
*/*      IF THE CURRENT REQUEST IS ONE WHICH                         */ 00301000
*/*      SHOULD BE TREATED AS PRIVATE BY UNIT UNALLOCATION, AN       */ 00302000
*/*      INDICATOR OF THE CONDITION IS SET IN THE COMMON UNALLOCATION*/ 00303000
*/*      REQUEST BLOCK                                               */ 00304000
*/*                                                                  */ 00305000
*/*     .BUILD DUMMY ETIOT                                           */ 00306000
*/*      FOR DIRECT ACCESS DELETE A DUMMY ETIOT AND/OR       @ZA00061*/ 00307000
*/*      ETIOT ENTRY MAY (AS INDICATED IN THE INPUT FUNCTION @ZA00061*/ 00308000
*/*      MAP) HAVE TO BE CREATED.                                    */ 00309000
*/*                                                                  */ 00310000
*/* ENTRY - IN-LINE SEGMENT                                          */ 00311000
*/*   PURPOSE - SEE FUNCTION                                         */ 00312000
*/*   INPUT -                                                        */ 00313000
*/*     CMUNFMAP - COMMON UNALLOC FUNCTION MAP                       */ 00314000
*/*       CMPUBPRV - TAPE VOLUMES WITH OVERIDE DISP = KEEP           */ 00315000
*/*                  TO BE TREATED AS PRIVATE                        */ 00316000
*/*       CMUNRB - COMMON UNALLOC REQUEST BLOCK                      */ 00317000
*/*         CMCNDISP- PROCESS CONDITIONAL DISP                       */ 00318000
*/*         CMORDISP- PROCESS OVERIDE DISP                           */ 00319000
*/*     DISPRCR - INDICATES DISPOSITION TO BE PROCESSED              */ 00320000
*/*     DSPFMAP - INPUT FUNCTION MAP                                 */ 00321000
*/*       INDICATES WHETHER MESSAGES ARE TO BE ISSUED AND THE        */ 00322000
*/*       RECEIVER                                                   */ 00323000
*/*     SIOT                                                         */ 00324000
*/*       SCTUTYPE - DEVICE TYPE ALLOCATED TO DATA SET               */ 00325000
*/*       SCTBYT3 - CONTAINS DATA SET STATUS INDICATORS              */ 00326000
*/*     JFCB                                                         */ 00327000
*/*       JFCBDSNM - DATA SET NAME                                   */ 00328000
*/*       JFCCAT - DATA SET ALREADY CATALOGED                        */ 00329000
*/*       JFCVSL - DATA SET VOL SER LIST HAS BEEN UPDATED            */ 00330000
*/*                                                                  */ 00331000
*/* EXIT -                                                           */ 00332000
*/*   CONDITION - INTERFACES COMPLETED                               */ 00333000
*/*     OUTPUT -                                                     */ 00334000
*/*       CMUNRB                                                     */ 00335000
*/*         CMSETPRV - REQUEST TO BE TREATED AS PRIVATE              */ 00336000
*/*       DEVTYPE - DEVICE TYPE - IF RECATLG, OBTAINED FROM JFCBX.   */ 00337000
*/*                 OTHERWISE OBTAINED FROM SIOT.            @YM03582*/ 00338000
*/*       DUMMY ETIOT ENTRY (ONLY IF NECESSARY)                      */ 00339000
*/*       PROCESSOR FUNCTION MAP                                     */ 00340000
*/*       REASON CODE - 0                                            */ 00341000
*/*       PROCESSOR DATA                                             */ 00342000
*/*       RETCODE = 0                                                */ 00343000
*/*   CONDITION - NO CORE FOR VOLUME LIST OR FAILURE ATTEMPTING      */ 00344000
*/*               TO ALLOCATE DUMMY ETIOT ENTRY                      */ 00345000
*/*       REASON CODE - 0                                            */ 00346000
*/*     RETURN CODES -                                               */ 00347000
*/*       RETCODE = 4                                                */ 00348000
*/*   CONDITION - NO CORE FOR VOLUME LIST                            */ 00349000
*/*     OUTPUT                                                       */ 00350000
*/*       PROCESSOR FUNCTION MAP                                     */ 00351000
*/*       PROCESSOR DATA                                             */ 00352000
*/*     RETURN CODES -                                               */ 00353000
*/*       RETCODE = 4                                                */ 00354000
*/********************************************************************/ 00355000
*AIRTCODE = ZERO;                      /*INIT ALT RET CODE   @YM06071*/ 00356000
         SLR   @08,@08                                          03S0124 00357000
         ST    @08,AIRTCODE                                     03S0124 00358000
*IRETCODE = ZERO;                      /*INIT INFO'L RETURN CODE     */ 00359000
         ST    @08,IRETCODE                                     03S0125 00360000
*PRCSDATA = PRCSDATA && PRCSDATA;      /*CLEAR PROCESSOR DATA AREA   */ 00361000
         XC    PRCSDATA(40),PRCSDATA                            03S0126 00362000
*PRCSFMAP = PRCSFMAP && PRCSFMAP;      /*CLEAR PROCESSOR FCN MAPS    */ 00363000
         XC    PRCSFMAP(6),PRCSFMAP                             03S0127 00364000
*IF DSPIMSGS = ON THEN                 /*ISSUE MSGS?                 */ 00365000
         L     @08,@PC00001                                     03S0128 00366000
         TM    DSPIMSGS(@08),B'10000000'                        03S0128 00367000
         BNO   @RF00128                                         03S0128 00368000
*   PRCRMSGS = ON;                     /*SET IN ALLPRMAP             */ 00369000
