@RT00573 DS    0H                                               03S0574 02281000
*          MSGOPSW = ON;               /*MUST SET INDIC'R FOR MSG RTN*/ 02282000
         OI    MSGOPSW,B'00010000'                              03S0574 02283000
*    END;                                                       03S0575 02284000
@RF00573 DS    0H                                               03S0575 02285000
@RC00571 AL    VOLCTR,@CF00052                                  03S0575 02286000
@DE00570 L     @03,VLSTPTR                                      03S0575 02287000
         CH    VOLCTR,VLENTNO(,@03)                             03S0575 02288000
         BNH   @DL00570                                         03S0575 02289000
*    IF MSG0RCDE = YES THEN            /*D.S. PARTIALLY SCRATCHED    */ 02290000
         TM    MSG0RCDE,B'00010000'                             03S0576 02291000
         BNO   @RF00576                                         03S0576 02292000
*      DO;                             /*ISSUE NORMAL DISP'N MSG     */ 02293000
*        MRETCODE = ZERO;              /*NO ERROR CODE FOR MSG       */ 02294000
         SLR   @15,@15                                          03S0578 02295000
         ST    @15,MRETCODE                                     03S0578 02296000
*        MSGMSTAT = OFF;               /*NO MN STATUS FOR 'DELETE'   */ 02297000
         NI    MSGMSTAT,B'10111111'                             03S0579 02298000
