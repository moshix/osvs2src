*  DO;                                 /*FIND/CREATE VUT BLK TO USE  */ 00091000
*    VUTINSW = OFF;                    /*INIT VUT READ-IN SWITCH     */ 00092000
*    VUTENDSW = OFF;                   /*INIT END-OF-TABLE INDICATOR */ 00093000
         NI    VUTINSW,B'10011111'                              02S0069 00094000
*    SMISVA = VUTSVA;                  /*SET SVA FOR READ            */ 00095000
         L     @12,@PC00001+12                                  02S0070 00096000
         MVC   SMISVA(3),VUTSVA(@12)                            02S0070 00097000
*    SMIBLKID = 38;                    /*SET ID OF VUT               */ 00098000
         MVI   SMIBLKID,X'26'                                   02S0071 00099000
*    SWACTLS = SWACTLS && SWACTLS;     /*CLEAR FCN MAP               */ 00100000
         SLR   @11,@11                                          02S0072 00101000
         ICM   @11,3,SWACTLS                                    02S0072 00102000
         LR    @08,@11                                          02S0072 00103000
         XR    @08,@11                                          02S0072 00104000
         STH   @08,SWACTLS                                      02S0072 00105000
*    SWREAD = ON;                      /*SET FCN MAP FOR READ        */ 00106000
*    SWNEW = ON;                       /*CREATE NEW EPA              */ 00107000
         OI    SWREAD,B'00101000'                               02S0074 00108000
*    IF VUTSVA ^= ZERO THEN            /*VUT CURRENTLY EXISTS        */ 00109000
         ICM   @11,7,VUTSVA(@12)                                02S0075 00110000
         BZ    @RF00075                                         02S0075 00111000
*      DO WHILE((VUTINSW = OFF &          /*NO USERABLE BLOCK READ      00112000
**/              VUTENDSW = OFF));/*                            02S0076 00113000
*                                        GET A USEABLE BLOCK OF TAB  */ 00114000
         B     @DE00076                                         02S0076 00115000
@DL00076 DS    0H                                               02S0077 00116000
*        CALL IEFAB4F7                 /*SWA MANAGER INTERFACE RTN   */ 00117000
*            (SWACTLS,FEPAPTR,DMYPARM,ADDR(SMIDATA),ADDR(VUTPTR),       00118000
*             TBLLNTH,NUMVBLKS);                                02S0077 00119000
