*            SWNEW = OFF;              /*CLEAR REQ FOR NEW EPA       */ 00141000
         NI    SWNEW,B'11110111'                                02S0078 00142000
*            IF VUTENTNO < DIM(VUTENTRY) THEN /*BLOCK NOT FULL       */ 00143000
         L     @12,VUTPTR                                       02S0079 00144000
         CLI   VUTENTNO(@12),24                                 02S0079 00145000
         BNL   @RF00079                                         02S0079 00146000
*              DO;                                              02S0080 00147000
*                VUTINSW = ON;         /*CAN USE THIS BLK - EXIT LOOP*/ 00148000
