         ICM   @11,7,UCBFSER(UCBPTR)                            02S0127 00305000
         CR    @12,@11                                          02S0127 00306000
         BE    @RF00127                                         02S0127 00307000
*  DO;                                 /*DELETE MSG AND UPDATE UCB   */ 00308000
*    MSGID = UCBFSER(1:3);             /*GET MSG ID FOR DOM          */ 00309000
         MVC   MSGID(3),UCBFSER(UCBPTR)                         02S0129 00310000
*     DO; /*MODESET  KEYADDR(ZERO)*/                            02S0130 00311000
*       SPKA (ZERO); /*SET PSW KEY*/                            02S0131 00312000
         SPKA  0(@12)                                           02S0131 00313000
*     END; /*MODESET  KEYADDR(ZERO)*/           /*GET IN KEY 0  02S0132 00314000
*      */                                                       02S0132 00315000
*     DO; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) REGS(USE) RELATE 00316000
