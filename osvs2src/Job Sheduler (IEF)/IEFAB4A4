         ICM   @12,15,LEPAPTR                                   02S0716 01990000
         BNZ   @RF00716                                         02S0716 01991000
*  LEPAPTR = FEPAPTR;                  /*SET PTR TO LAST EPA @Z40FPPD*/ 01992000
         MVC   LEPAPTR(4),FEPAPTR                               02S0717 01993000
*IF VUTASGND = NO THEN                 /*IF NO VUT BLK ASSGN'D YET      01994000
*                                        ON THIS ENTRY       @YM08200*/ 01995000
@RF00716 TM    VUTASGND,B'00000100'                             02S0718 01996000
         BNZ   @RF00718                                         02S0718 01997000
*  DO;                                                          02S0719 01998000
*    FSTVUTSV = SMISVA;                /*SAVE SVA OF FIRST   @YM08200*/ 01999000
