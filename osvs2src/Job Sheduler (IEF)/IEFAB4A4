*      END;                                                     02S0090 00171000
@RC00084 DS    0H                                               02S0090 00172000
@RC00079 DS    0H                                               02S0090 00173000
@DE00076 TM    VUTINSW,B'01100000'                              02S0090 00174000
         BZ    @DL00076                                         02S0090 00175000
*    LEPAPTR = FEPAPTR;                /*SET PTR TO LAST EPA         */ 00176000
@RF00075 MVC   LEPAPTR(4),FEPAPTR                               02S0091 00177000
*    IF VUTINSW = NO THEN              /*NO BLK TO USE-ASSIGN NEW BLK*/ 00178000
         TM    VUTINSW,B'01000000'                              02S0092 00179000
         BNZ   @RF00092                                         02S0092 00180000
*      CALL CRVUTAB;                   /*GET/INIT NEW BLOCK          */ 00181000
         BAL   @14,CRVUTAB                                      02S0093 00182000
*  END;                                                         02S0094 00183000
@RF00092 DS    0H                                               02S0095 00184000
*IF UNALNPST = NO THEN                 /*POST FUNCTION REQUIRED      */ 00185000
@RF00066 L     @12,@PC00001                                     02S0095 00186000
         TM    UNALNPST(@12),B'10000000'                        02S0095 00187000
         BNZ   @RF00095                                         02S0095 00188000
*  DO;                                 /*INIT ALLOC. Q-MGR INTERFACE */ 00189000
*    QMGRRB = QMGRRB && QMGRRB;        /*CLEAR REQUEST BLOCK         */ 00190000
         XC    QMGRRB(28),QMGRRB                                02S0097 00191000
*    QMGRBPTR = ADDR(QMGRRB);          /*SET PTR TO REQ BLK          */ 00192000
         LA    @12,QMGRRB                                       02S0098 00193000
