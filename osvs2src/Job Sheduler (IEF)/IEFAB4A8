*/*                                                                  */ 00057000
*/* SEGMENT NAME - SCNVRLST                                          */ 00058000
*/*                                                                  */ 00059000
*/* DESCRIPTIVE NAME - SCAN VOLUME RELEASE LIST                      */ 00060000
*/*                                                                  */ 00061000
*/* FUNCTION -                                                       */ 00062000
*/*   THIS ROUTINE SCANS THE VOLUME RELEASE LIST FOR DUPLICATE OR    */ 00063000
*/*   INVALID ENTRIES. EACH SUCH ENTRY IS FLAGGED IN THE FLAG BYTE   */ 00064000
*/*   (VRLFLAGS) OF THAT ENTRY.                                      */ 00065000
*/*                                                                  */ 00066000
*/* ENTRY - IN-LINE SEGMENT                                          */ 00067000
*/*   PURPOSE - TO IDENTIFY DUPLICATE/INVALID ENTRIES IN VRLIST      */ 00068000
*/*   INPUT -                                                        */ 00069000
*/*     VRLSTPTR - POINTER TO VOLUME RELEASE LIST                    */ 00070000
*/*     VRLIST - VOLUME RELEASE LIST (SEE INPUT SECTION OF A4A8DATA  */ 00071000
*/*              FOR FORMAT OF LIST)                                 */ 00072000
*/* EXIT -                                                           */ 00073000
*/*   CONDITION - SCAN COMPLETE                                      */ 00074000
*/*     OUTPUT -                                                     */ 00075000
*/*       VRLDUPE - ON IN ANY DUPLICATE/INVALID ENTRY                */ 00076000
*/*     RETURN CODE - NONE                                           */ 00077000
*/********************************************************************/ 00078000
*DO ENTCTR = 1 TO VRLENTNO;            /*CHECK EACH LIST ENTRY       */ 00079000
         LA    @10,1                                            02S0054 00080000
         B     @DE00054                                         02S0054 00081000
@DL00054 DS    0H                                               02S0055 00082000
*  IF VRLDUPE(ENTCTR) = NO THEN        /*ENTRY NOT ALREADY MARKED    */ 00083000
         LR    @15,@10                                          02S0055 00084000
         SLA   @15,3                                            02S0055 00085000
         L     @14,@PC00001+4                                   02S0055 00086000
         L     @14,VRLSTPTR(,@14)                               02S0055 00087000
         ST    @15,@TF00001                                     02S0055 00088000
         ALR   @15,@14                                          02S0055 00089000
         TM    VRLDUPE-8(@15),B'01000000'                       02S0055 00090000
         BNZ   @RF00055                                         02S0055 00091000
*    IF VRLSERNO(ENTCTR,1) ^= ZERO &   /*NON-ZERO ENTRY              */ 00092000
*        VRLSERNO(ENTCTR) ^= BLANKS &  /*NON-BLANK ENTRY             */ 00093000
*        VRLSERNO(ENTCTR,1) ^= TSCRSER THEN /*NOT A TAPE SCRATCH SER */ 00094000
         L     @15,@TF00001                                     02S0056 00095000
         SLR   @09,@09                                          02S0056 00096000
         IC    @09,VRLSERNO-8(@15,@14)                          02S0056 00097000
         CH    @09,@CH00121                                     02S0056 00098000
         BE    @RF00056                                         02S0056 00099000
         LR    @15,@14                                          02S0056 00100000
         AL    @15,@TF00001                                     02S0056 00101000
         CLC   VRLSERNO-8(6,@15),@CC00099                       02S0056 00102000
         BE    @RF00056                                         02S0056 00103000
         AL    @14,@TF00001                                     02S0056 00104000
         CLI   VRLSERNO-8(@14),X'FF'                            02S0056 00105000
         BE    @RF00056                                         02S0056 00106000
*      DO SCNCTR = ENTCTR+1 TO VRLENTNO; /*SEE IF SER IS DUPED       */ 00107000
         AH    @10,@CH00049                                     02S0057 00108000
         B     @DE00057                                         02S0057 00109000
@DL00057 DS    0H                                               02S0058 00110000
*        IF VRLSERNO(SCNCTR) =                                  02S0058 00111000
*            VRLSERNO(ENTCTR) THEN     /*DUPLICATE SER IN LIST       */ 00112000
         SLA   @10,3                                            02S0058 00113000
         L     @15,@PC00001+4                                   02S0058 00114000
         L     @15,VRLSTPTR(,@15)                               02S0058 00115000
         L     @14,ENTCTR                                       02S0058 00116000
         SLA   @14,3                                            02S0058 00117000
         ST    @10,@TF00001                                     02S0058 00118000
         ALR   @10,@15                                          02S0058 00119000
         ALR   @14,@15                                          02S0058 00120000
         CLC   VRLSERNO-8(6,@10),VRLSERNO-8(@14)                02S0058 00121000
         BNE   @RF00058                                         02S0058 00122000
*          DO;                                                  02S0059 00123000
*            VRLDUPE(SCNCTR) = ON;     /*FLAG DUPLICATE ENTRY        */ 00124000
         LR    @10,@15                                          02S0060 00125000
         AL    @10,@TF00001                                     02S0060 00126000
         OI    VRLDUPE-8(@10),B'01000000'                       02S0060 00127000
*            VRLRELNO = VRLRELNO - ONE;/*DECRECEMENT # TO BE RELEASED*/ 00128000
         L     @10,VRLRELNO(,@15)                               02S0061 00129000
         BCTR  @10,0                                            02S0061 00130000
         ST    @10,VRLRELNO(,@15)                               02S0061 00131000
*          END;                                                 02S0062 00132000
*      END;                                                     02S0063 00133000
@RF00058 LA    @10,1                                            02S0063 00134000
         AL    @10,SCNCTR                                       02S0063 00135000
@DE00057 ST    @10,SCNCTR                                       02S0063 00136000
         L     @15,@PC00001+4                                   02S0063 00137000
         L     @01,VRLSTPTR(,@15)                               02S0063 00138000
         C     @10,VRLENTNO(,@01)                               02S0063 00139000
         BNH   @DL00057                                         02S0063 00140000
*    ELSE                              /*INVALID SERIAL IN ENTRY     */ 00141000
*      DO;                                                      02S0064 00142000
         B     @RC00056                                         02S0064 00143000
@RF00056 DS    0H                                               02S0065 00144000
*        VRLDUPE(ENTCTR) = ON;         /*FLAG INVALID ENTRY          */ 00145000
         L     @10,ENTCTR                                       02S0065 00146000
         SLA   @10,3                                            02S0065 00147000
         L     @15,@PC00001+4                                   02S0065 00148000
         L     @15,VRLSTPTR(,@15)                               02S0065 00149000
         ALR   @10,@15                                          02S0065 00150000
         OI    VRLDUPE-8(@10),B'01000000'                       02S0065 00151000
*        VRLRELNO = VRLRELNO - ONE;    /*DECREMENT # TO BE RELEASED  */ 00152000
         L     @10,VRLRELNO(,@15)                               02S0066 00153000
         BCTR  @10,0                                            02S0066 00154000
         ST    @10,VRLRELNO(,@15)                               02S0066 00155000
*      END;                                                     02S0067 00156000
*  ELSE;                               /*ENTRY ALREADY MARKED        */ 00157000
@RF00055 DS    0H                                               02S0069 00158000
*END;                                                           02S0069 00159000
@RC00055 LA    @10,1                                            02S0069 00160000
         AL    @10,ENTCTR                                       02S0069 00161000
@DE00054 ST    @10,ENTCTR                                       02S0069 00162000
