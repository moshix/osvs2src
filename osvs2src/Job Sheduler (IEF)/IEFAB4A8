         TITLE 'IEFAB4A8 - VOLUME RELEASE                              *00001000
                        '                                               00002000
IEFAB4A8 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFAB4A8  73.325'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         MODESET EXTKEY=SCHED,SAVEKEY=(2)                               00015000
         GSPACE  R,LV=(0)                                               00016000
         LR    @11,@01                                          01S0001 00017000
         USING @DATD,@11                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         STC   @02,@OLDKEY                                      01S0001 00020000
         MODESET KEYADDR=(2)                                            00021000
         LM    @00,@02,20(@13)                                  01S0001 00022000
         ST    @11,8(,@13)                                      01S0001 00023000
         LR    @13,@11                                          01S0001 00024000
         MODESET EXTKEY=ZERO                                            00025000
         MVC   @PC00001(20),0(@01)                              01S0001 00026000
         MODESET EXTKEY=SCHED                                           00027000
*RETCODE = ZERO;                       /*INIT RETURN CODE AREA       */ 00028000
         SLR   @10,@10                                          01S0043 00029000
         ST    @10,RETCODE                                      01S0043 00030000
*IF VLRGNDEQ = YES THEN                /*GENERIC DEQ REQUESTED       */ 00031000
         L     @10,@PC00001                                     01S0044 00032000
         TM    VLRGNDEQ(@10),B'10000000'                        01S0044 00033000
         BNO   @RF00044                                         01S0044 00034000
*  DO;                                 /*RELEASE ALL VOLS FOR STEP   */ 00035000
*    GNDQLIST(1:GNENTLN) =             /*MOVE DEQ LIST SKELETON.     */ 00036000
*       GNDQFMT(1:GNENTLN);                                     01S0046 00037000
         L     @10,GNENTLN                                      01S0046 00038000
         BCTR  @10,0                                            01S0046 00039000
         EX    @10,@SM01094                                     01S0046 00040000
*    RFY (REG0,REG1,REG2) RSTD;                                 01S0047 00041000
*    REG1 = ADDR(GNDQLIST);            /*SET ADDR FOR EXECUTE FORM   */ 00042000
         LA    REG1,GNDQLIST                                    01S0048 00043000
*    REG2 = DQTCBP;                    /*TCB PTR FOR DIR DEQ @YM03580*/ 00044000
         L     @10,@PC00001+8                                   01S0049 00045000
