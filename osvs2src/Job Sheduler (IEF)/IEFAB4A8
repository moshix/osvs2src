*    GEN(DEQ MF=(E,(1)),TCB=(2),GENERIC=YES,RET=HAVE)           01S0050 00047000
*        REFS(REG1,REG2);              /*DO GENERIC DEQ FOR VOLS     */ 00048000
         DEQ MF=(E,(1)),TCB=(2),GENERIC=YES,RET=HAVE                    00049000
*    RFY (REG0,REG1,REG2) UNRSTD;                               01S0051 00050000
*  END;                                                         01S0052 00051000
*ELSE                                                           01S0053 00052000
*  DO;                                 /*DETERMINE VOLS TO BE REL'D  */ 00053000
         B     @RC00044                                         01S0053 00054000
@RF00044 DS    0H                                               01S0054 00055000
