*      DO;                             /*NO                          */ 00044000
*        CVRSKFLG = OFF;               /*INDICATE ALG MUST COVER     */ 00045000
         NI    CVRSKFLG(@10),B'01111111'                        01S0054 00046000
*      END;                                                     01S0055 00047000
*    ELSE;                             /*ALG SHOULD NOT PROCESS      */ 00048000
@RF00052 DS    0H                                               01S0057 00049000
*  END;                                                         01S0057 00050000
*ELSE;                                 /*NOT A NEW DEVICE REQ'D FUNC */ 00051000
@RF00049 DS    0H                                               01S0059 00052000
*IF ARUPDATE = ON & CVRIGNOR=OFF THEN  /*WAS A NON-DEMAND VU ENTRY      00053000
*                                        JUST ALLOCATED?             */ 00054000
         L     @10,@PC00001                                     01S0059 00055000
         TM    ARUPDATE(@10),B'01000000'                        01S0059 00056000
         BNO   @RF00059                                         01S0059 00057000
         L     @10,@PC00001+4                                   01S0059 00058000
         L     @10,VUPTR(,@10)                                  01S0059 00059000
         L     @10,VOLALGTP(,@10)                               01S0059 00060000
         TM    CVRIGNOR(@10),B'00000010'                        01S0059 00061000
         BNZ   @RF00059                                         01S0059 00062000
*  DO;                                 /*YES, DO FUNCTION            */ 00063000
*/********************************************************************/ 00064000
*/*                                                                  */ 00065000
*/* SEGMENT NAME - UPALGARU                                          */ 00066000
*/*                                                                  */ 00067000
*/* DESCRIPTIVE NAME - INDICATE THE DEVICE GROUP USED                */ 00068000
*/*                                                                  */ 00069000
*/* FUNCTION -                                                       */ 00070000
*/*     THE ALGORITHM REQUEST LIST ENTRY, ASSOCIATED WITH THE        */ 00071000
*/*     VOLUNIT TABLE ENTRY JUST ALLOCATED, AND THAT REQUEST LIST    */ 00072000
*/*     ENTRY'S GROUP ENTRY THAT WAS ALLOCATED ARE UPDATED           */ 00073000
*/*     TO INDICATE WHETHER OR NOT THE ALGORITHM CAN REARRANGE THIS  */ 00074000
*/*     ALLOCATION IF NECESSARY. THE REQUEST CANNOT BE REARRANGED    */ 00075000
*/*     IF IT IS ALLOCATED TO A PERMANENTLY-RESIDENT OR RESERVED     */ 00076000
*/*     DIRECT-ACCESS VOLUME.                                        */ 00077000
*/*                                                                  */ 00078000
*/*     THIS ROUTINE ALSO INDICATES IN THE ALGORITHM REQUEST LIST    */ 00079000
*/*     ENTRY WHETHER OR NOT THE UNIT JUST ALLOCATED WAS ALREADY     */ 00080000
*/*     ALLOCATED AND, IF IT ALREADY WAS ALLOCATED, WHETHER OR       */ 00081000
*/*     NOT IT WAS ALLOCATED TO THIS JOB.                            */ 00082000
*/*                                                                  */ 00083000
*/*   OPERATION - (THE OPERATION IS DOCUMENTED BY THE CODE ITSELF.)  */ 00084000
*/*                                                                  */ 00085000
*/* ENTRY - (INLINE SUBROUTINE)                                      */ 00086000
*/*                                                                  */ 00087000
*/*   PURPOSE - (SEE FUNCTION)                                       */ 00088000
*/*                                                                  */ 00089000
*/*   INPUT - FUNCMAP - FUNCTION MAP                                 */ 00090000
*/*             AFFPROS - INDICATOR WHETHER AFFINITY PROCESSING      */ 00091000
*/*                       IS BEING DONE.                             */ 00092000
*/*         - VUPTR - PTR TO VOLUNIT ENTRY JUST ALLOCATED            */ 00093000
*/*         - UCBPTR - PTR TO THE UCB JUST ALLOCATED                 */ 00094000
*/*         - ALCWAPTR - PTR TO THE ALLOCATION WORK AREA             */ 00095000
*/*         - UCBGRPID - PTR TO THE GROUP ID OF THE UCB JUST ALLOC'D */ 00096000
*/*                                                                  */ 00097000
*/* EXIT - NORMAL - (INLINE SUBROUTINE)                              */ 00098000
*/*                                                                  */ 00099000
*/*   CONDITION - THE FUNCTION WAS COMPLETED                         */ 00100000
*/*                                                                  */ 00101000
*/*   OUTPUT -                                                       */ 00102000
*/*       THE FOLLOWING ALG REQ LIST ENTRY FIELDS MAY BE             */ 00103000
*/*       SET: CVRALLCT, CVRSKFLG, CVRIGNOR, AND CVRWMOVE.           */ 00104000
*/*                                                                  */ 00105000
*/*       THE FOLLOWING ALG GROUP LIST ENTRY FIELDS MAY BE           */ 00106000
*/*       SET: CVRBOUND AND CVRGRPAL                                 */ 00107000
*/*                                                                  */ 00108000
*/*   RETURN CODE - (NONE)                                           */ 00109000
*/*                                                                  */ 00110000
*/********************************************************************/ 00111000
*GRPFOUND = OFF;                       /*INDICATE GROUP NOT YET FOUND*/ 00112000
         NI    GRPFOUND,B'01111111'                             02S0061 00113000
*GRPLISTP = CVRGRPPT;                  /*PT TO REQS FIRST GROUP ENTRY*/ 00114000
         L     GRPLISTP,CVRGRPPT(,@10)                          02S0062 00115000
*DO WHILE GRPFOUND = OFF;              /*SCAN UNTIL GROUP IS FOUND   */ 00116000
         B     @DE00063                                         02S0063 00117000
@DL00063 DS    0H                                               02S0064 00118000
*  IF UCBGRPID = CVRGRPID THEN         /*IS THIS THE GROUP ALC'D FROM*/ 00119000
         L     @10,@PC00001+16                                  02S0064 00120000
         L     @10,UCBGRPID(,@10)                               02S0064 00121000
         CH    @10,CVRGRPID(,GRPLISTP)                          02S0064 00122000
         BNE   @RF00064                                         02S0064 00123000
*    DO;                               /*YES, UPDATE THIS GROUP ENTRY*/ 00124000
*      GRPFOUND = ON;                  /*INDICATE GROUP IS FOUND     */ 00125000
         OI    GRPFOUND,B'10000000'                             02S0066 00126000
*      IF (UCB3DACC = ON | UCB3TAPE = ON)                       02S0067 00127000
*        & (UCBRESV=ON | UCBPRES=ON) THEN /*P/R OR RESERVED DEVICE   */ 00128000
         L     @10,@PC00001+8                                   02S0067 00129000
         L     @10,UCBPTR(,@10)                                 02S0067 00130000
         TM    UCB3DACC(@10),B'10100000'                        02S0067 00131000
         BZ    @RF00067                                         02S0067 00132000
         TM    UCBRESV(@10),B'00100100'                         02S0067 00133000
         BZ    @RF00067                                         02S0067 00134000
*        DO;                           /*YES,INDICATE NOT MOVABLE    */ 00135000
*          IF NOAFFERR = ON | AFFPROS=ON THEN /*ANY AFFINITY ERROR ? */ 00136000
         L     @10,@PC00001                                     02S0069 00137000
         TM    NOAFFERR(@10),B'00110000'                        02S0069 00138000
         BZ    @RF00069                                         02S0069 00139000
*            DO;                       /*NO, MARK UNMOVABLE          */ 00140000
*              CVRBOUND=CVRBOUND + ONE; /*INCREMENT THE NUMBER BOUND */ 00141000
         LA    @10,1                                            02S0071 00142000
         AH    @10,CVRBOUND(,GRPLISTP)                          02S0071 00143000
         STH   @10,CVRBOUND(,GRPLISTP)                          02S0071 00144000
*              CVRSKFLG = ON;          /*ALG NOT TO CONSIDER         */ 00145000
*              CVRIGNOR = ON;                                   02S0073 00146000
         L     @10,@PC00001+4                                   02S0073 00147000
         L     @10,VUPTR(,@10)                                  02S0073 00148000
         L     @10,VOLALGTP(,@10)                               02S0073 00149000
         OI    CVRSKFLG(@10),B'10000010'                        02S0073 00150000
*            END;                                               02S0074 00151000
*          ELSE                        /*AFFINITIES NOT PROCESSED YET*/ 00152000
*            RTNCODE = COMEBACK;                                02S0075 00153000
         B     @RC00069                                         02S0075 00154000
@RF00069 LA    RTNCODE,4                                        02S0075 00155000
*        END;                                                   02S0076 00156000
*      ELSE                            /*NOT A P/R OR RESERVED DA DEV*/ 00157000
*        DO;                           /*INDICATE ALG CAN REARRANGE  */ 00158000
         B     @RC00067                                         02S0077 00159000
@RF00067 DS    0H                                               02S0078 00160000
*          CVRALLCT = CVRALLCT + ONE;  /*INCREMENT NO.-ALLOC-TO-REQ  */ 00161000
         L     @10,@PC00001+4                                   02S0078 00162000
         L     @10,VUPTR(,@10)                                  02S0078 00163000
         L     @10,VOLALGTP(,@10)                               02S0078 00164000
         LA    @04,1                                            02S0078 00165000
         LH    @03,CVRALLCT(,@10)                               02S0078 00166000
         ALR   @03,@04                                          02S0078 00167000
         STH   @03,CVRALLCT(,@10)                               02S0078 00168000
*          CVRGRPAL = CVRGRPAL + ONE;  /*INCREMENT NO.-ALLOC-TO-GROUP*/ 00169000
         AH    @04,CVRGRPAL(,GRPLISTP)                          02S0079 00170000
         STH   @04,CVRGRPAL(,GRPLISTP)                          02S0079 00171000
*        END;                                                   02S0080 00172000
*    END;                                                       02S0081 00173000
*  ELSE;                               /*NOT THE GROUP ALLOC'D FROM  */ 00174000
@RF00064 DS    0H                                               02S0083 00175000
*  GRPLISTP=GRPLISTP+LENGTH(CVRGPLST); /*POINT TO NEXT GROUP ENTRY   */ 00176000
@RC00064 AH    GRPLISTP,@CH00257                                02S0083 00177000
*END;                                                           02S0084 00178000
@DE00063 TM    GRPFOUND,B'10000000'                             02S0084 00179000
         BZ    @DL00063                                         02S0084 00180000
*IF AFFPROS = ON THEN                  /*IS AFFINITY PROCESSING BEING   00181000
*                                        DONE?                       */ 00182000
         L     @10,@PC00001                                     02S0085 00183000
         TM    AFFPROS(@10),B'00100000'                         02S0085 00184000
         BNO   @RF00085                                         02S0085 00185000
*  DO;                                 /*YES,INDICATE IN ALG REQ LIST*/ 00186000
*    CVRSKFLG = ON;                    /*INDICATE ALG NOT TO CONSIDER*/ 00187000
*    CVRIGNOR = ON;                    /*ALG WILL NEVER CONSIDER     */ 00188000
         L     @10,@PC00001+4                                   02S0088 00189000
         L     @10,VUPTR(,@10)                                  02S0088 00190000
         L     @10,VOLALGTP(,@10)                               02S0088 00191000
         OI    CVRSKFLG(@10),B'10000010'                        02S0088 00192000
*  END;                                                         02S0089 00193000
*ELSE                                  /*THIS IS NOT AFFINITY PROCESS*/ 00194000
*  DO;                                                          02S0090 00195000
         B     @RC00085                                         02S0090 00196000
@RF00085 DS    0H                                               02S0091 00197000
*    CVRDUPLI = OFF;                   /*NOT A DUPLICATE UNIT REQ    */ 00198000
         L     @10,@PC00001+4                                   02S0091 00199000
         L     @10,VUPTR(,@10)                                  02S0091 00200000
         L     @10,VOLALGTP(,@10)                               02S0091 00201000
         NI    CVRDUPLI(@10),B'01111111'                        02S0091 00202000
*  IF CVRIGNOR = OFF THEN              /*MOVABLE REQUEST ?           */ 00203000
         TM    CVRIGNOR(@10),B'00000010'                        02S0092 00204000
         BNZ   @RF00092                                         02S0092 00205000
*    CVRSKFLG = OFF;                   /*YES, HAVE ALG CONSIDER      */ 00206000
         NI    CVRSKFLG(@10),B'01111111'                        02S0093 00207000
*  ELSE;                               /*NON-MOVABLE REQUEST         */ 00208000
@RF00092 DS    0H                                               02S0095 00209000
*    IF UCB3DACC=ON & UCBUSER>ONE &    /*IS THE UNIT ALREADY ALLOC'D?*/ 00210000
*      UCBRESV = OFF &UCBPRES = OFF THEN /*AND POSSIBLY MOVEABLE?    */ 00211000
         L     @10,@PC00001+8                                   02S0095 00212000
         L     @10,UCBPTR(,@10)                                 02S0095 00213000
         TM    UCB3DACC(@10),B'00100000'                        02S0095 00214000
         BNO   @RF00095                                         02S0095 00215000
         CLI   UCBUSER(@10),1                                   02S0095 00216000
         BNH   @RF00095                                         02S0095 00217000
         TM    UCBRESV(@10),B'00100100'                         02S0095 00218000
         BNZ   @RF00095                                         02S0095 00219000
*      DO;                             /*YES, PROCESS                */ 00220000
*        CVRSKFLG = ON;                /*INDICATE ALG NOT TO CONSIDER*/ 00221000
         L     @10,@PC00001+4                                   02S0097 00222000
         L     @10,VUPTR(,@10)                                  02S0097 00223000
         L     @10,VOLALGTP(,@10)                               02S0097 00224000
         OI    CVRSKFLG(@10),B'10000000'                        02S0097 00225000
*        OTHERALC = OFF;               /*ASSUME NOT ALC'D TO THIS JOB*/ 00226000
         NI    OTHERALC,B'01111111'                             02S0098 00227000
*/********************************************************************/ 00228000
*/*                                                                  */ 00229000
*/* SEGMENT NAME - DETWHOAL                                          */ 00230000
*/*                                                                  */ 00231000
*/* DESCRIPTIVE NAME - DETERMINE IF UNIT IS ALREADY ALLOCATED        */ 00232000
*/*                    TO THIS JOB                                   */ 00233000
*/*                                                                  */ 00234000
*/* FUNCTION -                                                       */ 00235000
*/*     THIS ROUTINE SCANS ALL THE SIOTS TO SEE IF ANY OTHER         */ 00236000
*/*     VOLUNIT ENTRIES ARE ALLOCATED TO THE SAME UNIT THE VOLUNIT   */ 00237000
*/*     ENTRY BEING PROCESSED WAS ALLOCATED TO.                      */ 00238000
*/*     IF SUCH A VOLUNIT ENTRY IS FOUND, THE VOLUNIT ENTRY          */ 00239000
*/*     BEING PROCESSED IS UPDATED TO HAVE VOLUME AFFINITY TO THIS   */ 00240000
*/*     REQUEST. VOLUNIT ENTRIES WITH AFFINITY                       */ 00241000
*/*     TO THE VOLUNIT ENTRY BEING PROCESSED ARE ALSO UPDATED TO     */ 00242000
*/*     MAINTAIN THEIR AFFINITIES.                                   */ 00243000
*/*     NOTE: THIS FUNCTION IS DONE TO INSURE THAT THE ALLOCATION'S  */ 00244000
*/*           DEVICE REQUIREMENTS DO NOT INCREASE IF THE VOLUME      */ 00245000
*/*           MUST BE REARRANGED TO ANOTHER UNIT LATER ON BY THE     */ 00246000
*/*           ALGORITHM.                                             */ 00247000
*/*                                                                  */ 00248000
*/*   OPERATION - (THE OPERATION IS DOCUMENTED BY THE CODE ITSELF.)  */ 00249000
*/*                                                                  */ 00250000
*/* ENTRY - (INLINE SUBROUTINE)                                      */ 00251000
*/*                                                                  */ 00252000
*/*   PURPOSE - (SEE FUNCTION)                                       */ 00253000
*/*                                                                  */ 00254000
*/*   INPUT -                                                        */ 00255000
*/*       ALCWAPTR - PTR TO THE ALLOCATION WORK AREA                 */ 00256000
*/*         SIOT1P - PTR TO THE FIRST SIOT IN THE CHAIN              */ 00257000
*/*         VOLUNPTR - PTR TO THE VOLUNIT TABLE                      */ 00258000
*/*         VOLUNSZE - SIZE OF THE VOLUNIT TABLE                     */ 00259000
*/*       VUPTR - PTR TO THE VOLUNIT ENTRY BEING PROCESSED           */ 00260000
*/*       UCBPTR - PTR TO THE UNIT ALLOCATED TO THE VOLUNIT ENTRY    */ 00261000
*/*                BEING PROCESSED.                                  */ 00262000
*/*                                                                  */ 00263000
*/* EXIT - NORMAL - (INLINE SUBROUTINE)                              */ 00264000
*/*                                                                  */ 00265000
*/*   CONDITION - THE FUNCTION HAS BEEN COMPLETED                    */ 00266000
*/*                                                                  */ 00267000
*/*   OUTPUT - OTHERALC SET TO 1 IF UNIT FOUND ALLOCATED TO ANOTHER  */ 00268000
*/*            VOLUNIT ENTRY.                                        */ 00269000
*/*          - THE VOLUNTID AND VOLID FIELDS OF SOME VOLUNIT         */ 00270000
*/*            ENTRIES MAY BE UPDATED.                               */ 00271000
*/*                                                                  */ 00272000
*/*   RETURN CODE - (NONE)                                           */ 00273000
*/*                                                                  */ 00274000
*/********************************************************************/ 00275000
*RFY DSAB BASED(SIOTETIO);             /*BASE ETIOT ON PTR IN SIOT   */ 00276000
*SIOTPTR = SIOT1P;                     /*POINT TO FIRST SIOT IN CHAIN*/ 00277000
         L     @10,@PC00001+12                                  03S0100 00278000
         L     @10,ALCWAPTR(,@10)                               03S0100 00279000
         MVC   SIOTPTR(4),SIOT1P(@10)                           03S0100 00280000
*DO WHILE (SIOTPTR^=ZERO & OTHERALC=OFF); /*SCAN ALL SIOTS           */ 00281000
         B     @DE00101                                         03S0101 00282000
@DL00101 DS    0H                                               03S0102 00283000
*  VUSCANP = SVOLUNAD;                 /*POINT TO SIOTS FIRST VU NTRY*/ 00284000
         L     @10,SIOTPTR                                      03S0102 00285000
         L     VUSCANP,SVOLUNAD(,@10)                           03S0102 00286000
*  RELDEVNO = ZERO;                    /*INITIALIZE TIOT DEVICE ENTRY*/ 00287000
         SLR   RELDEVNO,RELDEVNO                                03S0103 00288000
*  DO NUMVOLUN = ONE TO SVOLUNNO       /*SCAN ALL VU ENTRIES FOR SIOT*/ 00289000
*      WHILE OTHERALC=OFF;             /*..UNTIL UNIT FOUND ALLOC'D  */ 00290000
         LA    NUMVOLUN,1                                       03S0104 00291000
         B     @DE00104                                         03S0104 00292000
@DL00104 TM    OTHERALC,B'10000000'                             03S0104 00293000
         BNZ   @DC00104                                         03S0104 00294000
*    IF VDEVREQD = ON THEN             /*IS A DEVICE REQUIRED ?      */ 00295000
         L     @10,@PC00001+4                                   03S0105 00296000
         L     @10,VUPTR(,@10)                                  03S0105 00297000
         TM    VDEVREQD(@10),B'00100000'                        03S0105 00298000
         BNO   @RF00105                                         03S0105 00299000
*      DO;                             /*YES, PROCESS ENTRY          */ 00300000
*        RELDEVNO = RELDEVNO + ONE;    /*INCREMENT TIOT DEVICE ENTRY */ 00301000
         LA    @06,1                                            03S0107 00302000
         ALR   RELDEVNO,@06                                     03S0107 00303000
*        IF VUSCANP ^= VUPTR           /*IS THIS THE ENTRY JUST ALC'D*/ 00304000
*            & VUSCANP->VOLUNTID^=VOLUNTID /*OR AN AFFINITY REQ TO IT*/ 00305000
*            & VUSCANP->VOLALOC = ON   /*OR NOT ALREADY ALLOCATED    */ 00306000
*            & TIOUCBP(RELDEVNO)=UCBPTR THEN /*OR NOT THE RIGHT UCB ?*/ 00307000
         CR    VUSCANP,@10                                      03S0108 00308000
         BE    @RF00108                                         03S0108 00309000
         LH    @03,VOLUNTID(,VUSCANP)                           03S0108 00310000
         LH    @15,VOLUNTID(,@10)                               03S0108 00311000
         CR    @03,@15                                          03S0108 00312000
         BE    @RF00108                                         03S0108 00313000
         TM    VOLALOC(VUSCANP),B'10000000'                     03S0108 00314000
         BNO   @RF00108                                         03S0108 00315000
         LR    @14,RELDEVNO                                     03S0108 00316000
         SLA   @14,2                                            03S0108 00317000
         L     @01,SIOTPTR                                      03S0108 00318000
         L     @01,SIOTETIO(,@01)                               03S0108 00319000
         L     @01,DSABTIOT(,@01)                               03S0108 00320000
         L     @09,@PC00001+8                                   03S0108 00321000
         L     @08,TIOUCBP-5(@14,@01)                           03S0108 00322000
         LA    @08,0(,@08)                                      03S0108 00323000
         C     @08,UCBPTR(,@09)                                 03S0108 00324000
         BNE   @RF00108                                         03S0108 00325000
*          DO;                         /*NO, PROCESS                 */ 00326000
*            OTHERALC = ON;            /*INDICATE UNIT IS ALREADY ALC*/ 00327000
         OI    OTHERALC,B'10000000'                             03S0110 00328000
*            IF NOAFFERR = ON THEN                              03S0111 00329000
         L     @14,@PC00001                                     03S0111 00330000
         TM    NOAFFERR(@14),B'00010000'                        03S0111 00331000
         BNO   @RF00111                                         03S0111 00332000
*              DO;                                              03S0112 00333000
*                SAVUNIT = VOLUNTID;   /*SAVE UNIT ID OF VU JUST ALCD*/ 00334000
         LR    SAVUNIT,@15                                      03S0113 00335000
*                SAVOLID = VOLID;      /*SAVE VOL ID OF VU JUST ALC'D*/ 00336000
         MVC   SAVOLID(6),VOLID(@10)                            03S0114 00337000
*                VOLUNTID=VUSCANP->VOLUNTID;/*COPY UNIT + VOL ID ..  */ 00338000
         LH    @09,VOLUNTID(,VUSCANP)                           03S0115 00339000
         STH   @09,VOLUNTID(,@10)                               03S0115 00340000
*                VOLID = VUSCANP -> VOLID; /*..FROM THE VU ENTRY        00341000
*                                        ALREADY ALLOC'D TO THE SAME    00342000
*                                        UNIT                        */ 00343000
         MVC   VOLID(6,@10),VOLID(VUSCANP)                      03S0116 00344000
*                VUENTNO=VOLUNSZE/VOLENTLN; /*DETERMINE NUMBER OF       00345000
*                                        VU'S                        */ 00346000
         L     @10,@PC00001+12                                  03S0117 00347000
         L     @10,ALCWAPTR(,@10)                               03S0117 00348000
         L     @00,VOLUNSZE(,@10)                               03S0117 00349000
         SRDA  @00,32                                           03S0117 00350000
         D     @00,VOLENTLN                                     03S0117 00351000
         LR    VUENTNO,@01                                      03S0117 00352000
*                VOLUNP = VOLUNPTR;    /*POINT TO FIRST VOLUNIT ENTRY*/ 00353000
         L     VOLUNP,VOLUNPTR(,@10)                            03S0118 00354000
*                DO VUCTR=ONE TO VUENTNO;  /*SCAN ALL VOLUNIT ENTRIES*/ 00355000
         LA    @10,1                                            03S0119 00356000
         LR    VUCTR,@10                                        03S0119 00357000
         B     @DE00119                                         03S0119 00358000
@DL00119 DS    0H                                               03S0120 00359000
*                  IF VOLUNP->VOLUNTID=SAVUNIT THEN /*SAME UNIT ID      00360000
*                                        CHANGE                      */ 00361000
         CH    SAVUNIT,VOLUNTID(,VOLUNP)                        03S0120 00362000
         BNE   @RF00120                                         03S0120 00363000
*                    DO;               /*YES.,,                      */ 00364000
*                      VOLUNP->VOLUNTID=VOLUNTID; /*COPY NEW UNIT ID */ 00365000
         L     @10,@PC00001+4                                   03S0122 00366000
         L     @10,VUPTR(,@10)                                  03S0122 00367000
         MVC   VOLUNTID(2,VOLUNP),VOLUNTID(@10)                 03S0122 00368000
*                      VOLUNP->VOLID=VOLID; /*COPY NEW VOLUME ID     */ 00369000
         MVC   VOLID(6,VOLUNP),VOLID(@10)                       03S0123 00370000
*                    END;                                       03S0124 00371000
*                  ELSE;               /*NOT THE SAME UNIT ID        */ 00372000
@RF00120 DS    0H                                               03S0126 00373000
