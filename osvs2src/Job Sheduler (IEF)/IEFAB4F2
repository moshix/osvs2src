         TITLE 'IEFAB4F2 - UPDATE ALGORITHM TABLES                     *00001000
                        '                                               00002000
IEFAB4F2 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFAB4F2  74.165'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         L     @00,@SIZDATD                                     01S0001 00014000
         MODESET EXTKEY=SCHED,SAVEKEY=(2)                               00015000
         GSPACE   R,LV=(0)                                              00016000
         LR    @11,@01                                          01S0001 00017000
         USING @DATD,@11                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         STC   @02,@OLDKEY                                      01S0001 00020000
         MODESET KEYADDR=(2)                                            00021000
         LM    @00,@02,20(@13)                                  01S0001 00022000
         ST    @11,8(,@13)                                      01S0001 00023000
         LR    @13,@11                                          01S0001 00024000
         MODESET EXTKEY=ZERO                                            00025000
         MVC   @PC00001(20),0(@01)                              01S0001 00026000
         MODESET EXTKEY=SCHED                                           00027000
*RTNCODE = SUCCESS;                    /*SET A 0 RETURN CODE         */ 00028000
         SLR   RTNCODE,RTNCODE                                  01S0048 00029000
*IF NEWDVREQ = ON THEN                 /*ENTRY NOW REQUIRE A DEVICE? */ 00030000
         L     @10,@PC00001                                     01S0049 00031000
         TM    NEWDVREQ(@10),B'10000000'                        01S0049 00032000
         BNO   @RF00049                                         01S0049 00033000
*  DO;                                 /*YES, MARK ALG REQ LIST ENTRY*/ 00034000
*    CVRDUPLI = OFF;                   /*INDICATE NOT A DUP UNIT REQ */ 00035000
         L     @10,@PC00001+4                                   01S0051 00036000
         L     @10,VUPTR(,@10)                                  01S0051 00037000
         L     @10,VOLALGTP(,@10)                               01S0051 00038000
         NI    CVRDUPLI(@10),B'01111111'                        01S0051 00039000
*    IF CVRSCRTH=OFF & CVRIGNOR=OFF THEN /*IS IT A SCRATCH OR DEMAND    00040000
*                                          REQ ?                     */ 00041000
         TM    CVRSCRTH(@10),B'00001010'                        01S0052 00042000
