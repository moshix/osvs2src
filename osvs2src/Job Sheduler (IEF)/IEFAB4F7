         AL    @01,@CF00724                                     02S0039 00118000
         MVC   SWVA(3,SWEPAPTR),SVALIST(@01)                    02S0039 00119000
*              IF SWWRT = YES |        /* WRITE REQUEST OR           */ 00120000
*                  SWDEL = YES THEN    /* DELETE REQUEST             */ 00121000
         TM    SWWRT(@10),B'01010000'                           02S0040 00122000
         BZ    @RF00040                                         02S0040 00123000
*                SWBLKPTR = SVAB(EPACTR); /* BLOCK PTR TO EPA        */ 00124000
         L     @10,@PC00001+16                                  02S0041 00125000
