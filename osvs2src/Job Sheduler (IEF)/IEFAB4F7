*/******          *****     PROLOG  FOR  EPAINIT    ****       *******/ 00040000
*/*                                                                  */ 00041000
*/* SEGMENT NAME - EPAINIT                                           */ 00042000
*/*                                                                  */ 00043000
*/* DESCRIPTIVE NAME - EPA CREATION AND INITIALIZATION               */ 00044000
*/*                                                                  */ 00045000
*/* FUNCTION - CREATE AND/OR INITIALIZE EPA(S)                       */ 00046000
*/*                                                                  */ 00047000
*/* OPERATION - WILL GETMAIN CORE FOR EPA(S) IF 'NEW' IS SPECIFIED   */ 00048000
*/*    THE EPA FIELDS WILL BE INITIALIZED OR MODIFIED WITH THE       */ 00049000
*/*    APPROPRIATE INPUT PARAMETERS:                                 */ 00050000
*/*      ASSIGN: BLOCK ID AND LENGTH.                                */ 00051000
*/*      READ:   SWA VIRTUAL ADDRESS OF BLOCK PREFIX.                */ 00052000
*/*      WRITE:  ALL OF THE ABOVE AND THE BLOCK ADDRESS.             */ 00053000
*/*    IF MORE THAN ONE EPA WAS CREATED, THE CHAIN POINTER OF ONE EPA*/ 00054000
*/*    WILL POINT TO THE NEXT EPA. THE LAST CHAIN POINTER IS ZERO.   */ 00055000
*/*                                                                  */ 00056000
*/* ENTRY - INCLUDED SEGMENT                                         */ 00057000
*/*    PURPOSE - CREATE AND/OR INITIALIZE EPA'S FOR SWA INTERFACE    */ 00058000
*/*    INPUT DATA - FUNCTION CODE                                    */ 00059000
*/*               - EPA POINTER                                      */ 00060000
*/*               - BLOCK LENGTH                                     */ 00061000
*/*               - NUMBER OF EPA'S CONCERNED.                       */ 00062000
*/*                                                                  */ 00063000
*/* EXITS -                                                          */ 00064000
*/*    CONDITION - FUNCTION COMPLETED                                */ 00065000
*/*    OUTPUT DATA - INITIALIZED EPA'S                               */ 00066000
*/*    RETURN CODE - NONE USED                                       */ 00067000
*/*                                                                  */ 00068000
*/***********             ** END  OF  PROLOG **             **********/ 00069000
*      DO;                                                      02S0025 00070000
*        IF SWNEW = YES THEN           /* MUST CREATE EPA(S)         */ 00071000
         TM    SWNEW(@10),B'00001000'                           02S0026 00072000
         BNO   @RF00026                                         02S0026 00073000
*          DO;                                                  02S0027 00074000
*            LENTH= NUMBLKS * LENGTH(SWAEPA);  /* LENGTH TO GETMAIN  */ 00075000
         L     @10,@PC00001+24                                  02S0028 00076000
         L     @10,NUMBLKS(,@10)                                02S0028 00077000
         SLA   @10,4                                            02S0028 00078000
         STCM  @10,7,LENTH                                      02S0028 00079000
*            REG0 = SPLEN;             /* SET SUBPOOL+LENGTH IN REG 0*/ 00080000
         L     REG0,SPLEN                                       02S0029 00081000
*            GEN (GETMAIN R,LV=(0));   /* GETMAIN                    */ 00082000
         GETMAIN R,LV=(0)                                               00083000
*            EPADOP = REG1;            /* GETMAINED ADDRESS          */ 00084000
         ST    REG1,EPADOP                                      02S0031 00085000
*            IF SWFREE ^= YES &        /* INPUT EPA POINTER IS TO BE */ 00086000
*                SWCHN ^= YES THEN     /* UPDATED IF THE CALLER      */ 00087000
         L     @10,@PC00001                                     02S0032 00088000
         TM    SWFREE(@10),B'00000100'                          02S0032 00089000
         BO    @RF00032                                         02S0032 00090000
         TM    SWCHN(@10),B'10000000'                           02S0032 00091000
         BO    @RF00032                                         02S0032 00092000
*              EPAPTR = EPADOP;        /* INDICATED HE 'NEEDED' IT   */ 00093000
         L     @10,EPADOP                                       02S0033 00094000
         L     @15,@PC00001+4                                   02S0033 00095000
         ST    @10,EPAPTR(,@15)                                 02S0033 00096000
*          END;                        /* GETMAIN GROUP              */ 00097000
@RF00032 DS    0H                                               02S0035 00098000
*        SWEPAPTR = EPADOP;            /* MAP (FIRST) EPA            */ 00099000
@RF00026 L     SWEPAPTR,EPADOP                                  02S0035 00100000
*        DO EPACTR = 1 TO NUMBLKS      /* INITIALIZE EPA(S) UNLESS   */ 00101000
*            WHILE(SWEPAPTR ^= 0);     /* END OF EPA CHAIN REACHED   */ 00102000
         LA    EPACTR,1                                         02S0036 00103000
         B     @DE00036                                         02S0036 00104000
@DL00036 LTR   SWEPAPTR,SWEPAPTR                                02S0036 00105000
         BZ    @DC00036                                         02S0036 00106000
*          IF SWASN ^= YES THEN        /* READ OR WRITE REQUEST      */ 00107000
         L     @10,@PC00001                                     02S0037 00108000
         TM    SWASN(@10),B'10000000'                           02S0037 00109000
         BO    @RF00037                                         02S0037 00110000
*            DO;                                                02S0038 00111000
*              SWVA = SVALIST(EPACTR); /* PREFIX PTR TO EPA          */ 00112000
         LR    @15,EPACTR                                       02S0039 00113000
         SLA   @15,2                                            02S0039 00114000
         L     @14,@PC00001+12                                  02S0039 00115000
         L     @01,SVALISTP(,@14)                               02S0039 00116000
