*         /***********************************************************/ 03381000
*                                                               01S0362 03382000
*         QMSTAT=ACTIVE;                                        01S0362 03383000
*                                                               01S0362 03384000
         MVI   QMSTAT(SQPTR),C'E'                               01S0362 03385000
*         /***********************************************************/ 03386000
*         /*                                                         */ 03387000
*         /* AND POST THE TASK.                                      */ 03388000
*         /*                                                         */ 03389000
*         /***********************************************************/ 03390000
*                                                               01S0363 03391000
*          RFY (R10,R11,R12,R4) RSTD;POSTWRK=ADDR(QMECB);XMFLG='1'B;    03392000
         LA    @08,QMECB(,SQPTR)                                01S0364 03393000
         ST    @08,POSTWRK                                      01S0364 03394000
         OI    XMFLG,B'10000000'                                01S0365 03395000
*          GEN( STM 14,12,12(13));R4=R13;R10=OK;R11=POSTWRK;R12=POSTERA 03396000
*;ASVTPTR = CVTASVT;ASCBENTP = ADDR(ASVTENTY)+(LENGTH(ASVTENTY)*(QMASID 03397000
*1));R13=ASCBADDR;CALL IEA0PT01;R13=R4;                         01S0366 03398000
          STM 14,12,12(13)                                              03399000
         LR    R4,R13                                           01S0367 03400000
         SLR   R10,R10                                          01S0368 03401000
         L     R11,POSTWRK                                      01S0369 03402000
         L     R12,POSTERAD                                     01S0370 03403000
         L     @08,CVTPTR                                       01S0371 03404000
         L     ASVTPTR,CVTASVT(,@08)                            01S0371 03405000
         LA    ASCBENTP,ASVTENTY(,ASVTPTR)                      01S0372 03406000
         L     @06,QMASID(,SQPTR)                               01S0372 03407000
         BCTR  @06,0                                            01S0372 03408000
         SLL   @06,2                                            01S0372 03409000
         ALR   ASCBENTP,@06                                     01S0372 03410000
         L     R13,ASCBADDR(,ASCBENTP)                          01S0373 03411000
         L     @15,CVT0PT01(,@08)                               01S0374 03412000
         BALR  @14,@15                                          01S0374 03413000
         LR    R13,R4                                           01S0375 03414000
*          GEN( LM 14,12,12(13));RFY (R10,R11,R12,R4) UNRSTD;   01S0376 03415000
          LM 14,12,12(13)                                               03416000
*       END;                                                    01S0378 03417000
*     ELSE                                                      01S0379 03418000
*       ;                                                       01S0379 03419000
*                                                               01S0379 03420000
@RF00355 DS    0H                                               01S0380 03421000
*     /***************************************************************/ 03422000
*     /*                                                             */ 03423000
*     /* STEP Q ELEMENT PTR THRU CHAIN FOR NXT DO ITERATION.         */ 03424000
*     /*                                                             */ 03425000
*     /***************************************************************/ 03426000
*                                                               01S0380 03427000
*     SQPTR=QMNXTQF;                                            01S0380 03428000
         L     SQPTR,QMNXTQF(,SQPTR)                            01S0380 03429000
*   END;                                                        01S0381 03430000
@DE00352 LA    @12,QMNXTQF(,SQPTR)                              01S0381 03431000
         L     @11,@PC00003                                     01S0381 03432000
         CR    @12,@11                                          01S0381 03433000
         BNE   @DL00352                                         01S0381 03434000
*   RETURN;                                                     01S0382 03435000
@EL00003 DS    0H                                               01S0382 03436000
@EF00003 DS    0H                                               01S0382 03437000
@ER00003 LM    @14,@12,@SA00003                                 01S0382 03438000
         BR    @14                                              01S0382 03439000
*   END;                                                        01S0383 03440000
*                                                               01S0384 03441000
*   /*****************************************************************/ 03442000
*   /*                                                               */ 03443000
*   /* DELETE AN ELEMENT FROM A WAIT QUEUE                           */ 03444000
*   /*                                                               */ 03445000
*   /*****************************************************************/ 03446000
*                                                               01S0384 03447000
*   ;                                                           01S0384 03448000
*                                                               01S0384 03449000
*   /*****************************************************************/ 03450000
*   /*                                                               */ 03451000
*   /* THIS FUNCTION TRANSFERS THE FORWARD AND BACKWARD POINTERS FROM*/ 03452000
*   /* THE BLOCK INDICATED BY THE PARAMETER TO ITS NEIGHBORING       */ 03453000
*   /* BLOCKS, THUS DETACHING THE BLOCK FROM THE CHAIN. DUE TO THE   */ 03454000
*   /* METHOD OF CHAINING, THE ROUTINE IS EQUALLY APPLICABLE WHEN    */ 03455000
*   /* EITHER OR BOTH NEIGHBOR BLOCKS IS THE QUEUE ROOT. THE CHAINS  */ 03456000
*   /* ARE ALWAYS CIRCULAR, SO WHEN 'NEXT ELEMENT' IN EITHER         */ 03457000
*   /* DIRECTION IS THE Q HEAD, THE QUEUE HEAD ITSELF IS THE ACTUAL  */ 03458000
*   /* HEAD BEING MODIFIED.HOWEVER, NOTE THAT PTRS TO THE Q HEAD     */ 03459000
*   /* POINT ACTUALLY TO A LOC WHICH WHEN ADDED TO THE OFFSET OF THE */ 03460000
*   /* CHAIN FIELDS IN A Q ELEMENT YIELDS THE LOC OF THE Q HEAD. NOTE*/ 03461000
*   /* ALSO THAT ALL WAIT QUEUES USE SAME CHAIN FIELDS IN THE Q      */ 03462000
*   /* ELEMENT BUT ARE ROOTED IN DIFFERENT Q HEADS.                  */ 03463000
*   /*                                                               */ 03464000
*   /*****************************************************************/ 03465000
*                                                               01S0385 03466000
*QMDELETQ:                                                      01S0385 03467000
*   PROC(QDPTR);                                                01S0385 03468000
QMDELETQ STM   @14,@12,12(@13)                                  01S0385 03469000
         MVC   @PC00004(4),0(@01)                               01S0385 03470000
*   RFY                                                         01S0386 03471000
*     QMQBLK BASED(ADDR(QDPTR));                                01S0386 03472000
*                                                               01S0386 03473000
*   /*****************************************************************/ 03474000
*   /*                                                               */ 03475000
*   /* MOVE BKWD PTR FROM THIS BLOCK TO ITS NEXT FWD NEIGHBOR BLOCK. */ 03476000
*   /*                                                               */ 03477000
*   /*****************************************************************/ 03478000
*                                                               01S0387 03479000
*   QMNXTQF->QMNXTQB=QMNXTQB;                                   01S0387 03480000
*                                                               01S0387 03481000
         L     @12,@PC00004                                     01S0387 03482000
         L     @11,QMNXTQF(,@12)                                01S0387 03483000
         L     @12,QMNXTQB(,@12)                                01S0387 03484000
         ST    @12,QMNXTQB(,@11)                                01S0387 03485000
*   /*****************************************************************/ 03486000
*   /*                                                               */ 03487000
*   /* MOVE FWD PTR FROM THIS BLK TO ITS NEXT BKWD NEIGHBOR BLOCK.   */ 03488000
*   /*                                                               */ 03489000
*   /*****************************************************************/ 03490000
*                                                               01S0388 03491000
*   QMNXTQB->QMNXTQF=QMNXTQF;                                   01S0388 03492000
         ST    @11,QMNXTQF(,@12)                                01S0388 03493000
*   RETURN;                                                     01S0389 03494000
@EL00004 DS    0H                                               01S0389 03495000
@EF00004 DS    0H                                               01S0389 03496000
@ER00004 LM    @14,@12,12(@13)                                  01S0389 03497000
         BR    @14                                              01S0389 03498000
*   END;                                                        01S0390 03499000
*                                                               01S0391 03500000
*   /*****************************************************************/ 03501000
*   /*                                                               */ 03502000
*   /* DELETE AN ELEMENT FROM THE MASTER LIST                        */ 03503000
*   /*                                                               */ 03504000
*   /*****************************************************************/ 03505000
*                                                               01S0391 03506000
*   ;                                                           01S0391 03507000
*                                                               01S0391 03508000
*   /*****************************************************************/ 03509000
*   /*                                                               */ 03510000
*   /* IDENTICAL TO THE QMDELETQ PROCEDURE, EXCEPT THIS PROCEDURE    */ 03511000
*   /* USES THE SYMBOLS FOR THE MAIN QUEUE POINTERS.                 */ 03512000
*   /*                                                               */ 03513000
*   /*****************************************************************/ 03514000
*                                                               01S0392 03515000
*QMDELETM:                                                      01S0392 03516000
*   PROC(QEPTR);                                                01S0392 03517000
QMDELETM STM   @14,@12,12(@13)                                  01S0392 03518000
         MVC   @PC00005(4),0(@01)                               01S0392 03519000
*   RFY                                                         01S0393 03520000
*     QMQBLK BASED(ADDR(QEPTR));                                01S0393 03521000
*                                                               01S0393 03522000
*   /*****************************************************************/ 03523000
*   /*                                                               */ 03524000
*   /* MOVE BKWD PTR OUT OF CURRENT BLK TO ITS NXT FWD NEIGHBOR.     */ 03525000
*   /*                                                               */ 03526000
*   /*****************************************************************/ 03527000
*                                                               01S0394 03528000
*   QMNXTF->QMNXTB=QMNXTB;                                      01S0394 03529000
*                                                               01S0394 03530000
         L     @12,@PC00005                                     01S0394 03531000
         L     @11,QMNXTF(,@12)                                 01S0394 03532000
         L     @12,QMNXTB(,@12)                                 01S0394 03533000
         ST    @12,QMNXTB(,@11)                                 01S0394 03534000
*   /*****************************************************************/ 03535000
*   /*                                                               */ 03536000
*   /* MOVE FWD PTR OUT OF CURRENT BLK TO ITS NXT BKWD NEIGHBOR.     */ 03537000
*   /*                                                               */ 03538000
*   /*****************************************************************/ 03539000
*                                                               01S0395 03540000
*   QMNXTB->QMNXTF=QMNXTF;                                      01S0395 03541000
         ST    @11,QMNXTF(,@12)                                 01S0395 03542000
*   RETURN;                                                     01S0396 03543000
@EL00005 DS    0H                                               01S0396 03544000
@EF00005 DS    0H                                               01S0396 03545000
@ER00005 LM    @14,@12,12(@13)                                  01S0396 03546000
         BR    @14                                              01S0396 03547000
*   END;                                                        01S0397 03548000
*                                                               01S0398 03549000
*   /*****************************************************************/ 03550000
*   /*                                                               */ 03551000
*   /* THIS ROUTINE WILL INSERT AN ELEMENT ON ANY QUEUE, INCLUDING   */ 03552000
*   /* THE MAIN QUEUE. PARAMETERS ARE: BLOCK BEING INSERTED (NEW),   */ 03553000
*   /* ITS AFFECTED CHAINING FIELDS (CHAINS), AND THE AFFECTED       */ 03554000
*   /* CHAINING FIELDS OF BOTH OF ITS INTENDED NEIGHBOR BLOCKS       */ 03555000
*   /* (OLDL,OLDR). IN ACTUAL USE IN THIS MODULE, NXT FWD NEIGHBOR IS*/ 03556000
*   /* ALWAYS THE Q HEAD. HOWEVER ROUTINE AS IMPLEMENTED WILL INSERT */ 03557000
*   /* ANYWHERE ON A CHAIN OF THIS TYPE.                             */ 03558000
*   /*                                                               */ 03559000
*   /*****************************************************************/ 03560000
*                                                               01S0398 03561000
*QINSRT:                                                        01S0398 03562000
*   PROC(OLDL,CHAINS,OLDR,NEW);                                 01S0398 03563000
QINSRT   STM   @14,@12,12(@13)                                  01S0398 03564000
         MVC   @PC00006(16),0(@01)                              01S0398 03565000
*   DCL                                                         01S0399 03566000
*     1 CHAINS,                                                 01S0399 03567000
*      2 LEFT PTR(31),                                          01S0399 03568000
*      2 RIGHT PTR(31);                                         01S0399 03569000
*   DCL                                                         01S0400 03570000
*     OLDL PTR(31);                                             01S0400 03571000
*   DCL                                                         01S0401 03572000
*     OLDR PTR(31);                                             01S0401 03573000
*   DCL                                                         01S0402 03574000
*     NEW CHAR(*);                                              01S0402 03575000
*                                                               01S0402 03576000
*   /*****************************************************************/ 03577000
*   /*                                                               */ 03578000
*   /* COPY FWD PTR FROM LEFT NEIGHBOR.                              */ 03579000
*   /*                                                               */ 03580000
*   /*****************************************************************/ 03581000
*                                                               01S0403 03582000
*   LEFT=OLDL;                                                  01S0403 03583000
*                                                               01S0403 03584000
         L     @12,@PC00006+4                                   01S0403 03585000
         L     @11,@PC00006                                     01S0403 03586000
         MVC   LEFT(4,@12),OLDL(@11)                            01S0403 03587000
*   /*****************************************************************/ 03588000
*   /*                                                               */ 03589000
*   /* COPY BKWD PTR FROM RIGHT NEIGHBOR. (ALWAYS ROOT)              */ 03590000
*   /*                                                               */ 03591000
*   /*****************************************************************/ 03592000
*                                                               01S0404 03593000
*   RIGHT=OLDR;                                                 01S0404 03594000
*                                                               01S0404 03595000
         L     @08,@PC00006+8                                   01S0404 03596000
         MVC   RIGHT(4,@12),OLDR(@08)                           01S0404 03597000
*   /*****************************************************************/ 03598000
*   /*                                                               */ 03599000
*   /* CHANGE BOTH PTRS JUST COPIED TO POINT TO BLK JUST INSERTED.   */ 03600000
*   /*                                                               */ 03601000
*   /*****************************************************************/ 03602000
*                                                               01S0405 03603000
*   OLDL=ADDR(NEW);                                             01S0405 03604000
         L     @12,@PC00006+12                                  01S0405 03605000
         ST    @12,OLDL(,@11)                                   01S0405 03606000
*   OLDR=ADDR(NEW);                                             01S0406 03607000
         ST    @12,OLDR(,@08)                                   01S0406 03608000
*   IF EXITPRMP ^= ZERO THEN           /*ESTAE PARM LIST SUPPLIED    */ 03609000
         L     @12,@PC00001+4                                   01S0407 03610000
         L     @12,EXITPRMP(,@12)                               01S0407 03611000
         LTR   @12,@12                                          01S0407 03612000
         BZ    @RF00407                                         01S0407 03613000
*     DO;                                                       01S0408 03614000
*       FREECORE = OFF;                /*ESTAE WON'T FREE Q-MGR CORE */ 03615000
*       GENCLNUP = ON;                 /*DO GENERIC CLEANUP IF ABEND */ 03616000
         OI    GENCLNUP(@12),B'10000000'                        01S0410 03617000
         NI    FREECORE(@12),B'11111011'                        01S0410 03618000
*     END;                                                      01S0411 03619000
*   RETURN;                                                     01S0412 03620000
@EL00006 DS    0H                                               01S0412 03621000
@EF00006 DS    0H                                               01S0412 03622000
@ER00006 LM    @14,@12,12(@13)                                  01S0412 03623000
         BR    @14                                              01S0412 03624000
*   END;                                                        01S0413 03625000
         B     @EL00006                                         01S0413 03626000
*   END                                                         01S0414 03627000
*                                                               01S0414 03628000
*   /*****************************************************************/ 03629000
*   /*                                                               */ 03630000
*   /* END OF PROCEDURE IEFAB4FA.                                    */ 03631000
*   /*                                                               */ 03632000
*   /*****************************************************************/ 03633000
