*            IF UPDTDSAB = ON          /* UPDATE DSAB SIOT PTR?      */ 00174000
*              & SIOTETIO ^= ZERO THEN /* AND DSAB EXIST?            */ 00175000
         L     @08,@PC00001                                     01S0087 00176000
         TM    UPDTDSAB(@08),B'00000100'                        01S0087 00177000
         BNO   @RF00087                                         01S0087 00178000
