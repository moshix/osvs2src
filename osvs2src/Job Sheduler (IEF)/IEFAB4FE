         BNE   @RF00096                                         01S0096 00207000
*              DO;                     /* YES                        */ 00208000
*                ENDLOOP = YES;        /* END LOOP                   */ 00209000
         OI    ENDLOOP,B'10000000'                              01S0098 00210000
*                IF NOEPA = OFF THEN   /* EPA PROCESSING REQUESTED?  */ 00211000
         L     @08,@PC00001                                     01S0099 00212000
         TM    NOEPA(@08),B'00001000'                           01S0099 00213000
         BNZ   @RF00099                                         01S0099 00214000
*                  LEPAPTR = SRBLEPAP; /* YES,SET LAST EPA PTR       */ 00215000
         L     @08,@PC00001+12                                  01S0100 00216000
         MVC   LEPAPTR(4,@08),SRBLEPAP                          01S0100 00217000
*              END;                                             01S0101 00218000
