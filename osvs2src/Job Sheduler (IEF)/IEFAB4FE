         BNO   @RF00089                                         01S0089 00187000
*              DO;                                              01S0090 00188000
*                FIRSTSW= OFF;         /* RESET FIRST TIME SWITCH    */ 00189000
         NI    FIRSTSW,B'10111111'                              01S0091 00190000
*                FSIOTP = SRBSIOTP;    /* FIRST SIOT TO RET. AREA    */ 00191000
         L     @08,@PC00001+12                                  01S0092 00192000
         MVC   FSIOTP(4,@08),SRBSIOTP                           01S0092 00193000
*                IF NOEPA = OFF THEN   /* EPA PROCESSING REQUESTED?  */ 00194000
         L     @05,@PC00001                                     01S0093 00195000
         TM    NOEPA(@05),B'00001000'                           01S0093 00196000
         BNZ   @RF00093                                         01S0093 00197000
*                  FEPAPTR = SRBFEPAP; /* YES,FIRST EPA TO RET. AREA */ 00198000
         MVC   FEPAPTR(4,@08),SRBFEPAP                          01S0094 00199000
*              END;                                             01S0095 00200000
@RF00093 DS    0H                                               01S0096 00201000
*            IF SCTLSIOT = SIOTDSKA THEN /* LAST SIOT?               */ 00202000
@RF00089 L     @08,@PC00001+4                                   01S0096 00203000
