         L     @08,@PC00001                                     02S0283 01101000
         CLC   UNITNM(8,@08),UNITNAME(LUVPTR)                   02S0283 01102000
         BNE   @RF00283                                         02S0283 01103000
*    FOUNDSW = ON;                     /*INDICATE TO EXIT LOOP       */ 01104000
         OI    FOUNDSW,B'01000000'                              02S0284 01105000
*  ELSE                                /*NO MATCH                    */ 01106000
*    LUVPTR = LUVPTR + LENGTH(LUVENTRY); /*POINT TO NEXT EDT LUV ENT */ 01107000
         B     @RC00283                                         02S0285 01108000
@RF00283 AL    LUVPTR,@CF00140                                  02S0285 01109000
*END;                                  /*END EDT LUV SCAN            */ 01110000
@RC00283 AL    LUVCTR,@CF00040                                  02S0286 01111000
@DE00282 L     @08,EDTPTR                                       02S0286 01112000
         L     @08,EDTLUVSP(,@08)                               02S0286 01113000
         C     LUVCTR,LUVENTNO(,@08)                            02S0286 01114000
