*  RETCODE = NAMEERR;                  /*SET ERROR RETURN CODE       */ 01120000
         LA    RETCODE,4                                        02S0288 01121000
*END CKUNITNM;                                                  02S0289 01122000
@EL00002 DS    0H                                               02S0289 01123000
@EF00002 DS    0H                                               02S0289 01124000
@ER00002 BR    @14                                              02S0289 01125000
*SCANEDT: PROC ;                                           /*@Y30LPPC*/ 01126000
SCANEDT  STM   @14,@12,12(@13)                                  02S0290 01127000
*/********************************************************************/ 01128000
*/*                                                          @Y30LPPC*/ 01129000
*/* SEGMENT NAME - SCANEDT                                   @Y30LPPC*/ 01130000
*/*                                                          @Y30LPPC*/ 01131000
*/* DESCRIPTIVE NAME - SCAN EDT AND RETRIEVE INFORMATION     @Y30LPPC*/ 01132000
*/*                                                          @Y30LPPC*/ 01133000
*/* FUNCTION -                                               @Y30LPPC*/ 01134000
*/*   THIS SEGMENT TAKES A LOOK-UP-VALUE ENTRY IN THE        @Y30LPPC*/ 01135000
*/*   ELIGIBLE DEVICES TABLE (EDT) AS INPUT AND PERFORMS     @Y30LPPC*/ 01136000
*/*   ONE OF THE FOLLOWING:                                  @Y30LPPC*/ 01137000
*/*     1) COUNTS THE UCBS ASSOCIATED WITH THE               @Y30LPPC*/ 01138000
*/*        LUV ENTRY                                         @Y30LPPC*/ 01139000
*/*     2) FILLS IN A UCB LIST WITH THE ADDRESS OF EACH      @Y30LPPC*/ 01140000
*/*        UCB ASSOCIATED WITH THE LUV ENTRY                 @Y30LPPC*/ 01141000
*/*                                                          @Y30LPPC*/ 01142000
*/*   OPERATION -                                            @Y30LPPC*/ 01143000
*/*     DOCUMENTED BY THE CODE                               @Y30LPPC*/ 01144000
*/*                                                          @Y30LPPC*/ 01145000
*/* ENTRY - INTERNAL SUBROUTINE                              @Y30LPPC*/ 01146000
*/*   PURPOSE - RETRIEVE INFORMATION FROM THE EDT            @Y30LPPC*/ 01147000
*/*   INPUT -                                                @Y30LPPC*/ 01148000
*/*     LUVPTR - ADDRESS OF THE LUV ENTRY IN THE EDT         @Y30LPPC*/ 01149000
*/*     COUNTUCB - COUNTING FUNCTION DESIRED                 @Y30LPPC*/ 01150000
*/*     FILLINL - FILL-IN-UCB-LIST FUNCTION DESIRED          @Y30LPPC*/ 01151000
*/*     NUMUCBS - COUNT OF UCBS - MUST BE INITIALIZED        @Y30LPPC*/ 01152000
*/*               ON ENTRY (COUNTING FUNCTION ONLY)          @Y30LPPC*/ 01153000
*/*     UCBLPTR - ADDRESS OF UCB LIST (FILL-IN-UCB-LIST      @Y30LPPC*/ 01154000
*/*               FUNCTION ONLY)                             @Y30LPPC*/ 01155000
*/*                                                          @Y30LPPC*/ 01156000
*/* EXIT-NORMAL - RETURN TO CALLER                           @Y30LPPC*/ 01157000
*/*   CONDITION - FUNCTION COMPLETED                         @Y30LPPC*/ 01158000
*/*   OUTPUT -                                               @Y30LPPC*/ 01159000
*/*     NUMUCBS - NUMBER OF UCBS ASSOCIATED WITH THE         @Y30LPPC*/ 01160000
*/*               INPUT LUV (COUNTING FUNCTION ONLY)         @Y30LPPC*/ 01161000
*/*     UCBPUCBP - ARRAY OF UCB ADDRESSES (FILL-IN-UCB-LIST  @Y30LPPC*/ 01162000
*/*                FUNCTION ONLY)                            @Y30LPPC*/ 01163000
*/*   RETURN CODE - 0                                        @Y30LPPC*/ 01164000
*/*                                                          @Y30LPPC*/ 01165000
*/* EXIT-ERROR - NONE                                        @Y30LPPC*/ 01166000
*/*                                                          @Y30LPPC*/ 01167000
*/********************************************************************/ 01168000
*GENPTR = LUVGENP;                     /* FIRST GEN FOR LUV  @Y30LPPC*/ 01169000
         L     GENPTR,LUVGENP(,LUVPTR)                          02S0291 01170000
*UCBLINDX = ONE;                       /* INIT UCBLIST INDEX @Y30LPPC*/ 01171000
         LA    @05,1                                            02S0292 01172000
         LR    UCBLINDX,@05                                     02S0292 01173000
*RFY GRPENTRY BASED(GRPTR);                                /*@Y30LPPC*/ 01174000
*DO GENCTR = 1 TO LUVGENNO;            /* SCAN ALL GENERICS  @Y30LPPC*/ 01175000
         LR    GENCTR,@05                                       02S0294 01176000
         B     @DE00294                                         02S0294 01177000
@DL00294 DS    0H                                               02S0295 01178000
*  GRPTABP = GENGRPTR;                 /* PTR TO FIRST GROUP @Y30LPPC*/ 01179000
         L     GRPTABP,GENGRPTR(,GENPTR)                        02S0295 01180000
*  DO GRPCTR =1 TO GENGRPNO;           /* SCAN ALL GROUPS    @Y30LPPC*/ 01181000
         LA    GRPCTR,1                                         02S0296 01182000
         B     @DE00296                                         02S0296 01183000
@DL00296 DS    0H                                               02S0297 01184000
*    IF FILLINL = ON THEN              /* PUT UCBS IN LIST   @Y30LPPC*/ 01185000
         TM    FILLINL,B'00001000'                              02S0297 01186000
         BNO   @RF00297                                         02S0297 01187000
*      DO UCBCTR = 1 TO GRPUCBNO;      /* SCAN ALL UCBS      @Y30LPPC*/ 01188000
         LA    UCBCTR,1                                         02S0298 01189000
         B     @DE00298                                         02S0298 01190000
@DL00298 DS    0H                                               02S0299 01191000
*          UCBPUCBP(UCBLINDX) =        /* UCB LIST ENTRY     @Y30LPPC*/ 01192000
*            IOSLUT(EUCBTPOS(UCBCTR) + 1); /* UCB ADDRESS FROM IOS      01193000
*                                         LOOK-UP-TABLE      @Y30LPPC*/ 01194000
         LR    @09,UCBLINDX                                     02S0299 01195000
         SLA   @09,2                                            02S0299 01196000
         L     @05,@PC00001                                     02S0299 01197000
         L     @05,UCBLPTR(,@05)                                02S0299 01198000
         LR    @15,UCBCTR                                       02S0299 01199000
         ALR   @15,@15                                          02S0299 01200000
         L     @01,GRPTR(,GRPTABP)                              02S0299 01201000
         L     @01,GRPUCBP(,@01)                                02S0299 01202000
         BCTR  @01,0                                            02S0299 01203000
         BCTR  @01,0                                            02S0299 01204000
         LH    @15,EUCBTPOS(@15,@01)                            02S0299 01205000
         ALR   @15,@15                                          02S0299 01206000
         L     @01,IOSLUTP                                      02S0299 01207000
         LH    @15,IOSLUT(@15,@01)                              02S0299 01208000
         N     @15,@CF01280                                     02S0299 01209000
         ST    @15,UCBPUCBP-4(@09,@05)                          02S0299 01210000
*          UCBLINDX = UCBLINDX + 1;    /* NEXT SLOT IN LIST  @Y30LPPC*/ 01211000
         AL    UCBLINDX,@CF00040                                02S0300 01212000
*      END;                            /* END SCAN ALL UCBS  @Y30LPPC*/ 01213000
         AL    UCBCTR,@CF00040                                  02S0301 01214000
@DE00298 L     @09,GRPTR(,GRPTABP)                              02S0301 01215000
         C     UCBCTR,GRPUCBNO(,@09)                            02S0301 01216000
         BNH   @DL00298                                         02S0301 01217000
*    IF COUNTUCB = ON THEN             /* COUNTING REQUESTED @Y30LPPC*/ 01218000
@RF00297 TM    COUNTUCB,B'00010000'                             02S0302 01219000
         BNO   @RF00302                                         02S0302 01220000
*      NUMUCBS = NUMUCBS + GRPUCBNO;   /* ADD NUMBER OF UCBS IN 02S0303 01221000
*                                         THE GROUP          @Y30LPPC*/ 01222000
         L     @09,GRPTR(,GRPTABP)                              02S0303 01223000
         L     @06,NUMUCBS                                      02S0303 01224000
         AL    @06,GRPUCBNO(,@09)                               02S0303 01225000
         ST    @06,NUMUCBS                                      02S0303 01226000
*    GRPTABP = GRPTABP + LENGTH(GRPTRTAB); /* NEXR GROUP PTR @Y30LPPC*/ 01227000
@RF00302 AL    GRPTABP,@CF00058                                 02S0304 01228000
*  END;                                /* END SCAN ALL GROUPS@Y30LPPC*/ 01229000
         AL    GRPCTR,@CF00040                                  02S0305 01230000
@DE00296 C     GRPCTR,GENGRPNO(,GENPTR)                         02S0305 01231000
         BNH   @DL00296                                         02S0305 01232000
*  GENPTR = GENPTR + LENGTH(GENENTRY); /* NEXT GENERIC ENTRY @Y30LPPC*/ 01233000
         AL    GENPTR,@CF00125                                  02S0306 01234000
*END;                                  /* END SCAN ALL GENS  @Y30LPPC*/ 01235000
         AL    GENCTR,@CF00040                                  02S0307 01236000
@DE00294 C     GENCTR,LUVGENNO(,LUVPTR)                         02S0307 01237000
         BNH   @DL00294                                         02S0307 01238000
*END SCANEDT;                                              /*@Y30LPPC*/ 01239000
@EL00003 DS    0H                                               02S0308 01240000
