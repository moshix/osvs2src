*                     MSGPTR=ADDR(STARTED);/* SET MESSAGE BASE       */ 00466000
         L     @10,@CA00306                                     04S0167 00467000
         ST    @10,MSGPTR                                       04S0167 00468000
*                   END;                                        04S0168 00469000
*               END;                                            04S0169 00470000
*             ELSE                  /* MONITOR JOBNAMES NOT ISSUED   */ 00471000
*               IF BASESCT>ZERO THEN/* MONITOR SESSIONS ISSUED       */ 00472000
         B     @RC00157                                         04S0170 00473000
