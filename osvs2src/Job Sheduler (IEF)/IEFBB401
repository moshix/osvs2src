*                     WTOSMSGT=ON;  /* SET SESSIONS MSGTYP BIT       */ 00772000
         LR    @08,WTOLISTP                                     04S0243 00773000
         AH    @08,WTOTLNTH(,WTOLISTP)                          04S0243 00774000
         OI    WTOSMSGT(@08),B'00000100'                        04S0243 00775000
*                   RFY                                         04S0244 00776000
*                    (REG0,                                     04S0244 00777000
*                     REG1) RSTD;   /* RESERVE REGISTER              */ 00778000
@RF00242 DS    0H                                               04S0245 00779000
*                   REG1=ADDR(WTOLIST);/* SET PARM PTR FOR WTO MACRO */ 00780000
         LR    REG1,WTOLISTP                                    04S0245 00781000
*                   GEN(WTO MF=(E,(1)));/* ISSUE THE MESSAGE         */ 00782000
         WTO MF=(E,(1))                                                 00783000
*                   REG0=WVWTOLTH;  /* SET LENGTH FOR FREEMAIN       */ 00784000
         L     REG0,WVWTOLTH                                    04S0247 00785000
*                   REG0=REG0|SUBPL230;/* FREE FROM SUBPOOL 230.     */ 00786000
         O     REG0,@CF00299                                    04S0248 00787000
*                   REG1=ADDR(WTOLIST);/* SET ADDR. FOR FREEMAIN     */ 00788000
         LR    REG1,WTOLISTP                                    04S0249 00789000
*                   GEN(FREEMAIN R,LV=(0),A=(1));/* FREE CORE GOTTEN    00790000
*                                      FOR MSG                       */ 00791000
         FREEMAIN R,LV=(0),A=(1)                                        00792000
*                 END;                                          04S0251 00793000
*               ELSE                /* GETMAIN FAILED                */ 00794000
*                 DO;               /* SET BAD RETURN CODES          */ 00795000
         B     @RC00218                                         04S0252 00796000
@RF00218 DS    0H                                               04S0253 00797000
*                   WVBMRETN=BMFAIL;/* INDICATE BUILDMSG FAILED      */ 00798000
         MVC   WVBMRETN(2),@CH00107                             04S0253 00799000
*                   WVFAILCD=BMGETM;/* INDICATE GETMAIN ERROR        */ 00800000
         MVC   WVFAILCD(2),@CH00071                             04S0254 00801000
*                 END;                                          04S0255 00802000
*               IF WVBMRETN^=SUCCESS THEN/* MESSAGE NOT ISSUED       */ 00803000
@RC00218 ICM   @10,3,WVBMRETN                                   03S0256 00804000
         BZ    @RF00256                                         03S0256 00805000
*                 DO;               /* SET BAD RETURN CODE           */ 00806000
*                   WVJIRETN=JIFAIL;/* INDICATE JOBINIT FAILED       */ 00807000
         MVC   WVJIRETN(2),@CH00107                             03S0258 00808000
*                 END;                                          03S0259 00809000
*             END;                                              03S0260 00810000
*           ELSE                                                03S0261 00811000
*             ;                     /* NO MESSAGE TO ISSUE           */ 00812000
@RF00189 DS    0H                                               03S0262 00813000
*           IF WVJIRETN^=SUCCESS THEN/* JOB INIT FAILED              */ 00814000
@RC00189 ICM   @10,3,WVJIRETN                                   02S0262 00815000
         BZ    @RF00262                                         02S0262 00816000
*             WVPRRETN=JSPFAIL;     /* JOB STEP PREP FAILS           */ 00817000
         MVC   WVPRRETN(2),@CH00107                             02S0263 00818000
*         END;                                                  02S0264 00819000
@RF00262 DS    0H                                               02S0265 00820000
*                                                               03S0265 00821000
*/********************************************************************/ 00822000
*/*                                                                  */ 00823000
*/* SEGMENT NAME - STEPINIT                                          */ 00824000
*/*                                                                  */ 00825000
*/* DESCRIPTIVE NAME - STEP INITIALIZATION FUNCTIONS                 */ 00826000
*/*                                                                  */ 00827000
*/* FUNCTION -                                                       */ 00828000
*/*                                                                  */ 00829000
*/* IN GENERAL THIS SEGMENT PERFORMS INITIALIZATION FUNCTIONS THAT   */ 00830000
*/* MUST BE DONE FOR EACH STEP OF A JOB.                             */ 00831000
*/*                                                                  */ 00832000
*/* THESE FUNCTIONS ARE:                                             */ 00833000
*/*                                                                  */ 00834000
*/* . INITIALIZE LCTERROR BITS                                       */ 00835000
*/*   THE "SOME DATA SETS ALLOCATED" AND THE "STEP BYPASSED"         */ 00836000
*/*   BITS OF LCTERROR ARE INITIALIZED TO 0.                         */ 00837000
*/*                                                                  */ 00838000
*/* . SMF TIME STAMP                                                 */ 00839000
*/*   IF A TCT EXISTS FOR THIS STEP,                                 */ 00840000
*/*   A TIME MACRO IS ISSUED AND THE TIME PASSED BACK IS INSERTED    */ 00841000
*/*   IN THE TCTAST FIELD OF THE TCT.                                */ 00842000
*/*                                                                  */ 00843000
*/* . INITIALIZE ETIOT                                               */ 00844000
*/*   THE TIOT MANAGER IS INVOKED WITH THE FUNCTION MAP SET UP       */ 00845000
*/*   TO INITIALIZE AND CREATE TIOT AND DSAB QDB.                    */ 00846000
*/*                                                                  */ 00847000
*/* . INITIALIZE THE FOLLOWING FIELDS IN THE P/P JSCB:               */ 00848000
*/*     JSCTCTP - SVA OF THE JCT                                     */ 00849000
*/*     JSCSCTP - SVA OF THE SCT                                     */ 00850000
*/*     JSCALIMIT - CONCURRENT ALLOCATION LIMIT                      */ 00851000
*/*                                                                  */ 00852000
*/* ENTRY - IN-LINE SEGMENT                                          */ 00853000
*/*                                                                  */ 00854000
*/*   PURPOSE - SEE FUNCTION                                         */ 00855000
*/*                                                                  */ 00856000
*/*   INPUT -                                                        */ 00857000
*/*     LCTERROR                                                     */ 00858000
*/*     TCT (ADDRESSED FROM TCB)                                     */ 00859000
*/*     P/P JSCB                                                     */ 00860000
*/*     LCTTISZ SIZE TIOT (4K MULTIPLES) OR ZERO                     */ 00861000
*/*     JCT ADDRESS                                                  */ 00862000
*/*     SCT ADDRESS                                                  */ 00863000
*/*     INIT PARM LIST (SEE B401DATA)                                */ 00864000
*/* EXITS -                                                          */ 00865000
*/* 1 CONDITION - SUCCESSFUL                                         */ 00866000
*/*                                                                  */ 00867000
*/*   OUTPUT -                                                       */ 00868000
*/*     TIOT & DSAB QDB CREATED                                      */ 00869000
*/*     SMF TIME STAMP IN TCT                                        */ 00870000
*/*     LCTERROR BITS INITIALIZED                                    */ 00871000
*/*     TIOT LIST PTR IN INIT PARM LIST                              */ 00872000
*/*     SCT ADDRESS IN INIT PARM LIST                                */ 00873000
*/*   RETURN CODE - 0 IN WVSIRETN                                    */ 00874000
*/*                                                                  */ 00875000
*/* 2 CONDITION - GETMAIN FAILURE                                    */ 00876000
*/*                                                                  */ 00877000
*/*   OUTPUT -                                                       */ 00878000
*/*     SMF TIME STAMP IN TCT                                        */ 00879000
*/*     LCTERROR BITS INITIALIZED                                    */ 00880000
*/*     WVFAILCD SET TO 2                                            */ 00881000
*/*     SCT ADDRESS IN INIT PARM LIST                                */ 00882000
*/*                                                                  */ 00883000
*/*   RETURN CODE - 4                                                */ 00884000
*/*                                                                  */ 00885000
*/********************************************************************/ 00886000
*                                                               03S0265 00887000
*       WVSIRETN=SUCCESS;           /* INITIALIZE RETURN CODE        */ 00888000
@RF00149 SLR   WVSIRETN,WVSIRETN                                03S0265 00889000
*       LCTPALCD=OFF;               /* SET NO DATA SETS ALLOCD       */ 00890000
*       LCTSFAIL=OFF;               /* SET STEP NOT BYPASSED         */ 00891000
         L     @10,LCTPTR(,PARMP)                               03S0267 00892000
         NI    LCTPALCD(@10),B'11001111'                        03S0267 00893000
*       LTIOTLP=ZERO31;             /* INIT LOCAL TIOT LIST PTR      */ 00894000
         SLR   @10,@10                                          03S0268 00895000
         ST    @10,LTIOTLP                                      03S0268 00896000
*       IF TCBTCT^=ZERO31 THEN      /* TCT IS PRESENT                */ 00897000
         L     @08,CVTPTR                                       03S0269 00898000
         L     @08,CVTTCBP(,@08)                                03S0269 00899000
         L     @08,TCBPTR(,@08)                                 03S0269 00900000
         C     @10,TCBTCT(,@08)                                 03S0269 00901000
         BE    @RF00269                                         03S0269 00902000
*         DO;                       /* INSERT SMF TIME STAMP         */ 00903000
*           RFY                                                 03S0271 00904000
*            (REG0,                                             03S0271 00905000
*             REG1) RSTD;           /* RESERVE REGISTERS FOR MACRO   */ 00906000
*           GEN(TIME BIN);          /* GET THE TIME                  */ 00907000
         TIME BIN                                                       00908000
*           DO;                     /* MODESET KEYADDR(ZERO)         */ 00909000
*             SPKA(ZERO);           /* SET PSW KEY                   */ 00910000
         SLR   @10,@10                                          03S0274 00911000
         SPKA  0(@10)                                           03S0274 00912000
*           END;                    /* MODESET KEYADDR(ZERO) GET KEY    00913000
*                                      0 TO STORE INTO TCT           */ 00914000
*           TCTAST=REG0;            /* PUT THE TIME IN THE TCT       */ 00915000
