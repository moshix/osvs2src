         L     @10,TCBPTR(,@10)                                 03S0276 00918000
         L     @10,TCBTCT(,@10)                                 03S0276 00919000
         ST    REG0,TCTAST(,@10)                                03S0276 00920000
*           DO;                     /* MODESET EXTKEY(SCHED)         */ 00921000
*             SPKA(16);             /* SET PSW KEY                   */ 00922000
         LA    @10,16                                           03S0278 00923000
         SPKA  0(@10)                                           03S0278 00924000
*           END;                    /* MODESET EXTKEY(SCHED) BACK TO    00925000
*                                      SCHEDULER KEY                 */ 00926000
*           RFY                                                 03S0280 00927000
*            (REG0,                                             03S0280 00928000
*             REG1) UNRSTD;         /* RELEASE REGISTERS             */ 00929000
*         END;                                                  03S0281 00930000
*       IF WVPRRETN=SUCCESS THEN    /* JOB HAS NOT BEEN FAILED       */ 00931000
@RF00269 ICM   @10,3,WVPRRETN                                   03S0282 00932000
         BNZ   @RF00282                                         03S0282 00933000
*         DO;                       /* CONTINUE TO PROCESS           */ 00934000
*           RFY                                                 03S0284 00935000
*             IEZJSCB BASED(LCTJSCB);/* USE P/P JSCB                 */ 00936000
