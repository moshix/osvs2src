         BNE   @RF00171                                         04S0171 00483000
*                   DO;             /* SELECT LOGGED ON MESSAGE      */ 00484000
*                     MSGSW=ON;     /* INDICATE MESSAGE REQUIRED     */ 00485000
*                     SESSSW=ON;    /* INDICATE SESSIONS MESSAGE     */ 00486000
         OI    MSGSW,B'01010000'                                04S0174 00487000
*                     MSGPTR=ADDR(LOGGEDON);/* SET MESSAGE BASE      */ 00488000
         L     @10,@CA00304                                     04S0175 00489000
         ST    @10,MSGPTR                                       04S0175 00490000
*                   END;                                        04S0176 00491000
