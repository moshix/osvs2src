*   SPKA (16); /*SET PSW KEY*/                                  02S0133 00163000
         LA    @05,16                                           02S0133 00164000
         SPKA  0(@05)                                           02S0133 00165000
* END; /*MODESET  EXTKEY(SCHED)*/               /* BACK TO SCHED KEY  @ 00166000
*A00061*/                                                       02S0134 00167000
*FEPBLKP = ZERO;                       /* INIT EPA BLK PTR   @ZM41392*/ 00168000
         ST    @01,FEPBLKP                                      02S0135 00169000
*SWARDRET = SWARDRET && SWARDRET;      /* ZERO OUT SWA RETURN INFO   */ 00170000
