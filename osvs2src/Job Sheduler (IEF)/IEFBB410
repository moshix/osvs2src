*/********************************************************************/ 02618000
*/*                                                                  */ 02619000
*/* SEGMENT NAME - ENDSMSG                                           */ 02620000
*/*                                                                  */ 02621000
*/* DESCRIPTIVE NAME - ISSUE END OF STEP MESSAGES                    */ 02622000
*/*                                                                  */ 02623000
*/* FUNCTION -                                                       */ 02624000
*/*   THE FUNCTION OF THIS SEGMENT IS TO ISSUE END OF STEP STATUS    */ 02625000
*/*   MESSAGES TO THE OPERATOR AND THE PROGRAMMER.                   */ 02626000
*/*                                                                  */ 02627000
*/*   OPERATION -                                                    */ 02628000
*/*   1. COMPLETE THE MESSAGE -                                      */ 02629000
*/*      THE JOB NAME, STEP NAME, AND PROC STEP NAME ARE PUT IN      */ 02630000
*/*      THE MESSAGES. IN ADDITION IF THE MESSAGE IS TO BE SENT TO   */ 02631000
*/*      THE OPERATOR AND TIME IS REQUESTED, THE TIME IS INSERTED    */ 02632000
*/*      INTO THE MESSAGE.                                           */ 02633000
*/*                                                                  */ 02634000
*/*   2. ISSUE THE MESSAGE(S) -                                      */ 02635000
*/*      THE MESSAGE IS ISSUED TO THE PROGRAMMER. IN ADDITION        */ 02636000
*/*      A MESSAGE IS ISSUED TO THE OPERATOR IF REQUESTED.           */ 02637000
*/*                                                                  */ 02638000
*/* ENTRY - IN-LINE SEGMENT                                          */ 02639000
*/*                                                                  */ 02640000
*/*   INPUT -                                                        */ 02641000
*/*                                                                  */ 02642000
*/*     WPL - WTO PARAMETER LIST                                     */ 02643000
*/*       WPLTXT - TEXT OF MESSAGE                                   */ 02644000
*/*                                                                  */ 02645000
*/*     WTOSW - ISSUE WTO IF ON                                      */ 02646000
*/*                                                                  */ 02647000
*/*     TIMESW - PUT TIME IN THE MESSAGE                             */ 02648000
*/*                                                                  */ 02649000
*/*     TIME - STRUCTURE CONTAINING:                                 */ 02650000
*/*       HOURS - HOUR OF THE DAY                                    */ 02651000
*/*       MINUTES - MINUTE OF THE HOUR                               */ 02652000
*/*       SECONDS - SECOND OF THE MINUTE                             */ 02653000
*/*                                                                  */ 02654000
*/*     WMSGLNTH - LENGTH OF WTO MESSAGE TEXT                        */ 02655000
*/*                                                                  */ 02656000
*/*     LMSGLNTH - LENGTH OF PROGRAMMER MESSAGE TEXT                 */ 02657000
*/*                                                                  */ 02658000
*/*     JCT -                                                        */ 02659000
*/*       JCTJNAME - JOB NAME                                        */ 02660000
*/*                                                                  */ 02661000
*/*     SCT -                                                        */ 02662000
*/*       SCTSNAME - STEP NAME                                       */ 02663000
*/*       SCTSCLPC - PROC STEP NAME                                  */ 02664000
*/*                                                                  */ 02665000
*/* EXIT -                                                           */ 02666000
*/*   CONDITION - NORMAL                                             */ 02667000
*/*                                                                  */ 02668000
*/*   OUTPUT -                                                       */ 02669000
*/*     MESSAGE(S) ISSUED                                            */ 02670000
*/*     LCT                                                          */ 02671000
*/*       LCTERRM - SET ON IF JOB ABENDED MSG ISSUED                 */ 02672000
*/*                                                                  */ 02673000
*/*     RETURN CODE - NONE                                           */ 02674000
*/*                                                                  */ 02675000
*/********************************************************************/ 02676000
*ENDSMSG: PROC;                        /* MAKE ENDSMSG A PROCEDURE      02677000
*                                         SINCE IT IS CALLED TWICE      02678000
*                                         FROM ISSUMSGS SEG  @ZA08480*/ 02679000
ENDSMSG  STM   @14,@12,@SA00002                                 02S0689 02680000
*IF WTOSW = ON THEN                    /* ISSUE A WTO                */ 02681000
         TM    WTOSW,B'10000000'                                02S0690 02682000
         BNO   @RF00690                                         02S0690 02683000
*  DO;                                                          02S0691 02684000
*    WTOJNAME = JCTJNAME;              /* PUT JOB NAME IN MESSAGE    */ 02685000
         LA    @03,WTOL                                         02S0692 02686000
         LA    @15,4                                            02S0692 02687000
         ALR   @15,@03                                          02S0692 02688000
         L     @14,PARMPTR                                      02S0692 02689000
         L     @01,JCTPTR(,@14)                                 02S0692 02690000
         MVC   WTOJNAME(8,@15),JCTJNAME(@01)                    02S0692 02691000
*    WTOSNAME = SCTSNAME;              /* PUT STEP NAME IN MESSAGE   */ 02692000
         L     @14,SCTPTR(,@14)                                 02S0693 02693000
         MVC   WTOSNAME(8,@15),SCTSNAME(@14)                    02S0693 02694000
*    WTOPSNAM = SCTSCLPC;              /* PUT PROC STEP IN MESSAGE   */ 02695000
         MVC   WTOPSNAM(8,@15),SCTSCLPC(@14)                    02S0694 02696000
*    TIMEPTR = ADDR(WPLTXT)+WTOTDISP;  /* SET TIME POINTER           */ 02697000
*                                                               02S0695 02698000
         LA    TIMEPTR,WPLTXT(,@03)                             02S0695 02699000
         L     @03,@CA00332                                     02S0695 02700000
         AH    TIMEPTR,WTOTDISP-WTOABEND(,@03)                  02S0695 02701000
*    IF TIMESW = ON THEN               /* TIME REQUESTED             */ 02702000
         TM    TIMESW,B'00001000'                               02S0696 02703000
         BNO   @RF00696                                         02S0696 02704000
*      DO;                             /* PUT TIME IN MESSAGE        */ 02705000
*        WTOHH = HOURS;                /* HOURS IN MESSAGE           */ 02706000
         MVC   WTOHH(2,TIMEPTR),HOURS                           02S0698 02707000
*        WTOMM = MINUTES;              /* MINUTES IN MESSAGE         */ 02708000
         MVC   WTOMM(2,TIMEPTR),MINUTES                         02S0699 02709000
*        WTOSS = SECONDS;              /* SECONDS IN MESSAGE         */ 02710000
         MVC   WTOSS(2,TIMEPTR),SECONDS                         02S0700 02711000
*      END;                                                     02S0701 02712000
*    ELSE                              /* TIME NOT WANTED            */ 02713000
*      WTOTIME = '';                   /* BLANK OUT TIME             */ 02714000
         B     @RC00696                                         02S0702 02715000
@RF00696 MVI   WTOTIME(TIMEPTR),C' '                            02S0702 02716000
         MVC   WTOTIME+1(14,TIMEPTR),WTOTIME(TIMEPTR)           02S0702 02717000
*    CALL IEFAB4F3(ADDR(WPLTXT),WMSGLNTH,CMSGLNTH); /*COMPRESS BLANKS*/ 02718000
@RC00696 LA    @15,WTOL                                         02S0703 02719000
         LA    @15,WPLTXT(,@15)                                 02S0703 02720000
         ST    @15,@TF00001                                     02S0703 02721000
