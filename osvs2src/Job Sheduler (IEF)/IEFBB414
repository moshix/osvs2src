*     GTST253  FIXED(31) CONSTANT(253);                         02S0085 00278000
*   RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/               02S0086 00279000
*   GTSTR0 = RBSIZE; /* SET AMOUNT*/                            02S0087 00280000
         LR    GTSTR0,@08                                       02S0087 00281000
*   GTSTR2 = SPID; /* SET SUBPOOL*/                             02S0088 00282000
         LA    GTSTR2,230                                       02S0088 00283000
*   GENERATE CODE;                                              02S0089 00284000
     GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN                               00285000
*   GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/               02S0090 00286000
         ST    GTSTR15,GTSTRTCD                                 02S0090 00287000
*   FRBPTR = GTSTR1; /* ADDR OF GOTN STORG*/                    02S0091 00288000
         ST    GTSTR1,FRBPTR                                    02S0091 00289000
*   RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */                02S0092 00290000
*   IF GTSTRTCD = 0 THEN /*GETMAIN SUCCESSFUL? */               02S0093 00291000
         L     @08,GTSTRTCD                                     02S0093 00292000
         LTR   @08,@08                                          02S0093 00293000
         BNZ   @RF00093                                         02S0093 00294000
*     DO;               /* YES, CLEAR STORAGE.*/                02S0094 00295000
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0095 00296000
*                          GTSTR7,GTSTR8) RSTD;                 02S0095 00297000
*       GTSTR4 = FRBPTR; /*STORG PTR*/                          02S0096 00298000
         L     @03,FRBPTR                                       02S0096 00299000
         LR    GTSTR4,@03                                       02S0096 00300000
*       GTSTR5 = RBSIZE; /*STORG AMNT*/                         02S0097 00301000
         L     GTSTR5,RBSIZE                                    02S0097 00302000
*       GTSTR7 = '00000000'X; /*PAD CHAR & LNTH*/               02S0098 00303000
         SLR   GTSTR7,GTSTR7                                    02S0098 00304000
*       IF SPID = GTST253 THEN                                  02S0099 00305000
         B     @RF00099                                         02S0099 00306000
*      GEN                                                      02S0100 00307000
*   (MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8);           02S0100 00308000
         MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8                 00309000
*       GEN REFS(GTSTR4,GTSTR6);                                02S0101 00310000
@RF00099 DS    0H                                               02S0101 00311000
          MVCL  GTSTR4,GTSTR6 CLEAR GOTTEN STORG                        00312000
*       IF SPID = GTST253 THEN                                  02S0102 00313000
         B     @RF00102                                         02S0102 00314000
*         GEN (MODESET KEYADDR=(2));                            02S0103 00315000
         MODESET KEYADDR=(2)                                            00316000
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0104 00317000
*                          GTSTR7,GTSTR8) UNRSTD;               02S0104 00318000
@RF00102 DS    0H                                               02S0105 00319000
*     END;       /* STORAGE CLEARING COMPLETED */               02S0105 00320000
*   ELSE                                                        02S0106 00321000
*   FRBPTR= 0;   /* CLEAR RETURN ADDR. */                       02S0106 00322000
         B     @RC00093                                         02S0106 00323000
@RF00093 SLR   @08,@08                                          02S0106 00324000
         ST    @08,FRBPTR                                       02S0106 00325000
*   GTSTR15 = GTSTRTCD;    /* GTMN RETURN CODE */               02S0107 00326000
@RC00093 L     GTSTR15,GTSTRTCD                                 02S0107 00327000
*                                      /* RESTORE LIST PRINT STATUS  */ 00328000
*       /*GET & CLEAR CORE FOR RBS    */                        02S0108 00329000
*RETCODE = REG15;                      /*SAVE RETURN CODE            */ 00330000
         LR    RETCODE,REG15                                    02S0108 00331000
*IF RETCODE = ZERO THEN                /*CORE OBTAINED?              */ 00332000
         LTR   RETCODE,RETCODE                                  02S0109 00333000
         BNZ   @RF00109                                         02S0109 00334000
*  DO;                                 /*YES,SET UP RB CHAIN         */ 00335000
*    CMRBPTR = FRBPTR;                 /*SET BASE PTR TO FIRST RB    */ 00336000
         L     CMRBPTR,FRBPTR                                   02S0111 00337000
*      DO RBCTR = 1 TO NUMSIOTS;       /*BUILD RB CHAIN              */ 00338000
         LA    RBCTR,1                                          02S0112 00339000
         B     @DE00112                                         02S0112 00340000
@DL00112 DS    0H                                               02S0113 00341000
*        IF RBCTR ^= NUMSIOTS THEN     /*NOT LAST BLOCK              */ 00342000
         C     RBCTR,NUMSIOTS                                   02S0113 00343000
         BE    @RF00113                                         02S0113 00344000
*          CMNRBPTR = CMRBPTR + LENGTH(CMUNRB); /*SET PTR TO NEXT RB */ 00345000
         LA    @08,16                                           02S0114 00346000
         ALR   @08,CMRBPTR                                      02S0114 00347000
         ST    @08,CMNRBPTR(,CMRBPTR)                           02S0114 00348000
*        ELSE                          /*LAST BLOCK                  */ 00349000
*          CMNRBPTR = ZERO;            /*ZERO CHAIN FIELD            */ 00350000
         B     @RC00113                                         02S0115 00351000
@RF00113 SLR   @08,@08                                          02S0115 00352000
         ST    @08,CMNRBPTR(,CMRBPTR)                           02S0115 00353000
*        CMSIOPTR = SIOTPTR;           /*SET PTR TO SIOT             */ 00354000
@RC00113 ST    SIOTPTR,CMSIOPTR(,CMRBPTR)                       02S0116 00355000
*        SIOTDSPD = OFF;               /*TURN OFF DISP PROCESSED        00356000
*                                                            @YM06109*/ 00357000
         NI    SIOTDSPD(SIOTPTR),B'10111111'                    02S0117 00358000
*        IF SCTDUMMY = YES             /*'DUMMY REQUEST' ?           */ 00359000
*           | SIODUNAL = YES           /*OR ALREADY UNALLOCATED      */ 00360000
*           | (SIOTSSDS = YES          /*OR SUBSYSTEM DS     @YM02473*/ 00361000
