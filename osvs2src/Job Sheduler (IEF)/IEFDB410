         L     @10,SJFCBPTR(,@10)                               06S0312 01603000
         TM    JFCDISP(@10),B'10000000'                         06S0312 01604000
         BNO   @GL00137                                         06S0312 01605000
         TM    JFCDISP(@10),B'01000000'                         06S0312 01606000
         BZ    @RT00312                                         06S0312 01607000
@GL00137 L     @10,DSABSIOT(,DSABPTR)                           06S0312 01608000
         L     @10,SJFCBPTR(,@10)                               06S0312 01609000
         TM    JFCMODNW(@10),B'00010000'                        06S0312 01610000
         BNO   @RF00312                                         06S0312 01611000
@RT00312 DS    0H                                               06S0313 01612000
*            ELIGBLSW=OFF;             /*YES, DSAB NOT ELIBIBLE      */ 01613000
         NI    ELIGBLSW,B'01111111'                             06S0313 01614000
*     END;                                                      06S0314 01615000
@RF00312 DS    0H                                               06S0314 01616000
@RC00308 DS    0H                                               06S0315 01617000
*  END;                                                         06S0315 01618000
@RC00295 DS    0H                                               06S0316 01619000
*  END;                                                         05S0316 01620000
@RF00293 DS    0H                                               05S0317 01621000
*    IF ELIGBLSW=ON THEN               /*DSAB ELIGIBLE TO SATISFY REQ*/ 01622000
@RF00287 TM    ELIGBLSW,B'10000000'                             04S0317 01623000
         BNO   @RF00317                                         04S0317 01624000
*      DO;                                                      04S0318 01625000
*        DSNCNP=ADDR(DSAB);            /*SAVE ADDRESS OF DSAB TO CNVT*/ 01626000
         ST    DSABPTR,DSNCNP                                   04S0319 01627000
*        IF SAMDSASW=ON THEN           /*DSAB HAS BOTH DDN + DSN REQ */ 01628000
         TM    SAMDSASW,B'00010000'                             04S0320 01629000
         BNO   @RF00320                                         04S0320 01630000
*          ENDDNSSW=ON;                /*DON'T SEARCH FOR DDN REQ    */ 01631000
         OI    ENDDNSSW,B'00100000'                             04S0321 01632000
*      END;                                                     04S0322 01633000
*    ELSE                              /*CONVT REJECTED BUT VOL/UNIT    01634000
*                                        INFORMATION AVAILABLE       */ 01635000
*      IF VLSERTUP=0                   /*NO VOL/UNIT INFO SPECIFIED     01636000
*                                        IN REQUEST (NOTE THAT OTHER    01637000
*                                        VOL/UNIT INFO CAUSED   04S0323 01638000
*                                        CKDSCFSW TO BE SET ON)      */ 01639000
*        & COPYDSAB=0                  /*NEWER REFERENCE NOT SAVED   */ 01640000
*        & STATSTUP -> TEXTUPAR(ONE)^=NEW /*NOT NEW REQUEST  @YM08653*/ 01641000
*        & (STATSTUP->TEXTUPAR(ONE)^=MOD | /*NOT MOD REQ OR  @YM08653*/ 01642000
*           DSABPALC = ON                | /*PERM ALLOC'D OR @YM08653*/ 01643000
*           SIOTDLET = OFF               | /*NON-DELETE OR   @YM08653*/ 01644000
*           (DSABNUSE=ON | DSABOPCT^=0)) /*NOT CONVERTED BECAUSE        01645000
*                                        DSAB IS IN-USE      @YM08653*/ 01646000
*           & JFCORGAM = OFF THEN      /*NOT A VSAM DATASET  @YA00068*/ 01647000
         B     @RC00317                                         04S0323 01648000
@RF00317 SLR   @10,@10                                          04S0323 01649000
         C     @10,VLSERTUP(,KEYTABP)                           04S0323 01650000
         BNE   @RF00323                                         04S0323 01651000
         C     @10,COPYDSAB                                     04S0323 01652000
         BNE   @RF00323                                         04S0323 01653000
         L     @08,STATSTUP(,KEYTABP)                           04S0323 01654000
         CLI   TEXTUPAR(@08),X'04'                              04S0323 01655000
         BE    @RF00323                                         04S0323 01656000
         CLI   TEXTUPAR(@08),X'02'                              04S0323 01657000
         BNE   @GL00138                                         04S0323 01658000
         TM    DSABPALC(DSABPTR),B'01000000'                    04S0323 01659000
         BO    @GL00138                                         04S0323 01660000
         L     @08,DSABSIOT(,DSABPTR)                           04S0323 01661000
         TM    SIOTDLET(@08),B'00000100'                        04S0323 01662000
         BZ    @GL00138                                         04S0323 01663000
         TM    DSABNUSE(DSABPTR),B'00000001'                    04S0323 01664000
         BO    @GL00138                                         04S0323 01665000
         CH    @10,DSABOPCT(,DSABPTR)                           04S0323 01666000
         BE    @RF00323                                         04S0323 01667000
@GL00138 L     @10,DSABSIOT(,DSABPTR)                           04S0323 01668000
         L     @10,SJFCBPTR(,@10)                               04S0323 01669000
         TM    JFCORGAM(@10),B'00001000'                        04S0323 01670000
         BNZ   @RF00323                                         04S0323 01671000
*        DO;                                                    04S0324 01672000
*          COPYDSAB = ADDR(DSAB);      /* SAVE ADDR OF DSAB IN CASE     01673000
*                                         NEED TO DO A COPY LATER    */ 01674000
         ST    DSABPTR,COPYDSAB                                 04S0325 01675000
*          COPYSW = ON;                /*TELL NORMALOC ABOUT IT      */ 01676000
         L     @10,@PC00001+12                                  04S0326 01677000
