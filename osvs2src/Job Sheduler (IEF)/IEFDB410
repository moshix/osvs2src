         ST    @08,@AL00001                                     03S0413 02103000
         LA    @10,DEWRITP(,@10)                                03S0413 02104000
         ST    @10,@AL00001+4                                   03S0413 02105000
         L     @15,@CV00156                                     03S0413 02106000
         LA    @01,@AL00001                                     03S0413 02107000
         BALR  @14,@15                                          03S0413 02108000
*                                      /*CALL SWA MGR I/F            */ 02109000
*                  EPALSTP = ADDR(DEDMYEPA);/*RE-INIT PTR TO DUMMY EPA* 02110000
         L     @10,@PC00001+20                                  03S0414 02111000
         L     @08,@PC00001+16                                  03S0414 02112000
         LA    @08,DEDMYEPA(,@08)                               03S0414 02113000
         ST    @08,EPALSTP(,@10)                                03S0414 02114000
*                                      /*NOTE: IEFAB4F7 ZEROED DEWRITP* 02115000
*                END;                                           03S0415 02116000
*                                                               03S0415 02117000
*              IF DMYRSCD=0 THEN       /* UNALLOC SUCCESSFUL         */ 02118000
@RF00407 L     @10,DMYRSCD                                      03S0416 02119000
         LTR   @10,@10                                          03S0416 02120000
         BNZ   @RF00416                                         03S0416 02121000
*                LIMCOUNT = LIMCOUNT-1;     /* DECREMENT NOTINUSE       02122000
*                                              COUNT BY NUMBER OF DSABS 02123000
*                                              UNALLOCATED           */ 02124000
         LH    @10,LIMCOUNT                                     03S0417 02125000
         BCTR  @10,0                                            03S0417 02126000
         STH   @10,LIMCOUNT                                     03S0417 02127000
*              ELSE;                   /* UNALLOCATE FAILED          */ 02128000
@RF00416 DS    0H                                               03S0419 02129000
*            END;                      /* END UNALLOCATE PROCESSING  */ 02130000
*          ELSE;                       /* NOT ELIG FOR UNALLOCATE    */ 02131000
@RF00401 DS    0H                                               03S0421 02132000
*        END;                          /* END NOTINUSE PROCESSING    */ 02133000
*      ELSE;                           /* DSAB WAS IN USE OR OPEN    */ 02134000
@RF00366 DS    0H                                               03S0423 02135000
*      DSABPTR = NXTPTR;               /* POINT TO NEXT DSAB TO SRCH */ 02136000
         L     DSABPTR,NXTPTR                                   03S0423 02137000
*    END;                              /* ENDWHILE FOR LIMIT PROCESS */ 02138000
@DE00363 L     @10,@PC00001+8                                   03S0424 02139000
         L     @10,SCTPTR(,@10)                                 03S0424 02140000
         LH    @10,SCTNIUSL(,@10)                               03S0424 02141000
         N     @10,@CF02722                                     03S0424 02142000
         CH    @10,LIMCOUNT                                     03S0424 02143000
         BNL   @DC00363                                         03S0424 02144000
         LTR   DSABPTR,DSABPTR                                  03S0424 02145000
         BNZ   @DL00363                                         03S0424 02146000
@DC00363 DS    0H                                               03S0425 02147000
*    IF LIMCOUNT>SCTNIUSL THEN         /* IF CONTROL STILL EXCEEDED  */ 02148000
         L     @10,@PC00001+8                                   03S0425 02149000
         L     @10,SCTPTR(,@10)                                 03S0425 02150000
         LH    @10,SCTNIUSL(,@10)                               03S0425 02151000
         N     @10,@CF02722                                     03S0425 02152000
         CH    @10,LIMCOUNT                                     03S0425 02153000
         BNL   @RF00425                                         03S0425 02154000
*      ERRCODE = RCNTUNLC;             /* INHIBIT NORM ALLOCATION    */ 02155000
         L     @10,@PC00001+4                                   03S0426 02156000
         MVC   ERRCODE(2,@10),@CB02414                          03S0426 02157000
*    ELSE;                             /* OTHERWISE, CAN ALLOCATE    */ 02158000
@RF00425 DS    0H                                               03S0428 02159000
*  END;                                                         03S0428 02160000
*ELSE;                                 /* CONTROL NOT EXCEEDED=NO-OP */ 02161000
@RF00354 DS    0H                                               03S0430 02162000
*  END;                                                         02S0430 02163000
*                                                               02S0430 02164000
*IF (DDNCNP^=0                         /* DDN TO BE FREED FOR CONVERT   02165000
*                                         OR NORMAL ALLOC PROCESSING */ 02166000
*    | (DSNCNP^=0 & DDNAMTUP^=0 & DSNCNP->DSABTIOT->TIOEDDNM^=DDNREQ))  02167000
*                                      /* HAVE DSAB TO CONVERT, 02S0431 02168000
*                                         DDNAME WAS REQUESTED AND IS   02169000
*                                         DIFFERENT THAN THE CURRENT    02170000
*                                         DDNAME                     */ 02171000
*                                                               02S0431 02172000
*  & ERRCODE=0 THEN                    /* NO ERRORS YET              */ 02173000
@RF00352 SLR   @10,@10                                          02S0431 02174000
         C     @10,DDNCNP                                       02S0431 02175000
