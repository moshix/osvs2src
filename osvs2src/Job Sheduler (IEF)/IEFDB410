*              TEXTUKEY = DUNDDNAM;    /* BUILD TEXT UNIT            */ 02054000
         LA    @08,LIMTUP                                       03S0403 02055000
         L     @05,TXTPLENT(,@08)                               03S0403 02056000
         MVC   TEXTUKEY(2,@05),@CB01830                         03S0403 02057000
*              TEXTUPAR(1:8) = TIOEDDNM; /* DDNAME TO UNALLOCATED    */ 02058000
         L     @02,DSABTIOT(,DSABPTR)                           03S0404 02059000
         MVC   TEXTUPAR(8,@05),TIOEDDNM(@02)                    03S0404 02060000
*              DMYRSCD = 0;            /* INIT DUMMY REASON CODE     */ 02061000
         ST    @10,DMYRSCD                                      03S0405 02062000
*              CALL IEFDB4A0 (ADDR(TXTPLENT),DMYRSCD,           03S0406 02063000
*                            SYSCTBLK,ALFNCMAP,DEPARMS,EPALSTP, 03S0406 02064000
*                            SMFPARMS); /*                   @ZA02706*/ 02065000
         ST    @08,@TF00001                                     03S0406 02066000
         LA    @10,@TF00001                                     03S0406 02067000
         ST    @10,@AL00001                                     03S0406 02068000
         LA    @10,DMYRSCD                                      03S0406 02069000
         ST    @10,@AL00001+4                                   03S0406 02070000
         L     @10,@PC00001+8                                   03S0406 02071000
         ST    @10,@AL00001+8                                   03S0406 02072000
         L     @10,@PC00001+12                                  03S0406 02073000
         ST    @10,@AL00001+12                                  03S0406 02074000
         L     @10,@PC00001+16                                  03S0406 02075000
         ST    @10,@AL00001+16                                  03S0406 02076000
         L     @10,@PC00001+20                                  03S0406 02077000
         ST    @10,@AL00001+20                                  03S0406 02078000
         L     @10,@PC00001+24                                  03S0406 02079000
         ST    @10,@AL00001+24                                  03S0406 02080000
         L     @15,@CV00155                                     03S0406 02081000
         LA    @01,@AL00001                                     03S0406 02082000
         BALR  @14,@15                                          03S0406 02083000
*              IF DEWRITP^=0 THEN      /*SWA BLOCKS UPDATED?         */ 02084000
         L     @10,@PC00001+16                                  03S0407 02085000
         L     @08,DEWRITP(,@10)                                03S0407 02086000
         LTR   @08,@08                                          03S0407 02087000
         BZ    @RF00407                                         03S0407 02088000
*                DO;                   /*WRITE THEM BACK TO SWA      */ 02089000
*                  SWACTLS = SWACTLS && SWACTLS; /*INIT FUNCTION MAP */ 02090000
         LH    @08,SWACTLS                                      03S0409 02091000
         N     @08,@CF02722                                     03S0409 02092000
         LR    @05,@08                                          03S0409 02093000
         XR    @05,@08                                          03S0409 02094000
         STH   @05,SWACTLS                                      03S0409 02095000
*                  SWWRT = ON;         /*WRITE FUNCTION              */ 02096000
*                  SWFREE = ON;        /*FREE EPA'S WHEN DONE        */ 02097000
*                  SWOLD = ON;         /*EPA'S ALREADY EXIST         */ 02098000
         OI    SWWRT,B'00010101'                                03S0412 02099000
*                  CALL IEFAB4F7 (SWACTLS,DEWRITP,0,0,0,0,0);   03S0413 02100000
         MVC   @AL00001+8(20),@AL00413                          03S0413 02101000
