         BE    @RT00140                                         02S0140 00147000
@GL00001 L     @10,CDISPTUP(,KEYTABP)                           02S0140 00148000
         LTR   @10,@10                                          02S0140 00149000
         BZ    @RF00140                                         02S0140 00150000
         CLI   TEXTUPAR(@10),X'04'                              02S0140 00151000
         BNE   @RF00140                                         02S0140 00152000
@RT00140 DS    0H                                               02S0141 00153000
*    STATSTUP = ADDR(DFLTOLD);         /* CHANGE STATUS TO OLD  02S0141 00154000
*                                         TO INSURE EXCLUSIVE CONTROL*/ 00155000
*                                                               02S0141 00156000
         LA    @10,DFLTOLD                                      02S0141 00157000
         ST    @10,STATSTUP(,KEYTABP)                           02S0141 00158000
*RFY TEXTUNIT BASED(DSNAMTUP);                                  02S0142 00159000
@RF00140 DS    0H                                               02S0142 00160000
@RF00139 DS    0H                                               02S0143 00161000
*IF DSNAMTUP^=0                        /* DSNAME GIVEN               */ 00162000
*  & TEXTUPAR(1:2)='&&' THEN           /* AND ITS AN &&-NAME         */ 00163000
         L     @10,DSNAMTUP(,KEYTABP)                           02S0143 00164000
         LTR   @10,@10                                          02S0143 00165000
         BZ    @RF00143                                         02S0143 00166000
         CLC   TEXTUPAR(2,@10),@CC02662                         02S0143 00167000
         BNE   @RF00143                                         02S0143 00168000
*  DO;                                 /* BUILD TU FOR &-NAME        */ 00169000
*    AMPDSNTU(1:4) = TEXTUNIT(1:4);    /* COPY KEY AND NUMBER        */ 00170000
         MVC   AMPDSNTU(4),TEXTUNIT(@10)                        02S0145 00171000
*    AMPDSNTU(5:6) = TEXTULNG-1;       /* LENGTH MINUS ONE AMPERSAND */ 00172000
         ICM   @08,12,TEXTULNG(@10)                             02S0146 00173000
         SRA   @08,16                                           02S0146 00174000
         LR    @05,@08                                          02S0146 00175000
         BCTR  @05,0                                            02S0146 00176000
         STCM  @05,3,AMPDSNTU+4                                 02S0146 00177000
*    AMPDSNTU(7:TEXTULNG+5) = TEXTUPAR(2:TEXTULNG); /* MOVE &-DSNAME */ 00178000
         BCTR  @08,0                                            02S0147 00179000
         BCTR  @08,0                                            02S0147 00180000
         EX    @08,@SM02700                                     02S0147 00181000
*    DSNAMTUP = ADDR(AMPDSNTU);        /* NOW POINT TO &-NAME TU.    */ 00182000
         LA    @10,AMPDSNTU                                     02S0148 00183000
         ST    @10,DSNAMTUP(,KEYTABP)                           02S0148 00184000
*  END;                                                         02S0149 00185000
*    DSNCNP = 0;                       /*WILL BE SET TO ADDR OF DSAB    00186000
*                                        TO CONVERT, IF FOUND        */ 00187000
@RF00143 SLR   @10,@10                                          01S0150 00188000
         ST    @10,DSNCNP                                       01S0150 00189000
*/********************************************************************/ 00190000
*/*                                                                  */ 00191000
*/* SEGMENT NAME - CKCONVRT                                          */ 00192000
*/*                                                                  */ 00193000
*/* DESCRIPTIVE NAME - CHECK IF CAN CONVERT AN EXISTING ALLOCATION   */ 00194000
*/*                                                                  */ 00195000
*/* FUNCTION - CKCONVRT DETERMINES IF AN EXISTING ALLOCATION CAN BE  */ 00196000
*/*            USED TO SATISFY THE REQUEST OR IF IT'S NECESSARY      */ 00197000
*/*            (AND POSSIBLE, I.E., MORE RESOURCES CAN BE ALLOCATED  */ 00198000
*/*            TO THE USER) TO GO TO NORMAL ALLOCATION.              */ 00199000
*/*                                                                  */ 00200000
*/*   OPERATION -                                                    */ 00201000
*/*     .'CHECK ENVIRONMENT'. THIS SEGMENT WILL DETERMINE IF         */ 00202000
*/*      AN EXISTING ALLOCATION CAN BE CONVERTED TO SATISFY          */ 00203000
*/*      THE REQUEST. THE MAIN OUTPUT OF THIS SEGMENT IS             */ 00204000
*/*      POINTERS TO VARIOUS DSABS -                                 */ 00205000
*/*         . POINTER TO DSAB TO CONVERT IF ONE IS FOUND.            */ 00206000
*/*         . POINTER TO THE DSAB OF THE ALLOCATION FROM WHICH A     */ 00207000
*/*           DDNAME MUST BE DISASSOCIATED, IF NECESSARY.            */ 00208000
*/*         . POINTER TO THE DSAB OF AN ALLOCATION WHICH, ALTHOUGH   */ 00209000
*/*           NOT ELIGIBLE FOR CONVERSION, CONTAINED                 */ 00210000
*/*           VOL/UNIT INFORMATION WHICH MAY BE USED BY NORMAL       */ 00211000
*/*           ALLOCATION (IEFDB413) PROCESSING.                      */ 00212000
*/*                                                                  */ 00213000
*/*     .'LIMIT PROCESSING'. IF NO EXISTING ALLOCATION WAS ELIGIBLE  */ 00214000
*/*      FOR CONVERSION, THEN THIS SEGMENT WILL ATTEMPT TO           */ 00215000
*/*      UNALLOCATE NOT-IN-USE ALLOCATIONS, IF NECESSARY, TO         */ 00216000
*/*      BRING THEIR NUMBER DOWN TO THE LIMIT DETERMINED BY THE      */ 00217000
*/*      SUM OF 'DYNAMNBR' PLUS THE TOTAL NO. OF DD CARDS            */ 00218000
*/*      SPECIFIED IN JCL, OTHERWISE CALLED THE 'CONTROL VALUE' OR   */ 00219000
*/*      THE 'NOT IN USE LIMIT'.                                     */ 00220000
*/*                                                                  */ 00221000
*/*     .'FREE DDNAME'. IF A DDNAME WAS SPECIFIED IN THE REQUEST,    */ 00222000
*/*      BUT WAS FOUND (BY 'CHECK ENVIRONMENT') TO BE ASSOCIATED     */ 00223000
*/*      WITH AN EXISTING ALLOCATION FROM WHICH IT CAN BE            */ 00224000
*/*      DISASSOCIATED, THEN THIS SEGMENT PROCESSES THE              */ 00225000
*/*      DISASSOCIATION.                                             */ 00226000
*/*      NOTE THAT THIS IS DONE ONLY IF 'LIMIT PROCESSING' WAS       */ 00227000
*/*      EITHER NOT DONE OR WAS SUCCESSFUL.                          */ 00228000
*/*                                                                  */ 00229000
*/*      NOTE THAT IT IS POSSIBLE FOR TWO ALLOCATIONS                */ 00230000
*/*      TO BE MODIFIED IN THE CONVERSION PROCESS.                   */ 00231000
*/*               1 - THE ALLOCATION WITH A MATCHING DSNAME WHICH    */ 00232000
*/*                   WILL BE USED TO SATISFY THE REQUEST.           */ 00233000
*/*               2 - THE ALLOCATION FROM WHICH A DDNAME IS TO BE    */ 00234000
*/*                   DISASSOCIATED.                                 */ 00235000
*/*                                                                  */ 00236000
*/********************************************************************/ 00237000
*DDNCNP = 0;                           /*INIT PTR TO DSAB CORRESPONDING 00238000
*                                        TO DDNAME TO BE 'FREED'     */ 00239000
         ST    @10,DDNCNP                                       02S0151 00240000
*/********************************************************************/ 00241000
*/*                                                                  */ 00242000
*/* SEGMENT NAME - CKENVRON                                          */ 00243000
*/*                                                                  */ 00244000
*/* DESCRIPTIVE NAME - CHECK CURRENT ALLOCATION ENVIRONMENT          */ 00245000
*/*                                                                  */ 00246000
*/* FUNCTION - COMPARE THE INPUT REQUEST TO THE CURRENT              */ 00247000
*/*            ALLOCATION ENVIRONMENT AND DETERMINE WHAT PROCESSING  */ 00248000
*/*            (I.E., CONVERSION OR NORMAL ALLOCATION) IS            */ 00249000
*/*            NECESSARY TO HANDLE THE REQUEST.                      */ 00250000
*/*                                                                  */ 00251000
*/* OPERATION -                                                      */ 00252000
*/*     .'SET UP' . THIS SEGMENT EXAMINES THE REQUEST AND SETS       */ 00253000
*/*      VALUES WHICH CONTROL ALL ENVIRONMENT CHECKING.              */ 00254000
*/*                                                                  */ 00255000
*/*     .LOOP UP THE DSAB CHAIN (I.E. BEGIN WITH THOSE ALLOCATIONS   */ 00256000
*/*      MOST RECENTLY MARKED NOT-IN-USE).                           */ 00257000
*/*      WITHIN THIS LOOP:                                           */ 00258000
*/*                                                                  */ 00259000
*/*         .KEEP A COUNT OF THE NUMBER OF NOT-IN-USE RESOURCES      */ 00260000
*/*          IN CASE 'LIMIT PROCESSING'                              */ 00261000
*/*          NEEDS TO BE DONE LATER.                                 */ 00262000
*/*                                                                  */ 00263000
*/*         .'DSNAME EQUAL'. THIS SEGMENT DETERMINES IF THE          */ 00264000
*/*          REQUESTED DATASET NAME MATCHES THAT OF THE ALLOCATION   */ 00265000
*/*          BEING EXAMINED. AN ALLOCATION IS ELIGIBLE FOR           */ 00266000
*/*          CONVERSION ONLY IF THE DSNAME REQUESTED MATCHES         */ 00267000
*/*          THE DSNAME IN THE ALLOCATION.                           */ 00268000
*/*                                                                  */ 00269000
*/*         .'CKECK DATASET NAME ENVIRONMENT'. FOR EQUAL             */ 00270000
*/*          DATASET NAMES, THIS SEGMENT DETERMINES IF PROPERTIES    */ 00271000
*/*          OF THE EXISTING ALLOCATION PREVENT IT FROM BEING        */ 00272000
*/*          CONVERTED, OR PREVENT THE REQUEST FROM BEING SATISFIED  */ 00273000
*/*          AT ALL.                                                 */ 00274000
*/*                                                                  */ 00275000
*/*         .'CKDDNENV'. IF A REQUESTED DDNAME MATCHES THE DDNAME    */ 00276000
*/*          OF THE ALLOCATION UNDER EXAMINATION, AND THIS           */ 00277000
*/*          ALLOCATION IS INELIGIBLE FOR CONVERSION, THEN DETERMINE */ 00278000
*/*          IF THE DDNAME CAN BE DISASSOCIATED FROM THIS            */ 00279000
*/*          ALLOCATION. IF NOT, THEN THE REQUEST IS FAILED.         */ 00280000
*/*                                                                  */ 00281000
*/*     .'OBTAIN DSORG'. IF AN EXISTING ALLOCATION WAS FOUND         */ 00282000
*/*      ELIGIBLE FOR CONVERSION, AND THE USER REQUESTED             */ 00283000
*/*      THAT DSORG BE RETURNED, THEN CALL A SUBROUTINE              */ 00284000
*/*      TO OBTAIN IT.  IF THE OBTAIN FAILS, THEN FAIL THE           */ 00285000
*/*      REQUEST NOW, RATHER THAN USELESSLY PERFORMING               */ 00286000
*/*      THE CONVERSION.                                             */ 00287000
*/*                                                                  */ 00288000
*/*      NOTE - . IF MORE THAN ONE EXISTING ALLOCATION IS            */ 00289000
*/*               ELIGIBLE FOR CONVERSION, THEN THE ONE SELECTED IS: */ 00290000
*/*                 1) THE ONE ASSOCIATED WITH THE REQUESTED DDNAME, */ 00291000
*/*                    OR, IF NOT IS AVAILABLE, THEN                 */ 00292000
*/*                 2) THE ALLOCATION MOST RECENTLY MARKED NOT-IN-USE,* 00293000
*/*                    I.E., THE ONE FOUND FIRST.                    */ 00294000
*/*                                                                  */ 00295000
*/* EXTERNAL REFERENCES - IEFDB4FE - OBTAIN DATA SET ORGANIZATION    */ 00296000
*/*                                                                  */ 00297000
*/********************************************************************/ 00298000
*CKENVMAP = CKENVMAP && CKENVMAP;      /*ZERO CKENVRON CONTROL SWS   */ 00299000
         XC    CKENVMAP(1),CKENVMAP                             03S0152 00300000
*/********************************************************************/ 00301000
*/*                                                                  */ 00302000
*/* SEGMENT NAME - CKENSTUP                                          */ 00303000
*/*                                                                  */ 00304000
*/* DESCRIPTIVE NAME - SET UP INPUT FOR CKENVRON DSAB CHAIN SEARCH   */ 00305000
*/*                                                                  */ 00306000
*/* FUNCTION - THE USER'S REQUEST IS EXAMINED AND VALUES             */ 00307000
*/*            USED IN SEARCHING THE DSAB CHAIN ARE SET.             */ 00308000
*/*                                                                  */ 00309000
*/* OPERATION -                                                      */ 00310000
*/*    .SWITCHES ARE INITIALLY SET TO                                */ 00311000
*/*     END THE EXAMINATION OF EXISTING ALLOCATIONS.                 */ 00312000
*/*     EXAMINATION OF THE REQUEST CAN THEN ALTER THESE SWITCHES     */ 00313000
*/*     (I.E., CAUSE THE DSAB LOOP IN 'CKENVRON' TO RESUME).         */ 00314000
*/*     EXAMINATION OF THE REQUEST ALSO CAUSES OTHER SWITCHES/       */ 00315000
*/*     VALUES TO BE SET TO CONTROL PATHS WITHIN THE LOOP.           */ 00316000
*/*     ITEMS EXAMINED IN THE REQUESTOR'S TEXTUNITS ARE (IN          */ 00317000
*/*     THIS ORDER) -                                                */ 00318000
*/*                                                                  */ 00319000
*/*       DDNAME -        IF PRESENT, SEARCHING ON DDNAME IS         */ 00320000
*/*                       RESUMED.                                   */ 00321000
*/*                                                                  */ 00322000
*/*       DSNAME -        IF PRESENT AND NOT A GDG-SINGLE REQUEST    */ 00323000
*/*                       (GDG'S ARE NEVER CONVERTED SO THAT GDG     */ 00324000
*/*                       IDIOSYNCRASIES ARE ISOLATED TO JFCB        */ 00325000
*/*                       HOUSEKEEPING), THE  SEARCHING ON DSNAME    */ 00326000
*/*                       IS RESUMED.                                */ 00327000
*/*                                                                  */ 00328000
*/*       SYSOUT -        IN CASE BOTH DSNAME AND SYSOUT WERE        */ 00329000
*/*                       REQUESTED, THE DSNAME SEARCH IS ENDED.     */ 00330000
*/*                       SYSOUT IS NEVER CONVERTED. (SYSOUT IS      */ 00331000
*/*                       REALLY A SUBSYSTEM DATASET REQUEST).       */ 00332000
*/*                                                                  */ 00333000
*/*       SUBSYSTEM -     TREATED THE SAME AS SYSOUT.                */ 00334000
*/*                                                                  */ 00335000
*/*       DUMMY     -     THE DATASET NAME IS SET TO 'NULLFILE', AND */ 00336000
*/*                       THE SEARCH ON DSNAME IS RESUMED. NOTE THAT */ 00337000
*/*                       REQUESTING DUMMY OVERRIDES THE ABOVE       */ 00338000
*/*                       REQUESTS.                                  */ 00339000
*/*                                                                  */ 00340000
*/*       TERMINAL -      IF THE USER IS A TSO USER, THE DATASET     */ 00341000
*/*                       NAME IS SET TO 'NULLFILE', AND THE DSNAME  */ 00342000
*/*                       SEARCH IS RESUMED.  A SWITCH IS SET TO     */ 00343000
*/*                       REMEMBER THIS IS A TERMINAL REQUEST, TO    */ 00344000
*/*                       DISTINGUISH IT FROM A DUMMY REQUEST.       */ 00345000
*/*                       NOTE THAT REQUESTING TERMINAL OVERRIDES THE*/ 00346000
*/*                       ABOVE REQUESTS IF THE USER IS A TSO USER.  */ 00347000
*/*                       HOWEVER, IF THE USER IS NOT A TSO USER,    */ 00348000
*/*                       (I.E., IF THIS IS A NON-TSO        @Z40NQPE*/ 00349000
*/*                       DYNAMIC REQUEST FOR A TERMINAL),   @Z40NQPE*/ 00350000
*/*                       A CHECK OF THE SPECIFIED PARAMETERS@Z40NQPE*/ 00351000
*/*                       IS MADE TO SEE IF THE REQUEST IS TO@Z40NQPE*/ 00352000
*/*                       BE CONVERTED TO A SYSOUT REQUEST.  @Z40NQPE*/ 00353000
*/*                       THE FOLLOWING PARAMETERS ARE       @Z40NQPE*/ 00354000
*/*                       ACCEPTABLE AND WILL BE PROCESSED:  @Z40NQPE*/ 00355000
*/*                          DCB PARAMETERS (ALL)            @Z40NQPE*/ 00356000
*/*                          DCB REF                         @Z40NQPE*/ 00357000
*/*                          DDNAME                          @Z40NQPE*/ 00358000
*/*                          UNALLOCATE AT CLOSE             @Z40NQPE*/ 00359000
*/*                          PERMANENTLY ALLOCATED ATTRIBUTE @Z40NQPE*/ 00360000
*/*                          CONVERTIBLE ATTRIBUTE           @Z40NQPE*/ 00361000
*/*                          RETURN PARAMETERS (ALL)         @Z40NQPE*/ 00362000
*/*                       IF ANY OTHER PARAMETERS ARE USED,  @Z40NQPE*/ 00363000
*/*                       THEN THE TERMINAL REQUEST IS IGNORED, AND  */ 00364000
*/*                       THE INPUT REQUEST IS TREATED AS IF THIS    */ 00365000
*/*                       PARTICULAR TEXT UNIT WAS NOT SPECIFIED.    */ 00366000
*/*                                                                  */ 00367000
*/*       VOL/UNIT INFO - (SEE THE CODE FOR THE EXACT VALUES CHECKED)*/ 00368000
*/*                       IF VOLUME REQUESTS (OTHER THAN VOLSER)     */ 00369000
*/*                       AND/OR UNIT REQUESTS                       */ 00370000
*/*                       ARE MADE, SET A SWITCH WHICH DOES NOT      */ 00371000
*/*                       INHIBIT ANY SEARCHES ACTIVIATED ABOVE, BUT */ 00372000
*/*                       WHICH INDICATES THAT CONVERSION CAN'T BE   */ 00373000
*/*                       DONE.  THIS ALLOWS ANY CONFLICTS           */ 00374000
*/*                       BETWEEN THE REQUEST AND EXISTING           */ 00375000
*/*                       ALLOCATIONS TO BE DISCOVERED.              */ 00376000
*/*                                                                  */ 00377000
*/*       DON'T CONVERT SW - IF THE USER HAS REQUESTED THAT          */ 00378000
*/*                          CONVERSION NOT BE DONE, TREAT THIS THE  */ 00379000
*/*                          SAME AS VOL/UNIT INFORMATION IS         */ 00380000
*/*                          TREATED ABOVE, I.E., SEARCH             */ 00381000
*/*                          TO TEST FOR CONFLICTS ONLY.             */ 00382000
*/*                                                                  */ 00383000
*/*    . A SEGMENT IS INCLUDED WHICH SETS A SWITCH ON IF ANY         */ 00384000
*/*      DCB PARAMETERS WERE REQUESTED IN THE TEXT UNITS.            */ 00385000
*/*                                                                  */ 00386000
*/*   OUTPUT DATA -                                                  */ 00387000
*/*     CKENVMAP - WITH FOLLOWING SWITCHES TURNED ON IF NECESSARY:   */ 00388000
*/*                DSNREQSW - DSNAME ALLOC REQUESTED                 */ 00389000
*/*                DDNREQSW - DDNAME ALLOC REQUESTED                 */ 00390000
*/*                ENDDNSSW - DON'T SEARCH FOR DDNAME                */ 00391000
*/*                DSNTRMSW - TERM=TS REQUESTED                      */ 00392000
*/*                CKDSCFSW - DON'T USE EXISTING ALLOCATION, CHECK   */ 00393000
*/*                           FOR CONFLICTS ONLY                     */ 00394000
*/*     ALFNCMAP - DCBSPSW SET ON IF ANY DCB PARMS REQUESTED         */ 00395000
*/*     DDNREQ - DDNAME SPECIFIED IN INPUT, PADDED TO 8-CHARACTERS   */ 00396000
*/*     DSNREQ - DSNAME SPECIFIED IN INPUT, PADDED TO 44-CHARACTERS  */ 00397000
*/*                                                                  */ 00398000
*/********************************************************************/ 00399000
*ENDDNSSW = ON;                        /*INIT END DDN SEARCH SW ON   */ 00400000
         OI    ENDDNSSW,B'00100000'                             04S0153 00401000
*IF DDNAMTUP ^= ZERO THEN              /*DDNAME SPECIFIED IN REQUEST */ 00402000
