*                                                            @YM03581*/ 01031000
         B     @RC00218                                         04S0220 01032000
@RF00218 L     @10,DSABSIOT(,DSABPTR)                           04S0220 01033000
         TM    SIOALIAS(@10),B'10000000'                        04S0220 01034000
         BNO   @RF00220                                         04S0220 01035000
         L     @10,SIOTJFX(,@10)                                04S0220 01036000
         CLC   DSNREQ(44),JFCBXNAM(@10)                         04S0220 01037000
         BNE   @RF00220                                         04S0220 01038000
*            DSNEQUSW = ON;            /*REMEMBER WERE EQUAL @YM03581*/ 01039000
         OI    DSNEQUSW,B'00001000'                             04S0221 01040000
*        ELSE                          /*NO ALIAS OR MATCH   @YM03581*/ 01041000
*          IF DSABANMP^=0 THEN         /*ALT DSNAME PRESENT? @YM03581*/ 01042000
         B     @RC00220                                         04S0222 01043000
@RF00220 L     @10,DSABANMP(,DSABPTR)                           04S0222 01044000
         LTR   @10,@10                                          04S0222 01045000
         BZ    @RF00222                                         04S0222 01046000
*            DO;                       /*TRY ALTERNATE DSN   @YM03581*/ 01047000
*              DSNEXIST = DSABANAM(1:DSABANML); /*COPY ALTERNATE        01048000
*                                                 DSNAME     @YM03581*/ 01049000
         MVI   DSNEXIST+1,C' '                                  04S0224 01050000
