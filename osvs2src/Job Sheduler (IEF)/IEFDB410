         EX    @10,@SM02703                                     04S0157 00414000
*    DDNREQSW = ON;                    /*INDICATE HAVE A DDN REQUESTD*/ 00415000
*    ENDDNSSW = OFF;                   /*WANT TO SEARCH DSAB'S FOR DD*/ 00416000
         OI    DDNREQSW,B'01000000'                             04S0159 00417000
         NI    ENDDNSSW,B'11011111'                             04S0159 00418000
*  END;                                                         04S0160 00419000
*                                                               04S0160 00420000
*RFY TEXTUNIT BASED(MEMBRTUP);                                  04S0161 00421000
@RF00154 DS    0H                                               04S0162 00422000
*IF DSNAMTUP ^= ZERO                   /*DSNAME SPECIFIED IN REQUEST */ 00423000
