         BNZ   @RF00330                                         03S0330 01694000
*    DO;                                                        03S0331 01695000
*/********************************************************************/ 01696000
*/*                                                                  */ 01697000
*/* SEGMENT NAME - CKDDNENV                                          */ 01698000
*/*                                                                  */ 01699000
*/* DESCRIPTIVE NAME - CHECK EXISTING ALLOCATION OF SPECIFIED DDNAME */ 01700000
*/*                                                                  */ 01701000
*/* FUNCTION - DETERMINE WHETHER THE DDNAME ASSOCIATED WITH THE      */ 01702000
*/*            EXISTING ALLOCATION CAN BE DISASSOCIATED FROM IT.     */ 01703000
*/*                                                                  */ 01704000
*/*   OPERATION - THE DSAB IS CHECKED FOR ENVIRONMENTAL              */ 01705000
*/*     CONFLICTS. IF THE SPECIFIED DDNAME IS ASSOCIATED WITH AN     */ 01706000
*/*     EXISTING ALLOCATION WHICH IS IN-USE, OPEN, DOES NOT          */ 01707000
*/*     HAVE THE CONVERTIBLE ATTRIBUTE, OR IS ASSOCIATED WITH        */ 01708000
*/*     A PERMANENTLY CONCATENATED GROUP THAT IS NOT SYSTEM          */ 01709000
*/*     GENERATED (E.G. GDG-ALL OR VSAM MULTI-DEVICE),               */ 01710000
*/*     THEN AN ERROR CODE IS SET AND PROCESSING TERMINATES.         */ 01711000
*/*     OTHERWISE, THE DDNAME SPECIFIED CAN BE DISASSOCIATED FROM THE*/ 01712000
*/*     EXISTING ALLOCATION.  THE DDNAME SEARCH IS ENDED.            */ 01713000
*/*                                                                  */ 01714000
*/*   OUTPUT DATA -                                                  */ 01715000
*/*     DDNCNP   - ADDRESS OF DSAB FROM WHICH THE DDNAME             */ 01716000
*/*                IS TO BE DISASSOCIATED.                           */ 01717000
*/*     ENDDNSSW - ON TO END DDNAME SEARCH                           */ 01718000
*/*     ERRCODE  -                                                   */ 01719000
*/*                        - SPECIFIED DDNAME NOT AVAILABLE          */ 01720000
*/*                          (NOT CONVERTIBLE OR MARKED IN-USE, ETC.)*/ 01721000
*/*                        - SPECIFIED DDNAME IS ASSOCIATED WITH AN  */ 01722000
*/*                          OPEN DATA SET                           */ 01723000
*/*                                                                  */ 01724000
*/********************************************************************/ 01725000
*IF DSABNUSE = ON |                    /*DATA SET IN USE             */ 01726000
*  DSABCONV = OFF |                    /*DATA SET NOT CONVERTIBLE    */ 01727000
*  (DSABPCAT = ON &                    /*DATA SET PART OF PERMANENTLY   01728000
*                                        CONCATENATED GROUP WHICH IS    01729000
*                                        NOT SYSTEM-GEN'D ALLOCATION */ 01730000
*  DSABFCHN->DSABSIOT->SIOTGDGA = OFF) THEN                     04S0332 01731000
         TM    DSABNUSE(DSABPTR),B'00000001'                    04S0332 01732000
         BNZ   @RT00332                                         04S0332 01733000
         TM    DSABCONV(DSABPTR),B'00010000'                    04S0332 01734000
         BNO   @RT00332                                         04S0332 01735000
         TM    DSABPCAT(DSABPTR),B'00000100'                    04S0332 01736000
         BNO   @RF00332                                         04S0332 01737000
         L     @10,DSABFCHN(,DSABPTR)                           04S0332 01738000
         L     @10,DSABSIOT(,@10)                               04S0332 01739000
         TM    SIOTGDGA(@10),B'01000000'                        04S0332 01740000
         BNZ   @RF00332                                         04S0332 01741000
@RT00332 DS    0H                                               04S0333 01742000
*  ERRCODE = RCDDUNAV;                 /*DDNAME NOT AVAILABLE ERROR  */ 01743000
         L     @10,@PC00001+4                                   04S0333 01744000
         MVC   ERRCODE(2,@10),@CB02404                          04S0333 01745000
*ELSE                                  /*NO ERROR                    */ 01746000
*  IF DSABOPCT^=0 THEN                 /*DATASET IS OPEN             */ 01747000
         B     @RC00332                                         04S0334 01748000
@RF00332 LH    @10,DSABOPCT(,DSABPTR)                           04S0334 01749000
         LTR   @10,@10                                          04S0334 01750000
         BZ    @RF00334                                         04S0334 01751000
*    ERRCODE = RCOPENDS;               /*DDNAME WITH OPEN D.S. ERROR */ 01752000
         L     @10,@PC00001+4                                   04S0335 01753000
         MVC   ERRCODE(2,@10),@CB02406                          04S0335 01754000
*  ELSE                                /*NOT OPEN                    */ 01755000
*    DO;                               /*SET UP TO FREE DDNAME       */ 01756000
         B     @RC00334                                         04S0336 01757000
@RF00334 DS    0H                                               04S0337 01758000
*      DDNCNP = DSABPTR;               /*SAVE ADDR OF DSAB WITH DDNAME  01759000
*                                        TO FREE                     */ 01760000
         ST    DSABPTR,DDNCNP                                   04S0337 01761000
*      ENDDNSSW = ON;                  /*INDICATE WANT TO END DDNAME    01762000
*                                        SEARCH                      */ 01763000
         OI    ENDDNSSW,B'00100000'                             04S0338 01764000
*    END;                                                       04S0339 01765000
*    END;                                                       03S0340 01766000
@RC00334 DS    0H                                               03S0340 01767000
@RC00332 DS    0H                                               03S0341 01768000
*  DSABPTR = DSABBCHN;                 /*POINT TO NEXT DSAB IN CHAIN */ 01769000
@RF00330 L     DSABPTR,DSABBCHN(,DSABPTR)                       03S0341 01770000
*END;                                  /* END OF MAJOR SEARCH LOOP   */ 01771000
*                                                               03S0342 01772000
*                                                               03S0342 01773000
@DE00211 L     @10,@PC00001+4                                   03S0342 01774000
         ICM   @08,3,ERRCODE(@10)                               03S0342 01775000
         BNZ   @DC00211                                         03S0342 01776000
         SLR   @10,@10                                          03S0342 01777000
         CR    DSABPTR,@10                                      03S0342 01778000
         BE    @DC00211                                         03S0342 01779000
         C     @10,DSNCNP                                       03S0342 01780000
         BE    @DL00211                                         03S0342 01781000
         TM    ENDDNSSW,B'00100000'                             03S0342 01782000
         BZ    @DL00211                                         03S0342 01783000
@DC00211 DS    0H                                               03S0343 01784000
*IF ERRCODE = 0 THEN                   /*NO ERROR                    */ 01785000
         L     @10,@PC00001+4                                   03S0343 01786000
         ICM   @08,3,ERRCODE(@10)                               03S0343 01787000
         BNZ   @RF00343                                         03S0343 01788000
*  DO;                                                          03S0344 01789000
*    IF DSNCNP^=ZERO & RTORGTUP^=ZERO THEN /*REQ TO RETN DSORG       */ 01790000
         SLR   @10,@10                                          03S0345 01791000
         C     @10,DSNCNP                                       03S0345 01792000
         BE    @RF00345                                         03S0345 01793000
         C     @10,RTORGTUP(,KEYTABP)                           03S0345 01794000
         BE    @RF00345                                         03S0345 01795000
*      DO;                             /*RETURN DSORG                */ 01796000
*        CALL IEFDB4FE (DSNCNP);       /*GET DATA SET ORGANIZATION   */ 01797000
         LA    @10,DSNCNP                                       03S0347 01798000
         ST    @10,@AL00001                                     03S0347 01799000
         L     @15,@CV00136                                     03S0347 01800000
         LA    @01,@AL00001                                     03S0347 01801000
         BALR  @14,@15                                          03S0347 01802000
*        IF R15 ^= ZERO THEN           /*ERROR RETURN                */ 01803000
         LTR   R15,R15                                          03S0348 01804000
         BZ    @RF00348                                         03S0348 01805000
*          ERRCODE = RCOBTNER | R15;   /*SET REASON CODE             */ 01806000
         L     @14,@PC00001+4                                   03S0349 01807000
         L     @10,@CF02534                                     03S0349 01808000
         OR    @10,R15                                          03S0349 01809000
         STCM  @10,3,ERRCODE(@14)                               03S0349 01810000
*      END;                                                     03S0350 01811000
@RF00348 DS    0H                                               03S0351 01812000
*  END;                                                         03S0351 01813000
@RF00345 DS    0H                                               03S0352 01814000
*                                                               02S0352 01815000
*IF ERRCODE=0 & DSNCNP=0 THEN          /*NO ERROR & NEW ALLOC NEC.   */ 01816000
@RF00343 L     @10,@PC00001+4                                   02S0352 01817000
         ICM   @08,3,ERRCODE(@10)                               02S0352 01818000
         BNZ   @RF00352                                         02S0352 01819000
         L     @10,DSNCNP                                       02S0352 01820000
         LTR   @10,@10                                          02S0352 01821000
         BNZ   @RF00352                                         02S0352 01822000
*  DO;                                                          02S0353 01823000
*/********************************************************************/ 01824000
*/*                                                                  */ 01825000
*/* SEGMENT NAME - UPTOLIM                                           */ 01826000
*/*                                                                  */ 01827000
*/* DESCRIPTIVE NAME - LIMIT PROCESSING ROUTINE                      */ 01828000
*/*                                                                  */ 01829000
*/* FUNCTION - DETERMINE IF THE DYNAMIC ALLOCATION CONTROL VALUE     */ 01830000
*/*   FOR THE NUMBER OF RESOURCES HELD IN ANTICIPATION OF REUSE HAS  */ 01831000
*/*   BEEN EXCEEDED AND, IF SO, UNALLOCATE EXISTING ALLOCATIONS      */ 01832000
*/*   IN ORDER TO RETURN TO THE CONTROL VALUE.                       */ 01833000
*/*                                                                  */ 01834000
*/* OPERATION - LOOP THROUGH THE DSAB CHAIN BEGINNING AT THE TOP,    */ 01835000
*/*     I.E., WITH THE DSABS NOT IN USE FOR THE LONGEST TIME.        */ 01836000
*/*     FOR A NON-CONCATENATED DSAB THAT IS NOT-IN-USE AND NOT       */ 01837000
*/*     PERMANENTLY ALLOCATED, UNALLOCATE IT.  FOR A CONCATENATED    */ 01838000
*/*     GROUP, IF IT IS NOT-IN-USE AND AT LEAST 1 MEMBER IS NOT      */ 01839000
*/*     PERMANENTLY ALLOCATED, DECONCATENATE IT AND THEN RESUME      */ 01840000
*/*     THE LOOP AT THAT NON-PERMANENTLY ALLOCATED DSAB.             */ 01841000
*/*     NOTE THAT IN A PERMANENTLY CONCATENATED GROUP EITHER ALL     */ 01842000
*/*     MEMBERS ARE PERMANENTLY ALLOCATED, OR IN THE CASE OF A       */ 01843000
*/*     SYSTEM-GENERATED 'PCAT' GROUP, IT MAY BE THAT NO             */ 01844000
*/*     MEMBERS ARE PERMANENTLY ALLOCATED. NOTE ALSO THAT A SYSTEM-  */ 01845000
*/*     GENERATED PCAT GROUP COUNTED AS ONLY ONE TOWARD THE          */ 01846000
*/*     LIMIT COUNT.                                                 */ 01847000
*/*                                                                  */ 01848000
*/* NOTES -                                                          */ 01849000
*/*      .NO ATTEMPT IS MADE TO RECOVER FROM AN ERROR DURING         */ 01850000
*/*       UNALLOCATION - PROCESSING CONTINUES AND ANOTHER AVAILABLE  */ 01851000
*/*       DSAB IS UNALLOCATED IF POSSIBLE. IF UNALLOCATION ERRORS    */ 01852000
*/*       PREVENT SUCCESSFUL LIMIT PROCESSING - AN ERROR CODE IS SET.*/ 01853000
*/*                                                                  */ 01854000
*/*      .IT IS NECESSARY TO WRITE UPDATED CONTROL BLOCKS TO SWA     */ 01855000
*/*       AFTER EACH CALL TO UNALLOCATION INSTEAD OF ACCUMULATING    */ 01856000
*/*       THEM ON THE EPA CHAIN, IN ORDER TO HANDLE THE FOLLOWING    */ 01857000
*/*       CASE - ONE CALL TO UNALLOCATION ADDS AN UPDATED SIOT TO    */ 01858000
*/*       THE CHAIN AND A FOLLOWING CALL DELETES THIS SAME SIOT.     */ 01859000
*/*       IF THE EPA FOR THE UPDATED SIOT HAD NOT YET BEEN PROCESSED */ 01860000
*/*       AND REMOVED FROM THE CHAIN, THAT EPA WOULD REFER TO        */ 01861000
*/*       A DELETED SIOT.                                            */ 01862000
*/*                                                                  */ 01863000
*/*  INPUT DATA -                                                    */ 01864000
*/*             - SCTNIUSL - TOTAL NO. OF DD'S + DYNAMNBR            */ 01865000
*/*             - LIMCOUNT - COUNT OF NOT-IN-USE DSABS ON THE CHAIN  */ 01866000
*/*                                                                  */ 01867000
*/*  OUTPUT DATA -                                                   */ 01868000
*/*              - REDUCTION OF NOT-IN-USE ALLOCATIONS, IF NECESSARY.*/ 01869000
*/*              - ERRCODE  - SET IF REQUEST CAN'T BE PROCESSED      */ 01870000
*/*                                                                  */ 01871000
*/* EXTERNAL REFERENCES -                                            */ 01872000
*/*                     IEFDB460 - DECONCATENATION                   */ 01873000
*/*                     IEFDB4A0 - UNALLOCATION                      */ 01874000
*/********************************************************************/ 01875000
*IF LIMCOUNT>SCTNIUSL THEN             /* IF THE CURRENT NUMBER OF      01876000
*                                         ALLOCATIONS HELD FOR REUSE    01877000
*                                         EXCEEDS THE CONTROL VALUE  */ 01878000
         L     @10,@PC00001+8                                   03S0354 01879000
         L     @08,SCTPTR(,@10)                                 03S0354 01880000
         LH    @08,SCTNIUSL(,@08)                               03S0354 01881000
         N     @08,@CF02722                                     03S0354 01882000
         CH    @08,LIMCOUNT                                     03S0354 01883000
         BNL   @RF00354                                         03S0354 01884000
*  DO;                                 /* FIND DATASETS TO UNALLOC   */ 01885000
*    DSABPTR=DSQFRSTP;                 /* SEARCH ENTIRE DSAB CHAIN   */ 01886000
         L     @10,JSCBPTR(,@10)                                03S0356 01887000
         L     @10,JSCDSABQ(,@10)                               03S0356 01888000
         L     DSABPTR,DSQFRSTP(,@10)                           03S0356 01889000
*    RFY TXTUPELM BASED(ADDR(LIMTUP)); /* BASE FOR TEXTUNIT MAP      */ 01890000
*    TXTPLENT = ADDR(LIMTU);           /* POINT TO ACTUAL TU TO BUILD   01891000
*                                         FOR DECONCAT/UNALLOC CALLS */ 01892000
         LA    @10,LIMTUP                                       03S0358 01893000
         LA    @08,LIMTU                                        03S0358 01894000
         ST    @08,TXTPLENT(,@10)                               03S0358 01895000
*    TXTPLEND = ON;                    /* TURN ON LAST TUP INDICATOR */ 01896000
         OI    TXTPLEND(@10),B'10000000'                        03S0359 01897000
*    RFY TEXTUNIT BASED(TXTPLENT);                              03S0360 01898000
*    TEXTUNUM = 1;                     /* SET NO. OF PARMS           */ 01899000
         L     @10,TXTPLENT(,@10)                               03S0361 01900000
         MVC   TEXTUNUM(2,@10),@CH00084                         03S0361 01901000
*    TEXTULNG = 8;                     /* LENGTH OF DDNAME           */ 01902000
         MVC   TEXTULNG(2,@10),@CH00073                         03S0362 01903000
*                                      /* KEY IS FILLED IN LATER     */ 01904000
*    DO WHILE (LIMCOUNT>SCTNIUSL & DSABPTR^=ZERO);              03S0363 01905000
         B     @DE00363                                         03S0363 01906000
@DL00363 DS    0H                                               03S0364 01907000
*                                      /* PROCESS WHILE THE CONTROL     01908000
*                                         VALUE IS STILL EXCEEDED AND   01909000
*                                         NOT AT CHAIN END           */ 01910000
*      DMYRSCD = 0;                    /* INIT DUMMY REASON CODE TO BE  01911000
*                                         SET BY DECONCAT AND UNALLOC*/ 01912000
         SLR   @10,@10                                          03S0364 01913000
         ST    @10,DMYRSCD                                      03S0364 01914000
*      NXTPTR = DSABFCHN;              /*DEFAULT - NEXT DSAB TO SRCH */ 01915000
         L     @08,DSABFCHN(,DSABPTR)                           03S0365 01916000
         ST    @08,NXTPTR                                       03S0365 01917000
*      IF DSABNUSE=NOTINUSE & DSABOPCT=0 THEN /*ONLY FREE NOTINUSE      01918000
*                                               DSABS                */ 01919000
         TM    DSABNUSE(DSABPTR),B'00000001'                    03S0366 01920000
         BNZ   @RF00366                                         03S0366 01921000
         CH    @10,DSABOPCT(,DSABPTR)                           03S0366 01922000
         BNE   @RF00366                                         03S0366 01923000
*        DO;                                                    03S0367 01924000
*          IF DSABCATM=ON  THEN        /* 1ST MEMBER OF CONCAT GROUP */ 01925000
         TM    DSABCATM(DSABPTR),B'00000010'                    03S0368 01926000
         BNO   @RF00368                                         03S0368 01927000
*            DO;                       /* SO SEE IF SHOULD DECONCAT  */ 01928000
*              QUIT  = OFF;            /* ON - QUIT BELOW LOOP       */ 01929000
*              FOUND = OFF;            /* ON - AT LEAST ONE MEMBER OF   01930000
*                                         GROUP NOT PERM. ALLOC'D    */ 01931000
         NI    QUIT,B'00111111'                                 03S0371 01932000
*              NXTPTR = ADDR(DSAB);    /* BEGIN LOOP AT TOP OF GROUP */ 01933000
         ST    DSABPTR,NXTPTR                                   03S0372 01934000
*              DO WHILE QUIT=OFF;      /* SEARCH FOR A NON-PERM ALLOC   01935000
*                                         MEMBER                     */ 01936000
         B     @DE00373                                         03S0373 01937000
@DL00373 DS    0H                                               03S0374 01938000
*                                      /* LEAVE NXTPTR POINTING TO 1ST  01939000
*                                         DSAB AFTER THE GROUP       */ 01940000
*                IF NXTPTR->DSABPALC=OFF /* FOUND A NON-PERMALC      */ 01941000
*                   & FOUND=OFF THEN     /* AND ITS 1ST OR ONLY      */ 01942000
         L     @10,NXTPTR                                       03S0374 01943000
         TM    DSABPALC(@10),B'01000000'                        03S0374 01944000
         BNZ   @RF00374                                         03S0374 01945000
         TM    FOUND,B'10000000'                                03S0374 01946000
         BNZ   @RF00374                                         03S0374 01947000
*                  DO;                                          03S0375 01948000
*                    FOUND = ON;       /* REMEMBER AT LEAST ONE FOUND*/ 01949000
         OI    FOUND,B'10000000'                                03S0376 01950000
*                    NPALCPTR = NXTPTR;/* REMEMBER ITS LOCATION     */  01951000
         ST    @10,NPALCPTR                                     03S0377 01952000
*                  END;                                         03S0378 01953000
*                NXTPTR = NXTPTR->DSABFCHN; /* FIND NEXT DSAB        */ 01954000
@RF00374 L     @10,NXTPTR                                       03S0379 01955000
         L     @10,DSABFCHN(,@10)                               03S0379 01956000
         ST    @10,NXTPTR                                       03S0379 01957000
*                IF NXTPTR=0           /* IF END OF CHAIN OR GROUP   */ 01958000
*                  | NXTPTR->DSABTIOT->TIOEDDNM(1)^=' ' THEN    03S0380 01959000
         LTR   @10,@10                                          03S0380 01960000
         BZ    @RT00380                                         03S0380 01961000
         L     @10,DSABTIOT(,@10)                               03S0380 01962000
         CLI   TIOEDDNM(@10),C' '                               03S0380 01963000
         BE    @RF00380                                         03S0380 01964000
@RT00380 DS    0H                                               03S0381 01965000
*                  QUIT = ON;          /* THEN EXIT LOOP             */ 01966000
         OI    QUIT,B'01000000'                                 03S0381 01967000
*              END;                    /* END CONCAT GROUP SEARCH    */ 01968000
*                                                               03S0382 01969000
@RF00380 DS    0H                                               03S0382 01970000
@DE00373 TM    QUIT,B'01000000'                                 03S0382 01971000
         BZ    @DL00373                                         03S0382 01972000
*              IF FOUND=ON  THEN       /* WORTHWHILE DECONCAT'ING GRP*/ 01973000
         TM    FOUND,B'10000000'                                03S0383 01974000
         BNO   @RF00383                                         03S0383 01975000
*                DO;                   /* YES - CALL DECONCAT PROC   */ 01976000
*                  TEXTUKEY = DCCDDNAM;/* BUILD DECONCAT TEXT UNIT   */ 01977000
         LA    @10,LIMTUP                                       03S0385 01978000
         L     @08,TXTPLENT(,@10)                               03S0385 01979000
         MVC   TEXTUKEY(2,@08),@CB01830                         03S0385 01980000
*                  TEXTUPAR(1:8) = TIOEDDNM;/* DDNAME OF GROUP       */ 01981000
         L     @05,DSABTIOT(,DSABPTR)                           03S0386 01982000
         MVC   TEXTUPAR(8,@08),TIOEDDNM(@05)                    03S0386 01983000
*                  CALL IEFDB460 (ADDR(TXTPLENT),DMYRSCD,       03S0387 01984000
*                                SYSCTBLK,ALFNCMAP,DEPARMS,EPALSTP,     01985000
*                                SMFPARMS); /*               @ZA02706*/ 01986000
         ST    @10,@TF00001                                     03S0387 01987000
         LA    @10,@TF00001                                     03S0387 01988000
         ST    @10,@AL00001                                     03S0387 01989000
         LA    @10,DMYRSCD                                      03S0387 01990000
         ST    @10,@AL00001+4                                   03S0387 01991000
         L     @10,@PC00001+8                                   03S0387 01992000
         ST    @10,@AL00001+8                                   03S0387 01993000
         L     @10,@PC00001+12                                  03S0387 01994000
         ST    @10,@AL00001+12                                  03S0387 01995000
         L     @10,@PC00001+16                                  03S0387 01996000
         ST    @10,@AL00001+16                                  03S0387 01997000
         L     @10,@PC00001+20                                  03S0387 01998000
         ST    @10,@AL00001+20                                  03S0387 01999000
         L     @10,@PC00001+24                                  03S0387 02000000
         ST    @10,@AL00001+24                                  03S0387 02001000
         L     @15,@CV00154                                     03S0387 02002000
         LA    @01,@AL00001                                     03S0387 02003000
         BALR  @14,@15                                          03S0387 02004000
*                                      /* NOTE - NOP FOR A PCAT GROUP*/ 02005000
*                  IF DMYRSCD=0 THEN   /* IF DECONCAT SUCCESSFUL     */ 02006000
         L     @10,DMYRSCD                                      03S0388 02007000
         LTR   @10,@10                                          03S0388 02008000
         BNZ   @RF00388                                         03S0388 02009000
*                    DO;                                        03S0389 02010000
*                      DSABPTR = NPALCPTR;/* UNALLOC 1ST NON-PALC DSAB* 02011000
         L     DSABPTR,NPALCPTR                                 03S0390 02012000
*                      NXTPTR = DSABFCHN; /* NEXT DSAB AFTER UNALLOC */ 02013000
*                                                               03S0391 02014000
         L     @10,DSABFCHN(,DSABPTR)                           03S0391 02015000
         ST    @10,NXTPTR                                       03S0391 02016000
*                      /* IF HAVE A SYSTEM-GENERATED PCAT-GROUP TO BE*/ 02017000
*                      /* UNALLOCATED, SET 'NXTPTR' PAST THE GROUP   */ 02018000
*                      /* NOTE THAT THE 'WHILE' CONDITIONS WILL BE   */ 02019000
*                      /* SATISFIED FOR ALL BUT A PCAT GROUP.        */ 02020000
*                      DO WHILE (NXTPTR^=0 /*LOOP UNTIL END OF GROUP */ 02021000
*                            & NXTPTR->DSABTIOT->TIOEDDNM(1)=' ');      02022000
         B     @DE00392                                         03S0392 02023000
@DL00392 DS    0H                                               03S0393 02024000
*                        NXTPTR = NXTPTR->DSABFCHN; /*NEXT DSAB      */ 02025000
         L     @10,NXTPTR                                       03S0393 02026000
         L     @10,DSABFCHN(,@10)                               03S0393 02027000
         ST    @10,NXTPTR                                       03S0393 02028000
*                      END;                                     03S0394 02029000
@DE00392 L     @10,NXTPTR                                       03S0394 02030000
         LTR   @10,@10                                          03S0394 02031000
         BZ    @DC00392                                         03S0394 02032000
         L     @10,DSABTIOT(,@10)                               03S0394 02033000
         CLI   TIOEDDNM(@10),C' '                               03S0394 02034000
         BE    @DL00392                                         03S0394 02035000
@DC00392 DS    0H                                               03S0395 02036000
*                    END;                                       03S0395 02037000
*                  ELSE;               /* DECONCAT FAILED            */ 02038000
@RF00388 DS    0H                                               03S0397 02039000
*                END;                  /* FOUND=ON                   */ 02040000
*              ELSE;                   /* ALL GRP MEMBERS PALC       */ 02041000
@RF00383 DS    0H                                               03S0399 02042000
*            END;                      /* END CONCAT GRP PROCESSING  */ 02043000
*          ELSE;                       /* DSAB NOT CONCAT MEMBER     */ 02044000
@RF00368 DS    0H                                               03S0401 02045000
*          IF DSABPALC=OFF             /* IF ELIG. FOR UNALLOCATE    */ 02046000
*           & DMYRSCD=0 THEN           /* ENTRY NOT FROM DECAT FAIL  */ 02047000
         TM    DSABPALC(DSABPTR),B'01000000'                    03S0401 02048000
         BNZ   @RF00401                                         03S0401 02049000
         SLR   @10,@10                                          03S0401 02050000
         C     @10,DMYRSCD                                      03S0401 02051000
         BNE   @RF00401                                         03S0401 02052000
