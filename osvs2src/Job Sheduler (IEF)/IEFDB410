         LA    @10,JFCBVOLS(,@10)                               04S0238 01104000
         ST    @10,VLSRJFCP                                     04S0238 01105000
*        DO I=ONE TO NBVOLSER          /*DO COMP ON INPUT REQ        */ 01106000
*          WHILE (DSNEQUSW=ON);        /*STOP WHEN FIND FIRST^=COMPAR*/ 01107000
         LA    @10,1                                            04S0239 01108000
         B     @DE00239                                         04S0239 01109000
@DL00239 TM    DSNEQUSW,B'00001000'                             04S0239 01110000
         BNO   @DC00239                                         04S0239 01111000
*          INDXVLSR=INDXVLSR+ONE;      /*INCREMENT INDEX INTO JFCB(X)*/ 01112000
         LA    @10,1                                            04S0240 01113000
         AL    @10,INDXVLSR                                     04S0240 01114000
         ST    @10,INDXVLSR                                     04S0240 01115000
*          HOLD6BYT=TEXTUPRM(ONE:TEXTULEN);/*PAD VOLSER WITH BLANKS  */ 01116000
         MVI   HOLD6BYT+1,C' '                                  04S0241 01117000
         MVC   HOLD6BYT+2(4),HOLD6BYT+1                         04S0241 01118000
         L     @08,TXTUFLDP                                     04S0241 01119000
         ICM   @05,12,TEXTULEN(@08)                             04S0241 01120000
         SRA   @05,16                                           04S0241 01121000
         BCTR  @05,0                                            04S0241 01122000
         EX    @05,@SM02714                                     04S0241 01123000
*          IF HOLD6BYT^=VOLSRJFC(INDXVLSR) THEN /*SAME VOLUME SERIALS*/ 01124000
         MH    @10,@CH00071                                     04S0242 01125000
         L     @08,VLSRJFCP                                     04S0242 01126000
         ALR   @08,@10                                          04S0242 01127000
         AL    @08,@CF02716                                     04S0242 01128000
         CLC   HOLD6BYT(6),VOLSRJFC(@08)                        04S0242 01129000
         BE    @RF00242                                         04S0242 01130000
*            DSNEQUSW=OFF;             /*NO, DATASETS NOT SAME       */ 01131000
         NI    DSNEQUSW,B'11110111'                             04S0243 01132000
*          ELSE                        /*YES, CONTINUE COMPARISONS   */ 01133000
*            DO;                                                04S0244 01134000
         B     @RC00242                                         04S0244 01135000
@RF00242 DS    0H                                               04S0245 01136000
*              TXTUFLDP=TXTUFLDP+TEXTULEN+LENGTH(TEXTULEN);     04S0245 01137000
         L     @10,TXTUFLDP                                     04S0245 01138000
         ICM   @08,12,TEXTULEN(@10)                             04S0245 01139000
         SRA   @08,16                                           04S0245 01140000
         ALR   @08,@10                                          04S0245 01141000
         AL    @08,@CF00209                                     04S0245 01142000
         ST    @08,TXTUFLDP                                     04S0245 01143000
*                                      /*POINT TO NEXT ENTRY         */ 01144000
*              IF I=5 THEN             /*JFCB IS DONE AFTER 5 VOL SERS, 01145000
*                                        SO GO TO JFCBX              */ 01146000
         CLC   I(4),@CF01071                                    04S0246 01147000
         BNE   @RF00246                                         04S0246 01148000
*                DO;                                            04S0247 01149000
*                  INDXVLSR=ZERO;      /*INIT INDEX INTO JFCBX VOLSER*/ 01150000
         SLR   @10,@10                                          04S0248 01151000
         ST    @10,INDXVLSR                                     04S0248 01152000
*                  JFCBXPTR=SIOTJFX;   /*POINT TO JFCBX FOR NEXT COMP*/ 01153000
         L     @10,DSABSIOT(,DSABPTR)                           04S0249 01154000
         L     @10,SIOTJFX(,@10)                                04S0249 01155000
         ST    @10,JFCBXPTR                                     04S0249 01156000
*                  VLSRJFCP=ADDR(JFCBXVOL);/*COMP VOL SERS IN JFCBX  */ 01157000
         LA    @10,JFCBXVOL(,@10)                               04S0250 01158000
         ST    @10,VLSRJFCP                                     04S0250 01159000
*                END;                                           04S0251 01160000
*              IF INDXVLSR=15 THEN     /*15 VOL SER IN EACH JFCBX    */ 01161000
@RF00246 CLC   INDXVLSR(4),@CF00076                             04S0252 01162000
         BNE   @RF00252                                         04S0252 01163000
*                DO;                                            04S0253 01164000
*                  INDXVLSR=ZERO;      /*INIT INDEX INTO JFCBX VOLSER*/ 01165000
         SLR   @10,@10                                          04S0254 01166000
         ST    @10,INDXVLSR                                     04S0254 01167000
*                  JFCBXPTR=JFCBXNXT;  /*POINT TO NEXT JFCBX FOR COMP*/ 01168000
         L     @10,JFCBXPTR                                     04S0255 01169000
         L     @10,JFCBXNXT(,@10)                               04S0255 01170000
         ST    @10,JFCBXPTR                                     04S0255 01171000
*                  VLSRJFCP=ADDR(JFCBXVOL);/*COMP VOL SERS IN JFCBX  */ 01172000
         LA    @10,JFCBXVOL(,@10)                               04S0256 01173000
         ST    @10,VLSRJFCP                                     04S0256 01174000
*                END;                                           04S0257 01175000
*            END;                                               04S0258 01176000
@RF00252 DS    0H                                               04S0259 01177000
*        END;                                                   04S0259 01178000
@RC00242 LA    @10,1                                            04S0259 01179000
         AL    @10,I                                            04S0259 01180000
@DE00239 ST    @10,I                                            04S0259 01181000
         CH    @10,NBVOLSER                                     04S0259 01182000
         BNH   @DL00239                                         04S0259 01183000
@DC00239 DS    0H                                               04S0260 01184000
*      END;                                                     04S0260 01185000
*  END;                                                         04S0261 01186000
@RC00234 DS    0H                                               04S0262 01187000
*                                                               03S0262 01188000
*  IF DDNREQSW=ON & DDNREQ=TIOEDDNM THEN /*DDN SPECIFIED=DDN W/ DSAB */ 01189000
@RF00229 TM    DDNREQSW,B'01000000'                             03S0262 01190000
         BNO   @RF00262                                         03S0262 01191000
         L     @10,DSABTIOT(,DSABPTR)                           03S0262 01192000
         CLC   DDNREQ(8),TIOEDDNM(@10)                          03S0262 01193000
         BNE   @RF00262                                         03S0262 01194000
*    DDNEQUSW = ON;                    /*THIS DSAB RELATED TO DDN    */ 01195000
         OI    DDNEQUSW,B'00000100'                             03S0263 01196000
*  ELSE                                /*THIS DSAB NOT RELATED TO DDN*/ 01197000
*    DDNEQUSW = OFF;                                            03S0264 01198000
*                                                               03S0264 01199000
         B     @RC00262                                         03S0264 01200000
@RF00262 NI    DDNEQUSW,B'11111011'                             03S0264 01201000
*  IF DSNEQUSW=ON & DDNEQUSW=ON THEN   /*DSAB HAS BOTH DSN,DDN SPECFD*/ 01202000
@RC00262 TM    DSNEQUSW,B'00001100'                             03S0265 01203000
         BNO   @RF00265                                         03S0265 01204000
*    SAMDSASW = ON;                    /*ONE DSAB HAS BOTH DSN, DDN  */ 01205000
         OI    SAMDSASW,B'00010000'                             03S0266 01206000
*  ELSE                                                         03S0267 01207000
*    SAMDSASW = OFF;                   /*DSN, DDN NOT BOTH MATCHED   */ 01208000
*                                                               03S0267 01209000
         B     @RC00265                                         03S0267 01210000
@RF00265 NI    SAMDSASW,B'11101111'                             03S0267 01211000
*  IF DSNEQUSW = ON &                  /*REQUESTED DSN FOUND         */ 01212000
*    (DSNCNP=0 | SAMDSASW=ON) THEN     /*TAKE 1ST DSN OR DSN WITH DDN*/ 01213000
@RC00265 TM    DSNEQUSW,B'00001000'                             03S0268 01214000
         BNO   @RF00268                                         03S0268 01215000
         L     @10,DSNCNP                                       03S0268 01216000
         LTR   @10,@10                                          03S0268 01217000
         BZ    @RT00268                                         03S0268 01218000
         TM    SAMDSASW,B'00010000'                             03S0268 01219000
         BNO   @RF00268                                         03S0268 01220000
@RT00268 DS    0H                                               03S0269 01221000
*    DO;                                                        03S0269 01222000
*/********************************************************************/ 01223000
*/*                                                                  */ 01224000
*/* SEGMENT NAME - CKDSNENV                                          */ 01225000
*/*                                                                  */ 01226000
*/* DESCRIPTIVE NAME - CKECK DATASET NAME ENVIRONMENT                */ 01227000
*/*                                                                  */ 01228000
*/* FUNCTION - CKDSNENV IS INVOKED BY CKENVRON TO EXAMINE AN EXISTING*/ 01229000
*/*     ALLOCATION OF THE REQUESTED DSNAME. IT DETERMINES WHETHER    */ 01230000
*/*     OR NOT THE EXISTING ALLOCATION CAN BE USED TO SATISFY        */ 01231000
*/*     THE REQUEST.                                                 */ 01232000
*/*                                                                  */ 01233000
*/*   OPERATION - THE DSAB IS FIRST CHECKED FOR ENVIRONMENTAL        */ 01234000
*/*       CONFLICTS WITH THE REQUEST. IF THE REQUEST SPECIFIES A     */ 01235000
*/*       STATUS OF NEW, THEN FINDING IT                             */ 01236000
*/*       ASSOCIATED WITH AN EXISTING ALLOCATION IS AN ERROR.        */ 01237000
*/*       IF THE REQUEST SPECIFIES A STATUS OF SHR OR OLD            */ 01238000
*/*       AND AN EXISTING ALLOCATION IS FOUND NON-                   */ 01239000
*/*       PERMANENTLY ALLOCATED, NOT IN-USE, AND WITH A DISPOSITION  */ 01240000
*/*       OF DELETE, THEN AN ERROR EXISTS, SINCE THE USER COULD NOT  */ 01241000
*/*       PREDICT THAT THE DATA SET WOULD NOT HAVE BEEN UNALLOCATED, */ 01242000
*/*       AND THUS ACTUALLY DELETED, BY LIMIT PROCESSING.  IN BOTH   */ 01243000
*/*       CASES AN ERROR CODE IS SET AND PROCESSING TERMINATES. SEE  */ 01244000
*/*       THE CODE FOR THE EXACT TESTS MADE.                         */ 01245000
*/*       THIS ENDS ALL CONFLICT-ONLY CHECKING (SEE CKENSTUP).       */ 01246000
*/*                                                                  */ 01247000
*/*       'CHECK ELIGIBILITY SEGMENT'.                               */ 01248000
*/*       UNLESS THE USER HAS PRECLUDED THE USE OF EXISTING          */ 01249000
*/*       ALLOCATIONS, THE DSAB IS CHECKED TO SEE IF IT HAS THE      */ 01250000
*/*       NECESSARY PROPERTIES TO BE ELIGIBLE TO SATISFY THE REQUEST.*/ 01251000
*/*       THE DSAB IS ASSUMED ELIGIBLE UNTIL IT                      */ 01252000
*/*       FAILS ONE OF THE TESTS.                                    */ 01253000
*/*                                                                  */ 01254000
*/*       IF THE DSAB IS ELIGIBLE TO SATISFY THE REQUEST, THEN THE   */ 01255000
*/*       DSNAME SEARCH PART OF THE DSAB CHAIN SEARCH IS ENDED.      */ 01256000
*/*       IF A DDNAME WAS REQUESTED AND IS ASSOCIATED WITH THE DSAB, */ 01257000
*/*       THEN THE DDNAME PART OF THE DSAB CHAIN SEARCH IS ALSO ENDED.* 01258000
*/*                                                                  */ 01259000
*/*       IF THE DSAB IS NOT ELIGIBLE TO SATISFY THE REQUEST, THEN   */ 01260000
*/*       THE ADDRESS OF THE DSAB IS SAVED FOR POSSIBLE COPY OF      */ 01261000
*/*       VOL/UNIT INFORMATION. THIS IS DONE ONLY IF:                */ 01262000
*/*       . VOL/UNIT INFORMATION WAS NOT SPECIFIED IN THE REQUEST.   */ 01263000
*/*         (SINCE IF VOL/UNIT INFORMATION OTHER THAN VOLUME SERIAL  */ 01264000
*/*         WAS REQUESTED THEN CONFLICTS ONLY CHECKING WAS DONE, ONLY*/ 01265000
*/*         THE PRESENCE OF THE VOLUME SERIAL TEXT UNIT IS TESTED    */ 01266000
*/*         FOR AT THIS POINT IN THE CODE.)                          */ 01267000
*/*       . THE REQUEST IS NOT FOR A NEW DATA SET. (FOR A    @YM08653*/ 01268000
*/*         NEW REQUEST WHEN AN EXISTING ALLOCATION OF THE   @YM08653*/ 01269000
*/*         DATA SET CANNOT BE CONVERTED, IT IS ASSUMED A    @YM08653*/ 01270000
*/*         DIFFERENT DATA SET IS REQUESTED AND THEREFORE,   @YM08653*/ 01271000
*/*         A COPY OF VOL/UNIT INFORMATION WON'T BE DONE)    @YM08653*/ 01272000
*/*       . THE REQUEST DOES NOT SPECIFY MOD FOR A NON-      @YM08653*/ 01273000
*/*         PERMANENTLY ALLOCATED DATA SET WITH A DISP OF    @YM08653*/ 01274000
*/*         DELETE THAT IS NOT IN-USE. (THE REQUESTOR IN THIS@YM08653*/ 01275000
*/*         CASE CANNOT BE CERTAIN THAT THE DATA SET HAD NOT @YM08653*/ 01276000
*/*         BEEN UNALLOCATED AND DELETED AND THEREFORE, NO   @YM08653*/ 01277000
*/*         ATTEMPT IS MADE TO GIVE HIM THE SAME DATA SET.)  @YM08653*/ 01278000
*/*       . THE EXISTING ALLOCATION IS NOT A VSAM DATA SET.  @YA00068*/ 01279000
*/*         THIS IS TO AVOID ANY PROBLEMS OF COPYING         @YA00068*/ 01280000
*/*         INFORMATION FOR ONLY THE FIRST DEVICE TYPE OF A  @YA00068*/ 01281000
*/*         MULTI DEVICE TYPE VSAM DATASET AND TO AVOID      @YA00068*/ 01282000
*/*         PROBLEMS IN THE GENERAL CASE OF VSAM DATASETS    @YA00068*/ 01283000
*/*         WHERE VSAM LOCATE FILLS ADDITIONAL JFCB FIELDS.  @YA00068*/ 01284000
*/*                                                                  */ 01285000
*/*       NOTE ALSO:                                                 */ 01286000
*/*       IF THE EXISTING ALLOCATION IS NOT ELIGIBLE FOR CONVERSION, */ 01287000
*/*       BUT IS ASSOCIATED WITH THE REQUESTED DDNAME,               */ 01288000
*/*       THEN IN CKDDNENV IT WILL BE DETERMINED EITHER              */ 01289000
*/*       THAT THE DDNAME CAN BE DISASSOCIATED FROM THE EXISTING     */ 01290000
*/*       ALLOCATION OR, IF NOT, THAT THE EXISTING ALLOCATION IS     */ 01291000
*/*       IN CONFLICT WITH THE REQUEST.                              */ 01292000
*/*                                                                  */ 01293000
*/*   OUTPUT -                                                       */ 01294000
*/*       DSNCNP   - ADDRESS OF DSNAME DSAB TO CONVERT               */ 01295000
*/*       COPYDSAB - ADDRESS OF DSAB OF ALLOCATION FROM WHICH        */ 01296000
*/*                  VOL/UNIT INFORMATION CAN BE COPIED              */ 01297000
*/*       ENDDNSSW - ON IF DDNAME ASSOCIATED WITH DSAB AND DSAB      */ 01298000
*/*                  ELIGIBLE TO SATISFY REQUEST                     */ 01299000
*/*       ERRCODE SET FOR -                                          */ 01300000
*/*               - STATUS OF NEW SPECIFIED WITH A DSNAME            */ 01301000
*/*                 WHICH HAS BEEN FOUND ASSOCIATED WITH AN          */ 01302000
*/*                 EXISTING ALLOCATION                              */ 01303000
*/*               - STATUS OF SHR OR OLD SPECIFIED,                  */ 01304000
*/*                 AND AN EXISTING ALLOCATION WAS FOUND WITH A      */ 01305000
*/*                 DISPOSITION OF DELETE.                           */ 01306000
*/*                                                                  */ 01307000
*/********************************************************************/ 01308000
*RFY TEXTUNIT BASED(STATSTUP);                                  04S0270 01309000
*IF TEXTUPAR(ONE) = NEW                /*STATUS NEW SPECIFIED IN REQ */ 01310000
*  & SCTDUMMY=OFF                      /*DATASET NOT DUMMY TYPE      */ 01311000
*  & DSNREQ(ONE) ^= AMPSAND THEN       /*NON-&DSNAME SPECIFIED IN REQ*/ 01312000
         L     @10,STATSTUP(,KEYTABP)                           04S0271 01313000
         CLI   TEXTUPAR(@10),X'04'                              04S0271 01314000
         BNE   @RF00271                                         04S0271 01315000
         L     @10,DSABSIOT(,DSABPTR)                           04S0271 01316000
         TM    SCTDUMMY(@10),B'10000000'                        04S0271 01317000
         BNZ   @RF00271                                         04S0271 01318000
         CLI   DSNREQ,C'&&'                                     04S0271 01319000
         BE    @RF00271                                         04S0271 01320000
*  ERRCODE = RCNEWFND;                 /*NEW DS REQ BUT FOUND CODE   */ 01321000
*                                                               04S0272 01322000
         L     @10,@PC00001+4                                   04S0272 01323000
         MVC   ERRCODE(2,@10),@CB02420                          04S0272 01324000
*IF (TEXTUPAR(ONE) =SHR |              /*STATUS SHR SPECIFIED IN REQ */ 01325000
*  TEXTUPAR(ONE)=OLD) &                /*STATUS OLD SPECIFIED IN REQ */ 01326000
*  SCTDUMMY=OFF &                      /*DATASET NOT DUMMY TYPE      */ 01327000
*  DSABPALC = OFF &                    /*NOT PERMANENTLY ALLOCATED   */ 01328000
*  (DSABNUSE = OFF & DSABOPCT = 0) &   /*NOT IN USE                  */ 01329000
*  SIOTDLET = ON  THEN                 /*NORMAL DISP OF DELETE       */ 01330000
@RF00271 L     @10,STATSTUP(,KEYTABP)                           04S0273 01331000
         CLI   TEXTUPAR(@10),X'08'                              04S0273 01332000
         BE    @GL00125                                         04S0273 01333000
         CLI   TEXTUPAR(@10),X'01'                              04S0273 01334000
         BNE   @RF00273                                         04S0273 01335000
@GL00125 L     @10,DSABSIOT(,DSABPTR)                           04S0273 01336000
         TM    SCTDUMMY(@10),B'10000000'                        04S0273 01337000
         BNZ   @RF00273                                         04S0273 01338000
         TM    DSABPALC(DSABPTR),B'01000000'                    04S0273 01339000
         BNZ   @RF00273                                         04S0273 01340000
         TM    DSABNUSE(DSABPTR),B'00000001'                    04S0273 01341000
         BNZ   @RF00273                                         04S0273 01342000
         SLR   @08,@08                                          04S0273 01343000
         CH    @08,DSABOPCT(,DSABPTR)                           04S0273 01344000
         BNE   @RF00273                                         04S0273 01345000
         TM    SIOTDLET(@10),B'00000100'                        04S0273 01346000
         BNO   @RF00273                                         04S0273 01347000
*    DO;                               /*THE DATASET COULD HAVE BEEN    01348000
*                                        UNALLOCATED BY LIMIT PROC. &   01349000
*                                        THEN IT WOULDN'T BE FOUND  */  01350000
*      IF VLSERTUP^=ZERO THEN          /*IF VOLSER SPEC IN REQ       */ 01351000
         C     @08,VLSERTUP(,KEYTABP)                           04S0275 01352000
         BE    @RF00275                                         04S0275 01353000
*        ERRCODE = RCDELETE;           /*SET DYNAMIC ALOC ERROR CODE */ 01354000
         L     @10,@PC00001+4                                   04S0276 01355000
         MVC   ERRCODE(2,@10),@CB02422                          04S0276 01356000
*      ELSE                            /*VOLSER NOT SPEC IN REQ      */ 01357000
*        ERRCODE = RCLCAT08;           /*ERROR CODE=LOCATE ERROR SINCE  01358000
*                                        LOCATE WOULD HAVE FAILED IN    01359000
*                                        NORMAL ALLOC IF LET REQ THRU*/ 01360000
         B     @RC00275                                         04S0277 01361000
@RF00275 L     @10,@PC00001+4                                   04S0277 01362000
         MVC   ERRCODE(2,@10),@CB02484                          04S0277 01363000
*    END;                                                       04S0278 01364000
*                                                               04S0278 01365000
@RC00275 DS    0H                                               04S0279 01366000
*IF ERRCODE = 0 &                      /*NO ERROR                    */ 01367000
*  CKDSCFSW=OFF THEN                   /*AND DID NOT DECIDE TO  04S0279 01368000
*                                        CHECK ONLY FOR CONFLICTS    */ 01369000
@RF00273 L     @10,@PC00001+4                                   04S0279 01370000
         ICM   @08,3,ERRCODE(@10)                               04S0279 01371000
         BNZ   @RF00279                                         04S0279 01372000
         TM    CKDSCFSW,B'00000001'                             04S0279 01373000
         BNZ   @RF00279                                         04S0279 01374000
*  DO;                                                          04S0280 01375000
*    ELIGBLSW=ON;                      /*ASSUME DSAB ELIGIBLE TO        01376000
*                                        SATISFY REQUEST INITIALLY   */ 01377000
         OI    ELIGBLSW,B'10000000'                             04S0281 01378000
*/********************************************************************/ 01379000
*/*                                                                  */ 01380000
*/* SEGMENT NAME - CKDSNELG                                          */ 01381000
*/*                                                                  */ 01382000
*/* DESCRIPTIVE NAME - CHECK ALLOCATION FOR ELIGIBILITY FOR          */ 01383000
*/*                    FOR CONVERSION.                               */ 01384000
*/*                                                                  */ 01385000
*/* FUNCTION - CKDSNELG DETERMINES IF THE                            */ 01386000
*/*            EXISTING ALLOCATION IS ELIGIBLE FOR CONVERSION.       */ 01387000
*/*                                                                  */ 01388000
*/*   OPERATION - THE EXISTING ALLOCATION IS CHECKED FOR THE FOLLOWING* 01389000
*/*       PROPERTIES TO SEE IF IT IS ELIGIBLE TO SATISFY THE REQUEST:*/ 01390000
*/*       1) IT MUST NOT BE IN USE                                   */ 01391000
*/*       2) IT MUST NOT BE A MEMBER OF A CONCATENATED               */ 01392000
*/*          GROUP                                                   */ 01393000
*/*       3) IF IT IS NOT PERMANENTLY ALLOCATED, IS NOT IN USE AND   */ 01394000
*/*          HAS A DISPOSITION OF DELETE, THEN THE STATUS OF MOD     */ 01395000
*/*          MUST NOT HAVE BEEN SPECIFIED IN THE REQUEST             */ 01396000
*/*       4) IT MUST HAVE THE CONVERTIBLE ATTRIBUTE OR IF NOT, MUST  */ 01397000
*/*          SATISFY AN ADDITIONAL SET OF REQUIREMENTS (SEE CKDSNOCV */ 01398000
*/*          SEGMENT).                                               */ 01399000
*/*       THE 'ELIGBLSW' REMAINS ON UNTIL THE EXISTING ALLOCATION    */ 01400000
*/*       FAILS ONE OF THE TESTS. IT IS THEN TURNED OFF TO INDICATE  */ 01401000
*/*       THAT THE DSAB IS NOT ELIGIBLE AND THAT DSAB CHAIN          */ 01402000
*/*       PROCESSING MUST CONTINUE.                                  */ 01403000
*/*                                                                  */ 01404000
*/*   INPUT DATA  - ELIGBLSW - SET ON BY AN OUTER SEGMENT            */ 01405000
*/*                                                                  */ 01406000
*/*   OUTPUT DATA - ELIGBLSW - ON IF DSAB HAD ALL NECESSARY          */ 01407000
*/*                            REQUIREMENTS SO THAT IT CAN BE USED   */ 01408000
*/*                            TO SATISFY REQUEST.                   */ 01409000
*/*                                                                  */ 01410000
*/********************************************************************/ 01411000
*IF DSABNUSE=ON |                      /*DSAB IN USE                 */ 01412000
*  DSABOPCT^=0 |                       /*DATASET IS OPEN             */ 01413000
*  DSABCATM=ON THEN                    /*MEMBER OF A CONCAT GROUP    */ 01414000
         TM    DSABNUSE(DSABPTR),B'00000001'                    05S0282 01415000
         BO    @RT00282                                         05S0282 01416000
         LH    @10,DSABOPCT(,DSABPTR)                           05S0282 01417000
         LTR   @10,@10                                          05S0282 01418000
         BNZ   @RT00282                                         05S0282 01419000
         TM    DSABCATM(DSABPTR),B'00000010'                    05S0282 01420000
         BNO   @RF00282                                         05S0282 01421000
@RT00282 DS    0H                                               05S0283 01422000
*    ELIGBLSW=OFF;                     /*DSAB NOT ELIG TO SATISFY REQ*/ 01423000
*                                                               05S0283 01424000
         NI    ELIGBLSW,B'01111111'                             05S0283 01425000
*IF ELIGBLSW=ON THEN                   /*REQUEST ELIGIBLE SO FAR     */ 01426000
@RF00282 TM    ELIGBLSW,B'10000000'                             05S0284 01427000
         BNO   @RF00284                                         05S0284 01428000
*  IF STATSTUP->TEXTUPAR(ONE) = MOD &  /*STATUS MOD SPECIFIED IN REQ */ 01429000
*    DSABPALC = OFF &                  /*EXIST ALOC NOT PERM ALLOC   */ 01430000
*    SIOTDLET = ON THEN                /*EXIST ALOC HAS DISP=DELETE  */ 01431000
         L     @10,STATSTUP(,KEYTABP)                           05S0285 01432000
         CLI   TEXTUPAR(@10),X'02'                              05S0285 01433000
         BNE   @RF00285                                         05S0285 01434000
         TM    DSABPALC(DSABPTR),B'01000000'                    05S0285 01435000
         BNZ   @RF00285                                         05S0285 01436000
         L     @10,DSABSIOT(,DSABPTR)                           05S0285 01437000
         TM    SIOTDLET(@10),B'00000100'                        05S0285 01438000
         BNO   @RF00285                                         05S0285 01439000
*      ELIGBLSW = OFF;                 /*DSAB NOT ELIG TO SATISFY REQ*/ 01440000
*                                                               05S0286 01441000
         NI    ELIGBLSW,B'01111111'                             05S0286 01442000
*IF DSABCONV=OFF & ELIGBLSW=ON THEN    /*ELG DSAB W/O CONVERT ATTRIB.*/ 01443000
@RF00285 DS    0H                                               05S0287 01444000
@RF00284 TM    DSABCONV(DSABPTR),B'00010000'                    05S0287 01445000
         BNZ   @RF00287                                         05S0287 01446000
         TM    ELIGBLSW,B'10000000'                             05S0287 01447000
         BNO   @RF00287                                         05S0287 01448000
*  DO;                                                          05S0288 01449000
*/********************************************************************/ 01450000
*/*                                                                  */ 01451000
*/* SEGMENT NAME - CKDSNOCV                                          */ 01452000
*/*                                                                  */ 01453000
*/* DESCRIPTIVE NAME - CHECK NON-CONVERTIBLE ALLOCATIONS             */ 01454000
*/*                                                                  */ 01455000
*/* FUNCTION - EXAMINE THE EXISTING ALLOCATION (WHICH DOES NOT HAVE  */ 01456000
*/*            THE CONVERTIBLE ATTRUBUTE) TO SEE IF IT FULFILLS      */ 01457000
*/*            REQUIREMENTS WHICH WOULD STILL ALLOW IT               */ 01458000
*/*            TO BE CONVERTED TO SATISFY THE REQUEST.               */ 01459000
*/*                                                                  */ 01460000
*/*   OPERATION - THE FOLLOWING CONDITIONS ARE CHECKED. ALL          */ 01461000
*/*       OF THEM MUST HOLD IF THE EXISTING ALLOCATION IS TO BE USED */ 01462000
*/*       TO SATISFY THE REQUEST, EXCEPT THAT ONLY THE FIRST         */ 01463000
*/*       CONDITION MUST HOLD FOR AN &DSNAME.                        */ 01464000
*/*       1) THE REQUEST DOES NOT SPECIFY A DDNAME OR THE SPECIFIED  */ 01465000
*/*          DDNAME MATCHES THE DDNAME ASSOCIATED WITH THE EXISTING  */ 01466000
*/*          ALLOCATION. (A TERMINAL REQUEST WHICH DOES NOT SPECIFY  */ 01467000
*/*          A DDNAME CAN NOT BE SATISFIED BY A NON-CONVERTIBLE      */ 01468000
*/*          EXISTING ALLOCATION BECAUSE OF POSSIBLE JFCB-DCB PARM   */ 01469000
*/*          CONFLICTS).                                             */ 01470000
*/*       2) THE USER HAS NOT REQUESTED THAT THE CONVERTIBLE         */ 01471000
*/*          ATTRIBUTE BE ASSIGNED TO THE RESULTANT ALLOCATION.      */ 01472000
*/*       3) THE USER HAS NOT SPECIFIED IN THE REQUEST THAT ONLY     */ 01473000
*/*          CONVERTIBLE ALLOCATIONS MAY BE USED TO SATISFY          */ 01474000
*/*          THE REQUEST.                                            */ 01475000
*/*       4) THE MEMBER NAME SPECIFIED IN THE REQUEST IS ASSOCIATED  */ 01476000
*/*          WITH THE EXISTING ALLOCATION OR A MEMBER NAME IS NOT    */ 01477000
*/*          SPECIFIED IN THE REQUEST AND NO MEMBER NAME IS ASSOCI-  */ 01478000
*/*          ATED WITH THE EXISTING ALLOCATION.                      */ 01479000
*/*       5) DCB PARAMETERS, INPUT ONLY, OR OUTPUT ONLY ARE NOT      */ 01480000
*/*          SPECIFIED IN THE REQUEST.                               */ 01481000
*/*       6) A STATUS OF MOD IS (IS NOT) SPECIFIED IN THE REQUEST    */ 01482000
*/*          AND IS (IS NOT) ASSOCIATED WITH THE EXISTING ALLOCATION.*/ 01483000
*/*                                                                  */ 01484000
*/*       THE 'ELIGBLSW' REMAINS ON UNTIL THE EXISTING ALLOCATION    */ 01485000
*/*       FAILS ONE OF THE TESTS. IT IS THEN TURNED OFF TO INDICATE  */ 01486000
*/*       THAT THE DSAB IS NOT ELIGIBLE AND THAT DSAB CHAIN PROCESS- */ 01487000
*/*       ING MUST CONTINUE.                                         */ 01488000
*/*                                                                  */ 01489000
*/*   INPUT  - ELIBLSW - SET ON BY AN OUTER SEGMENT                  */ 01490000
*/*                                                                  */ 01491000
*/*   OUTPUT - ELIGBLSW - ON IF DSAB HAD ALL REQUIREMENTS SO THAT IT */ 01492000
*/*                       BE USED TO SATISFY THE REQUEST. OTHERWISE, */ 01493000
*/*                       TURNED OFF.                                */ 01494000
*/*                                                                  */ 01495000
*/********************************************************************/ 01496000
*IF DDNREQSW=ON                        /*DDNAME REQUESTED            */ 01497000
*   & SAMDSASW=OFF THEN                /*NOT SAME AS DDNAME IN ALLOC */ 01498000
         TM    DDNREQSW,B'01000000'                             06S0289 01499000
         BNO   @RF00289                                         06S0289 01500000
         TM    SAMDSASW,B'00010000'                             06S0289 01501000
         BNZ   @RF00289                                         06S0289 01502000
*  ELIGBLSW=OFF;                       /*NO, THEN DSAB NOT ELIGIBLE  */ 01503000
*                                                               06S0290 01504000
         NI    ELIGBLSW,B'01111111'                             06S0290 01505000
*IF DDNREQSW=OFF &                     /*TERM REQ WITH NO DDN SPECIFD*/ 01506000
*  DSNTRMSW=ON THEN                                             06S0291 01507000
@RF00289 TM    DSNTRMSW,B'00000010'                             06S0291 01508000
         BNO   @RF00291                                         06S0291 01509000
         TM    DDNREQSW,B'01000000'                             06S0291 01510000
         BNZ   @RF00291                                         06S0291 01511000
*  ELIGBLSW=OFF;                       /*CAN'T BE SATISFIED BY EXISTING 01512000
*                                        ALLOC W/O CONVERTIBLE ATTRIB*/ 01513000
         NI    ELIGBLSW,B'01111111'                             06S0292 01514000
*IF DSNREQ(ONE)^=AMPSAND THEN          /*REQ NOT &NAME MUST ALSO        01515000
*                                        SATISFY THE FOLLOWING  06S0293 01516000
*                                        CONDITIONS                  */ 01517000
@RF00291 CLI   DSNREQ,C'&&'                                     06S0293 01518000
         BE    @RF00293                                         06S0293 01519000
*  DO;                                                          06S0294 01520000
*    IF ONCNVTSW=ON                    /*CONVERT ONLY CONVERTIBLE'S     01521000
*                                        REQUESTED IN S99RB FLAGS    */ 01522000
*      | CNVRTTUP^=0 THEN              /*OR CONVERT. ATTRIBUTE REQ'D    01523000
*                                        TO BE ASSIGNED              */ 01524000
         L     @10,@PC00001+12                                  06S0295 01525000
         TM    ONCNVTSW(@10),B'10000000'                        06S0295 01526000
         BO    @RT00295                                         06S0295 01527000
         L     @10,CNVRTTUP(,KEYTABP)                           06S0295 01528000
         LTR   @10,@10                                          06S0295 01529000
         BZ    @RF00295                                         06S0295 01530000
@RT00295 DS    0H                                               06S0296 01531000
*      ELIGBLSW=OFF;                   /*YES, DSAB NOT ELIGIBLE      */ 01532000
         NI    ELIGBLSW,B'01111111'                             06S0296 01533000
*    ELSE                              /*STILL ELG TO SATISFY REQ    */ 01534000
*      DO;                                                      06S0297 01535000
         B     @RC00295                                         06S0297 01536000
@RF00295 DS    0H                                               06S0298 01537000
*        IF MEMBRTUP^=ZERO THEN        /*MEMBER NAME IN REQ          */ 01538000
         L     @10,MEMBRTUP(,KEYTABP)                           06S0298 01539000
         LTR   @10,@10                                          06S0298 01540000
         BZ    @RF00298                                         06S0298 01541000
*          DO;                                                  06S0299 01542000
*            RFY TEXTUNIT BASED(MEMBRTUP); /*POINT TO TEXT UNIT      */ 01543000
*            HOLD8BYT=TEXTUPAR(ONE:TEXTULNG); /*COPY NAME FOR COMPARE*/ 01544000
         MVI   HOLD8BYT+1,C' '                                  06S0301 01545000
         MVC   HOLD8BYT+2(6),HOLD8BYT+1                         06S0301 01546000
         ICM   @08,12,TEXTULNG(@10)                             06S0301 01547000
         SRA   @08,16                                           06S0301 01548000
         BCTR  @08,0                                            06S0301 01549000
         EX    @08,@SM02720                                     06S0301 01550000
*          END;                                                 06S0302 01551000
*        ELSE                          /*NO MEMBER NAME SPECFD IN REQ*/ 01552000
*          HOLD8BYT='';                /*COMP DONE TO BLANK ENTRY    */ 01553000
*                                                               06S0303 01554000
         B     @RC00298                                         06S0303 01555000
@RF00298 MVI   HOLD8BYT,C' '                                    06S0303 01556000
         MVC   HOLD8BYT+1(7),HOLD8BYT                           06S0303 01557000
*        IF HOLD8BYT^=JFCBELNM THEN    /*MEMBER NAME WITH EXIST ALLOC*/ 01558000
@RC00298 L     @10,DSABSIOT(,DSABPTR)                           06S0304 01559000
         L     @10,SJFCBPTR(,@10)                               06S0304 01560000
         CLC   HOLD8BYT(8),JFCBELNM(@10)                        06S0304 01561000
         BE    @RF00304                                         06S0304 01562000
*          ELIGBLSW=OFF;               /*NOT EQUAL, DSAB NOT ELIGIBLE*/ 01563000
*                                                               06S0305 01564000
         NI    ELIGBLSW,B'01111111'                             06S0305 01565000
*        IF DCBSPCSW=ON |              /*DCB PARMS SPECIFIED IN REQ  */ 01566000
*          INOUTTUP^=ZERO THEN         /*INPUT, OUTPUT ONLY IN REQ   */ 01567000
@RF00304 L     @10,@PC00001+12                                  06S0306 01568000
         TM    DCBSPCSW(@10),B'00000100'                        06S0306 01569000
         BO    @RT00306                                         06S0306 01570000
         L     @10,INOUTTUP(,KEYTABP)                           06S0306 01571000
         LTR   @10,@10                                          06S0306 01572000
         BZ    @RF00306                                         06S0306 01573000
@RT00306 DS    0H                                               06S0307 01574000
*          ELIGBLSW=OFF;               /*YES, DSAB NOT ELIGIBLE      */ 01575000
*                                                               06S0307 01576000
         NI    ELIGBLSW,B'01111111'                             06S0307 01577000
*        IF STATSTUP->TEXTUPAR(1)=MOD THEN /*MOD SPECIFIED IN REQ    */ 01578000
@RF00306 L     @10,STATSTUP(,KEYTABP)                           06S0308 01579000
         CLI   TEXTUPAR(@10),X'02'                              06S0308 01580000
         BNE   @RF00308                                         06S0308 01581000
*          IF JFCDISP^='10'B           /*BUT EXISTING ALLOC NOT MOD  */ 01582000
*            & JFCMODNW^=ON THEN                                06S0309 01583000
         L     @10,DSABSIOT(,DSABPTR)                           06S0309 01584000
         L     @10,SJFCBPTR(,@10)                               06S0309 01585000
         TM    JFCDISP(@10),B'10000000'                         06S0309 01586000
         BNO   @GL00136                                         06S0309 01587000
         TM    JFCDISP(@10),B'01000000'                         06S0309 01588000
         BZ    @RF00309                                         06S0309 01589000
@GL00136 L     @10,DSABSIOT(,DSABPTR)                           06S0309 01590000
         L     @10,SJFCBPTR(,@10)                               06S0309 01591000
         TM    JFCMODNW(@10),B'00010000'                        06S0309 01592000
         BO    @RF00309                                         06S0309 01593000
*            ELIGBLSW=OFF;             /*YES, DSAB NOT ELIGIBLE      */ 01594000
         NI    ELIGBLSW,B'01111111'                             06S0310 01595000
