         BR    @14                                              01S0401 01279000
*CHARSCAN: PROC;                                                02S0402 01280000
CHARSCAN STM   @14,@12,@SA00002                                 02S0402 01281000
*/********************************************************************/ 01282000
*/*                                                                  */ 01283000
*/* SEGMENT NAME - CHARSCAN                                          */ 01284000
*/*                                                                  */ 01285000
*/* DESCRIPTIVE NAME - SUBROUTINE SCAN OF CHARACTER PARM VALUES      */ 01286000
*/*                                                                  */ 01287000
*/* FUNCTION - CALLED BY THE CHARCHK SEGMENT TO SCAN THE PARM        */ 01288000
*/*            IN A TEXT UNIT FOR INVALID CHARACTERS                 */ 01289000
*/*                                                                  */ 01290000
*/* OPERATION  -  IF THE FIRST CHARACTER OF THE TEXT UNIT PARM MUST  */ 01291000
*/*               BE ALPHANATIONAL, A CHECK IS MADE TO ENSURE THIS   */ 01292000
*/*               CHARACTER IS NOT NUMERIC, THE ALPHACHK BIT IS      */ 01293000
*/*               TURNED OFF, AND THE REMAINING NECESSARY CHECKS     */ 01294000
*/*               FOLLOW.                                            */ 01295000
*/*                                                                  */ 01296000
*/*               THIS SEGMENT USES THE BAL TRT INSTRUCTION TO SCAN  */ 01297000
*/*               THE PARM FOR THE LENGTH SPECIFIED IN THE TEXT UNIT.*/ 01298000
*/*               CHARMASK(A 256 BYTE MASK WITH ZEROS IN THE         */ 01299000
*/*               POSITIONS CORRESPONDING TO @, #, $, A-Z, 0-9, AND  */ 01300000
*/*               THE CORRESPONDING HEX VALUES IN ALL THE OTHER      */ 01301000
*/*               POSITIONS) IS USED IN THE TRT TO DETERMINE VALID   */ 01302000
*/*               CHARACTERS. (NOTE: FF IS IN THE POSITION           */ 01303000
*/*               CORRESPONDING TO '00'X TO PREVENT THIS FROM BEING  */ 01304000
*/*               TAKEN AS A VALID VALUE) IF REGISTER 1 IS ZERO AFTER*/ 01305000
*/*               THE TRT, ALL CHARACTERS ARE VALID. OTHERWISE REG. 1*/ 01306000
*/*               HAS THE ADDRESS OF THE INVALID CHARACTER, AND REG.2*/ 01307000
*/*               THE CHARACTER ITSELF AS FOUND IN CHARMASK. SPECIAL */ 01308000
*/*               CHECKS ARE THEN MADE AS FOLLOWS TO SEE IF THE      */ 01309000
*/*               INVALID CHARACTER IS ALLOWABLE                     */ 01310000
*/*                                                                  */ 01311000
*/*                 - A BLANK IS ALLOWED AS LONG AS THE BLANK IS NOT */ 01312000
*/*                   IN THE FIRST POSITION, DOES NOT FOLLOW A PERIOD*/ 01313000
*/*                   IN DSN, AND ALL REMAINING CHARACTERS ARE BLANK.*/ 01314000
*/*                                                                  */ 01315000
*/*                 - HYPHENS ARE ALLOWED IN VOLUME SERIALS          */ 01316000
*/*                                                                  */ 01317000
*/*                 - HYPHENS AND 12-0 PUNCHES ARE ALLOWED IN DSN,   */ 01318000
*/*                   AS LONG AS NOT IN THE FIRST POSITION, OR       */ 01319000
*/*                   FOLLOWING A PERIOD.                            */ 01320000
*/*                                                                  */ 01321000
*/*                 - PERIOD IS ALLOWED IN DSN AS LONG AS NOT IN     */ 01322000
*/*                   FIRST OR LAST POSITION, THERE ARE 1-8          */ 01323000
*/*                   CHARACTERS BETWEEN PERIODS, THE DSN IS NOT     */ 01324000
*/*                   TEMPORARY, AND THE CHARACTER FOLLOWING THE     */ 01325000
*/*                   PERIOD IS ALPHANATIONAL.                       */ 01326000
*/*                                                                  */ 01327000
*/*               IF THE CHARACTER IS TO BE ALLOWED, THE TRT PROCESS */ 01328000
*/*               IS THEN REPEATED FOR THE REMAINING CHARACTERS IN   */ 01329000
*/*               THE PARM UNTIL ALL HAVE BEEN LOOKED AT OR AN ERROR */ 01330000
*/*               HAS BEEN DETERMINED. A FINAL CHECK IS MADE TO      */ 01331000
*/*               ENSURE THAT NO MORE THAN EIGHT CHARACTERS WERE     */ 01332000
*/*               SPECIFIED IN A DSN WITHOUT A PERIOD. IF AT ANY     */ 01333000
*/*               TIME AN ERROR IS DETERMINED IN THE PROCESS THE     */ 01334000
*/*               ERRORSW BIT IS SET ON AND NO FURTHER VALIDITY      */ 01335000
*/*               CHECKING IS DONE.                                  */ 01336000
*/*                                                                  */ 01337000
*/* ENTRY -                                                          */ 01338000
*/*                                                                  */ 01339000
*/*   INPUT DATA - UNITPTR  - PTR TO TEXTUNIT WHOSE PARM             */ 01340000
*/*                           IS TO BE SCANNED                       */ 01341000
*/*                ALPHACHK - BIT SET ON IF FIRST CHARACTER          */ 01342000
*/*                           MUST BE ALPHANATIONAL                  */ 01343000
*/*                AMPDSN   - BIT SET ON IF PARM BEING SCANNED       */ 01344000
*/*                           IS AN &DSNAME                          */ 01345000
*/*                ERRORSW  - BIT SET ON IF ERROR IS FOUND           */ 01346000
*/*                           (ZERO ON ENTRY)                        */ 01347000
*/*                                                                  */ 01348000
*/* EXIT -                                                           */ 01349000
*/*                                                                  */ 01350000
*/*   OUTPUT DATA - ERRORSW  - TURNED ON IF ERROR IS FOUND           */ 01351000
*/*                 ALPHACHK - TURNED OFF IF IT WAS SET ON ENTRY     */ 01352000
*/*                            AND THE PARM IS VALID                 */ 01353000
*/*                                                                  */ 01354000
*/********************************************************************/ 01355000
*RFY (TRTSTART,TRTLEN,INVALPTR,INVALCHR) RESTRICTED;            02S0403 01356000
*                                      /*RESTRICT WORK REGISTERS     */ 01357000
*RFY TEXTUNIT BASED(UNITPTR);          /*RESET TEXT UNIT MAPPING BASE*/ 01358000
*DSNCNTR=ADDR(TEXTUPAR);               /*INIT CNTR FOR DSN KEY       */ 01359000
         LA    @04,TEXTUPAR(,UNITPTR)                           02S0405 01360000
         LR    DSNCNTR,@04                                      02S0405 01361000
*BLNKADDR=0;                           /*INIT ADDR OF 1ST PARM BLANK */ 01362000
         SLR   BLNKADDR,BLNKADDR                                02S0406 01363000
*TRTSTART=ADDR(TEXTUPAR);              /*POINT TO PARM FOR TRT       */ 01364000
         LR    TRTSTART,@04                                     02S0407 01365000
*TRTLEN=TEXTULNG-1;                    /*LENGTH LESS 1 FOR EXECUTE   */ 01366000
         ICM   TRTLEN,12,TEXTULNG(UNITPTR)                      02S0408 01367000
         SRA   TRTLEN,16                                        02S0408 01368000
         BCTR  TRTLEN,0                                         02S0408 01369000
*DO WHILE (ERRORSW='0'B & TRTLEN>=0);  /*SCAN TILL ERROR OR PARM END */ 01370000
         B     @DE00409                                         02S0409 01371000
@DL00409 DS    0H                                               02S0410 01372000
*  IF ALPHACHK='1'B THEN               /*MUST 1ST CHAR BE ALPHANAT'L.*/ 01373000
         TM    ALPHACHK,B'01000000'                             02S0410 01374000
         BNO   @RF00410                                         02S0410 01375000
*    IF TRTSTART->ALPCKCHR > 'Z' THEN  /*YES,IS CHAR > EBCDIC 'Z'?   */ 01376000
         CLI   ALPCKCHR(TRTSTART),C'Z'                          02S0411 01377000
         BNH   @RF00411                                         02S0411 01378000
*      ERRORSW='1'B;                   /*YES,THEN ERROR              */ 01379000
         OI    ERRORSW,B'10000000'                              02S0412 01380000
*    ELSE                              /*NO,THEN CHAR IS NOT NUMERIC */ 01381000
*      ALPHACHK='0'B;                  /*TURN OFF ALPHANAT'L CHECK   */ 01382000
         B     @RC00411                                         02S0413 01383000
@RF00411 NI    ALPHACHK,B'10111111'                             02S0413 01384000
*  ELSE;                               /*ALPHANAT'L. CHECK NOT NEEDED*/ 01385000
*                                                               02S0414 01386000
@RF00410 DS    0H                                               02S0415 01387000
*  IF ERRORSW='0'B THEN                /*HAS ERROR BEEN DETECTED     */ 01388000
@RC00410 TM    ERRORSW,B'10000000'                              02S0415 01389000
         BNZ   @RF00415                                         02S0415 01390000
*    DO;                               /*NO, CONTINUE CHECKS         */ 01391000
*      INVALPTR=0;                     /*INIT TRT RESULT REG TO 0    */ 01392000
         SLR   INVALPTR,INVALPTR                                02S0417 01393000
*      INVALCHR=0;                     /*INIT TRT RESULT REG TO 0    */ 01394000
         SLR   INVALCHR,INVALCHR                                02S0418 01395000
*      GEN (EX TRTLEN,CHARTST)         /*EXECUTE TRT FOR VALID CHARS */ 01396000
*          REFS(INVALPTR,INVALCHR,TRTSTART,TRTLEN)              02S0419 01397000
*          SETS(INVALPTR,INVALCHR);                             02S0419 01398000
         EX TRTLEN,CHARTST                                              01399000
*      IF INVALPTR ^= 0 THEN           /*WAS AN INVALID CHAR FOUND   */ 01400000
         LTR   INVALPTR,INVALPTR                                02S0420 01401000
         BZ    @RF00420                                         02S0420 01402000
*        DO;                           /*YES,MAKE EXCEPTION CHECKS   */ 01403000
*          BADCHAR=INVALCHR;           /*SAVE INVALID CHARACTER      */ 01404000
         STC   INVALCHR,BADCHAR                                 02S0422 01405000
*          PRMINDX=INVALPTR-ADDR(TEXTUPAR)+1;                   02S0423 01406000
         LA    @03,TEXTUPAR(,UNITPTR)                           02S0423 01407000
         LR    PRMINDX,INVALPTR                                 02S0423 01408000
         SLR   PRMINDX,@03                                      02S0423 01409000
         AL    PRMINDX,@CF00063                                 02S0423 01410000
*                                      /*FIND ITS INDEX WITHIN PARM  */ 01411000
*                                                               02S0424 01412000
*          IF BADCHAR=' ' THEN         /*IS IT A BLANK ?             */ 01413000
         CLI   BADCHAR,C' '                                     02S0424 01414000
         BNE   @RF00424                                         02S0424 01415000
*            IF (INVALPTR=ADDR(TEXTUPAR)  |  /*YES,IS IT 1ST IN PARM */ 01416000
*                TEXTUPAR(PRMINDX-1)='.') THEN/*OR AFTER A DSN PERIOD*/ 01417000
         CR    INVALPTR,@03                                     02S0425 01418000
         BE    @RT00425                                         02S0425 01419000
         LR    @03,UNITPTR                                      02S0425 01420000
         ALR   @03,PRMINDX                                      02S0425 01421000
         CLI   TEXTUPAR-2(@03),C'.'                             02S0425 01422000
         BNE   @RF00425                                         02S0425 01423000
@RT00425 DS    0H                                               02S0426 01424000
*              ERRORSW='1'B;           /*YES,THEN ERROR              */ 01425000
         OI    ERRORSW,B'10000000'                              02S0426 01426000
*            ELSE                      /*NO,THEN CONTINUE CHECKS     */ 01427000
*              DO;                     /*ENSURE REST OF PARM IS BLANK*/ 01428000
         B     @RC00425                                         02S0427 01429000
@RF00425 DS    0H                                               02S0428 01430000
*                BLNKADDR=INVALPTR;    /*SAVE ADDR OF 1ST BLANK      */ 01431000
         LR    BLNKADDR,INVALPTR                                02S0428 01432000
*                DO WHILE (PRMINDX<=TEXTULNG & /*DO UNTIL END OF PARM*/ 01433000
*                          ERRORSW='0'B);      /*OR NON-BLANK ERROR  */ 01434000
         B     @DE00429                                         02S0429 01435000
@DL00429 DS    0H                                               02S0430 01436000
