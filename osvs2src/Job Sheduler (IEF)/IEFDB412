         C     @15,SSREQTUP(,@01)                               01S0384 01239000
         BE    @RF00384                                         01S0384 01240000
*                    DO;               /*YES, CHECK AUTHORIZATION    */ 01241000
*                      GEN(TESTAUTH FCTN=1,STATE=YES,KEY=YES);  01S0386 01242000
         TESTAUTH FCTN=1,STATE=YES,KEY=YES                              01243000
*                                      /*CHECK FOR APF, SUP.STATE,      01244000
*                                        OR SYSTEM PROTECTION KEY    */ 01245000
*                      IF RETCODE^=0 THEN  /*IS USER AUTHORIZED?     */ 01246000
         LTR   RETCODE,RETCODE                                  01S0387 01247000
         BZ    @RF00387                                         01S0387 01248000
*                        ERRCODE = RCSUBSYS;  /*NO,SET ERROR CODE    */ 01249000
         L     @15,@PC00001                                     01S0388 01250000
         MVC   ERRCODE(2,@15),@CH02833                          01S0388 01251000
*                      ELSE;           /*USER IS AUTHORIZED          */ 01252000
@RF00387 DS    0H                                               01S0390 01253000
*                    END;              /*END IF SUBSYSTEM REQUEST    */ 01254000
*                  ELSE;               /*NOT A SUBSYSTEM REQUEST     */ 01255000
@RF00384 DS    0H                                               01S0392 01256000
*                ELSE;                 /*CHARCHK ERROR FOUND         */ 01257000
@RF00383 DS    0H                                               01S0393 01258000
*              END;                    /*END NO MAXVALCK ERROR       */ 01259000
*            ELSE;                     /*A PARM EXCEEDS MAX ALLOWED  */ 01260000
@RF00200 DS    0H                                               01S0395 01261000
*          END;                        /*END NO VALUECHK ERROR       */ 01262000
*        ELSE;                         /*A PARM IS NOT 1 OF N ALLOWED*/ 01263000
@RF00174 DS    0H                                               01S0397 01264000
*      END;                            /*END NO INCLUCHK ERROR       */ 01265000
*    ELSE;                             /*ELSE REQUIRED KEY MISSING   */ 01266000
@RF00144 DS    0H                                               01S0399 01267000
*  END;                                /*END NO SYNTAX CHECKER ERROR */ 01268000
*ELSE;                                 /*ERROR IN SYNTAX CHECKER     */ 01269000
@RF00094 DS    0H                                               01S0401 01270000
*RETURN CODE(0);                                                01S0401 01271000
         L     @13,4(,@13)                                      01S0401 01272000
         L     @00,@SIZDATD                                     01S0401 01273000
