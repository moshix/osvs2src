         TITLE 'IEFDB413 - NORMAL DYNAMIC ALLOCATION CONTROL           *00001000
                        '                                               00002000
IEFDB413 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(24)                                          01S0001 00007000
         DC    C'IEFDB413 78089  UZ17548 '                      01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  LA    @09,4095(,@12)                                   01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         USING @PSTART+4095,@09                                 01S0001 00014000
         L     @00,@SIZDATD                                     01S0001 00015000
         GSPACE   R,LV=(0)                                              00016000
         LR    @11,@01                                          01S0001 00017000
         USING @DATD,@11                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         LM    @00,@01,20(@13)                                  01S0001 00020000
         ST    @11,8(,@13)                                      01S0001 00021000
         LR    @13,@11                                          01S0001 00022000
         MVC   @PC00001(32),0(@01)                              01S0001 00023000
JSCBDISP EQU 188                                                        00024000
JSCBAOS1 EQU  72                                                        00025000
JSCBAOS2 EQU 192                                                        00026000
*/********************************************************************/ 00027000
*FSIOTPTR=ZEROPTR;                     /*CLEAR PTR TO FIRST SIOT     */ 00028000
         L     @08,@PC00001+24                                  01S0134 00029000
         SLR   @06,@06                                          01S0134 00030000
         ST    @06,FSIOTPTR(,@08)                               01S0134 00031000
*SSERRBLK = SSERRBLK && SSERRBLK;      /*CLEAR SUBSYSTEM ERROR  01S0135 00032000
*                                        BLOCK               @G29AN2F*/ 00033000
         XC    SSERRBLK(8),SSERRBLK                             01S0135 00034000
*CALL IEFDB414                         /*BUILD SIOT,JFCB,AND JFCBX'S */ 00035000
*              (SYSCTBLK,LSJXEPAP,KEYTABP,FSIOTPTR,ALFNCMAP,    01S0136 00036000
*               REASONCD,LASTEPAP);                             01S0136 00037000
         L     @06,@PC00001+4                                   01S0136 00038000
         ST    @06,@AL00001                                     01S0136 00039000
         LA    @06,LSJXEPAP(,@08)                               01S0136 00040000
         ST    @06,@AL00001+4                                   01S0136 00041000
         L     @06,@PC00001                                     01S0136 00042000
         ST    @06,@AL00001+8                                   01S0136 00043000
         LA    @08,FSIOTPTR(,@08)                               01S0136 00044000
         ST    @08,@AL00001+12                                  01S0136 00045000
         L     @08,@PC00001+20                                  01S0136 00046000
         ST    @08,@AL00001+16                                  01S0136 00047000
         L     @08,@PC00001+16                                  01S0136 00048000
         ST    @08,@AL00001+20                                  01S0136 00049000
         L     @08,@PC00001+8                                   01S0136 00050000
         ST    @08,@AL00001+24                                  01S0136 00051000
         L     @15,@CV00100                                     01S0136 00052000
         LA    @01,@AL00001                                     01S0136 00053000
         BALR  @14,@15                                          01S0136 00054000
*IF ERRCODE=ZERO                       /*SUCCESSFUL BUILD?           */ 00055000
*  & SIOTGDSN=OFF                      /*AND NOT A GENERATED DSNAME? */ 00056000
*  & SIOTSSDS=OFF                      /*AND NOT A SUBSYSTEM  D.S.?  */ 00057000
*  & SCTDUMMY=OFF                      /*AND NOT DUMMY (TERM,QNAME)? */ 00058000
*  & NORESSW=OFF THEN                  /*AND BYPASS DSN ENQ NOT SPEC?*/ 00059000
         L     @08,@PC00001+16                                  01S0137 00060000
         ICM   @06,3,ERRCODE(@08)                               01S0137 00061000
         BNZ   @RF00137                                         01S0137 00062000
         L     @08,@PC00001+24                                  01S0137 00063000
         L     @08,FSIOTPTR(,@08)                               01S0137 00064000
         TM    SIOTGDSN(@08),B'00010000'                        01S0137 00065000
         BNZ   @RF00137                                         01S0137 00066000
         TM    SIOTSSDS(@08),B'10000000'                        01S0137 00067000
         BNZ   @RF00137                                         01S0137 00068000
         TM    SCTDUMMY(@08),B'10000000'                        01S0137 00069000
         BNZ   @RF00137                                         01S0137 00070000
         L     @06,@PC00001+20                                  01S0137 00071000
         TM    NORESSW(@06),B'00100000'                         01S0137 00072000
         BNZ   @RF00137                                         01S0137 00073000
*  DO;                                 /*YES,RESERVE DATA SET VIA ENQ*/ 00074000
*/********************************************************************/ 00075000
*/*                                                                  */ 00076000
*/* SEGMENT NAME - DSRRINTF                                          */ 00077000
*/*                                                                  */ 00078000
*/* DESCRIPTIVE NAME - DATA SET RESERVATION/RELEASE ROUTINE INTERFACE*/ 00079000
*/*                                                                  */ 00080000
*/* FUNCTION -                                                       */ 00081000
*/*   RESERVE USE OF THE DATA SET TO INSURE DATA SET INTEGRITY, AND  */ 00082000
*/*   UPDATE THE DATA SET ENQUEUE TABLE.                             */ 00083000
*/*                                                                  */ 00084000
*/*   OPERATION -                                                    */ 00085000
*/*     THE INPUT TO THE DATA SET RESERVATION/RELEASE ROUTINE,       */ 00086000
*/*     IEFAB4DC, IS SET UP AS DESCRIBED BY THE CODE, AND NOTES      */ 00087000
*/*     SECTION BELOW. THE ROUTINE IS THEN INVOKED. IF THE ROUTINE   */ 00088000
*/*     INDICATES THAT THE JCT HAS BEEN UPDATED AND AN EPA FOR IT    */ 00089000
*/*     HAS BEEN CHAINED, AN INDICATOR IS SET SO THAT A DUPLICATE    */ 00090000
*/*     EPA WILL NOT BE CHAINED IF THE JCT IS UPDATED DURING         */ 00091000
*/*     SUBSEQUENT DYNAMIC ALLOCATION PROCESSING OF THE REQUEST.     */ 00092000
*/*                                                                  */ 00093000
*/* INPUT -                                                          */ 00094000
*/*   EPAJCTSW - IF ON, INDICATES THAT A JCT EPA HAS ALREADY BEEN    */ 00095000
*/*              CHAINED, AND THEREFORE, IEFAB4DC SHOULD NOT CHAIN   */ 00096000
*/*              AN EPA IF IT UPDATES THE JCT.                       */ 00097000
*/*                                                                  */ 00098000
*/* OUTPUT -                                                         */ 00099000
*/*   EPAJCTSW - SET ON IF A JCT EPA HAS BEEN CHAINED.               */ 00100000
*/*                                                                  */ 00101000
*/* NOTES -                                                          */ 00102000
*/*   * AN INDICATOR IS SET TO PREVENT IEFAB4DC FROM CHANGING THE    */ 00103000
*/*     USE ATTRIBUTE FROM SHARED TO EXCLUSIVE, IF IT DETERMINES     */ 00104000
*/*     THIS TO BE NECESSARY (THAT IS, THE USER CURRENTLY HAS SHARED */ 00105000
*/*     USAGE OF THE DATA SET BUT NOW REQUIRES EXCLUSIVE USE). THIS  */ 00106000
*/*     IS NECESSARY BECAUSE THERE IS NO WAY TO RETURN THE USE       */ 00107000
*/*     ATTRIBUTE TO SHARE IF AN ERROR OCCURS DURING SUBSEQUENT      */ 00108000
*/*     DYNAMIC ALLOCATION. AN INDICATOR (RETINDIC) IS SET TO HAVE   */ 00109000
*/*     IEFAB4DC SET ON AN INDICATOR IF THIS SITUATION OCCURS, AND   */ 00110000
*/*     THE CHANGE FROM SHARED TO EXCLUSIVE USAGE WILL BE DONE AT THE*/ 00111000
*/*     END OF DYNAMIC ALLOCATION PROCESSING (SEE ALWRAPUP SEGMENT). */ 00112000
*/*     THIS SAME INDICATOR CAUSES IEFAB4DC TO INDICATE TO DYNAMIC   */ 00113000
*/*     ALLOCATION THAT THE DATA SET SHOULD BE DEQUEUED              */ 00114000
*/*     (CASE WHEN THE USER DID NOT ALREADY HAVE USE OF THE DATA SET */ 00115000
*/*     BEFORE THIS DYNAMIC ALLOCATION REQUEST) IF AN ERROR          */ 00116000
*/*     OCCURS DURING SUBSEQUENT DYNAMIC ALLOCATION PROCESSING.      */ 00117000
*/*   * AN INDICATOR IS SET TO HAVE IEFAB4DC RETURN THE ADDRESS OF   */ 00118000
*/*     THE DSNAME IN THE DSENQ TABLE. THIS INFORMATION WILL BE      */ 00119000
*/*     RETURNED TO IEFAB4DC IF IT IS CALLED TO DEQ THE DATA SET (IF */ 00120000
*/*     AN ERROR OCCURS DURING LATER DYNAMIC ALLOCATION PROCESSING)  */ 00121000
*/*     OR TO CHANGE THE USE ATTRIBUTE FROM SHARED TO EXCLUSIVE.     */ 00122000
*/*                                                                  */ 00123000
*/********************************************************************/ 00124000
*DSRRFMAP=DSRRFMAP && DSRRFMAP;        /*CLEAR FUNCTION MAP          */ 00125000
         XC    DSRRFMAP(2),DSRRFMAP                             02S0139 00126000
*ADCHENQ=ON;                           /*ADD DSNQ TABLE ENTRY AND ENQ*/ 00127000
         OI    ADCHENQ,B'00100000'                              02S0140 00128000
*IF JFCSHARE=ON THEN                   /*STATUS = SHR?               */ 00129000
         L     @08,SJFCBPTR(,@08)                               02S0141 00130000
         TM    JFCSHARE(@08),B'00001000'                        02S0141 00131000
         BNO   @RF00141                                         02S0141 00132000
*  ENQSHR=ON;                          /*YES,RESERVE D.S. AS SHR     */ 00133000
         OI    ENQSHR,B'00000010'                               02S0142 00134000
*ELSE                                  /*OR, IF                      */ 00135000
*  DO;                                                          02S0143 00136000
         B     @RC00141                                         02S0143 00137000
@RF00141 DS    0H                                               02S0144 00138000
*    ENQEXCL=ON;                       /*NO,RESERVE D.S. AS EXCLUSIVE*/ 00139000
*    NOSHRXCL=ON;                      /*DON'T DO SHR TO EXCL ENQ    */ 00140000
         OI    ENQEXCL,B'00000101'                              02S0145 00141000
*  END;                                                         02S0146 00142000
*IF WTDSNSW=ON THEN                    /*AUTH USER REQ WAIT FOR D.S.?*/ 00143000
@RC00141 L     @08,@PC00001+20                                  02S0147 00144000
         TM    WTDSNSW(@08),B'01000000'                         02S0147 00145000
         BNO   @RF00147                                         02S0147 00146000
*  DO;                                 /*YES, DO UNCONDITIONAL ENQ   */ 00147000
*    ENQWAIT=ON;                       /*USE UNCONDITIONAL ENQ       */ 00148000
         OI    ENQWAIT,B'10000000'                              02S0149 00149000
*    DSRRMSGP=TCBTIO;                  /*ADDR OF JOBNAME AND STEPNAME*/ 00150000
         L     @08,@PC00001+4                                   02S0150 00151000
         L     @08,TCBPTR(,@08)                                 02S0150 00152000
         L     @08,TCBTIO(,@08)                                 02S0150 00153000
         ST    @08,DSRRMSGP                                     02S0150 00154000
*  END;                                /*END UNCONDITIONAL ENQ       */ 00155000
*ELSE                                  /*DO CONDITIONAL ENQ          */ 00156000
*  DSRRMSGP=ZEROPTR;                   /*MSG PARMS NOT NEEDED        */ 00157000
         B     @RC00147                                         02S0152 00158000
@RF00147 SLR   @08,@08                                          02S0152 00159000
         ST    @08,DSRRMSGP                                     02S0152 00160000
*IF EPAJCTSW=ON THEN                   /*EPA FOR JCT ON CHAIN        */ 00161000
@RC00147 L     @08,@PC00001+20                                  02S0153 00162000
         TM    EPAJCTSW(@08),B'00000001'                        02S0153 00163000
         BNO   @RF00153                                         02S0153 00164000
*  EPAJCTEX=ON;                        /*YES, INDICATE THAT IN DSRR     00165000
*                                        INPUT FUNCTION MAP          */ 00166000
         OI    EPAJCTEX,B'00000100'                             02S0154 00167000
*RETADDR=ON;                           /*RETURN ADDRESS OF DSNQ ENTRY   00168000
*                                        AND DSNQ RECORD (IN NQRETPS)*/ 00169000
@RF00153 DS    0H                                               02S0156 00170000
*RETINDIC=ON;                          /*RETURN RESULTS INDICATORS   */ 00171000
         OI    RETADDR,B'01100000'                              02S0156 00172000
*NQRETPS = NQRETPS && NQRETPS;         /*CLEAR RETURN AREA           */ 00173000
         L     @08,@PC00001+24                                  02S0157 00174000
         XC    NQRETPS(8,@08),NQRETPS(@08)                      02S0157 00175000
*IF JSCBTCBP ^= ZEROPTR THEN           /*IF INIT. TCB IS IN JSCB        00176000
*                                                            @YM03580*/ 00177000
         L     @08,@PC00001+4                                   02S0158 00178000
         L     @08,JSCBPTR(,@08)                                02S0158 00179000
         L     @08,JSCBTCBP(,@08)                               02S0158 00180000
         LTR   @08,@08                                          02S0158 00181000
         BZ    @RF00158                                         02S0158 00182000
*  ENQTCBP = JSCBTCBP;                 /*USE IT FOR DIRECTED ENQ        00183000
*                                                            @YM03580*/ 00184000
         ST    @08,ENQTCBP                                      02S0159 00185000
*ELSE                                  /*INITIATOR'S TCB FIELD IS       00186000
*                                        ZERO                @YM03580*/ 00187000
*  ENQTCBP = TCBPTR;                   /*USE CURRENT TCB FOR ENQ        00188000
*                                                            @YM03580*/ 00189000
         B     @RC00158                                         02S0160 00190000
@RF00158 L     @08,@PC00001+4                                   02S0160 00191000
         L     @08,TCBPTR(,@08)                                 02S0160 00192000
         ST    @08,ENQTCBP                                      02S0160 00193000
*CALL IEFAB4DC                         /*DSENQ RTN           @YM03580*/ 00194000
*              (DSRRFMAP,JCTPTR,DSNAMTUP->TEXTULNG,LASTEPAP,    02S0161 00195000
*  ENQTCBP,SCTSNUMB,NQRETPS,ALFNCMAP,REASONCD,0,DSRRMSGP);      02S0161 00196000
*                                                               02S0161 00197000
@RC00158 LA    @08,DSRRFMAP                                     02S0161 00198000
         ST    @08,@AL00001                                     02S0161 00199000
         L     @08,@PC00001+4                                   02S0161 00200000
         LA    @06,JCTPTR(,@08)                                 02S0161 00201000
         ST    @06,@AL00001+4                                   02S0161 00202000
         L     @06,@PC00001                                     02S0161 00203000
         L     @06,KEYTABP(,@06)                                02S0161 00204000
         L     @06,DSNAMTUP(,@06)                               02S0161 00205000
         LA    @06,TEXTULNG(,@06)                               02S0161 00206000
         ST    @06,@AL00001+8                                   02S0161 00207000
         L     @06,@PC00001+8                                   02S0161 00208000
         ST    @06,@AL00001+12                                  02S0161 00209000
         LA    @06,ENQTCBP                                      02S0161 00210000
         ST    @06,@AL00001+16                                  02S0161 00211000
         L     @08,SCTPTR(,@08)                                 02S0161 00212000
         LA    @08,SCTSNUMB(,@08)                               02S0161 00213000
         ST    @08,@AL00001+20                                  02S0161 00214000
         L     @08,@PC00001+24                                  02S0161 00215000
         LA    @08,NQRETPS(,@08)                                02S0161 00216000
         ST    @08,@AL00001+24                                  02S0161 00217000
         L     @08,@PC00001+20                                  02S0161 00218000
         ST    @08,@AL00001+28                                  02S0161 00219000
         L     @08,@PC00001+16                                  02S0161 00220000
         ST    @08,@AL00001+32                                  02S0161 00221000
         LA    @08,@CF00125                                     02S0161 00222000
         ST    @08,@AL00001+36                                  02S0161 00223000
         LA    @08,DSRRMSGP                                     02S0161 00224000
         ST    @08,@AL00001+40                                  02S0161 00225000
         L     @15,@CV00098                                     02S0161 00226000
         LA    @01,@AL00001                                     02S0161 00227000
         BALR  @14,@15                                          02S0161 00228000
*IF EPAJCTEX=ON THEN                   /*EPA FOR JCT ON CHAIN OR PUT    00229000
*                                        ON CHAIN BY IEFAB4DC        */ 00230000
         TM    EPAJCTEX,B'00000100'                             02S0162 00231000
         BNO   @RF00162                                         02S0162 00232000
*  EPAJCTSW=ON;                        /*YES, INDICATE THAT IN THE      00233000
*                                        ALLOCATION FUNCTION MAP     */ 00234000
         L     @08,@PC00001+20                                  02S0163 00235000
