         TITLE 'IEFDB414 - BUILD TABLES                                *00001000
                        '                                               00002000
IEFDB414 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(24)                                          01S0001 00007000
         DC    C'IEFDB414 78089  UZ17548 '                      01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @12,0                                            01S0001 00011000
@PSTART  LA    @09,4095(,@12)                                   01S0001 00012000
         USING @PSTART,@12                                      01S0001 00013000
         USING @PSTART+4095,@09                                 01S0001 00014000
         L     @00,@SIZDATD                                     01S0001 00015000
         GSPACE   R,LV=(0)                                              00016000
         LR    @11,@01                                          01S0001 00017000
         USING @DATD,@11                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         LM    @00,@01,20(@13)                                  01S0001 00020000
         ST    @11,8(,@13)                                      01S0001 00021000
         LR    @13,@11                                          01S0001 00022000
         MVC   @PC00001(28),0(@01)                              01S0001 00023000
JSCBDISP EQU 188                                                        00024000
JSCBAOS1 EQU  72                                                        00025000
JSCBAOS2 EQU 192                                                        00026000
*                                       USE                          */ 00027000
*                                                               01S0084 00028000
*IF JSCDDNUM<MAXDD THEN                /*NUMBER ALLOC LESS THAN LIMIT*/ 00029000
         L     @10,@PC00001                                     01S0085 00030000
         L     @10,JSCBPTR(,@10)                                01S0085 00031000
         LH    @10,JSCDDNUM(,@10)                               01S0085 00032000
         C     @10,@CF00092                                     01S0085 00033000
         BNL   @RF00085                                         01S0085 00034000
*  DO;                                 /*YES,SET UP FOR RECORD ASSIGN*/ 00035000
*    SWACTLS=SWACTLS && SWACTLS;       /*CLEAR SWA INTERFACE CONTROLS*/ 00036000
         LH    @10,SWACTLS                                      01S0087 00037000
         N     @10,@CF02505                                     01S0087 00038000
         LR    @15,@10                                          01S0087 00039000
         XR    @15,@10                                          01S0087 00040000
         STH   @15,SWACTLS                                      01S0087 00041000
*    SWASN=ON;                         /*INDICATE ASSIGN FUNCTION    */ 00042000
*    SWNEW=ON;                         /*INDICATE TO CREATE EPA'S    */ 00043000
         OI    SWASN,B'10001000'                                01S0089 00044000
*    SWCHN=ON;                         /*INDICATE TO CHAIN EPA'S     */ 00045000
         OI    SWCHN,B'10000000'                                01S0090 00046000
*    NUMRCORD=2;                       /*INDICATE TWO RECORDS        */ 00047000
         MVC   NUMRCORD(4),@CF00058                             01S0091 00048000
*    SMIID(SIOTPOS)=3;                 /*SET SIOT SWA RECORD ID      */ 00049000
         MVI   SMIID,X'03'                                      01S0092 00050000
*    SMIID(JFCBPOS)=28;                /*SET JFCB SWA RECORD ID      */ 00051000
         MVI   SMIID+4,X'1C'                                    01S0093 00052000
*    CALL IEFAB4F7 (SWACTLS,0,LSJXEPAP,ADDR(SMISVAID),          01S0094 00053000
*                   ADDR(SMISVAB),RCORDLEN,NUMRCORD); /*SWA INTERFACE   00054000
*                                        RTN. TO ASSIGN SIOT AND JFCB*/ 00055000
         LA    @10,SWACTLS                                      01S0094 00056000
         ST    @10,@AL00001                                     01S0094 00057000
         LA    @10,@CF00090                                     01S0094 00058000
         ST    @10,@AL00001+4                                   01S0094 00059000
         L     @10,@PC00001+4                                   01S0094 00060000
         ST    @10,@AL00001+8                                   01S0094 00061000
         LA    @10,SMISVAID                                     01S0094 00062000
         ST    @10,@TF00001                                     01S0094 00063000
         LA    @10,@TF00001                                     01S0094 00064000
         ST    @10,@AL00001+12                                  01S0094 00065000
         LA    @10,SMISVAB                                      01S0094 00066000
         ST    @10,@TF00002                                     01S0094 00067000
         LA    @10,@TF00002                                     01S0094 00068000
         ST    @10,@AL00001+16                                  01S0094 00069000
         LA    @10,@CF00102                                     01S0094 00070000
         ST    @10,@AL00001+20                                  01S0094 00071000
         LA    @10,NUMRCORD                                     01S0094 00072000
         ST    @10,@AL00001+24                                  01S0094 00073000
         L     @15,@CV00086                                     01S0094 00074000
         LA    @01,@AL00001                                     01S0094 00075000
         BALR  @14,@15                                          01S0094 00076000
*    RFY INDMSIOT BASED(FSIOTPTR);     /*TEMP BASE FOR SIOT MAPPING  */ 00077000
*    FSIOTPTR=SMISVAB(SIOTPOS);        /*RETURN SIOT PTR TO CALLER   */ 00078000
         L     @10,@PC00001+12                                  01S0096 00079000
         L     @15,SMISVAB                                      01S0096 00080000
         ST    @15,FSIOTPTR(,@10)                               01S0096 00081000
*    SIOTDSKA=SMISVA(SIOTPOS);         /*SET SIOT SELF SVA           */ 00082000
         MVC   SIOTDSKA(3,@15),SMISVA                           01S0097 00083000
*    SIOTTYPE=3;                       /*SET SIOT RECORD ID          */ 00084000
         MVI   SIOTTYPE(@15),X'03'                              01S0098 00085000
*    SCTPJFCB=SMISVA(JFCBPOS);         /*SET SIOT JFCB SVA           */ 00086000
         MVC   SCTPJFCB(3,@15),SMISVA+4                         01S0099 00087000
*    IF PCATINIT = ON THEN             /*IF PRIVATE CATALOG ALLOC       00088000
*                                        TO INITIATOR THEN   @G24LPSJ*/ 00089000
         L     @10,@PC00001+16                                  01S0100 00090000
         TM    PCATINIT(@10),B'01000000'                        01S0100 00091000
         BNO   @RF00100                                         01S0100 00092000
*       SCTDDINO = 0;                  /*SET DD NUM TO ZERO  @G24LPSJ*/ 00093000
         SLR   @10,@10                                          01S0101 00094000
         STH   @10,SCTDDINO(,@15)                               01S0101 00095000
*    ELSE                              /*ELSE                @G24LPSJ*/ 00096000
*       SCTDDINO = 1;                  /*SET TO ONE          @G24LPSJ*/ 00097000
         B     @RC00100                                         01S0102 00098000
@RF00100 L     @10,@PC00001+12                                  01S0102 00099000
         L     @10,FSIOTPTR(,@10)                               01S0102 00100000
         MVC   SCTDDINO(2,@10),@CH00072                         01S0102 00101000
*    SJFCBPTR=SMISVAB(JFCBPOS);        /*SET SIOT JFCB IN-CORE PTR   */ 00102000
@RC00100 L     @10,@PC00001+12                                  01S0103 00103000
         L     @10,FSIOTPTR(,@10)                               01S0103 00104000
         L     @15,SMISVAB+4                                    01S0103 00105000
         ST    @15,SJFCBPTR(,@10)                               01S0103 00106000
*     DO; /*MODESET  KEYADDR(KEYZERO)*/                         01S0104 00107000
*       SPKA (KEYZERO); /*SET PSW KEY*/                         01S0105 00108000
         L     @01,KEYZERO                                      01S0105 00109000
         SPKA  0(@01)                                           01S0105 00110000
*     END; /*MODESET  KEYADDR(KEYZERO)*/       /*CHANGE TO ZERO PROTECT 00111000
*KEY  */                                                        01S0106 00112000
*    JSCDDNUM=JSCDDNUM+1;              /*UPDATE NUMBER OF ALLOCATIONS*/ 00113000
         L     @15,@PC00001                                     01S0107 00114000
         L     @15,JSCBPTR(,@15)                                01S0107 00115000
         LA    @14,1                                            01S0107 00116000
         AH    @14,JSCDDNUM(,@15)                               01S0107 00117000
         STH   @14,JSCDDNUM(,@15)                               01S0107 00118000
*     DO; /*MODESET  EXTKEY(SCHED)*/                            01S0108 00119000
*       SPKA (16); /*SET PSW KEY*/                              01S0109 00120000
         LA    @01,16                                           01S0109 00121000
         SPKA  0(@01)                                           01S0109 00122000
*     END; /*MODESET  EXTKEY(SCHED)*/          /*CHANGE BACK TO SCHEDUL 00123000
*R KEY*/                                                        01S0110 00124000
*    RFY INDMSIOT BASED(SIOTPTR);      /*RESET SIOT MAPPING BASE     */ 00125000
*/********************************************************************/ 00126000
*/*                                                                  */ 00127000
*/* SEGMENT NAME - TABSETUP                                          */ 00128000
*/*                                                                  */ 00129000
*/* DESCRIPTIVE NAME - SIOT, JFCB, JFCBX TABLE SET UP                */ 00130000
*/*                                                                  */ 00131000
*/* FUNCTION - FILL IN SIOT, JFCB, AND JFCBX'S ACCORDING TO          */ 00132000
*/*   INFORMATION SPECIFIED IN USER'S TEXT UNITS. SUPPLY DEFAULTS    */ 00133000
*/*   WHERE APPLICABLE.                                              */ 00134000
*/*                                                                  */ 00135000
*/*   OPERATION - THE TIME MACRO IS ISSUED TO OBTAIN THE CURRENT     */ 00136000
*/*     DATE AND TIME IF A STATUS OF NEW OR MOD IS SPECIFIED,        */ 00137000
*/*     RETENTION PERIOD IS SPECIFIED, DSNAME IS NOT SPECIFIED,      */ 00138000
*/*     OR AN &NAME IS SPECIFIED. THE DATE WILL BE USED IF           */ 00139000
*/*     A DSNAME IS TO BE GENERATED AND FOR THE DATA SET             */ 00140000
*/*     CREATE DATE AND RETENTION PERIOD. THE TIME IS                */ 00141000
*/*     USED IN THE GENERATED DSNAME.                                */ 00142000
*/*                                                                  */ 00143000
*/*     IF A DDNAME IS SPECIFIED, IT IS PLACED IN THE SIOT.          */ 00144000
*/*     OTHERWISE, THE DDNAME GENERATION ROUTINE, IEFDB4FD, IS       */ 00145000
*/*     INVOKED TO GENERATE A DDNAME WHICH IS PLACED IN THE SIOT.    */ 00146000
*/*                                                                  */ 00147000
*/*     OTHER TEXT UNITS ARE PROCESSED AS FOLOWS:                    */ 00148000
*/*       SEGMENT  TEXT UNITS                                        */ 00149000
*/*       DUMDSN   DUMMY, TERM, QNAME, DSNAME, MEMBER NAME           */ 00150000
*/*       STATDISP STATUS, NORMAL DISP, CONDITIONAL DISP             */ 00151000
*/*       SPACE    SPACE RELATED TEXT UNITS                          */ 00152000
*/*       UNIT     UNIT  RELATED TEXT UNITS                          */ 00153000
*/*       VOLUME   VOLUME RELATED TEXT UNITS                         */ 00154000
*/*       SUBSYSOU SYSOUT RELATED TEXT UNITS, SUBSYSTEM TEXT UNIT    */ 00155000
*/*       LABEL    LABEL TEXT UNITS, INCLUDING EXPIRATION DATE       */ 00156000
*/*                AND RETENTION PERIOD, CREATE DATE                 */ 00157000
*/*       FCBUCS   FCB AND UCS TEXT UNITS                            */ 00158000
*/*       DCB      DCB ATTRIBUTES AND DCB REFERENCE                  */ 00159000
*/*                                                                  */ 00160000
*/*     IF THE UNALLOC AT CLOSE TEXT UNIT IS SPECIFIED, THE          */ 00161000
*/*     APPROPRIATE SIOT INDICATOR IS SET.                           */ 00162000
*/*                                                                  */ 00163000
*/*     IF SPECIFIED BY THE USER, SPECIAL BITS ARE SET TO            */ 00164000
*/*     INDICATE THE DATA SET IS A CATALOG.                          */ 00165000
*/*                                                                  */ 00166000
*/*     IF MSVGP IS SPECIFIED AND DUMMY IS NOT SPECIFIED, THE@Y30LPPD*/ 00167000
*/*     MSVGP NAME IS MOVED INTO THE JFCB, AND BIT JFCBMSGP  @Y30LPPD*/ 00168000
*/*     IS SET TO INDICATE THAT THE USER SPECIFIED MSVGP. THE@Y30LPPD*/ 00169000
*/*     THE PRIVATE BIT IS TURNED ON IN THE SIOT.            @Y30LPPD*/ 00170000
*/*                                                          @Y30LPPD*/ 00171000
*/* INPUT - KEYTABLE - TO DETERMINE IF TEXT UNIT IS SPECIFIED        */ 00172000
*/*         ALFNCMAP - TO DETERMINE IF CATALOG DATA SET INDICATORS   */ 00173000
*/*                    ARE TO BE SET.                                */ 00174000
*/*                                                                  */ 00175000
*/* OUTPUT - SIOT, JFCB, AND JFCBX(S) REFLECT USER'S REQUEST         */ 00176000
*/*                                                                  */ 00177000
*/********************************************************************/ 00178000
*KEYTABP = KEYTBPTR;                   /*SET KEY TABLE MAP BASE PTR */  00179000
         L     @15,@PC00001+8                                   02S0112 00180000
         L     KEYTABP,KEYTBPTR(,@15)                           02S0112 00181000
*SIOTPTR = FSIOTPTR;                   /*SET SIOT MAP BASE PTR       */ 00182000
         LR    SIOTPTR,@10                                      02S0113 00183000
*JFCBPTR = SJFCBPTR;                   /*SET JFCB MAP BASE PTR       */ 00184000
*                                                               02S0114 00185000
         L     JFCBPTR,SJFCBPTR(,SIOTPTR)                       02S0114 00186000
*IF STATSTUP->TEXTUPAR(1) = NEW        /*NEW SPECIFIED?              */ 00187000
*  | STATSTUP->TEXTUPAR(1) = MOD       /*OR MOD SPECIFIED?           */ 00188000
*  | RETPDTUP ^= ZERO                  /*RETENTION PERIOD SPECIFIED? */ 00189000
*  | DSNAMTUP = ZERO |                                          02S0115 00190000
*    DSNAMTUP->TEXTUPAR(1) = '&'THEN   /*OR WILL GENERATE A DSNAME?  */ 00191000
         L     @10,STATSTUP(,KEYTABP)                           02S0115 00192000
         CLI   TEXTUPAR(@10),X'04'                              02S0115 00193000
         BE    @RT00115                                         02S0115 00194000
         CLI   TEXTUPAR(@10),X'02'                              02S0115 00195000
         BE    @RT00115                                         02S0115 00196000
         SLR   @10,@10                                          02S0115 00197000
         C     @10,RETPDTUP(,KEYTABP)                           02S0115 00198000
         BNE   @RT00115                                         02S0115 00199000
         L     @15,DSNAMTUP(,KEYTABP)                           02S0115 00200000
         CR    @15,@10                                          02S0115 00201000
         BE    @RT00115                                         02S0115 00202000
         CLI   TEXTUPAR(@15),C'&&'                              02S0115 00203000
         BNE   @RF00115                                         02S0115 00204000
@RT00115 DS    0H                                               02S0116 00205000
*  DO;                                                          02S0116 00206000
*    RFY (R0,R1) RESTRICTED;                                    02S0117 00207000
*    GEN ( TIME  DEC)                  /*YES,GET CURRENT DATE & TIME */ 00208000
*      SETS(R0,R1);                                             02S0118 00209000
          TIME  DEC                                                     00210000
*    TIME = R0;                        /*SAVE TIME                   */ 00211000
         STCM  R0,15,TIME                                       02S0119 00212000
*    DATE = R1;                        /*SAVE DATE                   */ 00213000
         STCM  R1,15,DATE                                       02S0120 00214000
*    RFY (R0,R1) UNRESTRICTED;                                  02S0121 00215000
*  END;                                                         02S0122 00216000
*                                                               02S0122 00217000
*RFY TEXTUNIT BASED(DDNAMTUP);         /*BASE FOR DDNAME TEXTUNIT    */ 00218000
@RF00115 DS    0H                                               02S0124 00219000
*IF DDNAMTUP ^= ZERO THEN              /*DDNAME SPECIFIED?           */ 00220000
         L     @10,DDNAMTUP(,KEYTABP)                           02S0124 00221000
         LTR   @10,@10                                          02S0124 00222000
         BZ    @RF00124                                         02S0124 00223000
*  DO;                                                          02S0125 00224000
*    SCTDDNAM = '';                    /*YES,BLANK OUT SIOT DDN FIELD*/ 00225000
         MVI   SCTDDNAM(SIOTPTR),C' '                           02S0126 00226000
         MVC   SCTDDNAM+1(7,SIOTPTR),SCTDDNAM(SIOTPTR)          02S0126 00227000
*    SCTDDNAM(1:TEXTULNG) = TEXTUPAR(1:TEXTULNG); /*PUT DDN IN SIOT  */ 00228000
         ICM   @15,12,TEXTULNG(@10)                             02S0127 00229000
         SRA   @15,16                                           02S0127 00230000
         BCTR  @15,0                                            02S0127 00231000
         EX    @15,@SM02507                                     02S0127 00232000
*  END;                                                         02S0128 00233000
*ELSE                                                           02S0129 00234000
*  CALL IEFDB4FD (JSCBPTR,ADDR(SCTDDNAM),0); /*GENERATE A DDNAME     */ 00235000
*                                                               02S0129 00236000
         B     @RC00124                                         02S0129 00237000
@RF00124 L     @10,@PC00001                                     02S0129 00238000
         ST    @10,@AL00001                                     02S0129 00239000
         LA    @10,SCTDDNAM(,SIOTPTR)                           02S0129 00240000
         ST    @10,@TF00001                                     02S0129 00241000
         LA    @10,@TF00001                                     02S0129 00242000
         ST    @10,@AL00001+4                                   02S0129 00243000
         LA    @10,@CF00090                                     02S0129 00244000
         ST    @10,@AL00001+8                                   02S0129 00245000
         L     @15,@CV00172                                     02S0129 00246000
         LA    @01,@AL00001                                     02S0129 00247000
         BALR  @14,@15                                          02S0129 00248000
*/********************************************************************/ 00249000
*/*                                                                  */ 00250000
*/* SEGMENT NAME - DUMDSN                                            */ 00251000
*/*                                                                  */ 00252000
*/* DESCRIPTIVE NAME - DUMMY, TERM, QNAME, DSNAME AND MEMBER NAME    */ 00253000
*/*   TEXT UNIT PROCESSING.                                          */ 00254000
*/*                                                                  */ 00255000
*/*   OPERATION - THE JFCB DSNAME FIELD AND SIOT ALTERNATE NAME      */ 00256000
*/*     FIELD ARE INITIALIZED TO BLANKS. IF TERM IS SPECIFIED AND    */ 00257000
*/*     THE USER IS IN A TSO ENVIRONMENT, THE SIOT TERM AND DUMMY    */ 00258000
*/*     BITS ARE SET ON, AND NULLFILE IS SET AS THE DSNAME IN THE    */ 00259000
*/*     JFCB.                                                        */ 00260000
*/*                                                                  */ 00261000
*/*     IF TERM IS NOT PROCESSED AND DUMMY IS SPECIFIED, THE SIOT    */ 00262000
*/*     DUMMY BIT IS SET AND NULLFILE IS SET AS THE DSNAME IN THE    */ 00263000
*/*     JFCB.                                                        */ 00264000
*/*                                                                  */ 00265000
*/*     IF NEITHER TERM NOR DUMMY ARE PROCESSED AND QNAME IS         */ 00266000
*/*     SPECIFIED, THE SIOT QNAME AND DUMMY INDICATORS ARE SET, AND  */ 00267000
*/*     THE QNAME IS SET IN THE JFCB.                                */ 00268000
*/*                                                                  */ 00269000
*/*     IF NEITHER TERM, NOR DUMMY, NOR QNAME IS SPECIFIED, THEN     */ 00270000
*/*     IF A NON-&DSNAME IS SPECIFIED, IT IS PLACED IN THE JFCB.     */ 00271000
*/*     IF THE DSNAME IS NULLFILE, THE SIOT DUMMY BIT IS SET ON. IF  */ 00272000
*/*     THE DSNAME CONTAINS A PERIOD, THE SIOT QUALIFIED DSNAME      */ 00273000
*/*     INDICATOR IS SET. IF AN &DSNAME IS SPECIFIED, THE NAME IS    */ 00274000
*/*     PLACED IN THE SIOT ALTERNATE FIELD AND THE GENDSNAM          */ 00275000
*/*     SUBROUTINE IS INVOKED TO GENERATE A REAL DSNAME.             */ 00276000
*/*                                                                  */ 00277000
*/*     IF NO DSNAME IS SPECIFIED AND SYSOUT IS NOT SPECIFIED,       */ 00278000
*/*     THE GENERATE DSNAME SUBROUTINE IS INVOKED. (THE SUBSYSTEM    */ 00279000
*/*     GENERATES DSNAMES FOR SYSOUT DATA SETS.)                     */ 00280000
*/*                                                                  */ 00281000
*/*     THE JFCB MEMBER NAME FIELD IS BLANKED. IF A MEMBER NAME IS   */ 00282000
*/*     SPECIFIED, IT IS FILLED IN.  IF THE FIRST CHARACTER OF THE   */ 00283000
*/*     MEMBER IS '+', '0', OR '-', THE JFCB GDG SINGLE BIT IS SET.  */ 00284000
*/*     OTHERWISE THE JFCB  PDS MEMBER INDICATOR IS SET.             */ 00285000
*/*                                                                  */ 00286000
*/* INPUT -                                                          */ 00287000
*/*   KEYTABLE - TO DETERMINE IF TEXT UNITS ARE SPECIFIED.           */ 00288000
*/*   ASCB - TO DETERMINE IF USER IS A TSO USER                      */ 00289000
*/*                                                                  */ 00290000
*/* OUTPUT -                                                         */ 00291000
*/*   SIOT AND JFCB   FILLED IN FOR SPECIFIED TEXT UNITS             */ 00292000
*/*                                                                  */ 00293000
*/********************************************************************/ 00294000
*JFCBDSNM = '';                        /*BLANK JFCB DSNAME FIELD     */ 00295000
@RC00124 MVI   JFCBDSNM(JFCBPTR),C' '                           03S0130 00296000
         MVC   JFCBDSNM+1(43,JFCBPTR),JFCBDSNM(JFCBPTR)         03S0130 00297000
*SCTANAME = '';                        /*BLANK SIOT ALT. NAME FIELD  */ 00298000
         MVI   SCTANAME(SIOTPTR),C' '                           03S0131 00299000
         MVC   SCTANAME+1(7,SIOTPTR),SCTANAME(SIOTPTR)          03S0131 00300000
*IF (TERMTUP ^= ZERO                   /*TERMINAL SPECIFIED?         */ 00301000
*    & ASCBTSB ^= ZERO)                /*AND USER IN TSO ENVIRONMENT */ 00302000
*  | DUMMYTUP ^= ZERO THEN             /*OR DUMMY SPECIFIED?         */ 00303000
         SLR   @10,@10                                          03S0132 00304000
         C     @10,TERMTUP(,KEYTABP)                            03S0132 00305000
         BE    @GL00005                                         03S0132 00306000
         L     @15,@PC00001                                     03S0132 00307000
         L     @01,ASCBPTR(,@15)                                03S0132 00308000
         C     @10,ASCBTSB(,@01)                                03S0132 00309000
         BNE   @RT00132                                         03S0132 00310000
@GL00005 L     @10,DUMMYTUP(,KEYTABP)                           03S0132 00311000
         LTR   @10,@10                                          03S0132 00312000
         BZ    @RF00132                                         03S0132 00313000
@RT00132 DS    0H                                               03S0133 00314000
*  DO;                                                          03S0133 00315000
*    SCTDUMMY = ON;                    /*YES,SET SIOT DUMMY TYPE BIT */ 00316000
         OI    SCTDUMMY(SIOTPTR),B'10000000'                    03S0134 00317000
*    JFCBDSNM(1:8) = 'NULLFILE';       /*SET NULL DSNAME             */ 00318000
         MVC   JFCBDSNM(8,JFCBPTR),@CC02430                     03S0135 00319000
*    IF (TERMTUP ^= ZERO               /*PROCESSING FOR TERMINAL?    */ 00320000
*        & ASCBTSB ^= ZERO) THEN       /*NOTE: TERM OVERRIDES DUMMY  */ 00321000
         SLR   @10,@10                                          03S0136 00322000
         C     @10,TERMTUP(,KEYTABP)                            03S0136 00323000
         BE    @RF00136                                         03S0136 00324000
         L     @15,@PC00001                                     03S0136 00325000
         L     @01,ASCBPTR(,@15)                                03S0136 00326000
         C     @10,ASCBTSB(,@01)                                03S0136 00327000
         BE    @RF00136                                         03S0136 00328000
*      SIOTTERM = ON;                  /*YES,SET SIOT TERM INDICATOR */ 00329000
         OI    SIOTTERM(SIOTPTR),B'01000000'                    03S0137 00330000
*  END;                                                         03S0138 00331000
*                                                               03S0138 00332000
@RF00136 DS    0H                                               03S0139 00333000
*RFY TEXTUNIT BASED(QNAMETUP);         /*BASE FOR QNAME TEXTUNIT     */ 00334000
@RF00132 DS    0H                                               03S0140 00335000
*IF QNAMETUP ^= ZERO                   /*QNAME SPECIFIED?            */ 00336000
*  & SCTDUMMY = OFF THEN               /*AND NOT DUMMY OR TERM?      */ 00337000
         L     @10,QNAMETUP(,KEYTABP)                           03S0140 00338000
         LTR   @10,@10                                          03S0140 00339000
         BZ    @RF00140                                         03S0140 00340000
         TM    SCTDUMMY(SIOTPTR),B'10000000'                    03S0140 00341000
         BNZ   @RF00140                                         03S0140 00342000
*  DO;                                                          03S0141 00343000
*    JFCBQNAM(1:TEXTULNG) = TEXTUPAR(1:TEXTULNG); /*YES,MOVE TO JFCB */ 00344000
         ICM   @15,12,TEXTULNG(@10)                             03S0142 00345000
         SRA   @15,16                                           03S0142 00346000
         BCTR  @15,0                                            03S0142 00347000
         EX    @15,@SM02510                                     03S0142 00348000
*    SIOTQNAM = ON;                    /*SET SIOT QNAME INDICATOR    */ 00349000
         OI    SIOTQNAM(SIOTPTR),B'00000001'                    03S0143 00350000
*    SCTDUMMY = ON;                    /*SET SIOT DUMMY TYPE BIT     */ 00351000
         OI    SCTDUMMY(SIOTPTR),B'10000000'                    03S0144 00352000
*  END;                                                         03S0145 00353000
*                                                               03S0145 00354000
*RFY TEXTUNIT BASED(DSNAMTUP);         /*BASE FOR DSNAME TEXTUNIT    */ 00355000
@RF00140 DS    0H                                               03S0147 00356000
*IF SCTDUMMY = OFF THEN                /*DUMMY TYPE D.S. SPECIFIED?  */ 00357000
