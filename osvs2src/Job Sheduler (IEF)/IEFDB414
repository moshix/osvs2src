*/*                                                                  */ 02261000
*/* SEGMENT NAME - GENDSNAM                                          */ 02262000
*/*                                                                  */ 02263000
*/* DESCRIPTIVE NAME - GENERATE DSNAME ROUTINE                       */ 02264000
*/*                                                                  */ 02265000
*/* FUNCTION - GENERATE A UNIQUE DSNAME IN THE JFCB DSNAME FIELD.    */ 02266000
*/*   THE FORMAT IS 'SYSYYDDD.TXXXXXX.RA000.JOBNAME.ZZZZZZZZ' WHERE  */ 02267000
*/*   'YYDDD' IS THE CURRENT DATE, 'XXXXXX' IS THE CURRENT TIME, 'R' */ 02268000
*/*   INDICATES A TEMPORARY DATA SET, 'A' INDICATES THE NAME IS      */ 02269000
*/*   GENERATED BY THE VS/2 SYSTEM, 'JOBNAME' IS THE NAME OF THE JOB,*/ 02270000
*/*   AND 'ZZZZZZZZ' IS THE NAME IN THE &NAME, IF SPECIFIED, OR      */ 02271000
*/*   'R' PLUS A UNIQUE NUMBER.  THE NUMBER USED IS JSCDDNNO,@ZA02396*/ 02272000
*/*   THE COUNT OF DDNAMES GENERATED FOR THE JOBSTEP.        @ZA02396*/ 02273000
*/*   THIS IS DONE TO AVOID DUPLICATE NAMES.                 @ZA02396*/ 02274000
*/*                                                                  */ 02275000
*/*   OPERATION - THE STATIC PORTION OF THE NAME IS PLACED IN THE    */ 02276000
*/*     JFCB. THE DATE AND TIME ARE PLACED IN THE NAME. THE JOBNAME  */ 02277000
*/*     FROM THE JCT IS PLACED IN THE NAME. A PERIOD AND THE SIOT    */ 02278000
*/*     ALTERNATE NAME ARE PLACED IN THE NAME AFTER THE LAST         */ 02279000
*/*     CHARACTER IN THE JOB NAME, UNLESS THERE IS NO ALTERNATE NAME.*/ 02280000
*/*     IN THIS CASE, .R + A UNIQUE NUMBER IS PLACED AFTER   @ZA02396*/ 02281000
*/*     JOBNAME. SEE FUNCTION ABOVE.                         @ZA02396*/ 02282000
*/*                                                                  */ 02283000
*/* INPUT                                                            */ 02284000
*/*   CURRENT DATE AND TIME                                          */ 02285000
*/*   NAME PART OF &NAME = SIOT ALTERNATE NAME                       */ 02286000
*/*                                                                  */ 02287000
*/* OUTPUT                                                           */ 02288000
*/*   GENERATED NAME IN JFCB DSNAME FILED                            */ 02289000
*/*                                                                  */ 02290000
*/********************************************************************/ 02291000
*GENDSNAM: PROC;                       /*INTERNAL PROC-GENERATE DSN  */ 02292000
@EL00001 L     @13,4(,@13)                                      02S0570 02293000
@EF00001 L     @00,@SIZDATD                                     02S0570 02294000
         LR    @01,@11                                          02S0570 02295000
         FSPACE   R,LV=(0),A=(1)                                        02296000
@ER00001 LM    @14,@12,12(@13)                                  02S0570 02297000
         BR    @14                                              02S0570 02298000
GENDSNAM STM   @14,@12,12(@13)                                  02S0570 02299000
*RFY (JFCBPTR,SIOTPTR,KEYTABP) RESTRICTED;                      02S0571 02300000
*JFCBDSNM(1:LENGTH(GENNAME1)) = GENNAME1; /*BASE OF GEN NAME TO JFCB */ 02301000
         MVC   JFCBDSNM(23,JFCBPTR),GENNAME1                    02S0572 02302000
*UNPK (YYDDD,DATE(2:4));               /*DATE TO JFCB GENERATED NAME */ 02303000
         UNPK  YYDDD(5,JFCBPTR),DATE+1(3)                       02S0573 02304000
*TIME(4) = TIME(4) | '0F'X;            /*PREPARE TIME BUFFER FOR UNPK*/ 02305000
         OI    TIME+3,X'0F'                                     02S0574 02306000
*MVO (TIME,TIME(1:3));                 /*TIME = 0H HM MS SF          */ 02307000
         MVO   TIME(4),TIME(3)                                  02S0575 02308000
*UNPK (TTTTTT,TIME);                   /*TIME TO JFCB GENERATED NAME */ 02309000
         UNPK  TTTTTT(6,JFCBPTR),TIME(4)                        02S0576 02310000
*JOBNAME = JCTJNAME;                   /*JOBNAME TO JFCB GEN NAME    */ 02311000
         L     @10,@PC00001                                     02S0577 02312000
         L     @10,JCTPTR(,@10)                                 02S0577 02313000
         MVC   JOBNAME(8,JFCBPTR),JCTJNAME(@10)                 02S0577 02314000
*I=2;                                  /*I WILL INDEX INTO JOBNAME   */ 02315000
         LA    I,2                                              02S0578 02316000
*DO WHILE(JOBNAME(I)^=' ');            /*FIND JOBNAME END            */ 02317000
         B     @DE00579                                         02S0579 02318000
@DL00579 DS    0H                                               02S0580 02319000
*  I = I + 1;                                                   02S0580 02320000
         AL    I,@CF00072                                       02S0580 02321000
*END;                                                           02S0581 02322000
@DE00579 LR    @10,JFCBPTR                                      02S0581 02323000
         ALR   @10,I                                            02S0581 02324000
         CLI   JOBNAME-1(@10),C' '                              02S0581 02325000
         BNE   @DL00579                                         02S0581 02326000
*WORKPTR = ADDR(JOBNAME(I));           /*ADDR OF JOBNAME END IN JFCB */ 02327000
         LA    WORKPTR,JOBNAME-1(I,JFCBPTR)                     02S0582 02328000
*WORKPTR -> DOT = '.';                 /*SET . IF JFCB NAME  @ZA02396*/ 02329000
         MVI   DOT(WORKPTR),C'.'                                02S0583 02330000
*IF SCTANAME(1) = ' ' THEN             /*&NAME SPECIFIED?            */ 02331000
         CLI   SCTANAME(SIOTPTR),C' '                           02S0584 02332000
         BNE   @RF00584                                         02S0584 02333000
*   DO;                                /*                    @ZA02396*/ 02334000
*     GENDSNUM = JSCDDNNO;             /*USE DD NAME COUNTER TO 02S0586 02335000
*                                        GENERATE UNIQUE TEMPORARY      02336000
*                                        DATA SET NAME       @ZA02396*/ 02337000
         L     @10,@PC00001                                     02S0586 02338000
         L     @10,JSCBPTR(,@10)                                02S0586 02339000
         LH    @15,JSCDDNNO(,@10)                               02S0586 02340000
         LR    GENDSNUM,@15                                     02S0586 02341000
*     CVD(GENDSNUM,DSNUM1);            /*CONVERT HEX TO PRINTABLE       02342000
*                                        CHARACTERS          @ZA02396*/ 02343000
         CVD   GENDSNUM,DSNUM1                                  02S0587 02344000
*     UNPK(DSNUM2,DSNUM1);             /*                    @ZA02396*/ 02345000
         UNPK  DSNUM2(8),DSNUM1(8)                              02S0588 02346000
*     DSNUM2(8) = DSNUM2(8) | CHGSIGN; /*CHANGE SIGN OF LAST CHARACTER  02347000
*                                                            @ZA02396*/ 02348000
         OI    DSNUM2+7,B'11110000'                             02S0589 02349000
*     DSNUM2(1) = GENNAME2;            /*INSERT 'R'          @ZA02396*/ 02350000
         MVC   DSNUM2(1),GENNAME2                               02S0590 02351000
*     WORKPTR->AMPNAME = DSNUM2;       /*MOVE TO JFCB NAME   @ZA02396*/ 02352000
         MVC   AMPNAME(8,WORKPTR),DSNUM2                        02S0591 02353000
*      DO; /*MODESET  KEYADDR(KEYZERO)*/                        02S0592 02354000
*        SPKA (KEYZERO); /*SET PSW KEY*/                        02S0593 02355000
         L     @01,KEYZERO                                      02S0593 02356000
         SPKA  0(@01)                                           02S0593 02357000
*      END; /*MODESET  KEYADDR(KEYZERO)*/       /*GET INTO KEY ZERO   @ 02358000
*A02396*/                                                       02S0594 02359000
*     JSCDDNNO = JSCDDNNO + ONE;       /*UPDATE DDNAME COUNTER SO THAT  02360000
*                                        NEXT DSNAME WILL BE UNIQUE     02361000
*                                        EVEN IF NO DDNAME GENERATED    02362000
*                                                            @ZA02396*/ 02363000
         AL    @15,@CF00072                                     02S0595 02364000
         STH   @15,JSCDDNNO(,@10)                               02S0595 02365000
*      DO; /*MODESET  EXTKEY(SCHED)*/                           02S0596 02366000
*        SPKA (16); /*SET PSW KEY*/                             02S0597 02367000
         LA    @10,16                                           02S0597 02368000
         SPKA  0(@10)                                           02S0597 02369000
*      END; /*MODESET  EXTKEY(SCHED)*/          /*RETURN TO SCHED KEY @ 02370000
*A02396*/                                                       02S0598 02371000
*   END;                               /*                    @ZA02396*/ 02372000
*ELSE                                  /*YES-&NAME SPECIFIED         */ 02373000
*  DO;                                                          02S0600 02374000
         B     @RC00584                                         02S0600 02375000
@RF00584 DS    0H                                               02S0601 02376000
*    WORKPTR->AMPNAME = SCTANAME;      /*&NAME TO JFCB GEN NAME W/O &*/ 02377000
         MVC   AMPNAME(8,WORKPTR),SCTANAME(SIOTPTR)             02S0601 02378000
*  END;                                                         02S0602 02379000
*SIOTGDSN = ON;                        /*SET SIOT GENERATED DSN BIT  */ 02380000
@RC00584 DS    0H                                               02S0604 02381000
*SIOTQDSN = ON;                        /*SET SIOT QUALIFIED DSN BIT  */ 02382000
         OI    SIOTGDSN(SIOTPTR),B'00011000'                    02S0604 02383000
*END GENDSNAM;                                                  02S0605 02384000
@EL00002 DS    0H                                               02S0605 02385000
@EF00002 DS    0H                                               02S0605 02386000
@ER00002 LM    @14,@12,12(@13)                                  02S0605 02387000
         BR    @14                                              02S0605 02388000
*/********************************************************************/ 02389000
*/*                                                                  */ 02390000
*/* SEGMENT NAME - DSNTPROC                                          */ 02391000
*/*                                                                  */ 02392000
*/* DESCRIPTIVE NAME - DATASET NAME TABLE (DSNT) PROCESSING          */ 02393000
*/*                                                                  */ 02394000
*/* FUNCTION - ADD AN ENTRY TO THE DATASET NAME TABLE FOR DCB OR     */ 02395000
*/*   VOLUME REFERENCE TO A DATASET NAME. RETURN THE OFFSET OF THE   */ 02396000
*/*   NAME IN THE DSNT, AND THE ACTUAL LENGTH OF DATASET NAME.       */ 02397000
*/*                                                                  */ 02398000
*/*   OPERATION -                                                    */ 02399000
*/*     DETERMINE THE ACTUAL LENGTH OF THE DSNAME SINCE              */ 02400000
*/*     IT MAY BE PADDED WITH BLANKS IN THE INPUT TEXT UNIT.         */ 02401000
*/*                                                                  */ 02402000
*/*     IF THE DSNT ALREADY EXISTS, READ THE LAST RECORD (BY READING */ 02403000
*/*     ONE RECORD AT A TIME, STARTING WITH THE FIRST) USING THE SWA */ 02404000
*/*     MANAGER INTERFACE ROUTINE, IEFAB4F7.                         */ 02405000
*/*                                                                  */ 02406000
*/*     IF THE DSNT DOESN'T EXIST OR THE LAST RECORD DOES NOT HAVE   */ 02407000
*/*     ENOUGH ROOM FOR THE INPUT DATASET NAME, USE IEFAB4F7 TO      */ 02408000
*/*     ASSIGN A NEW RECORD.  THE HEADER OF THE ASSIGNED RECORD IS   */ 02409000
*/*     INITIALIZED BY THIS ROUTINE.                                 */ 02410000
*/*                                                                  */ 02411000
*/*     WHEN THE ASSIGN IS COMPLETE OR IF IT WAS NOT NECESSARY, THE  */ 02412000
*/*     INPUT DATASET NAME IS ADDED TO THE TABLE.  THE OFFSET OF THIS*/ 02413000
*/*     NEW ENTRY IS DETERMINED AND THE SCT IS UPDATED TO REFLECT    */ 02414000
*/*     THE NEW NUMBER OF BYTES USED IN THE DSNT.                    */ 02415000
*/*                                                                  */ 02416000
*/*     THE UPDATED DSNT RECORD(S) IS(ARE) WRITTEN VIA THE SWA       */ 02417000
*/*     MANAGER INTERFACE ROUTINE.  (THIS IS NECESSARY SINCE THE     */ 02418000
*/*     DSNT WILL HAVE TO BE READ BY JFCB HOUSEKEEPING BEFORE THE    */ 02419000
*/*     TABLES WOULD BE WRITTEN AT THE END OF SVC99 CONTROL.) THE    */ 02420000
*/*     EPA(S) FOR THIS WRITE WILL HAVE BEEN FILLED IN BY THE SWA    */ 02421000
*/*     MANAGER INTERFACE ROUTINE: THE EPA FOR THE LAST DSNT RECORD  */ 02422000
*/*     WAS FILLED IN WHEN IT WAS READ, AND THE EPA FOR THE NEW      */ 02423000
*/*     DSNT RECORD WAS FILLED IN DURING THE ASSIGN.                 */ 02424000
*/*                                                                  */ 02425000
*/*     AFTER THE WRITE IS DONE, IF A RECORD WAS ASSIGNED,   @YM08602*/ 02426000
*/*     IT IS NOW CHAINED INTO EITHER THE SCT (IF IT IS THE  @YM08602*/ 02427000
*/*     FIRST DSNT RECORD), OR INTO THE PREVIOUSLY LAST DSNT @YM08602*/ 02428000
*/*     RECORD. THIS IS DONE NOW TO PREVENT ANY PROBLEMS     @YM08602*/ 02429000
*/*     READING IN THE DSNT ON SUBSEQUENT CALLS TO SVC99,    @YM08602*/ 02430000
*/*     SHOULD THIS ROUTINE LOSE CONTROL WITH THE RECORDS    @YM08602*/ 02431000
*/*     CHAINED, BUT BEFORE THE WRITE WAS DONE.              @YM08602*/ 02432000
*/*                                                                  */ 02433000
*/*     SINCE THE SCT IS ALWAYS UPDATED BY THIS ROUTINE, IF AN EPA   */ 02434000
*/*     FOR THE SCT IS NOT ALREADY ON THE CHAIN, AN EPA IS GOTTEN,   */ 02435000
*/*     FILLED IN AND ADDED TO THE CHAIN. (THE SCT IS NOT WRITTEN    */ 02436000
*/*     HERE SINCE JFCB HOUSEKEEPING WILL NOT ATTEMPT TO READ IT.)   */ 02437000
*/*                                                                  */ 02438000
*/* INPUT -                                                          */ 02439000
*/*   REFRNCTU - ADDRESS OF THE VOLUME OR DCB DSN REFERENCE TEXT UNIT*/ 02440000
*/*   SCTLDSTB - NUMBER OF BYTES USED IN DSNT                        */ 02441000
*/*   SCTADSTB - SVA OF FIRST DSNT RECORD OR ZERO IF THE TABLE DOES  */ 02442000
*/*              NOT YET EXIST                                       */ 02443000
*/*   LASTEPAP - ADDRESS OF LAST EPA OF RECORDS TO BE WRITTEN        */ 02444000
*/*   EPASCTSW - IF ON, INDICATES SCT EPA ALREADY ON CHAIN           */ 02445000
*/*                                                                  */ 02446000
*/* OUTPUT-                                                          */ 02447000
*/*   LENGHDSN - ACTUAL LENGTH OF DSNAME IN INPUT TEXT UNIT          */ 02448000
*/*   OFFSET   - OFFSET OF ENTRY IN DSNT                             */ 02449000
*/*   SCTLDSTB - NUMBER OF BYTES USED IN DSNT                        */ 02450000
*/*   SCTADSTB - SVA OF FIRST DSNT RECORD                            */ 02451000
*/*   EPASCTSW - ON, INDICATING SCT EPA ON CHAIN                     */ 02452000
*/*   LASTEPAP - ADDRESS OF EPA FOR SCT IF EPA FOR SCT NOT ALREADY ON*/ 02453000
*/*              THE EPA CHAIN OF RECORDS TO BE WRITTEN              */ 02454000
*/*   NEW ENTRY MADE IN DSNT                                         */ 02455000
*/*                                                                  */ 02456000
*/********************************************************************/ 02457000
*DSNTPROC:PROC;                                                 02S0606 02458000
DSNTPROC STM   @14,@03,@SA00003                                 02S0606 02459000
         STM   @06,@12,@SA00003+24                              02S0606 02460000
*EPAS=EPAS && EPAS;                    /*CLEAR THE EPA CORE          */ 02461000
         XC    EPAS(32),EPAS                                    02S0607 02462000
*RFY TEXTUNIT BASED(REFRNCTU);         /*REFERENCE DSNAME TEXT UNIT  */ 02463000
