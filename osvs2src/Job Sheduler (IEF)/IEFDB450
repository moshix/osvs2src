         ICM   @04,3,ERRCODE(@10)                               02S0068 00122000
         BNZ   @DC00068                                         02S0068 00123000
*                                      /* TEST DDNAME LOOP           */ 00124000
*        DDNAME = '';                  /* CLEAR DDNAME               */ 00125000
         MVI   DDNAME,C' '                                      02S0069 00126000
         MVC   DDNAME+1(7),DDNAME                               02S0069 00127000
*        DDNAME(1:TEXTULEN) = TEXTUPRM(1:TEXTULEN); /* MOVE TEXT PARM*/ 00128000
         ICM   @04,12,TEXTULEN(TXTUFLDP)                        02S0070 00129000
         SRA   @04,16                                           02S0070 00130000
         BCTR  @04,0                                            02S0070 00131000
         EX    @04,@SM01151                                     02S0070 00132000
*        IF DDNAME(1) = ' ' THEN       /* IS DDNAME BLANK ?          */ 00133000
         CLI   DDNAME,C' '                                      02S0071 00134000
         BNE   @RF00071                                         02S0071 00135000
*          DO;                         /* YES, SET ERROR & INFO CODES*/ 00136000
*            ERRCODE = RCINPARM;       /* INDICATE INVALID PARM      */ 00137000
         MVC   ERRCODE(2,@10),@CB00781                          02S0073 00138000
*            INFOCODE = DCCDDNAM;      /* INDICATE FAILING KEY       */ 00139000
         MVC   INFOCODE(2,@10),@CB00138                         02S0074 00140000
*          END;                        /* END BLANK DDNAME TEST      */ 00141000
*        ELSE                          /* DDNAME NOT BLANK           */ 00142000
*          IF DDNAME = JOBLIB | DDNAME = STEPLIB                02S0076 00143000
*            | DDNAME = JOBCAT | DDNAME = STEPCAT THEN          02S0076 00144000
         B     @RC00071                                         02S0076 00145000
@RF00071 CLC   DDNAME(8),@CC00110                               02S0076 00146000
         BE    @RT00076                                         02S0076 00147000
         CLC   DDNAME(8),@CC00121                               02S0076 00148000
         BE    @RT00076                                         02S0076 00149000
         CLC   DDNAME(8),@CC00108                               02S0076 00150000
         BE    @RT00076                                         02S0076 00151000
         CLC   DDNAME(8),@CC00119                               02S0076 00152000
         BNE   @RF00076                                         02S0076 00153000
@RT00076 DS    0H                                               02S0077 00154000
*            ERRCODE = RCRSVDDN;       /* RESERVED DDNAME SPECIFIED  */ 00155000
         L     @10,@PC00001+4                                   02S0077 00156000
         MVC   ERRCODE(2,@10),@CB00785                          02S0077 00157000
*          ELSE                        /* NOT A RESERVED DDNAME      */ 00158000
*            TXTUFLDP = TXTUFLDP + TEXTULEN + LENGTH(TEXTULEN); 02S0078 00159000
         B     @RC00076                                         02S0078 00160000
@RF00076 LR    @10,TXTUFLDP                                     02S0078 00161000
         ICM   @04,12,TEXTULEN(TXTUFLDP)                        02S0078 00162000
         SRA   @04,16                                           02S0078 00163000
         ALR   @10,@04                                          02S0078 00164000
         AL    @10,@CF00050                                     02S0078 00165000
         LR    TXTUFLDP,@10                                     02S0078 00166000
*                                      /* INCREMENT TO NEXT TXT ENTRY*/ 00167000
*      END;                            /* END TEST DDNAME LOOP       */ 00168000
@RC00076 DS    0H                                               02S0079 00169000
@RC00071 AL    I,@CF00080                                       02S0079 00170000
@DE00068 L     @10,CCDDNTUP                                     02S0079 00171000
         ICM   @10,12,TEXTUNUM(@10)                             02S0079 00172000
         SRA   @10,16                                           02S0079 00173000
         CR    I,@10                                            02S0079 00174000
         BNH   @DL00068                                         02S0079 00175000
@DC00068 DS    0H                                               02S0080 00176000
*    END;                              /* END REQUIRED KEY PRESENT   */ 00177000
*  ELSE                                /* REQUIRE KEY NOT SPECIFIED  */ 00178000
*    ERRCODE = RCKEYREQ;               /* INDICATE REQUIRED KEY NOT     00179000
*                                         SPECIFIED                  */ 00180000
         B     @RC00065                                         02S0081 00181000
@RF00065 L     @10,@PC00001+4                                   02S0081 00182000
         MVC   ERRCODE(2,@10),@CB00801                          02S0081 00183000
*ELSE;                                 /* ERRCODE NOT ZERO           */ 00184000
@RF00064 DS    0H                                               02S0083 00185000
*IF ERRCODE = ZERO THEN                /* ANY ERRORS FOUND IN SYNTAX?*/ 00186000
@RC00064 SLR   @10,@10                                          01S0083 00187000
         L     @04,@PC00001+4                                   01S0083 00188000
         SLR   @15,@15                                          01S0083 00189000
         ICM   @15,3,ERRCODE(@04)                               01S0083 00190000
         CR    @10,@15                                          01S0083 00191000
         BNE   @RF00083                                         01S0083 00192000
*  DO;                                 /* NO, INITIALIZE TIOT MGR REQ   00193000
*                                         BLOCK & SEARCH FOR DDNAMES */ 00194000
*    TIORBPTR = ADDR(TMGRCORE);        /* PTR TO TIOT MGR REQ BLOCK  */ 00195000
         LA    @04,TMGRCORE                                     01S0085 00196000
         ST    @04,TIORBPTR                                     01S0085 00197000
*    TIOMGRRB = TIOMGRRB && TIOMGRRB;  /* ZERO REQUEST BLOCK         */ 00198000
         XC    TIOMGRRB(38,@04),TIOMGRRB(@04)                   01S0086 00199000
*    TIOMCCAT = ON;                    /* TURN ON CONCAT BIT         */ 00200000
         OI    TIOMCCAT(@04),B'00000100'                        01S0087 00201000
*    TIOMJSCP = CCJSCBP;               /* STORE JSCB ADDR IN REQ BLK */ 00202000
