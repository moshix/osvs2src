         LA    @10,@TF00003                                     02S0063 00094000
         ST    @10,@AL00001+12                                  02S0063 00095000
         LA    @10,KEYTABLE                                     02S0063 00096000
         ST    @10,@TF00004                                     02S0063 00097000
         LA    @10,@TF00004                                     02S0063 00098000
         ST    @10,@AL00001+16                                  02S0063 00099000
         ST    @07,@AL00001+20                                  02S0063 00100000
         L     @15,@CV00164                                     02S0063 00101000
         LA    @01,@AL00001                                     02S0063 00102000
         BALR  @14,@15                                          02S0063 00103000
*IF ERRCODE = ZERO THEN                /* IS SYNTAX GOOD ?           */ 00104000
         SLR   @10,@10                                          02S0064 00105000
         L     @04,@PC00001+4                                   02S0064 00106000
         SLR   @15,@15                                          02S0064 00107000
         ICM   @15,3,ERRCODE(@04)                               02S0064 00108000
         CR    @10,@15                                          02S0064 00109000
         BNE   @RF00064                                         02S0064 00110000
*  IF CCDDNTUP ^= ZERO THEN            /* YES, IS REQUIRED KEY SPEC'D*/ 00111000
         L     @04,CCDDNTUP                                     02S0065 00112000
         CR    @04,@10                                          02S0065 00113000
         BE    @RF00065                                         02S0065 00114000
*    DO;                               /* YES, MAKE ADDITIONAL TESTS */ 00115000
*      TXTUFLDP = ADDR(TEXTUENT);      /* GET ADDRESS OF 1ST ENTRY   */ 00116000
         LA    TXTUFLDP,TEXTUENT(,@04)                          02S0067 00117000
*      DO I = 1 BY 1 TO TEXTUNUM WHILE (ERRCODE = ZERO);        02S0068 00118000
