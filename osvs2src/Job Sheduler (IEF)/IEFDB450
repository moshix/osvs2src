*TIOMDSAP = R1;                        /* SET PTR TO DSAB ADDR LIST  */ 00302000
         L     @10,TIORBPTR                                     02S0097 00303000
         ST    R1,TIOMDSAP(,@10)                                02S0097 00304000
*RESPECIFY (R0,R1) UNRESTRICTED;       /* RELEASE REGS 0 AND 1       */ 00305000
*DO I = 1 BY 1 TO TIOMNDSA WHILE(ERRCODE = ZERO); /* DDNAME SRCH LOOP*/ 00306000
         LA    I,1                                              02S0099 00307000
         B     @DE00099                                         02S0099 00308000
@DL00099 SLR   @10,@10                                          02S0099 00309000
         L     @04,@PC00001+4                                   02S0099 00310000
         SLR   @15,@15                                          02S0099 00311000
         ICM   @15,3,ERRCODE(@04)                               02S0099 00312000
         CR    @10,@15                                          02S0099 00313000
         BNE   @DC00099                                         02S0099 00314000
*  DSABPTR = DSQFRSTP;                 /* GET 1ST DSAB ADDRESS       */ 00315000
         L     @04,@PC00001+8                                   02S0100 00316000
         L     @04,CCJSCBP(,@04)                                02S0100 00317000
         L     @04,JSCDSABQ(,@04)                               02S0100 00318000
         L     @04,DSQFRSTP(,@04)                               02S0100 00319000
         ST    @04,DSABPTR                                      02S0100 00320000
*  DDNAME = '';                        /* BLANK DDNAME               */ 00321000
         MVI   DDNAME,C' '                                      02S0101 00322000
         MVC   DDNAME+1(7),DDNAME                               02S0101 00323000
*  DDNAME(1:TEXTULEN) = TEXTUPRM(1:TEXTULEN); /* GET SEARCH DDNAME   */ 00324000
         ICM   @04,12,TEXTULEN(TXTUFLDP)                        02S0102 00325000
         SRA   @04,16                                           02S0102 00326000
         BCTR  @04,0                                            02S0102 00327000
         EX    @04,@SM01151                                     02S0102 00328000
*  RTRNADDR = ZERO;                    /* ZERO RETURN AREA           */ 00329000
         ST    @10,RTRNADDR                                     02S0103 00330000
*  CALL IEFDB4FC(DSABPTR,ADDR(DDNAME),ADDR(RTRNADDR));          02S0104 00331000
         LA    @10,DSABPTR                                      02S0104 00332000
         ST    @10,@AL00001                                     02S0104 00333000
         LA    @10,DDNAME                                       02S0104 00334000
         ST    @10,@TF00001                                     02S0104 00335000
         LA    @10,@TF00001                                     02S0104 00336000
         ST    @10,@AL00001+4                                   02S0104 00337000
         LA    @10,RTRNADDR                                     02S0104 00338000
         ST    @10,@TF00002                                     02S0104 00339000
         LA    @10,@TF00002                                     02S0104 00340000
         ST    @10,@AL00001+8                                   02S0104 00341000
         L     @15,@CV00163                                     02S0104 00342000
         LA    @01,@AL00001                                     02S0104 00343000
         BALR  @14,@15                                          02S0104 00344000
*                                      /* CALL DDNAME SEARCH         */ 00345000
*  IF RTRNADDR = ZERO THEN             /* WAS DDNAME FOUND IN TIOT ? */ 00346000
         L     @10,RTRNADDR                                     02S0105 00347000
         LTR   @10,@10                                          02S0105 00348000
         BNZ   @RF00105                                         02S0105 00349000
*    ERRCODE = RCDDNFND;               /* INDICATE DDNAME NOT FOUND  */ 00350000
         L     @10,@PC00001+4                                   02S0106 00351000
         MVC   ERRCODE(2,@10),@CB00821                          02S0106 00352000
*  ELSE                                /* OTHERWISE DDNAME FOUND     */ 00353000
*    DO;                               /* SAVE DSAB ADDRESS AND TEST    00354000
*                                         FOR DUPLICATE DDNAMES      */ 00355000
         B     @RC00105                                         02S0107 00356000
@RF00105 DS    0H                                               02S0108 00357000
*      DSABNTRY(I) = RTRNADDR;         /* STORE DSAB ADDRESS         */ 00358000
         LR    @10,I                                            02S0108 00359000
         SLA   @10,2                                            02S0108 00360000
         L     @04,TIORBPTR                                     02S0108 00361000
         L     @04,TIOMDSAP(,@04)                               02S0108 00362000
         L     @15,RTRNADDR                                     02S0108 00363000
         AL    @04,@CF01154                                     02S0108 00364000
         ST    @15,DSABNTRY(@10,@04)                            02S0108 00365000
*      NUMBDSAB = NUMBDSAB + 1;        /* INCREMENT COUNT OF DSABS   */ 00366000
         LA    @10,1                                            02S0109 00367000
         AL    @10,NUMBDSAB                                     02S0109 00368000
         ST    @10,NUMBDSAB                                     02S0109 00369000
*      IF DSABNTRY(I) -> DSABOPCT ^= ZERO THEN /* IS DD ASSOCIATED      00370000
*                                                 WITH OPEN DATA SET?*/ 00371000
         LH    @10,DSABOPCT(,@15)                               02S0110 00372000
         LTR   @10,@10                                          02S0110 00373000
         BZ    @RF00110                                         02S0110 00374000
*        ERRCODE = RCOPENDS;           /*YES, SET ERRCODE TO RCOPENDS*/ 00375000
