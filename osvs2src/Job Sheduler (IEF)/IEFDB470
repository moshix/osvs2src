         BCTR  @15,0                                            02S0106 00249000
         EX    @15,@SM01721                                     02S0106 00250000
*            DSNAMTUP = ADDR(DSNAMTU); /* SET PTR TO NEW TEXT UNIT   */ 00251000
         LA    @10,DSNAMTU                                      02S0107 00252000
         ST    @10,DSNAMTUP                                     02S0107 00253000
*          END;                                                 02S0108 00254000
*        TEXTPTR = DSNAMTUP;           /* GET ADDRESS OF TEXT UNIT   */ 00255000
