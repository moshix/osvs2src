*        RTNADDR = ZERO;               /* ZERO RETURN AREA           */ 00265000
         SLR   @10,@10                                          02S0113 00266000
         ST    @10,RTNADDR                                      02S0113 00267000
*        CALL IEFDB4FA                 /* CALL DSNAME SEARCH ROUTINE */ 00268000
*                     (DSABPTR,DSNPTR,RTNADDR,FUNCTION);        02S0114 00269000
         LA    @10,DSABPTR                                      02S0114 00270000
