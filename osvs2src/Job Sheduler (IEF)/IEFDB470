         SRA   @03,16                                           02S0089 00188000
         BCTR  @03,0                                            02S0089 00189000
         EX    @03,@SM01719                                     02S0089 00190000
*        DDNAMEP = ADDR(DDNAREA);      /* GET ADDR OF DDNAME AREA    */ 00191000
         LA    @15,DDNAREA                                      02S0090 00192000
         ST    @15,DDNAMEP                                      02S0090 00193000
*        RTNADDR = ZERO;               /* ZERO RETURN AREA ADDRESS   */ 00194000
         ST    @10,RTNADDR                                      02S0091 00195000
*        CALL IEFDB4FC                 /* CALL DDNAME SEARCH         */ 00196000
*                     (DSABPTR,DDNAMEP,ADDR(RTNADDR));          02S0092 00197000
         LA    @10,DSABPTR                                      02S0092 00198000
         ST    @10,@AL00001                                     02S0092 00199000
         LA    @10,DDNAMEP                                      02S0092 00200000
         ST    @10,@AL00001+4                                   02S0092 00201000
         LA    @10,RTNADDR                                      02S0092 00202000
         ST    @10,@TF00001                                     02S0092 00203000
         LA    @10,@TF00001                                     02S0092 00204000
         ST    @10,@AL00001+8                                   02S0092 00205000
         L     @15,@CV00124                                     02S0092 00206000
         LA    @01,@AL00001                                     02S0092 00207000
         BALR  @14,@15                                          02S0092 00208000
*        IF RTNADDR = ZERO THEN        /* WAS DDNAME FOUND IN DSABS ?*/ 00209000
         L     @10,RTNADDR                                      02S0093 00210000
         LTR   @10,@10                                          02S0093 00211000
         BNZ   @RF00093                                         02S0093 00212000
*          ERRCODE = RCDDNFND;         /* NO, SET DD NOT FND ERR CODE*/ 00213000
         L     @10,@PC00001+4                                   02S0094 00214000
         MVC   ERRCODE(2,@10),@CB01355                          02S0094 00215000
*        ELSE                          /* YES, DDNAME WAS FOUND      */ 00216000
*          DSABPTR = RTNADDR;          /* POINT TO PROPER DSAB       */ 00217000
         B     @RC00093                                         02S0095 00218000
@RF00093 L     @10,RTNADDR                                      02S0095 00219000
         ST    @10,DSABPTR                                      02S0095 00220000
*      END;                            /* END DDNAME SEARCH          */ 00221000
*    ELSE;                             /* FALL THROUGH-DDNAME NOT       00222000
*                                         SPECIFIED                  */ 00223000
@RF00085 DS    0H                                               02S0098 00224000
*    IF DSNAMTUP ^= ZERO THEN          /* IS DSNAME TEXT UNIT PRESENT*/ 00225000
@RC00085 L     @10,DSNAMTUP                                     02S0098 00226000
         LTR   @10,@10                                          02S0098 00227000
         BZ    @RF00098                                         02S0098 00228000
*      DO;                             /* YES,SEARCH BY DSNAME       */ 00229000
*        RESPECIFY S99TUNIT BASED(DSNAMTUP);                    02S0100 00230000
*        IF S99TUPAR(1:2) = '&&' THEN  /* IS DSN OF FORM,&&NAME      */ 00231000
         CLC   S99TUPAR(2,@10),@CC01704                         02S0101 00232000
         BNE   @RF00101                                         02S0101 00233000
*          DO;                         /* YES, MAKE TU OF FORM,&NAME */ 00234000
*            DSNAMKEY = S99TUKEY;      /* MOVE IN DSNAME KEY         */ 00235000
         MVC   DSNAMKEY(2),S99TUKEY(@10)                        02S0103 00236000
*            DSNAMNUM = S99TUNUM;      /* MOVE IN NUMBER OF PARMS    */ 00237000
         MVC   DSNAMNUM(2),S99TUNUM(@10)                        02S0104 00238000
