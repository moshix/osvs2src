         ST    @10,DSABPTR                                      02S0117 00292000
*      END;                            /* END DSNAME TEXT UNIT PRESENT  00293000
*                                         PROCESSING                 */ 00294000
*    ELSE;                             /* FALL THROUGH - DSNAME NOT     00295000
*                                         SPECIFIED                  */ 00296000
@RF00098 DS    0H                                               02S0120 00297000
*    IF RELNOTUP ^= ZERO THEN          /* IS RELATIVE # TEXT PRESENT */ 00298000
@RC00098 L     @10,RELNOTUP                                     02S0120 00299000
         SLR   @15,@15                                          02S0120 00300000
         CR    @10,@15                                          02S0120 00301000
         BE    @RF00120                                         02S0120 00302000
*      DO;                             /* YES, SEARCH BY RELATIVE #  */ 00303000
*        TEXTPTR = RELNOTUP;           /* GET ADDRESS OF TEXT UNIT   */ 00304000
         LR    TEXTPTR,@10                                      02S0122 00305000
*        IF TEXTUPAR(1:2) <= ZERO | DSABPTR = ZERO THEN /* IS RELATIVE  00306000
*                                       # <= 0 OR NO DSABS PRESENT ? */ 00307000
         SLR   @10,@10                                          02S0123 00308000
         ICM   @10,3,TEXTUPAR(TEXTPTR)                          02S0123 00309000
         CR    @15,@10                                          02S0123 00310000
         BNL   @RT00123                                         02S0123 00311000
         C     @15,DSABPTR                                      02S0123 00312000
         BNE   @RF00123                                         02S0123 00313000
@RT00123 DS    0H                                               02S0124 00314000
*          ERRCODE = RCRELNNF;         /* YES SET ERRCODE            */ 00315000
         L     @10,@PC00001+4                                   02S0124 00316000
         MVC   ERRCODE(2,@10),@CB01361                          02S0124 00317000
*        CONTINUE = ON;                /* INITIALIZE SWITCH          */ 00318000
@RF00123 OI    CONTINUE,B'10000000'                             02S0125 00319000
*        DSABNBR = 1;                  /* SET DSAB COUNT TO 1        */ 00320000
         LA    DSABNBR,1                                        02S0126 00321000
*        DO WHILE (ERRCODE = ZERO & CONTINUE = ON); /* LOOP CONTROL  */ 00322000
         B     @DE00127                                         02S0127 00323000
@DL00127 DS    0H                                               02S0128 00324000
*          IF DSABNBR = TEXTUPAR(1:2) THEN /*WAS RELATIVE DSAB FOUND?*/ 00325000
         SLR   @10,@10                                          02S0128 00326000
         ICM   @10,3,TEXTUPAR(TEXTPTR)                          02S0128 00327000
         CR    DSABNBR,@10                                      02S0128 00328000
         BNE   @RF00128                                         02S0128 00329000
*            CONTINUE = OFF;           /* YES, STOP PROCESSING       */ 00330000
         NI    CONTINUE,B'01111111'                             02S0129 00331000
*          ELSE                        /* OTHERWISE, CONTINUE SEARCH */ 00332000
*            DO;                       /* CONTINUE DSAB SEARCH       */ 00333000
         B     @RC00128                                         02S0130 00334000
@RF00128 DS    0H                                               02S0131 00335000
*              IF DSABFCHN ^= ZERO THEN /* END OF DSAB CHAIN ?       */ 00336000
         L     @10,DSABPTR                                      02S0131 00337000
         L     @10,DSABFCHN(,@10)                               02S0131 00338000
         LTR   @10,@10                                          02S0131 00339000
         BZ    @RF00131                                         02S0131 00340000
*                DO;                   /* NO, UPDATE POINTER & COUNT */ 00341000
*                  DSABPTR = DSABFCHN; /* UPDATE DSABPTR             */ 00342000
         ST    @10,DSABPTR                                      02S0133 00343000
*                  DSABNBR = DSABNBR + 1; /* INCREMENT DSAB COUNT    */ 00344000
         AL    DSABNBR,@CF00084                                 02S0134 00345000
*                END;                  /* END OF PTR UPDATE          */ 00346000
*              ELSE                    /* OTHERWISE, END OF DSAB CHN */ 00347000
*                ERRCODE = RCRELNNF;   /* SET DSAB NOT FOUND ERRCODE */ 00348000
         B     @RC00131                                         02S0136 00349000
@RF00131 L     @10,@PC00001+4                                   02S0136 00350000
         MVC   ERRCODE(2,@10),@CB01361                          02S0136 00351000
*            END;                      /* END DSAB SEARCH            */ 00352000
@RC00131 DS    0H                                               02S0138 00353000
*        END;                          /* END OF LOOP CONTROL        */ 00354000
@RC00128 DS    0H                                               02S0138 00355000
@DE00127 L     @10,@PC00001+4                                   02S0138 00356000
