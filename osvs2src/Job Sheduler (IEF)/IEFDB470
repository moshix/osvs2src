         L     @15,@CV00126                                     02S0075 00083000
         LA    @01,@AL00001                                     02S0075 00084000
         BALR  @14,@15                                          02S0075 00085000
*IF ERRCODE = ZERO THEN                /* WAS SYNTAX OK ?            */ 00086000
         SLR   @10,@10                                          02S0076 00087000
         L     @15,@PC00001+4                                   02S0076 00088000
         SLR   @14,@14                                          02S0076 00089000
         ICM   @14,3,ERRCODE(@15)                               02S0076 00090000
         CR    @10,@14                                          02S0076 00091000
         BNE   @RF00076                                         02S0076 00092000
*  DO;                                 /* YES                        */ 00093000
*    IF DDNAMTUP = ZERO & DSNAMTUP = ZERO & RELNOTUP = ZERO THEN        00094000
         C     @10,DDNAMTUP                                     02S0078 00095000
         BNE   @RF00078                                         02S0078 00096000
