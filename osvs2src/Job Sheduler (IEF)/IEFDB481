         L     @10,@PC00001+20                                  01S0089 00205000
         ST    @10,@AL00001+24                                  01S0089 00206000
         L     @10,@PC00001+24                                  01S0089 00207000
         ST    @10,@AL00001+28                                  01S0089 00208000
         L     @15,@CV00075                                     01S0089 00209000
         LA    @01,@AL00001                                     01S0089 00210000
         BALR  @14,@15                                          01S0089 00211000
*                                      /*UNALLOC PROCESSING  @ZA02706*/ 00212000
*  END;                                /*END DSABS FOR UNALLOC       */ 00213000
*RETURN CODE(0);                       /*RETURN TO INVOKER           */ 00214000
@RF00085 L     @13,4(,@13)                                      01S0091 00215000
         L     @00,@SIZDATD                                     01S0091 00216000
         LR    @01,@11                                          01S0091 00217000
         FSPACE   R,LV=(0),A=(1)                                        00218000
         SLR   @15,@15                                          01S0091 00219000
         L     @14,12(,@13)                                     01S0091 00220000
         LM    @00,@12,20(@13)                                  01S0091 00221000
         BR    @14                                              01S0091 00222000
*/********************************************************************/ 00223000
*/*                                                                  */ 00224000
*/* SEGMENT NAME - DSABCHNR                                          */ 00225000
*/*                                                                  */ 00226000
*/* DESCRIPTIVE NAME - DSAB CHAINER SUBRTN                           */ 00227000
*/*                                                                  */ 00228000
*/* FUNCTION  - MARK DSABS NOT IN-USE AND RECHAIN THEM AT            */ 00229000
*/*             THE END OF THE DSAB CHAIN                            */ 00230000
*/*                                                                  */ 00231000
*/* OPERATION - THE INPUT DSAB IS MARKED NOT IN-USE. IF THE DSAB IS A*/ 00232000
*/*             MEMBER OF A CONCATENATED GROUP, THE LAST MEMBER OF   */ 00233000
*/*             THE GROUP IS DETERMINED, ITS ADDRESS PLACED IN       */ 00234000
*/*             DSABLAST, AND ALL MEMBERS OF THE GROUP ARE ALSO      */ 00235000
*/*             MARKED NOT IN-USE. (IF THE DSAB IS NOT CONCATENATED, */ 00236000
*/*             DSABLAST IS SET TO THE ADDRESS OF THE DSAB ITSELF.)  */ 00237000
*/*             IF THEY ARE NOT ALREADY LOCATED AT THE END OF THE    */ 00238000
*/*             CHAIN, THE DSABS FROM THE INPUT DSAB TO DSABLAST ARE */ 00239000
*/*             THEN MOVED TO THE END OF THE CHAIN IN THE FOLLOWING  */ 00240000
*/*             STEPS:                                               */ 00241000
*/*             - DSQBADFC IN THE DSABQDB IS SET SAYING THAT THE DSAB*/ 00242000
*/*               FORWARD CHAIN PTRS ARE NO LONGER VALID. IF THE     */ 00243000
*/*               ESTAE EXIT RTN (IEFDB402) GETS CONTROL, IT WILL    */ 00244000
*/*               USE THIS BIT, RECONSTRUCTING THE DSAB CHAIN FROM   */ 00245000
*/*               BACKWARD PTRS WHEN THIS BIT IS ON.                 */ 00246000
*/*             - THE NECESSARY FORWARD CHAIN PTRS ARE UPDATED,      */ 00247000
*/*               INCLUDING DSQFRSTP IF REQUIRED.                    */ 00248000
*/*             - DSQBADBC IN THE DSABQDB IS SET SAYING THAT THE     */ 00249000
*/*               DSAB BACKWARD CHAIN PTRS ARE NO LONGER VALID. THE  */ 00250000
*/*               ESTAE EXIT RTN (IEFDB402) WILL RECONSTRUCT THE     */ 00251000
*/*               DSAB CHAIN USING FORWARD PTRS IF THIS BIT IS ON    */ 00252000
*/*               AND DSQBADFC IS OFF. DSQBADFC IS THEN TURNED OFF.  */ 00253000
*/*             - THE NECESSARY BACKWARD CHAIN PTRS ARE UPDATED,     */ 00254000
*/*               INCLUDING DSQLASTP,AND DSQBADBC IS THEN TURNED OFF.*/ 00255000
*/*                                                                  */ 00256000
*/* ENTRY -                                                          */ 00257000
*/*   INPUT DATA - DSAB TO BE PROCESSED                              */ 00258000
*/*              - DSAB QBD                                          */ 00259000
*/*                                                                  */ 00260000
*/* EXIT -                                                           */ 00261000
*/*   OUTPUT DATA - DSABNUSE BIT SET OFF (FOR ALL MEMBERS OF A       */ 00262000
*/*                 CONCATENATED GROUP)                              */ 00263000
*/*               - DSAB(S) MOVED TO END OF CHAIN AND NECESSARY      */ 00264000
*/*                 PTRS IN DSABS AND QBD UPDATED TO REFLECT THIS    */ 00265000
*/*                 RECHAINING.                                      */ 00266000
*/*                                                                  */ 00267000
*/********************************************************************/ 00268000
*DSABCHNR: PROC;                                                02S0092 00269000
DSABCHNR STM   @14,@12,12(@13)                                  02S0092 00270000
*DSABNUSE = '0'B;                      /*SET DSAB NOT IN-USE         */ 00271000
         NI    DSABNUSE(DSABPTR),B'11111110'                    02S0093 00272000
*LASTDSAB = DSABPTR;                   /*INIT PTR TO LAST CONCAT DSAB*/ 00273000
         LR    LASTDSAB,DSABPTR                                 02S0094 00274000
*RFY DSAB BASED(LASTDSAB);             /*RESET DSAB BASE PTR         */ 00275000
*IF DSABCATM = '1'B THEN               /*IF DSAB IS IN CONCAT GROUP  */ 00276000
         TM    DSABCATM(LASTDSAB),B'00000010'                   02S0096 00277000
         BNO   @RF00096                                         02S0096 00278000
*  DO WHILE (DSABFCHN ^= 0   &         /*PROCESS TO END OF CHAIN OR  */ 00279000
*           DSABFCHN->DSABTIOT->TIOEDDNM(1) = ' ');             02S0097 00280000
         B     @DE00097                                         02S0097 00281000
@DL00097 DS    0H                                               02S0098 00282000
*                                      /*TO END OF CONCAT GROUP      */ 00283000
*    LASTDSAB = LASTDSAB->DSABFCHN;    /*UPDATE LAST CONCAT DSABPTR  */ 00284000
         L     LASTDSAB,DSABFCHN(,LASTDSAB)                     02S0098 00285000
*    DSABNUSE = '0'B;                  /*SET THIS DSAB NOT IN-USE    */ 00286000
         NI    DSABNUSE(LASTDSAB),B'11111110'                   02S0099 00287000
*  END;                                /*END CONCAT DSAB PROCESS     */ 00288000
@DE00097 L     @10,DSABFCHN(,LASTDSAB)                          02S0100 00289000
         LTR   @10,@10                                          02S0100 00290000
         BZ    @DC00097                                         02S0100 00291000
         L     @10,DSABTIOT(,@10)                               02S0100 00292000
         CLI   TIOEDDNM(@10),C' '                               02S0100 00293000
         BE    @DL00097                                         02S0100 00294000
@DC00097 DS    0H                                               02S0101 00295000
*ELSE;                                 /*DSAB IS NOT CONCATENATED    */ 00296000
@RF00096 DS    0H                                               02S0102 00297000
*RFY DSAB BASED(DSABPTR);              /*RESET DSAB BASE PTR         */ 00298000
*IF LASTDSAB->DSABFCHN ^= 0 THEN       /*IF DSAB (OR GROUP) IS NOT AT   00299000
*                                        END OF CHAIN ALREADY        */ 00300000
         SLR   @10,@10                                          02S0103 00301000
         L     @15,DSABFCHN(,LASTDSAB)                          02S0103 00302000
         CR    @15,@10                                          02S0103 00303000
         BE    @RF00103                                         02S0103 00304000
*  DO;                                 /*MOVE TO END OF DSAB CHAIN   */ 00305000
*    CHNRSAVP = LASTDSAB -> DSABFCHN;  /*SAVE PTR TO DSAB AFTER LAST    00306000
*                                        DSAB BEING MOVED            */ 00307000
         LR    CHNRSAVP,@15                                     02S0105 00308000
*    DSQBADFC = '1'B;                  /*SAY BAD FORWARD CHAIN PTRS  */ 00309000
         L     @05,@PC00001+16                                  02S0106 00310000
         L     @05,JSCBPTR(,@05)                                02S0106 00311000
         L     @05,JSCDSABQ(,@05)                               02S0106 00312000
         OI    DSQBADFC(@05),B'10000000'                        02S0106 00313000
*    IF DSABBCHN = 0 THEN              /*IF DSAB IS 1ST ON CHAIN     */ 00314000
         C     @10,DSABBCHN(,DSABPTR)                           02S0107 00315000
         BNE   @RF00107                                         02S0107 00316000
*      DSQFRSTP = LASTDSAB->DSABFCHN;  /*UPDATE NEW 1ST DSAB QDB PTR */ 00317000
         ST    @15,DSQFRSTP(,@05)                               02S0108 00318000
*    ELSE                              /*DSAB IS NOT 1ST ON CHAIN    */ 00319000
*      DSABBCHN->DSABFCHN = LASTDSAB->DSABFCHN; /*UPDATE FORWARD PTR    00320000
*                                        OF PRECEEDING DSAB ON CHAIN */ 00321000
         B     @RC00107                                         02S0109 00322000
@RF00107 L     @10,DSABBCHN(,DSABPTR)                           02S0109 00323000
         L     @05,DSABFCHN(,LASTDSAB)                          02S0109 00324000
         ST    @05,DSABFCHN(,@10)                               02S0109 00325000
*    DSQLASTP->DSABFCHN = DSABPTR;     /*UPDATE FORWARD PTR OF CURRENT  00326000
*                                        LAST DSAB ON CHAIN          */ 00327000
@RC00107 L     @10,@PC00001+16                                  02S0110 00328000
         L     @10,JSCBPTR(,@10)                                02S0110 00329000
         L     @10,JSCDSABQ(,@10)                               02S0110 00330000
         L     @05,DSQLASTP(,@10)                               02S0110 00331000
         ST    DSABPTR,DSABFCHN(,@05)                           02S0110 00332000
*    LASTDSAB->DSABFCHN = 0;           /*NEW LAST DSAB ON CHAIN HAS     00333000
*                                        FORWARD PTR OF ZERO         */ 00334000
         SLR   @05,@05                                          02S0111 00335000
         ST    @05,DSABFCHN(,LASTDSAB)                          02S0111 00336000
*    DSQBADBC = '1'B;                  /*SAY BAD BACKWARD CHAIN PTRS */ 00337000
         OI    DSQBADBC(@10),B'10000000'                        02S0112 00338000
*    DSQBADFC = '0'B;                  /*FORWARD PTRS ARE GOOD NOW   */ 00339000
         NI    DSQBADFC(@10),B'01111111'                        02S0113 00340000
*    CHNRSAVP->DSABBCHN = DSABBCHN;    /*UPDATE BACK PTR OF DSAB NEXT   00341000
*                                        AFTER LAST ONE MOVED        */ 00342000
         L     @05,DSABBCHN(,DSABPTR)                           02S0114 00343000
         ST    @05,DSABBCHN(,CHNRSAVP)                          02S0114 00344000
*    DSABBCHN = DSQLASTP;              /*NEW BACK PTR FOR 1ST DSAB      00345000
*                                        MOVED TO END OF CHAIN       */ 00346000
         L     @15,DSQLASTP(,@10)                               02S0115 00347000
         ST    @15,DSABBCHN(,DSABPTR)                           02S0115 00348000
*    DSQLASTP = LASTDSAB;              /*UPDATE NEW QDB LAST DSAB PTR*/ 00349000
         ST    LASTDSAB,DSQLASTP(,@10)                          02S0116 00350000
*    DSQBADBC = '0'B;                  /*DSAB BACK PTRS ARE GOOD NOW */ 00351000
         NI    DSQBADBC(@10),B'01111111'                        02S0117 00352000
*  END;                                /*END DSAB CHAINING PROCESS   */ 00353000
