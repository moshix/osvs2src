*/********************************************************************/ 00088000
*/*                                                                  */ 00089000
*/* SEGMENT NAME - DISPPRCR                                          */ 00090000
*/*                                                                  */ 00091000
*/* DESCRIPTIVE NAME - CONDITIONAL DISPOSITION REMOVAL               */ 00092000
*/*                                                                  */ 00093000
*/* FUNCTION  - REMOVE CONDITIONAL DISPOSITION FROM A SIOT WHEN      */ 00094000
*/*             NECESSARY, AND CREATE AND ENCHAIN A WRITE EPA FOR THE*/ 00095000
*/*             UPDATED SIOT.                                        */ 00096000
*/*                                                                  */ 00097000
*/* OPERATION - A CHECK IS MADE TO ENSURE THAT A CONDITIONAL         */ 00098000
*/*             DISPOSITION WAS SPECIFIED ON THE ORIGINAL ALLOCATION */ 00099000
*/*             REQUEST. IF ONE WAS NOT, NO PROCESSING IS NECESSARY. */ 00100000
*/*             IF ONE WAS, IT IS CHECKED AGAINST THE NORMAL DISP TO */ 00101000
*/*             SEE IF THEY ARE DIFFERENT. IF NOT DIFFERENT, NO      */ 00102000
*/*             REMOVAL OF COND. DISP IS NECESSARY AS THE CORRECT    */ 00103000
*/*             NORMAL DISP PROCESSING WILL TAKE PLACE, EVEN IN CASE */ 00104000
*/*             OF ABEND. HOWEVER IF THEY ARE DIFFERENT, THE         */ 00105000
*/*             CONDITIONAL DISPOSITION FIELD OF THE SIOT IS CLEARED */ 00106000
*/*             AND A GETMAIN IS ISSUED FOR CORE FOR AN EPA. THIS EPA*/ 00107000
*/*             IS THEN FILLED IN APPROPRIATELY AND PLACED ON A CHAIN*/ 00108000
*/*             OF EPA'S FOR UPDATED SWA CONTROL BLOCKS TO BE WRITTEN*/ 00109000
*/* ENTRY -                                                          */ 00110000
*/*   INPUT  - SIOT TO BE PROCESSED                                  */ 00111000
*/*          - PTR TO LAST EPA ON CHAIN                              */ 00112000
*/*                                                                  */ 00113000
*/* EXIT -                                                           */ 00114000
*/*   OUTPUT - SIOTALTD CLEARED  IF NECESSARY                        */ 00115000
*/*          - SWAEPA CREATED AND CHAINED IF SIOTALTD WAS CLEARED    */ 00116000
*/*          - LASTEPAP UPDATED IF EPA WAS CREATED                   */ 00117000
*/*                                                                  */ 00118000
*/********************************************************************/ 00119000
*RFY SWAEPA BASED(LASTEPAP);           /*RESET BASE OF EPA MAP       */ 00120000
*IF SIOTALTD(5:8) ^= '0000'B THEN      /*WAS ALT. DISP SPECD AT ALLOC*/ 00121000
         L     @10,DSABSIOT(,DSABPTR)                           02S0060 00122000
         TM    SIOTALTD(@10),B'00001111'                        02S0060 00123000
         BZ    @RF00060                                         02S0060 00124000
*  DO;                                 /*YES,COMPARE WITH NORMAL DISP*/ 00125000
*    WRKBITS=SCTSDISP && SIOTALTD;     /*SET BITS THAT ARE DIFFERENT */ 00126000
         MVC   WRKBITS(1),SCTSDISP(@10)                         02S0062 00127000
         XC    WRKBITS(1),SIOTALTD(@10)                         02S0062 00128000
*    IF WRKBITS(5:8) ^='0000'B THEN    /*ARE NORMAL & COND DISPS DIFF*/ 00129000
         TM    WRKBITS,B'00001111'                              02S0063 00130000
         BZ    @RF00063                                         02S0063 00131000
*      DO;                             /*YES,REMOVE CONDITIONAL DISP */ 00132000
*        SIOTALTD(5:8)='0000'B;        /*CLEAR COND. DISP IN THE SIOT*/ 00133000
         NI    SIOTALTD(@10),B'11110000'                        02S0065 00134000
*        R0=LENGTH(SWAEPA);            /*SIZE OF EPA FROM            */ 00135000
         LA    R0,16                                            02S0066 00136000
*        R0 = R0 | SCHDSUBP;           /*GET FROM CORRECT SUBPOOL    */ 00137000
         O     R0,@CF00072                                      02S0067 00138000
*        GEN (GETMAIN R,LV=(0))        /*GET CORE FOR EPA            */ 00139000
*             SETS(R1) REFS(R0);                                02S0068 00140000
         GETMAIN R,LV=(0)                                               00141000
*        SWCHNPTR=R1;                  /*CHAIN INTO LAST EPA         */ 00142000
         L     @10,@PC00001+12                                  02S0069 00143000
         L     @05,LASTEPAP(,@10)                               02S0069 00144000
         ST    R1,SWCHNPTR(,@05)                                02S0069 00145000
*        LASTEPAP=SWCHNPTR;            /*UPDATE FOR NEW LAST EPA     */ 00146000
         L     @05,@PC00001+12                                  02S0070 00147000
         L     @05,LASTEPAP(,@05)                               02S0070 00148000
         L     @05,SWCHNPTR(,@05)                               02S0070 00149000
         ST    @05,LASTEPAP(,@10)                               02S0070 00150000
*        SWAEPA=SWAEPA && SWAEPA;      /*ZERO OUT NEW EPA            */ 00151000
         XC    SWAEPA(16,@05),SWAEPA(@05)                       02S0071 00152000
*        SWBLKPTR=DSABSIOT;            /*POINT TO UPDATED SIOT       */ 00153000
         L     @10,DSABSIOT(,DSABPTR)                           02S0072 00154000
         ST    @10,SWBLKPTR(,@05)                               02S0072 00155000
*        SWVA=SIOTDSKA;                /*PUT IN SIOT'S SVA           */ 00156000
         MVC   SWVA(3,@05),SIOTDSKA(@10)                        02S0073 00157000
*        SWBLKID=3;                    /*PUT IN BLOCK ID OF SIOT     */ 00158000
         MVI   SWBLKID(@05),X'03'                               02S0074 00159000
*        SWLNGTH=176;                  /*PUT IN LENGTH OF SIOT BLK   */ 00160000
         MVC   SWLNGTH(4,@05),@CF00585                          02S0075 00161000
*      END;                            /*END COND. DISP PROCESS      */ 00162000
*    ELSE;                             /*NORMAL AND COND. DISPS SAME */ 00163000
@RF00063 DS    0H                                               02S0078 00164000
*  END;                                /*END ALT. DISP SPECIFIED     */ 00165000
*ELSE;                                 /*COND. DISP NOT SPECIFIED    */ 00166000
@RF00060 DS    0H                                               02S0080 00167000
*RFY SWAEPA BASED(SWEPAPTR);           /*RESET BASE OF EPA MAP       */ 00168000
*        CALL DSABCHNR;                /*IN-USE BIT OFF & RECHAIN    */ 00169000
         BAL   @14,DSABCHNR                                     01S0081 00170000
*      END;                            /*END NORMAL PROCESS FOR DSAB */ 00171000
*  RLSTINDX=RLSTINDX+1;                /*INDEX TO NEXT DSAB IN LIST  */ 00172000
@RC00051 DS    0H                                               01S0083 00173000
@RC00049 AL    RLSTINDX,@CF00115                                01S0083 00174000
*END;                                  /*END INPUT LIST PROCESSING   */ 00175000
@DE00047 L     @10,@PC00001+4                                   01S0084 00176000
         C     RLSTINDX,RDSABCNT(,@10)                          01S0084 00177000
         BNH   @DL00047                                         01S0084 00178000
*IF ULSTINDX > 0 THEN                  /*ANY DSABS FOR UNALLOC ?     */ 00179000
         L     @10,ULSTINDX                                     01S0085 00180000
         LTR   @10,@10                                          01S0085 00181000
         BNP   @RF00085                                         01S0085 00182000
*  DO;                                 /*YES, PROCESS THEM           */ 00183000
*    UNALKEYT = UNALKEYT && UNALKEYT;  /*ZEROS FOR DUMMY UNALKEY TAB */ 00184000
         XC    UNALKEYT(40),UNALKEYT                            01S0087 00185000
*    DUMRSNCD = DUMRSNCD && DUMRSNCD;  /*ZERO OUT DUMMY RSNCODE 01S0088 00186000
*                                        UNALLOC                     */ 00187000
         XC    DUMRSNCD(4),DUMRSNCD                             01S0088 00188000
*    CALL IEFDB4A1 (RDSABSP,ULSTINDX,DUMRSNCD,LASTEPAP,         01S0089 00189000
*                   CTLBLKPS,ADDR(UNALKEYT),DEPARMS,SMFPARMS);  01S0089 00190000
         L     @10,@PC00001                                     01S0089 00191000
         ST    @10,@AL00001                                     01S0089 00192000
         LA    @10,ULSTINDX                                     01S0089 00193000
         ST    @10,@AL00001+4                                   01S0089 00194000
         LA    @10,DUMRSNCD                                     01S0089 00195000
         ST    @10,@AL00001+8                                   01S0089 00196000
