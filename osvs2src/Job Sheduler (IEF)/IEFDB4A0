         BNE   @RF00113                                         02S0113 00136000
*  ERRCODE = RCKEYREQ;                 /*THEN REQUIRED KEY MISSING   */ 00137000
*                                                               02S0114 00138000
         MVC   ERRCODE(2,@15),@CH01952                          02S0114 00139000
*IF (DDNAMTUP ^= 0 &                   /*IF DDNAME WAS SPEC'D AND    */ 00140000
*    ERRCODE = 0 ) THEN                /*IF NO ERROR THEN            */ 00141000
@RF00113 L     @10,DDNAMTUP                                     02S0115 00142000
         SLR   @15,@15                                          02S0115 00143000
         CR    @10,@15                                          02S0115 00144000
         BE    @RF00115                                         02S0115 00145000
         L     @14,@PC00001+4                                   02S0115 00146000
         CH    @15,ERRCODE(,@14)                                02S0115 00147000
         BNE   @RF00115                                         02S0115 00148000
*  DO;                                 /*CHECK DDNAME                */ 00149000
*    RFY TEXTUNIT BASED(DDNAMTUP);     /*RESET TEXT UNIT BASE        */ 00150000
*    IF (TEXTULNG >= 6 & (TEXTUPAR(1:TEXTULNG) = 'JOBLIB  ' |   02S0118 00151000
*                          TEXTUPAR(1:TEXTULNG) = 'JOBCAT  '))  |       00152000
