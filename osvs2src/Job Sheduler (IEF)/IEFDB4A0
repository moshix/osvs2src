         BE    @RF00166                                         02S0166 00368000
@RT00166 DS    0H                                               02S0167 00369000
*        ERRCODE = RCSSDSER;           /*UNABLE TO PROCESS REQ. ERROR*/ 00370000
         L     @10,@PC00001+4                                   02S0167 00371000
         MVC   ERRCODE(2,@10),@CH02004                          02S0167 00372000
*      ELSE;                           /*OVERRIDING DEST IS VALID    */ 00373000
@RF00166 DS    0H                                               02S0169 00374000
*    END;                              /*END SYSOUT DEST VERIFY      */ 00375000
@RC00164 DS    0H                                               02S0170 00376000
*IF ERRCODE = 0 THEN                   /*IF NO VALIDITY ERROR        */ 00377000
@RF00151 SLR   @10,@10                                          01S0170 00378000
         L     @15,@PC00001+4                                   01S0170 00379000
         CH    @10,ERRCODE(,@15)                                01S0170 00380000
         BNE   @RF00170                                         01S0170 00381000
*  DO;                                 /*CONTINUE PROCESSING         */ 00382000
*    DSABPTR = 0;                      /*INIT SEARCH RTNS. RETURN PTR*/ 00383000
         ST    @10,DSABPTR                                      01S0172 00384000
*    DSABCNT = 0;                      /*INIT GETMAIN DSAB CNT       */ 00385000
         LR    DSABCNT,@10                                      01S0173 00386000
*    UNALCNT = 0;                      /*INIT COUNT OF UNALLOC DSABS */ 00387000
         ST    @10,UNALCNT                                      01S0174 00388000
*    RIUCNT = 0;                       /*INIT COUNT OF RIU DSABS     */ 00389000
         ST    @10,RIUCNT                                       01S0175 00390000
*    RFY TEXTUNIT BASED(DSNAMTUP);     /*RESET TEXT UNIT BASE        */ 00391000
*                                                               01S0176 00392000
*    IF DSNAMTUP ^= 0  &               /*IF DSN WAS SPECIFIED        */ 00393000
*       TEXTUPAR(1:2) =  '&&' THEN     /*AND BEGINS WITH &&          */ 00394000
         L     @05,DSNAMTUP                                     01S0177 00395000
         CR    @05,@10                                          01S0177 00396000
         BE    @RF00177                                         01S0177 00397000
         CLC   TEXTUPAR(2,@05),@CC02164                         01S0177 00398000
         BNE   @RF00177                                         01S0177 00399000
*      DO;                             /*STRIP OFF 1ST AMPERSAND     */ 00400000
*        AMPDSNTU(1:4) = TEXTUNIT(1:4); /*COPY TEXT KEY-NUMBER FIELDS*/ 00401000
         MVC   AMPDSNTU(4),TEXTUNIT(@05)                        01S0179 00402000
*        AMPDSNLN = TEXTULNG-1;        /*COPY LENGTH LESS ONE        */ 00403000
         ICM   @10,12,TEXTULNG(@05)                             01S0180 00404000
         SRA   @10,16                                           01S0180 00405000
         BCTR  @10,0                                            01S0180 00406000
         STCM  @10,3,AMPDSNLN                                   01S0180 00407000
*        AMPDSNPM(1:AMPDSNLN) = TEXTUPAR(2:TEXTULNG);           01S0181 00408000
         BCTR  @10,0                                            01S0181 00409000
         EX    @10,@SM02190                                     01S0181 00410000
*                                      /*COPY DSN WITH 1 AMPERSAND   */ 00411000
*        DSNAMTUP = ADDR(AMPDSNTU);    /*PUT DUMMY TUP INTO KEY TABLE*/ 00412000
         LA    @10,AMPDSNTU                                     01S0182 00413000
         ST    @10,DSNAMTUP                                     01S0182 00414000
*      END;                            /*END &&DSN PROCESS           */ 00415000
*                                                               01S0183 00416000
*    IF DDNAMTUP ^= 0 THEN             /*IF DDNAME WAS SPECIFIED     */ 00417000
@RF00177 L     @10,DDNAMTUP                                     01S0184 00418000
         LTR   @10,@10                                          01S0184 00419000
         BZ    @RF00184                                         01S0184 00420000
*      DO;                             /*SEARCH FOR SPEC'D DDN       */ 00421000
*/********************************************************************/ 00422000
*/*                                                                  */ 00423000
*/* SEGMENT NAME - DDNPRCSS                                          */ 00424000
*/*                                                                  */ 00425000
*/* DESCRIPTIVE NAME - DDNAME PROCESS                                */ 00426000
*/*                                                                  */ 00427000
*/* FUNCTION  - BUILD DSAB PTR LIST FOR DYNAMIC UNALLOCATION         */ 00428000
*/*             REQUESTS VIA DDNAME                                  */ 00429000
*/*                                                                  */ 00430000
*/* OPERATION - THE DDNAME SEARCH RTN (IEFDB4FC) IS INVOKED TO SEARCH*/ 00431000
*/*             FOR THE DSAB ASSOCIATED WITH THE SPECIFIED DDNAME.   */ 00432000
*/*             IF IT IS NOT FOUND, A CODE OF RCDDNFND IS PLACED IN  */ 00433000
*/*             ERRCODE. IF THE DSAB IS FOUND, CHECKS ARE MADE TO    */ 00434000
*/*             ENSURE THAT IF DSN AND MEMBER NAME WERE ALSO         */ 00435000
*/*             SPECIFIED, THEY MATCH THAT OF THE RETURNED DSAB, AND */ 00436000
*/*             THAT THE DSAB IS NOT FOR A QNAME OR FOR A TERMINAL.  */ 00437000
*/*             OTHERWISE, ERRCODE IS SET TO RCNOUNAL. IF            */ 00438000
*/*             THE RETURNED DSAB IS CORRECT, IT IS DETERMINED       */ 00439000
*/*             WHETHER THE DSAB IS FOR UNALLOCATION OR FOR REMOVE   */ 00440000
*/*             IN-USE PROCESSING AS FOLLOWS:                        */ 00441000
*/*                                                                  */ 00442000
*/*           . IF THE UNALLOC OPTION WAS SPECIFIED, THE DSAB        */ 00443000
*/*             IS FOR UNALLOCATION                                  */ 00444000
*/*           . IF THE REMOVE IN-USE OPTION WAS SPECIFIED BUT THE    */ 00445000
*/*             DATA SET IS NOT PERMANENTLY ALLOCATED, HAS A         */ 00446000
*/*             NON-&DSN, AND HAS A DISP OF DELETE, THE DSAB         */ 00447000
*/*             IS FOR UNALLOCATION.                                 */ 00448000
*/*           . IF NEITHER OPTION WAS SPECIFIED AND THE DATA SET     */ 00449000
*/*             IS NOT PERMANENTLY ALLOCATED, THE DSAB IS FOR        */ 00450000
*/*             UNALLOCATION.                                        */ 00451000
*/*           . OTHERWISE, THE DSAB IS FOR REMOVE IN-USE (IF THE     */ 00452000
*/*             DSAB IS ALREADY NOT IN-USE OR FOR A PRIVATE          */ 00453000
*/*             CATALOG, REMOVE IN-USE PROCESSING WILL BE            */ 00454000
*/*             BYPASSED)                                            */ 00455000
*/*                                                                  */ 00456000
*/*             THE ADDRESS OF THE DSAB IS THEN PLACED IN THE        */ 00457000
*/*             APPROPRIATE LIST AND THE CORRESPONDING COUNT IS      */ 00458000
*/*             SET TO ONE  (ONLY ONE DSAB WILL BE IN THE LIST FOR   */ 00459000
*/*             A REQUEST VIA DDNAME.)                               */ 00460000
*/*                                                                  */ 00461000
*/* ENTRY -                                                          */ 00462000
*/*   INPUT DATA - DSQFRSTP - PTR TO FIRST DSAB ON CHAIN             */ 00463000
*/*                DSABPTR  - PTR TO DSAB TO BE PROCESSED            */ 00464000
*/*                           (ZERO ON ENTRY)                        */ 00465000
*/*                ERRCODE  - ERROR CODE FIELD - ZERO ON ENTRY       */ 00466000
*/*                UNALKEYT - KEY TABLE OF TEXT UNIT PTRS            */ 00467000
*/*                DSNWORK  - WORK AREA FOR DSN AND MEMBER           */ 00468000
*/*                           NAME COMPARES                          */ 00469000
*/*                                                                  */ 00470000
*/* EXIT -                                                           */ 00471000
*/*   OUTPUT DATA - UNALSTP - POINTS TO DSABPTR IF FOR UNALLOC       */ 00472000
*/*                 UNALCNT - SET TO 1 IF DSAB IS FOR UNALLOC        */ 00473000
*/*                 RIULSTP - POINTS TO DSABPTR IF FOR REMOVE IN-USE */ 00474000
*/*                 RIUCNT  - SET TO 1 IF DSAB IS FOR REMOVE IN-USE  */ 00475000
*/*                 ERRCODE - SET TO RCDDNFND IF A DSAB WITH THE     */ 00476000
*/*                           SPECIFIED DDN IS NOT FOUND             */ 00477000
*/*                         - SET TO RCNOUNAL IF A SPECIFIED DSN OR  */ 00478000
*/*                           MEMBER NAME IS NOT ASSOCIATED WITH THE */ 00479000
*/*                           SPECIFIED DDN; OR IF THE DDN WITH A    */ 00480000
*/*                           SPECIFIED DSN IS FOR A QNAME OR A      */ 00481000
*/*                           TERMINAL.                              */ 00482000
*/*                                                                  */ 00483000
*/********************************************************************/ 00484000
*RFY TEXTUNIT BASED(DDNAMTUP);         /*RESET TEXT UNIT BASE        */ 00485000
*DDNWORK = '';                         /*BLANK OUT WORK AREA         */ 00486000
         MVI   DDNWORK,C' '                                     02S0187 00487000
         MVC   DDNWORK+1(7),DDNWORK                             02S0187 00488000
*DDNWORK(1:TEXTULNG) = TEXTUPAR(1:TEXTULNG); /*COPY DDN INTO W/A     */ 00489000
         ICM   @05,12,TEXTULNG(@10)                             02S0188 00490000
         SRA   @05,16                                           02S0188 00491000
         BCTR  @05,0                                            02S0188 00492000
         EX    @05,@SM02192                                     02S0188 00493000
*CALL IEFDB4FC(DSQFRSTP,ADDR(DDNWORK),ADDR(DSABPTR));           02S0189 00494000
         L     @10,@PC00001+8                                   02S0189 00495000
         L     @10,JSCBPTR(,@10)                                02S0189 00496000
         L     @10,JSCDSABQ(,@10)                               02S0189 00497000
         LA    @10,DSQFRSTP(,@10)                               02S0189 00498000
         ST    @10,@AL00001                                     02S0189 00499000
         LA    @10,DDNWORK                                      02S0189 00500000
         ST    @10,@TF00001                                     02S0189 00501000
         LA    @10,@TF00001                                     02S0189 00502000
         ST    @10,@AL00001+4                                   02S0189 00503000
         LA    @10,DSABPTR                                      02S0189 00504000
         ST    @10,@TF00002                                     02S0189 00505000
         LA    @10,@TF00002                                     02S0189 00506000
         ST    @10,@AL00001+8                                   02S0189 00507000
         L     @15,@CV00103                                     02S0189 00508000
         LA    @01,@AL00001                                     02S0189 00509000
         BALR  @14,@15                                          02S0189 00510000
*                                      /*CALL DDNAME SEARCH RTN      */ 00511000
*IF DSABPTR = 0 THEN                   /*IF SPEC'D DDN WAS NOT FOUND */ 00512000
         L     @10,DSABPTR                                      02S0190 00513000
         LTR   @10,@10                                          02S0190 00514000
         BNZ   @RF00190                                         02S0190 00515000
*  ERRCODE = RCDDNFND;                 /*DDNAME NOT FOUND ERROR      */ 00516000
         L     @10,@PC00001+4                                   02S0191 00517000
         MVC   ERRCODE(2,@10),@CH01972                          02S0191 00518000
*ELSE                                  /*DDNAME WAS FOUND            */ 00519000
*  DO;                                 /*CONTINUE PROCESSING         */ 00520000
*                                                               02S0192 00521000
         B     @RC00190                                         02S0192 00522000
@RF00190 DS    0H                                               02S0193 00523000
*    IF DSNAMTUP ^= 0 THEN             /*IF DSNAME WAS ALSO SPEC'D   */ 00524000
         L     @10,DSNAMTUP                                     02S0193 00525000
         LTR   @10,@10                                          02S0193 00526000
         BZ    @RF00193                                         02S0193 00527000
*      DO;                             /*CHECK IF SPEC'D DSN MATCHES    00528000
*                                        DSN ASSOCIATED WITH DDN     */ 00529000
*        DSNWORK = '';                 /*BLANK OUT WORK AREA         */ 00530000
         MVI   DSNWORK,C' '                                     02S0195 00531000
         MVC   DSNWORK+1(43),DSNWORK                            02S0195 00532000
*        RFY TEXTUNIT BASED(DSNAMTUP); /*RESET TEXT UNIT BASE        */ 00533000
*        DSNWORK(1:TEXTULNG)=TEXTUPAR(1:TEXTULNG);              02S0197 00534000
         ICM   @05,12,TEXTULNG(@10)                             02S0197 00535000
         SRA   @05,16                                           02S0197 00536000
         BCTR  @05,0                                            02S0197 00537000
         EX    @05,@SM02194                                     02S0197 00538000
*                                      /*COPY SPEC'D DSN INTO W/A    */ 00539000
*        IF DSNWORK ^= JFCBDSNM THEN   /*IF DSNAMES DON'T MATCH      */ 00540000
         L     @10,DSABPTR                                      02S0198 00541000
         L     @10,DSABSIOT(,@10)                               02S0198 00542000
         L     @05,SJFCBPTR(,@10)                               02S0198 00543000
         CLC   DSNWORK(44),JFCBDSNM(@05)                        02S0198 00544000
         BE    @RF00198                                         02S0198 00545000
*          IF SIOALIAS = '0'B |        /*NO ALIAS DSNAME     @YM03581*/ 00546000
*             DSNWORK ^= JFCBXNAM THEN /*OR ALIAS NAME PRESENT  02S0199 00547000
*                                        AND NO MATCH        @YM03581*/ 00548000
         TM    SIOALIAS(@10),B'10000000'                        02S0199 00549000
         BZ    @RT00199                                         02S0199 00550000
         L     @10,SIOTJFX(,@10)                                02S0199 00551000
         CLC   DSNWORK(44),JFCBXNAM(@10)                        02S0199 00552000
         BE    @RF00199                                         02S0199 00553000
@RT00199 DS    0H                                               02S0200 00554000
*            IF DSABANMP ^= 0 THEN     /*ALTERNATE DSNAME ?  @YM03581*/ 00555000
         L     @10,DSABPTR                                      02S0200 00556000
         L     @10,DSABANMP(,@10)                               02S0200 00557000
         LTR   @10,@10                                          02S0200 00558000
         BZ    @RF00200                                         02S0200 00559000
*              DO;                     /*YES, TEST FOR MATCH @YM03581*/ 00560000
*                DSNWORK = '';         /*BLANK OUT WORK AREA @YM03581*/ 00561000
         MVI   DSNWORK,C' '                                     02S0202 00562000
         MVC   DSNWORK+1(43),DSNWORK                            02S0202 00563000
*                DSNWORK(1:DSABANML) = DSABANAM(1:DSABANML);    02S0203 00564000
         SLR   @05,@05                                          02S0203 00565000
         IC    @05,DSABANML(,@10)                               02S0203 00566000
         LR    @15,@05                                          02S0203 00567000
         BCTR  @15,0                                            02S0203 00568000
         EX    @15,@SM02196                                     02S0203 00569000
*                                      /*COPY ALT DSNAME     @YM03581*/ 00570000
*                IF DSABANML > TEXTULNG | /*ALT DSN LEN > SPEC'D        00571000
*                                           DSN LEN          @YM03581*/ 00572000
*                   DSNWORK(1:TEXTULNG) ^= TEXTUPAR(1:TEXTULNG) THEN    00573000
         L     @10,DSNAMTUP                                     02S0204 00574000
         ICM   @15,12,TEXTULNG(@10)                             02S0204 00575000
         SRA   @15,16                                           02S0204 00576000
         CR    @05,@15                                          02S0204 00577000
         BH    @RT00204                                         02S0204 00578000
         BCTR  @15,0                                            02S0204 00579000
         EX    @15,@SC02198                                     02S0204 00580000
         BE    @RF00204                                         02S0204 00581000
@RT00204 DS    0H                                               02S0205 00582000
*                                      /*DO DSNAMES MATCH ?  @YM03581*/ 00583000
*                  ERRCODE = RCNOUNAL; /*NO, SET ERROR CODE  @YM03581*/ 00584000
         L     @10,@PC00001+4                                   02S0205 00585000
         MVC   ERRCODE(2,@10),@CH01992                          02S0205 00586000
*                ELSE;                 /*NAMES MATCH         @YM03581*/ 00587000
@RF00204 DS    0H                                               02S0207 00588000
*              END;                    /*END ALT NAME TEST   @YM03581*/ 00589000
*            ELSE                      /*NO ALT OR ALIAS DSNAME 02S0208 00590000
*                                        MATCH               @YM03581*/ 00591000
*              ERRCODE = RCNOUNAL;     /*SET ERROR CODE      @YM03581*/ 00592000
         B     @RC00200                                         02S0208 00593000
@RF00200 L     @10,@PC00001+4                                   02S0208 00594000
         MVC   ERRCODE(2,@10),@CH01992                          02S0208 00595000
*          ELSE;                       /*ALIAS MATCH FOUND   @YM03581*/ 00596000
@RF00199 DS    0H                                               02S0210 00597000
*        ELSE                          /*MATCH IS MADE ON JFCBDSNM   */ 00598000
*          IF SIOTQNAM = '1'B  |       /*IF DSAB IS FOR QNAME        */ 00599000
*             SIOTTERM = '1'B THEN     /*OR A TERMINAL               */ 00600000
         B     @RC00198                                         02S0210 00601000
@RF00198 L     @10,DSABPTR                                      02S0210 00602000
         L     @10,DSABSIOT(,@10)                               02S0210 00603000
         TM    SIOTQNAM(@10),B'01000001'                        02S0210 00604000
         BZ    @RF00210                                         02S0210 00605000
*            ERRCODE = RCNOUNAL;       /*SET ERROR RETURN            */ 00606000
         L     @10,@PC00001+4                                   02S0211 00607000
         MVC   ERRCODE(2,@10),@CH01992                          02S0211 00608000
*          ELSE;                       /*DSAB IS VALID               */ 00609000
@RF00210 DS    0H                                               02S0213 00610000
*        IF (ERRCODE = 0  &            /*IF NO ERROR AND             */ 00611000
*            MEMBRTUP ^= 0) THEN       /*MEMBER WAS SPECIFIED        */ 00612000
@RC00198 SLR   @10,@10                                          02S0213 00613000
         L     @05,@PC00001+4                                   02S0213 00614000
         CH    @10,ERRCODE(,@05)                                02S0213 00615000
         BNE   @RF00213                                         02S0213 00616000
         L     @15,MEMBRTUP                                     02S0213 00617000
         CR    @15,@10                                          02S0213 00618000
         BE    @RF00213                                         02S0213 00619000
*         DO;                          /*COMPARE MEMBER NAMES        */ 00620000
*            DSNWORK(1:8) = '';        /*BLANK OUT 8 BYTES OF W/A    */ 00621000
         MVI   DSNWORK,C' '                                     02S0215 00622000
         MVC   DSNWORK+1(7),DSNWORK                             02S0215 00623000
*            RFY TEXTUNIT BASED (MEMBRTUP); /*RESET TEXTUNIT BASE    */ 00624000
*            DSNWORK(1:TEXTULNG)=TEXTUPAR(1:TEXTULNG);          02S0217 00625000
         ICM   @10,12,TEXTULNG(@15)                             02S0217 00626000
         SRA   @10,16                                           02S0217 00627000
         BCTR  @10,0                                            02S0217 00628000
         EX    @10,@SM02200                                     02S0217 00629000
*                                      /*COPY MEMBER NAME INTO W/A   */ 00630000
*            IF DSNWORK(1:8)^= JFCBELNM THEN /*IF NO MATCH FOR MEMBER*/ 00631000
         L     @10,DSABPTR                                      02S0218 00632000
         L     @10,DSABSIOT(,@10)                               02S0218 00633000
         L     @10,SJFCBPTR(,@10)                               02S0218 00634000
         CLC   DSNWORK(8),JFCBELNM(@10)                         02S0218 00635000
         BE    @RF00218                                         02S0218 00636000
*              ERRCODE = RCNOUNAL;     /*SET ERROR RETURN            */ 00637000
         MVC   ERRCODE(2,@05),@CH01992                          02S0219 00638000
*            ELSE;                     /*MEMBER NAMES MATCH          */ 00639000
@RF00218 DS    0H                                               02S0221 00640000
*         END;                         /*END MEMBER NAME CHECK       */ 00641000
*        ELSE;                         /*DSN ERROR OR NO MEMBER NAME */ 00642000
@RF00213 DS    0H                                               02S0223 00643000
*      END;                            /*END IF DSN WAS SPEC'D,      */ 00644000
*    ELSE;                             /*DSN NOT SPECIFIED           */ 00645000
*                                                               02S0224 00646000
@RF00193 DS    0H                                               02S0225 00647000
*    IF ERRCODE = 0 THEN               /*IF NO ERROR ENCOUNTERED     */ 00648000
         SLR   @10,@10                                          02S0225 00649000
         L     @05,@PC00001+4                                   02S0225 00650000
         CH    @10,ERRCODE(,@05)                                02S0225 00651000
         BNE   @RF00225                                         02S0225 00652000
*      DO;                             /*CONTINUE PROCESSING         */ 00653000
*        DSABLST1 = DSABPTR;           /*PUT DSAB ADDR IN A LIST     */ 00654000
         L     @05,DSABPTR                                      02S0227 00655000
         ST    @05,DSABLST1                                     02S0227 00656000
*        IF(UNAOPTUP ^= 0          |   /*IF UNALLOC OPTION WAS SPEC'D*/ 00657000
*                                                               02S0228 00658000
*           (RIUOPTUP = 0 &            /*OR NEITHER UNALLOC OR REMOVE   00659000
*                                        IN-USE OPTIONS WERE SPEC'D &*/ 00660000
*            DSABPALC = '0'B)      |   /*THE D.S. IS NON PERM ALLOC.,*/ 00661000
*                                                               02S0228 00662000
*           (RIUOPTUP ^= 0 &           /*OR REMOVE IN-USE WAS SPEC'D */ 00663000
*            DSABPALC = '0'B &         /*AND D.S. IS NON PERM ALLOC &*/ 00664000
*            (DSABANMP = 0 |           /*THERE IS NO ALTERNATE NAME  */ 00665000
*             DSABANAM(1) ^= '&') &    /*OR THERE IS BUT NON & AND   */ 00666000
*            SIOTDLET = '1'B))   THEN  /*THE DS HAS A DISP OF DELETE */ 00667000
*                                                               02S0228 00668000
         C     @10,UNAOPTUP                                     02S0228 00669000
         BNE   @RT00228                                         02S0228 00670000
         C     @10,RIUOPTUP                                     02S0228 00671000
         BNE   @GL00028                                         02S0228 00672000
         TM    DSABPALC(@05),B'01000000'                        02S0228 00673000
         BZ    @RT00228                                         02S0228 00674000
@GL00028 SLR   @10,@10                                          02S0228 00675000
         C     @10,RIUOPTUP                                     02S0228 00676000
         BE    @RF00228                                         02S0228 00677000
         L     @05,DSABPTR                                      02S0228 00678000
         TM    DSABPALC(@05),B'01000000'                        02S0228 00679000
         BNZ   @RF00228                                         02S0228 00680000
         L     @05,DSABANMP(,@05)                               02S0228 00681000
         CR    @05,@10                                          02S0228 00682000
         BE    @GL00031                                         02S0228 00683000
         CLI   DSABANAM(@05),C'&&'                              02S0228 00684000
         BE    @RF00228                                         02S0228 00685000
@GL00031 L     @10,DSABPTR                                      02S0228 00686000
         L     @10,DSABSIOT(,@10)                               02S0228 00687000
         TM    SIOTDLET(@10),B'00000100'                        02S0228 00688000
         BNO   @RF00228                                         02S0228 00689000
@RT00228 DS    0H                                               02S0229 00690000
*          DO;                         /*THEN DSAB IS FOR UNALLOC.   */ 00691000
*            UNALSTP = ADDR(DSABLST1); /*POINT LIST PTR TO LIST DSAB */ 00692000
         LA    @10,DSABLST1                                     02S0230 00693000
         ST    @10,UNALSTP                                      02S0230 00694000
*            UNALCNT = 1;              /*SET UNALLOC LIST COUNT TO 1 */ 00695000
         MVC   UNALCNT(4),@CF00230                              02S0231 00696000
*          END;                        /*END IF FOR UNALLOCATION     */ 00697000
*        ELSE                          /*DSAB IS FOR REMOVE IN-USE   */ 00698000
*          IF (DSABNUSE = '1'B   &     /*IF DSAB IS IN-USE AND       */ 00699000
*              DSABCATL = '0'B) THEN   /*NOT FOR A PRIVATE CATALOG   */ 00700000
         B     @RC00228                                         02S0233 00701000
@RF00228 L     @10,DSABPTR                                      02S0233 00702000
         TM    DSABNUSE(@10),B'00000001'                        02S0233 00703000
         BNO   @RF00233                                         02S0233 00704000
         TM    DSABCATL(@10),B'00001000'                        02S0233 00705000
         BNZ   @RF00233                                         02S0233 00706000
*            DO;                       /*SET IT UP FOR REMOVE IN-USE */ 00707000
*              RIULSTP = ADDR(DSABLST1);/*POINT LIST PTR TO LIST DSAB*/ 00708000
         LA    @10,DSABLST1                                     02S0235 00709000
         ST    @10,RIULSTP                                      02S0235 00710000
*              RIUCNT = 1;             /*SET R-I-U COUNT TO 1        */ 00711000
         MVC   RIUCNT(4),@CF00230                               02S0236 00712000
*            END;                      /*END IF FOR REMOVE IN-USE    */ 00713000
*          ELSE;                       /*DSAB NOT IN-USE OR A CATLG  */ 00714000
@RF00233 DS    0H                                               02S0239 00715000
*      END;                            /*END  NO DSN ERROR           */ 00716000
*    ELSE;                             /*ERROR IN DSN CHECK          */ 00717000
@RF00225 DS    0H                                               02S0241 00718000
*  END;                                /*END IF DDN WAS FOUND        */ 00719000
*      END;                            /*END OF REQUEST VIA DDNAME   */ 00720000
*    ELSE                              /*DDN NOT SPECIFIED;DSN WAS   */ 00721000
*      DO;                             /*SEARCH FOR SPEC'D DSN       */ 00722000
         B     @RC00184                                         01S0243 00723000
@RF00184 DS    0H                                               01S0244 00724000
*/********************************************************************/ 00725000
*/*                                                                  */ 00726000
*/* SEGMENT NAME - DSNPRCSS                                          */ 00727000
*/*                                                                  */ 00728000
*/* DESCRIPTIVE NAME - DSNAME PROCESS                                */ 00729000
*/*                                                                  */ 00730000
*/* FUNCTION  - BUILD LIST OF DSAB PTRS FOR DYNAMIC UNALLOCATION     */ 00731000
*/*             REQUESTS VIA DSNAME                                  */ 00732000
*/*                                                                  */ 00733000
*/* OPERATION - THE DSNAME SEARCH RTN (IEFDB4FA) IS INVOKED TO       */ 00734000
*/*             SEARCH DOWN FROM THE TOP OF THE DSAB CHAIN FOR AN    */ 00735000
*/*             ALLOCATION OF THE SPECIFIED DSN. (A BIT IS SET       */ 00736000
*/*             SAYING LOOK AT BOTH DSAB ALTERNATE NAME AND          */ 00737000
*/*             JFCBDSNM FIELDS IN THE SEARCH.) IF MEMBER NAME WAS   */ 00738000
*/*             SPECIFIED, THE DSAB RETURNED IS CHECKED TO SEE IF    */ 00739000
*/*             IT ALSO CORRESPONDS TO THE SPECIFIED MEMBER NAME.    */ 00740000
*/*             IF IT DOES, OR IF MEMBER NAME WAS NOT SPECIFIED,     */ 00741000
*/*             THE DSAB IS ELIGIBLE FOR PROCESSING AND DSABCNT      */ 00742000
*/*             IS INCREMENTED BY ONE. THE ABOVE PROCESS IS THEN     */ 00743000
*/*             REPEATED, STARTING THE SEARCH FROM THE DSAB IN THE   */ 00744000
*/*             CHAIN AFTER THE ONE THAT WAS RETURNED (OR, IF THE    */ 00745000
*/*             DSAB WAS PERMANENTLY CONCATENATED, AFTER THE END     */ 00746000
*/*             OF THE PCAT GROUP), UNTIL ALL DSABS HAVE BEEN LOOKED */ 00747000
*/*             AT, AND DSABCNT HAS THE TOTAL NUMBER OF DSABS        */ 00748000
*/*             ELIGIBLE FOR PROCESSING.                             */ 00749000
*/*                                                                  */ 00750000
*/*             IF THERE ARE NO DSABS ELIGIBLE, AN ERRCODE OF        */ 00751000
*/*             RCDSNFND IS SET AND NO FURTHER PROCESSING IS DONE.   */ 00752000
*/*             OTHERWISE, A GETMAIN IS ISSUED FOR CORE FOR A LIST   */ 00753000
*/*             OF PTRS TO DSABS TO BE PROCESSED (NO GETMAIN IS      */ 00754000
*/*             NEEDED IF ONLY ONE DSAB WILL BE IN THE LIST), AND    */ 00755000
*/*             THE ABOVE ELIGIBLE DSAB DETERMINATION IS REPEATED.   */ 00756000
*/*             THIS TIME HOWEVER, THE DSABS ARE ALSO CHECKED NOT    */ 00757000
*/*             TO BE ASSOCIATED WITH THE DDNAMES JOBCAT, STEPCAT,   */ 00758000
*/*             JOBLIB OR STEPLIB. IF ONE IS, AN ERROR CODE          */ 00759000
*/*             OF RCRSVDDN IS SET & THE DSAB ISN'T ELIGIBLE.        */ 00760000
*/*             OTHERWISE THE ADDRESSES OF THE ELIGIBLE              */ 00761000
*/*             DSABS ARE PLACED INTO EITHER THE LIST FOR UN-        */ 00762000
*/*             ALLOCATION(BUILT DOWN FROM THE TOP OF THE GETMAIN'D  */ 00763000
*/*             CORE) OR THE LIST FOR REMOVE IN-USE (BUILT UP FROM   */ 00764000
*/*             THE BOTTOM OF THE GETMAIN'D AREA). THE CORRESPONDING */ 00765000
*/*             COUNTS OF DSABS IN EACH LIST ARE INCREMENTED         */ 00766000
*/*             ACCORDINGLY. DETERMINATION OF WHICH LIST A DSAB PTR  */ 00767000
*/*             GOES INTO IS AS FOLLOWS:                             */ 00768000
*/*             - IF THE UNALLOC OPTION WAS SPECIFIED, THE DSAB      */ 00769000
*/*               IS FOR UNALLOCATION                                */ 00770000
*/*             - IF THE REMOVE IN-USE OPTION WAS SPECIFIED BUT THE  */ 00771000
*/*               DATA SET IS NON-PERMANENTLY ALLOCATED, HAS A       */ 00772000
*/*               NON-&DSN, AND HAS A DISP OF DELETE, THE DSAB       */ 00773000
*/*               IS FOR UNALLOCATION                                */ 00774000
*/*             - IF NEITHER OPTION WAS SPECIFIED AND THE DATA SET   */ 00775000
*/*               IS NOT PERMANENTLY ALLOCATED, THE DSAB IS FOR      */ 00776000
*/*               UNALLOCATION.                                      */ 00777000
*/*             - OTHERWISE, THE DSAB IS FOR REMOVE IN-USE. (IF THE  */ 00778000
*/*               DSAB IS ALREADY NOT IN-USE, OR IS FOR A PRIVATE    */ 00779000
*/*               CATALOG, REMOVE IN-USE PROCESSING WILL BE BYPASSED)*/ 00780000
*/*               NOTE THAT IF THE DSAB IS A MEMBER OF A CONCATENATED*/ 00781000
*/*               GROUP, A PTR TO THE FIRST DSAB IN THE GROUP WILL BE*/ 00782000
*/*               THE ONE PLACED IN THE LIST, CAUSING REMOVE IN-USE  */ 00783000
*/*               PROCESSING TO BE DONE FOR ALL MEMBERS.             */ 00784000
*/*                                                                  */ 00785000
*/* ENTRY -                                                          */ 00786000
*/*   INPUT DATA - DSQFRSTP - PTR TO FIRST DSAB ON CHAIN             */ 00787000
*/*                DSABPTR  - ADDR OF DSAB RETURNED FROM SEARCH RTN  */ 00788000
*/*                           (ZERO ON ENTRY)                        */ 00789000
*/*                DSABCNT  - COUNT OF DSABPTRS FOR GETMAIN          */ 00790000
*/*                           (ZERO ON ENTRY)                        */ 00791000
*/*                ERRCODE  - ERROR CODE FIELD - ZERO ON ENTRY       */ 00792000
*/*                UNALKEYT - KEY TABLE OF TEXT UNIT PTRS            */ 00793000
*/*                                                                  */ 00794000
*/* EXIT -                                                           */ 00795000
*/*   OUTPUT DATA - UNALSTP - POINTS TO DSAB PTR LIST FOR UNALLOC    */ 00796000
*/*                 UNALCNT - COUNT OF DSABS FOR UNALLOC             */ 00797000
*/*                 RIULSTP - POINTS TO DSAB PTR LIST FOR REMOVE     */ 00798000
*/*                           IN-USE                                 */ 00799000
*/*                 RIUCNT  - COUNT OF DSABS FOR REMOVE IN-USE       */ 00800000
*/*                 ERRCODE - SET TO RCDSNFND IF THERE ARE NO        */ 00801000
*/*                           DSABS TO PROCESS                       */ 00802000
*/*                         - SET TO RCRSVDDN IF A DSAB FOUND IS PART*/ 00803000
*/*                           OF A JOBLIB, STEPLIB, JOBCAT OR STEPCAT*/ 00804000
*/*                           DDNAME                                 */ 00805000
*/*                                                                  */ 00806000
*/********************************************************************/ 00807000
*RFY TEXTUNIT BASED(DSNAMTUP);         /*RESET TEXT UNIT BASE        */ 00808000
*DSNPRCSW = DSNPRCSW && DSNPRCSW;      /*ZERO OUT CONTROL SWITCHES   */ 00809000
         XC    DSNPRCSW(1),DSNPRCSW                             02S0245 00810000
*STARTPTR = DSQFRSTP;                  /*START FROM 1ST DSAB ON CHAIN*/ 00811000
         L     @10,@PC00001+8                                   02S0246 00812000
         L     @10,JSCBPTR(,@10)                                02S0246 00813000
         L     @10,JSCDSABQ(,@10)                               02S0246 00814000
         L     @10,DSQFRSTP(,@10)                               02S0246 00815000
         ST    @10,STARTPTR                                     02S0246 00816000
*DSNSCHMP = DSNSCHMP && DSNSCHMP;      /*ZERO OUT DSN SEARCH FN. MAP */ 00817000
         XC    DSNSCHMP(2),DSNSCHMP                             02S0247 00818000
*JFCBALNM = '1'B;                      /*CHECK JFCB & ALT. NAME FIELD*/ 00819000
*                                                               02S0248 00820000
         OI    JFCBALNM,B'00100000'                             02S0248 00821000
*DO WHILE OUT = '0'B;                  /*LOOP DSAB SEARCH BY DSN     */ 00822000
         B     @DE00249                                         02S0249 00823000
@DL00249 DS    0H                                               02S0250 00824000
*  CALL IEFDB4FA(STARTPTR,ADDR(TEXTUENT),DSABPTR,DSNSCHMP);     02S0250 00825000
         LA    @10,STARTPTR                                     02S0250 00826000
         ST    @10,@AL00001                                     02S0250 00827000
         L     @10,DSNAMTUP                                     02S0250 00828000
         LA    @10,TEXTUENT(,@10)                               02S0250 00829000
         ST    @10,@TF00001                                     02S0250 00830000
         LA    @10,@TF00001                                     02S0250 00831000
         ST    @10,@AL00001+4                                   02S0250 00832000
         LA    @10,DSABPTR                                      02S0250 00833000
         ST    @10,@AL00001+8                                   02S0250 00834000
         LA    @10,DSNSCHMP                                     02S0250 00835000
         ST    @10,@AL00001+12                                  02S0250 00836000
         L     @15,@CV00104                                     02S0250 00837000
         LA    @01,@AL00001                                     02S0250 00838000
         BALR  @14,@15                                          02S0250 00839000
*                                      /*CALL DSN SEARCH RTN         */ 00840000
*  IF DSABPTR = 0 THEN                 /*IF NO DSAB FOR DSN WAS FOUND*/ 00841000
         L     @10,DSABPTR                                      02S0251 00842000
         LTR   @10,@10                                          02S0251 00843000
         BNZ   @RF00251                                         02S0251 00844000
*    OUT = '1'B;                       /*GET OUT OF SEARCH LOOP      */ 00845000
         OI    OUT,B'10000000'                                  02S0252 00846000
*  ELSE                                /*DSAB FOR SPECD DSN WAS FOUND*/ 00847000
*    DO;                               /*CONTINUE PROCESSING         */ 00848000
*                                                               02S0253 00849000
         B     @RC00251                                         02S0253 00850000
@RF00251 DS    0H                                               02S0254 00851000
*      IF MEMBRTUP ^= 0 THEN           /*IF MEMBER NAME WAS SPEC'D   */ 00852000
         L     @10,MEMBRTUP                                     02S0254 00853000
         LTR   @10,@10                                          02S0254 00854000
         BZ    @RF00254                                         02S0254 00855000
*        DO;                           /*CHECK FOR MATCH WITH DSAB'S */ 00856000
*          DSNWORK(1:8) = '';          /*BLANK OUT 8 BYTES IN W/A    */ 00857000
         MVI   DSNWORK,C' '                                     02S0256 00858000
         MVC   DSNWORK+1(7),DSNWORK                             02S0256 00859000
*          RFY TEXTUNIT BASED(MEMBRTUP); /*RESET TEXT UNIT BASE      */ 00860000
*          DSNWORK(1:TEXTULNG) = TEXTUPAR(1:TEXTULNG);          02S0258 00861000
         ICM   @05,12,TEXTULNG(@10)                             02S0258 00862000
         SRA   @05,16                                           02S0258 00863000
         BCTR  @05,0                                            02S0258 00864000
         EX    @05,@SM02194                                     02S0258 00865000
*                                      /*COPY MEMBER NAME INTO W/A   */ 00866000
*          IF DSNWORK(1:8) = JFCBELNM THEN /*IF MEMBER NAMES MATCH   */ 00867000
         L     @10,DSABPTR                                      02S0259 00868000
         L     @05,DSABSIOT(,@10)                               02S0259 00869000
         L     @05,SJFCBPTR(,@05)                               02S0259 00870000
         CLC   DSNWORK(8),JFCBELNM(@05)                         02S0259 00871000
         BNE   @RF00259                                         02S0259 00872000
*            DO;                       /*ADJUST DSABPTR AND COUNT    */ 00873000
*              DSABCNT = DSABCNT + 1;  /*INCREMENT COUNT OF DSABS    */ 00874000
         AL    DSABCNT,@CF00230                                 02S0261 00875000
*              IF DSABPCAT = '1'B THEN /*IF DSAB IS PERM CONCAT      */ 00876000
         TM    DSABPCAT(@10),B'00000100'                        02S0262 00877000
         BNO   @RF00262                                         02S0262 00878000
*                DO WHILE (DSABFCHN ^= 0 & /*SKIP TO END OF CHAIN OR */ 00879000
*                   DSABFCHN->DSABSIOT->SCTDDNAM(1)=' ');       02S0263 00880000
         B     @DE00263                                         02S0263 00881000
@DL00263 DS    0H                                               02S0264 00882000
*                                      /*TO END OF PERM CONCAT GROUP */ 00883000
*                  DSABPTR = DSABFCHN; /*POINT TO NEXT DSAB          */ 00884000
         L     @10,DSABPTR                                      02S0264 00885000
         L     @10,DSABFCHN(,@10)                               02S0264 00886000
         ST    @10,DSABPTR                                      02S0264 00887000
*                END;                  /*END IF PERM CONCAT          */ 00888000
@DE00263 L     @10,DSABPTR                                      02S0265 00889000
         L     @10,DSABFCHN(,@10)                               02S0265 00890000
         LTR   @10,@10                                          02S0265 00891000
         BZ    @DC00263                                         02S0265 00892000
         L     @10,DSABSIOT(,@10)                               02S0265 00893000
         CLI   SCTDDNAM(@10),C' '                               02S0265 00894000
         BE    @DL00263                                         02S0265 00895000
@DC00263 DS    0H                                               02S0266 00896000
*            END;                      /*END IF MEMBER MATCH         */ 00897000
*          ELSE;                       /*NO MATCH; DSAB NOT ELIGIBLE */ 00898000
@RF00259 DS    0H                                               02S0268 00899000
*        END;                          /*END MEMBER NAME PROCESS     */ 00900000
*                                                               02S0268 00901000
*      ELSE                            /*MEMBER NAME NOT SPEC'D      */ 00902000
*        DO;                           /*ADJUST DSABPTR AND COUNT    */ 00903000
         B     @RC00254                                         02S0269 00904000
@RF00254 DS    0H                                               02S0270 00905000
*          DSABCNT = DSABCNT + 1;      /*INCREMENT COUNT OF DSABS    */ 00906000
         AL    DSABCNT,@CF00230                                 02S0270 00907000
*          IF DSABPCAT = '1'B THEN     /*IF DSAB IS PERM CONCAT      */ 00908000
         L     @10,DSABPTR                                      02S0271 00909000
         TM    DSABPCAT(@10),B'00000100'                        02S0271 00910000
         BNO   @RF00271                                         02S0271 00911000
*            DO WHILE (DSABFCHN ^= 0 & /*SKIP TO END OF CHAIN OR     */ 00912000
*               DSABFCHN->DSABSIOT->SCTDDNAM(1)=' ');           02S0272 00913000
         B     @DE00272                                         02S0272 00914000
@DL00272 DS    0H                                               02S0273 00915000
*                                      /*TO END OF PERM CONCAT GROUP */ 00916000
*              DSABPTR = DSABFCHN;     /*POINT TO NEXT DSAB          */ 00917000
         L     @10,DSABPTR                                      02S0273 00918000
         L     @10,DSABFCHN(,@10)                               02S0273 00919000
         ST    @10,DSABPTR                                      02S0273 00920000
*            END;                      /*END IF PERM CONCAT          */ 00921000
@DE00272 L     @10,DSABPTR                                      02S0274 00922000
         L     @10,DSABFCHN(,@10)                               02S0274 00923000
         LTR   @10,@10                                          02S0274 00924000
         BZ    @DC00272                                         02S0274 00925000
         L     @10,DSABSIOT(,@10)                               02S0274 00926000
         CLI   SCTDDNAM(@10),C' '                               02S0274 00927000
         BE    @DL00272                                         02S0274 00928000
@DC00272 DS    0H                                               02S0275 00929000
*        END;                          /*END IF MEMBER NOT SPECIFIED */ 00930000
*                                                               02S0275 00931000
@RF00271 DS    0H                                               02S0276 00932000
*      IF DSABFCHN ^= 0 THEN           /*IF NOT AT END OF DSAB CHAIN */ 00933000
@RC00254 SLR   @10,@10                                          02S0276 00934000
         L     @05,DSABPTR                                      02S0276 00935000
         L     @05,DSABFCHN(,@05)                               02S0276 00936000
         CR    @05,@10                                          02S0276 00937000
         BE    @RF00276                                         02S0276 00938000
*        DO;                           /*THEN SET UP FOR NEXT CALL   */ 00939000
*          STARTPTR = DSABFCHN;        /*START FROM NEXT DSAB        */ 00940000
         ST    @05,STARTPTR                                     02S0278 00941000
*          DSABPTR = 0;                /*ZERO RETURN DSAB PTR        */ 00942000
         ST    @10,DSABPTR                                      02S0279 00943000
*        END;                          /*END IF MORE DSABS TO SEARCH */ 00944000
*      ELSE                            /*AT END OF DSAB CHAIN        */ 00945000
*        OUT = '1'B;                   /*GET OUT OF SEARCH LOOP      */ 00946000
         B     @RC00276                                         02S0281 00947000
@RF00276 OI    OUT,B'10000000'                                  02S0281 00948000
*    END;                              /*END IF DSAB OF DSN WAS FOUND*/ 00949000
@RC00276 DS    0H                                               02S0283 00950000
*END;                                  /*END SEARCH LOOP             */ 00951000
*                                                               02S0283 00952000
@RC00251 DS    0H                                               02S0283 00953000
@DE00249 TM    OUT,B'10000000'                                  02S0283 00954000
         BZ    @DL00249                                         02S0283 00955000
*IF DSABCNT = 0 THEN                   /*IF NO DSABS WERE FOUND      */ 00956000
         LTR   DSABCNT,DSABCNT                                  02S0284 00957000
         BNZ   @RF00284                                         02S0284 00958000
*  ERRCODE = RCDSNFND;                 /*DSN NOT FOUND ERROR         */ 00959000
*                                                               02S0285 00960000
         L     @10,@PC00001+4                                   02S0285 00961000
         MVC   ERRCODE(2,@10),@CH01976                          02S0285 00962000
*ELSE                                  /*DSABS WERE FOUND            */ 00963000
*  DO;                                 /*CONTINUE PROCESSING         */ 00964000
         B     @RC00284                                         02S0286 00965000
@RF00284 DS    0H                                               02S0287 00966000
*    RFY TEXTUNIT BASED(DSNAMTUP);     /*RESET TEXT UNIT BASE PTR    */ 00967000
*    RFY (R0,R1) RSTD;                 /*RESTRICT REG. 0 AND REG 1   */ 00968000
*    IF DSABCNT > 1 THEN               /*IF MORE THAN 1 DSAB THEN    */ 00969000
         C     DSABCNT,@CF00230                                 02S0289 00970000
         BNH   @RF00289                                         02S0289 00971000
*      DO;                             /*NEED CORE FOR LIST          */ 00972000
*        R0 = DSABCNT * 4;             /*CORE FOR DSAB PTR LIST      */ 00973000
         LR    R0,DSABCNT                                       02S0291 00974000
         SLA   R0,2                                             02S0291 00975000
*        R0 = R0 | SUBPOOLN;           /*GET FROM CORRECT SUBPOOL    */ 00976000
         O     R0,@CF00097                                      02S0292 00977000
*        GEN (GETMAIN R,LV=(0));       /*GET CORE NEEDED             */ 00978000
         GETMAIN R,LV=(0)                                               00979000
*      END;                            /*END GETMAIN PROCESSING      */ 00980000
*    ELSE                              /*NO GETMAIN NECESSARY        */ 00981000
*      R1 = ADDR (DSABLST1);           /*USE WORK DSAB LIST OF 1 DSAB*/ 00982000
         B     @RC00289                                         02S0295 00983000
@RF00289 LA    R1,DSABLST1                                      02S0295 00984000
*    UNALSTP = R1;                     /*UNALLOC DSABS AT TOP OF LIST*/ 00985000
@RC00289 ST    R1,UNALSTP                                       02S0296 00986000
*    RIULSTP = R1 + DSABCNT * 4;       /*REMOVE IN-USE DSABS AT END  */ 00987000
         LR    @10,DSABCNT                                      02S0297 00988000
         SLA   @10,2                                            02S0297 00989000
         ALR   @10,R1                                           02S0297 00990000
         ST    @10,RIULSTP                                      02S0297 00991000
*    RFY (R0,R1) UNRSTD;               /*RELEASE REGS 0 AND 1        */ 00992000
*    STARTPTR = DSQFRSTP;              /*START FROM FIRST DSAB       */ 00993000
         L     @10,@PC00001+8                                   02S0299 00994000
         L     @10,JSCBPTR(,@10)                                02S0299 00995000
         L     @10,JSCDSABQ(,@10)                               02S0299 00996000
         L     @10,DSQFRSTP(,@10)                               02S0299 00997000
         ST    @10,STARTPTR                                     02S0299 00998000
*    COUNT = DSABCNT;                  /*INIT LOOP CTL COUNT         */ 00999000
*                                                               02S0300 01000000
         LR    COUNT,DSABCNT                                    02S0300 01001000
*    DO WHILE COUNT ^= 0;              /*LOOP TO BUILD DSAB PTR LIST */ 01002000
         B     @DE00301                                         02S0301 01003000
@DL00301 DS    0H                                               02S0302 01004000
*      CALL IEFDB4FA(STARTPTR,ADDR(TEXTUENT),DSABPTR,DSNSCHMP); 02S0302 01005000
         LA    @10,STARTPTR                                     02S0302 01006000
         ST    @10,@AL00001                                     02S0302 01007000
         L     @10,DSNAMTUP                                     02S0302 01008000
         LA    @10,TEXTUENT(,@10)                               02S0302 01009000
         ST    @10,@TF00001                                     02S0302 01010000
         LA    @10,@TF00001                                     02S0302 01011000
         ST    @10,@AL00001+4                                   02S0302 01012000
         LA    @10,DSABPTR                                      02S0302 01013000
         ST    @10,@AL00001+8                                   02S0302 01014000
         LA    @10,DSNSCHMP                                     02S0302 01015000
         ST    @10,@AL00001+12                                  02S0302 01016000
         L     @15,@CV00104                                     02S0302 01017000
         LA    @01,@AL00001                                     02S0302 01018000
         BALR  @14,@15                                          02S0302 01019000
*                                      /*CALL DSN SEARCH RTN         */ 01020000
*                                                               02S0303 01021000
*      IF MEMBRTUP ^= 0 THEN           /*IF MEMBER NAME WAS SPEC'D   */ 01022000
         L     @10,MEMBRTUP                                     02S0303 01023000
         LTR   @10,@10                                          02S0303 01024000
         BZ    @RF00303                                         02S0303 01025000
*        DO;                           /*CHECK IF SAME AS DSAB'S     */ 01026000
*          DSNWORK(1:8) = '';          /*BLANK OUT 8 BYTES IN W/A    */ 01027000
         MVI   DSNWORK,C' '                                     02S0305 01028000
         MVC   DSNWORK+1(7),DSNWORK                             02S0305 01029000
*          RFY TEXTUNIT BASED(MEMBRTUP); /*RESET TEXT UNIT BASE      */ 01030000
*          DSNWORK(1:TEXTULNG) = TEXTUPAR(1:TEXTULNG);          02S0307 01031000
         ICM   @15,12,TEXTULNG(@10)                             02S0307 01032000
         SRA   @15,16                                           02S0307 01033000
         BCTR  @15,0                                            02S0307 01034000
         EX    @15,@SM02194                                     02S0307 01035000
*                                      /*COPY MEMBER NAME TO W/A     */ 01036000
*          IF DSNWORK(1:8) = JFCBELNM THEN /*IF MEMBER NAMES MATCH   */ 01037000
         L     @10,DSABPTR                                      02S0308 01038000
         L     @10,DSABSIOT(,@10)                               02S0308 01039000
         L     @10,SJFCBPTR(,@10)                               02S0308 01040000
         CLC   DSNWORK(8),JFCBELNM(@10)                         02S0308 01041000
         BNE   @RF00308                                         02S0308 01042000
*            DSABFND = '1'B;           /*DSAB IS ELIGIBLE TO PROCESS */ 01043000
         OI    DSABFND,B'01000000'                              02S0309 01044000
*          ELSE;                       /*NO MATCH;DSAB NOT ELIGIBLE  */ 01045000
@RF00308 DS    0H                                               02S0311 01046000
*        END;                          /*END MEMBER NAME PROCESS     */ 01047000
*      ELSE                            /*MEMBER NAME NOT SPEC'D      */ 01048000
*        DSABFND = '1'B;               /*DSAB IS ELIGIBLE TO PROCESS */ 01049000
*                                                               02S0312 01050000
         B     @RC00303                                         02S0312 01051000
@RF00303 OI    DSABFND,B'01000000'                              02S0312 01052000
*      IF DSABFND = '1'B THEN          /*IF DSAB IS ELIGIBLE         */ 01053000
@RC00303 TM    DSABFND,B'01000000'                              02S0313 01054000
         BNO   @RF00313                                         02S0313 01055000
*        DO;                           /*THEN CONTINUE PROCESSING    */ 01056000
*          RFY DSAB BASED(FRSTCATM);     /*RESET DSAB BASE           */ 01057000
*          FRSTCATM = DSABPTR;         /*GET RETURNED DSAB ADDR      */ 01058000
         L     FRSTCATM,DSABPTR                                 02S0316 01059000
*          DO WHILE TIOEDDNM(1) = ' '; /*GET 1ST DSAB IF IN CONCAT      01060000
*                                        GROUP                       */ 01061000
         B     @DE00317                                         02S0317 01062000
@DL00317 DS    0H                                               02S0318 01063000
*            FRSTCATM = DSABBCHN;      /*POINT TO PREVIOUS DSAB      */ 01064000
         L     FRSTCATM,DSABBCHN(,FRSTCATM)                     02S0318 01065000
*          END;                        /*END OF CONCAT GROUP LOOP    */ 01066000
@DE00317 L     @10,DSABTIOT(,FRSTCATM)                          02S0319 01067000
         CLI   TIOEDDNM(@10),C' '                               02S0319 01068000
         BE    @DL00317                                         02S0319 01069000
*          IF TIOEDDNM(1:8) = 'JOBLIB  ' | /*IF DDN IS JOBLIB OR,    */ 01070000
*             TIOEDDNM(1:8) = 'STEPLIB ' | /*STEPLIB OR,             */ 01071000
*             TIOEDDNM(1:8) = 'JOBCAT  ' | /*JOBCAT OR,              */ 01072000
*             TIOEDDNM(1:8) = 'STEPCAT ' THEN /*STEPCAT THEN         */ 01073000
         CLC   TIOEDDNM(8,@10),@CC02141                         02S0320 01074000
         BE    @RT00320                                         02S0320 01075000
         CLC   TIOEDDNM(8,@10),@CC02143                         02S0320 01076000
         BE    @RT00320                                         02S0320 01077000
