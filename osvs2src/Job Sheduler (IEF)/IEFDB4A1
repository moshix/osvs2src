         TITLE 'IEFDB4A1 - DYNAMIC UNALLOCATION PROCESSOR              *00001000
                        '                                               00002000
IEFDB4A1 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(24)                                          01S0001 00007000
         DC    C'IEFDB4A1 78089  UZ17548 '                      01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @11,0                                            01S0001 00011000
@PSTART  LA    @12,4095(,@11)                                   01S0001 00012000
         USING @PSTART,@11                                      01S0001 00013000
         USING @PSTART+4095,@12                                 01S0001 00014000
         L     @00,@SIZDATD                                     01S0001 00015000
         GSPACE   R,LV=(0)                                              00016000
         LR    @10,@01                                          01S0001 00017000
         USING @DATD,@10                                        01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         LM    @00,@01,20(@13)                                  01S0001 00020000
         ST    @10,8(,@13)                                      01S0001 00021000
         LR    @13,@10                                          01S0001 00022000
         MVC   @PC00001(32),0(@01)                              01S0001 00023000
JSCBDISP EQU 188                                                        00024000
JSCBAOS1 EQU  72                                                        00025000
JSCBAOS2 EQU 192                                                        00026000
*CMUNFMAP = CMUNFMAP && CMUNFMAP;      /*ZERO OUT FUNCTION MAP       */ 00027000
         LA    @15,FNCTMAP                                      01S0134 00028000
         XC    CMUNFMAP(4,@15),CMUNFMAP(@15)                    01S0134 00029000
*CMDISPRC = '1'B;                      /*PROCESS DISPOSITIONS        */ 00030000
*CMDSRLSE = '1'B;                      /*RELEASE DATA SETS           */ 00031000
*CMUNUNIT = '1'B;                      /*UNALLOCATE UNITS            */ 00032000
*CMVLRLSE = '1'B;                      /*RELEASE VOLUMES             */ 00033000
*CMFDSABS = '1'B;                      /*FREE DSABS                  */ 00034000
         OI    CMDISPRC(@15),B'00101111'                        01S0139 00035000
*CMRDDENT = '1'B;                      /*RELEASE TIOT DD ENTRIES     */ 00036000
*CMTIOTNQ = '1'B;                      /*TIOT IS ENQ'D ON            */ 00037000
         OI    CMRDDENT(@15),B'00001001'                        01S0141 00038000
*CMJES3='1'B;                          /*INVOKE JES3         @Y30QPPB*/ 00039000
         OI    CMJES3(@15),B'00000001'                          01S0142 00040000
*IF INCMALL = '1'B THEN                /*IF MSGLEVEL = 1             */ 00041000
         L     @14,@PC00001+16                                  01S0143 00042000
         L     @01,JCTPTR(,@14)                                 01S0143 00043000
         TM    INCMALL(@01),B'10000000'                         01S0143 00044000
         BNO   @RF00143                                         01S0143 00045000
*  CMDMSGS = '1'B;                     /*ISSUE MESSAGES              */ 00046000
         OI    CMDMSGS(@15),B'10000000'                         01S0144 00047000
*IF BASTCNT ^= 0 THEN                  /*IF MONITOR STATUS COUNT ^= 0*/ 00048000
@RF00143 L     @01,CVTPTR                                       01S0145 00049000
         L     @01,CVTMSER(,@01)                                01S0145 00050000
         LH    @15,BASTCNT(,@01)                                01S0145 00051000
         LTR   @15,@15                                          01S0145 00052000
         BZ    @RF00145                                         01S0145 00053000
*  CMMNSTAT = '1'B;                    /*SET MONITOR STATUS BIT      */ 00054000
         LA    @15,FNCTMAP                                      01S0146 00055000
         OI    CMMNSTAT(@15),B'01000000'                        01S0146 00056000
*/********************************************************************/ 00057000
*/*                                                                  */ 00058000
*/* SEGMENT NAME - RBBUILD                                           */ 00059000
*/*                                                                  */ 00060000
*/* DESCRIPTIVE NAME - REQUEST BLOCK BUILD                           */ 00061000
*/*                                                                  */ 00062000
*/* FUNCTION  - ISSUE GETMAIN FOR AND FILL IN REQUEST BLOCKS FOR     */ 00063000
*/*             COMMON UNALLOCATION, AND SMF PARM LIST ENTRIES.      */ 00064000
*/*                                                                  */ 00065000
*/* OPERATION - A SCAN OF THE LIST OF DSABPTRS IS MADE, DETERMINING  */ 00066000
*/*             THE NUMBER OF ALLOCATIONS TO BE UNALLOCATED,         */ 00067000
*/*             COUNTING ONE FOR EACH DSABPTR IN THE LIST AND ONE FOR*/ 00068000
*/*             ANY DSAB THAT IS PERMANENTLY CONCATENATED TO IT.     */ 00069000
*/*             A GETMAIN IS THEN ISSUED FOR CORE FOR THIS NUMBER    */ 00070000
*/*             OF COMMON UNALLOCATION REQUEST BLOCKS (INCLUDING     */ 00071000
*/*             THE OPTIONAL FIELD PTR IF OVERRIDING SYSOUT          */ 00072000
*/*             USER WAS SPECIFIED), AND FOR SMF PARM LIST           */ 00073000
*/*             ENTRIES. FOR EACH DSAB TO BE UNALLOCATED, A          */ 00074000
*/*             REQUEST BLOCK IS FILLED IN AND CHAINED TO THE        */ 00075000
*/*             PRECEEDING RB. ALSO AN ENTRY IN THE SMF PARM         */ 00076000
*/*             LIST IS FILLED IN. THIS LIST IS BUILT NOW AS         */ 00077000
*/*             TIOT ENTRIES ARE FREED DURING UNALLOCATION,          */ 00078000
*/*             DISALLOWING THE BUILDING OF THE PARM LIST            */ 00079000
*/*             AFTER INVOKING COMMON UNALLOCATION.                  */ 00080000
*/*                                                                  */ 00081000
*/* ENTRY -                                                          */ 00082000
*/*   INPUT DATA - DSABPLST - LIST OF PTRS TO DSABS FOR UNALLOCATION */ 00083000
*/*                DSABPCNT - COUNT OF DSABPTRS IN LIST              */ 00084000
*/*                UNALKEYT - UNALLOCATION KEY TABLE                 */ 00085000
*/*                                                                  */ 00086000
*/* EXIT -                                                           */ 00087000
*/*  OUTPUT DATA - CHAIN OF COMMON UNALLOCATION REQUEST BLOCKS       */ 00088000
*/*                SMF PARAMETER LIST BUILT                          */ 00089000
*/*                                                                  */ 00090000
*/********************************************************************/ 00091000
*IF OVSUSTUP ^= 0 THEN                 /*IF OVERRIDING USER WAS SPECD*/ 00092000
@RF00145 L     @15,@PC00001+20                                  02S0147 00093000
         L     @01,KEYTABP(,@15)                                02S0147 00094000
         L     @15,OVSUSTUP(,@01)                               02S0147 00095000
         LTR   @15,@15                                          02S0147 00096000
         BZ    @RF00147                                         02S0147 00097000
*  DO;                                 /*WILL NEED OPTIONAL RB FLD   */ 00098000
*    OPTFLDLN = 4;                     /*SET OPTFLDLN FOR RB GETMAIN */ 00099000
         LA    OPTFLDLN,4                                       02S0149 00100000
*    RFY TEXTUNIT BASED(OVSUSTUP);     /*RESET TEXT UNIT BASE PTR    */ 00101000
*    OVRDNGID = '';                    /*BLANK OUT 8 BYTE WORKAREA   */ 00102000
         MVI   OVRDNGID,C' '                                    02S0151 00103000
         MVC   OVRDNGID+1(7),OVRDNGID                           02S0151 00104000
*    OVRDNGID(1:TEXTULNG) = TEXTUPAR(1:TEXTULNG);               02S0152 00105000
         ICM   @09,12,TEXTULNG(@15)                             02S0152 00106000
         SRA   @09,16                                           02S0152 00107000
         BCTR  @09,0                                            02S0152 00108000
         EX    @09,@SM02875                                     02S0152 00109000
*                                      /*MOVE ID INTO WORKAREA       */ 00110000
*  END;                                /*END IF SYSOUT USER SPECD    */ 00111000
*ELSE                                  /*SYSOUT USER NOT SPEC'D      */ 00112000
*  OPTFLDLN = 0;                       /*DON'T NEED OPTIONAL FIELD   */ 00113000
         B     @RC00147                                         02S0154 00114000
@RF00147 SLR   OPTFLDLN,OPTFLDLN                                02S0154 00115000
*COUNT = 0;                            /*INIT TOTAL COUNT OF RBS     */ 00116000
@RC00147 SLR   COUNT,COUNT                                      02S0155 00117000
*DO WRKINDEX = 1 BY 1 WHILE WRKINDEX <= DSABPCNT;               02S0156 00118000
         LA    WRKINDEX,1                                       02S0156 00119000
         B     @DE00156                                         02S0156 00120000
@DL00156 DS    0H                                               02S0157 00121000
*                                      /*LOOP THROUGH DSAB PTR LIST  */ 00122000
*  DSABPTR = DSABPLST(WRKINDEX);       /*POINT TO DSAB IN LIST       */ 00123000
         LR    @09,WRKINDEX                                     02S0157 00124000
         SLA   @09,2                                            02S0157 00125000
         L     @06,@PC00001                                     02S0157 00126000
         L     @06,DSABPLP(,@06)                                02S0157 00127000
         AL    @06,@CF02877                                     02S0157 00128000
         L     DSABPTR,DSABPLST(@09,@06)                        02S0157 00129000
*  COUNT = COUNT + 1;                  /*INCREMENT TOTAL COUNT OF RBS*/ 00130000
         AL    COUNT,@CF00088                                   02S0158 00131000
*  DO WHILE (DSABFCHN ^= 0  &          /*LOOP TO END OF CHAIN OR     */ 00132000
*      DSABFCHN->DSABSIOT->SCTDDNAM(1) = ' ');                  02S0159 00133000
         B     @DE00159                                         02S0159 00134000
@DL00159 DS    0H                                               02S0160 00135000
*                                      /*TO END OF PERM. CONCAT GROUP*/ 00136000
*    COUNT = COUNT + 1;                /*INCREMENT TOTAL COUNT OF RBS*/ 00137000
         AL    COUNT,@CF00088                                   02S0160 00138000
*    DSABPTR = DSABFCHN;               /*POINT TO NEXT DSAB          */ 00139000
         L     DSABPTR,DSABFCHN(,DSABPTR)                       02S0161 00140000
*  END;                                /*END LOOP FOR TOTAL COUNT    */ 00141000
@DE00159 L     @09,DSABFCHN(,DSABPTR)                           02S0162 00142000
         LTR   @09,@09                                          02S0162 00143000
         BZ    @DC00159                                         02S0162 00144000
         L     @09,DSABSIOT(,@09)                               02S0162 00145000
         CLI   SCTDDNAM(@09),C' '                               02S0162 00146000
         BE    @DL00159                                         02S0162 00147000
@DC00159 DS    0H                                               02S0163 00148000
*END;                                  /*END LOOP THROUGH LIST       */ 00149000
         AL    WRKINDEX,@CF00088                                02S0163 00150000
@DE00156 L     @09,@PC00001+4                                   02S0163 00151000
         C     WRKINDEX,DSABPCNT(,@09)                          02S0163 00152000
         BNH   @DL00156                                         02S0163 00153000
*R0 = COUNT * (LENGTH(CMUNRB)+OPTFLDLN+LENGTH(SMFARRAY(1)));    02S0164 00154000
         LA    @01,21                                           02S0164 00155000
         ALR   @01,OPTFLDLN                                     02S0164 00156000
         MR    R0,COUNT                                         02S0164 00157000
         LR    R0,@01                                           02S0164 00158000
*                                      /*CORE SIZE OF RBS & SMF ARRAY*/ 00159000
*R0 = R0 |SCHDSUBP;                    /*GET FROM CORRECT SUBPOOL    */ 00160000
         O     R0,@CF00118                                      02S0165 00161000
*GEN (GETMAIN R,LV=(0));               /*GET CORE FOR UNALLOC REQ BLK*/ 00162000
         GETMAIN R,LV=(0)                                               00163000
*CMRBPTR = R1;                         /*POINT TO FIRST RB           */ 00164000
         LR    CMRBPTR,R1                                       02S0167 00165000
*FSTRBPTR = R1;                        /*INIT PTR TO FIRST RB        */ 00166000
         ST    R1,FSTRBPTR                                      02S0168 00167000
*SMFARYP = R1 + COUNT *(LENGTH(CMUNRB)+OPTFLDLN);               02S0169 00168000
         LA    @05,16                                           02S0169 00169000
         ALR   @05,OPTFLDLN                                     02S0169 00170000
         MR    @04,COUNT                                        02S0169 00171000
         ALR   @05,R1                                           02S0169 00172000
         ST    @05,SMFARYP                                      02S0169 00173000
*                                      /*INIT PTR TO SMF ARRAY       */ 00174000
*SMFAINDX = 0;                         /*INIT INDEX INTO SMF ARRAY   */ 00175000
         SLR   SMFAINDX,SMFAINDX                                02S0170 00176000
*DO WRKINDEX = 1 BY 1 WHILE WRKINDEX <= DSABPCNT;               02S0171 00177000
         LA    WRKINDEX,1                                       02S0171 00178000
         B     @DE00171                                         02S0171 00179000
@DL00171 DS    0H                                               02S0172 00180000
*                                      /*LOOP THROUGH DSABPTR LIST   */ 00181000
*  DSABPTR = DSABPLST(WRKINDEX);       /*POINT TO DSAB IN LIST       */ 00182000
         LR    @06,WRKINDEX                                     02S0172 00183000
         SLA   @06,2                                            02S0172 00184000
         L     @02,@PC00001                                     02S0172 00185000
         L     @02,DSABPLP(,@02)                                02S0172 00186000
         AL    @02,@CF02877                                     02S0172 00187000
         L     DSABPTR,DSABPLST(@06,@02)                        02S0172 00188000
*  OUT = '0'B;                         /*INIT LOOP CONTROL SWITCH    */ 00189000
         NI    OUT,B'01111111'                                  02S0173 00190000
*  DO WHILE OUT = '0'B;                /*LOOP TO FILL IN  REQUEST BKS*/ 00191000
         B     @DE00174                                         02S0174 00192000
@DL00174 DS    0H                                               02S0175 00193000
*/********************************************************************/ 00194000
*/*                                                                  */ 00195000
*/* SEGMENT NAME - RBFILLIN                                          */ 00196000
*/*                                                                  */ 00197000
*/* DESCRIPTIVE NAME - COMMON UNALLOCATION REQUEST BLOCK FILL IN     */ 00198000
*/*                                                                  */ 00199000
*/* OPERATION - THIS SEGMENT SETS THE APPROPRIATE BITS AND FIELDS    */ 00200000
*/*             OF THE COMMON UNALLOCATION REQUEST BLOCK AND USES    */ 00201000
*/*             THE FOLLOWING LOGIC:                                 */ 00202000
*/*                                                                  */ 00203000
*/*             .DISPOSITION PROCESSING IS TO BE BYPASSED FOR        */ 00204000
*/*              DUMMY DATA SETS.                                    */ 00205000
*/*             .THE NORMAL DISP OF THE SIOT IS TO BE USED IF NO     */ 00206000
*/*              OVERRIDING DISP WAS SPECIFIED, OR IF ONE WAS,       */ 00207000
*/*              BUT THE DATA SET IS A PASSED DATA SET,              */ 00208000
*/*              IS A VSAM D.S., OR IS A NON SUBSYSTEM               */ 00209000
*/*              D.S. WITH A GENERATED NAME. (IN THESE LATTER CASES, */ 00210000
*/*              AN INFO CODE OF 8 IS RETURNED SAYING THE OVERRIDE   */ 00211000
*/*              WAS IGNORED.) OTHERWISE, THE OVERRIDING DISP IS     */ 00212000
*/*              USED.                                               */ 00213000
*/*             .A DETERMINATION IS MADE AS TO WHETHER OR NOT        */ 00214000
*/*              DATA SET RELEASE SHOULD BE PROCESSED FOR THIS SIOT. */ 00215000
*/*              SEE DSRELCHK SEGMENT FOR THESE CHECKS.              */ 00216000
*/*             .IF THE D.S. WAS JCL ALLOCATED, MULTIPLY ALLOCATED   */ 00217000
*/*              UNITS AND VOLUMES MUST BE CHECKED FOR. OTHERWISE,   */ 00218000
*/*              IT WAS DYNAMICALLY ALLOCATED AND MULTIPLY ALLOCATED */ 00219000
*/*              VOLUMES MUST BE CHECKED FOR ON DASD ONLY.           */ 00220000
*/*             .OVERRIDING SYSOUT INFORMATION WILL BE PROCESSED     */ 00221000
*/*              AS SPECIFIED.                                       */ 00222000
*/*                                                                  */ 00223000
*/* ENTRY -                                                          */ 00224000
*/*   INPUT DATA - COMMON UNALLOCATION REQUEST BLOCK TO BE FILLED IN */ 00225000
*/*              - SIOT TO BE UNALLOCATED                            */ 00226000
*/*              - UNALLOCATION KEY TABLE                            */ 00227000
*/*                                                                  */ 00228000
*/* EXIT -                                                           */ 00229000
*/*  OUTPUT DATA - REQUEST BLOCK FILLED IN                           */ 00230000
*/*              - INFOCODE OF 8 IF OVERRIDING DISP WAS IGNORED      */ 00231000
*/*                FOR A PARTICULAR ALLOCATION                       */ 00232000
*/*                                                                  */ 00233000
*/********************************************************************/ 00234000
*CMUNRB = CMUNRB && CMUNRB;            /*ZERO OUT REQUEST BLOCK      */ 00235000
         XC    CMUNRB(16,CMRBPTR),CMUNRB(CMRBPTR)               03S0175 00236000
*CMSIOPTR = DSABSIOT;                  /*POINT TO SIOT FOR UNALLOC   */ 00237000
*                                                               03S0176 00238000
         L     @06,DSABSIOT(,DSABPTR)                           03S0176 00239000
         ST    @06,CMSIOPTR(,CMRBPTR)                           03S0176 00240000
*IF SCTDUMMY = '1'B THEN               /*IF DATA SET IS A DUMMY      */ 00241000
         TM    SCTDUMMY(@06),B'10000000'                        03S0177 00242000
         BNO   @RF00177                                         03S0177 00243000
*  CMNODISP = '1'B;                    /*BYPASS DISP PROCESSING      */ 00244000
         OI    CMNODISP(CMRBPTR),B'10000000'                    03S0178 00245000
*ELSE                                  /*DATA SET IS NOT DUMMY       */ 00246000
*  IF OVDSPTUP = 0 THEN                /*IF NO OVERRIDING DISP SPEC'D*/ 00247000
         B     @RC00177                                         03S0179 00248000
@RF00177 L     @06,@PC00001+20                                  03S0179 00249000
         L     @06,KEYTABP(,@06)                                03S0179 00250000
         L     @06,OVDSPTUP(,@06)                               03S0179 00251000
         LTR   @06,@06                                          03S0179 00252000
         BNZ   @RF00179                                         03S0179 00253000
*    CMNMDISP = '1'B;                  /*USE NORMAL DISP             */ 00254000
         OI    CMNMDISP(CMRBPTR),B'01000000'                    03S0180 00255000
*  ELSE                                /*OVERRIDING DISP WAS SPEC'D  */ 00256000
*    IF SIOTPASS = '1'B         |      /*IF DATA SET HAS PASS DISP,  */ 00257000
*       JFCORGAM = '1'B         |      /*OR IS A VSAM DATA SET       */ 00258000
*       (SIOTGDSN = '1'B &             /*OR HAS A GENERATED DSN AND  */ 00259000
*        SIOTSSDS = '0'B) THEN         /*IS NOT A SUBSYSTEM DATA SET */ 00260000
         B     @RC00179                                         03S0181 00261000
@RF00179 L     @06,CMSIOPTR(,CMRBPTR)                           03S0181 00262000
         TM    SIOTPASS(@06),B'00010000'                        03S0181 00263000
         BO    @RT00181                                         03S0181 00264000
         L     @02,SJFCBPTR(,@06)                               03S0181 00265000
         TM    JFCORGAM(@02),B'00001000'                        03S0181 00266000
         BO    @RT00181                                         03S0181 00267000
         TM    SIOTGDSN(@06),B'00010000'                        03S0181 00268000
         BNO   @RF00181                                         03S0181 00269000
         TM    SIOTSSDS(@06),B'10000000'                        03S0181 00270000
         BNZ   @RF00181                                         03S0181 00271000
@RT00181 DS    0H                                               03S0182 00272000
*      DO;                             /*THEN IGNORE OVERRIDING DISP */ 00273000
*        INFOCODE = '0008'X;           /*RETURN INFO CODE FOR IGNORE */ 00274000
         L     @06,@PC00001+8                                   03S0183 00275000
         MVC   INFOCODE(2,@06),@CH02825                         03S0183 00276000
*        CMNMDISP = '1'B;              /*USE NORMAL DISP             */ 00277000
         OI    CMNMDISP(CMRBPTR),B'01000000'                    03S0184 00278000
*      END;                            /*END IGNORE OVERRIDING DISP  */ 00279000
*    ELSE                              /*OVERRIDING DISP NOT IGNORED */ 00280000
*      DO;                             /*HANDLE THE OVERRIDING DISP  */ 00281000
         B     @RC00181                                         03S0186 00282000
@RF00181 DS    0H                                               03S0187 00283000
*        CMORDISP = '1'B;              /*USE OVERRIDING DISP         */ 00284000
         OI    CMORDISP(CMRBPTR),B'00010000'                    03S0187 00285000
*        RFY TEXTUNIT BASED(OVDSPTUP); /*RESET TEXT UNIT BASE        */ 00286000
*        IF TEXTUPAR(1) = '08'X THEN   /*IF DISP SPEC'D IS KEEP      */ 00287000
         L     @06,@PC00001+20                                  03S0189 00288000
         L     @06,KEYTABP(,@06)                                03S0189 00289000
         L     @06,OVDSPTUP(,@06)                               03S0189 00290000
         CLI   TEXTUPAR(@06),X'08'                              03S0189 00291000
         BNE   @RF00189                                         03S0189 00292000
*          CMOKEEP = '1'B;             /*OVERRIDE WITH KEEP          */ 00293000
         OI    CMOKEEP(CMRBPTR),B'00001000'                     03S0190 00294000
*        ELSE                          /*KEEP NOT SPECIFIED          */ 00295000
*          IF TEXTUPAR(1) = '04'X THEN /*IF SPEC'D. DISP IS DELETE   */ 00296000
         B     @RC00189                                         03S0191 00297000
@RF00189 L     @06,@PC00001+20                                  03S0191 00298000
         L     @06,KEYTABP(,@06)                                03S0191 00299000
         L     @06,OVDSPTUP(,@06)                               03S0191 00300000
         CLI   TEXTUPAR(@06),X'04'                              03S0191 00301000
         BNE   @RF00191                                         03S0191 00302000
*            DO;                       /*HANDLE DELETE               */ 00303000
*              CMODEL = '1'B;          /*OVERRIDE WITH DELETE        */ 00304000
         OI    CMODEL(CMRBPTR),B'00000100'                      03S0193 00305000
*              CMDEL = '1'B;           /*SET DELETE FOR SUBSYSTEM    */ 00306000
         OI    CMDEL(CMRBPTR),B'10000000'                       03S0194 00307000
*            END;                      /*END IF OVRRDING DELETE SPECD*/ 00308000
*          ELSE                        /*DELETE OVERRIDE NOT SPEC'D  */ 00309000
*            IF TEXTUPAR(1) = '02'X THEN /*IF SPEC'D DISP IS CATALOG */ 00310000
         B     @RC00191                                         03S0196 00311000
@RF00191 L     @06,@PC00001+20                                  03S0196 00312000
         L     @06,KEYTABP(,@06)                                03S0196 00313000
         L     @06,OVDSPTUP(,@06)                               03S0196 00314000
         CLI   TEXTUPAR(@06),X'02'                              03S0196 00315000
         BNE   @RF00196                                         03S0196 00316000
*              CMOCATLG = '1'B;        /*OVERRIDE WITH CATALOG       */ 00317000
         OI    CMOCATLG(CMRBPTR),B'00000010'                    03S0197 00318000
*            ELSE                      /*CATALOG NOT SPEC'D          */ 00319000
*              CMOUNCAT = '1'B;        /*MUST BE UNCATALOG OVERRIDE  */ 00320000
         B     @RC00196                                         03S0198 00321000
@RF00196 OI    CMOUNCAT(CMRBPTR),B'00000001'                    03S0198 00322000
*      END;                            /*END IF USING OVRRIDING DISP */ 00323000
*                                                               03S0199 00324000
@RC00196 DS    0H                                               03S0199 00325000
@RC00191 DS    0H                                               03S0199 00326000
@RC00189 DS    0H                                               03S0200 00327000
*/********************************************************************/ 00328000
*/*                                                                  */ 00329000
*/* SEGMENT NAME - DSRELCHK                                          */ 00330000
*/*                                                                  */ 00331000
*/* DESCRIPTIVE NAME - DATA SET RELEASE CHECKS                       */ 00332000
*/*                                                                  */ 00333000
*/* FUNCTION  - DETERMINE WHETHER OR NOT DATA SET RELEASE SHOULD BE  */ 00334000
*/*             DONE FOR THE PARTICULAR DSAB BEING UNALLOCATED. IF   */ 00335000
*/*             RELEASE PROCESSING SHOULD BE BYPASSED, THIS BIT IN   */ 00336000
*/*             THE RB BEING BUILT FOR THE DSAB IS SET ON.           */ 00337000
*/*                                                                  */ 00338000
*/* OPERATION - .IF THE DSAB IS FOR A DUMMY DATA SET, A SUBSYSTEM    */ 00339000
*/*              DATA SET, OR A VAM DATA SET, THEN RELEASE PROCESSING*/ 00340000
*/*              IS NOT NECESSARY AND WILL BE BYPASSED.              */ 00341000
*/*             .OTHERWISE CHECKS ARE MADE TO DETERMINE IF THERE IS  */ 00342000
*/*              ANOTHER ALLOCATION OF THIS SAME DSN, WHICH IS       */ 00343000
*/*              NOT GOING TO BE UNALLOCATED AS PART OF THIS REQUEST.*/ 00344000
*/*              IF THERE IS, THE DSN IS CONSIDERED MULTI-           */ 00345000
*/*              ALLOCATED AND RELEASE PROCESSING IS BYPASSED FOR IT.*/ 00346000
*/*             .IF THERE IS MORE THAN ONE ALLOCATION OF THE DSN     */ 00347000
*/*              BEING UNALLOCATED, BUT ALL OF WHICH ARE ALSO TO     */ 00348000
*/*              BE UNALLOCATED AS PART OF THIS REQUEST, D.S. RELEASE*/ 00349000
*/*              WILL NOT BE BYPASSED IF THIS RB IS THE LAST ONE TO  */ 00350000
*/*              BE BUILT FOR THIS DATA SET. OTHERWISE THE BIT TO    */ 00351000
*/*              BYPASS IT IS SET IN THE RB.                         */ 00352000
*/*             .IF THERE IS NO OTHER ALLOCATION OF THIS DSN,        */ 00353000
*/*              THEN RELEASE PROCESSING SHOULD BE PERFORMED AND THE */ 00354000
*/*              BIT TO BYPASS IT IN THE RB IS LEFT OFF.             */ 00355000
*/*             .IF IT HAS BEEN DETERMINED THAT D.S. RELEASE @ZA04136*/ 00356000
*/*              SHOULD BE PERFORMAED, AN ENQ TEST ON        @ZA04136*/ 00357000
*/*              SYSZOPEN IS ISSUED TO ENSURE NO DCBS        @ZA04136*/ 00358000
*/*              ARE OPEN (TYPE=J) TO THE DATA SET WHEN      @ZA04136*/ 00359000
*/*              IT IS UNALLOCATED AND DEQ'D. (OPENJ WILL    @ZA04136*/ 00360000
*/*              ENQ ON THIS MAJOR NAME WHEN THE DSN IN      @ZA04136*/ 00361000
*/*              THE JFCB IS BEING CHANGED.) IF AN OPENJ     @ZA04136*/ 00362000
*/*              TO THIS DATA SET IS OUTSTANDING, THE BIT    @ZA04136*/ 00363000
*/*              IN THE RB TO BYPASS D.S. RELEASE FOR IT     @ZA04136*/ 00364000
*/*              WILL BE SET.                                @ZA04136*/ 00365000
*/*             .THE DETERMINATIONS MADE ARE AS FOLLOWS:             */ 00366000
*/*              - A SCAN OF THE DSAB CHAIN IS MADE UNTIL EACH DSAB  */ 00367000
*/*                IS CHECKED, OR UNTIL A DETERMINATION IS MADE      */ 00368000
*/*                THAT D.S. RELEASE SHOULD BE BYPASSED FOR THIS     */ 00369000
*/*                RB BEING BUILT.                                   */ 00370000
*/*              - IF THERE ARE NO DSABS IN THE CHAIN WITH THE       */ 00371000
*/*                SAME DSN AS THE ONE BEING PROCESSED, D.S. RELEASE */ 00372000
*/*                WILL NOT BE BYPASSED AND THE BIT IN THE RB IS     */ 00373000
*/*                LEFT OFF.(ALIAS/GDG STRUCTURE DSN'S       @YM06112*/ 00374000
*/*                MUST ALSO BE CONSIDERED HERE, AS THEY     @YM06112*/ 00375000
*/*                TOO ARE ENQ'D UPON AND APPEAR THE SAME    @YM06112*/ 00376000
*/*                IN THE DSENQ TABLE)                       @YM06112*/ 00377000
*/*              - IF ANOTHER DSAB IN THE CHAIN IS FOR THE SAME DSN  */ 00378000
*/*                (NOT A QNAME) AS THE ONE FOR WHICH THE RB IS BEING*/ 00379000
*/*                BUILT, THE DSAB LIST FOR UNALLOCATION IS CHECKED. */ 00380000
*/*              - IF THE OTHER DSAB IN THE CHAIN IS NOT IN THE      */ 00381000
*/*                UNALLOC LIST, DATA SET RELEASE IS BYPASSED.       */ 00382000
*/*              - IF THE OTHER DSAB IN THE CHAIN IS ALSO IN THE     */ 00383000
*/*                UNALLOC LIST, D.S. RESERVATION FOR THE DSAB BEING */ 00384000
*/*                PROCESSED WILL BE BYPASSED IF THE INDEX INTO THE  */ 00385000
*/*                UNALLOC LIST OF THE OTHER DSAB IN THE CHAIN IS    */ 00386000
*/*                GREATER THAN THAT OF THE DSAB BEING PROCESSED (THE*/ 00387000
*/*                RELEASE FOR THE DATA SET, IF NECESSARY, WILL BE   */ 00388000
*/*                DONE WHEN THE LATTER DSABPTR IN THE LIST IS       */ 00389000
*/*                PROCESSED). OTHERWISE D.S. RELEASE IS NOT         */ 00390000
*/*                BYPASSED, BUT THE SEARCH OF THE DSAB CHAIN        */ 00391000
*/*                CONTINUES TO SEE IF THERE ARE OTHER DSABS FOR     */ 00392000
*/*                WHICH THE ABOVE CHECKS SHOULD BE REPEATED.        */ 00393000
*/*              - WHEN COMPARING WITH A DSAB ON THE DSABPTR LIST,   */ 00394000
*/*                ALL MEMBERS OF THE GROUP ARE CHECKED, IF THE      */ 00395000
*/*                DSAB IN THE LIST IS PERMANENTLY CONCATENATED.     */ 00396000
*/*                THIS IS BECAUSE RB'S WILL BE BUILT FOR EACH       */ 00397000
*/*                DSAB IN A PERMANENTLY CONCATED GROUP.             */ 00398000
*/*               -AN ENQUEUE WITH RET=TEST IS ISSUED USING  @ZA04136*/ 00399000
*/*                SYSZOPEN AS MAJOR AND THE DATA SET NAME AS@ZA04136*/ 00400000
*/*                A MINOR NAME. IF THE DATA SET NAME IS     @ZA04136*/ 00401000
*/*                ENQUEUED THEN RELEASE PROCESSING IS NOT   @ZA04136*/ 00402000
*/*                PERFORMED.                                @ZA04136*/ 00403000
*/*                                                                  */ 00404000
*/* ENTRY -                                                          */ 00405000
*/*    INPUT DATA - DSQFRSTP - PTR TO FIRST DSAB IN CHAIN            */ 00406000
*/*                 DSABPLST - LIST OF PTRS TO DSABS FOR UNALLOCATION*/ 00407000
*/*                 DSABPCNT - COUNT OF DSABPTRS IN LIST             */ 00408000
*/*                 CMUNRB   - UNALLOCATION REQUEST BLOCK BEING BUILT*/ 00409000
*/*                            WITH CMNODSR BIT ZERO ON ENTRY        */ 00410000
*/*                 WRKINDEX - INDEX INTO LIST OF DSABPTR BEING      */ 00411000
*/*                            PROCESSED                             */ 00412000
*/*                                                                  */ 00413000
*/* EXIT -                                                           */ 00414000
*/*    OUTPUT DATA - CMNODSR  - BIT IN RB SET ON IF DATA SET         */ 00415000
*/*                             RELEASE IS TO BE BYPASSED            */ 00416000
*/*                                                                  */ 00417000
*/********************************************************************/ 00418000
*RFY DSAB BASED(WRKDSABP);             /*RESET DSAB BASE PTR         */ 00419000
@RC00181 DS    0H                                               04S0200 00420000
@RC00179 DS    0H                                               04S0200 00421000
@RC00177 DS    0H                                               04S0201 00422000
