         B     @DE00482                                         04S0482 01969000
@DL00482 DS    0H                                               04S0483 01970000
*      IF SIOTSAVP = CMSIOPTR THEN     /*IF THIS IS SIOT TO DECHAIN  */ 01971000
         L     @09,CMSIOPTR(,CMRBPTR)                           04S0483 01972000
         CR    SIOTSAVP,@09                                     04S0483 01973000
         BNE   @RF00483                                         04S0483 01974000
*        DO;                           /*THEN DECHAIN SIOT           */ 01975000
*          PRESIOTP -> SIOTNPTR = SIOTNPTR;                     04S0485 01976000
         L     @05,SIOTNPTR(,@09)                               04S0485 01977000
         ST    @05,SIOTNPTR(,PRESIOTP)                          04S0485 01978000
*                                      /*UPDATE PRIOR SIOTS CHAIN PTR*/ 01979000
*          PRESIOTP -> SCTPSIOT = SCTPSIOT;                     04S0486 01980000
         MVC   SCTPSIOT(3,PRESIOTP),SCTPSIOT(@09)               04S0486 01981000
*                                      /*UPDATE PRIOR SIOTS CHAIN SVA*/ 01982000
*          IF SCTLSIOT = SIOTDSKA THEN /*IF DECHAINED SIOT WAS LAST  */ 01983000
         L     @05,@PC00001+16                                  04S0487 01984000
