         OR    @02,@15                                          02S0318 01028000
         STH   @02,INFOCODE(,@09)                               02S0318 01029000
*                  RSNCDSET = '1'B;    /*SAY REASON CODE HAS BEEN SET*/ 01030000
@RC00316 DS    0H                                               02S0319 01031000
@RC00312 OI    RSNCDSET,B'10000000'                             02S0319 01032000
*                END;                  /*END IF INTERNAL(NO CORE)       01033000
*                                        ERROR                       */ 01034000
*              ELSE                    /*NOT INTERNAL ERROR          */ 01035000
*                IF CMNODEN = '1'B THEN /*IF NO TAPE DENSITY FOR        01036000
*                                        CATALOG             @YM06419*/ 01037000
         B     @RC00310                                         02S0321 01038000
@RF00310 TM    CMNODEN(CMRBPTR),B'00000100'                     02S0321 01039000
         BNO   @RF00321                                         02S0321 01040000
*                  DO;                 /*PROCESS ERROR       @YM06419*/ 01041000
*                    INFOCODE = '0020'X | CMRETCDE; /*SET INFO CODE     01042000
*                                                            @YM06419*/ 01043000
         L     @09,@PC00001+8                                   02S0323 01044000
         LA    @02,32                                           02S0323 01045000
         LH    @15,CMRETCDE(,CMRBPTR)                           02S0323 01046000
         OR    @02,@15                                          02S0323 01047000
         STH   @02,INFOCODE(,@09)                               02S0323 01048000
*                    RSNCDSET = '1'B;  /*REASON CODE SET     @YM06419*/ 01049000
         OI    RSNCDSET,B'10000000'                             02S0324 01050000
*                  END;                /*END DENSITY ERROR   @YM06419*/ 01051000
*                ELSE;                 /*IGNORE RET CODE     @YM06419*/ 01052000
@RF00321 DS    0H                                               02S0327 01053000
*        END;                          /*END RB RETCDE PROCESS       */ 01054000
*                                                               02S0327 01055000
@RC00310 DS    0H                                               02S0327 01056000
@RC00305 DS    0H                                               02S0327 01057000
@RC00300 DS    0H                                               02S0328 01058000
*/********************************************************************/ 01059000
*/*                                                                  */ 01060000
*/* SEGMENT NAME - ANAMEDEQ                                  @YM06112*/ 01061000
*/*                                                                  */ 01062000
*/* DESCRIPTIVE NAME - ALIAS/ALTERNATE DSN DEQ PROCESSING            */ 01063000
*/*                    (SEGMENT ADDED FOR PTM 06112)                 */ 01064000
*/*                                                                  */ 01065000
*/* FUNCTION -                                                       */ 01066000
*/*           DEQ PROCESSING FOR REAL DSNAMES IN THE JFCB WILL       */ 01067000
*/*           HAVE BEEN DONE BY COMMON UNALLOCATION AS NECESSARY.    */ 01068000
*/*           ALIAS AND ALTERNATE DSNAMES HOWEVER WILL NOT HAVE      */ 01069000
*/*           BEEN DEQ'D. THEREFORE IT IS THE PURPOSE OF THIS        */ 01070000
*/*           SEGMENT TO PERFORM DEQ PROCESSING FOR THESE NAMES      */ 01071000
*/*           RATHER THAN WAIT UNTIL THE END OF THE STEP             */ 01072000
*/*           FOR THEM TO BE FREED.                                  */ 01073000
*/*                                                                  */ 01074000
*/* OPERATION -                                                      */ 01075000
*/*           THIS SEGMENT LIES WITHIN A LOOP OF THE COMMON UN-      */ 01076000
*/*           ALLOCATION RBS, PERFORMING WRAP UP PROCESSING FOR EACH */ 01077000
*/*           JUST UNALLOCATED SIOT. IF AN ALIAS OR GDG STRUCTURE    */ 01078000
*/*           DSN IS ASSOCIATED WITH A SIOT, A SCAN OF THE DSAB CHAIN*/ 01079000
*/*           IS MADE TO SEE IF THE NAME IS ASSOCIATED WITH SOME     */ 01080000
*/*           OTHER SIOT WHICH IS STILL ALLOCATED. (REAL NAMES,      */ 01081000
*/*           ALIASES, AND ALTERNATES MUST ALL BE LOOKED AT WHILE    */ 01082000
*/*           SCANNING THE DSAB CHAIN, BECAUSE A NAME WILL BE ENQ'D  */ 01083000
*/*           UPON ONLY ONCE, EVEN IF IT IS THE NAME FOR MORE THAN   */ 01084000
*/*           ONE OF THE ABOVE TYPES OF DATA SETS.)                  */ 01085000
*/*                                                                  */ 01086000
*/*           IF THE DSN IS STILL ASSOCIATED WITH SOME OTHER         */ 01087000
*/*           ALLOCATION, NO DEQ PROCESSING IS DONE FOR IT.          */ 01088000
*/*                                                                  */ 01089000
*/*           IF THE DSN IS NOT ASSOCIATED WITH ANY OTHER            */ 01090000
*/*           CURRENTLY ALLOCATED SIOT, THE DATA SET RESERVATION/    */ 01091000
*/*           RELEASE SERVICE ROUTINE IS CALLED TO DEQ THE PARTICULAR*/ 01092000
*/*           DSNAME.                                                */ 01093000
*/*                                                                  */ 01094000
*/*           NOTES - A SIOT CANNOT HAVE BOTH AN ALIAS AND AN        */ 01095000
*/*                   ALTERNATE NAME ASSOCIATED WITH IT.             */ 01096000
*/*                                                                  */ 01097000
*/*                 - GDG STRUCTURE NAMES ARE THE ONLY ALTERNATE     */ 01098000
*/*                   NAMES ENQ'D ON.                                */ 01099000
*/*                                                                  */ 01100000
*/*                 - AT THIS POINT DSAB'S HAVE BEEN FREED. THEREFORE*/ 01101000
*/*                   THE GDG STRUCTURE NAME TO BE DEQ'D IS TAKEN    */ 01102000
*/*                   FROM THE FIRST PART OF THE REAL NAME IN THE    */ 01103000
*/*                   JFCB, RATHER THAN THE DSAB ALTERNATE NAME FIELD*/ 01104000
*/*                                                                  */ 01105000
*/* ENTRY -                                                          */ 01106000
*/*    INPUT DATA -                                                  */ 01107000
*/*                 CMUNRB   - COMMON UNALLOCATION RB WITH PTR TO    */ 01108000
*/*                            THE SIOT JUST UNALLOCATED.            */ 01109000
*/*                 DSQFRSTP - PTR TO 1ST DSAB IN THE CHAIN          */ 01110000
*/*                 DEQCALL1 - IF ON, AT LEAST ONE DSN HAS           */ 01111000
*/*                            ALREADY BEEN DEQ'D, AND DSRRFMAP AND  */ 01112000
*/*                            DSENQFP ARE SET UP APPROPRIATELY      */ 01113000
*/*                                                                  */ 01114000
*/* EXIT -                                                           */ 01115000
*/*    OUTPUT DATA -                                                 */ 01116000
*/*                 DSRRFMAP - OTHER BITS SET AFTER THE 1ST CALL FOR */ 01117000
*/*                            SUBSEQUENT CALLS TO IEFAB4DC          */ 01118000
*/*                                                                  */ 01119000
*/*                 DSENQFP  - PTR TO 1ST DSENQ RECORD, SET AFTER 1ST*/ 01120000
*/*                            CALL FOR SUBSEQUENT CALLS             */ 01121000
*/*                                                                  */ 01122000
*/*                 DEQCALL1 - SET ON AFTER 1ST CALL OF IEFAB4DC,    */ 01123000
*/*                            USED IN SUBSEQUENT CALLS              */ 01124000
*/*                                                                  */ 01125000
*/*                 LSTWEPAP - UPDATED BY IEFAB4DC FOR ANY EPAS      */ 01126000
*/*                            ADDED TO THE WRITE CHAIN              */ 01127000
*/*                                                                  */ 01128000
*/********************************************************************/ 01129000
*/* THIS CODE SEGMENT WAS ADDED FOR FIXING PTM06112          @YM06112*/ 01130000
*                                                               03S0328 01131000
*RFY DSAB BASED(WRKDSABP);             /*RESET DSAB BASE PTR         */ 01132000
@RF00298 DS    0H                                               03S0329 01133000
*RFY JFCBX BASED(SIOTJFX);             /*RESET JFCBX BASE PTR        */ 01134000
*DEQDSNAM = '';                        /*BLANK OUT DSN TO BE DEQ'D   */ 01135000
         MVI   DEQDSNAM,C' '                                    03S0330 01136000
         MVC   DEQDSNAM+1(43),DEQDSNAM                          03S0330 01137000
*IF SIOALIAS = '1'B THEN               /*IF ALIAS DSN EXISTS         */ 01138000
         L     @09,CMSIOPTR(,CMRBPTR)                           03S0331 01139000
         TM    SIOALIAS(@09),B'10000000'                        03S0331 01140000
         BNO   @RF00331                                         03S0331 01141000
*  DEQDSNAM = JFCBXNAM;                /*COPY ALIAS FOR DEQ          */ 01142000
         L     @09,SIOTJFX(,@09)                                03S0332 01143000
         MVC   DEQDSNAM(44),JFCBXNAM(@09)                       03S0332 01144000
*ELSE                                  /*NO ALIAS DSN                */ 01145000
*  IF SIOTGALL = '1'B |                /*IF GDG ALL MEMBER           */ 01146000
*     SCTSGDGS = '1'B THEN             /*OR A GDG SINGLE             */ 01147000
         B     @RC00331                                         03S0333 01148000
@RF00331 L     @09,CMSIOPTR(,CMRBPTR)                           03S0333 01149000
         TM    SIOTGALL(@09),B'00100000'                        03S0333 01150000
         BO    @RT00333                                         03S0333 01151000
         TM    SCTSGDGS(@09),B'10000000'                        03S0333 01152000
         BNO   @RF00333                                         03S0333 01153000
@RT00333 DS    0H                                               03S0334 01154000
*    DO;                               /*GET GDG STRUCTURE NAME      */ 01155000
*      GDGNMPTR=ADDR(JFCBDSNM) + 43;   /*GOTO END OF DSN FIELD       */ 01156000
         L     @09,CMSIOPTR(,CMRBPTR)                           03S0335 01157000
         LA    GDGNMPTR,43                                      03S0335 01158000
         AL    GDGNMPTR,SJFCBPTR(,@09)                          03S0335 01159000
*      DO WHILE GDGCHAR = ' ';         /*GET LAST NON-BLANK          */ 01160000
         B     @DE00336                                         03S0336 01161000
@DL00336 DS    0H                                               03S0337 01162000
*        GDGNMPTR = GDGNMPTR -1;       /*BACK UP ONE                 */ 01163000
         BCTR  GDGNMPTR,0                                       03S0337 01164000
*      END;                            /*AT LAST NON-BLANK CHAR      */ 01165000
@DE00336 CLI   GDGCHAR(GDGNMPTR),C' '                           03S0338 01166000
         BE    @DL00336                                         03S0338 01167000
*      GDGLEN = GDGNMPTR - ADDR(JFCBDSNM)-8; /*GET STRUCTURE NAME LEN*/ 01168000
         L     @09,CMSIOPTR(,CMRBPTR)                           03S0339 01169000
         L     @09,SJFCBPTR(,@09)                               03S0339 01170000
         LR    GDGLEN,GDGNMPTR                                  03S0339 01171000
         SLR   GDGLEN,@09                                       03S0339 01172000
         SL    GDGLEN,@CF00075                                  03S0339 01173000
*      DEQDSNAM(1:GDGLEN) = JFCBDSNM(1:GDGLEN); /*COPY NAME FOR DEQ  */ 01174000
         LR    @05,GDGLEN                                       03S0340 01175000
         BCTR  @05,0                                            03S0340 01176000
         EX    @05,@SM02890                                     03S0340 01177000
*    END;                              /*END IF GDG                  */ 01178000
*  ELSE;                               /*NO GDG STRUCTURE NAME       */ 01179000
@RF00333 DS    0H                                               03S0343 01180000
*IF DEQDSNAM(1) ^= ' ' THEN            /*IF DSN TO DEQ               */ 01181000
@RC00331 CLI   DEQDSNAM,C' '                                    03S0343 01182000
         BE    @RF00343                                         03S0343 01183000
*  DO;                                 /*DEQ PROCESSING              */ 01184000
*    DODEQSW = '1'B;                   /*ASSUME DEQ TO BE DONE       */ 01185000
         OI    DODEQSW,B'10000000'                              03S0345 01186000
*    WRKDSABP = DSQFRSTP;              /*GET 1ST ALLOCATED DSAB      */ 01187000
         L     @09,@PC00001+16                                  03S0346 01188000
         L     @09,JSCBPTR(,@09)                                03S0346 01189000
         L     @09,JSCDSABQ(,@09)                               03S0346 01190000
         L     WRKDSABP,DSQFRSTP(,@09)                          03S0346 01191000
*    DO WHILE WRKDSABP ^= 0 &          /*PROCESS TO END OF DSAB CHAIN*/ 01192000
*             DODEQSW = '1'B;          /*OR TILL NO DEQ IS DETERMINED*/ 01193000
         B     @DE00347                                         03S0347 01194000
@DL00347 DS    0H                                               03S0348 01195000
*      IF (DSABSIOT->SJFCBPTR->JFCBDSNM = DEQDSNAM & /*IF DSAB DSN      01196000
*                                       MATCHES                      */ 01197000
*          DSABSIOT->SIOTQNAM = '0'B) | /*AND IS NOT A QNAME, OR     */ 01198000
*          (DSABSIOT->SIOALIAS = '1'B & /*DSAB HAS ALIAS ASSOCIATED  */ 01199000
*          DSABSIOT->SIOTJFX->JFCBXNAM=DEQDSNAM) |  /*AND       03S0348 01200000
*                                        ALIAS DSN FIELD MATCHES, OR */ 01201000
*          (DSABANMP ^= 0 &            /*AN ALT. NAME EXISTS         */ 01202000
*          DSABANAM(1:DSABANML)=DEQDSNAM(1:DSABANML) &          03S0348 01203000
*          DEQDSNAM(DSABANML+1) = ' ') THEN /*AND ALTERNATE MATCHES  */ 01204000
         L     @09,DSABSIOT(,WRKDSABP)                          03S0348 01205000
         L     @01,SJFCBPTR(,@09)                               03S0348 01206000
         CLC   JFCBDSNM(44,@01),DEQDSNAM                        03S0348 01207000
         BNE   @GL00022                                         03S0348 01208000
         TM    SIOTQNAM(@09),B'00000001'                        03S0348 01209000
         BZ    @RT00348                                         03S0348 01210000
@GL00022 L     @09,DSABSIOT(,WRKDSABP)                          03S0348 01211000
         TM    SIOALIAS(@09),B'10000000'                        03S0348 01212000
         BNO   @GL00021                                         03S0348 01213000
         L     @09,SIOTJFX(,@09)                                03S0348 01214000
         CLC   JFCBXNAM(44,@09),DEQDSNAM                        03S0348 01215000
         BE    @RT00348                                         03S0348 01216000
@GL00021 L     @09,DSABANMP(,WRKDSABP)                          03S0348 01217000
         LTR   @09,@09                                          03S0348 01218000
         BZ    @RF00348                                         03S0348 01219000
         SLR   @15,@15                                          03S0348 01220000
         IC    @15,DSABANML(,@09)                               03S0348 01221000
         LR    @14,@15                                          03S0348 01222000
         BCTR  @14,0                                            03S0348 01223000
         EX    @14,@SC02892                                     03S0348 01224000
         BNE   @RF00348                                         03S0348 01225000
         LA    @09,DEQDSNAM(@15)                                03S0348 01226000
         CLI   0(@09),C' '                                      03S0348 01227000
         BNE   @RF00348                                         03S0348 01228000
@RT00348 DS    0H                                               03S0349 01229000
*        DODEQSW = '0'B;               /*DEQ SHOULD NOT BE DONE      */ 01230000
         NI    DODEQSW,B'01111111'                              03S0349 01231000
*      ELSE                            /*DSN NOT ASSOC'D WITH DSAB   */ 01232000
*        WRKDSABP = DSABFCHN;          /*CHECK NEXT DSAB IN CHAIN    */ 01233000
         B     @RC00348                                         03S0350 01234000
@RF00348 L     WRKDSABP,DSABFCHN(,WRKDSABP)                     03S0350 01235000
*    END;                              /*END CHECK OF DSABS          */ 01236000
@RC00348 DS    0H                                               03S0351 01237000
@DE00347 LTR   WRKDSABP,WRKDSABP                                03S0351 01238000
         BZ    @DC00347                                         03S0351 01239000
         TM    DODEQSW,B'10000000'                              03S0351 01240000
         BO    @DL00347                                         03S0351 01241000
@DC00347 DS    0H                                               03S0352 01242000
*    IF DODEQSW = '1'B THEN            /*IF STILL TO BE DEQ'D        */ 01243000
         TM    DODEQSW,B'10000000'                              03S0352 01244000
         BNO   @RF00352                                         03S0352 01245000
*      DO;                             /*SET UP FOR AND DO DEQ       */ 01246000
*        IF DEQCALL1 = '0'B THEN       /*IF NOTHING DEQ'D YET        */ 01247000
         TM    DEQCALL1,B'00010000'                             03S0354 01248000
         BNZ   @RF00354                                         03S0354 01249000
*          DO;                         /*SET UP FOR 1ST CALL         */ 01250000
*            DSRRFMAP = DSRRFMAP && DSRRFMAP; /*ZERO OUT FUNCTION MAP*/ 01251000
         XC    DSRRFMAP(2),DSRRFMAP                             03S0356 01252000
*            READALL = '1'B;           /*READ ALL DSENQ RECORDS      */ 01253000
         OI    READALL,B'00010000'                              03S0357 01254000
*            DELEDEQ = '1'B;           /*REQUEST DELETE/DEQ FUNCTION */ 01255000
         OI    DELEDEQ,B'00001000'                              03S0358 01256000
*            DSENQFP = 0;              /*INIT PTR TO 1ST DSENQ RECORD*/ 01257000
         SLR   @09,@09                                          03S0359 01258000
         ST    @09,DSENQFP                                      03S0359 01259000
*            DEQDSNLN=LENGTH(DEQDSNAM); /*INIT LEN FIELD OF DSN PARM */ 01260000
         MVC   DEQDSNLN(2),@CH00141                             03S0360 01261000
*          END;                        /*END IF 1ST CALL FOR DEQ     */ 01262000
*        ELSE;                         /*SERVICE RTN ALREADY CALLED  */ 01263000
@RF00354 DS    0H                                               03S0363 01264000
*        IF JSCBTCBP = 0 THEN          /*IF INIT TCB PTR IS ZERO        01265000
*                                                            @ZA12405*/ 01266000
         L     @09,@PC00001+16                                  03S0363 01267000
         L     @02,JSCBPTR(,@09)                                03S0363 01268000
         L     @02,JSCBTCBP(,@02)                               03S0363 01269000
         LTR   @02,@02                                          03S0363 01270000
         BNZ   @RF00363                                         03S0363 01271000
*          DEQTCBP = TCBPTR;           /*SET DEQ TCB PTR TO CURRENT  */ 01272000
         L     @09,TCBPTR(,@09)                                 03S0364 01273000
         ST    @09,DEQTCBP                                      03S0364 01274000
*        ELSE                          /*USE INITIATOR'S TCB FOR DEQ */ 01275000
*          DEQTCBP = JSCBTCBP;         /*SET DEQ TCB PTR TO INIT.'S  */ 01276000
         B     @RC00363                                         03S0365 01277000
@RF00363 L     @09,@PC00001+16                                  03S0365 01278000
         L     @09,JSCBPTR(,@09)                                03S0365 01279000
         L     @09,JSCBTCBP(,@09)                               03S0365 01280000
         ST    @09,DEQTCBP                                      03S0365 01281000
*        CALL IEFAB4DC                 /*SERVICE RTN FOR DEQ         */ 01282000
*                     (DSRRFMAP,JCTPTR,DEQDSNPM,LSTWEPAP,       03S0366 01283000
*                      DEQTCBP,SCTSNUMB,0,0,DEQDUMRC,DSENQFP,0);        01284000
@RC00363 LA    @09,DSRRFMAP                                     03S0366 01285000
         ST    @09,@AL00001                                     03S0366 01286000
         L     @09,@PC00001+16                                  03S0366 01287000
         LA    @02,JCTPTR(,@09)                                 03S0366 01288000
         ST    @02,@AL00001+4                                   03S0366 01289000
         LA    @02,DEQDSNPM                                     03S0366 01290000
         ST    @02,@AL00001+8                                   03S0366 01291000
         LA    @02,LSTWEPAP                                     03S0366 01292000
         ST    @02,@AL00001+12                                  03S0366 01293000
         LA    @02,DEQTCBP                                      03S0366 01294000
         ST    @02,@AL00001+16                                  03S0366 01295000
         L     @09,SCTPTR(,@09)                                 03S0366 01296000
         LA    @09,SCTSNUMB(,@09)                               03S0366 01297000
         ST    @09,@AL00001+20                                  03S0366 01298000
         LA    @09,@CF00108                                     03S0366 01299000
         ST    @09,@AL00001+24                                  03S0366 01300000
         LA    @09,@CF00108                                     03S0366 01301000
         ST    @09,@AL00001+28                                  03S0366 01302000
         LA    @09,DEQDUMRC                                     03S0366 01303000
         ST    @09,@AL00001+32                                  03S0366 01304000
         LA    @09,DSENQFP                                      03S0366 01305000
         ST    @09,@AL00001+36                                  03S0366 01306000
         LA    @09,@CF00108                                     03S0366 01307000
         ST    @09,@AL00001+40                                  03S0366 01308000
         L     @15,@CV00126                                     03S0366 01309000
         LA    @01,@AL00001                                     03S0366 01310000
         BALR  @14,@15                                          03S0366 01311000
*        IF DEQCALL1 = '0'B THEN       /*IF THIS WAS 1ST CALL        */ 01312000
         TM    DEQCALL1,B'00010000'                             03S0367 01313000
         BNZ   @RF00367                                         03S0367 01314000
*          DO;                         /*SET UP FOR ANY OTHERS       */ 01315000
*            DEQCALL1 = '1'B;          /*SAY 1ST CALL WAS MADE       */ 01316000
         OI    DEQCALL1,B'00010000'                             03S0369 01317000
*            READALL = '0'B;           /*DONT READ RECORDS NEXT TIME */ 01318000
*            INCORREC = '1'B;          /*DSENQ RECORDS ARE IN CORE   */ 01319000
         OI    INCORREC,B'00001000'                             03S0371 01320000
         NI    READALL,B'11101111'                              03S0371 01321000
*          END;                        /*END IF 1ST CALL             */ 01322000
*        ELSE;                         /*THIS WAS NOT 1ST DEQ        */ 01323000
@RF00367 DS    0H                                               03S0374 01324000
*      END;                            /*END DSN DEQ                 */ 01325000
*    ELSE;                             /*DSN NOT TO BE DEQ'D         */ 01326000
@RF00352 DS    0H                                               03S0376 01327000
*  END;                                /*END DEQ PROCESSING          */ 01328000
*ELSE;                                 /*NOTHING TO BE DEQ'D         */ 01329000
@RF00343 DS    0H                                               03S0378 01330000
*RFY JFCBX BASED(JFCBXPTR);            /*RESET JFCBX MAPPING BASE PTR*/ 01331000
*RFY DSAB BASED(DSABPTR);              /*RESET DSAB MAPPING BASE PTR */ 01332000
*/********************************************************************/ 01333000
*/*                                                                  */ 01334000
*/* SEGMENT NAME - RSTRTPCS                                          */ 01335000
*/*                                                                  */ 01336000
*/* DESCRIPTIVE NAME - CHECKPOINT RESTART PROCESSING                 */ 01337000
*/*                                                                  */ 01338000
*/* OPERATION - THIS CODE IS ENTERED TO DO ANY NECESSARY CHECKPOINT  */ 01339000
*/*             RESTART PROCESSING FOR THE PERFORMED UNALLOCATION.   */ 01340000
*/*                                                                  */ 01341000
*/*             IF AN UNALLOCATION WAS FOR A JCL ALLOCATED SIOT,     */ 01342000
*/*             A BIT IS SET IN THE JSCB (JSCSIOTS) TELLING CHECK-   */ 01343000
*/*             POINT TO SEARCH THE SIOTS WHEN CREATING THE DDNAME   */ 01344000
*/*             TABLE. A BIT IN THE SIOT IS ALSO SET SAYING THAT     */ 01345000
*/*             THE DDNAME FOR THIS SIOT MUST BE PUT INTO THE TABLE. */ 01346000
*/*             THIS JCL SIOT IS THEN MARKED DYNAMICALLY UNALLOCATED */ 01347000
*/*             AND THE WRTEPA SUBROUTINE IS CALLED TO BUILD AND     */ 01348000
*/*             ENCHAIN A WRITE EPA FOR THIS UPDATED SIOT, IF ONE    */ 01349000
*/*             DOES NOT ALREADY EXIST. ALL THIS PROCESSING IS       */ 01350000
*/*             BYPASSED IF THE SIOT HAD BEEN DYNAMICALLY ALLOCATED. */ 01351000
*/*                                                                  */ 01352000
*/*             IF THE 'NO RESTART TO BE DONE' BIT IN THE JCT        */ 01353000
*/*             (JCTNORST) IS OFF, AND IF THE 'NO AUTO RESTART' BIT  */ 01354000
*/*             IN THE JCT(JCTNARST) IS ALSO OFF, THE FOLLOWING      */ 01355000
*/*             CHECKS ARE MADE TO DETERMINE IF AUTOMATIC RESTART    */ 01356000
*/*             SHOULD STILL BE ALLOWED (THE PHILOSOPHY BEHIND THESE */ 01357000
*/*             CHECKS IS TO PREVENT RESTART ONLY IN THE CASE WHERE  */ 01358000
*/*             THERE IS NO CHANCE OF RESTARTING CORRECTLY.)         */ 01359000
*/*                                                                  */ 01360000
*/*               - IF THE UNALLOCATED SIOT WAS EITHER FOR A SYSOUT  */ 01361000
*/*                 DATA SET(CAUSING SPINOFF) OR HAD A DISPOSITION   */ 01362000
*/*                 OF DELETE PROCESSED, THE DATA SET NO LONGER      */ 01363000
*/*                 EXISTS. THEREFORE IN THESE CASES, IF A CHECKPOINT*/ 01364000
*/*                 HAD BEEN TAKEN (JCTCKFT ON IN THE JCT), AND THE  */ 01365000
*/*                 DATA SET WAS OPEN AT THE TIME OF CHECKPOINT      */ 01366000
*/*                 (SIOTOCKP ON IN THE SIOT - CAUSING A REPOSITION- */ 01367000
*/*                 ING OF THE READ/WRITE HEADS AT RESTART TIME AND  */ 01368000
*/*                 GIVING ACCESS TO GARBAGE), NO AUTOMATIC RESTART  */ 01369000
*/*                 SHOULD BE ALLOWED AND THE JCTNARST BIT IS SET.   */ 01370000
*/*                 IF THIS IS A VIO DATA SET (SIOVAMDS IS   @ZA09215*/ 01371000
*/*                 ON) AND IT WAS ALLOCATED AT THE LAST     @ZA09215*/ 01372000
*/*                 CHECKPOINT (SCTALCHK IS ON) THEN TURN ON @ZA09215*/ 01373000
*/*                 JCTNARST MAKING THE PROGRAM INELLIGIBLE  @ZA09215*/ 01374000
*/*                 FOR AUTOMATIC RESTART. IF THIS IS SYSOUT @ZA09215*/ 01375000
*/*                 DATA SET (SCTSYSOU IS ON) AND IT WAS     @ZA09215*/ 01376000
*/*                 ALLOCATED AT THE LAST CHECKPOINT         @ZA09215*/ 01377000
*/*                 (SCTALCHK IS ON) THEN TURN ON JCTNARST   @ZA09215*/ 01378000
*/*                 MAKING THE PROGRAM INELLIGIBLE FOR AUTO- @ZA09215*/ 01379000
*/*                 MATIC RESTART.                           @ZA09215*/ 01380000
*/*                 (IF THE DATA SET WAS NOT OPEN AT THE CHECKPOINT, */ 01381000
*/*                 AND IT IS NOT ONE OF THE ABOVE DATASETS  @ZA09215*/ 01382000
*/*                 RESTART IS NOT PROHIBITED AS IT IS POSSIBLE NO   */ 01383000
*/*                 ATTEMPT WILL BE MADE TO OPEN THE DATA SET. IF ONE*/ 01384000
*/*                 IS MADE, THE OPEN WOULD FAIL)                    */ 01385000
*/*                                                                  */ 01386000
*/*               - IF NO CHECKPOINT HAS BEEN TAKEN BUT THE 'ELIGIBLE*/ 01387000
*/*                 FOR STEP RESTART' BIT IN THE JCT(JCTRESTT) IS ON,*/ 01388000
*/*                 THE JCTNARST BIT IS SET IF THE UNALLOCATED SIOT  */ 01389000
*/*                 IS:                                              */ 01390000
*/*                 . FOR A NEW JCL ALLOCATED DATA SET WHOSE         */ 01391000
*/*                   PROCESSED DISPOSITION WAS KEEP, CATLG, OR      */ 01392000
*/*                   UNCTLG. THIS IS BECAUSE THE STEP WOULD FAIL AT */ 01393000
*/*                   RESTART TIME DUE TO THE DATA SET ALREADY       */ 01394000
*/*                   EXISTING. (IF THE DATA SET HAD BEEN DYNAMICALLY*/ 01395000
*/*                   ALLOCATED, ONLY AN ERROR REASON CODE WOULD BE  */ 01396000
*/*                   RETURNED FROM WHICH A STEP COULD STILL         */ 01397000
*/*                   CONTINUE)                                      */ 01398000
*/*                                                                  */ 01399000
*/*                 . FOR AN OLD JCL ALLOCATED DATA SET WHERE THE    */ 01400000
*/*                   CATALOG WAS USED (IE VOL SERS NOT SPECIFIED) AT*/ 01401000
*/*                   ALLOCATION, AND WHOSE PROCESSED DISPOSITION    */ 01402000
*/*                   WAS UNCATLG. THIS IS TO PREVENT STEP FAILURE   */ 01403000
*/*                   AT RESTART TIME DUE TO NOT BEING ABLE TO LOCATE*/ 01404000
*/*                   THE DATA SET. (IF VOL SERS HAD BEEN SPECIFIED  */ 01405000
*/*                   ON THE REQUEST, THE CATALOG WOULD NOT BE USED. */ 01406000
*/*                   IF IT HAD BEEN A DYNAMIC ALLOCATION, ONLY AN   */ 01407000
*/*                   ERROR REASON CODE WOULD BE RETURNED AT RESTART */ 01408000
*/*                   WITH WHICH THE STEP COULD STILL CONTINUE.)     */ 01409000
*/*                                                                  */ 01410000
*/*                 . FOR AN OLD JCL ALLOCATED DATA SET WHOSE        */ 01411000
*/*                   PROCESSED DISPOSITION WAS DELETE. THIS IS TO   */ 01412000
*/*                   PREVENT EITHER STEP FAILURE (IN LOCATING THE   */ 01413000
*/*                   DATA SET IF NO VOLUME SERIALS WERE SPECIFIED)  */ 01414000
*/*                   OR OPEN FAILURE. (HERE, AS OPPOSED TO          */ 01415000
*/*                   CHECKPOINT RESTART, IT IS ASSUMED THAT SOME    */ 01416000
*/*                   ATTEMPT WILL BE MADE TO OPEN THE DATA SET      */ 01417000
*/*                   SOMETIME DURING THE STEP)                      */ 01418000
*/*                                                                  */ 01419000
*/*                 . FOR AN OLD DYNAMICALLY ALLOCATED DATA SET WITH */ 01420000
*/*                   VOL SERS SPECIFIED ON THE REQUEST, WHOSE       */ 01421000
*/*                   PROCESSED DISPOSITION WAS DELETE. THIS IS      */ 01422000
*/*                   BECAUSE THE RESTART DYNAMIC ALLOCATION IN THIS */ 01423000
*/*                   CASE WOULD BE SUCESSFUL, CAUSING THE OPEN OF   */ 01424000
*/*                   THE DATA SET TO FAIL. (IF VOL SERS WERE NOT    */ 01425000
*/*                   SPECIFIED ON THE REQUEST, THE LOCATE IN THE    */ 01426000
*/*                   RESTART DYNAMIC ALLOCATION WOULD CAUSE AN      */ 01427000
*/*                   ERROR TO BE RETURNED WITH WHICH THE STEP COULD */ 01428000
*/*                   STILL CONTINUE.)                               */ 01429000
*/*                                                                  */ 01430000
*/*                 NOTE THAT IN THE ABOVE STATUS CHECKS TO          */ 01431000
*/*                 DETERMINE WHETHER A DATA SET IS NEW OR OLD, THE  */ 01432000
*/*                 SIOTCNEW BIT IS USED IN ADDITION TO THE NORMAL   */ 01433000
*/*                 STATUS BITS, TO DETERMINE THE STATUS OF THE DATA */ 01434000
*/*                 SET ON ENTRY TO THE STEP.                        */ 01435000
*/*                                                                  */ 01436000
*/*             IF THE JCTNARST BIT WAS SET IN THE ABOVE PROCESSING, */ 01437000
*/*             AN INTERNAL BIT SAYING THE JCT WAS UPDATED IS ALSO   */ 01438000
*/*             SET. THIS WILL CAUSE THE JCT TO LATER BE WRITTEN OUT */ 01439000
*/*             TO SWA.                                              */ 01440000
*/*                                                                  */ 01441000
*/* ENTRY -                                                          */ 01442000
*/*   INPUT DATA - CMRBPTR - PTR TO UNALLOCATION RB                  */ 01443000
*/*                JCTPTR   - PTR TO JCT                             */ 01444000
*/*                                                                  */ 01445000
*/* EXIT -                                                           */ 01446000
*/*  OUTPUT DATA - JSCSIOTS - SET ON IF DATA SET WAS JCL ALLOCATED   */ 01447000
*/*                SIOTDDNT - SET ON IF JCL ALLOCATION               */ 01448000
*/*                SIODUNAL - SET ON IF JCL ALLOCATION               */ 01449000
*/*                SWAEPA   - WRITE EPA ADDED TO CHAIN IF JCL SIOT   */ 01450000
*/*                           (IF NOT ON ALREADY)                    */ 01451000
*/*                JCTNARST - ON IF AUTO RESTART NOT TO BE ALLOWED   */ 01452000
*/*                JCTUPDT  - SET ON IF THE JCT WAS UPDATED          */ 01453000
*/*                                                                  */ 01454000
*/********************************************************************/ 01455000
*IF SIOTDYAL = '0'B  THEN              /*IF D.S. WAS NOT A DYN. ALLOC*/ 01456000
         L     @09,CMSIOPTR(,CMRBPTR)                           03S0380 01457000
         TM    SIOTDYAL(@09),B'01000000'                        03S0380 01458000
         BNZ   @RF00380                                         03S0380 01459000
*  DO;                                 /*PROCESS JCL ALLOCATION      */ 01460000
*     DO; /*MODESET  KEYADDR(KEYZERO)*/                         03S0382 01461000
*       SPKA (KEYZERO); /*SET PSW KEY*/                         03S0383 01462000
         L     @02,KEYZERO                                      03S0383 01463000
         SPKA  0(@02)                                           03S0383 01464000
*     END; /*MODESET  KEYADDR(KEYZERO)*/        /*KEY 0 TO UPDATE JSCB  01465000
*      */                                                       03S0384 01466000
*    JSCSIOTS = '1'B;                  /*CHECKPOINT RESTART TO SEARCH   01467000
