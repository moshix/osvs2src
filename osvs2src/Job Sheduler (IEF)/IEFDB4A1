         OI    SIOTDDNT(@09),B'00010001'                        03S0390 01481000
*    WRKSIOTP = CMSIOPTR;              /*SET SIOTPTR FOR SUBRTN      */ 01482000
         LR    WRKSIOTP,@09                                     03S0391 01483000
