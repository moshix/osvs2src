         CLC   SCTLSIOT(3,@05),SIOTDSKA(@09)                    04S0487 01986000
         BNE   @RF00487                                         04S0487 01987000
*            DO;                       /*UPDATE SVA OF LAST IN SCT   */ 01988000
*              SCTLSIOT = PRESIOTP -> SIOTDSKA;                 04S0489 01989000
         MVC   SCTLSIOT(3,@05),SIOTDSKA(PRESIOTP)               04S0489 01990000
*                                      /*SVA OF NEW LAST SIOT IN SCT */ 01991000
*              SCTUPDT = '1'B;         /*SET INTERNAL SCT UPDATED BIT*/ 01992000
         OI    SCTUPDT,B'01000000'                              04S0490 01993000
*            END;                      /*END IF DECHAINING LAST SIOT */ 01994000
*          IF PRESIOTP -> SIOTDYAL = '0'B |                     04S0492 01995000
*                                      /*IF PRIOR SIOT IS JCL,OR A   */ 01996000
*             PRESIOTP -> SIOTSTEP = '0'B THEN                  04S0492 01997000
@RF00487 TM    SIOTDYAL(PRESIOTP),B'01000000'                   04S0492 01998000
         BZ    @RT00492                                         04S0492 01999000
         TM    SIOTSTEP(PRESIOTP),B'00001000'                   04S0492 02000000
         BNZ   @RF00492                                         04S0492 02001000
@RT00492 DS    0H                                               04S0493 02002000
*                                      /*STILL ALLOCATED DYNAMIC SIOT*/ 02003000
*            DO;                       /*UPDATED SIOT MUST BE WRITTEN*/ 02004000
*              WRKSIOTP = PRESIOTP;    /*POINT TO SIOT TO BE WRITTEN */ 02005000
         LR    WRKSIOTP,PRESIOTP                                04S0494 02006000
*              CALL WRTEPA;            /*WRITE EPA CHAINING PROCESS  */ 02007000
         BAL   @14,WRTEPA                                       04S0495 02008000
*            END;                      /*END PRIOR SIOT NEEDS WRITE  */ 02009000
*          SIOTFND = '1'B;             /*SIOT IS FOUND - EXIT LOOP   */ 02010000
@RF00492 OI    SIOTFND,B'10000000'                              04S0497 02011000
*        END;                          /*END IF SIOT IS FOUND        */ 02012000
*      ELSE                            /*THIS IS NOT SIOT TO DECHAIN */ 02013000
*        DO;                           /*CONTINUE LOOKING DOWN CHAIN */ 02014000
         B     @RC00483                                         04S0499 02015000
@RF00483 DS    0H                                               04S0500 02016000
*          PRESIOTP = SIOTSAVP;        /*UPDATE PRIOR PTR TO THIS PTR*/ 02017000
         LR    PRESIOTP,SIOTSAVP                                04S0500 02018000
*          SIOTSAVP = SIOTSAVP -> SIOTNPTR; /*LOOK AT NEXT ON CHAIN  */ 02019000
         L     SIOTSAVP,SIOTNPTR(,SIOTSAVP)                     04S0501 02020000
*        END;                          /*END IF NOT SIOT LOOKING FOR */ 02021000
*    END;                              /*END SEARCH FOR SIOT         */ 02022000
@RC00483 DS    0H                                               04S0503 02023000
@DE00482 TM    SIOTFND,B'10000000'                              04S0503 02024000
         BZ    @DL00482                                         04S0503 02025000
*  END;                                /*END IF SIOT IS NOT IST ONE  */ 02026000
*        END;                          /*END IF DYNAMICALLY ALLOCATED*/ 02027000
*      ELSE;                           /*D.S. WAS JCL ALLOCATED      */ 02028000
@RF00427 DS    0H                                               02S0507 02029000
*       DO; /*MODESET  KEYADDR(KEYZERO)*/                       02S0507 02030000
@RC00427 DS    0H                                               02S0508 02031000
*         SPKA (KEYZERO); /*SET PSW KEY*/                       02S0508 02032000
         L     @09,KEYZERO                                      02S0508 02033000
         SPKA  0(@09)                                           02S0508 02034000
*       END; /*MODESET  KEYADDR(KEYZERO)*/      /*GET INTO KEY 0 TO UPD 02035000
*TE                                               JSCB          02S0509 02036000
*      */                                                       02S0509 02037000
*      JSCDDNUM = JSCDDNUM-1;          /*DECREMENT COUNT OF ALLOCATED   02038000
*                                        SIOTS                       */ 02039000
         L     @09,@PC00001+16                                  02S0510 02040000
         L     @09,JSCBPTR(,@09)                                02S0510 02041000
         LH    @02,JSCDDNUM(,@09)                               02S0510 02042000
         BCTR  @02,0                                            02S0510 02043000
         STH   @02,JSCDDNUM(,@09)                               02S0510 02044000
*       DO; /*MODESET  EXTKEY(SCHED)*/                          02S0511 02045000
*         SPKA (16); /*SET PSW KEY*/                            02S0512 02046000
         LA    @09,16                                           02S0512 02047000
         SPKA  0(@09)                                           02S0512 02048000
*       END; /*MODESET  EXTKEY(SCHED)*/         /*RETURN TO SCHEDULER K 02049000
*Y     */                                                       02S0513 02050000
*      CMRBPTR = CMNRBPTR;             /*POINT TO NEXT UNALLOC RB    */ 02051000
         L     CMRBPTR,CMNRBPTR(,CMRBPTR)                       02S0514 02052000
