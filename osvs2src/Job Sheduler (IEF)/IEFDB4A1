*  SWLNGTH = JFCBXLEN;                 /*PUT JFCBX LENGTH INTO EPA   */ 01803000
         MVC   SWLNGTH(4,LASTEPAP),@CF00113                     03S0448 01804000
*  SVAJFCBX = JFCBXTTR;                /*GET SVA OF NEXT JFCBX       */ 01805000
         SLR   SVAJFCBX,SVAJFCBX                                03S0449 01806000
         ICM   SVAJFCBX,7,JFCBXTTR(JFCBXPTR)                    03S0449 01807000
*  JFCBXPTR = JFCBXNXT;                /*GET CORE ADDR OF NEXT JFCBX */ 01808000
         L     JFCBXPTR,JFCBXNXT(,JFCBXPTR)                     03S0450 01809000
*END;                                  /*END LOOP THRU JFCBX(S)      */ 01810000
*                                                               03S0451 01811000
@DE00443 LTR   JFCBXPTR,JFCBXPTR                                03S0451 01812000
         BNZ   @DL00443                                         03S0451 01813000
*IF SIOTSSDS = ON                      /*SUBSYSTEM DATA SET? @G29AN2F*/ 01814000
*  & SIOTSSGP = ON                     /*AND SUBSYS REQUEST? @G29AN2F*/ 01815000
*  & SIOTSSWA ^= ZERO THEN             /*AND SSWA EXISTS ?   @G29AN2F*/ 01816000
         L     @09,CMSIOPTR(,CMRBPTR)                           03S0452 01817000
         TM    SIOTSSDS(@09),B'10000000'                        03S0452 01818000
         BNO   @RF00452                                         03S0452 01819000
         TM    SIOTSSGP(@09),B'00010000'                        03S0452 01820000
         BNO   @RF00452                                         03S0452 01821000
