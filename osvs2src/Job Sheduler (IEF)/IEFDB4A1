         L     @02,@PC00001+16                                  03S0385 01469000
         L     @02,JSCBPTR(,@02)                                03S0385 01470000
         OI    JSCSIOTS(@02),B'00000010'                        03S0385 01471000
*     DO; /*MODESET  EXTKEY(SCHED)*/                            03S0386 01472000
*       SPKA (16); /*SET PSW KEY*/                              03S0387 01473000
         LA    @04,16                                           03S0387 01474000
         SPKA  0(@04)                                           03S0387 01475000
*     END; /*MODESET  EXTKEY(SCHED)*/           /*RETURN TO SCHEDULER K 01476000
*Y     */                                                       03S0388 01477000
*    SIOTDDNT = '1'B;                  /*CHECKPOINT RESTART MUST PUT    01478000
