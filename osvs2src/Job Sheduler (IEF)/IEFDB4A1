         BZ    @RF00452                                         03S0452 01825000
*  DO;                                 /*YES, SETUP TO DELETE   03S0453 01826000
*                                        SSWA                @G29AN2F*/ 01827000
*    CALL GETEPA;                      /*GET EPA FOR SSWA    @G29AN2F*/ 01828000
         BAL   @14,GETEPA                                       03S0454 01829000
*    SWBLKPTR = ZERO;                  /*DON'T POINT TO BLK  @G29AN2F*/ 01830000
         SLR   @09,@09                                          03S0455 01831000
         ST    @09,SWBLKPTR(,LASTEPAP)                          03S0455 01832000
*    SWVA = SIOTSSWA;                  /*PUT SSWA SVA IN EPA @G29AN2F*/ 01833000
         L     @06,CMSIOPTR(,CMRBPTR)                           03S0456 01834000
         MVC   SWVA(3,LASTEPAP),SIOTSSWA(@06)                   03S0456 01835000
*    SWBLKID = '31'X;                  /*PUT SSWA BLK ID IN  @G29AN2F*/ 01836000
         MVI   SWBLKID(LASTEPAP),X'31'                          03S0457 01837000
*    SWLNGTH = ZERO;                   /*INDICATE USE LENGTH    03S0458 01838000
*                                        FROM BLOCK HEADER   @G29AN2F*/ 01839000
         ST    @09,SWLNGTH(,LASTEPAP)                           03S0458 01840000
*  END;                                /*END SSWA DELETE     @G29AN2F*/ 01841000
*LSTDEPAP = LASTEPAP;                  /*POINT TO NEW LAST DELETE EPA*/ 01842000
@RF00452 LR    LSTDEPAP,LASTEPAP                                03S0460 01843000
*IF FSTSIOTP->SIOTDSKA^=SCTFSIOT THEN  /*IF NOT POINTING TO 1ST SIOT    01844000
*                                        IN CHAIN                    */ 01845000
         L     @09,@PC00001+16                                  03S0461 01846000
         L     @09,SCTPTR(,@09)                                 03S0461 01847000
         L     @02,FSTSIOTP                                     03S0461 01848000
         CLC   SIOTDSKA(3,@02),SCTFSIOT(@09)                    03S0461 01849000
         BE    @RF00461                                         03S0461 01850000
*  DO;                                 /*READ 1ST SIOT TO GET ITS ADDR* 01851000
*    SWACTLS = SWACTLS && SWACTLS;     /*ZERO OUT FUNCTION MAP       */ 01852000
         LH    @02,SWACTLS                                      03S0463 01853000
         N     @02,@CF02894                                     03S0463 01854000
         LR    @15,@02                                          03S0463 01855000
         XR    @15,@02                                          03S0463 01856000
         STH   @15,SWACTLS                                      03S0463 01857000
*    SWREAD = '1'B;                    /*PERFORM READ FUNCTION       */ 01858000
*    SWNEW  = '1'B;                    /*CREATE EPA'S                */ 01859000
*    SWFREE = '1'B;                    /*FREE EPA'S AFTER READ       */ 01860000
         OI    SWREAD,B'00101100'                               03S0466 01861000
*    CALL IEFAB4F7 (SWACTLS,0,0,ADDR(SCTFSIOT),ADDR(FSTSIOTP),0,1);     01862000
         MVC   @AL00001+4(24),@AL00467                          03S0467 01863000
