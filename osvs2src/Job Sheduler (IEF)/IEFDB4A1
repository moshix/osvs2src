         L     @06,DSABSIOT(,@06)                               01S0283 00768000
         ST    @06,FSTSIOTP                                     01S0283 00769000
*JOBNM = JCTJNAME;                     /*JOB NAME FOR - 4A0  @ZD03010*/ 00770000
         L     @06,JCTPTR(,@09)                                 01S0284 00771000
         MVC   JOBNM(8),JCTJNAME(@06)                           01S0284 00772000
*STPNM = SCTSNAME;                     /*STEP NAME FOR -4A0  @ZD03010*/ 00773000
         L     @15,SCTPTR(,@09)                                 01S0285 00774000
         MVC   STPNM(8),SCTSNAME(@15)                           01S0285 00775000
*CMNUNARY = CMNUNARY && CMNUNARY;      /*ZERO REASON CODE ARRAY      */ 00776000
         XC    CMNUNARY(8),CMNUNARY                             01S0286 00777000
*CALL IEFAB4A0                         /*CALL COMMON UNALLOCATION    */ 00778000
*             (CMUNFMAP,FSTRBPTR,JSCBPTR,FSTSIOTP,              01S0287 00779000
*              JCTDETDA,SCTSNUMB,JCTVULDP,CMNUNARY,ADDR(NAMES));        00780000
         LA    @14,FNCTMAP                                      01S0287 00781000
