*/********************************************************************/ 02187000
*/*                                                                  */ 02188000
*/* SUBROUTINE NAME - WRTEPA                                         */ 02189000
*/*                                                                  */ 02190000
*/* DESCRIPTIVE NAME - WRITE EPA PROCESSING FOR UPDATED SIOTS        */ 02191000
*/*                                                                  */ 02192000
*/* OPERATION - THIS SUBROUTINE IS CALLED BY THE RSTRTPCS AND        */ 02193000
*/*             SIOTDCHN SEGMENTS TO DO WRITE EPA PROCESSING FOR     */ 02194000
*/*             AN UPDATED SIOT. A SCAN OF THE WRITE EPA CHAIN       */ 02195000
*/*             IS MADE, DETERMINING WHETHER OR NOT AN EPA FOR THE   */ 02196000
*/*             SIOT ALREADY EXISTS ON THE CHAIN. IF THERE IS ONE,   */ 02197000
*/*             NO EPA IS NEEDED. HOWEVER IF ONE IS NOT ALREADY      */ 02198000
*/*             ON THE CHAIN, THE GETEPA SUBRTN IS CALLED TO GET     */ 02199000
*/*             CORE FOR AN EPA, ZERO IT OUT, AND CHAIN IT TO THE END*/ 02200000
*/*             OF THE WRITE CHAIN. THIS NEW EPA IS THEN FILLED      */ 02201000
*/*             IN WITH THE APPROPRIATE INFO FOR THE SIOT.           */ 02202000
*/*                                                                  */ 02203000
*/* ENTRY -                                                          */ 02204000
*/*   INPUT DATA - FSTWEPAP - PTR TO FIRST WRITE EPA ON CHAIN        */ 02205000
*/*                WRKSIOTP - PTR TO UPDATED SIOT                    */ 02206000
*/*                LSTWEPAP - PTR TO LAST WRITE EPA ON CHAIN         */ 02207000
*/*                                                                  */ 02208000
*/* EXIT -                                                           */ 02209000
*/*   OUTPUT DATA - SWAEPA - EPA BUILT FOR UPDATED SIOT AND ENCHAINED*/ 02210000
*/*                          (IF ONE DOES NOT ALREADY EXIST)         */ 02211000
*/*                 LSTWEPAP - POINTS TO NEW EPA CREATED             */ 02212000
*/*                                                                  */ 02213000
*/********************************************************************/ 02214000
*WRTEPA: PROC;                                                  02S0555 02215000
WRTEPA   STM   @14,@12,@SA00002                                 02S0555 02216000
*RFY SWAEPA BASED(SWEPAPTR);           /*RESET BASE OF EPA MAP       */ 02217000
*RFY INDMSIOT BASED(WRKSIOTP);         /*RESET BASE OF SIOT MAP      */ 02218000
*SWEPAPTR = FSTWEPAP;                  /*POINT TO 1ST  WRITE EPA     */ 02219000
         L     SWEPAPTR,FSTWEPAP                                02S0558 02220000
*WRTEPASW = WRTEPASW && WRTEPASW;      /*ZERO OUT PROCESSING SWITCHES*/ 02221000
         XC    WRTEPASW(1),WRTEPASW                             02S0559 02222000
*DO WHILE (SWEPAPTR ^= 0 &             /*LOOP TO END OF EPA CHAIN OR */ 02223000
*          ONCHAIN = '0'B );           /*TILL EPA FOR SIOT IS FOUND  */ 02224000
         B     @DE00560                                         02S0560 02225000
@DL00560 DS    0H                                               02S0561 02226000
*  IF WRKSIOTP = SWBLKPTR THEN         /*IF EPA IS FOR THE SIOT      */ 02227000
         C     WRKSIOTP,SWBLKPTR(,SWEPAPTR)                     02S0561 02228000
         BNE   @RF00561                                         02S0561 02229000
*    ONCHAIN = '1'B;                   /*GET OUT OF LOOP SEARCH      */ 02230000
         OI    ONCHAIN,B'10000000'                              02S0562 02231000
*  ELSE                                /*THIS EPA IS NOT FOR THE SIOT*/ 02232000
*    SWEPAPTR = SWCHNPTR;              /*POINT TO NEXT EPA           */ 02233000
         B     @RC00561                                         02S0563 02234000
@RF00561 L     SWEPAPTR,SWCHNPTR(,SWEPAPTR)                     02S0563 02235000
*END;                                  /*END SEARCH LOOP             */ 02236000
@RC00561 DS    0H                                               02S0564 02237000
@DE00560 LTR   SWEPAPTR,SWEPAPTR                                02S0564 02238000
         BZ    @DC00560                                         02S0564 02239000
         TM    ONCHAIN,B'10000000'                              02S0564 02240000
         BZ    @DL00560                                         02S0564 02241000
@DC00560 DS    0H                                               02S0565 02242000
*IF ONCHAIN = '0'B THEN                /*IF EPA FOR SIOT WASN'T FOUND*/ 02243000
         TM    ONCHAIN,B'10000000'                              02S0565 02244000
         BNZ   @RF00565                                         02S0565 02245000
*  DO;                                 /*GET A NEW WRITE EPA FOR IT  */ 02246000
*    LASTEPAP = LSTWEPAP;              /*POINT TO LAST WRITE EPA     */ 02247000
         L     LASTEPAP,LSTWEPAP                                02S0567 02248000
*    CALL GETEPA;                      /*GET NEW WRITE EPA FOR SIOT  */ 02249000
         BAL   @14,GETEPA                                       02S0568 02250000
*    LSTWEPAP = LASTEPAP;              /*UPDATE PTR TO NEW LAST EPA  */ 02251000
         ST    LASTEPAP,LSTWEPAP                                02S0569 02252000
*    RFY SWAEPA BASED(LSTWEPAP);       /*RESET BASE PTR OF EPA MAP   */ 02253000
*    SWBLKPTR = WRKSIOTP;              /*POINT EPA TO SIOT           */ 02254000
         L     @04,LSTWEPAP                                     02S0571 02255000
         ST    WRKSIOTP,SWBLKPTR(,@04)                          02S0571 02256000
*    SWVA = SIOTDSKA;                  /*PUT SVA OF SIOT INTO EPA    */ 02257000
         MVC   SWVA(3,@04),SIOTDSKA(WRKSIOTP)                   02S0572 02258000
*    SWBLKID = SIOTTYPE;               /*PUT SIOT BLOCK ID INTO EPA  */ 02259000
         MVC   SWBLKID(1,@04),SIOTTYPE(WRKSIOTP)                02S0573 02260000
*    SWLNGTH = SIOTBKLN;               /*PUT SIOT LENGTH INTO EPA    */ 02261000
         MVC   SWLNGTH(4,@04),@CF00113                          02S0574 02262000
*  END;                                /*END WRITE EPA CHAINING      */ 02263000
*ELSE;                                 /*EPA FOR SIOT ALREADY EXISTS */ 02264000
@RF00565 DS    0H                                               02S0577 02265000
*END WRTEPA;                                                    02S0577 02266000
@EL00002 DS    0H                                               02S0577 02267000
@EF00002 DS    0H                                               02S0577 02268000
@ER00002 LM    @14,@12,@SA00002                                 02S0577 02269000
         BR    @14                                              02S0577 02270000
*/********************************************************************/ 02271000
*/*                                                                  */ 02272000
*/* SUBROUTINE NAME - GETEPA                                         */ 02273000
*/*                                                                  */ 02274000
*/* DESCRIPTIVE NAME - GET SPACE FOR AN EPA                          */ 02275000
*/*                                                                  */ 02276000
*/* OPERATION - THIS SUBROUTINE IS CALLED BY THE UNALWPUP AND        */ 02277000
*/*            SIOTDPCS SEGMENTS, AND BY THE WRTEPA SUBROUTINE,      */ 02278000
*/*            TO GETMAIN CORE FOR AN EPA.  A GETMAIN IS     @ZM41392*/ 02279000
*/*            ISSUED FOR THIS CORE AND THE EPA IS ADDED     @ZM41392*/ 02280000
*/*            TO THE END OF THE                             @ZM41392*/ 02281000
*/*            EPA CHAIN, POINTING THE CHAIN PTR FIELD OF THE        */ 02282000
*/*            CURRENT LAST EPA TO THIS NEW LAST EPA. LASTEPAP IS    */ 02283000
*/*            THEN UPDATED TO POINT TO THIS LAST EPA AND THE        */ 02284000
*/*            EPA CORE IS ZEROED OUT. NOTE THAT THE GOTTEN EPA      */ 02285000
*/*            IS BEING ADDED TO EITHER A WRITE OR DELETE EPA CHAIN, */ 02286000
*/*            DEPENDENT UPON HOW THE WORK PTR, LASTEPAP, IS SET     */ 02287000
*/*            UP BY THE INVOKER.                                    */ 02288000
*/*                                                                  */ 02289000
*/* ENTRY -                                                          */ 02290000
*/*    INPUT DATA - LASTEPAP - PTR TO CURRENT LAST EPA ON            */ 02291000
*/*                           WRITE OR DELETE CHAIN                  */ 02292000
*/*                 EPALAST - LAST WRITE EPA(MODULE INPUT)           */ 02293000
*/*                                                                  */ 02294000
*/* EXIT -                                                           */ 02295000
*/*    OUTPUT DATA - LASTEPAP - POINTS TO NEW LAST EPA               */ 02296000
*/*                  SWCHNPTR - CHAIN PTR OF FORMER LAST EPA POINTS  */ 02297000
*/*                             TO NEW LAST EPA                      */ 02298000
*/*                  SWAEPA   - NEW LAST EPA, ZEROED OUT             */ 02299000
*/*                                                                  */ 02300000
*/********************************************************************/ 02301000
*GETEPA: PROC;                                                  02S0578 02302000
GETEPA   STM   @14,@03,@SA00003                                 02S0578 02303000
         STM   @05,@12,@SA00003+24                              02S0578 02304000
*RFY SWAEPA BASED(LASTEPAP);           /*RESET BASE OF EPA MAP       */ 02305000
*R0 = LENGTH(SWAEPA);                  /*CORE NEEDED FOR EPA         */ 02306000
         LA    R0,16                                            02S0580 02307000
*R0 = R0 | SCHDSUBP;                   /*GET FROM CORRECT SUBPOOL    */ 02308000
         O     R0,@CF00118                                      02S0581 02309000
*GEN (GETMAIN R,LV=(0));               /*GET CORE FOR EPA    @ZM41392*/ 02310000
         GETMAIN R,LV=(0)                                               02311000
*SWCHNPTR = R1;                        /*UPDATE CHAIN PTR OF FORMER     02312000
*                                        LAST EPA                    */ 02313000
         ST    R1,SWCHNPTR(,LASTEPAP)                           02S0583 02314000
*LASTEPAP = SWCHNPTR;                  /*POINT TO NEW LAST EPA       */ 02315000
         L     @15,SWCHNPTR(,LASTEPAP)                          02S0584 02316000
         LR    LASTEPAP,@15                                     02S0584 02317000
*SWAEPA = SWAEPA && SWAEPA;            /*ZERO OUT NEW LAST EPA       */ 02318000
         XC    SWAEPA(16,LASTEPAP),SWAEPA(LASTEPAP)             02S0585 02319000
*END GETEPA;                                                    02S0586 02320000
@EL00003 DS    0H                                               02S0586 02321000
@EF00003 DS    0H                                               02S0586 02322000
@ER00003 LM    @14,@03,@SA00003                                 02S0586 02323000
         LM    @05,@12,@SA00003+24                              02S0586 02324000
         BR    @14                                              02S0586 02325000
*END IEFDB4A1                                                   01S0587 02326000
