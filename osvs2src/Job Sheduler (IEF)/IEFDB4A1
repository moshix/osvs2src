         OR    @02,@00                                          02S0307 00975000
         STH   @02,INFOCODE(,@09)                               02S0307 00976000
*                                      /*TRANSLATE & SET INFO CODE   */ 00977000
*                RSNCDSET = '1'B;      /*DONT PROCESS OTHER RB RETCDE*/ 00978000
