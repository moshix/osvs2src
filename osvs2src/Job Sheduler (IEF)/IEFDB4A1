*              END;                    /*END IF CATALOG ERROR        */ 00980000
*            ELSE                      /*NOT A CATALOG MNGMT ERROR   */ 00981000
*              IF CMINTERR = '1'B THEN /*IF NO CORE FOR SCRATCH/CATLG   00982000
*                                        VOLUME LIST                 */ 00983000
         B     @RC00305                                         02S0310 00984000
@RF00305 TM    CMINTERR(CMRBPTR),B'00010000'                    02S0310 00985000
         BNO   @RF00310                                         02S0310 00986000
*                DO;                   /*PROCESS INTERNAL ERROR      */ 00987000
*                  IF CMORDISP = '1'B THEN /*IF OVERRIDING DISP WAS     00988000
*                                            PROCESSED               */ 00989000
         TM    CMORDISP(CMRBPTR),B'00010000'                    02S0312 00990000
         BNO   @RF00312                                         02S0312 00991000
*                    IF CMDEL = '1'B THEN /*IF PROCESSED DISP WAS       00992000
*                                           DELETE                   */ 00993000
         TM    CMDEL(CMRBPTR),B'10000000'                       02S0313 00994000
         BNO   @RF00313                                         02S0313 00995000
*                      INFOCODE = '0030'X | CMRETCDE; /*SET INFO CODE*/ 00996000
         L     @09,@PC00001+8                                   02S0314 00997000
         LA    @02,48                                           02S0314 00998000
         LH    @15,CMRETCDE(,CMRBPTR)                           02S0314 00999000
         OR    @02,@15                                          02S0314 01000000
         STH   @02,INFOCODE(,@09)                               02S0314 01001000
*                    ELSE              /*DISP MUST HAVE BEEN CATALOG */ 01002000
*                      INFOCODE = '0020'X | CMRETCDE; /*SET INFO CODE*/ 01003000
         B     @RC00313                                         02S0315 01004000
@RF00313 L     @09,@PC00001+8                                   02S0315 01005000
         LA    @02,32                                           02S0315 01006000
         LH    @15,CMRETCDE(,CMRBPTR)                           02S0315 01007000
         OR    @02,@15                                          02S0315 01008000
         STH   @02,INFOCODE(,@09)                               02S0315 01009000
*                  ELSE                /*NORMAL DISP WAS PROCESSED   */ 01010000
*                    IF SIOTDLET = '1'B THEN /*IF NORM DISP IS DELETE*/ 01011000
         B     @RC00312                                         02S0316 01012000
@RF00312 L     @09,CMSIOPTR(,CMRBPTR)                           02S0316 01013000
         TM    SIOTDLET(@09),B'00000100'                        02S0316 01014000
         BNO   @RF00316                                         02S0316 01015000
*                      INFOCODE = '0030'X | CMRETCDE; /*SET INFO CODE*/ 01016000
         L     @09,@PC00001+8                                   02S0317 01017000
         LA    @02,48                                           02S0317 01018000
         LH    @15,CMRETCDE(,CMRBPTR)                           02S0317 01019000
         OR    @02,@15                                          02S0317 01020000
         STH   @02,INFOCODE(,@09)                               02S0317 01021000
*                    ELSE              /*NORMAL DISP MUST BE CATALOG */ 01022000
*                      INFOCODE = '0020'X | CMRETCDE; /*SET INFO CODE*/ 01023000
         B     @RC00316                                         02S0318 01024000
