*  DO;                                                          01S0199 00762000
*/********************************************************************/ 00763000
*/*                                                                  */ 00764000
*/* SEGMENT NAME - UPTCTIOT                                          */ 00765000
*/*                                                                  */ 00766000
*/* DESCRIPTIVE NAME - UPDATE TCTIOT                                 */ 00767000
*/*                                                                  */ 00768000
*/* FUNCTION - TO UPDATE THE TCTIOT WITH ENTRIES RELATING TO THE     */ 00769000
*/*            DYNAMIC ALLOCATION REQUEST(S).IF A NEW TCTIOT HAS BEEN*/ 00770000
*/*            CREATED AND AN OLD ONE EXISTED THE OLD ONE IS FREED.  */ 00771000
*/*                                                                  */ 00772000
*/*   OPERATION - OPERATION IS DOCUMENTED BY THE CODE                */ 00773000
*/*                                                                  */ 00774000
*/* ENTRY POINT -(INLINE SUBROUTINE - ENTERED AT THE TOP)            */ 00775000
*/*                                                                  */ 00776000
*/*   PURPOSE - SEE FUNCTION                                         */ 00777000
*/*                                                                  */ 00778000
*/*   INPUT  - NEXTLUEP - PTR TO FIRST AVAILABLE L/U TABLE SLOT      */ 00779000
*/*              NXTCNTEP - PTR TO FIRST AVAILABLE CNTR TABLE SLOT   */ 00780000
*/*              ADDLDDNO - NO. NEW DD'S                             */ 00781000
*/*              LIST - LIST OF TIOT ENTRY PTRS AND SWITCHES         */ 00782000
*/*              SPACEPTR - PTR TO TCTIOT SPACE                      */ 00783000
*/*                                                                  */ 00784000
*/* EXIT - NORMAL - (INLINE SUBROUTINE - EXITS AT BOTTOM)            */ 00785000
*/*                                                                  */ 00786000
*/*   CONDITION - TCTIOT UPDATED WITH NEW ENTRIES                    */ 00787000
*/*                                                                  */ 00788000
*/*   OUTPUT DATA - UPDATED TCTIOT                                   */ 00789000
*/*                 IF A NEW TCTIOT HAS BEEN CREATED AND AN OLD ONE  */ 00790000
*/*                 EXISTED IT IS FREED.                             */ 00791000
*/*                                                                  */ 00792000
*/*   RETURN CODE - NONE                                             */ 00793000
*/*                                                                  */ 00794000
*/********************************************************************/ 00795000
*TCTLUENP = NEXTLUEP;                  /*PTR TO FIRST AVAILABLE L/U     00796000
*                                        TABLE ENTRY                 */ 00797000
         L     TCTLUENP,NEXTLUEP                                02S0200 00798000
*DO I = ONE TO ADDLDDNO;               /*PROCESS EACH DD ENTRY       */ 00799000
         LA    I,1                                              02S0201 00800000
         B     @DE00201                                         02S0201 00801000
@DL00201 DS    0H                                               02S0202 00802000
*  TIOTDDP = TIOTENTP(I);              /*PICK UP PTR TO TIOT ENTRY      00803000
*                                        FROM INPUT LIST             */ 00804000
         LR    @12,I                                            02S0202 00805000
         MH    @12,@CH00041                                     02S0202 00806000
         L     @11,@PC00001+4                                   02S0202 00807000
         AL    @11,@CF01128                                     02S0202 00808000
         L     TIOTDDP,TIOTENTP(@12,@11)                        02S0202 00809000
*  TCTIOTO(I) = TIOTDDP - TCBTIO;      /*OFFSET FOR TIOT DD ENTRY    */ 00810000
         LR    @12,I                                            02S0203 00811000
         SLA   @12,2                                            02S0203 00812000
         L     @11,@PC00001                                     02S0203 00813000
         L     @11,TCBPTR(,@11)                                 02S0203 00814000
         LR    @03,TIOTDDP                                      02S0203 00815000
         SL    @03,TCBTIO(,@11)                                 02S0203 00816000
         LR    @11,TCTLUENP                                     02S0203 00817000
         AL    @11,@CF01131                                     02S0203 00818000
         STH   @03,TCTIOTO(@12,@11)                             02S0203 00819000
*  TCTCNTEO(I) = NXTCNTEP - SPACEPTR;  /*OFFSET FOR TCTIOT CNTR ENTRY*/ 00820000
         L     @11,NXTCNTEP                                     02S0204 00821000
         LR    @03,@11                                          02S0204 00822000
         SL    @03,SPACEPTR                                     02S0204 00823000
         BCTR  @12,0                                            02S0204 00824000
         BCTR  @12,0                                            02S0204 00825000
         STH   @03,TCTCNTEO-2(@12,TCTLUENP)                     02S0204 00826000
*  TCTDVENP = NXTCNTEP;                /*PTR TO 1ST DEV.ENT THIS DD  */ 00827000
         LR    TCTDVENP,@11                                     02S0205 00828000
*  RFY TIOENTRY BASED(TIOTDDP);        /*ADDRESS TIOT ENTRY          */ 00829000
*  TCTDSCTR(1) = TIOEWTCT;             /*NUMBER OF DEVICE ENTRIES    */ 00830000
         MVC   TCTDSCTR(1,TCTDVENP),TIOEWTCT(TIOTDDP)           02S0207 00831000
*  DO K = ONE TO TIOEWTCT;             /*PROCESS ALL DEVICE ENTRIES     00832000
*                                        FOR THIS DD ENTRY           */ 00833000
         LA    K,1                                              02S0208 00834000
         B     @DE00208                                         02S0208 00835000
@DL00208 DS    0H                                               02S0209 00836000
*    IF VAMINDC(I) = ON THEN           /*IF VAM DATA SET ENTRY       */ 00837000
         LR    @12,I                                            02S0209 00838000
         MH    @12,@CH00041                                     02S0209 00839000
         L     @11,@PC00001+4                                   02S0209 00840000
         ALR   @11,@12                                          02S0209 00841000
         BCTR  @11,0                                            02S0209 00842000
         TM    VAMINDC-4(@11),B'01000000'                       02S0209 00843000
         BNO   @RF00209                                         02S0209 00844000
*      DO;                                                      02S0210 00845000
*        TCTDUCBP(K) = TCTDUCBP(K) && TCTDUCBP(K);  /*CLEAR UCB FIELD*/ 00846000
         LR    @12,K                                            02S0211 00847000
         SLA   @12,3                                            02S0211 00848000
         ST    @12,@TF00001                                     02S0211 00849000
         ALR   @12,TCTDVENP                                     02S0211 00850000
         AL    @12,@CF01132                                     02S0211 00851000
         XC    TCTDUCBP(2,@12),TCTDUCBP(@12)                    02S0211 00852000
*        TCTFLAG3(K) = ON;             /*SET VAM FLAG                */ 00853000
         LR    @12,TCTDVENP                                     02S0212 00854000
         AL    @12,@TF00001                                     02S0212 00855000
         AL    @12,@CF01128                                     02S0212 00856000
         OI    TCTFLAG3-3(@12),B'01000000'                      02S0212 00857000
*      END;                                                     02S0213 00858000
*    ELSE                              /*NOT VAM ENTRY               */ 00859000
*      DO;                                                      02S0214 00860000
         B     @RC00209                                         02S0214 00861000
@RF00209 DS    0H                                               02S0215 00862000
*        TCTDUCBP(K) = TIOUCBP(K);     /*PTR UCB FOR THIS DEVICE     */ 00863000
         LR    @12,K                                            02S0215 00864000
         SLA   @12,3                                            02S0215 00865000
         LR    @11,K                                            02S0215 00866000
         SLA   @11,2                                            02S0215 00867000
         L     @11,TIOUCBP-5(@11,TIOTDDP)                       02S0215 00868000
         LA    @11,0(,@11)                                      02S0215 00869000
         AL    @12,@CF01132                                     02S0215 00870000
         STH   @11,TCTDUCBP(@12,TCTDVENP)                       02S0215 00871000
*      END;                                                     02S0216 00872000
*  END;                                                         02S0217 00873000
@RC00209 AH    K,@CH00044                                       02S0217 00874000
@DE00208 SLR   @12,@12                                          02S0217 00875000
         IC    @12,TIOEWTCT(,TIOTDDP)                           02S0217 00876000
         CR    K,@12                                            02S0217 00877000
         BNH   @DL00208                                         02S0217 00878000
*  NXTCNTEP = NXTCNTEP                 /*UPDATE COUNTER TAB ENTRY PTR*/ 00879000
*            + (TIOEWTCT * LENGTH(TCTDVENT)) /*LENGTH DEVICE ENTRIES */ 00880000
*            + LENGTH(TCTOUTLM);       /*PLUS LEN OUTLIM ENTRY       */ 00881000
         SLR   @12,@12                                          02S0218 00882000
         IC    @12,TIOEWTCT(,TIOTDDP)                           02S0218 00883000
         SLA   @12,3                                            02S0218 00884000
         AL    @12,NXTCNTEP                                     02S0218 00885000
         AH    @12,@CH00105                                     02S0218 00886000
         ST    @12,NXTCNTEP                                     02S0218 00887000
*END;                                                           02S0219 00888000
         AH    I,@CH00044                                       02S0219 00889000
@DE00201 C     I,ADDLDDNO                                       02S0219 00890000
         BNH   @DL00201                                         02S0219 00891000
*IF SPACESW = ON THEN                  /*IF OLD TCTIOT REPLACED      */ 00892000
         TM    SPACESW,B'01000000'                              02S0220 00893000
         BNO   @RF00220                                         02S0220 00894000
*  DO;                                                          02S0221 00895000
