*              + LENGTH(TCTDCBLE)      /*END L/U TAB FIELD           */ 01434000
*              + (DDCOUNT * LENGTH(TCTLUENT))  /*L/U TABLE ENTRIES   */ 01435000
*              + (DDCOUNT * LENGTH(TCTOUTLM))  /*OUTLIM FIELDS       */ 01436000
*              + (DEVENTNO * LENGTH(TCTDVENT));  /*DEVICE ENTRIES    */ 01437000
         LR    @15,@08                                          02S0391 01438000
         SLA   @15,2                                            02S0391 01439000
