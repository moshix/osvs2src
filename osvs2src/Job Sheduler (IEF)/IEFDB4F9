         SLA   @08,3                                            02S0391 01444000
         ALR   @15,@08                                          02S0391 01445000
         ST    @15,GETSIZE                                      02S0391 01446000
*    GETSIZE = MAX(GETSIZE,EXTRSPAC);  /*GET LARGER OF NEEDED SIZE OR   01447000
*                                        DEFAULT SIZE                */ 01448000
         LA    @08,200                                          02S0392 01449000
         CR    @15,@08                                          02S0392 01450000
         BNL   *+6                                                      01451000
         LR    @15,@08                                          02S0392 01452000
         ST    @15,GETSIZE                                      02S0392 01453000
*    IF TCTIOTBL ^= ZERO               /*IF OLD TCTIOT EXISTS        */ 01454000
