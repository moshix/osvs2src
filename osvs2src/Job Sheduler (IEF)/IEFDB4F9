         CR    @15,@12                                          02S0393 01460000
         BE    @RF00393                                         02S0393 01461000
         TM    LOCKSW,B'00001000'                               02S0393 01462000
         BNO   @RF00393                                         02S0393 01463000
*      DO;                                                      02S0394 01464000
*        FREEPTR = TCTIOTBL;           /*OLD TCTIOT PTR              */ 01465000
         ST    @15,FREEPTR                                      02S0395 01466000
*        TCTIOTBL = ZERO;              /*ZERO TCTIOT PTR             */ 01467000
         ST    @12,TCTIOTBL(,@08)                               02S0396 01468000
*         RFY (R11,R12,R13,R14) RSTD;SV13=R13;GEN;              02S0397 01469000
         LR    SV13,R13                                         02S0398 01470000
          SETLOCK RELEASE,TYPE=CMS,                                   XX01471000
               REGS=USE,RELATED=(TCTIOT,IEFDB4F9(B4F9CONC))             01472000
