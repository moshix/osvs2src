         TITLE 'IEFIB620, INITIATOR TASK RECOVERY EXIT ROUTINE         *00001000
                        '                                               00002000
IEFIB620 CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'IEFIB620  76.155'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                            01S0001 00010000
@PSTART  DS    0H                                               01S0001 00011000
         USING @PSTART,@12                                      01S0001 00012000
JSCBDISP EQU 188                                                        00013000
JSCBAOS1 EQU  72                                                        00014000
JSCBAOS2 EQU 192                                                        00015000
*   R2=R1;                          /* SAVE PTR. TO RTCA (IF ANY)    */ 00016000
         LR    R2,R1                                            01S0047 00017000
*   R3=R14;                         /* SAVE RETURN ADDRESS           */ 00018000
         LR    R3,R14                                           01S0048 00019000
*   IF R0=NORTCA                                                01S0049 00020000
*                                                               01S0049 00021000
*   /*****************************************************************/ 00022000
*   /*                                                               */ 00023000
*   /* IF NO RTCA IS PRESENT, INDICATE NO RETRY AND DO NO OTHER      */ 00024000
*   /* PROCESSING.                                                   */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                               01S0049 00028000
*     THEN                                                      01S0049 00029000
         CH    R0,@CH01632                                      01S0049 00030000
         BNE   @RF00049                                         01S0049 00031000
*     R15=NORETRY1;                 /* SET RETURN CODE TO INDICATE NO   00032000
*                                      RETRY                         */ 00033000
         SLR   R15,R15                                          01S0050 00034000
*   ELSE                                                        01S0051 00035000
*                                                               01S0051 00036000
*     /***************************************************************/ 00037000
*     /*                                                             */ 00038000
*     /* IN GENERAL, AN RTCA WILL BE PROVIDED                        */ 00039000
*     /*                                                             */ 00040000
*     /***************************************************************/ 00041000
*                                                               01S0051 00042000
*     DO;                                                       01S0051 00043000
         B     @RC00049                                         01S0051 00044000
@RF00049 DS    0H                                               01S0052 00045000
*       GEN;                                                    01S0052 00046000
*       AUTOMATIC STORAGE MUST BE EXPLICITLY                            00047000
*       GOTTEN AND FREED, BECAUSE IT IS NECESSARY TO                    00048000
*       EXAMINE BOTH REGISTERS 0 AND 1 UPON ENTRY TO THIS               00049000
*       MODULE, AND THE PLS GETMAIN WOULD DESTROY REGISTER 0            00050000
         L     R0,@SIZDATD         LOAD SUBPOOL AND SIZE                00051000
         GETMAIN R,LV=(0)                                               00052000
         LR    R11,R1                                                   00053000
         USING @DATD,R11                                                00054000
*       RESPECIFY                                               01S0053 00055000
*        (R0,                                                   01S0053 00056000
*         R1,                                                   01S0053 00057000
*         R14,                                                  01S0053 00058000
*         R15) UNRSTD;                                          01S0053 00059000
*                                                               01S0053 00060000
*       /*************************************************************/ 00061000
*       /*                                                           */ 00062000
*       /* PERFORM CLEANUP PROCESSING. (RESTORE INITIATOR TIOT PTR.  */ 00063000
*       /* IF NECESSARY AND DEQ OFF PROTECT KEY AND CSDCPUJS         */ 00064000
*       /* RESOURCES IN CASE THEY ARE HELD.) THEN SET RETURN CODE IN */ 00065000
*       /* THE IEL.                                                  */ 00066000
*       /*                                                           */ 00067000
*       /*************************************************************/ 00068000
*                                                               01S0054 00069000
*                                                               02S0054 00070000
*       /*************************************************************/ 00071000
*       /*                                                           */ 00072000
*       /* RESTORE THE FIELD IN THE LCT WHICH POINTS TO THE CURRENT  */ 00073000
*       /* TCB. (IT MAY HAVE BEEN RESET TO POINT TO THE DUMMY TCB FOR*/ 00074000
*       /* UNALLOCATION.) THIS INSTRUCTION HAS BEEN MOVED FOR PTM    */ 00075000
*       /* 5499, TO INSURE THAT THE FIELD IS RESTORED BEFORE IT IS   */ 00076000
*       /* USED.                                              YM05499*/ 00077000
*       /*                                                           */ 00078000
*       /*************************************************************/ 00079000
*                                                               02S0054 00080000
*       LCTTCBAD=PSATOLD;           /*                        YM03148*/ 00081000
         L     @10,SDWAPARM(,R2)                                02S0054 00082000
         L     @01,STELCT(,@10)                                 02S0054 00083000
         L     @00,PSATOLD                                      02S0054 00084000
         ST    @00,LCTTCBAD(,@01)                               02S0054 00085000
*       IF STEPARAM^=0 THEN         /* MAKE SURE IEFPARAM EXISTS        00086000
*                                      BEFORE USING IT (IT IS NOT       00087000
*                                      SUPPOSED TO EXIST DURING THE     00088000
*                                      ENTIRE LIFE OF AN INITIATOR).    00089000
*                                                             YM02212*/ 00090000
         SLR   @01,@01                                          02S0055 00091000
         L     @10,STEPARAM(,@10)                               02S0055 00092000
         CR    @10,@01                                          02S0055 00093000
         BE    @RF00055                                         02S0055 00094000
*         IF PARTIOTB^=0                                        02S0056 00095000
*                                                               02S0056 00096000
*         /***********************************************************/ 00097000
*         /*                                                         */ 00098000
*         /* IF PARTIOTB IS NON-ZERO, THEN IT CONTAINS THE           */ 00099000
*         /* INITIATOR'S TIOT POINTER. IT IS SAVED THERE WHEN THE    */ 00100000
*         /* INITIATOR'S TCB IS SET TO POINT TO THE P/P'S TIOT. THIS */ 00101000
*         /* HAPPENS WHEN JOBLIB IS OPENED OR CLOSED OR WHEN THE P/P */ 00102000
*         /* IS ATTACHED. THE POINTER IS RESTORED HERE, SO RETRY OR  */ 00103000
*         /* TERMINATION CAN CONTINUE IN A NORMAL ENVIRONMENT.       */ 00104000
*         /*                                                         */ 00105000
*         /***********************************************************/ 00106000
*                                                               02S0056 00107000
*           THEN                                                02S0056 00108000
         L     @15,PARTIOTB(,@10)                               02S0056 00109000
         CR    @15,@01                                          02S0056 00110000
         BE    @RF00056                                         02S0056 00111000
*           DO;                                                 02S0057 00112000
*             TCBTIO=PARTIOTB;                                  02S0058 00113000
         LR    @14,@00                                          02S0058 00114000
         ST    @15,TCBTIO(,@14)                                 02S0058 00115000
*             PARTIOTB=0;           /* CLEAR SO THE PTR. WILL NOT BE    00116000
*                                      RESTORED AGAIN                */ 00117000
         ST    @01,PARTIOTB(,@10)                               02S0059 00118000
*           END;                                                02S0060 00119000
*                                                               02S0060 00120000
*       /*************************************************************/ 00121000
*       /*                                                           */ 00122000
*       /* IF A DAUGHTER TCB EXISTS DETACH IT                @ZM30470*/ 00123000
*       /*                                                           */ 00124000
*       /*************************************************************/ 00125000
*                                                               02S0061 00126000
*       IF TCBLTC^=0 THEN           /* CHECK FOR DAUGHTER    @ZM30470*/ 00127000
@RF00056 DS    0H                                               02S0061 00128000
@RF00055 L     @10,SDWAPARM(,R2)                                02S0061 00129000
         L     @10,STELCT(,@10)                                 02S0061 00130000
         L     @10,LCTTCBAD(,@10)                               02S0061 00131000
         ICM   @15,15,TCBLTC(@10)                               02S0061 00132000
         BZ    @RF00061                                         02S0061 00133000
*         DO;                       /*                       @ZM30470*/ 00134000
*           RFY                                                 02S0063 00135000
*            (R1) RSTD;                                         02S0063 00136000
*           R1=ADDR(TCBLTC);        /*                       @ZM30470*/ 00137000
         LA    R1,TCBLTC(,@10)                                  02S0064 00138000
*           GEN( DETACH (1));       /* REMOVE DAUGHTER       @ZM30470*/ 00139000
          DETACH (1)                                                    00140000
*           RFY                                                 02S0066 00141000
*            (R1) UNRSTD;                                       02S0066 00142000
*         END;                      /*                       @ZM30470*/ 00143000
*                                                               02S0067 00144000
*       /*************************************************************/ 00145000
*       /*                                                           */ 00146000
*       /* IF A POINTER TO THE P/P JSCB EXISTS, SET UP THE INITIATORS*/ 00147000
*       /* JSCB TO POINT TO THAT JSCB (AS THE ACTIVE JSCB). WHEN A   */ 00148000
*       /* P/P JSCB EXISTS, IT IS NORMALLY THE ACTIVE JSCB. IN ORDER */ 00149000
*       /* TO PERMIT SMF EXIT ROUTINES TO OPEN DATA SETS WHICH ARE   */ 00150000
*       /* ALLOCATED TO THE INITIATOR, THE INITIATOR JSCB IS POINTED */ 00151000
*       /* TO AS THE ACTIVE JSCB FOR THE PERIOD OF TIME WHEN THESE   */ 00152000
*       /* EXITS ARE INVOKED. THE POINTER IS RESTORED HERE, BECAUSE  */ 00153000
*       /* IEFSMFIE DOES NOT HAVE ITS OWN RECOVERY ENVIRONMENT.      */ 00154000
*       /*                                                    YM04108*/ 00155000
*       /*                                                           */ 00156000
*       /*************************************************************/ 00157000
*                                                               02S0068 00158000
*       IF LCTJSCB^=0               /*                        YM04108*/ 00159000
*         THEN                                                  02S0068 00160000
@RF00061 L     @10,SDWAPARM(,R2)                                02S0068 00161000
         L     @10,STELCT(,@10)                                 02S0068 00162000
         L     @15,LCTJSCB(,@10)                                02S0068 00163000
         LTR   @15,@15                                          02S0068 00164000
         BZ    @RF00068                                         02S0068 00165000
*         JSCBACT=LCTJSCB;          /*                        YM04108*/ 00166000
*                                                               02S0069 00167000
         L     @10,LCTTCBAD(,@10)                               02S0069 00168000
         L     @10,TCBJSCB(,@10)                                02S0069 00169000
         ST    @15,JSCBACT(,@10)                                02S0069 00170000
*       /*************************************************************/ 00171000
*       /*                                                           */ 00172000
*       /* DEQ FROM RESOURCES WHICH THE INITIATOR MAY HOLD, IN CASE  */ 00173000
*       /* THEY ARE CURRENTLY HELD.                            Y02651*/ 00174000
*       /*                                                           */ 00175000
*       /*************************************************************/ 00176000
*                                                               02S0070 00177000
*       DEQCORE(1:DEQLEN)=DEQKEY(1:DEQLEN);/* MOVE LIST FOR PROTECT     00178000
*                                      KEY RESOURCE TO AUTOMATIC        00179000
*                                      STORAGE                 Y02651*/ 00180000
@RF00068 L     @10,DEQLEN                                       02S0070 00181000
         BCTR  @10,0                                            02S0070 00182000
         EX    @10,@SM01701                                     02S0070 00183000
*       GEN REFS(DEQCORE);                                      02S0071 00184000
         DEQ   MF=(E,DEQCORE),                                        **00185000
               RELATED=('BAPKES, ENQ AND DEQ IN IEFSD161 OR IEFSD166')  00186000
*                                                               Y02651  00187000
*       DEQCORE(1:DEQLEN)=DEQCSD(1:DEQLEN);/* MOVE LIST FOR ALIVE-CPU   00188000
*                                      MASK RESOURCE TO AUTOMATIC       00189000
*                                      STORAGE                 Y02651*/ 00190000
         L     @10,DEQLEN                                       02S0072 00191000
         BCTR  @10,0                                            02S0072 00192000
         EX    @10,@SM01703                                     02S0072 00193000
*       GEN REFS(DEQCORE);                                      02S0073 00194000
*                                                       /*    Y02651*/  00195000
         DEQ   MF=(E,DEQCORE),                                        **00196000
               RELATED=('CSDCPUJS, ENQ AND DEQ USUALLY IN IEFICPUA')    00197000
*                                                               Y02651  00198000
*       ASCBAFFN=NOAFFIN;           /* TURN OFF CPU-TASK AFFINITY IN    00199000
*                                      CASE IT HAD BEEN SET    Y02651*/ 00200000
         L     @10,SDWAPARM(,R2)                                02S0074 00201000
         L     @05,STELCT(,@10)                                 02S0074 00202000
         L     @01,LCTASCBA(,@05)                               02S0074 00203000
         MVC   ASCBAFFN(2,@01),NOAFFIN                          02S0074 00204000
*       ABENDCOD=SDWAABCC&OPENMASK; /* CLEAR PRIOR TO CHECKING FOR      00205000
*                                      'OPEN' ABEND CODE             */ 00206000
         L     ABENDCOD,SDWAABCC(,R2)                           02S0075 00207000
         N     ABENDCOD,OPENMASK                                02S0075 00208000
*/********************************************************************/ 00209000
*/*                                                                  */ 00210000
*/*       IF NO RETURN CODE IS ALREADY SET IN THE IEL, SET ONE OF    */ 00211000
*/*       THE FOLLOWING CODES:                                       */ 00212000
*/*          OPEN ERRORS:                                            */ 00213000
*/*             JOBLIB ERROR                                         */ 00214000
*/*             STEPLIB ERROR                                        */ 00215000
*/*             FETCHLIB ERROR                                       */ 00216000
*/*             ALLOCATION ERROR (ACTUALLY OPEN CATALOG ERROR)       */ 00217000
*/*          PROGRAM ERROR (ANY ERROR NOT LISTED ABOVE)              */ 00218000
*/*       IF THIS ROUTINE SETS A RETURN CODE IN THE IEL, IT WILL SET */ 00219000
*/*       ONE OF THE FOLLOWING RETURN CODE ORIGINS:                  */ 00220000
*/*          SUBSYSTEM INTERFACE ERROR                               */ 00221000
*/*          INITIATOR ERROR                                         */ 00222000
*/*                                                                  */ 00223000
*/********************************************************************/ 00224000
*                                                               02S0076 00225000
*       IF IELRTNCD='00'X THEN                                  02S0076 00226000
         L     @05,LCTENTR(,@05)                                02S0076 00227000
         L     @05,IELEXIT(,@05)                                02S0076 00228000
         CLI   IELRTNCD(@05),0                                  02S0076 00229000
         BNE   @RF00076                                         02S0076 00230000
*         DO;                       /* STORE A RETURN CODE IN THE IEL   00231000
*                                      IF ONE WAS NOT ALREADY STORED */ 00232000
*           IF ABENDCOD=OPENCODE THEN                           02S0078 00233000
         CL    ABENDCOD,OPENCODE                                02S0078 00234000
         BNE   @RF00078                                         02S0078 00235000
*             DO;                   /* OPEN ABEND CODE PROCESSING       00236000
*                                                            @ZA10755*/ 00237000
*               SDWARCRD='0'B;      /* TURN OFF RECORDING    @ZA10755*/ 00238000
         NI    SDWARCRD(R2),B'01111111'                         02S0080 00239000
*               IF STEJOBLB='1'B THEN                           02S0081 00240000
         TM    STEJOBLB(@10),B'00100000'                        02S0081 00241000
         BNO   @RF00081                                         02S0081 00242000
*                 IELRTNCD='18'X;   /* SET JOBLIB ERROR              */ 00243000
         MVI   IELRTNCD(@05),X'18'                              02S0082 00244000
*               ELSE                                            02S0083 00245000
*                 IF STESTPLB='1'B THEN                         02S0083 00246000
         B     @RC00081                                         02S0083 00247000
@RF00081 L     @10,SDWAPARM(,R2)                                02S0083 00248000
         TM    STESTPLB(@10),B'00010000'                        02S0083 00249000
         BNO   @RF00083                                         02S0083 00250000
*                   IELRTNCD='1C'X; /* SET STEPLIB ERROR             */ 00251000
         L     @10,STELCT(,@10)                                 02S0084 00252000
         L     @10,LCTENTR(,@10)                                02S0084 00253000
         L     @10,IELEXIT(,@10)                                02S0084 00254000
         MVI   IELRTNCD(@10),X'1C'                              02S0084 00255000
*                 ELSE                                          02S0085 00256000
*                   IF STEFCHLB='1'B THEN                       02S0085 00257000
         B     @RC00083                                         02S0085 00258000
@RF00083 L     @10,SDWAPARM(,R2)                                02S0085 00259000
         TM    STEFCHLB(@10),B'00001000'                        02S0085 00260000
         BNO   @RF00085                                         02S0085 00261000
*                     IELRTNCD='20'X;/* SET FETCHLIB ERROR           */ 00262000
         L     @10,STELCT(,@10)                                 02S0086 00263000
         L     @10,LCTENTR(,@10)                                02S0086 00264000
         L     @10,IELEXIT(,@10)                                02S0086 00265000
         MVI   IELRTNCD(@10),X'20'                              02S0086 00266000
*                   ELSE                                        02S0087 00267000
*                     IELRTNCD='04'X;/* SET ALLOCATION ERROR IF NOT     00268000
*                                      JOBLIB, STEPLIB, OR FETCHLIB     00269000
*                                      (TO INDICATE AN OPEN CATALOG     00270000
*                                      FAILURE)                      */ 00271000
         B     @RC00085                                         02S0087 00272000
@RF00085 L     @10,SDWAPARM(,R2)                                02S0087 00273000
         L     @10,STELCT(,@10)                                 02S0087 00274000
         L     @10,LCTENTR(,@10)                                02S0087 00275000
         L     @10,IELEXIT(,@10)                                02S0087 00276000
         MVI   IELRTNCD(@10),X'04'                              02S0087 00277000
*             END;                  /* END OPEN ABEND PROCESSING        00278000
*                                                            @ZA10755*/ 00279000
*           ELSE                                                02S0089 00280000
*             IELRTNCD='24'X;       /* SET PROGRAM ERROR IF NOT AN      00281000
*                                      OPEN ERROR                    */ 00282000
         B     @RC00078                                         02S0089 00283000
@RF00078 L     @10,SDWAPARM(,R2)                                02S0089 00284000
         L     @10,STELCT(,@10)                                 02S0089 00285000
         L     @10,LCTENTR(,@10)                                02S0089 00286000
         L     @10,IELEXIT(,@10)                                02S0089 00287000
         MVI   IELRTNCD(@10),X'24'                              02S0089 00288000
*           SYSCODE=SDWAABCC&ABENDMSK;/* CLEAR ALL BUT THE 3 DIGITS     00289000
*                                      OF THE SYSTEM CODE            */ 00290000
@RC00078 L     SYSCODE,SDWAABCC(,R2)                            02S0090 00291000
         N     SYSCODE,ABENDMSK                                 02S0090 00292000
*           IF SYSCODE=SSICODE THEN                             02S0091 00293000
         CL    SYSCODE,SSICODE                                  02S0091 00294000
         BNE   @RF00091                                         02S0091 00295000
*             IELRCXT='05'X;        /* SET SUBSYSTEM INTERFACE ORIGIN   00296000
*                                      IF SUBSYSTEM INTERFACE ABEND     00297000
*                                      CODE                          */ 00298000
         L     @10,SDWAPARM(,R2)                                02S0092 00299000
         L     @10,STELCT(,@10)                                 02S0092 00300000
         L     @10,LCTENTR(,@10)                                02S0092 00301000
         L     @10,IELEXIT(,@10)                                02S0092 00302000
         MVI   IELRCXT(@10),X'05'                               02S0092 00303000
*           ELSE                                                02S0093 00304000
*             IELRCXT='03'X;        /* OTHERWISE SET INITIATOR ORIGIN*/ 00305000
         B     @RC00091                                         02S0093 00306000
@RF00091 L     @10,SDWAPARM(,R2)                                02S0093 00307000
         L     @10,STELCT(,@10)                                 02S0093 00308000
         L     @10,LCTENTR(,@10)                                02S0093 00309000
         L     @10,IELEXIT(,@10)                                02S0093 00310000
         MVI   IELRCXT(@10),X'03'                               02S0093 00311000
*         END;                                                  02S0094 00312000
@RC00091 DS    0H                                               02S0095 00313000
*                                                               01S0095 00314000
*       /*************************************************************/ 00315000
*       /*                                                           */ 00316000
*       /* CONTRIBUTE TO ERROR RECORDING, UNLESS USER ERROR (OPEN    */ 00317000
*       /* FAILURE) OR PERCOLATION.                           YM02669*/ 00318000
*       /*                                                           */ 00319000
*       /*************************************************************/ 00320000
*                                                               01S0095 00321000
*       IF ABENDCOD^=OPENCODE&SDWAPERC='0'B THEN/*            YM02669*/ 00322000
@RF00076 CL    ABENDCOD,OPENCODE                                01S0095 00323000
         BE    @RF00095                                         01S0095 00324000
         TM    SDWAPERC(R2),B'00010000'                         01S0095 00325000
         BNZ   @RF00095                                         01S0095 00326000
*                                                               02S0096 00327000
*         /***********************************************************/ 00328000
*         /*                                                         */ 00329000
*         /* ISSUE SETRP                                             */ 00330000
*         /*                                                         */ 00331000
*         /***********************************************************/ 00332000
*                                                               02S0096 00333000
*         DO;                       /* SETRP RECORD(YES)WKAREA(R2)REC   00334000
*                                      PARM(RECINFO)                 */ 00335000
*           RESPECIFY                                           02S0097 00336000
*            (GPR00P,                                           02S0097 00337000
*             GPR01P,                                           02S0097 00338000
*             GPR14P,                                           02S0097 00339000
*             GPR15P) RSTD;                                     02S0097 00340000
*           GPR01P=R2;                                          02S0098 00341000
         LR    GPR01P,R2                                        02S0098 00342000
*           GPR01P->SDWARCDE=0;     /* SAVE RC VALUE                 */ 00343000
         MVI   SDWARCDE(GPR01P),X'00'                           02S0099 00344000
*           GPR01P->SDWARCRD='1'B;  /* TURN ON RECORD INDICATOR      */ 00345000
         OI    SDWARCRD(GPR01P),B'10000000'                     02S0100 00346000
*           GPR15P=ADDR(RECINFO);   /* ACCESS RECORD PARAMETER LIST  */ 00347000
         LA    GPR15P,RECINFO                                   02S0101 00348000
*           GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD 02S0102 00349000
*                                      PARAMETERS                    */ 00350000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                02S0102 00351000
*           RESPECIFY                                           02S0103 00352000
*            (GPR00P,                                           02S0103 00353000
*             GPR01P,                                           02S0103 00354000
*             GPR14P,                                           02S0103 00355000
*             GPR15P) UNRSTD;                                   02S0103 00356000
*         END;                                                  02S0104 00357000
*       RESPECIFY                                               02S0105 00358000
*         SDWA BASED(R2);                                       02S0105 00359000
@RF00095 DS    0H                                               02S0106 00360000
*                                                               01S0106 00361000
*       /*************************************************************/ 00362000
*       /*                                                           */ 00363000
*       /* TAKE DUMP IF NOT RECURSION, PERCOLATION, OPEN FAILURE, OR */ 00364000
*       /* MACHINE CHECK.                                     YM02669*/ 00365000
*       /*                                                           */ 00366000
*       /*************************************************************/ 00367000
*                                                               01S0106 00368000
*       IF STERETRY='0'B&STETJMR='0'B&SDWAPERC='0'B&ABENDCOD^=OPENCODE& 00369000
*           SDWAMCHK='0'B           /*                        YM02669   00370000
*                                      STERETRY IS SET IN THIS MODULE   00371000
*                                      WHEN RETRY IS ATTEMPTED AND      00372000
*                                      REMAINS ON UNTIL IEFSD161 HAS    00373000
*                                      ANOTHER JOB SELECTED. IF 01S0106 00374000
*                                      STERETRY IS ON WHEN IEFSD161     00375000
*                                      IS ENTERED, IEFSD161 TURNS OFF   00376000
*                                      STERETRY AND TURNS ON STETJMR    00377000
*                                      TO INDICATE THAT THIS IS THE     00378000
*                                      JOB SELECTED IMMEDIATELY AFTER   00379000
*                                      AN ERROR WAS ENCOUNTERED AND     00380000
*                                      RETRY WILL NOT BE ATTEMPTED IF   00381000
*                                      ANOTHER ERROR IS ENCOUNTERED.    00382000
*                                      IF STETJMR IS ON WHEN IEFSD161   00383000
*                                      IS ENTERED, IT IS TURNED OFF.    00384000
*                                      THEREFORE, IF EITHER STERETRY    00385000
*                                      OR STETJMR ARE ON WHEN THIS      00386000
*                                      MODULE IS ENTERED, THE   01S0106 00387000
*                                      SITUATION IS A RECURSION.     */ 00388000
*         THEN                                                  01S0106 00389000
         L     @10,SDWAPARM(,R2)                                01S0106 00390000
         TM    STERETRY(@10),B'11000000'                        01S0106 00391000
         BNZ   @RF00106                                         01S0106 00392000
         TM    SDWAPERC(R2),B'00010000'                         01S0106 00393000
         BNZ   @RF00106                                         01S0106 00394000
         CL    ABENDCOD,OPENCODE                                01S0106 00395000
         BE    @RF00106                                         01S0106 00396000
         TM    SDWAMCHK(R2),B'10000000'                         01S0106 00397000
         BNZ   @RF00106                                         01S0106 00398000
*         GEN(SDUMP MF=(E,DUMPPARM));                           01S0107 00399000
*                                                               01S0107 00400000
         SDUMP MF=(E,DUMPPARM)                                          00401000
*       /*************************************************************/ 00402000
*       /*                                                           */ 00403000
*       /* CHECK WHETHER OR NOT TO RETRY. RETRY IS ATTEMPTED UNLESS  */ 00404000
*       /* THE LCT INDICATES THAT THE ERROR OCCURED PRIOR TO OR      */ 00405000
*       /* DURING ALLOCATION, OR THE ERROR OCCURED DURING OR AFTER   */ 00406000
*       /* UNALLOCATION, THIS IS A RECURSION OR NO JOB HAS BEEN      */ 00407000
*       /* SELECTED (IE, THE LCT DOES NOT CONTAIN BOTH A JCT SWA     */ 00408000
*       /* ADDRESS AND AN SCT SWA ADDRESS. ACTUALLY, THE ONLY TIME   */ 00409000
*       /* THE SCT POINTER IS EXPECTED TO BE ZERO IS BEFORE IEFSD161 */ 00410000
*       /* STORES THE POINTER TO THE FIRST STEP'S SCT). FOR AN OPEN  */ 00411000
*       /* FAILURE, RETRY IS ATTEMPTED BUT THE RECURSION INDICATOR   */ 00412000
*       /* STERETRY IS NOT SET. (SEE 'OPERATION' IN THE PROLOG FOR AN*/ 00413000
*       /* EXPLANATION.)                                      YM07219*/ 00414000
*       /*                                                           */ 00415000
*       /*************************************************************/ 00416000
*                                                               01S0108 00417000
*       IF LCTACOMP='1'B&STERETRY='0'B&STETJMR='0'B&/*        YM07219*/ 00418000
*           LCTJCTVA^=0&LCTSCTVA^=0 THEN                        01S0108 00419000
@RF00106 L     @10,SDWAPARM(,R2)                                01S0108 00420000
         L     @05,STELCT(,@10)                                 01S0108 00421000
         TM    LCTACOMP(@05),B'00001000'                        01S0108 00422000
         BNO   @RF00108                                         01S0108 00423000
         TM    STERETRY(@10),B'11000000'                        01S0108 00424000
         BNZ   @RF00108                                         01S0108 00425000
         ICM   @15,7,LCTJCTVA(@05)                              01S0108 00426000
         BZ    @RF00108                                         01S0108 00427000
         ICM   @15,7,LCTSCTVA(@05)                              01S0108 00428000
         BZ    @RF00108                                         01S0108 00429000
*         DO;                                                   01S0109 00430000
*                                                               01S0109 00431000
*           /*********************************************************/ 00432000
*           /*                                                       */ 00433000
*           /* INDICATE RETRY                                        */ 00434000
*           /*                                                       */ 00435000
*           /*********************************************************/ 00436000
*                                                               01S0110 00437000
*                                                               02S0110 00438000
*           /*********************************************************/ 00439000
*           /*                                                       */ 00440000
*           /* ISSUE SETRP                                           */ 00441000
*           /*                                                       */ 00442000
*           /*********************************************************/ 00443000
*                                                               02S0110 00444000
*           DO;                     /* SETRP RETADDR(IEFIB621)WKAREA(   00445000
*                                      R2)RC(4)DUMP(NO)              */ 00446000
*             RESPECIFY                                         02S0111 00447000
*              (GPR00P,                                         02S0111 00448000
*               GPR01P,                                         02S0111 00449000
*               GPR14P,                                         02S0111 00450000
*               GPR15P) RSTD;                                   02S0111 00451000
*             GPR01P=R2;                                        02S0112 00452000
         LR    GPR01P,R2                                        02S0112 00453000
*             GPR01P->SDWARCDE=4;   /* STORE RC INTO SDWA            */ 00454000
         MVI   SDWARCDE(GPR01P),X'04'                           02S0113 00455000
*             GPR01P->SDWARTYA=ADDR(IEFIB621);/* SAVE RETRY ADDRESS  */ 00456000
         L     @05,@CV01651                                     02S0114 00457000
         ST    @05,SDWARTYA(,GPR01P)                            02S0114 00458000
*             GPR01P->SDWAREQ='0'B; /* TURN OFF DUMP INDICATOR       */ 00459000
         NI    SDWAREQ(GPR01P),B'01111111'                      02S0115 00460000
*             RESPECIFY                                         02S0116 00461000
*              (GPR00P,                                         02S0116 00462000
*               GPR01P,                                         02S0116 00463000
*               GPR14P,                                         02S0116 00464000
*               GPR15P) UNRSTD;                                 02S0116 00465000
*           END;                    /*                        YM02669*/ 00466000
*           RESPECIFY                                           02S0118 00467000
*             SDWA BASED(R2);                                   02S0118 00468000
*           IF ABENDCOD^=OPENCODE THEN                          01S0119 00469000
         CL    ABENDCOD,OPENCODE                                01S0119 00470000
         BE    @RF00119                                         01S0119 00471000
*             STERETRY='1'B;                                    01S0120 00472000
         OI    STERETRY(@10),B'10000000'                        01S0120 00473000
*           IF ABENDCOD=VSMCODE THEN                            01S0121 00474000
@RF00119 CL    ABENDCOD,VSMCODE                                 01S0121 00475000
         BNE   @RF00121                                         01S0121 00476000
*             TCBTCT=STESMFTC;      /* RESTORE ADDR OF TCT IN TCB IF    00477000
