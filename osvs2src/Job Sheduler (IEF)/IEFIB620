*                                      JUST IN CASE CODE IS ADDED       00523000
*                                      AFTER HERE WHICH USES THE        00524000
*                                      SDWA.                         */ 00525000
*           END;                                                02S0134 00526000
*                                                               01S0135 00527000
*           /*********************************************************/ 00528000
*           /*                                                       */ 00529000
*           /* ON NO RETRY SITUATIONS THE ACTIVE JSCB POINTER MAY BE */ 00530000
*           /* LEFT POINTING TO AN AREA TO BE FREED. CHANGE THE      */ 00531000
*           /* ACTIVE JSCB POINTER IN THE JSCB POINTED TO BY THE TCB */ 00532000
*           /* TO POINT TO ITSELF AS THE ACTIVE JSCB. ZERO THE       */ 00533000
*           /* LCTJSCB POINTER.                               YM06978*/ 00534000
*           /*                                                       */ 00535000
*           /*********************************************************/ 00536000
*                                                               01S0135 00537000
*           JSCBACT=TCBJSCB;        /* MAKE THE INIT JSCB THE ACTIVE    00538000
*                                      JSCB                   YM06978*/ 00539000
         L     @10,SDWAPARM(,R2)                                01S0135 00540000
         L     @10,STELCT(,@10)                                 01S0135 00541000
         L     @01,LCTTCBAD(,@10)                               01S0135 00542000
         L     @15,TCBJSCB(,@01)                                01S0135 00543000
         ST    @15,JSCBACT(,@15)                                01S0135 00544000
*           LCTJSCB=0;              /* CLEAR THE JSCB PTR IN THE LCT    00545000
*                                                             YM06978*/ 00546000
         SLR   @15,@15                                          01S0136 00547000
         ST    @15,LCTJSCB(,@10)                                01S0136 00548000
*         END;                      /*                        YM06978*/ 00549000
*       GEN;                                                    01S0138 00550000
@RC00108 DS    0H                                               01S0138 00551000
*       FREE THE AUTOMATIC STORAGE WHICH HAD BEEN EXPLICITLY            00552000
*       GOTTEN                                                          00553000
         L     R0,@SIZDATD        LOAD SUBPOOL AND SIZE TO FREE         00554000
         FREEMAIN R,LV=(0),A=(R11)                                      00555000
*     END;                                                      01S0139 00556000
*   RESPECIFY                                                   01S0140 00557000
*     R14 RSTD;                                                 01S0140 00558000
@RC00049 DS    0H                                               01S0141 00559000
*   R14=R3;                         /* RESTORE RETURN ADDRESS        */ 00560000
*                                                               01S0141 00561000
         LR    R14,R3                                           01S0141 00562000
*   /*****************************************************************/ 00563000
*   /*                                                               */ 00564000
*   /* EPILOG MACRO                                                  */ 00565000
