         TITLE 'IEFQB555-SWA MGR-LOCATE MODE                           *00001000
                        '                                               00002000
IEFQB555 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEFQB555  78.059'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         MODESET EXTKEY=SCHED,SAVEKEY=(2)                               00014000
JSCBDISP EQU 188                                                        00015000
JSCBAOS1 EQU  72                                                        00016000
JSCBAOS2 EQU 192                                                        00017000
*                                  /*                                   00018000
*        BECAUSE OF PROLOGUE CODE GENERATED BY PLS,             Y02669  00019000
*        R2 NOW CONTAINS THE CALLER'S KEY, AND CANNOT BE USED   Y02669  00020000
*        UNTIL IT IS SAVED IN CALLRKEY.                         Y02669  00021000
         L     R3,PSATOLD          POINT TO CURRENT TCB         Y02669  00022000
         L     @11,TCBSWASA(R3)    POINT TO WORK AREA PREFIX    Y02669  00023000
*                                  (OR 0)                       Y02669  00024000
         LTR   @11,@11             CHECK FOR A WORK AREA        Y02669  00025000
         BNZ   CHECKUSE            CHECK WHETHER WORK AREA IS  YM08504  00026000
*                                  IN USE                      YM08504  00027000
*        GET STORAGE FOR A WORK AREA FOR THIS TCB.             YM08504  00028000
*        CHECK WHETHER THIS MODULE'S WORK AREA WOULD BE LARGE  YM08504  00029000
*        ENOUGH FOR EITHER IEFQB550 OR IEFQB555.               YM08504  00030000
         L     R4,@SIZDATD         GET SIZE REQUIRED FOR THIS   Y02669  00031000
*                                  MODULE'S WORK AREA           Y02669  00032000
         LA    R4,0(,R4)           CLEAR SUBPOOL FROM HIGH      Y02669  00033000
*                                  ORDER BYTE                   Y02669  00034000
         L     R1,SPSZ50N          PT. TO ADCON IN QB550       YM08504  00035000
         L     R1,0(,R1)           PT. TO VALUE IN QB550       YM08504  00036000
         L     R1,0(,R1)           GET VALUE IN QB550          YM08504  00037000
         LA    R1,0(,R1)           CLEAR HIGH ORDER BYTE       YM08504  00038000
         CR    R4,R1                                           YM08504  00039000
         BM    GETOTHER            GO GET SIZE REQUIRED FOR    YM08504  00040000
*                                  OTHER MODULE                YM08504  00041000
*        IN EITHER CASE, RESTORE R4 TO GET THE SUBPOOL.        YM08504  00042000
         L     R4,@SIZDATD         USE THIS MODULE'S SIZE      YM08504  00043000
         B     GETPERM             SKIP ELSE CLAUSE            YM08504  00044000
GETOTHER EQU   *                                               YM08504  00045000
         L     R1,SPSZ50N          PT. TO ADCON IN QB555       YM08504  00046000
         L     R1,0(,R1)           PT. TO VALUE IN QB555       YM08504  00047000
         L     R4,0(,R1)           GET VALUE IN QB555          YM08504  00048000
GETPERM  EQU   *                                               YM08504  00049000
         LA    R1,WKPFEX                                        Y02669  00050000
         ALR   R4,R1               ADD SIZE OF PREFIX TO AMOUNT Y02669  00051000
*                                  OF STORAGE REQUIRED BY PLS   Y02669  00052000
*                                  TO GET THE SIZE REQUIRED FOR Y02669  00053000
*                                  THE WORK AREA. (THE SUBPOOL  Y02669  00054000
*                                  NUMBER IN THE HIGH ORDER     Y02669  00055000
*                                  BYTE IS PRESERVED.)          Y02669  00056000
         GETMAIN R,LV=(R4)         GET WORK AREA                Y02669  00057000
         ST    R4,SPSIZE(,R1)      PUT SIZE AND SUBPOOL OF     YM08504  00058000
*                                  WORK AREA IN PREFIX          Y02669  00059000
         MVI   FLAGS(R1),ZERO      CLEAR FLAGS                 YM08504  00060000
         MODESET EXTKEY=ZERO       GET IN KEY ZERO TO STORE           **00061000
                                   IN TCB                       Y02669  00062000
         ST    R1,TCBSWASA(,R3)    SAVE PTR. TO WORKAREA PREFIX Y02669  00063000
         LR    @11,R1              POINT TO WORK AREA PREFIX    Y02669  00064000
*                                  (EXPECTED BY THE FOLLOWING   Y02669  00065000
*                                  PIECE OF CODE)               Y02669  00066000
         MODESET EXTKEY=SCHED      GET IN KEY FOR GETMAIN      YM08504  00067000
CHECKUSE EQU   *                                               YM08504  00068000
         TM    FLAGS(@11),INUSE    CHECK WHETHER WORK AREA     YM08504  00069000
*                                  IS IN USE                   YM08504  00070000
         BNO   PROLOG              NOT IN USE, USE IT          YM08504  00071000
*        WORK AREA IS IN USE (BY AN INTERRUPTED RB ALSO UNDER  YM08504  00072000
*        THIS TCB). GET A WORK AREA TO BE USED JUST FOR THIS   YM08504  00073000
*        INVOCATION OF THIS MODULE.                            YM08504  00074000
         L     R4,@SIZDATD                                     YM08504  00075000
         LA    R1,WKPFEX                                       YM08504  00076000
         ALR   R4,R1                                           YM08504  00077000
         GETMAIN R,LV=(R4)                                     YM08504  00078000
         LR    @11,R1              SAVE PTR. TO WORK AREA      YM08504  00079000
         ST    R4,SPSIZE(,@11)     SAVE FOR FREEMAIN           YM08504  00080000
         MVI   FLAGS(@11),MUSTFREE INDICATE MUST BE FREED ON   YM08504  00081000
*                                  EXIT                        YM08504  00082000
PROLOG   EQU   *                                                Y02669  00083000
         OI    FLAGS(@11),INUSE    SET IN USE                  YM08504  00084000
         LA    @11,WKPFEX(,@11)    POINT TO ACTUAL WORK AREA    Y02669  00085000
*                                  (SKIP THE PREFIX)            Y02669  00086000
         USING @DATD,@11           PROVIDE ADDRESSABILITY       Y02669  00087000
         ST    @13,@SA00001+SLOTBACK  BACKWARD CHAIN SAVE AREAS Y02669  00088000
         STC   @02,CALLRKEY        SAVE CALLER'S KEY            Y02669  00089000
*        NOTE THAT THE MODESET MACRO DOES NOT SUPPORT SYMBOLIC  Y02669  00090000
*        REGISTER NAMES.                                        Y02669  00091000
         MODESET KEYADDR=(2)       GET IN CALLER'S KEY TO             **00092000
                                   STORE IN CALLER'S SAVE AREA  Y02669  00093000
         LM    @00,@04,SLOTR0(@13) RESTORE INPUT REGISTERS      Y02669  00094000
         ST    @11,SLOTFORE(,@13)  FOREWARD CHAIN SAVE AREAS    Y02669  00095000
         LR    @13,@11             POINT TO CURRENT SAVE AREA   Y02669  00096000
*        GET IN KEY 0, IN CASE THE PARAMETER LIST IS IN STORAGE Y02669  00097000
*        WHICH IS FETCH PROTECTED BUT NOT KEY 1.                Y02669  00098000
         MODESET EXTKEY=ZERO                                    Y02669  00099000
         MVC   @PC00001(PARMLEN),0(@01) COPY PARAMETER LIST TO  Y02669  00100000
*                                  WORK AREA                    Y02669  00101000
         MODESET EXTKEY=SCHED      RETURN TO SCHEDULER KEY FOR        **00102000
                                   NORMAL PROCESSING            Y02669  00103000
*   SWEPAPTR=EPAPTR;                /* INITIALIZE EPA BASE POINTER   */ 00104000
         L     @10,@PC00001                                        0083 00105000
         L     SWEPAPTR,EPAPTR(,@10)                               0083 00106000
*   RETNCODE=ZERO;                  /* INITIALIZE RETURN CODE TO 0   */ 00107000
         SLR   @10,@10                                             0084 00108000
         ST    @10,RETNCODE                                        0084 00109000
*   IF FCODE=ASGNLOC THEN           /* IF THIS IS AN ASSIGN LOCATE   */ 00110000
         L     @10,@PC00001+4                                      0085 00111000
         CLC   FCODE(2,@10),@CC00962                               0085 00112000
         BNE   @RF00085                                            0085 00113000
*     CALL QB555ASN;                /* CALL THAT INTERNAL PROCEDURE  */ 00114000
         BAL   @14,QB555ASN                                        0086 00115000
*   ELSE                                                           0087 00116000
*     IF FCODE=WRTLOC THEN          /* IF THIS IS A WRITE LOCATE     */ 00117000
         B     @RC00085                                            0087 00118000
@RF00085 L     @10,@PC00001+4                                      0087 00119000
         CLC   FCODE(2,@10),@CC00964                               0087 00120000
         BNE   @RF00087                                            0087 00121000
*       CALL QB555WRT;              /* CALL THAT INTERNAL PROCEDURE  */ 00122000
         BAL   @14,QB555WRT                                        0088 00123000
*     ELSE                                                         0089 00124000
*       IF FCODE=READLOC THEN       /* IF THIS IS A READ LOCATE      */ 00125000
         B     @RC00087                                            0089 00126000
@RF00087 L     @10,@PC00001+4                                      0089 00127000
         CLC   FCODE(2,@10),@CC00966                               0089 00128000
         BNE   @RF00089                                            0089 00129000
*         CALL QB555RD;             /* CALL THAT INTERNAL PROCEDURE  */ 00130000
         BAL   @14,QB555RD                                         0090 00131000
*       ELSE                                                       0091 00132000
*         IF FCODE=DELBLOCK THEN    /* IF THIS IS A DELETE BLOCK     */ 00133000
         B     @RC00089                                            0091 00134000
@RF00089 L     @10,@PC00001+4                                      0091 00135000
         CLC   FCODE(2,@10),@CC00968                               0091 00136000
         BNE   @RF00091                                            0091 00137000
*           CALL QB555DEL;          /* CALL THAT INTERNAL PROCEDURE  */ 00138000
         BAL   @14,QB555DEL                                        0092 00139000
*         ELSE                                                     0093 00140000
*           RETNCODE=INVFUNCT;      /* THEN IT'S AN INVALID REQUEST  */ 00141000
         B     @RC00091                                            0093 00142000
@RF00091 LA    @10,4                                               0093 00143000
         ST    @10,RETNCODE                                        0093 00144000
*   IF RETNCODE^=ZERO THEN          /* IF ANY ERRORS                 */ 00145000
@RC00091 DS    0H                                                  0094 00146000
@RC00089 DS    0H                                                  0094 00147000
@RC00087 DS    0H                                                  0094 00148000
@RC00085 L     @10,RETNCODE                                        0094 00149000
         LTR   @10,@10                                             0094 00150000
         BZ    @RF00094                                            0094 00151000
*     DO;                                                          0095 00152000
*       RESPECIFY                                                  0096 00153000
*        (R1,                                                      0096 00154000
*         R15) RESTRICTED;                                         0096 00155000
*       R15=RETNCODE;               /* LOAD ERROR INDICATION         */ 00156000
         L     R15,RETNCODE                                        0097 00157000
*       R1=ZEROBZER;                /* LOAD ABEND CODE OF 0B0 SYSTEM */ 00158000
         ICM   R1,15,ZEROBZER                                      0098 00159000
*       GEN(ABEND (1),,,SYSTEM);    /* ISSUE ABEND                   */ 00160000
         ABEND (1),,,SYSTEM                                             00161000
*       RESPECIFY                                                  0100 00162000
*        (R1,                                                      0100 00163000
*         R15) UNRESTRICTED;        /* RELEASE FOR PLS USE           */ 00164000
*     END;                                                         0101 00165000
*   GEN REFS(CALLRKEY,RETNCODE);                                   0102 00166000
@RF00094 DS    0H                                                  0102 00167000
*                                     /*                                00168000
*        THE EPILOG CODE MUST BE CODED IN ASSEMBLER BECAUSE    YM08504  00169000
*        EITHER THE 'IN USE' FLAG MUST BE TURNED OFF OR THE    YM08504  00170000
*        WORK AREA WAS A TEMPORARY ONE AND MUST BE FREED.      YM08504  00171000
         L     R4,RETNCODE         SAVE RETURN CODE            YM08504  00172000
         IC    @02,CALLRKEY        SAVE CALLER'S PROTECT KEY   YM08504  00173000
         L     @13,SLOTBACK(,@13)  SAVE PTR. TO THE CALLER'S   YM08504  00174000
*                                  SAVE AREA                   YM08504  00175000
         LA    @01,WKPFEX                                      YM08504  00176000
         SR    @11,@01             SUBTRACT SIZE OF PREFIX TO  YM08504  00177000
*                                  POINT TO ACTUAL BEGINNING   YM08504  00178000
*                                  OF WORK AREA                YM08504  00179000
         TM    FLAGS(@11),MUSTFREE CHECK WHETHER AREA IS TO BE YM08504  00180000
*                                  FREED                       YM08504  00181000
         BO    GOFREE              YES, GO FREE                YM08504  00182000
         NI    FLAGS(@11),X'FF'-INUSE DO NOT FREE, TURN OFF IN YM08504  00183000
*                                  USE FLAG                    YM08504  00184000
         B     EPILOG              SKIP ELSE CLAUSE            YM08504  00185000
GOFREE   EQU   *                                               YM08504  00186000
         L     @00,SPSIZE(,@11)                                YM08504  00187000
         FREEMAIN R,LV=(0),A=(@11)                             YM08504  00188000
EPILOG   EQU   *                                               YM08504  00189000
         MODESET KEYADDR=(2)       SET CALLER'S KEY            YM08504  00190000
         LR    @15,R4              SET RETURN CODE             YM08504  00191000
         L     @14,SLOTR14(,@13)   RESTORE REGISTERS           YM08504  00192000
         LM    @00,@12,SLOTR0(@13)                             YM08504  00193000
         BR    @14                 RETURN                      YM08504  00194000
*                                                                  0103 00195000
*/********************************************************************/ 00196000
*/* THIS IS THE INTERNAL PROCEDURE WHICH DOES THE ASSIGNS. THERE IS  */ 00197000
*/* A 16 BYTE EPA FOR EACH BLOCK TO BE ASSIGNED. THEY ARE CHAINED    */ 00198000
*/* TOGETHER WITH THE END OF THE CHAIN INDICATED BY A ZERO CHAIN     */ 00199000
*/* POINTER. THERE IS NO LIMIT ON THE NUMBER OF BLOCKS TO BE         */ 00200000
*/* ASSIGNED. EACH EPA CONTAINS THE LENGTH OF THAT BLOCK TO BE       */ 00201000
*/* ASSIGNED. THE SUBPOOL NUMBER FOR THE BLOCK IS OBTAINED FROM THE  */ 00202000
*/* QMPA. THE QMPA IS LOCATED VIA THE 'ACTIVE JSCB'. THIS JSCB       */ 00203000
*/* IS POINTED TO BY THE JSCBACT FIELD OF THE JSCB WHICH IS LOCATED  */ 00204000
*/* VIA THE CURRENT TCB. AFTER EACH GETMAIN IS ACCOMPLISHED THE      */ 00205000
*/* ASSOCIATED SWA PREFIX IS PARTIALLY INITIALIZED AND INFO IS       */ 00206000
*/* RETURNED IN THE EPA. A MODESET IS ISSUED TO PUT US IN THE        */ 00207000
*/* CALLERS KEY BEFORE PUTTING INFO IN THE EPA. ANOTHER MODESET PUTS */ 00208000
*/* US IN SUPERVISOR KEY BEFORE UPDATING THE RBN FIELD OF THE QMPA.  */ 00209000
*/* THE ONLY ERROR CONDITION DETECTED IS A ZERO OR NEGATIVE LENGTH   */ 00210000
*/* VALUE BEING PASSED. THE GETMAINS ARE UNCONDITIONAL. UPON         */ 00211000
*/* COMPLETION, CONTROL IS RETURNED TO THE EXTERNAL PROCEDURE.       */ 00212000
*/********************************************************************/ 00213000
*                                                                  0103 00214000
*QB555ASN:                                                         0103 00215000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* ASSIGN PROCEDURE        Y02669*/ 00216000
QB555ASN DS    0H                                                  0104 00217000
*   RFY                                                            0104 00218000
*    (R14) RSTD;                    /*                        YM08414*/ 00219000
*   RESPECIFY                                                      0105 00220000
*     IEZJSCB BASED(JSCBPTR);       /* BASE ON POINTER INTERNAL TO 0105 00221000
*                                      THIS MODULE                   */ 00222000
*   JSCBPTR=TCBJSCB;                /* GET CURRENT JSCB POINTER      */ 00223000
         L     @15,PSATOLD                                         0106 00224000
         L     JSCBPTR,TCBJSCB(,@15)                               0106 00225000
*   JSCBPTR=JSCBACT;                /* SET POINTER TO JSCB TO BE USED   00226000
*                                      FOR THIS ASSIGN               */ 00227000
         L     JSCBPTR,JSCBACT(,JSCBPTR)                           0107 00228000
*   QMPAPTR=JSCBQMPI;               /* SET QMPA BASE POINTER         */ 00229000
         L     QMPAPTR,JSCBQMPI(,JSCBPTR)                          0108 00230000
*   DO WHILE(SWEPAPTR^=ZERO&RETNCODE=ZERO);/* LOOP UNTIL DONE OR   0109 00231000
*                                      UNTIL ERROR                   */ 00232000
         B     @DE00109                                            0109 00233000
@DL00109 DS    0H                                                  0110 00234000
*     IF SWLNGTH>ZERO THEN          /* IS LENGTH A POSITIVE VALUE    */ 00235000
         L     @15,SWLNGTH(,SWEPAPTR)                              0110 00236000
         LTR   @15,@15                                             0110 00237000
         BNP   @RF00110                                            0110 00238000
*       DO;                                                        0111 00239000
*         LNGTH=SWLNGTH+SWPRELNG;   /* GET LENGTH FROM EPA AND ADD 0112 00240000
*                                      SWA PREFIX LENGTH             */ 00241000
         LA    @15,16(,@15)                                        0112 00242000
         STCM  @15,7,LNGTH                                         0112 00243000
*         SPNUM=QMSWSP;             /* GET SUBPOOL # FROM QMPA       */ 00244000
         MVC   SPNUM(1),QMSWSP(QMPAPTR)                            0113 00245000
*         RESPECIFY                                                0114 00246000
*          (R0,                                                    0114 00247000
*           R1) RESTRICTED;         /* RESTRICT FOR GETMAIN          */ 00248000
*         R0=GETFRSPC;              /* LOAD INTO REGISTER 0          */ 00249000
         L     R0,GETFRSPC                                         0115 00250000
*         GEN(GETMAIN R,LV=(0));    /* GET SPACE FOR 1 BLOCK AND   0116 00251000
*                                      BLOCK PREFIX                  */ 00252000
         GETMAIN R,LV=(0)                                               00253000
*         SPFXPTR=R1;               /* INITIALIZE SWA PREFIX PTR     */ 00254000
         LR    SPFXPTR,R1                                          0117 00255000
*         GOTSPACE=R1;              /* SAVE FOR USE AS 3 BYTE SVA    */ 00256000
         ST    R1,GOTSPACE                                         0118 00257000
*         GEN(MODESET KEYADDR=CALLRKEY,WORKREG=1);/* SWITCH TO CALLER   00258000
*                                      KEY                           */ 00259000
         MODESET KEYADDR=CALLRKEY,WORKREG=1                             00260000
*         RESPECIFY                                                0120 00261000
*          (R0,                                                    0120 00262000
*           R1) UNRESTRICTED;       /* RELEASE FOR PL/S USE          */ 00263000
*         SWVA=SWPREADR;            /* PUT SVA IN CALLERS EPA        */ 00264000
         L     @15,SWPREADR-1                                      0121 00265000
         LA    @15,0(,@15)                                         0121 00266000
         STCM  @15,7,SWVA(SWEPAPTR)                                0121 00267000
*         SWBLKPTR=SWPREADR+SWPRELNG;/* SWA BLOCK ADDRESS = PREFIX 0122 00268000
*                                      ADDRESS + PREFIX LENGTH       */ 00269000
         AL    @15,@CF00183                                        0122 00270000
         ST    @15,SWBLKPTR(,SWEPAPTR)                             0122 00271000
*         GEN(MODESET EXTKEY=SUPR); /* SWITCH TO KEY 0               */ 00272000
         MODESET EXTKEY=SUPR                                            00273000
*         TEMPSAV1=QMRBN;           /* GET RBN FROM QMPA             */ 00274000
         L     TEMPSAV1,QMRBN(,QMPAPTR)                            0124 00275000
*LOOP:                                                             0125 00276000
*         TEMPSAV2=TEMPSAV1+ONE;    /* INCREMENT RBN BY 1            */ 00277000
LOOP     LA    TEMPSAV2,1                                          0125 00278000
         ALR   TEMPSAV2,TEMPSAV1                                   0125 00279000
*         CS(TEMPSAV1,TEMPSAV2,QMRBN);/* COMPARE AND SWAP IF QMRBN 0126 00280000
*                                      HAS NOT CHANGED               */ 00281000
         CS    TEMPSAV1,@09,QMRBN(QMPAPTR)                         0126 00282000
*         BC(4,LOOP);               /* TRY AGAIN IF QMRBN HAS CHANGED*/ 00283000
         BC    4,LOOP                                              0127 00284000
*         GEN(MODESET EXTKEY=SCHED);/* SWITCH BACK TO SCHEDULER KEY  */ 00285000
         MODESET EXTKEY=SCHED                                           00286000
*         CLEARPTR=SPFXPTR;         /* SET BASE FOR SWASPACE         */ 00287000
         LR    CLEARPTR,SPFXPTR                                    0129 00288000
*         IF LNGTH>TWOFIFSX THEN    /* WAS GETMAIN > 256             */ 00289000
         L     @15,LNGTH-1                                         0130 00290000
         LA    @15,0(,@15)                                         0130 00291000
         C     @15,@CF00959                                        0130 00292000
         BNH   @RF00130                                            0130 00293000
*           DO;                                                    0131 00294000
*             QUOTIENT=LNGTH/TWOFIFSX;/* 256 BYTE MULTIPLES          */ 00295000
         LR    QUOTIENT,@15                                        0132 00296000
         SRL   QUOTIENT,8                                          0132 00297000
*             REMAINDR=LNGTH//TWOFIFSX;/* REMAINDER                  */ 00298000
         LR    @00,@15                                             0133 00299000
         SRDA  @00,32                                              0133 00300000
         D     @00,@CF00959                                        0133 00301000
         LR    REMAINDR,@00                                        0133 00302000
*             DO I=1 TO QUOTIENT;   /* ONCE PER 256 BYTE AREA        */ 00303000
         LA    I,1                                                 0134 00304000
         B     @DE00134                                            0134 00305000
@DL00134 DS    0H                                                  0135 00306000
*               SWASPACE=SWASPACE&&SWASPACE;/* CLEAR 256 BYTES       */ 00307000
         XC    SWASPACE(256,CLEARPTR),SWASPACE(CLEARPTR)           0135 00308000
*               CLEARPTR=CLEARPTR+TWOFIFSX;/* UP BASE POINTER        */ 00309000
         AL    CLEARPTR,@CF00959                                   0136 00310000
*             END;                                                 0137 00311000
         AL    I,@CF00046                                          0137 00312000
@DE00134 CR    I,QUOTIENT                                          0137 00313000
         BNH   @DL00134                                            0137 00314000
*           END;                                                   0138 00315000
*         ELSE                                                     0139 00316000
*           REMAINDR=LNGTH;         /* PICK UP LENGTH IF 256 OR LESS */ 00317000
         B     @RC00130                                            0139 00318000
@RF00130 L     REMAINDR,LNGTH-1                                    0139 00319000
         LA    REMAINDR,0(,REMAINDR)                               0139 00320000
*         IF REMAINDR^=ZERO THEN    /* WAS THERE ANY REMAINDR        */ 00321000
@RC00130 LTR   REMAINDR,REMAINDR                                   0140 00322000
         BZ    @RF00140                                            0140 00323000
*           SWASPACE(1:REMAINDR)=SWASPACE(1:REMAINDR)&&SWASPACE(1: 0141 00324000
*               REMAINDR);          /* CLEAR THE REMAINDER           */ 00325000
         LR    @15,REMAINDR                                        0141 00326000
         BCTR  @15,0                                               0141 00327000
         EX    @15,@SX01044                                        0141 00328000
*         SWPRBN=TEMPSAV2;          /* PUT RBN IN SWA PREFIX         */ 00329000
@RF00140 ST    TEMPSAV2,SWPRBN(,SPFXPTR)                           0142 00330000
*         SWPVA=SWPREADR;           /* PUT SELF SVA IN SWA PREFIX    */ 00331000
         MVC   SWPVA(3,SPFXPTR),SWPREADR                           0143 00332000
*         SWPLNGTH=SWLNGTH;         /* PUT LENGTH IN SWA PREFIX      */ 00333000
         MVC   SWPLNGTH(3,SPFXPTR),SWLNGTH+1(SWEPAPTR)             0144 00334000
*         SWEPAPTR=SWCHNPTR;        /* UPDATE BASE TO NEXT EPA       */ 00335000
         L     SWEPAPTR,SWCHNPTR(,SWEPAPTR)                        0145 00336000
*       END;                        /* END OF DO LOOP                */ 00337000
*     ELSE                                                         0147 00338000
*       RETNCODE=INVLNGTH;          /* INVALID LENGTH ERROR          */ 00339000
         B     @RC00110                                            0147 00340000
@RF00110 LA    @15,16                                              0147 00341000
         ST    @15,RETNCODE                                        0147 00342000
*   END;                                                           0148 00343000
@RC00110 DS    0H                                                  0148 00344000
@DE00109 SLR   @15,@15                                             0148 00345000
         CR    SWEPAPTR,@15                                        0148 00346000
         BE    @DC00109                                            0148 00347000
         C     @15,RETNCODE                                        0148 00348000
         BE    @DL00109                                            0148 00349000
@DC00109 DS    0H                                                  0149 00350000
*   RFY                                                            0149 00351000
*    (R14) UNRSTD;                  /*                        YM08414*/ 00352000
*   END QB555ASN;                   /* END OF ASSIGN INT. PROCEDURE  */ 00353000
@EL00002 DS    0H                                                  0150 00354000
@EF00002 DS    0H                                                  0150 00355000
@ER00002 BR    @14                                                 0150 00356000
*                                                                  0151 00357000
*/********************************************************************/ 00358000
*/* THIS IS THE INTERNAL PROCEDURE THAT DOES THE WRITES. AS WITH     */ 00359000
*/* OTHER LOCATE MODE FUNCTIONS THERE IS A 16 BYTE EPA FOR EACH      */ 00360000
*/* BLOCK. THEY ARE CHAINED TOGETHER WITH THE END OF CHAIN INDICATED */ 00361000
*/* BY A ZERO CHAIN POINTER. THERE IS NO LIMIT ON THE NUMBER OF      */ 00362000
*/* EPAS. EACH EPA IS CHECKED FOR A VALID SVA AND BLOCK POINTER.     */ 00363000
*/* IF EITHER ERROR CONDITION EXISTS THE APPROPRIATE ERROR CODE IS   */ 00364000
*/* SET AND RETURN IS MADE TO THE EXTERNAL PROCEDURE WITHOUT FURTHER */ 00365000
*/* PROCESSING. IF NO ERROR IS DETECTED THE SWA PREFIX IS TESTED TO  */ 00366000
*/* SEE IF THIS BLOCK HAS BEEN PREVIOUSLY WRITTEN. IF NOT, THE SWA   */ 00367000
