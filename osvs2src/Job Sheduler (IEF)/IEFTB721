*IF TERMTYPE='10'X & JCTOPT1='1'B |    /* AT JOB TERM. WITH OPT=1 OR 2  00045000
*                                         SMF RECORDING WILL BE DONE */ 00046000
*   TERMTYPE='0C'X & JCTOPT2='1'B THEN  /* AT STEP TERM WITH OPT=2   */ 00047000
