         SL    @12,@CF02465                                     02S0165 00331000
         STH   @12,RECORDLN                                     02S0165 00332000
*ACTLEN=JACTSIZE;                      /* JOB ACTSIZE FOR IEFTB722   */ 00333000
         STC   JACTSIZE,ACTLEN                                  02S0166 00334000
*IF TERMTYPE='0C'X THEN                /* IS THIS STEP TERMINATION?  */ 00335000
         CLI   TERMTYPE,X'0C'                                   02S0167 00336000
         BNE   @RF00167                                         02S0167 00337000
*  DO;                                 /* YES- DO UNIQUE STEP PROCESS*/ 00338000
*                                      /* DEVICE ENTRY LENGTH FIELD  */ 00339000
