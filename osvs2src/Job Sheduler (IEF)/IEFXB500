*                                                                  0296 00532000
*ERRMSG:                                                           0296 00533000
*   PROCEDURE;                                                     0296 00534000
ERRMSG   STM   @14,@09,@SA00005                                    0296 00535000
         STM   @11,@12,@SA00005+48                                 0296 00536000
*   GEN( LOAD EP=IEFXB603  );                                      0297 00537000
          LOAD EP=IEFXB603                                              00538000
*   R3=R0+SKPID;                    /* OBTAIN BEGINNING TO MSGTB     */ 00539000
         LA    R3,24                                               0298 00540000
         ALR   R3,R0                                               0298 00541000
*   DO WHILE MSGID^=MSGNO;          /* FIND THE PROPER MSG BY        */ 00542000
         B     @DE00299                                            0299 00543000
@DL00299 DS    0H                                                  0300 00544000
*     R3=R3+NXT;                    /* CHECKING ID                   */ 00545000
         AL    R3,@CF00064                                         0300 00546000
*   END;                            /* MSG FOUND                     */ 00547000
@DE00299 CLC   MSGID(4,R3),@CC02623                                0301 00548000
         BNE   @DL00299                                            0301 00549000
*/*   MOVE MSG TXT INTO GETMAIN AREA                                 */ 00550000
*                                                                  0302 00551000
*   MSG(ONE:MSGLNG)=MSGADDR->MESS1(ONE:MSGLNG);                    0302 00552000
         SLR   @15,@15                                             0302 00553000
         IC    @15,MSGLNG(,R3)                                     0302 00554000
         BCTR  @15,0                                               0302 00555000
         L     @14,MSGADDR-1(,R3)                                  0302 00556000
         EX    @15,@SM02701                                        0302 00557000
*   R1=ADDR(MSG);                                                  0303 00558000
         LA    R1,MSG                                              0303 00559000
*   GEN(WTO MF=(E,(R1)));           /* ISSUE THE MSG TO PGMR         */ 00560000
         WTO MF=(E,(R1))                                                00561000
*   GEN( MODESET EXTKEY=SUPR);      /* CHANGE TO KEY ZERO            */ 00562000
          MODESET EXTKEY=SUPR                                           00563000
*   JSCBJNLE=ON;                    /* SET JNL ERROR BIT IN JSCB     */ 00564000
         OI    JSCBJNLE(JSCBPTR),B'00100000'                       0306 00565000
*   GEN( MODESET EXTKEY=SCHED);     /* RESET TO KEY ONE              */ 00566000
          MODESET EXTKEY=SCHED                                          00567000
*   ERROR=BAD;                      /* SET INTERNAL ERROR            */ 00568000
         LA    ERROR,2                                             0308 00569000
*   GEN(MODESET KEYADDR=(2) );      /* SET TO CALLER'S KEY           */ 00570000
         MODESET KEYADDR=(2)                                            00571000
*   JNLERR=ON;                      /* SEND RETURN CODE              */ 00572000
         OI    JNLERR(JNLPTR),B'10000000'                          0310 00573000
*   GEN( MODESET EXTKEY=SCHED );    /* RESET TO SCHED KEY            */ 00574000
          MODESET EXTKEY=SCHED                                          00575000
*   END ERRMSG;                                                    0312 00576000
@EL00005 DS    0H                                                  0312 00577000
@EF00005 DS    0H                                                  0312 00578000
@ER00005 LM    @14,@09,@SA00005                                    0312 00579000
         LM    @11,@12,@SA00005+48                                 0312 00580000
         BR    @14                                                 0312 00581000
*                                                                  0313 00582000
