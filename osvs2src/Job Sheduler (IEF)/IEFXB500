*   VIRTUAL=JCTVULDP;               /* OBTAIN PTR TO 1ST VUT         */ 00779000
         SLR   VIRTUAL,VIRTUAL                                     0367 00780000
         ICM   VIRTUAL,7,JCTVULDP(JCTPTR)                          0367 00781000
*   DO WHILE VIRTUAL^=NO&ERROR<BAD; /* CONTINUE IS NO ERROR          */ 00782000
         B     @DE00368                                            0368 00783000
@DL00368 DS    0H                                                  0369 00784000
*     CALL JOURNAL;                 /* JOURNAL VUT                   */ 00785000
