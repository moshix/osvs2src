         BCTR  @04,0                                               0285 00492000
         EX    @04,@SM02699                                        0285 00493000
*       GEN REFS(ENQE);                                            0286 00494000
    ENQ MF=(E,ENQE)        ISSUE EXECUTE FORM                           00495000
*     END;                                                         0287 00496000
*   RFY                                                            0288 00497000
*     IFGRPL BASED(JSCBJNLR);       /* SET UP RPL BASE PTR           */ 00498000
@RF00280 DS    0H                                                  0289 00499000
*   RPLAREA=VIRTUAL;                /* PTR TO BLK TO BE JOURNALLED   */ 00500000
         L     @04,JSCBJNLR(,JSCBPTR)                              0289 00501000
         ST    VIRTUAL,RPLAREA(,@04)                               0289 00502000
*   RPLRLEN=VIRTUAL->SWPLNGTH+PRFX; /* OBTAIN RECORD LENGTH          */ 00503000
         L     @03,SWPLNGTH-1(,VIRTUAL)                            0290 00504000
         LA    @03,0(,@03)                                         0290 00505000
         AL    @03,PRFX                                            0290 00506000
         ST    @03,RPLRLEN(,@04)                                   0290 00507000
*   R4=JSCBJNLR;                    /* LOAD ADDR OF RPL LIST         */ 00508000
         L     @03,JSCBJNLR(,JSCBPTR)                              0291 00509000
         LR    R4,@03                                              0291 00510000
*   GEN( PUT RPL=(R4));             /* JOURNAL THE BLOCK             */ 00511000
          PUT RPL=(R4)                                                  00512000
*   IF R15^=ZERO THEN                                              0293 00513000
         LTR   R15,R15                                             0293 00514000
         BZ    @RF00293                                            0293 00515000
*     CALL ERRMSG;                  /* ERROR ON JOURNAL              */ 00516000
         BAL   @14,ERRMSG                                          0294 00517000
*   END;                            /* SUBSYSTEM                     */ 00518000
@EL00004 DS    0H                                                  0295 00519000
@EF00004 DS    0H                                                  0295 00520000
@ER00004 LM    @14,@09,@SA00004                                    0295 00521000
