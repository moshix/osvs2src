*                                                                  0350 00703000
*/********************************************************************/ 00704000
*/*                                                                  */ 00705000
*/*  THIS FUNCTION IS CALLED WHEN THE CONTROL IS PASSED FROM THE     */ 00706000
*/*  INITIATOR MODULE IEFSD162 AFTER ALLOCATION IS DONE,     @YM07086*/ 00707000
*/*  OR WHEN THE CONTROL IS PASSED FROM IEFXB604 AFTER JOB   @YM07086*/ 00708000
*/*  HAS RESTARTED BUT BEFORE JOB RESUMES NORMAL PROCESSING. @YM07086*/ 00709000
*/*  A CHAIN OF CONTROL BLOCKS ARE JOURNALLED-JCT,PDI,GDGNT, @YM07086*/ 00710000
*/*  VUT AND SCT.  PROCEDURE RUNSIOT WILL BE CALLED TO       @YM07086*/ 00711000
*/*  JOURNAL SIOT,JFCB,JFCBX AND JFCBE. NOTE THAT FOR        @Z40MPSJ*/ 00712000
*/*  RESTARTED JOBS, ALL SCTS (FROM FIRST STEP TO THE        @YM07086*/ 00713000
*/*  FAILING STEP BUT NOT INCLUDEING THE FAILING STEP) ARE   @YM07086*/ 00714000
*/*  JOURNALLED AND FOR NORMAL JOB ONLY THE CURRENT STEP'S   @YM07086*/ 00715000
*/*  SCT IS JOURNALLED.    THE JOURNALLING PROCESSING IS     @YM07086*/ 00716000
*/*  TERMINATED IF ERROR SHOULD OCCUR DURING WRITE TO JOB    @YM07086*/ 00717000
*/*  JOURNAL DATA SET.                                       @YM07086*/ 00718000
*/*                                                                  */ 00719000
*/********************************************************************/ 00720000
*                                                                  0350 00721000
*RUNCHAIN:                                                         0350 00722000
*   PROCEDURE;                                                     0350 00723000
RUNCHAIN STM   @14,@09,@SA00008                                    0350 00724000
         STM   @11,@12,@SA00008+48                                 0350 00725000
*   LCTPTR=JNLPPTRX;                /* SAVE LCT PTR                  */ 00726000
         L     @15,JNLPPTRX(,JNLPTR)                               0351 00727000
         LR    LCTPTR,@15                                          0351 00728000
*   JCTPTR=JNLPPTRX->LCTJCTAD;      /* PTR TO IN CORE JCT            */ 00729000
         L     JCTPTR,LCTJCTAD(,@15)                               0352 00730000
*   VIRTUAL=JCTDSKAD;               /* OBTAIN PTR TO JCT             */ 00731000
         SLR   VIRTUAL,VIRTUAL                                     0353 00732000
         ICM   VIRTUAL,7,JCTDSKAD(JCTPTR)                          0353 00733000
*   CALL JOURNAL;                   /* JOURNAL JCT                   */ 00734000
         BAL   @14,JOURNAL                                         0354 00735000
*   VIRTUAL=JCTGDGNT;               /* OBTAIN PTR TO GDG     @YM07086*/ 00736000
         SLR   VIRTUAL,VIRTUAL                                     0355 00737000
         ICM   VIRTUAL,7,JCTGDGNT(JCTPTR)                          0355 00738000
*   DO WHILE ERROR<BAD&VIRTUAL^=NO; /* CONTINUE TO END IF NO ERROR   */ 00739000
         B     @DE00356                                            0356 00740000
@DL00356 DS    0H                                                  0357 00741000
*     CALL JOURNAL;                 /* JOURNAL GDG           @YM07086*/ 00742000
         BAL   @14,JOURNAL                                         0357 00743000
*     VIRTUAL=VIRTUAL+PRFX;         /* SKIP THE PREFIX       @YM07086*/ 00744000
         AL    VIRTUAL,PRFX                                        0358 00745000
*     VIRTUAL=VIRTUAL->GNTNEXTP;    /* OBTAIN PTR TO NEXT GDG      0359 00746000
*                                                            @YM07086*/ 00747000
         SLR   @15,@15                                             0359 00748000
         ICM   @15,7,GNTNEXTP(VIRTUAL)                             0359 00749000
         LR    VIRTUAL,@15                                         0359 00750000
*   END;                            /*                       @YM07086*/ 00751000
@DE00356 C     ERROR,@CF00078                                      0360 00752000
         BNL   @DC00356                                            0360 00753000
         LTR   VIRTUAL,VIRTUAL                                     0360 00754000
         BNZ   @DL00356                                            0360 00755000
@DC00356 DS    0H                                                  0361 00756000
*   VIRTUAL=JCTPDIP;                /* OBTAIN PTR TO PDI BLOCK     0361 00757000
*                                                            @YM07086*/ 00758000
         SLR   VIRTUAL,VIRTUAL                                     0361 00759000
         ICM   VIRTUAL,7,JCTPDIP(JCTPTR)                           0361 00760000
*   DO WHILE VIRTUAL^=NO&ERROR<BAD; /* CONTINUE IF NO ERROR  @YM07086*/ 00761000
         B     @DE00362                                            0362 00762000
@DL00362 DS    0H                                                  0363 00763000
*     CALL JOURNAL;                 /* JOURNAL PDI BLOCK     @YM07086*/ 00764000
         BAL   @14,JOURNAL                                         0363 00765000
*     PDIBLPTR=VIRTUAL+PRFX;        /* SKIP THE PREFIX               */ 00766000
         LR    @15,VIRTUAL                                         0364 00767000
         AL    @15,PRFX                                            0364 00768000
         ST    @15,PDIBLPTR                                        0364 00769000
*     VIRTUAL=PDIBLPTR->PDINPDIP;   /* OBTAIN PTR TO NEXT PDI BLK    */ 00770000
         SLR   VIRTUAL,VIRTUAL                                     0365 00771000
         ICM   VIRTUAL,7,PDINPDIP(@15)                             0365 00772000
*   END;                                                           0366 00773000
@DE00362 LTR   VIRTUAL,VIRTUAL                                     0366 00774000
         BZ    @DC00362                                            0366 00775000
         C     ERROR,@CF00078                                      0366 00776000
         BL    @DL00362                                            0366 00777000
