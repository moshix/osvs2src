*     DO;                           /* NO ERROR              @YM07086*/ 00658000
         B     @RC00331                                            0333 00659000
@RF00331 DS    0H                                                  0334 00660000
*       JHRPTR=ADDR(WORK)+PRFX;     /* OBTAIN JHR PTR        @YM07086*/ 00661000
         LA    JHRPTR,WORK                                         0334 00662000
         AL    JHRPTR,PRFX                                         0334 00663000
*       JHRSTRBA=JSCBJRBA;          /* RBA AT RESTART        @YM07086*/ 00664000
         MVC   JHRSTRBA(8,JHRPTR),JSCBJRBA(JSCBPTR)                0335 00665000
*       JHRCKRBA=SAVERBA;           /* RBA FOR POINT AFTER CHKPT   0336 00666000
*                                      RESTART               @YM07086*/ 00667000
         MVC   JHRCKRBA(8,JHRPTR),SAVERBA                          0336 00668000
*       GEN(PUT RPL=(R4));          /* PUT UPDATE            @YM07086*/ 00669000
         PUT RPL=(R4)                                                   00670000
*       IF R15^=ZERO THEN                                          0338 00671000
         LTR   R15,R15                                             0338 00672000
         BZ    @RF00338                                            0338 00673000
*         CALL ERRMSG;              /*                       @YM07086*/ 00674000
         BAL   @14,ERRMSG                                          0339 00675000
*       ELSE                                                       0340 00676000
*         DO;                       /* NO ERROR              @YM07086*/ 00677000
         B     @RC00338                                            0340 00678000
@RF00338 DS    0H                                                  0341 00679000
*           RPLDIR=OF;              /* TURN OFF DIRECT BIT   @YM07086*/ 00680000
         L     @03,JSCBJNLR(,JSCBPTR)                              0341 00681000
         NI    RPLDIR(@03),B'10111111'                             0341 00682000
*           RPLRBAR=SAVERBA;        /* RESTORE PUT RBA IN RPL      0342 00683000
*                                                            @YM07086*/ 00684000
         MVC   RPLRBAR(8,@03),SAVERBA                              0342 00685000
*           RPLUPD=OF;              /* TURN OFF UPDATE BIT   @YM07086*/ 00686000
         NI    RPLUPD(@03),B'11111101'                             0343 00687000
*           GEN( MODESET EXTKEY=SUPR );/* SET KEY TO ZERO    @YM07086*/ 00688000
          MODESET EXTKEY=SUPR                                           00689000
*           JSCBJRBA=NOTHING;       /* CLEAR SAVED RBA AFTER UPDATE     00690000
*                                                            @YM07086*/ 00691000
         MVC   JSCBJRBA(8,JSCBPTR),NOTHING                         0345 00692000
*           GEN( MODESET EXTKEY=SCHED );/* SET KEY TO ONE    @YM07086*/ 00693000
          MODESET EXTKEY=SCHED                                          00694000
*         END;                      /*                       @YM07086*/ 00695000
*     END;                          /*                       @YM07086*/ 00696000
