         OI    ENTMADE,B'00100000'                                 0692 01542000
         NI    PUTENTRY,B'10111111'                                0692 01543000
*                 BLKENTRY=VATNO;   /* ENTRY LOCATION IN VAT         */ 01544000
         SLR   BLKENTRY,BLKENTRY                                   0693 01545000
         IC    BLKENTRY,VATNO(,VATPTR)                             0693 01546000
*               END;                                               0694 01547000
*           END;                                                   0695 01548000
@RC00678 DS    0H                                                  0696 01549000
*       END;                                                       0696 01550000
@RC00669 DS    0H                                                  0697 01551000
*   END;                                                           0697 01552000
@RF00667 DS    0H                                                  0697 01553000
@DE00642 TM    PUTENTRY,B'01000000'                                0697 01554000
         BO    @DL00642                                            0697 01555000
*   END VATPUT;                                                    0698 01556000
@EL00008 DS    0H                                                  0698 01557000
@EF00008 DS    0H                                                  0698 01558000
@ER00008 LM    @14,@01,@SA00008                                    0698 01559000
         LM    @03,@04,@SA00008+16                                 0698 01560000
