*                 VATRBN(VATNO)=JNLRBN;/* FILL IN RBN                */ 01505000
         MH    @10,@CH02079                                        0685 01506000
         L     @15,JNLRBN                                          0685 01507000
         ST    @10,@TF00001                                        0685 01508000
         ALR   @10,VATPTR                                          0685 01509000
         BCTR  @10,0                                               0685 01510000
         BCTR  @10,0                                               0685 01511000
         STCM  @15,15,VATRBN-12(@10)                               0685 01512000
*                 VATROVA(VATNO)=JNLVA;/* FILL IN OLD VIRTUAL ADDRESS*/ 01513000
         LR    @10,VATPTR                                          0686 01514000
         AL    @10,@TF00001                                        0686 01515000
         MVC   VATROVA-14(3,@10),JNLVA                             0686 01516000
*                 VATRNVA(VATNO)=SWROWVA;/* FILL IN NEW VIRTUAL    0687 01517000
*                                      ADDRESS                       */ 01518000
         LR    @10,VATPTR                                          0687 01519000
         AL    @10,@TF00001                                        0687 01520000
         MVC   VATRNVA-14(3,@10),SWROWVA(EPAPTR)                   0687 01521000
*                 VATBLKID(VATNO)=JNLID;/* FILL IN BLOCK ID          */ 01522000
         LR    @10,VATPTR                                          0688 01523000
         AL    @10,@TF00001                                        0688 01524000
         MVC   VATBLKID-14(1,@10),JNLID                            0688 01525000
*                 IF(JNLID=ISIOTID|JNLID=IJFCBID) THEN/* SIOT OR   0689 01526000
*                                      JFCB?                         */ 01527000
         CLI   JNLID,X'03'                                         0689 01528000
         BE    @RT00689                                            0689 01529000
         CLI   JNLID,X'1C'                                         0689 01530000
         BNE   @RF00689                                            0689 01531000
@RT00689 DS    0H                                                  0690 01532000
*                   VATDYNAM(VATNO)=ON;/* MARK VAT ENTRY AS DYNAMIC  */ 01533000
         SLR   @10,@10                                             0690 01534000
