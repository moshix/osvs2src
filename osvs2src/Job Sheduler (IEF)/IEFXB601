         CH    @14,MELSCTNO(,@15)                                  0323 00120000
         BNE   @RF00323                                            0323 00121000
*             DO;                                                  0324 00122000
*               RBAJOURN=SHRJRBA;   /* SAVE JOURNAL RBA              */ 00123000
         MVC   RBAJOURN(8),SHRJRBA(@03)                            0325 00124000
*               RBASMSG=SHRMRBA;    /* SAVE SYSTEM MESSAGE RBA       */ 00125000
         MVC   RBASMSG(8),SHRMRBA(@03)                             0326 00126000
*               FAILSTEP=ON;        /* FAILING STEP HAS BEEN FOUND   */ 00127000
         OI    FAILSTEP,B'10000000'                                0327 00128000
*               IF MELRSTRT=AUTOSTEP THEN                          0328 00129000
         CLI   MELRSTRT(@15),X'80'                                 0328 00130000
         BNE   @RF00328                                            0328 00131000
*                 CALL STEPMRGE;    /* MERGE FAILING STEP            */ 00132000
         BAL   @14,STEPMRGE                                        0329 00133000
*               ELSE                                               0330 00134000
*                 IF MELRSTRT=AUTOCKPT THEN/* IF AUTO CKPT RESTART 0330 00135000
*                                      MERGE                         */ 00136000
         B     @RC00328                                            0330 00137000
@RF00328 L     @03,@PC00001                                        0330 00138000
         CLI   MELRSTRT(@03),X'40'                                 0330 00139000
         BNE   @RF00330                                            0330 00140000
*                   CALL CKPTMRGE;  /* FAILING STEP                  */ 00141000
         BAL   @14,CKPTMRGE                                        0331 00142000
*             END;                                                 0332 00143000
*           ELSE                    /* AUTO RESTART BUT NOT          */ 00144000
*             CALL SYSMERGE;        /* FAILING STEP                  */ 00145000
         B     @RC00323                                            0333 00146000
@RF00323 BAL   @14,SYSMERGE                                        0333 00147000
*         END;                                                     0334 00148000
@RC00323 DS    0H                                                  0334 00149000
@DE00322 LTR   RCAREA,RCAREA                                       0334 00150000
         BNZ   @DC00322                                            0334 00151000
         CLI   JNLID,X'C0'                                         0334 00152000
         BE    @DL00322                                            0334 00153000
@DC00322 DS    0H                                                  0335 00154000
*     END;                                                         0335 00155000
*                                                                  0335 00156000
@RC00320 DS    0H                                                  0336 00157000
*/* FOR AN AUTOMATIC RESTART IF THE FAILING STEP IS NOT FOUND ON THE */ 00158000
*/* JOURNAL,THIS IS AN ERROR CONDITION.AN ERROR CODE IS SET AND A    */ 00159000
*/* MESSAGE IS WRITTEN.                                              */ 00160000
*                                                                  0336 00161000
*   IF((MELRSTRT=AUTOSTEP|MELRSTRT=AUTOCKPT)&RCAREA=ENDFILE) THEN  0336 00162000
@RF00317 L     @07,@PC00001                                        0336 00163000
         CLI   MELRSTRT(@07),X'80'                                 0336 00164000
         BE    @GL00003                                            0336 00165000
         CLI   MELRSTRT(@07),X'40'                                 0336 00166000
         BNE   @RF00336                                            0336 00167000
@GL00003 C     RCAREA,@CF02600                                     0336 00168000
         BNE   @RF00336                                            0336 00169000
*     IF FAILSTEP=OFFSW THEN                                       0337 00170000
