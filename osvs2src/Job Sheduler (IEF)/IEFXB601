         BZ    @RF00352                                            0352 00221000
*               DO;                 /* YES HANDLE POINT ERROR      0353 00222000
*                                                            @ZA13822*/ 00223000
*                 JSCBJNLE=ON;      /* TURN JNL ERROR BIT ON @ZA13822*/ 00224000
         OI    JSCBJNLE(JSCBPTR),B'00100000'                       0354 00225000
*                 RCAREA=JOURNERR;  /* SET JNL ERROR RETCDE  @ZA13822*/ 00226000
         LA    RCAREA,4                                            0355 00227000
*               END;                /* END POINT ERROR PROCESS     0356 00228000
*                                                            @ZA13822*/ 00229000
*             ELSE                  /* NO POINT ERROR CONTINUE     0357 00230000
*                                                            @ZA13822*/ 00231000
*               DO;                 /* MAKE JOB LOOK NEW     @ZA13822*/ 00232000
         B     @RC00352                                            0357 00233000
@RF00352 DS    0H                                                  0358 00234000
*                 LCTRFBSM=OFFSW;   /* TURN OFF CALL IEFXB601 SWITCH    00235000
