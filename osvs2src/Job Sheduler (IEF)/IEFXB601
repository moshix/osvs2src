         LTR   @07,@07                                             0341 00189000
         BNZ   @RF00341                                            0341 00190000
*       DO;                         /* NO JNL JCT PROCESSING @ZA13822*/ 00191000
*         JCTPTR=LCTJCTAD;          /* INITIALIZE FOR CLEANUP      0343 00192000
*                                                            @ZA13822*/ 00193000
         L     @10,LCTJCTAD(,LCTPTR)                               0343 00194000
         ST    @10,JCTPTR                                          0343 00195000
*         IF SHRFOUND=OFFSW|        /* FIRST JHR ONLY        @ZA13822*/ 00196000
*             SHRSNUMB=ONE THEN     /* LAST SHR FOR STEP ONE @ZA13822*/ 00197000
         TM    SHRFOUND,B'10000000'                                0344 00198000
         BZ    @RT00344                                            0344 00199000
         LA    @10,JNLBLK                                          0344 00200000
         CLC   SHRSNUMB(2,@10),@CH00085                            0344 00201000
         BNE   @RF00344                                            0344 00202000
@RT00344 DS    0H                                                  0345 00203000
*           DO;                     /* YES, PROCESS AS NEW JOB     0345 00204000
*                                                            @ZA13822*/ 00205000
*             RPLRBAR=JSCBFRBA;     /* RESET JNL FOR NEW JOB @ZA13822*/ 00206000
         MVC   RPLRBAR(8,RPLPTR),JSCBFRBA(JSCBPTR)                 0346 00207000
*             RPLARG=ADDR(RPLRBAR); /* ADDRESS OF JNL RBA    @ZA13822*/ 00208000
