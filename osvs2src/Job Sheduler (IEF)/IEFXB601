         SLR   RCAREA,RCAREA                                       0361 00244000
*               END;                /* END NEW JOB PROC      @ZA13822*/ 00245000
*           END;                    /* END NEW JOB PROCESSING      0363 00246000
*                                                            @ZA13822*/ 00247000
*         ELSE                      /* OTHER THAN FIRST STEP @ZA13822*/ 00248000
*           DO;                     /* PROCESS AS JNL ERROR  @ZA13822*/ 00249000
         B     @RC00344                                            0364 00250000
@RF00344 DS    0H                                                  0365 00251000
*             JSCBJNLE=ON;          /* SET JNL ERROR BIT ON  @ZA13822*/ 00252000
         OI    JSCBJNLE(JSCBPTR),B'00100000'                       0365 00253000
*             RCAREA=JOURNERR;      /* SET JNL ERROR RETCDE  @ZA13822*/ 00254000
         LA    RCAREA,4                                            0366 00255000
*           END;                    /* END JNL ERROR PROCESS @ZA13822*/ 00256000
*       END;                        /* END NO JNL JCT PROCESS      0368 00257000
*                                                            @ZA13822*/ 00258000
*     ELSE                          /* JNL JCT HAS BEEN READ @ZA13822*/ 00259000
*       CALL ADDRUPDT;              /* UPDATE ADDRESSES IN BLOCKS  0369 00260000
*                                                            @ZA13822*/ 00261000
         B     @RC00341                                            0369 00262000
@RF00341 BAL   @14,ADDRUPDT                                        0369 00263000
*   IF(RCAREA^=ZERO&MSGSW=ZERO) THEN                               0370 00264000
@RC00341 DS    0H                                                  0370 00265000
@RC00339 SLR   @10,@10                                             0370 00266000
         CR    RCAREA,@10                                          0370 00267000
