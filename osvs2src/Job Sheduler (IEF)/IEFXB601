*             GEN(POINT RPL=(2));   /* POINT TO JNL BEGIN    @ZA13822*/ 00215000
         POINT RPL=(2)                                                  00216000
*             RESPECIFY                                            0351 00217000
*               R2 UNRSTD;          /* RELEASE R2 FROM POINT @ZA13822*/ 00218000
*             IF RETCODE^=ZERO THEN /* ERROR IN POINT?       @ZA13822*/ 00219000
