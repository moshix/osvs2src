*             LUVPTR=LUVPTR+LUVENTLN;/* UPDATE PTR TO NEXT ENTRY     */ 03174000
         L     @09,EDTPTR                                          1193 03175000
         L     @09,EDTLUVSP(,@09)                                  1193 03176000
         AL    @11,LUVENTLN(,@09)                                  1193 03177000
         ST    @11,LUVPTR                                          1193 03178000
*           END;                                                   1194 03179000
         AL    @12,@CF00069                                        1194 03180000
@DE01192 ST    @12,I                                               1194 03181000
