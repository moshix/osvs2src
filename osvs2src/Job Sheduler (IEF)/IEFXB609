@RF01464 SLR   @12,@12                                             1469 04263000
         L     @11,DJFCXPTR                                        1469 04264000
         SLR   @09,@09                                             1469 04265000
         ICM   @09,7,JFCBXTTR(@11)                                 1469 04266000
         CR    @12,@09                                             1469 04267000
         BNE   @RF01469                                            1469 04268000
         L     @11,JFCBXPTR                                        1469 04269000
         SLR   @09,@09                                             1469 04270000
         ICM   @09,7,JFCBXTTR(@11)                                 1469 04271000
         CR    @12,@09                                             1469 04272000
         BE    @RF01469                                            1469 04273000
*         STPXLOOP=ON;              /* STOP THE LOOP BUT TEST        */ 04274000
         OI    STPXLOOP,B'10000000'                                1470 04275000
*/***********************************************************@ZA20563*/ 04276000
*/*  CODE TO ZERO THE JFCBXTTR FIELD ON A DEFERRED CHECKPOINT@ZA20563*/ 04277000
*/*  RESTART WAS REMOVED TO ALLOW THE VOLUME COUNT TO BE     @ZA20563*/ 04278000
*/*  INCREASED WHEN THE JOB WAS RESUMED                      @ZA20563*/ 04279000
*/***********************************************************@ZA20563*/ 04280000
*                                                                  1471 04281000
*   END;                                                           1471 04282000
@RF01469 DS    0H                                                  1471 04283000
@RC01464 DS    0H                                                  1471 04284000
@DE01436 TM    STPXLOOP,B'10000000'                                1471 04285000
         BZ    @DL01436                                            1471 04286000
*   END JFCBXJOB;                                                  1472 04287000
@EL00018 DS    0H                                                  1472 04288000
@EF00018 DS    0H                                                  1472 04289000
@ER00018 LM    @14,@06,@SA00018                                    1472 04290000
