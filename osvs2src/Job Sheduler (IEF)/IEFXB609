@RF01170 DS    0H                                                  1190 03159000
*           EDTPTR=ADDR(EDTTAB);    /* ADDR'ABILITY FOR EDT          */ 03160000
         L     @12,@CV03915                                        1190 03161000
         ST    @12,EDTPTR                                          1190 03162000
*           LUVPTR=EDTLUVSP+LENGTH(LUVHDR);/* INIT SEARCH PTR.       */ 03163000
         LA    @11,8                                               1191 03164000
         AL    @11,EDTLUVSP(,@12)                                  1191 03165000
         ST    @11,LUVPTR                                          1191 03166000
*           DO I=1 TO LUVENTNO-1 WHILE SCTUTYPE^=UNITNAME;         1192 03167000
         LA    @12,1                                               1192 03168000
