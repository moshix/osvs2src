         L     @12,DJFCBPTR                                        1209 03240000
         IC    @11,JFCBVOLS(,@12)                                  1209 03241000
         STC   @11,JFCBTVOL                                        1209 03242000
*   JFCBTENQ=DJFCBPTR->JFCBIND2;    /* SAVE FOR ENQ          @ZA07018*/ 03243000
         L     @12,DJFCBPTR                                        1210 03244000
         IC    @11,JFCBIND2(,@12)                                  1210 03245000
         STC   @11,JFCBTENQ                                        1210 03246000
*   IF SCTDUMMY=NOTON THEN          /* DO NOT PROCESS JFCB FOR DUMMY */ 03247000
         L     @12,SIOTPTR                                         1211 03248000
         TM    SCTDUMMY(@12),B'10000000'                           1211 03249000
         BNZ   @RF01211                                            1211 03250000
*     IF(SCTSGDGS=ON|DJFCBPTR->JFCGDG=NOTON) THEN/* ONLY MERGE JFCB     03251000
*                                      FOR GDG SINGLE OR NONGDG, NOT    03252000
*                                      FOR GDGALL            @ZA06230*/ 03253000
