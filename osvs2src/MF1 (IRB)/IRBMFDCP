         ALR   @11,@08                                             0056 00295000
         ST    @11,STRVNSGT(,@12)                                  0056 00296000
*   /*****************************************************************/ 00297000
*   /*                                                               */ 00298000
*   /* CAUSE OTHER CPU'S TO EXIT WAIT STATE SO THAT WAIT TIME CAN BE */ 00299000
*   /* ACCOUNTED FOR BY IEAQWAIT BEFORE IT IS GATHERED BY IRBMFDCP   */ 00300000
*   /*                                                               */ 00301000
*   /*****************************************************************/ 00302000
*                                                                  0057 00303000
*   DCCPUNUM=1;                     /* SET LOW ORDER CPU MASK, CPU #N*/ 00304000
         STH   @08,DCCPUNUM                                        0057 00305000
*   DO I=DCMAXCPU BY-1 TO 1;        /* FOR ALL CPU'S                 */ 00306000
         LA    I,16                                                0058 00307000
@DL00058 DS    0H                                                  0059 00308000
*     IF(DCCPUBIT&CSDCPUAL)^=0      /* IF CPU IS ALIVE, SIGNAL IT    */ 00309000
*       THEN                                                       0059 00310000
         L     @12,CVTPTR                                          0059 00311000
         L     @11,CVTCSD(,@12)                                    0059 00312000
         LH    @11,CSDCPUAL(,@11)                                  0059 00313000
         N     @11,@CF01381                                        0059 00314000
         LH    @08,DCCPUBIT                                        0059 00315000
         N     @08,@CF01381                                        0059 00316000
         NR    @11,@08                                             0059 00317000
