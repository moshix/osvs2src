*         RFY                                                      0067 00360000
*          (GPR11P,                                                0067 00361000
*           GPR12P) UNRSTD;                                        0067 00362000
*       END;                        /* END IF                        */ 00363000
*     DCCPUNUM=DCCPUNUM*2;          /* SHIFT CPU BIT MASK TO LEFT    */ 00364000
@RF00059 LH    @12,DCCPUNUM                                        0069 00365000
         N     @12,@CF01381                                        0069 00366000
         ALR   @12,@12                                             0069 00367000
         STH   @12,DCCPUNUM                                        0069 00368000
*   END;                            /* END I LOOP                    */ 00369000
*                                                                  0070 00370000
         BCTR  I,0                                                 0070 00371000
         LTR   I,I                                                 0070 00372000
         BP    @DL00058                                            0070 00373000
*   /*****************************************************************/ 00374000
*   /*                                                               */ 00375000
*   /* RETURN TO CALLER BY SPECIAL RETURN SEQUENCE WHICH DOES NOT    */ 00376000
*   /* FREE AUTOMATIC DATA OR INVALIDATE ADDRESSING. THE PURPOSE IS  */ 00377000
*   /* TO ALLOW ALL PROLOGS, MOVES, AND EPILOGS TO EXECUTE TOGETHER. */ 00378000
*   /* THE EFFECT IN ANY INTERVAL-DRIVEN MG ROUTINE IS AS IF THE     */ 00379000
*   /* PROLOG, MOVE, AND EPILOG WERE EXECUTED AS SEQUENTIAL CODE     */ 00380000
*   /* DCMOVE: IRBBREAK MACDATE Y-2 73018                            */ 00381000
*   /*                                                               */ 00382000
*   /*****************************************************************/ 00383000
*                                                                  0071 00384000
*DCMOVE:                                                           0071 00385000
*                                                                  0071 00386000
*   /*****************************************************************/ 00387000
*   /*                                                               */ 00388000
*   /* INDICATE NEXT ROUTINE ADDRESS TO SVC MAINLINE FOR REENTRY     */ 00389000
*   /*                                                               */ 00390000
*   /*****************************************************************/ 00391000
*                                                                  0071 00392000
*   STSMENTR=ADDR(DCRT0019);                                       0071 00393000
DCMOVE   L     @12,@PC00001                                        0071 00394000
         L     @12,DCSMAPTR(,@12)                                  0071 00395000
         LA    @11,DCRT0019                                        0071 00396000
         ST    @11,STSMENTR(,@12)                                  0071 00397000
*   RESPECIFY                                                      0072 00398000
*    (GPR00P,                                                      0072 00399000
*     GPR01P,                                                      0072 00400000
*     GPR14P,                                                      0072 00401000
*     GPR15P) RESTRICTED;                                          0072 00402000
*   GENERATE;                                                      0073 00403000
         STM   @02,@12,28(@13)     SAVE REGISTERS FOR NEXT SEGMENT      00404000
         L     @13,4(,@13)         BACK CHAIN TO CALLERS SAVEAREA       00405000
         LM    @14,@12,12(@13)     RESTORE CALLERS REGISTERS            00406000
         BR    @14                 RETURN TO CALLER                     00407000
*                                                                  0074 00408000
*   /*****************************************************************/ 00409000
*   /*                                                               */ 00410000
*   /* REENTRY POINT FROM CALLER, IGX00014.                          */ 00411000
*   /*                                                               */ 00412000
*   /*****************************************************************/ 00413000
*                                                                  0074 00414000
*DCRT0019:                                                         0074 00415000
*   GENERATE;                                                      0074 00416000
DCRT0019 DS    0H                                                  0074 00417000
         STM   @14,@12,12(@13)     SAVE CALLERS REGISTERS               00418000
         L     @01,0(,@01)         GET ADDRESS OF SAVE AREA ADDRESS     00419000
         L     @01,0(,@01)         GET SAVE AREA ADDRESS                00420000
         LM    @02,@12,28(@01)     RELOAD REGISTERS                     00421000
         ST    @13,4(,@01)         BACK CHAIN NEW SAVEAREA TO OLD       00422000
         ST    @01,8(,@13)         FORWARD CHAIN FROM OLD TO NEW        00423000
         LR    @13,@01             R13 NOW POINTS TO NEW SAVEAREA       00424000
*   RESPECIFY                                                      0075 00425000
*    (GPR00P,                                                      0075 00426000
*     GPR01P,                                                      0075 00427000
*     GPR14P,                                                      0075 00428000
*     GPR15P) UNRESTRICTED;                                        0075 00429000
*                                                                  0075 00430000
*   /*****************************************************************/ 00431000
*   /*                                                               */ 00432000
*   /* BEGIN MOVE SEGMENT. THIS CODE IS ENTERED DISABLED WITH NO     */ 00433000
*   /* LOCKS AND EXECUTES WITH NO PAGE FAULTS. THE FUNCTION IS TO    */ 00434000
*   /* MOVE DATA INTO STORAGE OBTAINED BY THE PROLOG. THE FIRST      */ 00435000
*   /* SEGMENT OF CODE DEALS WITH THE ONLINE/OFFLINE STATUS OF THE   */ 00436000
*   /* CPUS. THREE 16 BIT FIELDS ARE INVOLVED. CSDCPUAL IS A SET OF  */ 00437000
*   /* FLAGS WHICH INDICATE THE CURRENT ONLINE STATUS. CSDMF1CP      */ 00438000
*   /* INDICATES WHETHER ANY VARY ACTIVITY HAS TAKEN PLACE ON A CPU  */ 00439000
*   /* SINCE THE LAST INTERVAL. DCCPUVLD INDICATES THOSE CPUS WHICH  */ 00440000
*   /* HAVE BEEN OBSERVED ONLINE IN AT LEAST ONE INTERVAL            */ 00441000
*   /* OBSERVATION. SMF RECORD CPU ENTRIES WILL BE CREATED ONLY FOR  */ 00442000
*   /* THOSE CPUS INDICATED IN DCCPUVLD.                             */ 00443000
*   /*                                                               */ 00444000
*   /*****************************************************************/ 00445000
*                                                                  0076 00446000
*   DCCPUAL=CSDCPUAL;               /* MOVE CURRENT ONLINE FLAGS TO     00447000
*                                      TEMPORARY STORAGE             */ 00448000
         L     @11,CVTPTR                                          0076 00449000
         L     @11,CVTCSD(,@11)                                    0076 00450000
         MVC   DCCPUAL(2),CSDCPUAL(@11)                            0076 00451000
*   DCMF1CP=CSDMF1CP;               /* MOVE VARY ACTIVITY FLAGS TO 0077 00452000
*                                      TEMPORARY STORAGE             */ 00453000
         MVC   DCMF1CP(2),CSDMF1CP(@11)                            0077 00454000
*   CSDMF1CP=0;                     /* CLEAR CSD FIELD FOR ACTIVITY     00455000
*                                      DURING THE NEXT INTERVAL      */ 00456000
