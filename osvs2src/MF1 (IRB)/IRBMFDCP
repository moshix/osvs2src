         BZ    @RF00059                                            0059 00319000
*       DO;                         /* DO REMOTE IMMEDIATE INTER-  0060 00320000
*                                      PROCESSOR COMMUNICATION       */ 00321000
*         RFY                                                      0061 00322000
*          (GPR11P,                                                0061 00323000
*           GPR12P) RSTD;                                          0061 00324000
*                                                                  0061 00325000
*         /***********************************************************/ 00326000
*         /*                                                         */ 00327000
*         /* MACDATE Y-2 73018                                       */ 00328000
*         /*                                                         */ 00329000
*         /***********************************************************/ 00330000
*                                                                  0062 00331000
*         RESPECIFY                                                0062 00332000
*          (GPR00P,                                                0062 00333000
*           GPR01P,                                                0062 00334000
*           GPR14P,                                                0062 00335000
*           GPR15P) RESTRICTED;                                    0062 00336000
*         GPR01P=PCCAT00P(I);       /* PCCA ADDRESS OF CPU           */ 00337000
         LR    @08,I                                               0063 00338000
         SLA   @08,2                                               0063 00339000
         L     @07,CVTPTR                                          0063 00340000
         L     @06,CVTPCCAT(,@07)                                  0063 00341000
         AL    @06,@CF01380                                        0063 00342000
         L     GPR01P,PCCAT00P(@08,@06)                            0063 00343000
*         GPR12P=ADDR(CVTBRET);     /* ROUTINE EXECUTED IS BCR 15,14 */ 00344000
         LA    GPR12P,CVTBRET(,@07)                                0064 00345000
*         GEN(RISGNL SERIAL,CPU=(1),EP=(12));/* SIGNAL CPU           */ 00346000
*                                                                  0065 00347000
         RISGNL SERIAL,CPU=(1),EP=(12)                                  00348000
*         /***********************************************************/ 00349000
*         /*                                                         */ 00350000
*         /* MACDATE Y-2 73018                                       */ 00351000
*         /*                                                         */ 00352000
*         /***********************************************************/ 00353000
*                                                                  0066 00354000
*         RESPECIFY                                                0066 00355000
*          (GPR00P,                                                0066 00356000
*           GPR01P,                                                0066 00357000
