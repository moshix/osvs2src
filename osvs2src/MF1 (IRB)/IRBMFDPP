         L     @09,STRVSSGT(,@03)                                  0054 00257000
         LA    @08,0(@14,@09)                                      0054 00258000
         AL    @08,@CF01164                                        0054 00259000
         ST    GPR00P,STSGFREE(,@08)                               0054 00260000
*   STSGADD(STRVNSGT)=GPR01P;       /* ADDRESS                       */ 00261000
*                                                                  0055 00262000
         AL    @09,@CF01165                                        0055 00263000
         ST    GPR01P,STSGADD-4(@14,@09)                           0055 00264000
*   /*****************************************************************/ 00265000
*   /*                                                               */ 00266000
*   /* MACDATE Y-2 73018                                             */ 00267000
