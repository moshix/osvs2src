*                                                                  0056 00270000
*   RESPECIFY                                                      0056 00271000
*    (GPR00P,                                                      0056 00272000
*     GPR01P) UNRESTRICTED;                                        0056 00273000
*   STRVNSGT=STRVNSGT+1;            /* POINT TO NEXT SGT SLOT        */ 00274000
*                                                                  0057 00275000
         AL    @15,@CF00060                                        0057 00276000
         ST    @15,STRVNSGT(,@03)                                  0057 00277000
*   /*****************************************************************/ 00278000
*   /*                                                               */ 00279000
*   /* RETURN TO CALLER BY SPECIAL RETURN SEQUENCE WHICH DOES NOT    */ 00280000
*   /* FREE AUTOMATIC DATA OR INVALIDATE ADDRESSING. THE PURPOSE IS  */ 00281000
*   /* TO ALLOW ALL PROLOGS, MOVES, AND EPILOGS TO EXECUTE TOGETHER. */ 00282000
*   /* THE EFFECT IN ANY INTERVAL-DRIVEN MG ROUTINE IS AS IF THE     */ 00283000
*   /* PROLOG, MOVE, AND EPILOG WERE EXECUTED AS SEQUENTIAL CODE     */ 00284000
*   /* DPMOVE: IRBBREAK MACDATE Y-2 73018                            */ 00285000
*   /*                                                               */ 00286000
*   /*****************************************************************/ 00287000
*                                                                  0058 00288000
*DPMOVE:                                                           0058 00289000
*                                                                  0058 00290000
*   /*****************************************************************/ 00291000
*   /*                                                               */ 00292000
*   /* INDICATE NEXT ROUTINE ADDRESS TO SVC MAINLINE FOR REENTRY     */ 00293000
*   /*                                                               */ 00294000
*   /*****************************************************************/ 00295000
*                                                                  0058 00296000
*   STSMENTR=ADDR(DPRT0017);                                       0058 00297000
DPMOVE   L     @03,@PC00001                                        0058 00298000
         L     @03,DPSMAPTR(,@03)                                  0058 00299000
         LA    @15,DPRT0017                                        0058 00300000
         ST    @15,STSMENTR(,@03)                                  0058 00301000
*   RESPECIFY                                                      0059 00302000
*    (GPR00P,                                                      0059 00303000
*     GPR01P,                                                      0059 00304000
*     GPR14P,                                                      0059 00305000
*     GPR15P) RESTRICTED;                                          0059 00306000
*   GENERATE;                                                      0060 00307000
         STM   @02,@12,28(@13)     SAVE REGISTERS FOR NEXT SEGMENT      00308000
         L     @13,4(,@13)         BACK CHAIN TO CALLERS SAVEAREA       00309000
         LM    @14,@12,12(@13)     RESTORE CALLERS REGISTERS            00310000
         BR    @14                 RETURN TO CALLER                     00311000
*                                                                  0061 00312000
*   /*****************************************************************/ 00313000
*   /*                                                               */ 00314000
*   /* REENTRY POINT FROM CALLER, IGX00014.                          */ 00315000
*   /*                                                               */ 00316000
*   /*****************************************************************/ 00317000
*                                                                  0061 00318000
*DPRT0017:                                                         0061 00319000
*   GENERATE;                                                      0061 00320000
DPRT0017 DS    0H                                                  0061 00321000
         STM   @14,@12,12(@13)     SAVE CALLERS REGISTERS               00322000
         L     @01,0(,@01)         GET ADDRESS OF SAVE AREA ADDRESS     00323000
         L     @01,0(,@01)         GET SAVE AREA ADDRESS                00324000
         LM    @02,@12,28(@01)     RELOAD REGISTERS                     00325000
         ST    @13,4(,@01)         BACK CHAIN NEW SAVEAREA TO OLD       00326000
         ST    @01,8(,@13)         FORWARD CHAIN FROM OLD TO NEW        00327000
         LR    @13,@01             R13 NOW POINTS TO NEW SAVEAREA       00328000
*   RESPECIFY                                                      0062 00329000
*    (GPR00P,                                                      0062 00330000
*     GPR01P,                                                      0062 00331000
*     GPR14P,                                                      0062 00332000
*     GPR15P) UNRESTRICTED;                                        0062 00333000
*                                                                  0062 00334000
*/* BEGIN MOVE SEGMENT.  THIS CODE IS ENTERED DISABLED WITH NO LOCKS    00335000
*    AND EXECUTES WITH NO PAGE FAULTS.  THE FUNCTION IS TO MOVE DATA    00336000
*    INTO STORAGE OBTAINED BY THE PROLOG.                            */ 00337000
*                                                                  0063 00338000
*   /*****************************************************************/ 00339000
*   /*                                                               */ 00340000
*   /* PROVIDE ADDRESSIBILITY TO THE SMF RECORD                      */ 00341000
*   /*                                                               */ 00342000
*   /*****************************************************************/ 00343000
*                                                                  0063 00344000
*   RESPECIFY                                                      0063 00345000
*     SMFRCD71 BASED(DPSMIADD);                                    0063 00346000
*                                                                  0063 00347000
*   /*****************************************************************/ 00348000
*   /*                                                               */ 00349000
*   /* MOVE IN STANDARD SMF HEADER AND MF/1 CONTROL SECTION          */ 00350000
*   /*                                                               */ 00351000
*   /*****************************************************************/ 00352000
*                                                                  0064 00353000
*   SMFRCD71=DPSMFHDR(1:LENGTH(SMFRCD71));                         0064 00354000
*                                                                  0064 00355000
