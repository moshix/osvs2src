         LA    @15,@ENDDATD                                             00855000
         LA    @00,DWPGFECB                                             00856000
         PGFIX R,A=(@01),EA=(@15),ECB=(@00),LONG=N                      00857000
         WAIT  ECB=DWPGFECB                                             00858000
*   /*****************************************************************/ 00859000
*   /*                                                               */ 00860000
*   /* REMEMBER FIXED LOCATIONS IN STORAGE RESOURCE TABLE (STSCT)    */ 00861000
*   /*                                                               */ 00862000
*   /*****************************************************************/ 00863000
