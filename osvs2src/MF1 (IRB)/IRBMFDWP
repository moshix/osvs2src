*   /*****************************************************************/ 00842000
*   /*                                                               */ 00843000
*   /* PROGRAM FIX WAS REMEMBERED IN RESOURCE TABLE WHEN LOADED. FIX */ 00844000
*   /* AUTOMATIC DATA.                                               */ 00845000
*   /*                                                               */ 00846000
*   /*****************************************************************/ 00847000
*                                                                  0050 00848000
*   DWPGFECB=CLEARECB;              /* ZERO ECB FOR PAGE FIX         */ 00849000
