*   /* FIX CODE AND STATIC DATA.                                     */ 00828000
*   /*                                                               */ 00829000
*   /*****************************************************************/ 00830000
*                                                                  0048 00831000
*   DWPGFECB=CLEARECB;              /* ZERO ECB FOR PAGE FIX         */ 00832000
@RF00044 SLR   @10,@10                                             0048 00833000
         ST    @10,DWPGFECB                                        0048 00834000
*   GEN SETS(DWPGFECB);                                            0049 00835000
*                                                                  0049 00836000
         LA    @01,@PSTART                                              00837000
         LA    @15,@ENDDATA                                             00838000
