         BNH   @DL00165                                            0169 00794000
*   RFY                                                            0170 00795000
*     INPMA;                                                       0170 00796000
*   INCOXINV='0'B;                  /* CLEAR ALL                     */ 00797000
*   INCOXSYN='0'B;                  /* ERROR FLAGS                   */ 00798000
         NI    INCOXINV,B'11111001'                                0172 00799000
*   SRSTRM=SRSTRCAD;                /* STRUCTURE ENTRY               */ 00800000
