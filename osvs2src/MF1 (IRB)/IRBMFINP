*/* PROCEDURE NAME = MFRINTGR                                        */ 01955000
*/*                                                                  */ 01956000
*/*  DESCRIPTIVE NAME = INTEGER RECOGNIZER                           */ 01957000
*/*                                                                  */ 01958000
*/*  FUNCTION = RECOGNIZES ANY INTEGER, CONVERTS IT AND SAVES IT FOR */ 01959000
*/*             OPTION INITIALIZATION.                               */ 01960000
*/*                                                                  */ 01961000
*/* DEPENDENCY = CHARACTER CODE DEPENDENT                            */ 01962000
*/*                                                                  */ 01963000
*                                                                  0434 01964000
*MFRINTGR:                                                         0434 01965000
*   PROCEDURE(RISAVNUM,RISCB);                                     0434 01966000
MFRINTGR STM   @14,@12,@SA00008                                    0434 01967000
         MVC   @PC00008(8),0(@01)                                  0434 01968000
*   DECLARE                                                        0435 01969000
*     RISAVNUM                      /* PLACE TO STORE INTEGER PTR    */ 01970000
*         PTR(31),                                                 0435 01971000
*     1 RISCB,                      /* SCAN CONTROL BLOCK            */ 01972000
*      2 RICURBCB                   /* CURRENT BCB PTR               */ 01973000
*            PTR(31),                                              0435 01974000
*      2 RICURTXT                   /* CURRENT TEXT PTR              */ 01975000
*            PTR(31),                                              0435 01976000
*      2 RIBYTLFT                   /* # BYTES LEFT                  */ 01977000
*            FIXED(31),                                            0435 01978000
*        RINUMBER                   /* COMPUTED VALUE OF INTEGER     */ 01979000
*            FIXED(32),                                            0435 01980000
*        RIINTGER                   /* NUMBER IN SOURCE'S BUFFER     */ 01981000
*            CHAR(1)BASED(RICURTXT),                               0435 01982000
*        RIINTDEC                   /* ACTUAL NUMBER                 */ 01983000
*            CHAR(1),                                              0435 01984000
*        RITEMNUM                   /* TEMPORARY VALUE               */ 01985000
*            FIXED(32),                                            0435 01986000
*        RIINTFND                   /* INTEGER FOUND INDICATOR       */ 01987000
*            BIT(1),                                               0435 01988000
*        RINUMVAL                   /* PLACE TO STORE INTEGER        */ 01989000
*            BASED(ININTPTR(RINUMCOD))FIXED(32),                   0435 01990000
*        RINUMCOD                   /* CODE FOR WHERE TO SAVE        */ 01991000
*            FIXED(8)BASED(RISAVNUM);                              0435 01992000
*   RINUMBER=0;                                                    0436 01993000
         SR    @15,@15                                             0436 01994000
         ST    @15,RINUMBER                                        0436 01995000
*   RIINTFND='0'B;                                                 0437 01996000
         NI    RIINTFND,B'01111111'                                0437 01997000
*   IF RIBYTLFT=0 THEN              /* AT END OF BUFFER?             */ 01998000
         L     @14,@PC00008+4                                      0438 01999000
         C     @15,RIBYTLFT(,@14)                                  0438 02000000
         BNE   @RF00438                                            0438 02001000
*     DO;                                                          0439 02002000
*       CALL MFNXTREC(RISCB);       /* GET A RECORD IF ONE EXISTS    */ 02003000
         ST    @14,@AL00001                                        0440 02004000
         LA    @01,@AL00001                                        0440 02005000
         BAL   @14,MFNXTREC                                        0440 02006000
*       RFY                                                        0441 02007000
*        (GPR15F)RSTD;                                             0441 02008000
*       IF GPR15F=4 THEN            /* MFNXTREC UNSUCCESSFUL?        */ 02009000
         CH    GPR15F,@CH00254                                     0442 02010000
         BNE   @RF00442                                            0442 02011000
*         RETURN CODE(GPR15F);      /* YES, RETURN TO IRBMFANL       */ 02012000
         L     @14,@SA00008                                        0443 02013000
         LM    @00,@12,@SA00008+8                                  0443 02014000
         BR    @14                                                 0443 02015000
*       RFY                                                        0444 02016000
*        (GPR15F)UNRSTD;                                           0444 02017000
@RF00442 DS    0H                                                  0445 02018000
*     END;                                                         0445 02019000
*   DO I=1 TO RIBYTLFT WHILE(RIINTGER>='F0'X&RIINTGER<='F9'X);     0446 02020000
@RF00438 LA    @15,1                                               0446 02021000
         B     @DE00446                                            0446 02022000
