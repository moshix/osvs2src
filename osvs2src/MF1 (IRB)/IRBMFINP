*   END;                                                           0358 01574000
@DE00306 TM    RBFINISH,B'10000000'                                0358 01575000
         BZ    @DL00306                                            0358 01576000
*   IF RBFOUND='0'B&RBTYPBIT='1'B THEN/* NO BLANKS/COMMENTS FOUND?   */ 01577000
         TM    RBFOUND,B'10000000'                                 0359 01578000
         BNZ   @RF00359                                            0359 01579000
         L     @15,@PC00006                                        0359 01580000
         L     @01,RBTYPE(,@15)                                    0359 01581000
         TM    RBTYPBIT(@01),B'10000000'                           0359 01582000
         BNO   @RF00359                                            0359 01583000
*     RETURN CODE(4);               /* RETURN UNSUCCESSFUL           */ 01584000
         LA    @15,4                                               0360 01585000
         L     @14,@SA00006                                        0360 01586000
         LM    @00,@12,@SA00006+8                                  0360 01587000
         BR    @14                                                 0360 01588000
*   RETURN CODE(0);                 /* RETURN SUCCESSFUL             */ 01589000
