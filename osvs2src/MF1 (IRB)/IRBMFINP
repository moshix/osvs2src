         L     @14,@SA00006                                        0361 01591000
         LM    @00,@12,@SA00006+8                                  0361 01592000
         BR    @14                                                 0361 01593000
*   END MFRBLANK;                                                  0362 01594000
*                                                                  0363 01595000
*/*  PROCEDURE = MFRRERR                                             */ 01596000
*/*                                                                  */ 01597000
*/*  DESCRIPTIVE NAME = ERROR RECOGNIZER                             */ 01598000
*/*                                                                  */ 01599000
*/*  FUNCTION = SKIPS TEXT UP TO BUT NOT INCLUDING A COMMA, COMMENT, */ 01600000
*/*             BLANK OR RIGHT PARENTHESIS.                          */ 01601000
*/*                                                                  */ 01602000
*                                                                  0363 01603000
*MFRRERR:                                                          0363 01604000
*   PROCEDURE(RRPARM,RRSCB);                                       0363 01605000
MFRRERR  STM   @14,@12,@SA00007                                    0363 01606000
         MVC   @PC00007(8),0(@01)                                  0363 01607000
*   DECLARE                                                        0364 01608000
*     RRPARM                        /* PTR TO A PARAMETER LIST       */ 01609000
*         PTR(31),                                                 0364 01610000
*     1 RRPARMLS                    /* PARAMETER LIST                */ 01611000
*           BASED(RRPARM),                                         0364 01612000
*      2 RRTYPE                     /* TYPE OF ERROR RECOGNIZER      */ 01613000
*            BIT(1),                                               0364 01614000
*      2 *                          /* NOT USED                      */ 01615000
*            BIT(7),                                               0364 01616000
*      2 RRLENGTH                   /* LENGTH OF ERROR TEXT TO BE  0364 01617000
*                                      SAVED FOR FUTURE REFERENCE    */ 01618000
*            FIXED(8),                                             0364 01619000
*      2 RRSAVETX                   /* CODE FOR WHERE TO SAVE        */ 01620000
*            FIXED(8),                                             0364 01621000
*     1 RRSCB,                      /* SCB PASSED FROM ANALYZER      */ 01622000
*      2 RRCURBCB                   /* CURRENT BCB PTR               */ 01623000
*            PTR(31),                                              0364 01624000
*      2 RRTXTCUR                   /* CURRENT TEXT PTR              */ 01625000
*            PTR(31),                                              0364 01626000
*      2 RRBYTLFT                   /* NO. BYTES LEFT IN BUFFER      */ 01627000
*            FIXED(31),                                            0364 01628000
*        RRTXTPTR                   /* PTR TO TEXT IN BUFFER         */ 01629000
*            PTR(31),                                              0364 01630000
*        RRCHAR                     /* CHARS. IN BUFFER              */ 01631000
*            CHAR(2)BASED(RRTXTPTR),                               0364 01632000
*        RRERRTXT                   /* BEGINNING OF ERROR TEXT       */ 01633000
*            CHAR(*)BASED(INERRPTR(RRSAVETX)),                     0364 01634000
*        RRFINISH                   /* FINISHED LOOPING INDICATOR    */ 01635000
*            BIT(1),                                               0364 01636000
