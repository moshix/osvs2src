         L     @07,RBCURTXT(,@14)                                  0331 01493000
         A     @07,RBBYTLFT(,@14)                                  0331 01494000
         CR    @15,@07                                             0331 01495000
         BL    @DL00325                                            0331 01496000
*           RBCURTXT=RBCURTXT+RBBYTLFT;/* UPDATE CURRENT TXT PTR     */ 01497000
         ST    @07,RBCURTXT(,@14)                                  0332 01498000
*           RBBYTLFT=0;             /* INDICATE NO BYTES LEFT        */ 01499000
         SR    @15,@15                                             0333 01500000
         ST    @15,RBBYTLFT(,@14)                                  0333 01501000
*           CALL MFNXTREC(RBSCB);   /* GET NEXT RECORD               */ 01502000
         ST    @14,@AL00001                                        0334 01503000
         LA    @01,@AL00001                                        0334 01504000
         BAL   @14,MFNXTREC                                        0334 01505000
*           RFY                                                    0335 01506000
*            (GPR15F)RSTD;                                         0335 01507000
*           IF GPR15F=0 THEN        /* DID A RECORD EXIST?           */ 01508000
         LTR   GPR15F,GPR15F                                       0336 01509000
         BNZ   @RF00336                                            0336 01510000
*             DO;                                                  0337 01511000
*               RBTXTPTR=RBCURTXT;  /* POINT TO BEGINNING OF BUFFER  */ 01512000
         L     @14,@PC00006+4                                      0338 01513000
         MVC   RBTXTPTR(4),RBCURTXT(@14)                           0338 01514000
*               GOTO RBCOMSCN;      /* CONTINUE SCAN FOR COMMENTS    */ 01515000
         B     RBCOMSCN                                            0339 01516000
*               RFY                                                0340 01517000
*                (GPR15F)UNRSTD;                                   0340 01518000
*             END;                                                 0341 01519000
*           RBFINISH='1'B;          /* NO MORE TEXT, SCAN IS FINISHED*/ 01520000
@RF00336 OI    RBFINISH,B'10000000'                                0342 01521000
*           GOTO RBENDLOP;          /* END THIS LOOP                 */ 01522000
         B     RBENDLOP                                            0343 01523000
*         END;                                                     0344 01524000
*RBCHKBLN:                                                         0345 01525000
*       ;                                                          0345 01526000
*                                                                  0345 01527000
@RF00321 DS    0H                                                  0345 01528000
RBCHKBLN DS    0H                                                  0346 01529000
*/* SCAN FOR BLANKS                                                  */ 01530000
*                                                                  0346 01531000
*       IF RBCHAR(1:1)^=' ' THEN    /* NONBLANK CHARACTER?           */ 01532000
         L     @01,RBTXTPTR                                        0346 01533000
         CLI   RBCHAR(@01),C' '                                    0346 01534000
         BE    @RF00346                                            0346 01535000
*         DO;                                                      0347 01536000
*           RBFINISH='1'B;          /* YES, BLANK SCAN IS FINISHED   */ 01537000
         OI    RBFINISH,B'10000000'                                0348 01538000
*           GOTO RBENDSCN;          /* UPDATE BCB AND SCB            */ 01539000
         B     RBENDSCN                                            0349 01540000
*         END;                                                     0350 01541000
*       RBFOUND='1'B;               /* BLANK FOUND                   */ 01542000
@RF00346 OI    RBFOUND,B'10000000'                                 0351 01543000
*RBLOOP:                                                           0352 01544000
*       ;                                                          0352 01545000
RBLOOP   DS    0H                                                  0353 01546000
*     END;                                                         0353 01547000
         LA    @15,1                                               0353 01548000
         A     @15,RBTXTPTR                                        0353 01549000
@DE00318 ST    @15,RBTXTPTR                                        0353 01550000
         L     @14,@PC00006+4                                      0353 01551000
         L     @07,@PC00006+4                                      0353 01552000
         L     @14,RBCURTXT(,@14)                                  0353 01553000
         A     @14,RBBYTLFT(,@07)                                  0353 01554000
         BCTR  @14,0                                               0353 01555000
         CR    @15,@14                                             0353 01556000
         BNH   @DL00318                                            0353 01557000
*RBENDSCN:                                                         0354 01558000
*     ;                                                            0354 01559000
RBENDSCN DS    0H                                                  0355 01560000
*     RBBYTLFT=RBBYTLFT-(RBTXTPTR-RBCURTXT);/* NO. BYTES LEFT        */ 01561000
         L     @15,@PC00006+4                                      0355 01562000
         L     @14,RBTXTPTR                                        0355 01563000
         LR    @07,@14                                             0355 01564000
         S     @07,RBCURTXT(,@15)                                  0355 01565000
         LCR   @07,@07                                             0355 01566000
         A     @07,RBBYTLFT(,@15)                                  0355 01567000
         ST    @07,RBBYTLFT(,@15)                                  0355 01568000
*     RBCURTXT=RBTXTPTR;            /* UPDATE CURRENT TEXT PTR       */ 01569000
         ST    @14,RBCURTXT(,@15)                                  0356 01570000
*RBENDLOP:                                                         0357 01571000
*     ;                                                            0357 01572000
