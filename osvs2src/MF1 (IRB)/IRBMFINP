*   SRRECSTR=ADDR(MFSYNTAB);        /* INITIAL STRUCTURE             */ 00804000
         LA    @15,MFSYNTAB                                        0175 00805000
         ST    @15,SRRECSTR                                        0175 00806000
*   SRIPARM=0;                      /* NO PARAMETERS FOR STRUCTURE   */ 00807000
