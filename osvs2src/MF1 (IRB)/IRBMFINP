*               DO;                                                0327 01478000
*                 RBTXTPTR=RBTXTPTR+RBCOMENT;/* UPDATE PTR IN BUFFER */ 01479000
         L     @14,RBCOMENT                                        0328 01480000
         AR    @15,@14                                             0328 01481000
