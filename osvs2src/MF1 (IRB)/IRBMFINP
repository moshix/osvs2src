         ST    @15,SRIPARM                                         0176 00809000
*   CALL IRBMFANL(SRDUMITM,INSCB,INAUTPTR);/* SYNTAX CHECK INPUT     */ 00810000
*                                                                  0177 00811000
         LA    @15,SRDUMITM                                        0177 00812000
         ST    @15,@AL00001                                        0177 00813000
         LA    @15,INSCB                                           0177 00814000
