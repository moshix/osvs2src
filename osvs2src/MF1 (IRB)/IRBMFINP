*           GO TO SRSTRSCN;         /* REPROCESS                     */ 01001000
         B     SRSTRSCN                                            0226 01002000
*         END;                                                     0227 01003000
*                                                                  0227 01004000
*       /*************************************************************/ 01005000
*       /*                                                           */ 01006000
*       /* SOME OPTIONS MAY HAVE BEEN ACCEPTED FROM BAD INPUT IN THIS*/ 01007000
*       /* SOURCE. LIST THESE. ISSUE MF/1 OPTIONS ASSUMED FROM SOURCE*/ 01008000
*       /* IN ERROR MESSAGE                                          */ 01009000
*       /*                                                           */ 01010000
*       /*************************************************************/ 01011000
*                                                                  0228 01012000
*       CALL INMSGMOD(MID309I1,0,0,0);                             0228 01013000
*                                                                  0228 01014000
