*     DO;                                                          1189 05115000
*       INCOOPIC=OPOPTSET;          /* OPTION SET                    */ 05116000
         NI    INCOOPIC,B'01111111'                                1190 05117000
*       INCOOPIV='1'B;              /* OPERATOR INTERVENTION         */ 05118000
         OI    INCOOPIV,B'00000001'                                1191 05119000
*       INCOXINV='1'B;                                             1192 05120000
