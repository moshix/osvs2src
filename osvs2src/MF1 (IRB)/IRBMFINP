         L     GPR01P,NXTEXPTR                                     0513 02314000
*       GEN(FREEMAIN R,LV=(0),A=(1));/* FREE GOTTEN AREA             */ 02315000
         FREEMAIN R,LV=(0),A=(1)                                        02316000
*       RFY                                                        0515 02317000
*        (GPR00P,                                                  0515 02318000
*         GPR01P,                                                  0515 02319000
*         GPR14P,                                                  0515 02320000
*         GPR15P)UNRSTD;                                           0515 02321000
*       IF NXFOUND='1'B THEN        /* RECORD RETRIEVED?             */ 02322000
         TM    NXFOUND,B'10000000'                                 0516 02323000
         BNO   @RF00516                                            0516 02324000
*         RETURN CODE(NXSUCCES);    /* RETURN SUCCESSFUL             */ 02325000
         SR    @15,@15                                             0517 02326000
         L     @14,@SA00010                                        0517 02327000
         LM    @00,@12,@SA00010+8                                  0517 02328000
