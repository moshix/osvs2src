         NI    INCOSTPC,B'01111111'                                1195 05127000
*   RETURN;                                                        1196 05128000
         B     @EL00014                                            1196 05129000
*OPSTOP1:                                                          1197 05130000
*   ;                               /* STOP H INIT.                  */ 05131000
OPSTOP1  DS    0H                                                  1198 05132000
*   IF INCOSTPC=OPOPTSET&(          /* IF PREVIOUSLY SPECIFIED AND   */ 05133000
*       (INCOSTPU=OPUNTHRS&INCOSTPV^=SYPRMV01)|/* NOT SAME # HRS OR  */ 05134000
*       (INCOSTPU^=OPUNTHRS&INCOSTPV^=SYPRMV01*60)|/* NOT EQUIV #  1198 05135000
*                                      MIN. OR                       */ 05136000
