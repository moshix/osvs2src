*/*  PROCEDURE NAME = MFFREBUF                                       */ 02100000
*/*                                                                  */ 02101000
*/*  DESCRIPTIVE NAME = FREE UP BUFFERS AND BCBS                     */ 02102000
*/*                                                                  */ 02103000
*/*  FUNCTION = THIS ROUTINE FREES UP BUFFERS AND BCBS NO LONGER     */ 02104000
*/*             NEEDED IN SCANNING AN INPUT SOURCE. A CODE IS PASSED */ 02105000
*/*              TO THIS ROUTINE TO INDICATE WHETHER ALL BUFFERS AND */ 02106000
*/*              BCBS ARE TO BE FREED OR FREE ONLY UP TO CURRENT ONE */ 02107000
*/*                                                                  */ 02108000
*                                                                  0467 02109000
*MFFREBUF:                                                         0467 02110000
*   PROCEDURE(FRFRECOD,FRSCB);                                     0467 02111000
MFFREBUF STM   @14,@12,@SA00009                                    0467 02112000
         MVC   @PC00009(8),0(@01)                                  0467 02113000
*   DECLARE                                                        0468 02114000
*     FRFRECOD                      /* FREE CODE - 0=FREE ALL BCB'S     02115000
*                                      AND BUFFERS. 4=FREE UP TO   0468 02116000
*                                      CURRENT BCB AND BUFFER        */ 02117000
*         FIXED(31),                                               0468 02118000
*     1 FRSCB,                      /* SCAN CONTROL BLOCK            */ 02119000
*      2 FRCURBCB                   /* CURRENT BCB PTR               */ 02120000
*            PTR(31),                                              0468 02121000
*      2 FRCURTXT                   /* CURRENT TEXT PTR              */ 02122000
*            PTR(31),                                              0468 02123000
*      2 FRBYTLFT                   /* # BYTES LEFT                  */ 02124000
*            FIXED(31),                                            0468 02125000
*        FRFREBCB                   /* BCB TO FREE                   */ 02126000
*            PTR(31),                                              0468 02127000
*     1 FRBCBCOR                    /* FREEMAIN PARAMETERS           */ 02128000
*           BDY(WORD),                                             0468 02129000
*      2 FRSUBPOL                   /* SUBPOOL                       */ 02130000
*            FIXED(8),                                             0468 02131000
*      2 FRBCBLEN                   /* LENGTH                        */ 02132000
*            FIXED(24),                                            0468 02133000
*        FRCONTRL                   /* CONTROL FOR DO GROUP          */ 02134000
*            PTR(31);                                              0468 02135000
*   FRSUBPOL=0;                     /* SUPOOL 0                      */ 02136000
         MVI   FRSUBPOL,X'00'                                      0469 02137000
*   IF FRFRECOD=0|INBCBFRS=0 THEN   /* FREE ALL BUFFERS AND BCBS OR     02138000
*                                      NONE TO FREE?                 */ 02139000
         SR    @15,@15                                             0470 02140000
         L     @14,@PC00009                                        0470 02141000
         C     @15,FRFRECOD(,@14)                                  0470 02142000
         BE    @RT00470                                            0470 02143000
         C     @15,INBCBFRS                                        0470 02144000
         BNE   @RF00470                                            0470 02145000
@RT00470 DS    0H                                                  0471 02146000
*     FRCONTRL=0;                                                  0471 02147000
         SR    @15,@15                                             0471 02148000
         ST    @15,FRCONTRL                                        0471 02149000
*   ELSE                            /* FREE UP TO CURRENT            */ 02150000
*     FRCONTRL=FRCURBCB;                                           0472 02151000
         B     @RC00470                                            0472 02152000
@RF00470 L     @15,@PC00009+4                                      0472 02153000
