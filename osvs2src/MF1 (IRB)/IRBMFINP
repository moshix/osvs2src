         LA    @01,@AL00001                                        0177 00819000
         BALR  @14,@15                                             0177 00820000
*   /*****************************************************************/ 00821000
*   /*                                                               */ 00822000
*   /* CHECK FOR INVALID VALUES, MUTUALLY EXCLUSIVE OPTIONS, OR      */ 00823000
*   /* SYNTAX ERRORS                                                 */ 00824000
*   /*                                                               */ 00825000
*   /*****************************************************************/ 00826000
*                                                                  0178 00827000
*   IF INCOXINV='1'B|INCOXSYN='1'B THEN                            0178 00828000
         TM    INCOXINV,B'00000110'                                0178 00829000
         BZ    @RF00178                                            0178 00830000
