*    SUBROUTINE TO HARDCOPY A COMMAND                          @ZA03236 05181040
DOHCPY   LH    REG15,D0(REG4)        LENGTH OF CIB             @ZA03236 05182040
         L     REG10,D0(REG4)        SAVE FIRST WORD OF CIB    @ZA03236 05183040
         AR    REG15,REG4            ONE PAST BUFFER END       @ZA03236 05184040
TRSCAN   EQU   *                     SCAN FOR LAST NON-BLANK   @ZA03236 05185040
         BCTR  REG15,D0              BACK UP ONE CHARACTER     @ZA03236 05186040
         CLI   D0(REG15),BLANK       NON-BLANK YET             @ZA03236 05187040
         BE    TRSCAN                NO, LOOK AT NEXT CHAR     @ZA03236 05188040
         SR    REG15,REG4            LENGTH MINUS ONE          @ZA03236 05189040
         LA    REG15,D1(REG15)       GET REAL LENGTH FOR WTO   @ZA03236 05190040
         STH   REG15,D0(REG4)        STORE IN CIB FOR WTO      @ZA03236 05191040
         LA    REG15,HCPYFLG         Q-TO-HARDCPY FLAG         @ZA03236 05192040
         STH   REG15,D2(REG4)        STORE IN CIB FOR WTO      @ZA03236 05193040
         LR    REG1,REG4             ADDR OF WPL FOR WTO       @ZA03236 05194040
         WTO   MF=(E,(1))            HARDCOPY THE COMMAND      @ZA03236 05195040
         ST    REG10,D0(REG4)        RESTORE FIRST WD OF CIB   @ZA03236 05196040
         BR    REG14                 RETURN TO MAINLINE        @ZA03236 05197040
         EJECT                                                          05198040
