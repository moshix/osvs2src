*       WPLMLLTC=ON;                /* MARK AS A DATA LINE   @ZA05477*/ 01141000
*       WPLMLLTD=ON;                /* MARK AS THE END LINE  @ZA05477*/ 01142000
         OI    WPLMLLTC(WPLPTR),B'00110000'                        0429 01143000
*       CSCBFLAG=ON;                /* INDICATE MSG IN CSCB          */ 01144000
         OI    CSCBFLAG,B'10000000'                                0430 01145000
*       CALL DQCSCB;                /* DEQUEUE OFF THE CSCB CHAIN    */ 01146000
         BAL   @14,DQCSCB                                          0431 01147000
*       CALL ISSUEMSG;              /* ISSUE THE MESSAGE             */ 01148000
         BAL   @14,ISSUEMSG                                        0432 01149000
*     END;                          /* END IF GETMAIN NOT SUCCESSFUL */ 01150000
*   ELSE                            /* GETMAIN WAS SUCCESSFUL        */ 01151000
*     DO;                                                          0434 01152000
         B     @RC00414                                            0434 01153000
@RF00414 DS    0H                                                  0435 01154000
*       RFY                                                        0435 01155000
*        (R2,                                                      0435 01156000
*         R3,                                                      0435 01157000
*         R6,                                                      0435 01158000
*         R7) RSTD;                                                0435 01159000
*       R2=BUFPTR;                  /* ADDR OF GOTTEN CORE   @ZA07770*/ 01160000
         L     R2,BUFPTR                                           0436 01161000
*       R3=COREBYTE;                /* LENGTH OF GOTTEN CORE @ZA07770*/ 01162000
         L     R3,COREBYTE                                         0437 01163000
*       R6=R6&&R6;                  /* ZERO SOURCE ADDR      @ZA07770*/ 01164000
         XR    R6,R6                                               0438 01165000
