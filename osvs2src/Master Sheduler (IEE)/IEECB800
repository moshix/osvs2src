*                                                                  0514 01456000
*/********************************************************************* 01457000
**                J O B L I N E   P R O C E D U R E                   * 01458000
** ENTRY REASON - OPTIONS 'JOBS','JOBS,LIST','A' OR 'A,LIST' WANTED   * 01459000
** ENTRY FROM   - DISPLAY PROCEDURE                                   * 01460000
** EXIT TO      - DISPLAY PROCEDURE                                   * 01461000
** FUNCTION     - 1) BUILD THE JOB-INIT LINE                          * 01462000
**          B ------------------------------------------------------  * 01463000
**  WPLML    | MESSAGE LENGTH + 4 =     | MCS FLAGS =              |  * 01464000
**            ------------------------------------------------------  * 01465000
**           |   '00000 JOBS    00000 INITIATORS'                  |  * 01466000
**          C ------------------------------------------------------  * 01467000
**                2) THE JOB AND INIT COUNTS ARE CONVERTED AND PUT    * 01468000
**                   INTO THE DISPLAY LINE                            * 01469000
**                3) RETURN                                           * 01470000
**********************************************************************/ 01471000
*                                                                  0514 01472000
*   RFY                                                            0514 01473000
*    (R6,                                                          0514 01474000
*     R7,                                                          0514 01475000
*     R14) RSTD;                                                   0514 01476000
*JOBLINE:                                                          0515 01477000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0515 01478000
JOBLINE  DS    0H                                                  0516 01479000
*   JOBLSAVE=R14;                   /* SAVE RETURN ADDR TO DISPLAY 0516 01480000
*                                      PROC                          */ 01481000
         ST    R14,JOBLSAVE                                        0516 01482000
*   RFY                                                            0517 01483000
*     R14 UNRSTD;                                                  0517 01484000
*   TRCF5JBI=ON;                    /* MARK JOBLINE PROC AS IN     0518 01485000
*                                      PROCESSING                    */ 01486000
*   TRCF5JBE=ON;                    /* MARK JOBLINE PROC AS ENTERED  */ 01487000
         LA    @10,TRCURRSW                                        0519 01488000
         OI    TRCF5JBI(@10),B'00000011'                           0519 01489000
*   WPLML(ONE:JOBLGTH)=JOBTEXT(ONE:JOBLGTH);/* GET JOBS SUB MSG      */ 01490000
         SLR   @10,@10                                             0520 01491000
         IC    @10,JOBLGTH                                         0520 01492000
         BCTR  @10,0                                               0520 01493000
         EX    @10,@SM03168                                        0520 01494000
*   WPLSAVE=WPLPTR;                 /* SAVE ADDR OF LINE JUST FILLED */ 01495000
         ST    WPLPTR,WPLSAVE                                      0521 01496000
*   WPLMLLTC=ON;                    /* DATA LINE                     */ 01497000
         OI    WPLMLLTC(WPLPTR),B'00100000'                        0522 01498000
*   IF JOBNUM^=ZERO THEN            /* ANY JOBS CSCB'S?              */ 01499000
         LH    @10,JOBNUM                                          0523 01500000
         LTR   @10,@10                                             0523 01501000
         BZ    @RF00523                                            0523 01502000
*     DO;                           /* YES                           */ 01503000
*       R7=JOBNUM;                  /* SAVE # OF ACTIVE JOBS & TASKS */ 01504000
         LR    R7,@10                                              0525 01505000
*       R6=WPLPTR+FOUR;             /* JOBS SUB-HEADER ADDRESS       */ 01506000
         LA    R6,4                                                0526 01507000
         ALR   R6,WPLPTR                                           0526 01508000
*       CALL CVDCOUNT;              /* CONVERT NUMBER TO DECIMAL     */ 01509000
         BAL   @14,CVDCOUNT                                        0527 01510000
*     END;                                                         0528 01511000
*   IF INITNUM^=ZERO THEN           /* ANY INIT CSCB'S       @ZA07770*/ 01512000
@RF00523 LH    @10,INITNUM                                         0529 01513000
         LTR   @10,@10                                             0529 01514000
         BZ    @RF00529                                            0529 01515000
*     DO;                           /* YES                   @ZA07770*/ 01516000
*       R7=INITNUM;                 /* # OF ACTIVE INITS     @ZA07770*/ 01517000
         LR    R7,@10                                              0531 01518000
*       R6=WPLPTR+EIGHTEEN;         /* WHERE TO PUT CONVERTD NUMBER     01519000
*                                                            @ZA07770*/ 01520000
         LA    R6,18                                               0532 01521000
         ALR   R6,WPLPTR                                           0532 01522000
*       CALL CVDCOUNT;              /* CONVERT NUMBER TO DEC @ZA07770*/ 01523000
         BAL   @14,CVDCOUNT                                        0533 01524000
*     END;                          /*                       @ZA07770*/ 01525000
*   COUNT=COUNT+ONE;                /* INCREMENT LINE COUNT          */ 01526000
@RF00529 IC    @10,COUNT                                           0535 01527000
         LA    @10,1(,@10)                                         0535 01528000
         STC   @10,COUNT                                           0535 01529000
*   WPLPTR=WPLPTR+WPLMLLEN;         /* POINT TO NEXT LINE IN MLWTO   */ 01530000
         SLR   @10,@10                                             0536 01531000
         IC    @10,WPLMLLEN(,WPLPTR)                               0536 01532000
         ALR   WPLPTR,@10                                          0536 01533000
*   TRCF5JBI=OFF;                   /* MARK JOBLINE PROC AS CONTROL     01534000
*                                      LEAVING                       */ 01535000
         LA    @10,TRCURRSW                                        0537 01536000
         NI    TRCF5JBI(@10),B'11111101'                           0537 01537000
*   RFY                                                            0538 01538000
*     R14 RSTD;                                                    0538 01539000
*   R14=JOBLSAVE;                   /* RESTORE RETURN ADDR TO DISPLAY   01540000
*                                      PROC                          */ 01541000
         L     R14,JOBLSAVE                                        0539 01542000
*   RFY                                                            0540 01543000
*    (R6,                                                          0540 01544000
*     R7,                                                          0540 01545000
*     R14) UNRSTD;                                                 0540 01546000
*   END JOBLINE;                                                   0541 01547000
@EL00010 DS    0H                                                  0541 01548000
@EF00010 DS    0H                                                  0541 01549000
@ER00010 BR    @14                                                 0541 01550000
*                                                                  0542 01551000
*/********************************************************************* 01552000
**               L I S T J O B S   P R O C E D U R E                  * 01553000
** ENTRY REASON - OPTIONS 'JOBS,LIST' OR 'A,LIST' WAS WANTED          * 01554000
** ENTRY FROM   - DISPLAY PROCEDURE                                   * 01555000
** EXIT TO      - DISPLAY PROCEDURE                                   * 01556000
** FUNCTION     - 1) SCAN CSCB CHAIN FOR JOB CSCB                     * 01557000
**                2) WHEN ONE IS FOUND THEN                           * 01558000
**                   A) MOVE IN GENERAL LINE FORMAT                   * 01559000
**                   B) CALL JOBLFILL TO FILL IN THE SPECIFIC NAMES   * 01560000
**                3) FIND NEXT WPL ADDRESS                            * 01561000
** NOTE         - THE LINES OF OUTPUT LOOK DIFFERENT DEPENDING IF THE * 01562000
**                LINE IS THE 1ST LINE OF A CONNECTING MLWTO OR NOT.  * 01563000
**                CONNEXT  IS TESTED TO SEE WHICH LINE IS TO BE ISSUED* 01564000
**    WHEN 'CONNEXT ' IS OFF:                                         * 01565000
**          B ------------------------------------------------------  * 01566000
**  WPLML    | MESSAGE LENGTH + 4 =     | MCS FLAGS =              |  * 01567000
**            ------------------------------------------------------  * 01568000
**           |   'STARTING STARTING    V=R      00000K - 00000K'   |  * 01569000
**           |                         OR                          |  * 01570000
**           |   'STARTING STARTING    V=V      S'                 |  * 01571000
**          C ------------------------------------------------------  * 01572000
**    WHEN 'CONNEXT ' IS ON:                                          * 01573000
**         A' ------------------------------------------------------  * 01574000
**  WPL      | MESSAGE LENGTH + 4 =     | MCS FLAGS =              |  * 01575000
**            ------------------------------------------------------  * 01576000
**           |   'STARTING STARTING    V=R      00000K - 00000K'   |  * 01577000
**           |                         OR                          |  * 01578000
**           |   'STARTING STARTING    V=V      S'                 |  * 01579000
**            ------------------------------------------------------  * 01580000
**  WPLLS01  | LINE TYPE =   | AREA ID =   | NUMBER OF LINES =     |  * 01581000
**         B' ------------------------------------------------------  * 01582000
**********************************************************************/ 01583000
*                                                                  0542 01584000
*LISTJOBS:                                                         0542 01585000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0542 01586000
LISTJOBS DS    0H                                                  0543 01587000
*   RFY                                                            0543 01588000
*     R14 RSTD;                                                    0543 01589000
*   LISTJSAV=R14;                   /* SAVE RETURN ADDR TO DISPLAY 0544 01590000
*                                      PROC                          */ 01591000
         ST    R14,LISTJSAV                                        0544 01592000
*   RFY                                                            0545 01593000
*     R14 UNRSTD;                                                  0545 01594000
*   TRCF6LJI=ON;                    /* MARK LISTJOBS PROC AS IN    0546 01595000
*                                      PROCESSING                    */ 01596000
*   TRCF6LJE=ON;                    /* MARK LISTJOBS PROC AS ENTERED */ 01597000
         LA    @10,TRCURRSW                                        0547 01598000
         OI    TRCF6LJI(@10),B'00000011'                           0547 01599000
*   CSCBPTR=BACHN;                                                 0548 01600000
         L     @10,CVTPTR                                          0548 01601000
         L     @10,CVTMSER(,@10)                                   0548 01602000
         L     CSCBPTR,BACHN(,@10)                                 0548 01603000
*   DO WHILE CSCBPTR^=ZERO;                                        0549 01604000
         B     @DE00549                                            0549 01605000
@DL00549 DS    0H                                                  0550 01606000
*     IF CHTRKID='02'X THEN         /* IS THIS A JOB OR TASK CSCB?   */ 01607000
         CLI   CHTRKID(CSCBPTR),X'02'                              0550 01608000
         BNE   @RF00550                                            0550 01609000
*       DO;                                                        0551 01610000
*         IF CHRGNAD=ZERO|CHCLS=ZEROES THEN/*                @YM7265P*/ 01611000
         L     @10,CHRGNAD(,CSCBPTR)                               0552 01612000
         LTR   @10,@10                                             0552 01613000
         BZ    @RT00552                                            0552 01614000
         CLC   CHCLS(8,CSCBPTR),ZEROES                             0552 01615000
         BNE   @RF00552                                            0552 01616000
@RT00552 DS    0H                                                  0553 01617000
*           WPL(ONE:JOBLGTHV)=JOBLISTV(ONE:JOBLGTHV);/* LENGTH JOB,L    01618000
*                                      V=V WTO               @YM7265P*/ 01619000
         SLR   @10,@10                                             0553 01620000
         IC    @10,JOBLGTHV                                        0553 01621000
         BCTR  @10,0                                               0553 01622000
         EX    @10,@SM03170                                        0553 01623000
*         ELSE                      /*                       @YM7265P*/ 01624000
*           WPL(ONE:JOBLLGTH)=JOBLIST(ONE:JOBLLGTH);/*       @YM7265P   01625000
*                                      LENGTH JOB,L V=R WTO  @YM7265P*/ 01626000
         B     @RC00552                                            0554 01627000
