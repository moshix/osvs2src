         TITLE 'DISPLAY / TRACK COMMON PROCESSOR - BASE                *00001000
                        '                                               00002000
IEECB800 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEECB800  78.053'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @11,0                                               0001 00010000
@PSTART  LA    @04,4095(,@11)                                      0001 00011000
         USING @PSTART,@11                                         0001 00012000
         USING @PSTART+4095,@04                                    0001 00013000
         LR    @10,@01                                             0001 00014000
         L     @00,@SIZDATD                                        0001 00015000
         GETMAIN  R,LV=(0)                                              00016000
         LR    @12,@01                                             0001 00017000
         USING @DATD,@12                                           0001 00018000
         LR    @01,@10                                             0001 00019000
         ST    @13,@SA00001+4                                      0001 00020000
         LR    @13,@12                                             0001 00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
*   R14SAVE=R14;                    /* SAVE THE RETURN ADDRESS       */ 00023000
         ST    R14,R14SAVE                                         0181 00024000
*   TRACESWS=TRACESWS&&TRACESWS;    /* ZERO THE TRACE SWITCHES       */ 00025000
         XC    TRACESWS(32),TRACESWS                               0182 00026000
*   TRPF1TOP=ON;                    /* MARK FLAG FOR TOP PROC AS   0183 00027000
*                                      ENTERED                       */ 00028000
         LA    @15,TRCURRSW                                        0183 00029000
         OI    TRPF1TOP(@15),B'10000000'                           0183 00030000
*   TRTITLE=TRTITLES;               /* MOVE TITLE 'TRCE' IN CORE   0184 00031000
*                                      BEFORE SWITCHES               */ 00032000
         MVC   TRTITLE(4),@CC02938                                 0184 00033000
*   ABENDSW=OFF;                    /* ABEND NOT WANTED YET          */ 00034000
         NI    ABENDSW,B'11111101'                                 0185 00035000
*   CALL SETESTAE;                                                 0186 00036000
         BAL   @14,SETESTAE                                        0186 00037000
*   IF CHVCD=TRCMD THEN             /* IS THIS A TRACK COMMAND       */ 00038000
         CLI   CHVCD(CSCBPTR),X'C4'                                0187 00039000
         BNE   @RF00187                                            0187 00040000
*     DO;                           /* YES IT IS A TRACK COMMAND     */ 00041000
*                                                                  0188 00042000
*       /*************************************************************/ 00043000
*       /*                                                           */ 00044000
*       /* SET UP LOWER LEVEL TRACK ESTAE. IF ESTAE FAILS THEN       */ 00045000
*       /* CONTROL IS NOT RETURNED HERE BUT TO STAEFAIL WHICH THEN   */ 00046000
*       /* RETURNS TO THE TOP PROC AT RETSYS. ABENDSW WILL BE ON SO  */ 00047000
*       /* ABEND 102 IS ISSUED.                                      */ 00048000
*       /*                                                           */ 00049000
*       /*************************************************************/ 00050000
*                                                                  0189 00051000
*       CALL TRKESTAE;                                             0189 00052000
*                                                                  0189 00053000
         BAL   @14,TRKESTAE                                        0189 00054000
*       /*************************************************************/ 00055000
*       /*                                                           */ 00056000
*       /* DO TRACK PROCESSING. CONTROL IS PASSED HERE WHEN IT IS    */ 00057000
*       /* TIME FOR THE NEXT TRACK UPDATE OR THE TRACKING IS TO BE   */ 00058000
*       /* ENDED. IF THE DISPLAY IS TO STOP THEN CONTROL IS GIVEN TO */ 00059000
*       /* LABEL ALLDONE IN THIS PROCEDURE. IF THE DISPLAY IS NOT    */ 00060000
*       /* WANTED OR THE PREVIOUS DISPLAY IS NOT FINISHED, THEN      */ 00061000
*       /* CONTROL IS GIVEN TO THE STATEMENT AFTER THE CALL.         */ 00062000
*       /* PROCEDURE TRACK DETERMINES WHERE TO RETURN TO.            */ 00063000
*       /*                                                           */ 00064000
*       /*************************************************************/ 00065000
*                                                                  0190 00066000
*AWAKE:                                                            0190 00067000
*       CALL TRACK;                                                0190 00068000
*                                                                  0190 00069000
AWAKE    BAL   @14,TRACK                                           0190 00070000
*       /*************************************************************/ 00071000
*       /*                                                           */ 00072000
*       /* ISSUE THE STIMER AND THEN DETERMINE IF THE DISPLAY SHOULD */ 00073000
*       /* BE UPDATED.                                               */ 00074000
*       /*                                                           */ 00075000
*       /*************************************************************/ 00076000
*                                                                  0191 00077000
*       R1=ADDR(CHPEND);            /* ADDR OF TIME INTERVAL         */ 00078000
         LA    R1,CHPEND(,CSCBPTR)                                 0191 00079000
*       GEN REFS(TIMEPOP) FLOWS(ALLDONE) SETS(R0,R14,R15);         0192 00080000
*                                                                  0192 00081000
         STIMER REAL,TIMEPOP,BINTVL=(1),ERRET=ALLDONE                   00082000
*/********************************************************************/ 00083000
*/*  IS AN UPDATE WANTED AT THIS TIME ?                              */ 00084000
*/********************************************************************/ 00085000
*                                                                  0193 00086000
*       IF TRKNOUP=ON THEN          /* ANY REASON NOT TO ISSUE     0193 00087000
*                                      DISPLAY ?                     */ 00088000
         TM    TRKNOUP,B'00000100'                                 0193 00089000
         BNO   @RF00193                                            0193 00090000
*         CALL WAIT;                /* YES, DISPLAY SHOULD NOT BE  0194 00091000
*                                      UPDATED. CONTROL IS NOT     0194 00092000
*                                      RETURNED HERE BUT TO LABEL  0194 00093000
*                                      AWAKE IN THE TOP PROCEDURE    */ 00094000
         BAL   @14,WAIT                                            0194 00095000
*       FIRSTRAK=OFF;               /* 1ST TRACK HAS BEEN PROCESSED  */ 00096000
@RF00193 NI    FIRSTRAK,B'01111111'                                0195 00097000
*       CALL DISPLAY;               /* DO THE DISPLAY FOR THE TRACK     00098000
*                                      UPDATE                        */ 00099000
         BAL   @14,DISPLAY                                         0196 00100000
*       CALL WAIT;                  /* ISSUE WAIT. CONTROL IS NEVER     00101000
*                                      RETURNED HERE BUT TO LABEL  0197 00102000
*                                      AWAKE.                        */ 00103000
         BAL   @14,WAIT                                            0197 00104000
*     END;                                                         0198 00105000
*   ELSE                            /* DISPLAY COMMAND               */ 00106000
*     CALL DISPLAY;                                                0199 00107000
*                                                                  0199 00108000
         B     @RC00187                                            0199 00109000
@RF00187 BAL   @14,DISPLAY                                         0199 00110000
*   /*****************************************************************/ 00111000
*   /*                                                               */ 00112000
*   /* THIS CODE IS USED BY BOTH TRACK AND DISPLAY PROCESSING AS A   */ 00113000
*   /* FINAL CLEANUP ROUTINE. CONTROL MAY BE RETURNED TO ALLDONE FROM*/ 00114000
*   /* PROCEDURE TRACK. THE CSCB IS FREED VIA MGCR                   */ 00115000
*   /*                                                               */ 00116000
*   /*****************************************************************/ 00117000
*                                                                  0200 00118000
*ALLDONE:                                                          0200 00119000
*   TRPF1ALD=ON;                    /* MARK FLAG FOR LABEL ALLDONE AS   00120000
*                                      ENTERED                       */ 00121000
@RC00187 DS    0H                                                  0200 00122000
ALLDONE  LA    @15,TRCURRSW                                        0200 00123000
         OI    TRPF1ALD(@15),B'00010000'                           0200 00124000
*   R1=CSCBPTR;                     /* RESTORE CSCB ADDRESS          */ 00125000
         LR    R1,CSCBPTR                                          0201 00126000
*   CHFC=ON;                        /* MARK TO FREE CSCB CORE        */ 00127000
         OI    CHFC(CSCBPTR),B'00000010'                           0202 00128000
*   GEN SETS(R0,R1)(MGCR (1),CHAIN);/* CSCB FREED BY SVC 34          */ 00129000
*                                                                  0203 00130000
         MGCR (1),CHAIN                                                 00131000
*   /*****************************************************************/ 00132000
*   /*                                                               */ 00133000
*   /* CONTROL IS GIVEN HERE BY COMING FROM ALLDONE OR BY RETURNING  */ 00134000
*   /* FROM STAEFAIL. IF AN ABEND IS WANTED, THEN ABENDSW=ON. I.E.   */ 00135000
*   /* BECAUSE THE LOWER LEVEL TRACE ESTAE FAILED.                   */ 00136000
*   /*                                                               */ 00137000
*   /*****************************************************************/ 00138000
*                                                                  0204 00139000
*   RFY                                                            0204 00140000
*     R15 RSTD;                     /* RESTRICT RETURN CODE REGISTER */ 00141000
*RETSYS:                                                           0205 00142000
*   TRPF1RET=ON;                    /* MARK FLAG FOR LABEL RETSYS AS    00143000
*                                      ENTERED                       */ 00144000
RETSYS   LA    @10,TRCURRSW                                        0205 00145000
         OI    TRPF1RET(@10),B'00001000'                           0205 00146000
*   R14=R14SAVE;                    /* RESTORE THE RETURN ADDR       */ 00147000
         L     R14,R14SAVE                                         0206 00148000
*   IF ABENDSW=ON THEN              /* IS AN ABEND WANTED?           */ 00149000
         TM    ABENDSW,B'00000010'                                 0207 00150000
         BNO   @RF00207                                            0207 00151000
*     DO;                           /* YES                           */ 00152000
*       TRPF2ABN=ON;                /* MARK FLAG FOR LABEL ABEND AS     00153000
*                                      ENTERED                       */ 00154000
         OI    TRPF2ABN(@10),B'10000000'                           0209 00155000
*       RETURN TO ABEND;            /* YES, FREE CORE, ABEND         */ 00156000
         L     @13,4(,@13)                                         0210 00157000
         L     @00,@SIZDATD                                        0210 00158000
         LR    @01,@12                                             0210 00159000
         FREEMAIN R,LV=(0),A=(1)                                        00160000
         LA    @14,ABEND                                           0210 00161000
         BR    @14                                                 0210 00162000
*     END;                                                         0211 00163000
*   ELSE                            /* NORMAL RETURN                 */ 00164000
*     RETURN;                       /* RETURN TO SYSTEM              */ 00165000
*ABEND:                             /* TRACK ESTAE FAILED            */ 00166000
*   GEN SETS(R1) EXIT NOSEQFLOW(ABEND 102);/* USER ABEND 102         */ 00167000
ABEND    ABEND 102                                                      00168000
*   RFY                                                            0214 00169000
*    (R1,                                                          0214 00170000
*     R14,                                                         0214 00171000
*     R15) UNRSTD;                                                 0214 00172000
*                                                                  0215 00173000
*   /*****************************************************************/ 00174000
*   /*                                                               */ 00175000
*   /* END OF THE TOP PROCEDURE                                      */ 00176000
*   /*                                                               */ 00177000
*   /*****************************************************************/ 00178000
*                                                                  0215 00179000
*                                                                  0215 00180000
*/********************************************************************* 00181000
**               S E T E S T A E   P R O C E D U R E                  * 00182000
** ENTRY REASON - SET UP ESTAE ENVIRONMENT FOR DISPLAY AND TRACK CMDS * 00183000
** ENTRY FROM   - TOP PROCEDURE                                       * 00184000
** EXIT TO      - 1) PROCEDURE STAEFAIL (IF TRACK COMMAND AND THE     * 00185000
**                   ESTAE FAILED)                                    * 00186000
**                2) LABEL ALLDONE IN TOP PROCEDURE (IF DISPLAY CMD   * 00187000
**                   AND ESTAE FAILED)                                * 00188000
**                3) NORMAL RETURN TO TOP PROCEDURE (IF ESTAE WORKED) * 00189000
** FUNCTION     - 1) ESTABLISH CSCB ADDRESSIBILITY.                   * 00190000
**                2) HAVE AN ESTAE ENVIRONMENT CREATED BY IEECB860.   * 00191000
**                   A) PASS THE DATA AND BASE REGISTERS              * 00192000
**                   B) PASS THE CSCB ADDRESS AND RETURN ADDRESS      * 00193000
**                   C) PASS THE LENGTH AND NAME OF COMMAND BEING     * 00194000
**                      PROCESSED                                     * 00195000
**                   D) LOAD THE ESTAE CREATION MODULE - IEECB860     * 00196000
**                   E) PASS CONTROL TO IEECB860 TO INVOKE ESTAE      * 00197000
**                      CREATION                                      * 00198000
**                3) RESTORE THE BASE REGISTERS AND CSCB POINTER      * 00199000
**                4) SEE IF ESTAE CREATION SUCCESSFUL                 * 00200000
**                   A) ESTAE SETUP FAILED                            * 00201000
**                      1) DELETE IEECB860                            * 00202000
**                      2) FOR A TRACK COMMAND                        * 00203000
**                         A) INITIALIZE FOR STAEFAIL PROCEDURE       * 00204000
**                         B) CALL STAEFAIL WHO RETURNS TO THE TOP    * 00205000
**                            PROCEDURE AND THEN TO THE SYSTEM        * 00206000
**                      3) FOR A DISPLAY COMMAND                      * 00207000
**                         A) RETURN TO LABEL ALLDONE IN THE TOP PROC * 00208000
**                   B) ESTAE SETUP WORKED                            * 00209000
**                      1) ZERO PARM LIST                             * 00210000
**                      2) DELETE IEECB860                            * 00211000
**                      3) RETURN TO THE TOP PROC                     * 00212000
** NOTE         - CONTROL IS NEVER RETURNED HERE IF PROCEDURE STAEFAIL* 00213000
**                IS CALLED                                           * 00214000
**********************************************************************/ 00215000
*                                                                  0215 00216000
*SETESTAE:                                                         0215 00217000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0215 00218000
@EL00001 L     @13,4(,@13)                                         0215 00219000
@EF00001 L     @00,@SIZDATD                                        0215 00220000
         LR    @01,@12                                             0215 00221000
         FREEMAIN R,LV=(0),A=(1)                                        00222000
@ER00001 BR    @14                                                 0215 00223000
SETESTAE DS    0H                                                  0216 00224000
*   RFY                                                            0216 00225000
*    (R1,                                                          0216 00226000
*     R4,                                                          0216 00227000
*     R11,                                                         0216 00228000
*     R12,                                                         0216 00229000
*     R14) RSTD;                                                   0216 00230000
*   SETESAVE=R14;                   /* SAVE RETURN TO TOP PROC ADDR  */ 00231000
         ST    R14,SETESAVE                                        0217 00232000
*   TRPF1SET=ON;                    /* MARK FLAG FOR SETESTAE PROC AS   00233000
*                                      ENTERED                       */ 00234000
         LA    @15,TRCURRSW                                        0218 00235000
         OI    TRPF1SET(@15),B'01000000'                           0218 00236000
*   CSCBPTR=R1;                     /* ESTABLISH CSCB ADDRESSABILITY */ 00237000
         LR    CSCBPTR,R1                                          0219 00238000
*   REGDATA=R12;                    /* SAVE DATA REG                 */ 00239000
         LA    @15,STAELIST                                        0220 00240000
         ST    R12,REGDATA(,@15)                                   0220 00241000
*   REGCODE1=R4;                    /* SAVE BASE REG         @ZA00073*/ 00242000
         ST    R4,REGCODE1(,@15)                                   0221 00243000
*   REGCODE2=R11;                   /* SAVE BASE REG         @ZA00073*/ 00244000
         ST    R11,REGCODE2(,@15)                                  0222 00245000
*   CSCBPASS=CSCBPTR;               /* PUT CSCB ADDRESS IN PARM LIST */ 00246000
         ST    CSCBPTR,CSCBPASS(,@15)                              0223 00247000
*   RFY                                                            0224 00248000
*    (R11,                                                         0224 00249000
*     R12) UNRSTD;                                                 0224 00250000
*   IF CHVCD=TRCMD THEN             /* IS THIS FOR A TRACK COMMAND?  */ 00251000
         CLI   CHVCD(CSCBPTR),X'C4'                                0225 00252000
         BNE   @RF00225                                            0225 00253000
*     COMMAND=ADDR(TCOMM);          /* YES, ADDR AND LENGTH OF TRACK */ 00254000
         LA    @10,TCOMM                                           0226 00255000
         ST    @10,COMMAND(,@15)                                   0226 00256000
*   ELSE                                                           0227 00257000
*     COMMAND=ADDR(DCOMM);          /* NO, ADDR AND LENGTH OF DISPLAY*/ 00258000
         B     @RC00225                                            0227 00259000
@RF00225 LA    @15,STAELIST                                        0227 00260000
         LA    @10,DCOMM                                           0227 00261000
         ST    @10,COMMAND(,@15)                                   0227 00262000
*   GEN SETS(R0,R1)(LOAD EP=IEECB860);/* LOAD ESTAE CREATION MOD     */ 00263000
@RC00225 DS    0H                                                  0228 00264000
         LOAD EP=IEECB860                                               00265000
*   RESPECIFY                                                      0229 00266000
*    (R0,                                                          0229 00267000
*     R15) RESTRICTED;                                             0229 00268000
*   R15=R0;                         /* PUT ESTAE ENTRY ADDRESS IN R15*/ 00269000
         LR    R15,R0                                              0230 00270000
*   R1=ADDR(STAELIST);              /* GET ADDRESS OF STAELIST       */ 00271000
         LA    R1,STAELIST                                         0231 00272000
*   RESPECIFY                                                      0232 00273000
*     R0 UNRESTRICTED;                                             0232 00274000
*                                                                  0232 00275000
*   /*****************************************************************/ 00276000
*   /*                                                               */ 00277000
*   /* 2) SET UP ESTAE ENVIRONMENT                                   */ 00278000
*   /*                                                               */ 00279000
*   /*****************************************************************/ 00280000
*                                                                  0233 00281000
*   GEN SETS(R14) REFS(R15)(BALR R14,R15);/* ESTABLISH ESTAE       0233 00282000
*                                      ENVIRONMENT                   */ 00283000
         BALR R14,R15                                                   00284000
*   RFY                                                            0234 00285000
*    (R11,                                                         0234 00286000
*     R12) RSTD;                                                   0234 00287000
*   R12=R1->REGDATA;                /* RESTORE DATA REG              */ 00288000
         L     R12,REGDATA(,R1)                                    0235 00289000
*   R4=REGCODE1;                    /* RESTORE SECONDARY BASE REG  0236 00290000
*                                                            @ZA00073*/ 00291000
         LA    @10,STAELIST                                        0236 00292000
         L     R4,REGCODE1(,@10)                                   0236 00293000
*   R11=REGCODE2;                   /* RESTORE BASE REG      @ZA00073*/ 00294000
         L     R11,REGCODE2(,@10)                                  0237 00295000
*   CSCBPTR=CSCBPASS;               /* RESTORE CSCB BASE REG         */ 00296000
*                                                                  0238 00297000
         L     CSCBPTR,CSCBPASS(,@10)                              0238 00298000
*   /*****************************************************************/ 00299000
*   /*                                                               */ 00300000
*   /* 5) ESTAE ENVIRONMENT SET UP OK ?                              */ 00301000
*   /*                                                               */ 00302000
*   /*****************************************************************/ 00303000
*                                                                  0239 00304000
*   IF R15^=ZERO THEN               /* BAD RETURN CODE FROM ESTAE?   */ 00305000
         LTR   R15,R15                                             0239 00306000
         BZ    @RF00239                                            0239 00307000
*     DO;                           /* YES, REQUEST SVC34 TO FREE  0240 00308000
*                                      CSCB, RETURN                  */ 00309000
*       RESPECIFY                                                  0241 00310000
*        (R15) UNRESTRICTED;                                       0241 00311000
*       GEN SETS(R0)(DELETE EP=IEECB860);                          0242 00312000
         DELETE EP=IEECB860                                             00313000
*       IF CHVCD=TRCMD THEN         /* IS THIS A TRACK CMD?  @YM4795P*/ 00314000
         CLI   CHVCD(CSCBPTR),X'C4'                                0243 00315000
         BNE   @RF00243                                            0243 00316000
*         DO;                       /* YES,ZERO OUT REG 7 AND      0244 00317000
*                                                            @YM4795P*/ 00318000
*           HAVESDWA=OFF;           /* FOR STEAFAIL ROUTINE  @YM4795P*/ 00319000
         NI    HAVESDWA,B'11101111'                                0245 00320000
*           CALL STAEFAIL;          /* FOR CLEAN EXIT. CONTROL IS  0246 00321000
*                                      NEVER RETURNED HERE BUT TO THE   00322000
*                                      TOP PROCEDURE WHICH THEN    0246 00323000
*                                      RETURNS CONTROL TO THE SYSTEM */ 00324000
         BAL   @14,STAEFAIL                                        0246 00325000
*         END;                      /*                       @YM4795P*/ 00326000
*       ELSE                        /*                       @YM4795P*/ 00327000
*         RETURN TO ALLDONE;        /* RETURN TOP PROC               */ 00328000
         B     @RC00243                                            0248 00329000
@RF00243 LA    @14,ALLDONE                                         0248 00330000
@EL00002 DS    0H                                                  0248 00331000
@EF00002 DS    0H                                                  0248 00332000
@ER00002 BR    @14                                                 0248 00333000
*     END;                                                         0249 00334000
@RC00243 DS    0H                                                  0250 00335000
*   PARMLIST=PARMLIST&&PARMLIST;    /* INITIALIZE IEECB801   @ZA00055   00336000
*                                      PARAMETER LIST        @ZA00055*/ 00337000
@RF00239 XC    PARMLIST(17),PARMLIST                               0250 00338000
*   CSCBADDR=CSCBPTR;               /* SAVE CSCB ADDR IN PARMLIST    */ 00339000
         ST    CSCBPTR,CSCBADDR                                    0251 00340000
*   GEN SETS(R0)(DELETE EP=IEECB860);                              0252 00341000
         DELETE EP=IEECB860                                             00342000
*   R14=SETESAVE;                   /* RESTORE RETURN ADDR TO TOP    */ 00343000
         L     R14,SETESAVE                                        0253 00344000
*   RFY                                                            0254 00345000
*    (R1,                                                          0254 00346000
*     R4,                                                          0254 00347000
*     R11,                                                         0254 00348000
*     R12,                                                         0254 00349000
*     R14) UNRSTD;                                                 0254 00350000
*   END SETESTAE;                   /* RETURN TO TOP PROC            */ 00351000
         B     @EL00002                                            0255 00352000
*                                                                  0256 00353000
*/********************************************************************* 00354000
**               T R K E S T A E   P R O C E D U R E                  * 00355000
** ENTRY REASON - TRACK CMD ISSUED SO SET UP LOWER LEVEL ESTAE        * 00356000
** ENTRY FROM   - TOP PROCEDURE                                       * 00357000
** EXIT  TO     - STAEFAIL PROCEDURE (IF LOWER LEVEL ESTAE FAILED)    * 00358000
**                NORMAL RETURN TO TOP PROCEDURE (IF ESTAE WORKED)    * 00359000
** FUNCTION     - 1) A LOWER LEVEL ESTAE ENVIRONMENT IS CREATED FOR   * 00360000
**                   THE TRACK CMD FOR CLEANUP PURPOSES. AN ESTAE IS  * 00361000
**                   ISSUED SPECIFYING THAT ROUTINE 'STAEXIT' SHOULD  * 00362000
**                   GET CONTROL WITH ADDRESSABILITY TO 'STAELIST' IF * 00363000
**                   AN UNPREDICTABLE ERROR OCCURS.                   * 00364000
**                2) IF ESTAE WAS NOT SUCCESSFULLY CREATED THEN       * 00365000
**                   A) INITIALIZE FOR STAEFAIL PROCEDURE             * 00366000
**                   B) CALL STAEFAIL WHICH RETURNS TO THE TOP PROC   * 00367000
**                      AT LABEL RETSYS WHICH GIVES CONTROL TO LABEL  * 00368000
**                      ABEND WHICH ISSUES ABEND 102                  * 00369000
**                3) IF ESTAE WAS SUCCESSFUL THEN                     * 00370000
**                   A) TURN ON FLAG 'FIRSTRAK' TO INDICATE THAT THIS * 00371000
**                      IS THE FIRST TIME THROUGH THIS CODE FOR THIS  * 00372000
**                      TRACK COMMAND.                                * 00373000
**                   B) RETURN TO THE TOP PROCEDURE                   * 00374000
**********************************************************************/ 00375000
*                                                                  0256 00376000
*TRKESTAE:                                                         0256 00377000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0256 00378000
TRKESTAE DS    0H                                                  0257 00379000
*   RFY                                                            0257 00380000
*    (R1,                                                          0257 00381000
*     R8,                                                          0257 00382000
*     R9,                                                          0257 00383000
*     R14) RSTD;                                                   0257 00384000
*   TRKESAVE=R14;                   /* SAVE RETURN ADDR TO TOP PROC  */ 00385000
         ST    R14,TRKESAVE                                        0258 00386000
*   RFY                                                            0259 00387000
*     R14 UNRSTD;                                                  0259 00388000
*   TRPF1TES=ON;                    /* MARK FLAG FOR TRKESTAE PROC AS   00389000
*                                      ENTERED                       */ 00390000
         LA    @10,TRCURRSW                                        0260 00391000
         OI    TRPF1TES(@10),B'00100000'                           0260 00392000
*   ESTAELST=ESTAELST&&ESTAELST;    /* ESTAE INTERFACE               */ 00393000
         XC    ESTAELST(24),ESTAELST                               0261 00394000
*   SACBADDR=ZERO;                  /* ZERO THESE ADDR FOR ESTAE     */ 00395000
         LA    @10,STAELIST                                        0262 00396000
         SLR   @00,@00                                             0262 00397000
         ST    @00,SACBADDR(,@10)                                  0262 00398000
*   R1=ADDR(ESTAELST);              /* REQUIRED FOR ESTAE MACRO      */ 00399000
         LA    R1,ESTAELST                                         0263 00400000
*   R8=ADDR(STAEXIT);               /* THIS TASK'S STAE RTN ADDR     */ 00401000
         LA    R8,STAEXIT                                          0264 00402000
*   R9=ADDR(STAELIST);              /* INPUT TO THIS TASK'S STAE RTN */ 00403000
         LR    R9,@10                                              0265 00404000
*   GEN SETS(R0,R1,R8) REFS(R3,R9);                                0266 00405000
         ESTAE (8),RECORD=YES,PARAM=(9),MF=(E,(1))                      00406000
*   RFY                                                            0267 00407000
*    (R1,                                                          0267 00408000
*     R8,                                                          0267 00409000
*     R9) UNRSTD;                                                  0267 00410000
*   RESPECIFY                                                      0268 00411000
*    (R15) RESTRICTED;                                             0268 00412000
*   IF R15^=ZERO THEN               /* SUCCESSFYLLY CREATED?         */ 00413000
         LTR   R15,R15                                             0269 00414000
         BZ    @RF00269                                            0269 00415000
*     DO;                           /* ESTAE FAILED          @YM4795P*/ 00416000
*       HAVESDWA=OFF;               /* TURN SDWA BIT OFF     @YM4795P*/ 00417000
*       ABENDSW=ON;                 /* ABEND 102 IS WANTED           */ 00418000
         OI    ABENDSW,B'00000010'                                 0272 00419000
         NI    HAVESDWA,B'11101111'                                0272 00420000
*       CALL STAEFAIL;              /* FREE CSCB,CLEAR SACB. CONTROL    00421000
*                                      IS NOT RETURNED HERE BUT TO 0273 00422000
*                                      THE TOP PROCEDURE AND THEN  0273 00423000
*                                      ABEND 102 IS ISSUED.          */ 00424000
         BAL   @14,STAEFAIL                                        0273 00425000
*     END;                          /* IEECB860 WILL ISSUE COMMAND 0274 00426000
*                                      FAIL MESSAGE                  */ 00427000
*   RESPECIFY                                                      0275 00428000
*     R15 UNRSTD;                   /*                       @YM4795P*/ 00429000
@RF00269 DS    0H                                                  0276 00430000
*   FIRSTRAK=ON;                    /* SWITCH FOR DCMADEND CHECK   0276 00431000
*                                                            @YM01918*/ 00432000
         OI    FIRSTRAK,B'10000000'                                0276 00433000
*   RFY                                                            0277 00434000
*     R14 RSTD;                                                    0277 00435000
*   R14=TRKESAVE;                   /* RESTORE RETURN ADDR TO TOP  0278 00436000
*                                      PROC                          */ 00437000
         L     R14,TRKESAVE                                        0278 00438000
*   RFY                                                            0279 00439000
*     R14 UNRSTD;                                                  0279 00440000
*   END TRKESTAE;                   /* RETURN TO TOP PROC            */ 00441000
@EL00003 DS    0H                                                  0280 00442000
@EF00003 DS    0H                                                  0280 00443000
@ER00003 BR    @14                                                 0280 00444000
*                                                                  0281 00445000
*/********************************************************************* 00446000
**                    T R A C K    P R O C E D U R E                  * 00447000
** ENTRY REASON - AN ECB WAS POSTED OR THIS IS THE FIRST TIME DISPLAY * 00448000
**                SHOULD BE ISSUED                                    * 00449000
** ENTRY FROM   - TOP PROCEDURE                                       * 00450000
** EXIT TO      - 1) NORMAL RETURN TO THE TOP PROCEDURE.              * 00451000
**                2) LABEL ALLDONE IN TOP PROC (IF THE ECB WAS POSTED * 00452000
**                   BY DIDOCS).                                      * 00453000
**                3) LABEL ALLDONE IF THE ECB WAS POSTED BY STOP TRACK* 00454000
**                   OR IF THE TRACK OPTIONS ARE ZERO OR IF THE       * 00455000
**                   UPDATE TIME INTERVAL IS ZERO)                    * 00456000
** FUNCTION     - 1) THE CMS AND LOCAL  LOCKS  ARE OBTAINED TO INSURE * 00457000
**                   SERIALIZATION OF THE TRACK TERMINATION BITS IN   * 00458000
**                   THE SACB AND THE POSTING OF THE ECB IN THE CSCB  * 00459000
**                2) OBTAIN THE ADDRESS OF THE SACB (SCREEN AREA      * 00460000
**                   CONTROL BLOCK) WHICH DEFINES THE AREA INTO WHICH * 00461000
**                   THE TRACK DISPLAY SHOULD BE PLACED.              * 00462000
**                3) IF THE TRACK ECB IN THE CSCB HAS BEEN POSTED BY  * 00463000
**                   CONSOLE CLOSE OR BY DIDOCS: COMPLETION CODE =0   * 00464000
**                   A) THE LOCKS ARE RELEASED                        * 00465000
**                   B) RETURN TO LABEL ALLDONE IN THE TOP PROCEDURE  * 00466000
**                4) IF THE TRACK ECB IN THE CSCB HAS BEEN POSTED AS  * 00467000
**                   A RESULT OF A STOPTR COMMAND: COMPLETION CODE=4  * 00468000
**                   OR IF THE TRACK OPTIONS ARE ZERO OR IF THE TIME  * 00469000
**                   INTERVAL UPDATE IS ZERO THEN                     * 00470000
**                   A) CLEAR THE TRACK BITS, TIME INTERVAL AND THE   * 00471000
**                      ADDRESS OF THE TRACK ECB IN THE SACB          * 00472000
**                   B) RELEASE THE LOCKS                             * 00473000
**                   C) RETURN TO LABEL ALLDONE IN THE TOP PROCEDURE  * 00474000
**                5) MOVE THE MOST RECENT TRACK OPTIONS AND THE TIME  * 00475000
**                   INTERVAL FROM THE SACB TO THE CSCB               * 00476000
**                6) IF IN HOLD MODE THEN                             * 00477000
**                   A) RELEASE THE LOCKS AND SET TRKNOUP SWITCH      * 00478000
**                   B) NORMAL RETURN TO THE TOP PROCEDURE            * 00479000
**                7) IF THIS IS NOT THE FIRST TRACK DISPLAY AND DIDOCS* 00480000
**                   HAS NOT FINISHED WRITING THE SCREEN YET AND THE  * 00481000
**                   SCREEN AREA IS NOT FULL YET THEN                 * 00482000
**                   A) RELEASE THE LOCKS AND SET TRKNOUP SWITCH      * 00483000
**                   B) BUILD MSG 'IEE920I NO TR UPDATE AT HH.MM.SS'  * 00484000
**                   C) CALL IEECB801 TO ISSUE THE MESSAGE            * 00485000
**                   D) NORMAL RETURN TO THE TOP PROCEDURE            * 00486000
**                8) RELEASE THE LOCKS AND RETURN                     * 00487000
** NOTE         - FUNCTION (4) MENTIONED ABOVE CHECKS TO SEE IF THERE * 00488000
**                IS ANY REASON WHY THE TRACK SHOULD STOP AND IF      * 00489000
**                THERE IS THEN TRACK PROCESSING IS STOPPED.          * 00490000
**********************************************************************/ 00491000
*                                                                  0281 00492000
*TRACK:                                                            0281 00493000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0281 00494000
TRACK    DS    0H                                                  0282 00495000
*   RFY                                                            0282 00496000
*     R14 RSTD;                                                    0282 00497000
*   TRACKSAV=R14;                   /* SAVE RETURN ADDR TO TOP PROC  */ 00498000
         ST    R14,TRACKSAV                                        0283 00499000
*   RFY                                                            0284 00500000
*     R14 UNRSTD;                                                  0284 00501000
*   TRCF1TRI=ON;                    /* MARK TRACK PROC AS IN       0285 00502000
*                                      PROCESSING                    */ 00503000
*   TRCF1TRE=ON;                    /* MARK TRACK PROC AS ENTERED    */ 00504000
         LA    @10,TRCURRSW                                        0286 00505000
         OI    TRCF1TRI(@10),B'00110000'                           0286 00506000
*   TRKNOUP=OFF;                    /* UPDATE IS WANTED AT THIS TIME */ 00507000
         NI    TRKNOUP,B'11111011'                                 0287 00508000
*   GEN SETS(LOCKSAVE,R6) REFS(R11,R12,R14);                       0288 00509000
*                                                                  0288 00510000
             STM     R11,R14,LOCKSAVE    SAVE REGISTERS ACROSS SETLOCK  00511000
             LR      R6,R12              SAVE DATA REGISTER             00512000
             SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                    X00513000
               RELATED=(SACB,IEECB800(LOCKSOFF))                        00514000
             SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                      X00515000
               RELATED=(SACB,IEECB800(LOCKSOFF))                        00516000
             LR      R12,R6              RESTORE DATA REGISTER          00517000
             LM      R11,R14,LOCKSAVE    RESTORE REGISTERS              00518000
*   /*****************************************************************/ 00519000
*   /*                                                               */ 00520000
*   /* THE CSCB POINTER MUST BE RESTORED FROM THE STAELIST EACH TIME */ 00521000
*   /* THE TRACK PROCESSOR IS REACTIVATED.                           */ 00522000
*   /*                                                               */ 00523000
*   /*****************************************************************/ 00524000
*                                                                  0289 00525000
*   CSCBPTR=CSCBPASS;               /* TO INSURE PROPER CSCB ADDRESS */ 00526000
*                                                                  0289 00527000
         LA    @10,STAELIST                                        0289 00528000
         L     CSCBPTR,CSCBPASS(,@10)                              0289 00529000
*   /*****************************************************************/ 00530000
*   /*                                                               */ 00531000
*   /* 2) FIND THE SCREEN AREA CONTROL BLOCK                         */ 00532000
*   /*                                                               */ 00533000
*   /*****************************************************************/ 00534000
*                                                                  0290 00535000
*   CALL FINDSACB;                  /* LOCATE TRACKS SACB    @YM4795P*/ 00536000
*                                                                  0290 00537000
         BAL   @14,FINDSACB                                        0290 00538000
*   /*****************************************************************/ 00539000
*   /*                                                               */ 00540000
*   /* 3) ECB POSTED BY DIDOCS OR NO SACB FOUND ?                    */ 00541000
*   /*                                                               */ 00542000
*   /*****************************************************************/ 00543000
*                                                                  0291 00544000
*   IF CHCECB=POSTED|SACBPTR=ZERO THEN/* WAS ECB POSTED BY DIDOCS OR    00545000
*                                      NO SACB FOUND                 */ 00546000
         CLC   CHCECB(4,CSCBPTR),POSTED                            0291 00547000
         BE    @RT00291                                            0291 00548000
         LTR   SACBPTR,SACBPTR                                     0291 00549000
         BNZ   @RF00291                                            0291 00550000
@RT00291 DS    0H                                                  0292 00551000
*     DO;                           /* YES, RELEASE LOCKS, REQUEST 0292 00552000
*                                      SVC34 TO FREE CSCB AND RETURN */ 00553000
*       CALL LOCKSOFF;              /* RELEASE THE LOCKS             */ 00554000
         BAL   @14,LOCKSOFF                                        0293 00555000
*       TRCF1TRI=OFF;               /* MARK TRACK PROC AS CONTROL  0294 00556000
*                                      LEAVING                       */ 00557000
         LA    @10,TRCURRSW                                        0294 00558000
         NI    TRCF1TRI(@10),B'11011111'                           0294 00559000
*       RETURN TO ALLDONE;          /* FINISHED,RETURN TO ALLDONE IN    00560000
*                                      TOP PROCEDURE                 */ 00561000
         LA    @14,ALLDONE                                         0295 00562000
@EL00004 DS    0H                                                  0295 00563000
@EF00004 DS    0H                                                  0295 00564000
@ER00004 BR    @14                                                 0295 00565000
*     END;                                                         0296 00566000
*                                                                  0296 00567000
*   /*****************************************************************/ 00568000
*   /*                                                               */ 00569000
*   /* 4) ECB POSTED BY STOP TRK OR ANY OTHER REASON TO STOP ?       */ 00570000
*   /*                                                               */ 00571000
*   /*****************************************************************/ 00572000
*                                                                  0297 00573000
*   IF CHCECB=PTPOSTED              /* WAS ECB POSTED BY STOPTR?     */ 00574000
*       |DCMATRCK=OFFBYTE           /* ANY TRACK OPTION      @ZA00073*/ 00575000
*       |DCMAUTME=OFFHALF THEN      /* TIME INTERVAL?        @ZA00073*/ 00576000
@RF00291 CLC   CHCECB(4,CSCBPTR),PTPOSTED                          0297 00577000
         BE    @RT00297                                            0297 00578000
         CLI   DCMATRCK(SACBPTR),X'00'                             0297 00579000
         BE    @RT00297                                            0297 00580000
         LH    @10,DCMAUTME(,SACBPTR)                              0297 00581000
         LTR   @10,@10                                             0297 00582000
         BNZ   @RF00297                                            0297 00583000
@RT00297 DS    0H                                                  0298 00584000
*     DO;                           /* YES, CLEAR TRACK FIELDS IN  0298 00585000
*                                      SACB, RELEASE LOCKS, REQUEST     00586000
*                                      SVC34 TO FREE CSCB AND RETURN */ 00587000
*       DCMADFLG=DCMADFLG&CLEAROPT; /* TURN ALL OFF BUT DIDOCS BIT   */ 00588000
         NI    DCMADFLG(SACBPTR),X'20'                             0299 00589000
*       DCMATRCK=OFFBYTE;           /* TURN OFF TRACK OPTIONS        */ 00590000
         MVI   DCMATRCK(SACBPTR),X'00'                             0300 00591000
*       DCMAUTME=OFFHALF;           /* CLEAR TRACK TIME INTERVAL     */ 00592000
         SLR   @10,@10                                             0301 00593000
         STH   @10,DCMAUTME(,SACBPTR)                              0301 00594000
*       DCMATECB=CLEARECB;          /* CLEAR TRACK CANCEL ECB ADDR   */ 00595000
         SLR   @10,@10                                             0302 00596000
         ST    @10,DCMATECB(,SACBPTR)                              0302 00597000
*       CALL LOCKSOFF;              /* RELEASE THE LOCKS             */ 00598000
         BAL   @14,LOCKSOFF                                        0303 00599000
*       TRCF1TRI=OFF;               /* MARK TRACK PROC AS CONTROL  0304 00600000
*                                      LEAVING                       */ 00601000
         LA    @10,TRCURRSW                                        0304 00602000
         NI    TRCF1TRI(@10),B'11011111'                           0304 00603000
*       RETURN TO ALLDONE;          /* FINISHED,RETURN TO ALLDONE IN    00604000
*                                      TOP PROCEDURE                 */ 00605000
         LA    @14,ALLDONE                                         0305 00606000
         BR    @14                                                 0305 00607000
*     END;                                                         0306 00608000
*   CHTRSTAT=DCMATRCK;              /* SAVE TRACK REQUEST INDICATORS */ 00609000
@RF00297 MVC   CHTRSTAT(1,CSCBPTR),DCMATRCK(SACBPTR)               0307 00610000
*   CHPEND=DCMAUTME*ONEHUND;        /* SAVE TRACK TIME INTERVAL      */ 00611000
         LH    @10,DCMAUTME(,SACBPTR)                              0308 00612000
         MH    @10,@CH00934                                        0308 00613000
         ST    @10,CHPEND(,CSCBPTR)                                0308 00614000
*   CHCECB=ZERO;                    /* ZERO OUT DIDOCS/PT/K D,U ECB     00615000
*                                                            @YM2779P*/ 00616000
         SLR   @10,@10                                             0309 00617000
         ST    @10,CHCECB(,CSCBPTR)                                0309 00618000
*   STIMECB=ZERO;                   /* ZERO OUT STIMER ECB IN SACB 0310 00619000
*                                                            @YM2779P*/ 00620000
         LA    @03,STAELIST                                        0310 00621000
         ST    @10,STIMECB(,@03)                                   0310 00622000
*/********************************************************************* 00623000
**     6) IN HOLD MODE?                                               * 00624000
**     7) NOT IN HOLD MODE, THE SACB FLAGS ARE NEXT INTERROGATED TO   * 00625000
**        SEE IF DIDOCS HAS FINISHED WRITING THE PREVIOUS TRACK       * 00626000
**        DISPLAY TO THE SCREEN. FIRSTRAK WILL BE ON IF THIS IS THE   * 00627000
**        FIRST DISPLAY AND THERE IS NO PREVIOUS DISPLAY. DCMADEND    * 00628000
**        WILL BE ON IF DIDOCS HAS FINISHED ITS I/O.                  * 00629000
**           IF NOT:                                                  * 00630000
**           1)  'IEE920I NO TR A UPDATE AT HH.MM.SS YY.DDD' WILL BE  * 00631000
**               ISSUED.                                              * 00632000
**          A ------------------------------------------------------  * 00633000
**  WPL      | MESSAGE LENGTH + 4 =     | MCS FLAGS =              |  * 00634000
**            ------------------------------------------------------  * 00635000
**           |    'IEE920I NO TRACK UPDATE AT HH.MM.SS YY.DDD'     |  * 00636000
**            ------------------------------------------------------  * 00637000
**  WPLFLGS  | DESCRIPTOR CODES =      | ROUTING CODES =           |  * 00638000
**            ------------------------------------------------------  * 00639000
**  WPLLS01  | LINE TYPE =   | AREA ID =   | NUMBER OF LINES =     |  * 00640000
**          B ------------------------------------------------------  * 00641000
**********************************************************************/ 00642000
*                                                                  0311 00643000
*   /*****************************************************************/ 00644000
*   /*                                                               */ 00645000
*   /* 6) IN HOLD MODE ?                                             */ 00646000
*   /*                                                               */ 00647000
*   /*****************************************************************/ 00648000
*                                                                  0311 00649000
*   IF DCMAHOLD=OFF THEN            /* IS TRACK DISPLAY IN HOLD MODE?*/ 00650000
         TM    DCMAHOLD(SACBPTR),B'01000000'                       0311 00651000
         BNZ   @RF00311                                            0311 00652000
*     IF FIRSTRAK^=ON&              /* NO, IS THIS NOT FIRST TRACK &    00653000
*                                                            @YM01918*/ 00654000
*         DCMADEND^=ON&             /* HAS DIDOCS NOT FINISHED?      */ 00655000
*         DCMAFULL^=ON THEN         /* THE AREA IS NOT FULL  @YM2772P*/ 00656000
         TM    FIRSTRAK,B'10000000'                                0312 00657000
         BO    @RF00312                                            0312 00658000
         TM    DCMADEND(SACBPTR),B'00101000'                       0312 00659000
         BNZ   @RF00312                                            0312 00660000
*       DO;                         /* RIGHT ON                      */ 00661000
*         CALL LOCKSOFF;            /* RELEASE THE LOCKS             */ 00662000
         BAL   @14,LOCKSOFF                                        0314 00663000
*         TRKNOUP=ON;               /* DON'T UPDATE THE DISPLAY      */ 00664000
         OI    TRKNOUP,B'00000100'                                 0315 00665000
*         WTOBUF(ONE:NOUPLGTH)=NOUPDATE(ONE:NOUPLGTH);/* IEE920I     */ 00666000
         SLR   @10,@10                                             0316 00667000
         IC    @10,NOUPLGTH                                        0316 00668000
         BCTR  @10,0                                               0316 00669000
         EX    @10,@SM03157                                        0316 00670000
*         WPLPTR=ADDR(CHSWT)+SIXTEEN;/* SET FOR TIME STAMP   @YM01918*/ 00671000
         LA    WPLPTR,CHSWT(,CSCBPTR)                              0317 00672000
         AL    WPLPTR,@CF00963                                     0317 00673000
*         CALL TIMESNAP;            /* GO GET TIME STAMP             */ 00674000
         BAL   @14,TIMESNAP                                        0318 00675000
*         NOUPFLAG=ON;              /* INDICATE IEE920I TO BE ISSUED */ 00676000
         OI    NOUPFLAG,B'01000000'                                0319 00677000
*         CALL ISSUEMSG;            /* ISSUE NO TRK UPDATE MESSAGE   */ 00678000
         BAL   @14,ISSUEMSG                                        0320 00679000
*       END;                                                       0321 00680000
*     ELSE                                                         0322 00681000
*       CALL LOCKSOFF;              /* RELEASE THE LOCKS             */ 00682000
         B     @RC00312                                            0322 00683000
@RF00312 BAL   @14,LOCKSOFF                                        0322 00684000
*   ELSE                                                           0323 00685000
*     DO;                                                          0323 00686000
         B     @RC00311                                            0323 00687000
@RF00311 DS    0H                                                  0324 00688000
*       CALL LOCKSOFF;              /* RELEASE THE LOCKS             */ 00689000
         BAL   @14,LOCKSOFF                                        0324 00690000
*       TRKNOUP=ON;                 /* DON'T UPDATE THE DISPLAY      */ 00691000
         OI    TRKNOUP,B'00000100'                                 0325 00692000
*     END;                                                         0326 00693000
*   TRCF1TRI=OFF;                   /* MARK TRACK PROC AS CONTROL  0327 00694000
*                                      LEAVING                       */ 00695000
@RC00311 LA    @10,TRCURRSW                                        0327 00696000
         NI    TRCF1TRI(@10),B'11011111'                           0327 00697000
*   RFY                                                            0328 00698000
*     R14 RSTD;                                                    0328 00699000
*   R14=TRACKSAV;                   /* RESTORE RETURN ADDR OF TOP  0329 00700000
*                                      PROC                          */ 00701000
         L     R14,TRACKSAV                                        0329 00702000
*   RFY                                                            0330 00703000
*     R14 UNRSTD;                                                  0330 00704000
*   END TRACK;                      /* RETURN TO TOP PROC TO ISSUE 0331 00705000
*                                      DISPLAY                       */ 00706000
         B     @EL00004                                            0331 00707000
*                                                                  0332 00708000
*/********************************************************************* 00709000
**                D I S P L A Y    P R O C E D U R E                  * 00710000
** ENTRY REASON - A DISPLAY COMMAND WAS ISSUED OR A TRACK COMMAND     * 00711000
**                WAS ISSUED OR A TRACK UPDATE IS WANTED NOW          * 00712000
** ENTRY FROM   - TOP PROCEDURE (TWO PLACES)                          * 00713000
**                   1) TRACK COMMAND ISSUED                          * 00714000
**                   2) DISPLAY COMMAND ISSUED                        * 00715000
** EXIT  TO     - NORMAL RETURN TO TOP PROCEDURE                      * 00716000
** FUNCTION     - THIS PROCEDURE CONTROLS THE LOGIC FLOW FOR DISPLAY  * 00717000
**                PROCESSING                                          * 00718000
**                1) ENQ SHR ON TO THE CSCB CHAIN AND DETERMINE THE   * 00719000
**                   NUMBER OF JOBS, INITIATORS AND TSO USERS         * 00720000
**                2) GET BUFFER AREA FOR LINKED LIST AND BUILD THE    * 00721000
**                   HEADER LINE.                                     * 00722000
**                3) IF BUFFER NOT GOTTEN THEN RETURN.                * 00723000
**                4) DETERMINE WHICH  OPTIONS WANTED AND BUILD THE    * 00724000
**                   APPROPRIATE LINES.                               * 00725000
**                5) DEQUEUE OFF THE CSCB CHAIN                       * 00726000
**                6) WRITE THE DISPLAY                                * 00727000
**                7) FREE THE BUFFER AREA                             * 00728000
**                8) RETURN TO THE TOP PROCEDURE                      * 00729000
** NOTE         - THE BUFFER AREA CONTAINS A LINKED LIST OF CONNECTING* 00730000
**                    MLWTOS. EACH GROUP WILL CONTAIN 10 LINES EXCEPT * 00731000
**                    FOR THE LAST ONE WHICH MAY CONTAIN 1-10 LINES.  * 00732000
**                    THE SIZE OF THE BUFFER IS DETERMINED BY THE     * 00733000
**                    NUMBER OF LINES NEEDED TO FINISH THE DISPLAY    * 00734000
**                    AND THE TYPE OF LINES IN THE DISPLAY. THE TOTAL * 00735000
**                    DISPLAY IS BUILT IN THE BUFFER AND THEN IT IS   * 00736000
**                    WRITTEN OUT. THIS WAS DONE SO THE CSCB CHAIN    * 00737000
**                    COULD BE DEQUEUED FROM WHILE IEECB801 IS        * 00738000
**                    ISSUING TPUTS.                                  * 00739000
**                WPLPTR POINTS TO THE NEXT AVAILABLE WPL UPON ENTRY  * 00740000
**                    TO ANY PROCEDURES CALLED BY DISPLAY OR THEIR    * 00741000
**                    INTERNAL PROCEDURES EXCEPT NEXTWPL PROC WHICH   * 00742000
**                    EXPECTS WPLPTR TO POINT TO THE CURRENT WPL.     * 00743000
**********************************************************************/ 00744000
*                                                                  0332 00745000
*DISPLAY:                                                          0332 00746000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0332 00747000
DISPLAY  DS    0H                                                  0333 00748000
*   RFY                                                            0333 00749000
*     R14 RSTD;                                                    0333 00750000
*   DISPSAVE=R14;                   /* SAVE RETURN ADDR TO TOP PROC  */ 00751000
         ST    R14,DISPSAVE                                        0334 00752000
*   RFY                                                            0335 00753000
*     R14 UNRSTD;                                                  0335 00754000
*   TRCF3DPI=ON;                    /* MARK DISPLAY PROC AS IN     0336 00755000
*                                      PROCESSING                    */ 00756000
*   TRCF3DPE=ON;                    /* MARK DISPLAY PROC AS ENTERED  */ 00757000
         LA    @10,TRCURRSW                                        0337 00758000
         OI    TRCF3DPI(@10),B'00000011'                           0337 00759000
*   BUFPTR=ZERO;                    /* IF IT REMAINS ZERO THEN THE 0338 00760000
*                                      BUFFER COULD NOT BE GOTTEN  0338 00761000
*                                                            @ZA07770*/ 00762000
*                                                                  0338 00763000
         SLR   @10,@10                                             0338 00764000
         ST    @10,BUFPTR                                          0338 00765000
*   /*****************************************************************/ 00766000
*   /*                                                               */ 00767000
*   /* 1) THE CSCB CHAIN IS ENQUEUED UPON AND THEN SCANNED AND A     */ 00768000
*   /* COUNT OF THE NUMBER OF JOBS, INITIATORS AND TSO USERS IS KEPT.*/ 00769000
*   /*                                                               */ 00770000
*   /*****************************************************************/ 00771000
*                                                                  0339 00772000
*   CALL TYPECSCB;                                                 0339 00773000
*                                                                  0339 00774000
         BAL   @14,TYPECSCB                                        0339 00775000
*   /*****************************************************************/ 00776000
*   /*                                                               */ 00777000
*   /* 2) DO GETMAIN FOR THE NUMBER OF MLWTO LINES NEEDED. IF THE    */ 00778000
*   /* CORE IS NOT AVAILIABLE THEN ISSUE A MESSAGE STATING SO. IF THE*/ 00779000
*   /* CORE IS AVAILIABLE THEN BUILD THE HEADER LINE 'IEE102I        */ 00780000
*   /* HH.MM.SS YY.DDD ACTIVITY'                                     */ 00781000
*   /*                                                               */ 00782000
*   /*****************************************************************/ 00783000
*                                                                  0340 00784000
*   CALL HEADERLN;                                                 0340 00785000
*                                                                  0340 00786000
         BAL   @14,HEADERLN                                        0340 00787000
*   /*****************************************************************/ 00788000
*   /*                                                               */ 00789000
*   /* 3) BUFFER AREA GOTTEN ?                                       */ 00790000
*   /*                                                               */ 00791000
*   /*****************************************************************/ 00792000
*                                                                  0341 00793000
*   IF BUFPTR^=ZERO THEN                                           0341 00794000
         L     @10,BUFPTR                                          0341 00795000
         LTR   @10,@10                                             0341 00796000
         BZ    @RF00341                                            0341 00797000
*     DO;                           /* BUFFER GOTTEN                 */ 00798000
*                                                                  0342 00799000
*       /*************************************************************/ 00800000
*       /*                                                           */ 00801000
*       /* 4) IF 'JOBS','JOBS,LIST','A' OR 'A,LIST' THEN BUILD THE   */ 00802000
*       /* JOB INITIATOR LINE '00000 JOBS 00000 INITIATORS'          */ 00803000
*       /*                                                           */ 00804000
*       /*************************************************************/ 00805000
*                                                                  0343 00806000
*       IF TASKCSCB->CHJOBS=ON|     /* JOBS OR JOBS,LIST             */ 00807000
*           TASKCSCB->CHJOBSL=ON THEN/* OPTION SPECIFIED?            */ 00808000
         L     @10,TASKCSCB                                        0343 00809000
         TM    CHJOBS(@10),B'11000000'                             0343 00810000
         BZ    @RF00343                                            0343 00811000
*         DO;                       /* YES                           */ 00812000
*           CALL JOBLINE;           /* BUILD JOB-INITIATOR LINE      */ 00813000
         BAL   @14,JOBLINE                                         0345 00814000
*           IF TASKCSCB->CHJOBSL=ON&/* JOBS,LIST WANTED AND          */ 00815000
*               JOBNUM^=ZERO THEN   /* THERE WAS A JOB CSCB          */ 00816000
         L     @10,TASKCSCB                                        0346 00817000
         TM    CHJOBSL(@10),B'01000000'                            0346 00818000
         BNO   @RF00346                                            0346 00819000
         LH    @10,JOBNUM                                          0346 00820000
         LTR   @10,@10                                             0346 00821000
         BZ    @RF00346                                            0346 00822000
*             CALL LISTJOBS;        /* BUILD MLWTO LINES TO LIST JOB */ 00823000
         BAL   @14,LISTJOBS                                        0347 00824000
*         END;                                                     0348 00825000
*                                                                  0348 00826000
@RF00346 DS    0H                                                  0349 00827000
*       /*************************************************************/ 00828000
*       /*                                                           */ 00829000
*       /* IF 'TS','TS,LIST','A' OR 'A,LIST' THEN BUILD THE TSO LINE */ 00830000
*       /* AND THE VTAM TSO USER LINE                                */ 00831000
*       /*                                                           */ 00832000
*       /*************************************************************/ 00833000
*                                                                  0349 00834000
*       IF TASKCSCB->CHTS=ON|       /* TS OPTION OR                  */ 00835000
*           TASKCSCB->CHTSL=ON THEN /* TS,LIST OPTION SPECIFIED      */ 00836000
@RF00343 L     @10,TASKCSCB                                        0349 00837000
         TM    CHTS(@10),B'00001100'                               0349 00838000
         BZ    @RF00349                                            0349 00839000
*         DO;                       /* YES                           */ 00840000
*           CALL TSVTMLIN;          /* BUILD TSO LINE AND VTAM TSO 0351 00841000
*                                      LINE                  @Z40DQSS*/ 00842000
         BAL   @14,TSVTMLIN                                        0351 00843000
*           IF TASKCSCB->CHTSL=ON&  /* TS,LIST WANTED AND            */ 00844000
*               TSNUM^=ZERO THEN    /* THERE ARE TSO USERS?          */ 00845000
         L     @10,TASKCSCB                                        0352 00846000
         TM    CHTSL(@10),B'00000100'                              0352 00847000
         BNO   @RF00352                                            0352 00848000
         LH    @10,TSNUM                                           0352 00849000
         LTR   @10,@10                                             0352 00850000
         BZ    @RF00352                                            0352 00851000
*             CALL LISTSO;          /* YES, LIST TSO USERS           */ 00852000
         BAL   @14,LISTSO                                          0353 00853000
*         END;                                                     0354 00854000
@RF00352 DS    0H                                                  0355 00855000
*       CALL LASTLINE;              /* MARK LAST LINE AS END OF MLWTO*/ 00856000
@RF00349 BAL   @14,LASTLINE                                        0355 00857000
*       CALL DQCSCB;                /* DEQ FROM THE CSCB CHAIN     0356 00858000
*                                                            @ZA07770*/ 00859000
         BAL   @14,DQCSCB                                          0356 00860000
*       CALL CALL801;               /* ISSUE THE MLWTOS      @ZA07770*/ 00861000
         BAL   @14,CALL801                                         0357 00862000
*       CALL FREEBUF;               /* FREE THE BUFFER       @ZA07770*/ 00863000
         BAL   @14,FREEBUF                                         0358 00864000
*     END;                                                         0359 00865000
*   TRCF3DPI=OFF;                   /* MARK DISPLAY PROC AS CONTROL     00866000
*                                      LEAVING                       */ 00867000
@RF00341 LA    @10,TRCURRSW                                        0360 00868000
         NI    TRCF3DPI(@10),B'11111101'                           0360 00869000
*   RFY                                                            0361 00870000
*     R14 RSTD;                                                    0361 00871000
*   R14=DISPSAVE;                   /* RESTORE RETURN ADDR TO TOP  0362 00872000
*                                      PROC                          */ 00873000
         L     R14,DISPSAVE                                        0362 00874000
*   RFY                                                            0363 00875000
*     R14 UNRSTD;                                                  0363 00876000
*   END DISPLAY;                    /* RETURN TO TOP PROC            */ 00877000
@EL00005 DS    0H                                                  0364 00878000
@EF00005 DS    0H                                                  0364 00879000
@ER00005 BR    @14                                                 0364 00880000
*                                                                  0365 00881000
*/********************************************************************* 00882000
**                T Y P E C S C B   P R O C E D U R E                 * 00883000
** ENTRY REASON - DETERMINE NUMBER OF JOBS, INITIATORS AND TSO CSCBS  * 00884000
**                ON THE CHAIN. NUMBERS USED IN DETERMINING THE SIZE  * 00885000
**                OF THE BUFFER AREA AND ARE PUT INTO THE DISPLAY LINE* 00886000
** ENTRY FROM   - DISPLAY PROCEDURE                                   * 00887000
** EXIT  TO     - DISPLAY PROCEDURE                                   * 00888000
** FUNCTION     - 1) ENQUEUE SHARED ON THE CSCB CHAIN                 * 00889000
**                2) SCAN CSCB CHAIN AND DETERMINE THE NUMBER OF      * 00890000
**                   JOBS, INITIATORS AND TSO USERS                   * 00891000
**                   A) IF THE OPTION WAS 'JOBS','JOBS,LIST','A' OR   * 00892000
**                      'A,LIST' THEN                                 * 00893000
**                      1) INCREMENT JOB COUNTER FOR EACH JOB OR TASK * 00894000
**                         CSCB FOUND.                                * 00895000
**                      2) INCREMENT INIT COUNTER FOR EACH INITIATOR  * 00896000
**                         CSCB FOUND.                                * 00897000
**                   B) IF THE OPTION WAS 'TS','TS,LIST','A' OR       * 00898000
**                      'A,LIST' THEN                                 * 00899000
**                      1) INCREMENT TS COUNTER FOR EACH TSO USER     * 00900000
**                         CSCB FOUND.                                * 00901000
**********************************************************************/ 00902000
*                                                                  0365 00903000
*   RFY                                                            0365 00904000
*    (R1,                                                          0365 00905000
*     R14) RSTD;                                                   0365 00906000
*TYPECSCB:                                                         0366 00907000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0366 00908000
TYPECSCB DS    0H                                                  0367 00909000
*   TYPESAVE=R14;                   /* SAVE RETURN ADDR TO DISPLAY 0367 00910000
*                                      PROC                          */ 00911000
         ST    R14,TYPESAVE                                        0367 00912000
*   RFY                                                            0368 00913000
*     R14 UNRSTD;                                                  0368 00914000
*   TRCF4TCI=ON;                    /* MARK TYPECSCB PROC AS IN    0369 00915000
*                                      PROCESSING                    */ 00916000
*   TRCF4TCE=ON;                    /* MARK TYPECSCB PROC AS ENTERED */ 00917000
         LA    @10,TRCURRSW                                        0370 00918000
         OI    TRCF4TCI(@10),B'00110000'                           0370 00919000
*   ENQDEQL(ONE:ELEN)=EENQ(ONE:ELEN);/* BUILD ENQ LIST               */ 00920000
         LH    @10,ELEN                                            0371 00921000
         BCTR  @10,0                                               0371 00922000
         EX    @10,@SM03159                                        0371 00923000
*   R1=ADDR(ENQDEQL);               /* WORKAREA ADDRESSED BY REG 1   */ 00924000
         LA    R1,ENQDEQL                                          0372 00925000
*   GEN(ENQ MF=(E,(1)));                                           0373 00926000
         ENQ MF=(E,(1))                                                 00927000
*   RFY                                                            0374 00928000
*     R1 UNRSTD;                                                   0374 00929000
*   CSCBPTR=BACHN;                  /* GET ADDR OF 1ST CSCB FROM   0375 00930000
*                                      BASEA                         */ 00931000
         L     @10,CVTPTR                                          0375 00932000
         L     @10,CVTMSER(,@10)                                   0375 00933000
         L     CSCBPTR,BACHN(,@10)                                 0375 00934000
*   JOBNUM=ZERO;                    /* INIT ACTIVE JOBS & TASK     0376 00935000
*                                      COUNTER                       */ 00936000
         SLR   @10,@10                                             0376 00937000
         STH   @10,JOBNUM                                          0376 00938000
*   INITNUM=ZERO;                   /* INIT ACTIVE INIT COUNTER      */ 00939000
         STH   @10,INITNUM                                         0377 00940000
*   TSNUM=ZERO;                     /* INIT ACTIVE TS USERS COUNTER  */ 00941000
         STH   @10,TSNUM                                           0378 00942000
*   TASKCSCB=CSCBPASS;              /* INIT THIS TASK'S CSCB BASE    */ 00943000
         LA    @10,STAELIST                                        0379 00944000
         L     @10,CSCBPASS(,@10)                                  0379 00945000
         ST    @10,TASKCSCB                                        0379 00946000
*   DO WHILE CSCBPTR^=ZERO;         /* LOOP THRU ENTIRE CSCB CHAIN   */ 00947000
         B     @DE00380                                            0380 00948000
@DL00380 DS    0H                                                  0381 00949000
*     IF TASKCSCB->CHJOBS=ON|       /* JOBS OR JOBS,LIST             */ 00950000
*         TASKCSCB->CHJOBSL=ON THEN /* OPTION SPECIFIED?             */ 00951000
         L     @10,TASKCSCB                                        0381 00952000
         TM    CHJOBS(@10),B'11000000'                             0381 00953000
         BZ    @RF00381                                            0381 00954000
*       DO;                                                        0382 00955000
*         IF CHTRKID='02'X THEN     /* YES, THIS JOB OR TASK CSCB    */ 00956000
         CLI   CHTRKID(CSCBPTR),X'02'                              0383 00957000
         BNE   @RF00383                                            0383 00958000
*           JOBNUM=JOBNUM+ONE;      /* YES, INCREMENT COUNTER        */ 00959000
         LH    @10,JOBNUM                                          0384 00960000
         LA    @10,1(,@10)                                         0384 00961000
         STH   @10,JOBNUM                                          0384 00962000
*         IF CHTRKID='03'X THEN     /* IS THIS INIT CSCB?            */ 00963000
@RF00383 CLI   CHTRKID(CSCBPTR),X'03'                              0385 00964000
         BNE   @RF00385                                            0385 00965000
*           INITNUM=INITNUM+ONE;    /* YES,INCREMENT COUNTER         */ 00966000
         LH    @10,INITNUM                                         0386 00967000
         LA    @10,1(,@10)                                         0386 00968000
         STH   @10,INITNUM                                         0386 00969000
*       END;                                                       0387 00970000
@RF00385 DS    0H                                                  0388 00971000
*     IF TASKCSCB->CHTS=ON|         /* TS OR TS,LIST                 */ 00972000
*         TASKCSCB->CHTSL=ON THEN   /* OPTION SPECIFIED?             */ 00973000
@RF00381 L     @10,TASKCSCB                                        0388 00974000
         TM    CHTS(@10),B'00001100'                               0388 00975000
         BZ    @RF00388                                            0388 00976000
*       IF CHTRKID='01'X THEN       /* YES, IS THIS TSO USER         */ 00977000
         CLI   CHTRKID(CSCBPTR),X'01'                              0389 00978000
         BNE   @RF00389                                            0389 00979000
*         TSNUM=TSNUM+ONE;          /* YES, INCREMENT COUNTER        */ 00980000
         LH    @10,TSNUM                                           0390 00981000
         LA    @10,1(,@10)                                         0390 00982000
         STH   @10,TSNUM                                           0390 00983000
*     CSCBPTR=CHPTR;                /* POINTER TO NEXT CSCB          */ 00984000
@RF00389 DS    0H                                                  0391 00985000
@RF00388 L     CSCBPTR,CHPTR(,CSCBPTR)                             0391 00986000
*   END;                            /* WHEN 0, LAST ONE              */ 00987000
@DE00380 LTR   CSCBPTR,CSCBPTR                                     0392 00988000
         BNZ   @DL00380                                            0392 00989000
*   TRCF4TCI=OFF;                   /* MARK TYPECSCB PROC AS CONTROL    00990000
*                                      LEAVING                       */ 00991000
         LA    @10,TRCURRSW                                        0393 00992000
         NI    TRCF4TCI(@10),B'11011111'                           0393 00993000
*   RFY                                                            0394 00994000
*     R14 RSTD;                                                    0394 00995000
*   R14=TYPESAVE;                   /* RESTORE RETURN ADDR TO DISPLAY   00996000
*                                      PROC                          */ 00997000
         L     R14,TYPESAVE                                        0395 00998000
*   RFY                                                            0396 00999000
*     R14 UNRSTD;                                                  0396 01000000
*   END TYPECSCB;                                                  0397 01001000
@EL00006 DS    0H                                                  0397 01002000
@EF00006 DS    0H                                                  0397 01003000
@ER00006 BR    @14                                                 0397 01004000
*                                                                  0398 01005000
*/********************************************************************* 01006000
**               H E A D E R L N    P R O C E D U R E                 * 01007000
** ENTRY REASON - GET BUFFER AREA AND BUILD THE HEADER LINE.          * 01008000
** ENTRY FROM   - DISPLAY PROCEDURE                                   * 01009000
** EXIT  TO     - DISPLAY PROCEDURE                                   * 01010000
** FUNCTION     - 1) GET THE BUFFER AREA TO BUILD THE MLWTO LIST      * 01011000
**                   A) IF THE GETMAIN FAILED THEN                    * 01012000
**                      1) BUILD HEADER LINE STATING 'INSUFFICIENT    * 01013000
**                         STORAGE'  IN THE CSCB COMMAND BUFFER AREA  * 01014000
**          A ------------------------------------------------------  * 01015000
**  WPL      | MESSAGE LENGTH + 4 =     | MCS FLAGS =              |  * 01016000
**            ------------------------------------------------------  * 01017000
**           |    'IEE102I HH.MM.SS YY.DDD ACTIVITY'               |  * 01018000
**            ------------------------------------------------------  * 01019000
**  WPLFLGS  | DESCRIPTOR CODES =      | ROUTING CODES =           |  * 01020000
**            ------------------------------------------------------  * 01021000
**  WPLLS01  | LINE TYPE =   | AREA ID =   | NUMBER OF LINES =     |  * 01022000
**          B ------------------------------------------------------  * 01023000
**  WPLML    | MESSAGE LENGTH + 4 =     | MCS FLAGS =              |  * 01024000
**            ------------------------------------------------------  * 01025000
**           |   'DISPLAY TRUNCATED - INSUFFICIENT STORAGE'        |  * 01026000
**          C ------------------------------------------------------  * 01027000
**                      2) GET TIME STAMP. IF NO STAMP THEN TIME WILL * 01028000
**                         BE ZERO.                                   * 01029000
**                      3) DEQUEUE OFF THE CSCB CHAIN                 * 01030000
**                      4) ISSUE THE MESSAGE                          * 01031000
**                      5) RETURN                                     * 01032000
**                   B) IF THE GETMAIN WAS SUCCESSFUL  THEN           * 01033000
**                      1) ZERO OUT THE BUFFER                        * 01034000
**                      2) BUILD THE HEADER LINE                      * 01035000
**          A ------------------------------------------------------  * 01036000
**  WPL      | MESSAGE LENGTH + 4 =     | MCS FLAGS =              |  * 01037000
**            ------------------------------------------------------  * 01038000
**           |    'IEE102I HH.MM.SS YY.DDD ACTIVITY'               |  * 01039000
**            ------------------------------------------------------  * 01040000
**  WPLFLGS  | DESCRIPTOR CODES =      | ROUTING CODES =           |  * 01041000
**            ------------------------------------------------------  * 01042000
**  WPLLS01  | LINE TYPE =   | AREA ID =   | NUMBER OF LINES =     |  * 01043000
**          B ------------------------------------------------------  * 01044000
**                         A) IF TIME STAMP ERROR THEN THE TIME WILL  * 01045000
**                            BE ZERO                                 * 01046000
**                         B) RETURN TO DISPLAY PROCEDURE             * 01047000
**********************************************************************/ 01048000
*                                                                  0398 01049000
*   RFY                                                            0398 01050000
*    (R1,                                                          0398 01051000
*     R6,                                                          0398 01052000
*     R14,                                                         0398 01053000
*     R15) RSTD;                                                   0398 01054000
*HEADERLN:                                                         0399 01055000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0399 01056000
HEADERLN DS    0H                                                  0400 01057000
*   HEADSAVE=R14;                   /* SAVE RETURN ADDR TO DISPLAY 0400 01058000
*                                      PROC                          */ 01059000
         ST    R14,HEADSAVE                                        0400 01060000
*   RFY                                                            0401 01061000
*     R14 UNRSTD;                                                  0401 01062000
*   TRCF4HDI=ON;                    /* MARK HEADERLN PROC AS IN    0402 01063000
*                                      PROCESSING                    */ 01064000
*   TRCF4HDE=ON;                    /* MARK HEADERLN PROC AS ENTERED */ 01065000
         LA    @14,TRCURRSW                                        0403 01066000
         OI    TRCF4HDI(@14),B'00000011'                           0403 01067000
*   CONNEXT=OFF;                    /* SEE PROLOG IN PROCEDURE     0404 01068000
*                                      NEXTWPL                       */ 01069000
*   CONEND=OFF;                     /* SEE PROLOG IN PROCEDURE     0405 01070000
*                                      NEXTWPL                       */ 01071000
         NI    CONNEXT,B'10011111'                                 0405 01072000
*   FIRSTIME=ON;                    /* 1ST 10-AT-A-TIME SWITCH       */ 01073000
         OI    FIRSTIME,B'00100000'                                0406 01074000
*   GETLIST(ONE:GETLGTH)=GETAREA(ONE:GETLGTH);/* BUILD GETMAIN LIST  */ 01075000
         LH    @14,GETLGTH                                         0407 01076000
         BCTR  @14,0                                               0407 01077000
         EX    @14,@SM03161                                        0407 01078000
*   CALL CALCORE;                   /* CALCULATE THE BUFFER SIZE   0408 01079000
*                                                            @ZA07770*/ 01080000
         BAL   @14,CALCORE                                         0408 01081000
*   GENLGTH=COREBYTE;               /* THE CORE SIZE NEEDED IS     0409 01082000
*                                      INITIALIZED           @ZA07770*/ 01083000
         L     @14,COREBYTE                                        0409 01084000
         ST    @14,GENLGTH                                         0409 01085000
*   R6=ADDR(BUFPTR);                /* BUFPTR=ADDRESS OF GOTTEN CORE */ 01086000
         LA    R6,BUFPTR                                           0410 01087000
*   R1=ADDR(GETLIST);               /* REG 1 POINTS TO GETMAIN LIST  */ 01088000
         LA    R1,GETLIST                                          0411 01089000
*   GEN SETS(R6) REFS(R1)(GETMAIN MF=(E,(1)),A=(6));               0412 01090000
         GETMAIN MF=(E,(1)),A=(6)                                       01091000
*   RFY                                                            0413 01092000
*    (R1,                                                          0413 01093000
*     R6) UNRSTD;                                                  0413 01094000
*                                                                  0413 01095000
*   /*****************************************************************/ 01096000
*   /*                                                               */ 01097000
*   /* TEST FOR SUCCESSFUL GETMAIN                                   */ 01098000
*   /*                                                               */ 01099000
*   /*****************************************************************/ 01100000
*                                                                  0414 01101000
*   IF R15^=ZERO THEN               /* WAS IT SUCCESSFUL?            */ 01102000
         LTR   R15,R15                                             0414 01103000
         BZ    @RF00414                                            0414 01104000
*     DO;                           /* NO, MOVE MAIN & ERR INTO CSCB */ 01105000
*       RFY                                                        0416 01106000
*         R15 UNRSTD;                                              0416 01107000
*       CSCBPTR=CSCBPASS;           /* RESTORE TO THIS TASKS CSCB  0417 01108000
*                                                            @ZA07770*/ 01109000
         LA    @10,STAELIST                                        0417 01110000
         L     CSCBPTR,CSCBPASS(,@10)                              0417 01111000
*       WPLPTR=ADDR(CHSWT);         /* ESTABLISH ADDRESSABILITY FOR     01112000
*                                      MESSAGE TEXT                  */ 01113000
         LA    WPLPTR,CHSWT(,CSCBPTR)                              0418 01114000
*       WPL(ONE:HEADLGTH)=HEADTEXT(ONE:HEADLGTH);/* MAIN HEADER    0419 01115000
*                                                            @YM01918*/ 01116000
         SLR   @10,@10                                             0419 01117000
         IC    @10,HEADLGTH                                        0419 01118000
         BCTR  @10,0                                               0419 01119000
         EX    @10,@SM03163                                        0419 01120000
*       CALL TIMESNAP;              /* GO GET TIME STAMP             */ 01121000
         BAL   @14,TIMESNAP                                        0420 01122000
*       WPLPTR=WPLPTR+WPLLGH;       /*                       @ZA05477*/ 01123000
         AH    WPLPTR,WPLLGH(,WPLPTR)                              0421 01124000
*       WPLDESCJ=OFF;               /* TURN OFF DESC CODE 10 @ZA05477*/ 01125000
         NI    WPLDESCJ(WPLPTR),B'10111111'                        0422 01126000
*       WPLPTR=WPLPTR+LENGTH(WPLFLGS)-FOUR;/* NEXT WPL       @ZA05477*/ 01127000
         SL    WPLPTR,@CF03131                                     0423 01128000
*       WPLAREA=CHARID;             /* GET AREA ID FOR CRT (Z=PAPER) */ 01129000
         MVC   WPLAREA(1,WPLPTR),CHARID(CSCBPTR)                   0424 01130000
*       WPLLINES=TWO;               /* NUMBER OF LINES = 2           */ 01131000
         MVI   WPLLINES(WPLPTR),X'02'                              0425 01132000
*       WPLPTR=WPLPTR+LENGTH(WPLLS01);/*                     @YM01918*/ 01133000
         AL    WPLPTR,@CF00034                                     0426 01134000
*       WPLML(ONE:ERRLGTH)=ERRTEXT(ONE:ERRLGTH);/* ERROR LINE      0427 01135000
*                                                            @YM01918*/ 01136000
         SLR   @10,@10                                             0427 01137000
         IC    @10,ERRLGTH                                         0427 01138000
         BCTR  @10,0                                               0427 01139000
