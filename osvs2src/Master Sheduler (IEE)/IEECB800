*   TRCFBJFI=OFF;                   /* MARK FLAG AS CONTROL LEAVING     01833000
*                                      JOBLFILL PROC                 */ 01834000
@RF00612 LA    @10,TRCURRSW                                        0614 01835000
         NI    TRCFBJFI(@10),B'11111101'                           0614 01836000
*   RFY                                                            0615 01837000
*     R14 RSTD;                                                    0615 01838000
*   R14=JOBLFSAV;                   /* RESTORE RETURN ADDR TO      0616 01839000
*                                      LISTJOBS PROC                 */ 01840000
