         MVCL  R2,R6                                               0440 01169000
*       RFY                                                        0441 01170000
*        (R2,                                                      0441 01171000
*         R3,                                                      0441 01172000
*         R6,                                                      0441 01173000
*         R7) UNRSTD;                                              0441 01174000
*       COUNT=ZERO;                 /* INITIALIZE LINE COUNT TO 0    */ 01175000
         MVI   COUNT,X'00'                                         0442 01176000
*       WPLPTR=BUFPTR+LINKLGTH;     /* FIRST MLWTO PLACE     @ZA07770*/ 01177000
         LA    WPLPTR,4                                            0443 01178000
         AL    WPLPTR,BUFPTR                                       0443 01179000
*       WPL(ONE:HEADLGTH)=HEADTEXT(ONE:HEADLGTH);/* MOVE MAIN HEADER */ 01180000
         SLR   @10,@10                                             0444 01181000
         IC    @10,HEADLGTH                                        0444 01182000
         BCTR  @10,0                                               0444 01183000
         EX    @10,@SM03163                                        0444 01184000
*       CALL TIMESNAP;              /* GET TIME STAMP                */ 01185000
         BAL   @14,TIMESNAP                                        0445 01186000
*       WPLPTR=WPLPTR+WPLLGH;       /*                       @ZA05477*/ 01187000
         AH    WPLPTR,WPLLGH(,WPLPTR)                              0446 01188000
*       IF CSCBPASS->CHVCD^=TRCMD THEN/* IS THIS A TR CMD    @ZA05477*/ 01189000
         LA    @10,STAELIST                                        0447 01190000
         L     @10,CSCBPASS(,@10)                                  0447 01191000
         CLI   CHVCD(@10),X'C4'                                    0447 01192000
         BE    @RF00447                                            0447 01193000
*         WPLDESCJ=OFF;             /* NO ITS DISPLAY. TURN OFF DESC    01194000
*                                      10                    @ZA05477*/ 01195000
         NI    WPLDESCJ(WPLPTR),B'10111111'                        0448 01196000
*       WPLPTR=WPLPTR+LENGTH(WPLFLGS)-FOUR+LENGTH(WPLLS01);/* GET  0449 01197000
*                                      NEXT WPL              @ZA05477*/ 01198000
@RF00447 AL    WPLPTR,@CF00196                                     0449 01199000
*       COUNT=COUNT+ONE;            /* INCREMENT LINE COUNT          */ 01200000
         IC    @10,COUNT                                           0450 01201000
         LA    @10,1(,@10)                                         0450 01202000
         STC   @10,COUNT                                           0450 01203000
*     END;                                                         0451 01204000
*   TRCF4HDI=OFF;                   /* MARK HEADERLN PROC AS CONTROL    01205000
*                                      LEAVING                       */ 01206000
@RC00414 LA    @10,TRCURRSW                                        0452 01207000
         NI    TRCF4HDI(@10),B'11111101'                           0452 01208000
*   RFY                                                            0453 01209000
*     R14 RSTD;                                                    0453 01210000
*   R14=HEADSAVE;                   /* RESTORE RETURN ADDR TO DISPLAY   01211000
*                                      PROC                          */ 01212000
         L     R14,HEADSAVE                                        0454 01213000
*   RFY                                                            0455 01214000
*     R14 UNRSTD;                                                  0455 01215000
*   END HEADERLN;                                                  0456 01216000
@EL00007 DS    0H                                                  0456 01217000
@EF00007 DS    0H                                                  0456 01218000
@ER00007 BR    @14                                                 0456 01219000
*                                                                  0457 01220000
*/********************************************************************* 01221000
**             I S S U E M S G    P R O C E D U R E                   * 01222000
** ENTRY REASON - 1) ISSUE NO TRK UPDATE MESSAGE                      * 01223000
**                2) ISSUE INSUFFICENT STORAGE MESSAGE                * 01224000
** ENTRY FROM   - 1) TRACK PROCEDURE                                  * 01225000
**                2) HEADERLN PROCEDURE                               * 01226000
** EXIT TO      - RETURN TO CALLING PROCEDURE                         * 01227000
** FUNCTION     - 1) FILL IN PARM LIST                                * 01228000
**                2) HAVE IEECB801 ISSUE THE MESSAGES                 * 01229000
**                3) RETURN                                           * 01230000
**********************************************************************/ 01231000
*                                                                  0457 01232000
*ISSUEMSG:                                                         0457 01233000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0457 01234000
ISSUEMSG DS    0H                                                  0458 01235000
*   RFY                                                            0458 01236000
*    (R1,                                                          0458 01237000
*     R14) RSTD;                                                   0458 01238000
*   IMSGSAVE=R14;                   /* SAVE RETURN ADDR TO HEADERLN     01239000
*                                      PROC                          */ 01240000
         ST    R14,IMSGSAVE                                        0459 01241000
*   RFY                                                            0460 01242000
*     R14 UNRSTD;                                                  0460 01243000
*   TRCFCIMI=ON;                    /* MARK FLAG AS ISSUEMSG PROC IN    01244000
*                                      CONTROL                       */ 01245000
*   TRCFCIME=ON;                    /* MARK FLAG AS ISSUEMSG PROC  0462 01246000
*                                      ENTERED                       */ 01247000
         LA    @10,TRCURRSW                                        0462 01248000
         OI    TRCFCIMI(@10),B'00110000'                           0462 01249000
*   R1=ADDR(PARMLIST);              /* PARM LIST FOR IEECB801        */ 01250000
         LA    R1,PARMLIST                                         0463 01251000
*   MESSADDR=ADDR(CHSWT);           /* ADDR OF MSG IN PARM LIST      */ 01252000
         LA    @09,CHSWT(,CSCBPTR)                                 0464 01253000
         ST    @09,MESSADDR                                        0464 01254000
*   TRCFACBI=ON;                    /* MARK IEECB801 AS IN PROCESSING*/ 01255000
*   TRCFACBE=ON;                    /* MARK IEECB801 AS ENTERED      */ 01256000
         OI    TRCFACBI(@10),B'00000011'                           0466 01257000
*   CALL IEECB801;                  /* GO TO IEECB801 TO ISSUE MSG   */ 01258000
         L     @15,@CV02746                                        0467 01259000
         BALR  @14,@15                                             0467 01260000
*   TRCFACBI=OFF;                   /* MARK IEECB801 AS CONTROL HAS     01261000
*                                      COME BACK                     */ 01262000
         LA    @10,TRCURRSW                                        0468 01263000
         NI    TRCFACBI(@10),B'11111101'                           0468 01264000
*   TRCFCIMI=OFF;                   /* MARK FLAG AS CONTROL LEAVING     01265000
*                                      ISSUEMSG PROC                 */ 01266000
         NI    TRCFCIMI(@10),B'11011111'                           0469 01267000
*   RFY                                                            0470 01268000
*     R14 RSTD;                                                    0470 01269000
*   R14=IMSGSAVE;                   /* RESTORE RETURN ADDR OF      0471 01270000
*                                      HEADERLN PROC                 */ 01271000
         L     R14,IMSGSAVE                                        0471 01272000
*   RFY                                                            0472 01273000
*    (R1,                                                          0472 01274000
*     R14) UNRSTD;                                                 0472 01275000
*   END ISSUEMSG;                                                  0473 01276000
@EL00008 DS    0H                                                  0473 01277000
@EF00008 DS    0H                                                  0473 01278000
@ER00008 BR    @14                                                 0473 01279000
*                                                                  0474 01280000
*/********************************************************************* 01281000
**                 C A L C O R E   P R O C E D U R E                  * 01282000
** ENTRY REASON - CALCULATE THE NUMBER OF BYTES NEEDED FOR THE BUFFER * 01283000
** ENTRY FROM   - HEADERLN PROCEDURE                                  * 01284000
** EXIT TO      - HEADERLN PROCEDURE                                  * 01285000
** FUNCTION     - 1) FIND THE NUMBER OF LINES AND BYTES FOR THE       * 01286000
**                   HEADER LINE                                      * 01287000
**                2) IF 'JOBS','JOBS,LIST','A' OR 'A,LIST' THEN       * 01288000
**                   CALCULATE THE NUMBER OF LINES AND BYTES NEEDED   * 01289000
**                   FOR THE DISPLAY                                  * 01290000
**                3) IF 'TS','TS,LIST','A' OR 'A,LIST' THEN CALCULATE * 01291000
**                   THE NUMBER OF LINES AND BYTES NEEDED FOR THESE   * 01292000
**                   OPTIONS                                          * 01293000
**                4) FIND THE NUMBER OF MLWTO'S TO BE WRITTEN         * 01294000
**                5) ADD EXTRA BYTES TO THE BUFFER SIZE FOR THE LINK  * 01295000
**                   POINTERS (FOR THE MLWTO LINKED LIST) AND THE     * 01296000
**                   EXTRA BYTES FOR THE FIRST LINE OF EACH CONNECTING* 01297000
**                   MLWTO                                            * 01298000
**                   (THE 1ST LINE IS LONGER THAN THE OTHER LINES)    * 01299000
**                6) RETURN                                           * 01300000
**********************************************************************/ 01301000
*                                                                  0474 01302000
*CALCORE:                                                          0474 01303000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @ZA07770*/ 01304000
CALCORE  DS    0H                                                  0475 01305000
*   RFY                                                            0475 01306000
*     R14 RSTD;                     /*                       @ZA07770*/ 01307000
*   CALCSAVE=R14;                   /* SAVE RETURN ADDR TO HEADERLN     01308000
*                                      PROC                  @ZA07770*/ 01309000
         ST    R14,CALCSAVE                                        0476 01310000
*   RFY                                                            0477 01311000
*     R14 UNRSTD;                   /*                       @ZA07770*/ 01312000
*   TRCF5CRI=ON;                    /* MARK CALCORE PROC AS IN     0478 01313000
*                                      PROCESSING                    */ 01314000
*   TRCF5CRE=ON;                    /* MARK CALCORE PROC AS ENTERED  */ 01315000
         LA    @10,TRCURRSW                                        0479 01316000
         OI    TRCF5CRI(@10),B'00110000'                           0479 01317000
*   COREBYTE=HED#BYTE;              /* # BYTES FOR THE HEADER LINE 0480 01318000
*                                                            @ZA07770*/ 01319000
         LA    @10,44                                              0480 01320000
         ST    @10,COREBYTE                                        0480 01321000
*   NUMLINES=ONE;                   /* # LINES IS ONE FOR HEADER   0481 01322000
*                                                            @ZA07770*/ 01323000
         LA    @15,1                                               0481 01324000
         LR    NUMLINES,@15                                        0481 01325000
*   IF TASKCSCB->CHJOBS=ON|TASKCSCB->CHJOBSL=ON THEN/* JOBS OR JOBS     01326000
*                                      LIST WANTED ?         @ZA07770*/ 01327000
         L     @02,TASKCSCB                                        0482 01328000
         TM    CHJOBS(@02),B'11000000'                             0482 01329000
         BZ    @RF00482                                            0482 01330000
*     DO;                           /* YES                   @ZA07770*/ 01331000
*       COREBYTE=COREBYTE+JOB#BYTE; /* INCREASE FOR JOB-INIT LINE  0484 01332000
*                                                            @ZA07770*/ 01333000
         AL    @10,@CF02944                                        0484 01334000
         ST    @10,COREBYTE                                        0484 01335000
*       NUMLINES=NUMLINES+ONE;      /* INCREASE FOR JOB-INIT @ZA07770*/ 01336000
         ALR   NUMLINES,@15                                        0485 01337000
*       IF TASKCSCB->CHJOBSL=ON THEN/* JOBS, LIST WANTED?    @ZA07770*/ 01338000
         TM    CHJOBSL(@02),B'01000000'                            0486 01339000
         BNO   @RF00486                                            0486 01340000
*         DO;                       /* YES                   @ZA07770*/ 01341000
*           COREBYTE=COREBYTE+(JBL#BYTE*JOBNUM);/* INCREASE FOR # OF    01342000
*                                      JOBS TO BE LISTED     @ZA07770*/ 01343000
         LH    @02,JOBNUM                                          0488 01344000
         LR    @15,@02                                             0488 01345000
         MH    @15,@CH02946                                        0488 01346000
         ALR   @10,@15                                             0488 01347000
         ST    @10,COREBYTE                                        0488 01348000
*           NUMLINES=NUMLINES+JOBNUM;/* INCREASE FOR # OF JOBS TO BE    01349000
*                                      LISTED                @ZA07770*/ 01350000
         ALR   NUMLINES,@02                                        0489 01351000
*           IF JOBNUM>SEVEN THEN    /* MORE JOBS THAN NAX V=R JOBS ?    01352000
*                                                            @ZA07770*/ 01353000
         C     @02,@CF01401                                        0490 01354000
         BNH   @RF00490                                            0490 01355000
*             COREBYTE=COREBYTE+(SEVEN*JBREXTRA);/* ADD BYTES TO LIST   01356000
*                                      MAX NUMBER OF V=R JOBS. SEVEN    01357000
*                                      IS THE MAX NUMBER ALLOWED.  0491 01358000
*                                                            @ZA07770*/ 01359000
         AL    @10,@CF03132                                        0491 01360000
         ST    @10,COREBYTE                                        0491 01361000
*           ELSE                    /*                       @ZA07770*/ 01362000
*             COREBYTE=COREBYTE+(JOBNUM*JBREXTRA);/* ADD BYTES TO  0492 01363000
*                                      LIST POSSIBLE V=R JOBS      0492 01364000
*                                                            @ZA07770*/ 01365000
         B     @RC00490                                            0492 01366000
@RF00490 LH    @10,JOBNUM                                          0492 01367000
