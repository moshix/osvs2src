*         JOBNM=CHKEY;              /* NO MOVE IN JOBNAME    @YM6960P*/ 01740000
         MVC   JOBNM(8,WPLPTR),CHKEY(CSCBPTR)                      0582 01741000
*       ELSE                                                       0583 01742000
*         ;                         /* DEFAULT=STARTING      @YM6960P*/ 01743000
@RF00581 DS    0H                                                  0584 01744000
*       IF CHSTEP^=ZEROES THEN      /* IS IT ZERO?           @YM6960P*/ 01745000
         CLC   CHSTEP(8,CSCBPTR),ZEROES                            0584 01746000
         BE    @RF00584                                            0584 01747000
*         STEPNM=CHSTEP;            /* NO MOVE IN STEPNAME@YM6960P   */ 01748000
         MVC   STEPNM(8,WPLPTR),CHSTEP(CSCBPTR)                    0585 01749000
*       ELSE                                                       0586 01750000
*         ;                         /* DEFAULT=STARTING      @YM6960P*/ 01751000
@RF00584 DS    0H                                                  0587 01752000
