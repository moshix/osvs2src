         BCTR  @10,0                                               0554 01630000
         EX    @10,@SM03172                                        0554 01631000
*         CALL JOBLFILL;            /* FILL IN JOBNAMES, STEPNAMES 0555 01632000
*                                      PROCNAMES, ETC.               */ 01633000
@RC00552 BAL   @14,JOBLFILL                                        0555 01634000
*         CALL NEXTWPL;             /* GET ADDR OF NEXT WPL          */ 01635000
         BAL   @14,NEXTWPL                                         0556 01636000
*       END;                                                       0557 01637000
*     CSCBPTR=CHPTR;                /* POINT TO NEXT CSCB, IF 0      */ 01638000
@RF00550 L     CSCBPTR,CHPTR(,CSCBPTR)                             0558 01639000
*   END;                            /* FALL THROUGH - END OF CHAIN   */ 01640000
@DE00549 LTR   CSCBPTR,CSCBPTR                                     0559 01641000
         BNZ   @DL00549                                            0559 01642000
*   TRCF6LJI=OFF;                   /* MARK LISTJOBS PROC AS CONTROL    01643000
*                                      LEAVING                       */ 01644000
         LA    @10,TRCURRSW                                        0560 01645000
         NI    TRCF6LJI(@10),B'11111101'                           0560 01646000
*   RFY                                                            0561 01647000
*     R14 RSTD;                                                    0561 01648000
*   R14=LISTJSAV;                   /* RESTORE RETURN ADDR TO DISPLAY   01649000
*                                      PROC                          */ 01650000
         L     R14,LISTJSAV                                        0562 01651000
*   RFY                                                            0563 01652000
*     R14 UNRSTD;                                                  0563 01653000
*   END LISTJOBS;                                                  0564 01654000
@EL00011 DS    0H                                                  0564 01655000
@EF00011 DS    0H                                                  0564 01656000
@ER00011 BR    @14                                                 0564 01657000
*                                                                  0565 01658000
*/********************************************************************* 01659000
**               J O B L F I L L   P R O C E D U R E                  * 01660000
** ENTRY REASON - OPTIONS 'JOBS,LIST' OR 'A,LIST' IS WANTED AND THE   * 01661000
**                NUMBER OF JOBS DOES NOT EQUAL ZERO                  * 01662000
** ENTRY FROM   - LISTJOBS PROCEDURE                                  * 01663000
** EXIT TO      - LISTJOBS PROCEDURE                                  * 01664000
** FUNCTION     - 1) FILL IN THE JOBNAME, STEPNAME AND PROC STEPNAME  * 01665000
**                   A) IF THE JOB IS A START OR MOUNT THEN           * 01666000
**                      1) JOBNAME IS IN CHCLS                        * 01667000
**                      2) STEPNAME IS IN CHKEY                       * 01668000
**                      3) PROC STEPNAME IS IN CHPROCSN               * 01669000
**                   B) IF THE JOB IS EXECUTING THEN                  * 01670000
**                      1) JOBNAME IS IN CHKEY                        * 01671000
**                      2) STEPNAME IS IN CHSTEP                      * 01672000
**                      3) PROC STEPNAME IS IN CHPROCSN               * 01673000
**                   C) IF JOBNAME OR STEPNAME IS NOT SPECIFIED THEN  * 01674000
**                      THE DEFAULT IS 'STARTING'                     * 01675000
**                   D) IF PROC STEPNAME IS NOT SPECIFIED, THE DEFAULT* 01676000
**                      IS BLANKS                                     * 01677000
**                   E) IF SWAPPED OUT, FILL IN 'S'           @ZA07770* 01678000
**                2) FILL IN THE BEGINNING AND ENDING ADDRESSES FOR   * 01679000
**                   THE JOBS REGION                                  * 01680000
**                   A) IF THE JOB REGION BEGINNING ADDRESS(CHRGNAD)  * 01681000
**                      OR JOB REGION SIZE(CHRGNSZ) ARE ZERO THEN THE * 01682000
**                      DEFAULT IS V=V REGION.                        * 01683000
**                   B) IF NOT V=V REGION THEN ITS A V=R REGION       * 01684000
**                      1) JOB REGION ENDING ADDRESS IS CALCULATED    * 01685000
**                      2) BEGINNING AND ENDING ADDRESS ARE CHANGED   * 01686000
**                         TO K MULTIPLES AND CONVERTED TO DECIMAL,   * 01687000
**                         UNPACKED AND PLACED IN MESSAGE TEXT.       * 01688000
**                3) RETURN                                           * 01689000
**********************************************************************/ 01690000
*                                                                  0565 01691000
*JOBLFILL:                                                         0565 01692000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0565 01693000
JOBLFILL DS    0H                                                  0566 01694000
*   RFY                                                            0566 01695000
*     R14 RSTD;                                                    0566 01696000
*   JOBLFSAV=R14;                   /* SAVE RETURN ADDR TO LISTJOBS     01697000
*                                      PROC                          */ 01698000
         ST    R14,JOBLFSAV                                        0567 01699000
*   RFY                                                            0568 01700000
*     R14 UNRSTD;                                                  0568 01701000
*   TRCFBJFI=ON;                    /* MARK FLAG AS CONTROL GIVEN TO    01702000
*                                      JOBLFILL PROC                 */ 01703000
*   TRCFBJFE=ON;                    /* MARK FLAG AS JOBLFILL PROC AS    01704000
*                                      ENTERED                       */ 01705000
         LA    @10,TRCURRSW                                        0570 01706000
         OI    TRCFBJFI(@10),B'00000011'                           0570 01707000
*   IF CHVCD=STARTJOB|CHVCD=MOUNTJOB THEN/*                  @YM6960P   01708000
*                                      START OR MOUNT ?      @YM6960P*/ 01709000
         CLI   CHVCD(CSCBPTR),X'04'                                0571 01710000
         BE    @RT00571                                            0571 01711000
         CLI   CHVCD(CSCBPTR),X'0C'                                0571 01712000
         BNE   @RF00571                                            0571 01713000
@RT00571 DS    0H                                                  0572 01714000
*     DO;                           /* YES                   @YM6960P*/ 01715000
*       IF CHCLS^=ZEROES THEN       /* IS IT ZERO            @YM6960P*/ 01716000
         CLC   CHCLS(8,CSCBPTR),ZEROES                             0573 01717000
         BE    @RF00573                                            0573 01718000
*         JOBNM=CHCLS;              /* NO MOVE IN JOBNAME    @YM6960P*/ 01719000
         MVC   JOBNM(8,WPLPTR),CHCLS(CSCBPTR)                      0574 01720000
*       ELSE                                                       0575 01721000
*         ;                         /* DEFAULT=STARTING      @YM6960P*/ 01722000
@RF00573 DS    0H                                                  0576 01723000
*       IF CHKEY^=ZEROES THEN       /* IS IT ZERO            @YM6960P*/ 01724000
         CLC   CHKEY(8,CSCBPTR),ZEROES                             0576 01725000
         BE    @RF00576                                            0576 01726000
*         STEPNM=CHKEY;             /* NO MOVE IN STEPNAME   @YM6960P*/ 01727000
         MVC   STEPNM(8,WPLPTR),CHKEY(CSCBPTR)                     0577 01728000
*       ELSE                                                       0578 01729000
*         ;                         /* DEFAULT=STARTING      @YM6960P*/ 01730000
@RF00576 DS    0H                                                  0579 01731000
*     END;                          /*                       @YM6960P*/ 01732000
*   ELSE                            /* EXECUTING JOB         @YM6960P*/ 01733000
*     DO;                           /*                       @YM6960P*/ 01734000
         B     @RC00571                                            0580 01735000
@RF00571 DS    0H                                                  0581 01736000
*       IF CHKEY^=ZEROES THEN       /* IS IT ZERO?           @YM6960P*/ 01737000
         CLC   CHKEY(8,CSCBPTR),ZEROES                             0581 01738000
