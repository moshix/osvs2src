*   RFY                                                            0617 01842000
*     R14 UNRSTD;                                                  0617 01843000
*   END JOBLFILL;                                                  0618 01844000
@EL00012 DS    0H                                                  0618 01845000
@EF00012 DS    0H                                                  0618 01846000
@ER00012 BR    @14                                                 0618 01847000
*                                                                  0619 01848000
*/********************************************************************* 01849000
**               T S V T M L I N   P R O C E D U R E                  * 01850000
** ENTRY REASON - OPTION 'TS','TS,LIST','A' OR 'A,LIST' WANTED SO     * 01851000
**                BUILD THE TSO AND VTAM LINES                        * 01852000
** ENTRY FROM   - DISPLAY PROCEDURE                                   * 01853000
** EXIT TO      - DISPLAY PROCEDURE                                   * 01854000
** FUNCTION     - 1) BUILD TS LINE '00000 TIME SHARING USERS'         * 01855000
**                2) BUILD VTAM LINE '00000 ACTIVE  00000 MAX VTAM    * 01856000
**                   TSO USERS'                                       * 01857000
**                3) PUT IN MAXIMUM VTAM TSO USER NUMBER IN THE LINE  * 01858000
**                4) RETURN                                           * 01859000
**********************************************************************/ 01860000
*                                                                  0619 01861000
*TSVTMLIN:                                                         0619 01862000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @Z40DQSS*/ 01863000
TSVTMLIN DS    0H                                                  0620 01864000
*   RFY                                                            0620 01865000
*     R14 RSTD;                     /*                       @Z40DQSS*/ 01866000
*   TSVTSAVE=R14;                   /* SAVE RETURN ADDR TO DISPLAY 0621 01867000
*                                      PROC                  @Z40DQSS*/ 01868000
         ST    R14,TSVTSAVE                                        0621 01869000
*   RFY                                                            0622 01870000
*     R14 UNRSTD;                   /*                       @Z40DQSS*/ 01871000
*   TRCF7TVI=ON;                    /* MARK TSVTMLIN PROC AS IN    0623 01872000
*                                      PROCESSING                    */ 01873000
*   TRCF7TVE=ON;                    /* MARK TSVTMLIN PROC AS ENTERED */ 01874000
*                                                                  0624 01875000
         LA    @10,TRCURRSW                                        0624 01876000
         OI    TRCF7TVI(@10),B'00000011'                           0624 01877000
*   /*****************************************************************/ 01878000
*   /*                                                               */ 01879000
*   /* BUILD WPL FOR '00000 TIME SHARING USERS'                      */ 01880000
*   /*                                                               */ 01881000
*   /*****************************************************************/ 01882000
*                                                                  0625 01883000
*   CALL FILLINE(TSTEXT,TSLGTH,TSNUM);/* BUILD TS LINE       @Z40DQSS*/ 01884000
*                                                                  0625 01885000
         MVC   @AL00001(8),@AL00625                                0625 01886000
         LA    @10,TSNUM                                           0625 01887000
         ST    @10,@AL00001+8                                      0625 01888000
         LA    @01,@AL00001                                        0625 01889000
         BAL   @14,FILLINE                                         0625 01890000
*   /*****************************************************************/ 01891000
*   /*                                                               */ 01892000
*   /* BUILD WPL FOR '00000 ACTIVE 00000 MAX VTAM TSO USERS'         */ 01893000
*   /*                                                               */ 01894000
*   /*****************************************************************/ 01895000
*                                                                  0626 01896000
*   IF TCASTPTR^=ZERO THEN          /* DOES THE TCAST TABLE EXIST? 0626 01897000
*                                                            @Z40DQSS*/ 01898000
         L     @10,CVTPTR                                          0626 01899000
         L     @10,TCASTPTR(,@10)                                  0626 01900000
         LTR   @10,@10                                             0626 01901000
         BZ    @RF00626                                            0626 01902000
*     DO;                           /* YES                   @Z40DQSS*/ 01903000
*       CALL FILLINE(VTAMTEXT,VTAMLGTH,TCASUSEC);/* BUILD VTAM LINE     01904000
*                                                            @Z40DQSS*/ 01905000
*                                                                  0628 01906000
         MVC   @AL00001(8),@AL00628                                0628 01907000
         LA    @10,TCASUSEC(,@10)                                  0628 01908000
         ST    @10,@AL00001+8                                      0628 01909000
         LA    @01,@AL00001                                        0628 01910000
         BAL   @14,FILLINE                                         0628 01911000
*       /*************************************************************/ 01912000
*       /*                                                           */ 01913000
*       /* PROCEDURE FILLINE SAVES THE ADDRESS OF THE WPL IT JUST    */ 01914000
*       /* FILLED IN WPLSAVE. THIS IS SO THE MAX VTAM TSO USERS MAY  */ 01915000
*       /* BE FILLED INTO THE CORRECT WPL.                           */ 01916000
*       /*                                                           */ 01917000
*       /*************************************************************/ 01918000
*                                                                  0629 01919000
*       IF TCASUMAX^=ZERO THEN      /* IS THERE A MAXIMUM NUMBER?  0629 01920000
*                                                            @Z40DQSS*/ 01921000
         L     @10,CVTPTR                                          0629 01922000
         L     @10,TCASTPTR(,@10)                                  0629 01923000
         LH    @10,TCASUMAX(,@10)                                  0629 01924000
         LTR   @10,@10                                             0629 01925000
         BZ    @RF00629                                            0629 01926000
*         DO;                       /* YES                   @Z40DQSS*/ 01927000
*           RFY                                                    0631 01928000
*            (R6,                                                  0631 01929000
*             R7) RSTD;             /*                       @Z40DQSS*/ 01930000
*           R7=TCASUMAX;            /* MAX NUMBER TO PUT IN LINE   0632 01931000
*                                                            @Z40DQSS*/ 01932000
         LR    R7,@10                                              0632 01933000
*           R6=WPLSAVE+EIGHTEEN;    /* WHERE TO PUT NUMBER   @Z40DQSS*/ 01934000
         LA    R6,18                                               0633 01935000
         AL    R6,WPLSAVE                                          0633 01936000
*           CALL CVDCOUNT;          /* MAKE # PRINTABLE AND STORE IT    01937000
*                                      IN THE LINE           @Z40DQSS*/ 01938000
         BAL   @14,CVDCOUNT                                        0634 01939000
*           RFY                                                    0635 01940000
*            (R6,                                                  0635 01941000
*             R7) UNRSTD;           /*                       @Z40DQSS*/ 01942000
*         END;                      /*                       @Z40DQSS*/ 01943000
*     END;                          /*                       @Z40DQSS*/ 01944000
*   ELSE                                                           0638 01945000
*     ;                             /* THE TCAST TABLE DOES NOT EXIST   01946000
*                                                            @ZA29762*/ 01947000
@RF00626 DS    0H                                                  0639 01948000
*/* DELETE CALL TO FILLINE FOR 0 VTAM USERS IF NO TCAS       @ZA29762*/ 01949000
*                                                                  0639 01950000
*   TRCF7TVI=OFF;                   /* MARK TSVTMLIN PROC AS CONTROL    01951000
*                                      LEAVING                       */ 01952000
@RC00626 LA    @10,TRCURRSW                                        0639 01953000
         NI    TRCF7TVI(@10),B'11111101'                           0639 01954000
*   RFY                                                            0640 01955000
*     R14 RSTD;                                                    0640 01956000
*   R14=TSVTSAVE;                   /* RESTORE RETURN ADDR TO DISPLAY   01957000
*                                      PROC                  @Z40DQSS*/ 01958000
         L     R14,TSVTSAVE                                        0641 01959000
*   RFY                                                            0642 01960000
*     R14 UNRSTD;                                                  0642 01961000
*   END TSVTMLIN;                   /*                       @Z40DQSS*/ 01962000
@EL00013 DS    0H                                                  0643 01963000
@EF00013 DS    0H                                                  0643 01964000
@ER00013 BR    @14                                                 0643 01965000
*                                                                  0644 01966000
*/********************************************************************* 01967000
**                F I L L I N E   P R O C E D U R E                   * 01968000
** ENTRY REASON - OPTION 'TS','TS,LIST','A' OR 'A,LIST' WAS REQUESTED * 01969000
**                THE TS LINE  OR THE VTAM LINE ARE TO BE BUILT.      * 01970000
** ENTRY FROM   - TSVTMLIN (TWO PLACES-ONCE FOR THE TS LINE AND ONCE  * 01971000
**                FOR THE VTAM LINE)                                  * 01972000
** EXIT TO      - TSVTMLIN PROCEDURE                                  * 01973000
** FUNCTION     - 1) BUILD THE TS OR VTAM LINE                        * 01974000
**                2) FILL IN THE NUMBER OF USERS                      * 01975000
**                3) RETURN                                           * 01976000
** NOTE         - THIS PROCEDURE IS USED TO BUILD BOTH TS AND VTAM    * 01977000
**                    LINES DEPENDING ON THE INPUT PARAMETERS.        * 01978000
**                FOR BOTH THE TS AND VTAM LINES, THE LINES TO BE     * 01979000
**                    BUILT WILL LOOK DIFFERENT IF THE LINE IS THE    * 01980000
**                    1ST IN A CONNECTING MLWTO OF IF IT IS ANY OF THE* 01981000
**                    REST.  CONNEXT  IS TESTED TO DETERMINE WHAT TYPE* 01982000
**                    OF LINE IS BEING PROCESSED.                     * 01983000
**                WHEN CONNEXT  IS OFF THEN:                          * 01984000
**                A  -----------------------------------------------  * 01985000
**         WPLML     | MESSAGE LENGTH + 4 = | MCS FLAGS =          |  * 01986000
**                   -----------------------------------------------  * 01987000
**                   | '00000 TIME SHARING USERS'                  |  * 01988000
**                   |            OR                               |  * 01989000
**                   | '00000 ACTIVE  00000 MAX VTAM TSO USERS     |  * 01990000
**                B  -----------------------------------------------  * 01991000
**                                                                    * 01992000
**                WHEN CONNEXT  IS ON THEN:                           * 01993000
**                A' -----------------------------------------------  * 01994000
**         WPL       | MESSAGE LENGTH + 4 = | MCS FLAGS =          |  * 01995000
**                   -----------------------------------------------  * 01996000
**                   | '00000 TIME SHARING USERS'                  |  * 01997000
**                   |            OR                               |  * 01998000
**                   | '00000 ACTIVE  00000 MAX VTAM TSO USERS     |  * 01999000
**                   -----------------------------------------------  * 02000000
**         WPLLS01   | LINE TYPE = | AREA ID = | NUMBER OF LINES = |  * 02001000
**                B' -----------------------------------------------  * 02002000
**********************************************************************/ 02003000
*                                                                  0644 02004000
*FILLINE:                                                          0644 02005000
*   PROC(TEXT,TXTLGTH,NUMBER) OPTIONS(NOSAVE,NOSAVEAREA);          0644 02006000
FILLINE  MVC   @PC00014(12),0(@01)                                 0644 02007000
*   DCL                                                            0645 02008000
*     TEXT CHAR(*);                 /* TEXT TO BE PUT IN WPL @Z40DQSS*/ 02009000
*   DCL                                                            0646 02010000
*     TXTLGTH PTR(8);               /* LENGTH OF TEXT FIELD  @Z40DQSS*/ 02011000
*   DCL                                                            0647 02012000
*     NUMBER FIXED(15);             /* VALUE TO BE CONVERTED AND   0647 02013000
*                                      PLACED IN WPL LINE    @Z40DQSS*/ 02014000
*   RFY                                                            0648 02015000
*     R14 RSTD;                     /*                       @Z40DQSS*/ 02016000
*   FILLSAVE=R14;                   /* SAVE RETURN ADDR TO TSVTMLIN     02017000
*                                      PROC                  @Z40DQSS*/ 02018000
         ST    R14,FILLSAVE                                        0649 02019000
*   RFY                                                            0650 02020000
*     R14 UNRSTD;                   /*                       @Z40DQSS*/ 02021000
*   TRCF8FLI=ON;                    /* MARK FILLINE PROC AS IN     0651 02022000
*                                      PROCESSING                    */ 02023000
*   TRCF8FLE=ON;                    /* MARK FILLINE PROC AS ENTERED  */ 02024000
         LA    @10,TRCURRSW                                        0652 02025000
         OI    TRCF8FLI(@10),B'00110000'                           0652 02026000
*   WPL(ONE:TXTLGTH)=TEXT(ONE:TXTLGTH);/* MOVE TEXT TO WPL   @Z40DQSS*/ 02027000
         L     @10,@PC00014+4                                      0653 02028000
         SLR   @09,@09                                             0653 02029000
         IC    @09,TXTLGTH(,@10)                                   0653 02030000
         BCTR  @09,0                                               0653 02031000
         L     @10,@PC00014                                        0653 02032000
         EX    @09,@SM03174                                        0653 02033000
*   IF NUMBER^=ZERO THEN            /* ANY NUMBER TO BE CONVERTED ?     02034000
*                                                            @Z40DQSS*/ 02035000
         L     @10,@PC00014+8                                      0654 02036000
         LH    @10,NUMBER(,@10)                                    0654 02037000
         LTR   @10,@10                                             0654 02038000
         BZ    @RF00654                                            0654 02039000
*     DO;                           /* YES                   @Z40DQSS*/ 02040000
*       RFY                                                        0656 02041000
*        (R6,                                                      0656 02042000
*         R7) RSTD;                 /*                       @Z40DQSS*/ 02043000
*       R7=NUMBER;                  /* # TO CONVERT AND INSERT INTO     02044000
*                                      LINE                  @Z40DQSS*/ 02045000
         LR    R7,@10                                              0657 02046000
*       R6=WPLPTR+FOUR;             /* WHERE TO PUT NUMBER   @Z40DQSS*/ 02047000
         LA    R6,4                                                0658 02048000
         ALR   R6,WPLPTR                                           0658 02049000
*       CALL CVDCOUNT;              /* CONVERT # TO PRINTABLE FORM 0659 02050000
*                                      AND PUT IN THE LINE   @Z40DQSS*/ 02051000
         BAL   @14,CVDCOUNT                                        0659 02052000
*       RFY                                                        0660 02053000
*        (R6,                                                      0660 02054000
*         R7) UNRSTD;               /*                       @Z40DQSS*/ 02055000
*     END;                          /*                       @Z40DQSS*/ 02056000
*   CALL NEXTWPL;                   /* GET ADDR OF NEXT WPL  @Z40DQSS*/ 02057000
@RF00654 BAL   @14,NEXTWPL                                         0662 02058000
*   TRCF8FLI=OFF;                   /* MARK FILLINE PROC AS CONTROL     02059000
*                                      LEAVING                       */ 02060000
         LA    @10,TRCURRSW                                        0663 02061000
         NI    TRCF8FLI(@10),B'11011111'                           0663 02062000
*   RFY                                                            0664 02063000
*     R14 RSTD;                     /*                       @Z40DQSS*/ 02064000
*   R14=FILLSAVE;                   /* RESTORE RETURN ADDR TO      0665 02065000
*                                      TSVTMLIN PROC         @Z40DQSS*/ 02066000
         L     R14,FILLSAVE                                        0665 02067000
*   RFY                                                            0666 02068000
*     R14 UNRSTD;                   /*                       @Z40DQSS*/ 02069000
*   END FILLINE;                    /*                       @Z40DQSS*/ 02070000
@EL00014 DS    0H                                                  0667 02071000
@EF00014 DS    0H                                                  0667 02072000
@ER00014 BR    @14                                                 0667 02073000
*                                                                  0668 02074000
*/********************************************************************* 02075000
**                 L I S T S O   P R O C E D U R E                    * 02076000
** ENTRY REASON - OPTION 'TS,LIST' OR 'A,LIST' IS WANTED AND THE      * 02077000
**                TSO USER COUNT IS NOT ZERO. LIST THE USERIDS OF     * 02078000
**                THE TSO USERS.                                      * 02079000
** ENTRY FROM   - DISPLAY PROCEDURE                                   * 02080000
** EXIT TO      - DISPLAY PROCEDURE                                   * 02081000
** FUNCTION     - 1) SCAN CSCB CHAIN AND FIND TSO USERS CSCB          * 02082000
**                2) GET A WPL   LINE AND BLANK IT OUT.               * 02083000
**                3) INSERT THE TSO USERID INTO THE LINE. THE ID IS   * 02084000
**                   IN CHKEY BUT IF CHKEY IS ZERO OR BLANK THEN A    * 02085000
**                   DEFAULT OF 'STARTING' IS USED.                   * 02086000
**                4) IF USERS' ADDRESS SPACE IS SWAPPED OUT,  @ZA07770* 02087000
**                   MOVE IN 'S' AFTER THE USER ID.           @ZA07770* 02088000
**                5) RETURN                                   @ZA07770* 02089000
** NOTE         - A) THERE ARE TSNAME# OF USER IDS LISTED PER LINE    * 02090000
**                B) LINES ARE DIFFERENT DEPENDING IF THEY ARE THE    * 02091000
**                   1ST LINE OF A CONNECTING MLWTO OR NOT. CONNEXT   * 02092000
**                   IS TESTED TO SEE WHAT TYPE THE LINE IS.          * 02093000
**               WHEN CONNEXT  IS OFF THEN:                           * 02094000
**                 A  ----------------------------------------------- * 02095000
**          WPLML     | MESSAGE LENGTH + 4 = | MCS FLAGS =          | * 02096000
**                    ----------------------------------------------- * 02097000
**                    | 'STARTINGS' ONE TO TSNAM# OF THEM           | * 02098000
**                 B  ----------------------------------------------- * 02099000
**                                                                    * 02100000
**                   WHEN CONNEXT  IS ON THEN:                        * 02101000
**                 A' ----------------------------------------------- * 02102000
**          WPL       | MESSAGE LENGTH + 4 = | MCS FLAGS =          | * 02103000
**                    ----------------------------------------------- * 02104000
**                    | 'STARTINGS' ONE TO TSNAM# OF THEM           | * 02105000
**                    ----------------------------------------------- * 02106000
**          WPLLS01   | LINE TYPE = | AREA ID = | NUMBER OF LINES = | * 02107000
**                 B' ----------------------------------------------- * 02108000
**********************************************************************/ 02109000
*                                                                  0668 02110000
*LISTSO:                                                           0668 02111000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0668 02112000
LISTSO   DS    0H                                                  0669 02113000
*   RFY                                                            0669 02114000
*     R14 RSTD;                                                    0669 02115000
*   LISTSAVE=R14;                   /* SAVE RETURN ADDR TO DISPLAY 0670 02116000
*                                      PROC                          */ 02117000
         ST    R14,LISTSAVE                                        0670 02118000
*   RFY                                                            0671 02119000
*     R14 UNRSTD;                                                  0671 02120000
*   TRCF8LTI=ON;                    /* MARK LISTSO PROC AS IN      0672 02121000
*                                      PROCESSING                    */ 02122000
*   TRCF8LTE=ON;                    /* MARK LISTSO PROC AS ENTERED   */ 02123000
         LA    @10,TRCURRSW                                        0673 02124000
         OI    TRCF8LTI(@10),B'00000011'                           0673 02125000
*   CSCBPTR=BACHN;                  /* BEGIN CSCB CHAIN              */ 02126000
         L     @10,CVTPTR                                          0674 02127000
         L     @10,CVTMSER(,@10)                                   0674 02128000
         L     CSCBPTR,BACHN(,@10)                                 0674 02129000
*   MSGCTR=ZERO;                    /* INIT COUNTER                  */ 02130000
         SLR   MSGCTR,MSGCTR                                       0675 02131000
*   DO WHILE CSCBPTR^=ZERO;                                        0676 02132000
         B     @DE00676                                            0676 02133000
@DL00676 DS    0H                                                  0677 02134000
*     IF CHTRKID='01'X THEN         /* IS THIS A TSO CSCB            */ 02135000
         CLI   CHTRKID(CSCBPTR),X'01'                              0677 02136000
         BNE   @RF00677                                            0677 02137000
*       DO;                         /* YES                           */ 02138000
*         IF MSGCTR=ZERO THEN       /* NEED NEW LINE ?               */ 02139000
         LTR   MSGCTR,MSGCTR                                       0679 02140000
         BNZ   @RF00679                                            0679 02141000
*           DO;                     /* YES                           */ 02142000
*             WPL(ONE:TSLLGTH)=TSLIST(ONE:TSLLGTH);/*        @YM01918*/ 02143000
         SLR   @10,@10                                             0681 02144000
         IC    @10,TSLLGTH                                         0681 02145000
         BCTR  @10,0                                               0681 02146000
         EX    @10,@SM03176                                        0681 02147000
*             WPLSAVE=WPLPTR;       /* SAVE WPL ADDR THAT JUST FILLED*/ 02148000
         ST    WPLPTR,WPLSAVE                                      0682 02149000
*           END;                                                   0683 02150000
*         TSNAMPTR=ADDR(TSID1)+     /* POSITION ON LINE              */ 02151000
*             (LENGTH(TSID1)*MSGCTR);                              0684 02152000
@RF00679 LA    TSNAMPTR,TSID1(,WPLPTR)                             0684 02153000
         LR    @10,MSGCTR                                          0684 02154000
         MH    @10,@CH02881                                        0684 02155000
         ALR   TSNAMPTR,@10                                        0684 02156000
*         IF CHKEY^=BLANKS&CHKEY^=ZEROES THEN                      0685 02157000
         CLC   CHKEY(8,CSCBPTR),@CC02921                           0685 02158000
         BE    @RF00685                                            0685 02159000
         CLC   CHKEY(8,CSCBPTR),ZEROES                             0685 02160000
         BE    @RF00685                                            0685 02161000
*           TSID=CHKEY;             /* TS USERID                     */ 02162000
         MVC   TSID(8,TSNAMPTR),CHKEY(CSCBPTR)                     0686 02163000
*         ELSE                      /* DEFAULT = STARTING            */ 02164000
*           TSID=STARTING;                                         0687 02165000
         B     @RC00685                                            0687 02166000
@RF00685 MVC   TSID(8,TSNAMPTR),STARTING                           0687 02167000
*         ASCBENT=((CHASID*4)+ADDR(ASVTENTY)-4);/* ADDRESS USER'S  0688 02168000
*                                      ASCB                  @ZA07770*/ 02169000
@RC00685 LH    ASCBENT,CHASID(,CSCBPTR)                            0688 02170000
         SLA   ASCBENT,2                                           0688 02171000
         L     @10,CVTPTR                                          0688 02172000
         L     @10,ASVTPTR(,@10)                                   0688 02173000
         LA    @10,ASVTENTY(,@10)                                  0688 02174000
         ALR   ASCBENT,@10                                         0688 02175000
         SL    ASCBENT,@CF00034                                    0688 02176000
*         IF ASCBOUT=ON THEN        /* IF USER IS SWAPPED OUT,     0689 02177000
*                                                            @ZA07770*/ 02178000
         L     @10,ASVTASCB(,ASCBENT)                              0689 02179000
         TM    ASCBOUT(@10),B'00000100'                            0689 02180000
         BNO   @RF00689                                            0689 02181000
*           TSSWPIND=SWAPPED;       /* SHOW IT.              @ZA07770*/ 02182000
         MVI   TSSWPIND(TSNAMPTR),C'S'                             0690 02183000
*         MSGCTR=MSGCTR+ONE;        /* INCREMENT ID COUNT PER LINE   */ 02184000
@RF00689 AL    MSGCTR,@CF00042                                     0691 02185000
*         WPLMLLEN=(LENGTH(TSID1)*MSGCTR)+SIX;/* LENGTH MINOR LINE   */ 02186000
         LA    @10,6                                               0692 02187000
         LR    @09,MSGCTR                                          0692 02188000
         MH    @09,@CH02881                                        0692 02189000
         ALR   @09,@10                                             0692 02190000
         STC   @09,WPLMLLEN(,WPLPTR)                               0692 02191000
*         IF MSGCTR=TSNAM# THEN     /* MAX ON THIS LINE ?            */ 02192000
         CR    MSGCTR,@10                                          0693 02193000
         BNE   @RF00693                                            0693 02194000
*           DO;                     /* YES                           */ 02195000
*             CALL NEXTWPL;         /* GET ADDR OF NEXT WPL          */ 02196000
         BAL   @14,NEXTWPL                                         0695 02197000
*             MSGCTR=ZERO;          /* REINITIALIZE COUNTER          */ 02198000
         SLR   MSGCTR,MSGCTR                                       0696 02199000
*           END;                                                   0697 02200000
*       END;                                                       0698 02201000
@RF00693 DS    0H                                                  0699 02202000
*     CSCBPTR=CHPTR;                /* POINT TO NEXT CSCB, IF 0      */ 02203000
@RF00677 L     CSCBPTR,CHPTR(,CSCBPTR)                             0699 02204000
*   END;                            /* FULL THROUGH, END OF CHAIN    */ 02205000
@DE00676 LTR   CSCBPTR,CSCBPTR                                     0700 02206000
         BNZ   @DL00676                                            0700 02207000
*   IF WPLSAVE=WPLPTR THEN          /* WAS WPLPTR UPDATED?           */ 02208000
         C     WPLPTR,WPLSAVE                                      0701 02209000
         BNE   @RF00701                                            0701 02210000
*     CALL NEXTWPL;                 /* NO SO UPDATE IT               */ 02211000
         BAL   @14,NEXTWPL                                         0702 02212000
*   TRCF8LTI=OFF;                   /* MARK LISTSO PROC AS CONTROL 0703 02213000
*                                      LEAVING                       */ 02214000
@RF00701 LA    @10,TRCURRSW                                        0703 02215000
         NI    TRCF8LTI(@10),B'11111101'                           0703 02216000
*   RFY                                                            0704 02217000
*     R14 RSTD;                                                    0704 02218000
*   R14=LISTSAVE;                   /* RESTSORE RETURN ADDR TO     0705 02219000
*                                      DISPLAY PROC                  */ 02220000
         L     R14,LISTSAVE                                        0705 02221000
*   RFY                                                            0706 02222000
*     R14 UNRSTD;                                                  0706 02223000
*   END LISTSO;                                                    0707 02224000
@EL00015 DS    0H                                                  0707 02225000
@EF00015 DS    0H                                                  0707 02226000
@ER00015 BR    @14                                                 0707 02227000
*                                                                  0708 02228000
*/********************************************************************* 02229000
**              N E X T W P L      P R O C E D U R E                  * 02230000
** ENTRY REASON - GET THE ADDR OF THE NEXT WPL TO BE USED.            * 02231000
** ENTRY FROM   - LISTJOBS PROCEDURE                                  * 02232000
**                FILLINE PROCEDURE                           @ZA07770* 02233000
**                LISTSO PROCEDURE                                    * 02234000
** EXIT  TO     - CALLING PROCEDURE                                   * 02235000
** FUNCTION     - 1) SAVE ADDR OF WPL JUST FILLED IN.                 * 02236000
**                2) INCREASE COUNT OF # LINES IN MLWTO               * 02237000
**                3) IF 1ST LINE IS A CONNECTING MLWTO THEN           * 02238000
**                   A) MARK AS A DATA LINE                           * 02239000
**                   B) GET ADDR OF NEXT WPL                          * 02240000
**                   C) RETURN                                        * 02241000
**                4) IF 2ND LINE IN CONNECTING MLWTO THEN MARK SWITCH * 02242000
**                   TO SHOW DONE WITH THE 2ND LINE.                  * 02243000
**                5) GET ADDR OF NEXT WPL                             * 02244000
**                6) IF CONNECTING MLWTO IS FULL, ADD IT TO THE LIST  * 02245000
**                   AND MARK SWITCH TO SHOW THAT IT IS STARTING WITH * 02246000
**                   A NEW CONNECTING MLWTO.                          * 02247000
**                7) RETURN                                           * 02248000
** NOTE         - MEANINGS OF SWITCHES CONNEXT AND CONEND             * 02249000
**                IN THIS PROCEDURE:                                  * 02250000
**                   CONNEXT = ON : JUST FILLED IN THE FIRST LINE OF  * 02251000
**                                  A GROUP OF MLWTOS                 * 02252000
**                           = OFF: JUST FILLED IN ANY LINE BUT THE   * 02253000
**                                  FIRST IN THE GROUP OF MLWTOS      * 02254000
**                   CONEND  = ON : JUST FILLED IN THE SECOND LINE OF * 02255000
**                                  A GROUP                           * 02256000
**                           = OFF: FILLED IN ANY LINE BUT THE SECOND * 02257000
**                                  IN A GROUP                        * 02258000
**                IN PROCEDURE LASTLINE:                              * 02259000
**                   CONNEXT = ON : THE LINE JUST FILLED IS THE TENTH * 02260000
**                                  LINE OF THE GROUP AND SHOULD BE   * 02261000
**                                  MARKED AS THE END LINE.           * 02262000
**                           = OFF: THE LINE FILLED IN IS ANY LINE BUT* 02263000
**                                  THE TENTH.                        * 02264000
**                   CONEND  = ON : THE LINE FILLED IN IS THE FIRST   * 02265000
**                                  LINE OF A GROUP.                  * 02266000
**                           = OFF: THE LAST LINE IS ANY BUT THE FIRST* 02267000
**                                  LINE OF A GROUP                   * 02268000
**                ANY WHERE ELSE :                                    * 02269000
**                   CONNEXT = ON : THE FIRST LINE OF A GROUP IS NEXT * 02270000
**                                  TO BE FILLED IN.                  * 02271000
**                   CONEND  = ON : THE SECOND LINE OF A GROUP IS NEXT* 02272000
**                                  TO BE FILLED IN.                  * 02273000
**********************************************************************/ 02274000
*                                                                  0708 02275000
*   RFY                                                            0708 02276000
*     R14 RSTD;                                                    0708 02277000
*NEXTWPL:                                                          0709 02278000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0709 02279000
NEXTWPL  DS    0H                                                  0710 02280000
*   NEXTSAVE=R14;                   /* SAVE RETURN ADDR              */ 02281000
         ST    R14,NEXTSAVE                                        0710 02282000
*   RFY                                                            0711 02283000
*     R14 UNRSTD;                                                  0711 02284000
*   TRCFCNXI=ON;                    /* MARK NEXTWPL PROC AS IN     0712 02285000
*                                      PROCESSING                    */ 02286000
*   TRCFCNXE=ON;                    /* MARK NEXTWPL PROC AS ENTERED  */ 02287000
         LA    @10,TRCURRSW                                        0713 02288000
         OI    TRCFCNXI(@10),B'00000011'                           0713 02289000
*   WPLSAVE=WPLPTR;                 /* SAVE ADDR OF LAST WPL FILLED  */ 02290000
         ST    WPLPTR,WPLSAVE                                      0714 02291000
*   COUNT=COUNT+ONE;                /* INCREASE COUNT OF # LINES IN     02292000
*                                      MLWTO                         */ 02293000
         IC    @10,COUNT                                           0715 02294000
         LA    @10,1(,@10)                                         0715 02295000
         STC   @10,COUNT                                           0715 02296000
*   IF CONNEXT=ON THEN              /* PROCESSING 1ST LINE ?         */ 02297000
         TM    CONNEXT,B'01000000'                                 0716 02298000
         BNO   @RF00716                                            0716 02299000
*     DO;                           /* YES                           */ 02300000
*       WPLMCSFJ=ON;                /* MARK AS MLWTO LINE            */ 02301000
         OI    WPLMCSFJ(WPLPTR),B'01000000'                        0718 02302000
*       WPLPTR=WPLPTR+WPLLGH;       /* MOVE POINTER TO FLAGS IN WPL  */ 02303000
         AH    WPLPTR,WPLLGH(,WPLPTR)                              0719 02304000
*       WPLLTFC=ON;                 /* MARK AS DATA LINE             */ 02305000
         OI    WPLLTFC(WPLPTR),B'00100000'                         0720 02306000
*       WPLPTR=WPLPTR+LENGTH(WPLLS01);/* MOVE PTR TO NEXT WPL        */ 02307000
         AL    WPLPTR,@CF00034                                     0721 02308000
*       CONNEXT=OFF;                /* DONE PROCESSING 1ST LINE      */ 02309000
*       CONEND=ON;                  /* START PROCESSING 2ND LINE     */ 02310000
         OI    CONEND,B'00100000'                                  0723 02311000
         NI    CONNEXT,B'10111111'                                 0723 02312000
*     END;                                                         0724 02313000
*   ELSE                                                           0725 02314000
*     DO;                                                          0725 02315000
         B     @RC00716                                            0725 02316000
@RF00716 DS    0H                                                  0726 02317000
*       WPLMLLTC=ON;                /* MARK AS DATA LINE             */ 02318000
         OI    WPLMLLTC(WPLPTR),B'00100000'                        0726 02319000
*       IF CONEND=ON THEN           /* PROCESSING 2ND LINE ?         */ 02320000
         TM    CONEND,B'00100000'                                  0727 02321000
         BNO   @RF00727                                            0727 02322000
*         CONEND=OFF;               /* YES, DONE PROCESSING 2ND LINE */ 02323000
         NI    CONEND,B'11011111'                                  0728 02324000
*       WPLPTR=WPLPTR+WPLMLLEN;     /* POINT TO NEXT LINE IN MLWTO   */ 02325000
@RF00727 SLR   @10,@10                                             0729 02326000
         IC    @10,WPLMLLEN(,WPLPTR)                               0729 02327000
         ALR   WPLPTR,@10                                          0729 02328000
*       IF COUNT=TEN THEN           /* REACH MAX MLWTO LINES YET ?   */ 02329000
         CLI   COUNT,10                                            0730 02330000
         BNE   @RF00730                                            0730 02331000
*         DO;                       /* YES                           */ 02332000
*           CALL FINMLWTO;          /* PUT MLWTO ON LIST TO BE     0732 02333000
*                                      WRITTEN                       */ 02334000
         BAL   @14,FINMLWTO                                        0732 02335000
*           CONNEXT=ON;             /* PROCESSING 1ST LINE           */ 02336000
         OI    CONNEXT,B'01000000'                                 0733 02337000
*         END;                                                     0734 02338000
*     END;                                                         0735 02339000
@RF00730 DS    0H                                                  0736 02340000
*   TRCFCNXI=OFF;                   /* MARK NEXTWPL PROC AS CONTROL     02341000
*                                      LEAVING                       */ 02342000
@RC00716 LA    @10,TRCURRSW                                        0736 02343000
         NI    TRCFCNXI(@10),B'11111101'                           0736 02344000
*   RFY                                                            0737 02345000
*     R14 RSTD;                                                    0737 02346000
*   R14=NEXTSAVE;                   /* RESTORE RETURN ADDR TO CALLING   02347000
*                                      PROC                          */ 02348000
         L     R14,NEXTSAVE                                        0738 02349000
*   RFY                                                            0739 02350000
*     R14 UNRSTD;                                                  0739 02351000
*   END NEXTWPL;                                                   0740 02352000
@EL00016 DS    0H                                                  0740 02353000
@EF00016 DS    0H                                                  0740 02354000
@ER00016 BR    @14                                                 0740 02355000
*                                                                  0741 02356000
*/********************************************************************* 02357000
**               F I N M L W T O   P R O C E D U R E                  * 02358000
** ENTRY REASON - THE TEN LINES OF A MLWTO HAVE BEEN BUILT AND SHOULD * 02359000
**                NOW BE ADDED TO THE MLWTO LINKED LIST.              * 02360000
** ENTRY FROM   - NEXTWPL  PROCEDURE                                  * 02361000
**                LASTLINE PROCEDURE                                  * 02362000
** EXIT  TO     - THE PROCEDURE WHICH CALLED IT                       * 02363000
** FUNCTION     - 1) LINK THE MLWTO TO THE LIST                       * 02364000
**                2) FILL IN THE NUMBER OF LINES IN THIS MLWTO        * 02365000
**                3) FILL IN THE AREA ID OF WHERE TO WRITE THE DISPLAY* 02366000
**                4) RETURN                                           * 02367000
**********************************************************************/ 02368000
*                                                                  0741 02369000
*FINMLWTO:                                                         0741 02370000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @ZA07770*/ 02371000
FINMLWTO DS    0H                                                  0742 02372000
*   RFY                                                            0742 02373000
*     R14 RSTD;                                                    0742 02374000
*   FINMSAVE=R14;                   /* SAVE RETURN ADDR OF CALLING 0743 02375000
*                                      PROC                          */ 02376000
         ST    R14,FINMSAVE                                        0743 02377000
*   RFY                                                            0744 02378000
*     R14 UNRSTD;                                                  0744 02379000
*   TRCF7MLI=ON;                    /* MARK FINMLWTO PROC AS IN    0745 02380000
*                                      PROCESSING                    */ 02381000
*   TRCF7MLE=ON;                    /* MARK FINMLWTO PROC AS ENTERED */ 02382000
         LA    @10,TRCURRSW                                        0746 02383000
         OI    TRCF7MLI(@10),B'00110000'                           0746 02384000
*   IF FIRSTIME=ON THEN             /* IS THIS THE 1ST MLWTO?      0747 02385000
*                                                            @ZA07770*/ 02386000
         TM    FIRSTIME,B'00100000'                                0747 02387000
         BNO   @RF00747                                            0747 02388000
*     DO;                           /* YES.CONTROL LINE 1ST LINE   0748 02389000
*                                                            @ZA07770*/ 02390000
*       TEMPLACE=WPLPTR;            /* SAVE ADDR OF NEXT MLWTO     0749 02391000
*                                                            @ZA07770*/ 02392000
         LR    TEMPLACE,WPLPTR                                     0749 02393000
*       WPLPTR=BUFPTR+LINKLGTH;     /* POINT TO BEGINING OF BUFF   0750 02394000
*                                                            @ZA07770*/ 02395000
         L     @10,BUFPTR                                          0750 02396000
         LA    @03,4                                               0750 02397000
         LR    WPLPTR,@10                                          0750 02398000
         ALR   WPLPTR,@03                                          0750 02399000
*       WPLPTR=WPLPTR+WPLLGH+FOUR;  /* MOVE TO END 1ST LINE  @ZA07770*/ 02400000
         LR    @09,WPLPTR                                          0751 02401000
         AH    @09,WPLLGH(,WPLPTR)                                 0751 02402000
         ALR   @09,@03                                             0751 02403000
         LR    WPLPTR,@09                                          0751 02404000
*       LASTNODE=BUFPTR;            /* LAST NODE ON LIST IS BEG OF 0752 02405000
*                                      BUFF                  @ZA07770*/ 02406000
         ST    @10,LASTNODE                                        0752 02407000
*       FIRSTIME=OFF;               /* NOT FIRST TIME ANYMORE      0753 02408000
*                                                            @ZA07770*/ 02409000
         NI    FIRSTIME,B'11011111'                                0753 02410000
*     END;                          /*                       @ZA07770*/ 02411000
*   ELSE                            /* NO, 1ST LINE IS DATA LINE   0755 02412000
*                                                            @ZA07770*/ 02413000
*     DO;                           /*                       @ZA07770*/ 02414000
         B     @RC00747                                            0755 02415000
@RF00747 DS    0H                                                  0756 02416000
*       TEMPLACE=WPLPTR;            /* SAVE ADDR OF WHERE TO START 0756 02417000
*                                      NEXT MLWTO            @ZA07770*/ 02418000
         LR    TEMPLACE,WPLPTR                                     0756 02419000
*       WPLPTR=NEXTNODE+LINKLGTH;   /* POINT WPLPTR TO BEGINING OF 0757 02420000
*                                      FIRST LINE IN THIS MLWTO    0757 02421000
*                                                            @ZA07770*/ 02422000
         L     @10,NEXTNODE                                        0757 02423000
         LA    WPLPTR,4                                            0757 02424000
         ALR   WPLPTR,@10                                          0757 02425000
*       LASTNODE->LINK=NEXTNODE;    /* LINK THIS NODE TO END OF LIST    02426000
*                                                            @ZA07770*/ 02427000
         L     @09,LASTNODE                                        0758 02428000
         ST    @10,LINK(,@09)                                      0758 02429000
*       LASTNODE=NEXTNODE;          /* MAKE LASTNODE POINT TO THIS 0759 02430000
*                                      NODE                  @ZA07770*/ 02431000
         ST    @10,LASTNODE                                        0759 02432000
*       WPLPTR=WPLPTR+WPLLGH;       /* MOVE TO END OF 1ST LINE     0760 02433000
*                                                            @ZA07770*/ 02434000
         AH    WPLPTR,WPLLGH(,WPLPTR)                              0760 02435000
*     END;                          /*                       @ZA07770*/ 02436000
*   LASTNODE->LINK=ZERO;            /* MARK THIS NODE AS LAST      0762 02437000
*                                                            @ZA07770*/ 02438000
@RC00747 L     @10,LASTNODE                                        0762 02439000
         SLR   @09,@09                                             0762 02440000
         ST    @09,LINK(,@10)                                      0762 02441000
*   WPLLINES=COUNT;                 /* NUMBER OF LINES IN MLWTO    0763 02442000
*                                                            @ZA07770*/ 02443000
         IC    @10,COUNT                                           0763 02444000
         STC   @10,WPLLINES(,WPLPTR)                               0763 02445000
*   WPLAREA=TASKCSCB->CHARID;       /* GET AREA ID FOR CRT(Z=PAPER)     02446000
*                                                            @ZA07770*/ 02447000
         L     @10,TASKCSCB                                        0764 02448000
         MVC   WPLAREA(1,WPLPTR),CHARID(@10)                       0764 02449000
*   NEXTNODE=TEMPLACE;              /* ADDR OF LINK OF NEXT NODE   0765 02450000
*                                                            @ZA07770*/ 02451000
         ST    TEMPLACE,NEXTNODE                                   0765 02452000
*   WPLPTR=TEMPLACE+LINKLGTH;       /* SKIP OVER LINK FIELD. POINT TO   02453000
*                                      BEGINING OF NEXT NODE TO FILL.   02454000
*                                                            @ZA07770*/ 02455000
         LA    WPLPTR,4                                            0766 02456000
         ALR   WPLPTR,TEMPLACE                                     0766 02457000
*   COUNT=ZERO;                     /* ZERO COUNT OF LINES FOR NEXT     02458000
*                                      MLWTO                 @ZA07770*/ 02459000
         MVI   COUNT,X'00'                                         0767 02460000
*   TRCF7MLI=OFF;                   /* MARK FINMLWTO PROC AS CONTROL    02461000
*                                      LEAVING                       */ 02462000
         LA    @10,TRCURRSW                                        0768 02463000
         NI    TRCF7MLI(@10),B'11011111'                           0768 02464000
*   RFY                                                            0769 02465000
*     R14 RSTD;                                                    0769 02466000
*   R14=FINMSAVE;                   /* RESTORE RETURN ADDR OF CALLING   02467000
*                                      PROC                          */ 02468000
         L     R14,FINMSAVE                                        0770 02469000
*   RFY                                                            0771 02470000
*     R14 UNRSTD;                                                  0771 02471000
*   END FINMLWTO;                   /*                       @ZA07770*/ 02472000
@EL00017 DS    0H                                                  0772 02473000
@EF00017 DS    0H                                                  0772 02474000
@ER00017 BR    @14                                                 0772 02475000
*                                                                  0773 02476000
*/********************************************************************* 02477000
**               L A S T L I N E   P R O C E D U R E                  * 02478000
** ENTRY REASON - THE LAST LINE OF THE DISPLAY HAS BEEN BUILT SO FILL * 02479000
**                IN THE INFORMATION FOR THE LAST LINE OF A MLWTO.    * 02480000
** ENTRY FROM   - DISPLAY PROCEDURE                                   * 02481000
** EXIT  TO     - DISPLAY PROCEDURE                                   * 02482000
** FUNCTION     - 1) BACK UP AND MARK THE LAST LINE OF THE MLWTO AS   * 02483000
**                   THE END LINE.                                    * 02484000
**                2) PUT THE LAST MLWTO ON THE LIST                   * 02485000
**                3) RETURN                                           * 02486000
** NOTE         - IF CONNEXT  IS ON THEN NO NEED TO ADD THE CONNECTING* 02487000
**                MLWTO TO THE LIST SINCE IT HAS ALREADY BEEN ADDED.  * 02488000
**                                                                    * 02489000
**                FOR AN EXPLANATION OF SWITCHES CONNEXT AND CONEND   * 02490000
**                SEE THE PROLOG OF PROCEDURE NEXTWPL.                * 02491000
**********************************************************************/ 02492000
*                                                                  0773 02493000
*LASTLINE:                                                         0773 02494000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0773 02495000
LASTLINE DS    0H                                                  0774 02496000
*   RFY                                                            0774 02497000
*     R14 RSTD;                     /*                       @ZA07770*/ 02498000
*   LASTSAVE=R14;                   /* SAVE RETURN ADDR TO DISPLAY 0775 02499000
*                                      PROC                  @ZA07770*/ 02500000
         ST    R14,LASTSAVE                                        0775 02501000
*   RFY                                                            0776 02502000
*     R14 UNRSTD;                   /*                       @ZA07770*/ 02503000
*   TRCF9LLI=ON;                    /* MARK LASTLINE PROC AS IN    0777 02504000
*                                      PROCESSING                    */ 02505000
*   TRCF9LLE=ON;                    /* MARK LASTLINE PROC AS ENTERED */ 02506000
         LA    @10,TRCURRSW                                        0778 02507000
         OI    TRCF9LLI(@10),B'00110000'                           0778 02508000
*   TEMPLACE=WPLPTR;                /* SAVE NEXT WPL LINE ADDR     0779 02509000
*                                                            @ZA07770*/ 02510000
         LR    TEMPLACE,WPLPTR                                     0779 02511000
*   WPLPTR=WPLSAVE;                 /* GET ADDR OF LAST LINE FILLED     02512000
*                                      IN                    @ZA07770*/ 02513000
         L     WPLPTR,WPLSAVE                                      0780 02514000
*   IF CONEND=ON THEN               /* 1ST LINE OF MLWTO WAS LAST? 0781 02515000
*                                                            @ZA07770*/ 02516000
         TM    CONEND,B'00100000'                                  0781 02517000
         BNO   @RF00781                                            0781 02518000
*     DO;                           /* YES                   @ZA07770*/ 02519000
*       WPLPTR=WPLPTR+WPLLGH;       /* ADJUST THE POINTER    @ZA07770*/ 02520000
         AH    WPLPTR,WPLLGH(,WPLPTR)                              0783 02521000
*       WPLLTFD=ON;                 /* MARK AS ENDLINE       @ZA07770*/ 02522000
         OI    WPLLTFD(WPLPTR),B'00010000'                         0784 02523000
*     END;                          /*                       @ZA07770*/ 02524000
*   ELSE                            /* MORE THAN ONE LINE IN MLWTO 0786 02525000
*                                                            @ZA07770*/ 02526000
*     WPLMLLTD=ON;                  /* MARK AS ENDLINE       @ZA07770*/ 02527000
         B     @RC00781                                            0786 02528000
@RF00781 OI    WPLMLLTD(WPLPTR),B'00010000'                        0786 02529000
*   WPLPTR=TEMPLACE;                /* RESTORE POINTER       @ZA07770*/ 02530000
@RC00781 LR    WPLPTR,TEMPLACE                                     0787 02531000
*   IF CONNEXT=OFF THEN             /* HAS MLWTO BEEN ADDED TO THE 0788 02532000
*                                      LIST ?                        */ 02533000
         TM    CONNEXT,B'01000000'                                 0788 02534000
         BNZ   @RF00788                                            0788 02535000
*     CALL FINMLWTO;                /* NO, SO ADD THE MLWTO TO THE 0789 02536000
*                                      LIST                  @ZA07770*/ 02537000
         BAL   @14,FINMLWTO                                        0789 02538000
*   TRCF9LLI=OFF;                   /* MARK LASTLINE PROC AS CONTROL    02539000
*                                      LEAVING                       */ 02540000
@RF00788 LA    @10,TRCURRSW                                        0790 02541000
         NI    TRCF9LLI(@10),B'11011111'                           0790 02542000
*   RFY                                                            0791 02543000
*     R14 RSTD;                     /*                       @ZA07770*/ 02544000
*   R14=LASTSAVE;                   /* RESTORE RETURN ADDR TO DISPLAY   02545000
*                                      PROC                  @ZA07770*/ 02546000
         L     R14,LASTSAVE                                        0792 02547000
*   RFY                                                            0793 02548000
*     R14 UNRSTD;                   /*                       @ZA07770*/ 02549000
*   END LASTLINE;                                                  0794 02550000
@EL00018 DS    0H                                                  0794 02551000
@EF00018 DS    0H                                                  0794 02552000
@ER00018 BR    @14                                                 0794 02553000
*                                                                  0795 02554000
*/********************************************************************* 02555000
**                F R E E B U F   P R O C E D U R E                   * 02556000
** ENTRY REASON - TO FREE THE MLWTO LIST BUFFER                       * 02557000
** EXIT  TO     - DISPLAY PROCEDURE                                   * 02558000
** FUNCTION     - 1) FREE THE CORE GOTTEN FOR THE MLWTO LIST BUFFER   * 02559000
**                2) RETURN                                           * 02560000
**********************************************************************/ 02561000
*                                                                  0795 02562000
*   RFY                                                            0795 02563000
*    (R1,                                                          0795 02564000
*     R2,                                                          0795 02565000
*     R14) RSTD;                                                   0795 02566000
*FREEBUF:                                                          0796 02567000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0796 02568000
FREEBUF  DS    0H                                                  0797 02569000
*   FREESAVE=R14;                   /* SAVE RETURN ADDR TO DISPLAY 0797 02570000
*                                      PROC                          */ 02571000
         ST    R14,FREESAVE                                        0797 02572000
*   RFY                                                            0798 02573000
*     R14 UNRSTD;                                                  0798 02574000
*   TRCFBFBI=ON;                    /* MARK FREEBUF PROC AS IN     0799 02575000
*                                      PROCESSING                    */ 02576000
*   TRCFBFBE=ON;                    /* MARK FREEBUF PROC AS ENTERED  */ 02577000
         LA    @10,TRCURRSW                                        0800 02578000
         OI    TRCFBFBI(@10),B'00110000'                           0800 02579000
*   R2=COREBYTE;                    /* PLACE LENGTH IN R2            */ 02580000
         L     R2,COREBYTE                                         0801 02581000
*   R1=BUFPTR;                      /* PLACE BEGINNING ADDRESS IN R1 */ 02582000
         L     R1,BUFPTR                                           0802 02583000
*   GEN SETS(R0,R1) REFS(R2)(FREEMAIN R,SP=229,LV=(2),A=(1));      0803 02584000
         FREEMAIN R,SP=229,LV=(2),A=(1)                                 02585000
*   TRCFBFBI=OFF;                   /* MARK FREEBUF PROC AS CONTROL     02586000
*                                      LEAVING                       */ 02587000
         LA    @10,TRCURRSW                                        0804 02588000
         NI    TRCFBFBI(@10),B'11011111'                           0804 02589000
*   RFY                                                            0805 02590000
*     R14 RSTD;                                                    0805 02591000
*   R14=FREESAVE;                   /* RESTORE RETURN ADDR OF DISPLAY   02592000
*                                      PROC                          */ 02593000
         L     R14,FREESAVE                                        0806 02594000
*   RFY                                                            0807 02595000
*    (R1,                                                          0807 02596000
*     R2,                                                          0807 02597000
*     R14) UNRSTD;                                                 0807 02598000
*   END FREEBUF;                                                   0808 02599000
@EL00019 DS    0H                                                  0808 02600000
@EF00019 DS    0H                                                  0808 02601000
@ER00019 BR    @14                                                 0808 02602000
*                                                                  0809 02603000
*/********************************************************************* 02604000
**                 D Q C S C B   P R O C E D U R E                    * 02605000
** ENTRY REASON - TO DEQUEUE OFF THE CSCB CHAIN                       * 02606000
** ENTRY FROM   - DISPLAY PROCEDURE (AFTER THE MLWTO BUFFER IS FULL)  * 02607000
**                ISSUEMSG PROCEDURE(IF GETMAIN FAILED FOR THE BUFFER)* 02608000
** EXIT TO      - CALLING PROCEDURE                                   * 02609000
** FUNCTION     - 1) DEQUEUE OFF THE CSCB CHAIN                       * 02610000
**                2) RESTORE CSCBPTR TO THIS TASK'S CSCB ADDRESS      * 02611000
**                3) RETURN                                           * 02612000
**********************************************************************/ 02613000
*                                                                  0809 02614000
*   RFY                                                            0809 02615000
*    (R1,                                                          0809 02616000
*     R14) RSTD;                                                   0809 02617000
*DQCSCB:                                                           0810 02618000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0810 02619000
DQCSCB   DS    0H                                                  0811 02620000
*   DQCBSAVE=R14;                   /* SAVE RETURN ADDRESS OF CALLING   02621000
*                                      PROC                          */ 02622000
         ST    R14,DQCBSAVE                                        0811 02623000
*   RFY                                                            0812 02624000
*     R14 UNRSTD;                                                  0812 02625000
*   TRCF9DQI=ON;                    /* MARK DQCSCB PROC AS IN      0813 02626000
*                                      PROCESSING                    */ 02627000
*   TRCF9DQE=ON;                    /* MARK DQCSCB PROC AS ENTERED   */ 02628000
         LA    @10,TRCURRSW                                        0814 02629000
         OI    TRCF9DQI(@10),B'00000011'                           0814 02630000
*   ENQDEQL(1:DLEN)=DDEQ(1:DLEN);   /* BUILD DEQ LIST                */ 02631000
         LH    @10,DLEN                                            0815 02632000
         BCTR  @10,0                                               0815 02633000
         EX    @10,@SM03178                                        0815 02634000
*   R1=ADDR(ENQDEQL);               /* WITH REG 1 ADDRESSING IT      */ 02635000
         LA    R1,ENQDEQL                                          0816 02636000
*   GEN(DEQ ,MF=(E,(1)));           /* DEQ FROM THE CSCB CHAIN       */ 02637000
         DEQ ,MF=(E,(1))                                                02638000
*   CSCBPTR=CSCBPASS;               /* RESTORE THIS TASK'S CSCB    0818 02639000
*                                      ADDRESS                       */ 02640000
         LA    @10,STAELIST                                        0818 02641000
         L     CSCBPTR,CSCBPASS(,@10)                              0818 02642000
*   TRCF9DQI=OFF;                   /* MARK FLAG AS CONTROL LEAVING     02643000
*                                      DQCSCB PROC                   */ 02644000
         LA    @10,TRCURRSW                                        0819 02645000
         NI    TRCF9DQI(@10),B'11111101'                           0819 02646000
*   RFY                                                            0820 02647000
*     R14 RSTD;                                                    0820 02648000
*   R14=DQCBSAVE;                   /* RESTORE RETURN ADDR OF CALLING   02649000
*                                      PROC                          */ 02650000
         L     R14,DQCBSAVE                                        0821 02651000
*   RFY                                                            0822 02652000
*    (R1,                                                          0822 02653000
*     R14) UNRSTD;                                                 0822 02654000
*   END DQCSCB;                                                    0823 02655000
@EL00020 DS    0H                                                  0823 02656000
@EF00020 DS    0H                                                  0823 02657000
@ER00020 BR    @14                                                 0823 02658000
*                                                                  0824 02659000
*/********************************************************************* 02660000
**                 C A L L 8 0 1   P R O C E D U R E                  * 02661000
** ENTRY REASON - THE MLWTO LIST IS FINISHED AND THEY SHOULD BE ISSUED* 02662000
** ENTRY FROM   - DISPLAY PROCEDURE                                   * 02663000
** EXIT  TO     - DISPLAY PROCEDURE                                   * 02664000
** FUNCTION     - 1) POINT TO BEGINNING OF BUFFER                     * 02665000
**                2) LOOP THROUGH LIST UNTIL THE MLWTO IS ISSUED.     * 02666000
**                3) IF A NON-ZERO RETURN CODE FROM IEECB801 IS FOUND * 02667000
**                   THEN THE LOOPING IS STOPPED.                     * 02668000
**                4) RETURN                                           * 02669000
**********************************************************************/ 02670000
*                                                                  0824 02671000
*CALL801:                                                          0824 02672000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0824 02673000
CALL801  DS    0H                                                  0825 02674000
*   RFY                                                            0825 02675000
*    (R1,                                                          0825 02676000
*     R14) RSTD;                                                   0825 02677000
*   CALLSAVE=R14;                   /* SAVE RETURN ADDR TO DISPLAY 0826 02678000
*                                      PROC                  @ZA07770*/ 02679000
         ST    R14,CALLSAVE                                        0826 02680000
*   RFY                                                            0827 02681000
*     R14 UNRSTD;                                                  0827 02682000
*   TRCFACLI=ON;                    /* MARK CALL801 PROC AS IN     0828 02683000
*                                      PROCESSING                    */ 02684000
*   TRCFACLE=ON;                    /* MARK CALL801 PROC AS ENTERED  */ 02685000
         LA    @10,TRCURRSW                                        0829 02686000
         OI    TRCFACLI(@10),B'00110000'                           0829 02687000
*   R1=ADDR(PARMLIST);              /* PARMLIST TO IEECB801          */ 02688000
         LA    R1,PARMLIST                                         0830 02689000
*   LINKPTR=BUFPTR;                 /* POINT TO 1ST MLWTO IN BUF   0831 02690000
*                                                            @ZA07770*/ 02691000
         L     LINKPTR,BUFPTR                                      0831 02692000
*   FIRSTIME=ON;                    /* 1ST MLWTO HAS A CONTROL LINE.    02693000
*                                      IEECB801 WILL TURN FLAG OFF 0832 02694000
*                                                            @ZA07770*/ 02695000
*                                                                  0832 02696000
         OI    FIRSTIME,B'00100000'                                0832 02697000
*   /*****************************************************************/ 02698000
*   /*                                                               */ 02699000
*   /* WPLPTR POINTS TO THE BEGINING OF THE MLWTO. LINKPTR HAS THE   */ 02700000
*   /* ADDR OF THE LINK FIELD OF THE MLWTO. IF LINKPTR=ZERO THEN THE */ 02701000
*   /* LIST OF MLWTOS HAS BEEN WRITTEN OR AN ERROR WAS FOUND BY      */ 02702000
*   /* IEECB801 AND NO MORE MLWTOS SHOULD BE WRITTEN.                */ 02703000
*   /*                                                               */ 02704000
*   /*****************************************************************/ 02705000
*                                                                  0833 02706000
*   DO WHILE LINKPTR^=ZERO;         /*                       @ZA07770*/ 02707000
         B     @DE00833                                            0833 02708000
@DL00833 DS    0H                                                  0834 02709000
*     WPLPTR=LINKPTR+LINKLGTH;      /* POINT TO NEXT MLWTO TO ISSUE  */ 02710000
         LA    WPLPTR,4                                            0834 02711000
         ALR   WPLPTR,LINKPTR                                      0834 02712000
*     MESSADDR=WPLPTR;              /* ADDR OF MLWTO TO WRITE      0835 02713000
*                                                            @ZA07770*/ 02714000
         ST    WPLPTR,MESSADDR                                     0835 02715000
*     TRCFACBI=ON;                  /* MARK IEECB801 AS IN PROCESSING*/ 02716000
*     TRCFACBE=ON;                  /* MARK IEECB801 AS ENTERED      */ 02717000
         LA    @10,TRCURRSW                                        0837 02718000
         OI    TRCFACBI(@10),B'00000011'                           0837 02719000
*     CALL IEECB801;                /* WRITE MLWTO           @ZA07770*/ 02720000
         L     @15,@CV02746                                        0838 02721000
         BALR  @14,@15                                             0838 02722000
*     TRCFACBI=OFF;                 /* MARK IEECB801 AS CONTROL HAS     02723000
*                                      COME BACK                     */ 02724000
         LA    @10,TRCURRSW                                        0839 02725000
         NI    TRCFACBI(@10),B'11111101'                           0839 02726000
*     IF RETCODE=ZERO THEN          /* ANY ERRORS FOUND      @ZA07770*/ 02727000
         L     @10,RETCODE                                         0840 02728000
         LTR   @10,@10                                             0840 02729000
         BNZ   @RF00840                                            0840 02730000
*       LINKPTR=LINKPTR->LINK;      /* NO,POINT TO NEXT NODE @ZA07770*/ 02731000
         L     LINKPTR,LINK(,LINKPTR)                              0841 02732000
*     ELSE                          /* YES,THERE WAS AN ERROR      0842 02733000
*                                                            @ZA07770*/ 02734000
*       LINKPTR=ZERO;               /* ERROR SO STOP WRITING @ZA07770*/ 02735000
         B     @RC00840                                            0842 02736000
@RF00840 SLR   LINKPTR,LINKPTR                                     0842 02737000
*   END;                            /* END OF DO WHILE       @ZA07770*/ 02738000
@RC00840 DS    0H                                                  0843 02739000
@DE00833 LTR   LINKPTR,LINKPTR                                     0843 02740000
         BNZ   @DL00833                                            0843 02741000
*   TRCFACLI=OFF;                   /* MARK CALL801 PROC AS CONTROL     02742000
*                                      LEAVING                       */ 02743000
         LA    @10,TRCURRSW                                        0844 02744000
         NI    TRCFACLI(@10),B'11011111'                           0844 02745000
*   RFY                                                            0845 02746000
*     R14 RSTD;                                                    0845 02747000
*   R14=CALLSAVE;                   /* RESTORE RETURN ADDR OF DISPLAY   02748000
*                                      PROC                          */ 02749000
         L     R14,CALLSAVE                                        0846 02750000
*   RFY                                                            0847 02751000
*    (R1,                                                          0847 02752000
*     R14) UNRSTD;                                                 0847 02753000
*   END CALL801;                                                   0848 02754000
@EL00021 DS    0H                                                  0848 02755000
@EF00021 DS    0H                                                  0848 02756000
@ER00021 BR    @14                                                 0848 02757000
*                                                                  0849 02758000
*/********************************************************************* 02759000
**                   W A I T   P R O C E D U R E                      * 02760000
** ENTRY REASON - WAIT ON ECBS UNTIL NEXT TRACK UPDATE OR A STOP TRACK* 02761000
**                IS WANTED                                           * 02762000
** ENTRY FROM   - 1) TOP PROCEDURE                                    * 02763000
**                   A) A DISPLAY FOR A TRACK COMMAND HAS JUST        * 02764000
**                      FINISHED SO WAIT UNTIL THE TIME INTERVAL FOR  * 02765000
**                      THE NEXT UPDATE IS UP                         * 02766000
**                   B) IN HOLD MODE SO DISPLAY MAY NOT BE UPDATED    * 02767000
**                   C) PREVIOUS DISPLAY NOT FINISHED YET SO NO       * 02768000
**                      UPDATE IS POSSIBLE                            * 02769000
** EXIT  TO     - LABEL AWAKE IN THE TOP PROCEDURE                    * 02770000
** FUNCTION     - 1) WAIT ON ONE OF TWO ECBS                          * 02771000
**                   A) ONE IS THE UPDATE TIME INTERVAL ECB           * 02772000
**                   B) THE OTHER IS POSTED IF A STOPTR COMMAND IS    * 02773000
**                      ISSUED, OR IF A K D,U COMMAND WAS ISSUED OR   * 02774000
**                      IF DIDOCS OR CONSOLE CLOSE WANTS THE TRACK    * 02775000
**                      TO STOP.                                      * 02776000
**                2) RETURN TO LABEL AWAKE IN THE TOP PROCEDURE TO SEE* 02777000
**                   WHY AN ECB WAS POSTED                            * 02778000
**********************************************************************/ 02779000
*                                                                  0849 02780000
*   RFY                                                            0849 02781000
*     R1 RSTD;                                                     0849 02782000
*WAIT:                                                             0850 02783000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0850 02784000
WAIT     DS    0H                                                  0851 02785000
*   TRCF3WTI=ON;                    /* MARK WAIT PROC AS IN        0851 02786000
*                                      PROCESSING                    */ 02787000
*   TRCF3WTE=ON;                    /* MARK WAIT PROC AS ENTERED     */ 02788000
         LA    @10,TRCURRSW                                        0852 02789000
         OI    TRCF3WTI(@10),B'00110000'                           0852 02790000
*   ECB1=ADDR(STIMECB);             /* BUILD ECB LIST                */ 02791000
         LA    @10,STAELIST                                        0853 02792000
         LA    @10,STIMECB(,@10)                                   0853 02793000
         ST    @10,ECB1                                            0853 02794000
*   ECB2=ADDR(CHCECB);                                             0854 02795000
         LA    @10,CHCECB(,CSCBPTR)                                0854 02796000
         ST    @10,ECB2                                            0854 02797000
*   ECB2=ECB2|HIMASK;               /* MARK LAST ECB ON LIST         */ 02798000
         O     @10,HIMASK                                          0855 02799000
         ST    @10,ECB2                                            0855 02800000
*   R1=ADDR(ECBS);                  /* ADDR OF LIST IN R1            */ 02801000
         LA    R1,ECBS                                             0856 02802000
*   GEN SETS(R0,R1)(WAIT 1,ECBLIST=(1));/* WAIT TO BE POSTED         */ 02803000
         WAIT 1,ECBLIST=(1)                                             02804000
*   TRCF3WTI=OFF;                   /* MARK WAIT PROC AS CONTROL   0858 02805000
*                                      LEAVING                       */ 02806000
         LA    @10,TRCURRSW                                        0858 02807000
         NI    TRCF3WTI(@10),B'11011111'                           0858 02808000
*   TRPREVSW=TRCURRSW;              /* SAVE TRACE SWITCHES           */ 02809000
         MVC   TRPREVSW(14),TRCURRSW                               0859 02810000
*   TRCFLAGS=TRCFLAGS&&TRCFLAGS;    /* RESET CHANGABLE SWITCHES      */ 02811000
         XC    TRCFLAGS(12,@10),TRCFLAGS(@10)                      0860 02812000
*   RETURN TO AWAKE;                /* FIND OUT WHY POSTED AND ACT 0861 02813000
*                                      ACCORDINGLY                   */ 02814000
         LA    @14,AWAKE                                           0861 02815000
@EL00022 DS    0H                                                  0861 02816000
@EF00022 DS    0H                                                  0861 02817000
@ER00022 BR    @14                                                 0861 02818000
*   RFY                                                            0862 02819000
*     R1 UNRSTD;                                                   0862 02820000
*   END WAIT;                                                      0863 02821000
*                                                                  0864 02822000
*/********************************************************************* 02823000
**               T I M E S N A P   P R O C E D U R E                  * 02824000
** ENTRY REASON - TO GET A TIME STAMP FOR AN OUTPUT LINE              * 02825000
** ENTRY FROM   - 1) TRACK PROCEDURE (FILL IN FOR NO TR UPDATE MSG)   * 02826000
**                2) HEADERLN PROCEDURE (FILL IN FOR INSUFFICIENT     * 02827000
**                   STORAGE MESSAGE)                                 * 02828000
**                3) HEADERLN PROCEDURE (FILL IN FOR DISPLAY HEADER)  * 02829000
** EXIT TO      - 1) CALLING PROCEDURE IF NO ERROR FOUND              * 02830000
**                2) IF AN ERROR IS FOUND, CONTROL IS PASSED TO       * 02831000
**                   LABEL NOTIME IN THIS PROCEDURE.                  * 02832000
** FUNCTION     - 1) ISSUE THE TIME MACRO                             * 02833000
**                   A) IF IN ERROR, GO TO LABEL NOTIME.              * 02834000
**                2) UNPACK THE TIME                                  * 02835000
**                3) INSERT THE TIME INTO MESSAGE TEXT                * 02836000
**                   A) IEE102I HH.MM.SS YY.DDD ACTIVITY              * 02837000
**                      IEE921I NO TR UPDATE AT HH.MM.SS YY.DDD       * 02838000
**                   B) THE BUFFERS CONTAINING THE ABOVE MESSAGES ARE * 02839000
**                      MAPPED BY DSECT 'CLWTO' BASED ON WPLPTR.      * 02840000
**                      FOR MSG IEE921I, WPLPTR MUST BE ADVANCED 16   * 02841000
**                      BYTES TO MAP IN HRS, MIN, SEC  PROPERLY       * 02842000
**                4) UNPACK THE DATE                                  * 02843000
**                5) INSERT THE DATE INTO THE MESSAGE TEXT            * 02844000
**********************************************************************/ 02845000
*                                                                  0864 02846000
*   RFY                                                            0864 02847000
*     R14 RSTD;                                                    0864 02848000
*TIMESNAP:                                                         0865 02849000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0865 02850000
TIMESNAP DS    0H                                                  0866 02851000
*   TIMESAVE=R14;                   /* SAVE RETURN ADDR OF CALLING 0866 02852000
*                                      PROCEDURE                     */ 02853000
         ST    R14,TIMESAVE                                        0866 02854000
*   RFY                                                            0867 02855000
*     R14 UNRSTD;                                                  0867 02856000
*   TRCF2TMI=ON;                    /* MARK TIMESNAP PROC AS IN    0868 02857000
*                                      PROCESSING                    */ 02858000
*   TRCF2TME=ON;                    /* MARK TIMESNAP PROC AS ENTERED */ 02859000
         LA    @10,TRCURRSW                                        0869 02860000
         OI    TRCF2TMI(@10),B'00000011'                           0869 02861000
*   GEN SETS(R0,R1,R15) FLOWS(NOTIME)(TIME DEC,ERRET=NOTIME);      0870 02862000
         TIME DEC,ERRET=NOTIME                                          02863000
*   RFY                                                            0871 02864000
*    (R0,                                                          0871 02865000
*     R1) RSTD;                                                    0871 02866000
*   SAVETIME=R0;                    /* SAVE TIME STAMP VALUE         */ 02867000
         STCM  R0,15,SAVETIME                                      0872 02868000
*   RESPECIFY                                                      0873 02869000
*    (R0) UNRESTRICTED;                                            0873 02870000
*   UNPK(UNPACK(1:8),SAVETIME(1:4));/* UNPACK TIME VALUE             */ 02871000
         UNPK  UNPACK(8),SAVETIME(4)                               0874 02872000
*   HRS=HOURS;                      /* FILL IN HOURS                 */ 02873000
         MVC   HRS(2,WPLPTR),HOURS                                 0875 02874000
*   MIN=MINUTES;                    /* FILL IN MINUTES               */ 02875000
         MVC   MIN(2,WPLPTR),MINUTES                               0876 02876000
*   SEC=SECONDS;                    /* FILL IN SECONDS               */ 02877000
         MVC   SEC(2,WPLPTR),SECONDS                               0877 02878000
*   SAVEDATE=R1;                    /* SAVE THE PACKED FORM OF THE 0878 02879000
*                                      DATE                          */ 02880000
         STCM  R1,15,SAVEDATE                                      0878 02881000
*   RFY                                                            0879 02882000
*     R1 UNRSTD;                                                   0879 02883000
*   UNPK(UNPACK(1:8),DATEAREA(2:5));/* UNPACK THE DATE               */ 02884000
         UNPK  UNPACK(8),DATEAREA+1(4)                             0880 02885000
*   CLYEAR=YEAR;                    /* PUT THE YEAR IN THE MESSAGE 0881 02886000
*                                      TEXT                          */ 02887000
         MVC   CLYEAR(2,WPLPTR),YEAR                               0881 02888000
*   CLDAY=DAY;                      /* PUT THE DAY IN THE MESSAGE  0882 02889000
*                                      TEXT                          */ 02890000
         MVC   CLDAY(3,WPLPTR),DAY                                 0882 02891000
*NOTIME:                                                           0883 02892000
*   ;                                                              0883 02893000
NOTIME   DS    0H                                                  0884 02894000
*   TRCF2TMI=OFF;                   /* MARK TIMESNAP PROC AS CONTROL    02895000
*                                      LEAVING                       */ 02896000
         LA    @10,TRCURRSW                                        0884 02897000
         NI    TRCF2TMI(@10),B'11111101'                           0884 02898000
*   RFY                                                            0885 02899000
*     R14 RSTD;                                                    0885 02900000
*   R14=TIMESAVE;                   /* RESTORE RETURN ADDR OF CALLING   02901000
*                                      PROCEDURE                     */ 02902000
         L     R14,TIMESAVE                                        0886 02903000
*   RFY                                                            0887 02904000
*     R14 UNRSTD;                                                  0887 02905000
*   END TIMESNAP;                                                  0888 02906000
@EL00023 DS    0H                                                  0888 02907000
@EF00023 DS    0H                                                  0888 02908000
@ER00023 BR    @14                                                 0888 02909000
*                                                                  0889 02910000
*/********************************************************************* 02911000
**               C V D C O U N T   P R O C E D U R E                  * 02912000
** ENTRY REASON - TO CONVERT NUMBERS INTO PRINTABLE FORM AND INSERT   * 02913000
**                THEM INTO THE MESSAGE TEXT.                         * 02914000
** ENTRY FROM   - 1) JOBLINE PROCEDURE (2 PLACES)                     * 02915000
**                   A) FOR THE NUMBER OF JOBS                        * 02916000
**                   B) FOR THE NUMBER OF INITIATORS                  * 02917000
**                2) JOBLFILL PROCEDURE (2 PLACES)                    * 02918000
**                   A) FOR THE STARTING ADDRESS OF THE REGION        * 02919000
**                   B) FOR THE ENDING ADDRESS OF THE REGION          * 02920000
**                3) TSVTMLIN PROCEDURE                               * 02921000
**                   A) FILL IN MAX NUMBER OF VTAM TSO USERS  @ZA07770* 02922000
**                4) FILLINE PROCEDURE                                * 02923000
**                   A) FILL IN THE NUMBER OF ACTIVE TSO USERS OR TO  * 02924000
**                      FILL IN THE NUMBER OF ACTIVE VTAM TSO USERS   * 02925000
**                                                            @ZA07770* 02926000
**                5) STAEFAIL PROCEDURE                               * 02927000
**                   A) FILL IN CONSOLE ID FOR MESSAGE                * 02928000
** EXIT  TO     - CALLING PROCEDURE                                   * 02929000
** FUNCTION     - 1) CONVERT THE NUMBER TO DECIMAL                    * 02930000
**                2) UNPACK IT INTO THE MESSAGE                       * 02931000
**                3) MAKE LAST DIGIT A VALID NUMBER                   * 02932000
**                4) RETURN                                           * 02933000
** NOTE         - ON ENTRY:                                           * 02934000
**                   R6 - CONTAINS THE ADDRESS WITHIN THE BUFFER OF   * 02935000
**                        WHERE THE RESULTING VALUE IS TO BE PLACED.  * 02936000
**                   R7 - CONTAINS THE NUMBER TO BE CONVERTED.        * 02937000
**********************************************************************/ 02938000
*                                                                  0889 02939000
*CVDCOUNT:                                                         0889 02940000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0889 02941000
CVDCOUNT DS    0H                                                  0890 02942000
*   RESPECIFY                                                      0890 02943000
*    (R6,                                                          0890 02944000
*     R7,                                                          0890 02945000
*     R14) RSTD;                                                   0890 02946000
*   TRCF6CVI=ON;                    /* MARK CVDCOUNT PROC AS IN    0891 02947000
*                                      PROCESSING                    */ 02948000
*   TRCF6CVE=ON;                    /* MARK CVDCOUNT PROC AS ENTERED */ 02949000
         LA    @15,TRCURRSW                                        0892 02950000
         OI    TRCF6CVI(@15),B'00110000'                           0892 02951000
*   CVD(R7,UNPACK);                 /* CONVERT THE VALUE TO DECIMAL  */ 02952000
         CVD   R7,UNPACK                                           0893 02953000
*   UNPK(CONVERT(4:8),UNPACK(4:8)); /* UNPACK THE VALUE              */ 02954000
         UNPK  CONVERT+3(5,R6),UNPACK+3(5)                         0894 02955000
*   GEN REFS(R6)(OI 7(R6),X'F0');   /* MAKE LAST DIGIT VALID NUMBER  */ 02956000
         OI 7(R6),X'F0'                                                 02957000
*   TRCF6CVI=OFF;                   /* MARK CVDCOUNT PROC AS CONTROL    02958000
*                                      LEAVING                       */ 02959000
         LA    @15,TRCURRSW                                        0896 02960000
         NI    TRCF6CVI(@15),B'11011111'                           0896 02961000
*   RFY                                                            0897 02962000
*    (R6,                                                          0897 02963000
*     R7,                                                          0897 02964000
*     R14) UNRSTD;                                                 0897 02965000
*   END CVDCOUNT;                                                  0898 02966000
@EL00024 DS    0H                                                  0898 02967000
@EF00024 DS    0H                                                  0898 02968000
@ER00024 BR    @14                                                 0898 02969000
*                                                                  0899 02970000
*/********************************************************************* 02971000
**               F I N D S A C B    P R O C E D U R E                 * 02972000
** ENTRY REASON - TO FIND TRACKS SCREEN AREA CONTROL BLOCK            * 02973000
** ENTRY FROM   - 1) TRACK PROCEDURE (FOR EVERY TIME THROUGH TRACK)   * 02974000
**                2) STAEFAIL PROCEDURE (ESTAE CREATED BY IEECB860    * 02975000
**                   FAILED OR ESTAE FOR TRACK CMD FAILED.)           * 02976000
** EXIT  TO     - CALLING PROCEDURE                                   * 02977000
** FUNCTION     - 1) FIND TRACKS SACB (SCREEN AREA CONTROL BLOCK)     * 02978000
**                2) SAVE ITS ADDRESS IN THE ESTAE PARMLIST(STAELIST) * 02979000
**                3) RETURN                                           * 02980000
**********************************************************************/ 02981000
*                                                                  0899 02982000
*   RFY                                                            0899 02983000
*     R14 RSTD;                                                    0899 02984000
*FINDSACB:                                                         0900 02985000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0900 02986000
FINDSACB DS    0H                                                  0901 02987000
*   TRCF2FSI=ON;                    /* MARK FINDSACB PROC AS IN    0901 02988000
*                                      PROCESSING                    */ 02989000
*   TRCF2FSE=ON;                    /* MARK FINDSACB PROC AS ENTERED */ 02990000
         LA    @15,TRCURRSW                                        0902 02991000
         OI    TRCF2FSI(@15),B'00110000'                           0902 02992000
*   DISPLACE=UCMVEZ*(CHCNID-ONE);   /* INDEX TO UCME                 */ 02993000
         L     @15,CVTPTR                                          0903 02994000
         L     @15,UCMBASPT(,@15)                                  0903 02995000
         SLR   DISPLACE,DISPLACE                                   0903 02996000
         IC    DISPLACE,CHCNID(,CSCBPTR)                           0903 02997000
         BCTR  DISPLACE,0                                          0903 02998000
         LR    @01,DISPLACE                                        0903 02999000
         M     @00,UCMVEZ(,@15)                                    0903 03000000
         LR    DISPLACE,@01                                        0903 03001000
*   UCMDVEPT=UCMVEA+DISPLACE;       /* PROPER UCME ADDRESS           */ 03002000
         LR    UCMDVEPT,DISPLACE                                   0904 03003000
         AL    UCMDVEPT,UCMVEA(,@15)                               0904 03004000
*   SACBPTR=DCMADSDS;               /* FIRST SACB ADDRESS            */ 03005000
         L     @15,DCMTSPTR(,UCMDVEPT)                             0905 03006000
         L     SACBPTR,DCMADSDS(,@15)                              0905 03007000
*   SACBPRES=OFF;                   /* NO SACB FOUND YET             */ 03008000
         NI    SACBPRES,B'11110111'                                0906 03009000
*   DO WHILE(SACBPTR^=ZERO&SACBPRES=OFF);/* LOOP TIL END OF CHAIN OR    03010000
*                                      FIND SACB                     */ 03011000
         B     @DE00907                                            0907 03012000
@DL00907 DS    0H                                                  0908 03013000
*     IF DCMAID=CHARID THEN         /* TRACKS SACB ?                 */ 03014000
         CLC   DCMAID(1,SACBPTR),CHARID(CSCBPTR)                   0908 03015000
         BNE   @RF00908                                            0908 03016000
*       SACBPRES=ON;                /* YES                           */ 03017000
         OI    SACBPRES,B'00001000'                                0909 03018000
*     ELSE                                                         0910 03019000
*       SACBPTR=DCMACBNX;           /* NEXT SACB                     */ 03020000
         B     @RC00908                                            0910 03021000
@RF00908 L     SACBPTR,DCMACBNX(,SACBPTR)                          0910 03022000
*   END;                                                           0911 03023000
@RC00908 DS    0H                                                  0911 03024000
@DE00907 LTR   SACBPTR,SACBPTR                                     0911 03025000
         BZ    @DC00907                                            0911 03026000
         TM    SACBPRES,B'00001000'                                0911 03027000
         BZ    @DL00907                                            0911 03028000
@DC00907 DS    0H                                                  0912 03029000
*   IF SACBPRES=OFF THEN            /* WAS THE SACB FOUND ?          */ 03030000
         TM    SACBPRES,B'00001000'                                0912 03031000
         BNZ   @RF00912                                            0912 03032000
*     SACBPTR=ZERO;                 /* NO                            */ 03033000
         SLR   SACBPTR,SACBPTR                                     0913 03034000
*   SACBADDR=SACBPTR;               /* SAVE ADDRESS OF SACB          */ 03035000
@RF00912 LA    @15,STAELIST                                        0914 03036000
         ST    SACBPTR,SACBADDR(,@15)                              0914 03037000
*   TRCF2FSI=OFF;                   /* MARK FINDSACB PROC AS CONTROL    03038000
*                                      LEAVING                       */ 03039000
         LA    @15,TRCURRSW                                        0915 03040000
         NI    TRCF2FSI(@15),B'11011111'                           0915 03041000
*   RFY                                                            0916 03042000
*     R14 UNRSTD;                                                  0916 03043000
*   END FINDSACB;                                                  0917 03044000
@EL00025 DS    0H                                                  0917 03045000
@EF00025 DS    0H                                                  0917 03046000
@ER00025 BR    @14                                                 0917 03047000
*                                                                  0918 03048000
*/********************************************************************* 03049000
**               L O C K S O F F   P R O C E D U R E                  * 03050000
** ENTRY REASON - RELEASE THE CMS AND LOCAL LOCKS                     * 03051000
** ENTRY FROM   - 1) TRACK PROCEDURE (3 CALLS)                        * 03052000
**                   A) ECB WAS POSTED BY DIDOCS                      * 03053000
**                   B) ECB WAS POSTED FOR A STOP TRACK               * 03054000
**                   C) DONE IF IN HOLD MODE OR NO TR UPDATE OR A     * 03055000
**                      DISPLAY IS WANTED                             * 03056000
**                2) STAEFAIL                                         * 03057000
** EXIT  TO     - CALLING PROCEDURE                                   * 03058000
** FUNCTION     - 1) SAVE REGISTERS                                   * 03059000
**                2) RELEASE CMS LOCK                                 * 03060000
**                3) RELEASE LOCAL LOCK                               * 03061000
**                4) RESTORE REGS                                     * 03062000
**                5) RETURN                                           * 03063000
** NOTE         - REGISTER 12 MUST BE SAVED SEPARATELY BECAUSE IT IS  * 03064000
**                THE BASE REGISTER FOR 'LOCKSAVE'                    * 03065000
**********************************************************************/ 03066000
*                                                                  0918 03067000
*LOCKSOFF:                                                         0918 03068000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0918 03069000
LOCKSOFF DS    0H                                                  0919 03070000
*   RFY                                                            0919 03071000
*     R14 RSTD;                                                    0919 03072000
*   TRCF1LKI=ON;                    /* MARK LOCKSOFF PROC AS IN    0920 03073000
*                                      PROCESSING                    */ 03074000
*   TRCF1LKE=ON;                    /* MARK LOCKSOFF PROC AS ENTERED */ 03075000
         LA    @15,TRCURRSW                                        0921 03076000
         OI    TRCF1LKI(@15),B'00000011'                           0921 03077000
*   GENERATE SETS(LOCKSAVE,R6) REFS(R11,R12,R14);                  0922 03078000
         STM     R11,R14,LOCKSAVE SAVE REGISTERS ACROSS SETLOCK         03079000
         LR      R6,R12           SAVE DATA REGISTER                    03080000
         SETLOCK RELEASE,TYPE=CMS,                                     X03081000
               RELATED=(SACB,IEECB800(TRACK,STAEFAIL))                  03082000
         SETLOCK RELEASE,TYPE=LOCAL,                                   X03083000
               RELATED=(SACB,IEECB800(TRACK,STAEFAIL))                  03084000
         LR      R12,R6           RESTORE DATA REGISTER                 03085000
         LM      R11,R14,LOCKSAVE RESTORE REGISTERS                     03086000
*   TRCF1LKI=OFF;                   /* MARK LOCKSOFF PROC AS CONTROL    03087000
*                                      LEAVING                       */ 03088000
         LA    @15,TRCURRSW                                        0923 03089000
         NI    TRCF1LKI(@15),B'11111101'                           0923 03090000
*   RFY                                                            0924 03091000
*     R14 UNRSTD;                                                  0924 03092000
*   END LOCKSOFF;                                                  0925 03093000
@EL00026 DS    0H                                                  0925 03094000
@EF00026 DS    0H                                                  0925 03095000
@ER00026 BR    @14                                                 0925 03096000
*                                                                  0926 03097000
*/********************************************************************* 03098000
**           T I M E P O P   P R O C E D U R E                        * 03099000
**   THIS ROUTINE RECEIVES CONTROL WHEN THE STIMER EXPIRES.   @YM8271P* 03100000
**   IT RECEIVES CONTROL UNDER THE IRB SCHEDULED BY THE       @YM8271P* 03101000
**   STIMER. AT THIS POINT IEECB800 MUST RESTORE ITS          @YM8271P* 03102000
**   ADDRESSIBILITY AND WAKE ITSELF UP BY POSTING ITS OWN     @YM8271P* 03103000
**   ECB.                                                     @YM8271P* 03104000
**       1. REGISTER 15 HAS THE ADDRESS OF TIMEPOP. IT WILL   @YM8271P* 03105000
**          BE USED AS THE BASE REGISTER.                     @YM8271P* 03106000
**       2. LOCATE TRACKS'S SCB BY SCANNING THE SCB CHAIN.    @YM8271P* 03107000
**          COMPARE THE SCBEXIT ADDRESS WITH THE STAEXIT      @YM8271P* 03108000
**          ADDRESS. WHEN THEY ARE EQUAL ITS THE TRACK SCB.   @YM8271P* 03109000
**       3. LOCATE THE ECB IN THE STAE PARAMETER LIST.        @YM8271P* 03110000
**       4. POST IT.                                          @YM8271P* 03111000
**       5. RETURN TO THE SUPERVISOR TO ELIMINATE THE IRB AND @YM8271P* 03112000
**          GIVE RETURN TO IEECB800 AT ITS WAIT. INSIDE THE   @YM8271P* 03113000
**          PROCDEURE WAIT.                                           * 03114000
**       6. IF THE SCB CAN NOT BE LOCATED RETURN TO THE       @YM8271P* 03115000
**          SUPERVISOR.                                       @YM8271P* 03116000
**       7. DROP REGISTER 15 AS THE BASE REGISTER.            @YM8271P* 03117000
**********************************************************************/ 03118000
*                                                                  0926 03119000
*TIMEPOP:                                                          0926 03120000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0926 03121000
TIMEPOP  DS    0H                                                  0927 03122000
*   RFY                                                            0927 03123000
*    (R1,                                                          0927 03124000
*     R6,                                                          0927 03125000
*     R14,                                                         0927 03126000
*     R15) RSTD;                                                   0927 03127000
*   GEN REFS(R4,R15);                                              0928 03128000
         DROP  R4                PERMIT TIMER ROUTINE ADDRESSABILITY    03129000
         USING *,R15             ESTABLISH ROUTINE ADDRESSABILITY       03130000
*   R6=ADDR(STAEXIT);               /* TRACK STAE EXIT ADDRESS     0929 03131000
*                                                            @YM8271P*/ 03132000
         LA    R6,STAEXIT                                          0929 03133000
*   R1=ADDR(SCB);                   /* FIND 1ST SCB          @YM8271P*/ 03134000
         L     @10,CVTPTR                                          0930 03135000
         L     @10,CVTTCBP(,@10)                                   0930 03136000
         L     @10,TCBPTR(,@10)                                    0930 03137000
         L     R1,SCBPTR(,@10)                                     0930 03138000
*   DO WHILE R1^=ZERO;              /* SCAN SCB CHAIN        @YM8271P*/ 03139000
         B     @DE00931                                            0931 03140000
@DL00931 DS    0H                                                  0932 03141000
*     IF R1->SCBEXIT=R6 THEN        /* IF TRACK              @YM8271P*/ 03142000
         C     R6,SCBEXIT(,R1)                                     0932 03143000
         BNE   @RF00932                                            0932 03144000
*       DO;                         /* SCB THEN:             @YM8271P*/ 03145000
*         R1=R1->SCBPARM;           /* ADDRESS OF STAE PARMLIST    0934 03146000
*                                                            @ZA05507*/ 03147000
         L     R1,SCBPARM(,R1)                                     0934 03148000
*         R1=ADDR(R1->STIMECB);     /* ADDRESS OF THE ECB IN SCB     */ 03149000
         LA    @10,STIMECB(,R1)                                    0935 03150000
         LR    R1,@10                                              0935 03151000
*         GEN SETS(R0,R1) REFS(R14) EXIT NOSEQFLOW;                0936 03152000
*                                           /*POST WITH CODE OF 4    */ 03153000
         POST (1),4             POST WITH CODE OF FOUR                  03154000
         BR   R14               RETURN TO THE SUPERVISOR SO THAT THE   X03155000
                                IRB WILL BE ELIMINATED TO ALLOW THE     03156000
*                               MAIN LINE TO RECEIVE CONTROL.           03157000
*       END;                        /*                       @YM8271P*/ 03158000
*     ELSE                          /*                       @YM8271P*/ 03159000
*       R1=R1->SCBCHAIN;            /* ADDR NEXT SCB         @YM8271P*/ 03160000
@RF00932 L     R1,SCBCHAIN(,R1)                                    0938 03161000
*   END;                            /*                       @YM8271P*/ 03162000
@DE00931 LTR   R1,R1                                               0939 03163000
         BNZ   @DL00931                                            0939 03164000
*   GEN REFS(R4,R14,R15) EXIT NOSEQFLOW;                           0940 03165000
         BR   R14              NONE FOUND,SO RETURN                     03166000
         DROP R15              RESTORE R11 AS BASE REG       @YM8271P*/ 03167000
         USING @PSTART+4095,R4 RESTORE R4 AS SECONDARY BASE REG         03168000
*   RFY                                                            0941 03169000
*    (R1,                                                          0941 03170000
*     R6,                                                          0941 03171000
*     R14,                                                         0941 03172000
*     R15) UNRSTD;                                                 0941 03173000
*   END TIMEPOP;                                                   0942 03174000
*                                                                  0943 03175000
*/********************************************************************* 03176000
**                                                                    * 03177000
**             S T A E X I T    E N T R Y     P O I N T               * 03178000
**                                                                    * 03179000
**     THIS ROUTINE IN GIVEN CONTROL AS THE RESULT OF AN UNPREDICTABLE* 03180000
**   ERROR ENCOUNTERED WHILE PROCESSING A TRACK COMMAND.              * 03181000
**   ON ENTRY IF REG 0 CONTAINS 12 - REG 1 CONTAINS THE ABEND CODE    * 03182000
**                                   REG 2 ADDRESSES THE STAE PARM    * 03183000
**                                   LIST (STAELIST)                  * 03184000
**            IF REG 0 DOES NOT CONTAIN 12 - REG 1 ADDRESSES THE SDWA * 03185000
**                                   IN WHICH SDWAPARM ADDRESSES THE  * 03186000
**                                   STAE PARM LIST (STAELIST)        * 03187000
**            REG 14 CONTAINS THE ADDRESS TO WHICH CONTROL SHOULD BE  * 03188000
**                RETURNED WHEN THIS TASK'S ESTAE PROCESSING HAS      * 03189000
**                COMPLETED                                           * 03190000
**   THE STAE PARM LIST CONTAINS THE FOLLOWING INFORMATION:           * 03191000
**            1) THE MODULE'S BASE REGISTER                           * 03192000
**            2) THE DATA AREA'S BASE REGISTER                        * 03193000
**   THIS INFORMATION IS RECOVERED.                                   * 03194000
**   SDUMP IS NOT ISSUED IF PERCOLATED TO BY A LOWER LEVEL ESTAE,     * 03195000
**   SINCE THE DUMP SHOULD HAVE BEEN TAKEN AT THE EXIT CLOSER TO THE  * 03196000
**   ERROR.                                                           * 03197000
**********************************************************************/ 03198000
*                                                                  0943 03199000
*STAEXIT:                                                          0943 03200000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0943 03201000
STAEXIT  DS    0H                                                  0944 03202000
*   RFY                                                            0944 03203000
*    (R0,                                                          0944 03204000
*     R1,                                                          0944 03205000
*     SDWAPTR,                                                     0944 03206000
*     R2,                                                          0944 03207000
*     R4,                                                          0944 03208000
*     R6,                                                          0944 03209000
*     R11,                                                         0944 03210000
*     R12,                                                         0944 03211000
*     R14,                                                         0944 03212000
*     R15) RSTD;                                                   0944 03213000
*   R6=R15;                         /* SET BASE REG TO ENTRY POINT 0945 03214000
*                                      ADDRESS                       */ 03215000
         LR    R6,R15                                              0945 03216000
*   GEN REFS(R4,R6);                                               0946 03217000
*                                          /* SET UP TEMPORARY BASE */  03218000
         USING STAEXIT,R6                                               03219000
         DROP  R4                                                       03220000
*   IF R0=TWELVE THEN               /* IS THERE AN SDWA?             */ 03221000
         C     R0,@CF00035                                         0947 03222000
         BNE   @RF00947                                            0947 03223000
*     DO;                           /* NO, R2 ADDRESSES STAE PARM  0948 03224000
*                                      LIST                          */ 03225000
*       RESPECIFY                                                  0949 03226000
*        (R0) UNRESTRICTED;                                        0949 03227000
*       R4=R2->REGCODE1;            /* RESTORE BASE REG              */ 03228000
         L     R4,REGCODE1(,R2)                                    0950 03229000
*       R11=R2->REGCODE2;           /* RESTORE BASE REG              */ 03230000
         L     R11,REGCODE2(,R2)                                   0951 03231000
*       R12=R2->REGDATA;            /* RESTORE THE DATA AREA'S REG   */ 03232000
         L     R12,REGDATA(,R2)                                    0952 03233000
*       TRPF2SEX=ON;                /* MARK EXITSTAE PROC AS ENTERED */ 03234000
         LA    @10,TRCURRSW                                        0953 03235000
         OI    TRPF2SEX(@10),B'01000000'                           0953 03236000
*       HAVESDWA=OFF;               /* INDICATE THERE WAS NO SDWA  0954 03237000
*                                                            @YM01918*/ 03238000
         NI    HAVESDWA,B'11101111'                                0954 03239000
*       CSCBPTR=CSCBPASS;           /* RESTORE CSCB BASE REG         */ 03240000
         LA    @10,STAELIST                                        0955 03241000
         L     CSCBPTR,CSCBPASS(,@10)                              0955 03242000
*       WTOBUF(ONE:SLEN)=SLIST(ONE:SLEN);/* BUILD SDUMP LIST IN      */ 03243000
         LH    @10,SLEN                                            0956 03244000
         BCTR  @10,0                                               0956 03245000
         EX    @10,@SM03181                                        0956 03246000
*       R1=ADDR(CHSWT);             /* CSCB BUFFER ADDRESSED BY R1   */ 03247000
         LA    R1,CHSWT(,CSCBPTR)                                  0957 03248000
*       GEN SETS(R1)(SDUMP MF=(E,(1)));/* GET DUMP AS SOON AS      0958 03249000
*                                      POSSIBLE                      */ 03250000
         SDUMP MF=(E,(1))                                               03251000
*       RESPECIFY                                                  0959 03252000
*        (R2) UNRESTRICTED;                                        0959 03253000
*     END;                                                         0960 03254000
*   ELSE                            /* HAVE SDWA                     */ 03255000
*     DO;                           /* STAE PARM PTR IN SDWA         */ 03256000
         B     @RC00947                                            0961 03257000
@RF00947 DS    0H                                                  0962 03258000
*       RESPECIFY                                                  0962 03259000
*        (R8) RESTRICTED;                                          0962 03260000
*       R8=R1;                      /* SAVE BASE OF SDWA             */ 03261000
         LR    R8,R1                                               0963 03262000
*       R4=SDWAPARM->REGCODE1;      /* RESTORE BASE REG              */ 03263000
         L     @10,SDWAPARM(,SDWAPTR)                              0964 03264000
         L     R4,REGCODE1(,@10)                                   0964 03265000
*       R11=SDWAPARM->REGCODE2;     /* RESTORE BASE REG              */ 03266000
         L     R11,REGCODE2(,@10)                                  0965 03267000
*       R12=SDWAPARM->REGDATA;      /* RESTORE DATA AREA'S BASE      */ 03268000
         L     R12,REGDATA(,@10)                                   0966 03269000
*       TRPF2SEX=ON;                /* MARK EXITSTAE PROC AS ENTERED */ 03270000
         LA    @10,TRCURRSW                                        0967 03271000
         OI    TRPF2SEX(@10),B'01000000'                           0967 03272000
*       HAVESDWA=ON;                /* INDICATE THERE IS A SDWA    0968 03273000
*                                                            @YM01918*/ 03274000
         OI    HAVESDWA,B'00010000'                                0968 03275000
*       CSCBPTR=CSCBPASS;           /* RESTORE CSCB BASE REG         */ 03276000
         LA    @10,STAELIST                                        0969 03277000
         L     CSCBPTR,CSCBPASS(,@10)                              0969 03278000
*       IF SDWAPERC=OFF&            /* IS THERE STAE PERCULATION?    */ 03279000
*           SDWAMCHK=OFF THEN       /* WAS THERE A MACHINE CHECK?    */ 03280000
         TM    SDWAPERC(SDWAPTR),B'00010000'                       0970 03281000
         BNZ   @RF00970                                            0970 03282000
         TM    SDWAMCHK(SDWAPTR),B'10000000'                       0970 03283000
         BNZ   @RF00970                                            0970 03284000
*         DO;                       /* NO                            */ 03285000
*           WTOBUF(ONE:SLEN)=SLIST(ONE:SLEN);/* BUILD SDUMP LIST IN  */ 03286000
         LH    @10,SLEN                                            0972 03287000
         BCTR  @10,0                                               0972 03288000
         EX    @10,@SM03181                                        0972 03289000
*           R1=ADDR(CHSWT);         /* CSCB BUFFER ADDRESSED BY R1   */ 03290000
         LA    R1,CHSWT(,CSCBPTR)                                  0973 03291000
*           GEN SETS(R1)(SDUMP MF=(E,(1)));/* GET DUMP AS SOON AS  0974 03292000
*                                      POSSIBL                       */ 03293000
         SDUMP MF=(E,(1))                                               03294000
*         END;                                                     0975 03295000
*       ELSE                                                       0976 03296000
*         ;                         /* THERE IS PERCULATION, SO NO 0976 03297000
*                                      SDUMP                         */ 03298000
@RF00970 DS    0H                                                  0977 03299000
*     END;                                                         0977 03300000
*   GEN REFS(R4,R6);                                               0978 03301000
@RC00947 DS    0H                                                  0978 03302000
*                                    /* DROP TEMPORARY BASE REG */      03303000
         DROP R6                                                        03304000
         USING @PSTART+4095,R4    RESTORE R4 BASE REG                   03305000
*   R14SAVE=R14;                                                   0979 03306000
         ST    R14,R14SAVE                                         0979 03307000
*   CALL STAEFAIL;                  /* CONTROL IS NEVER RETURNED.  0980 03308000
*                                      STAEFAIL RETURNS TO THE SYSTEM*/ 03309000
         BAL   @14,STAEFAIL                                        0980 03310000
*   RFY                                                            0981 03311000
*    (R1,                                                          0981 03312000
*     SDWAPTR,                                                     0981 03313000
*     R4,                                                          0981 03314000
*     R6,                                                          0981 03315000
*     R8,                                                          0981 03316000
*     R11,                                                         0981 03317000
*     R12,                                                         0981 03318000
*     R14,                                                         0981 03319000
*     R15) UNRSTD;                                                 0981 03320000
*   END STAEXIT;                                                   0982 03321000
@EL00028 DS    0H                                                  0982 03322000
@EF00028 DS    0H                                                  0982 03323000
@ER00028 BR    @14                                                 0982 03324000
*                                                                  0983 03325000
*/********************************************************************* 03326000
**            S T A E F A I L    P R O C E D U R E                    * 03327000
** ENTRY REASON - AN ESTAE FAILED TO SET UP PROPERLY                  * 03328000
** ENTRY FROM   - 1) SETESTAE PROCEDURE                               * 03329000
**                   A) IEECB860 FAILED TO SET UP PROPERLY            * 03330000
**                2) TRKESTAE PROCEDURE                               * 03331000
**                   A) LOWER LEVEL TRACK ESTAE FAILED                * 03332000
**                3) EXITSTAE PROCEDURE                               * 03333000
**                   A) AN ERROR WAS FOUND SO ESTAE ROUTINE GOT       * 03334000
**                      CONTROL.                                      * 03335000
** EXIT  TO     - LABEL RETSYS IN THE TOP PROCEDURE WHERE CONTROL IS  * 03336000
**                RETURN TO THE SYSTEM OR ABEND 102 IS ISSUED.        * 03337000
** FUNCTION     -                                                     * 03338000
**  1) THE LOCAL AND CMS LOCKS ARE OBTAINED TO SERIALIZE ON THE SACB. * 03339000
**  2) FIND TRACKS SACB                                               * 03340000
**  3) IF AN SACB IS PRESENT AND THIS TASK WAS NOT POSTED BY DIDOCS,  * 03341000
**     THE TRACK FIELDS IN THE SACB ARE CLEARED TO PREVENT ANY        * 03342000
**     FURTHER PROCESSING BY SVC 34 OR DIDOCS.                        * 03343000
**  4)  THE LOCKS ARE RELEASED.                                       * 03344000
**  5)  IF THERE IS A VALID (NON-ZERO) UCME ECB ADDRESS,       @YM4795P 03345000
**      THEN ISSUE A MESSAGE TELLING THE OPERATOR TO ISSUE A          * 03346000
**      K E,D,L=CCA (AREA TRACK IS IN). THIS WILL                     * 03347000
**      INDICATE TO DIDOCS TO CLEAR THE SCREEN                 @YM4795P 03348000
**   6) THE TRACK CSCB IS FREED VIA MGCR.                             * 03349000
**   7) IF AN SDWA EXISTS, A SETRP IS ISSUED TO INDICATE THE ACTION   * 03350000
**      TO BE TAKEN BY ASIR (ABEND - STAE INTERFACE ROUTINE)          * 03351000
**   8) OTHERWISE REG 15 IS SET WITH A 0 RETURN CODE                  * 03352000
**   9) RETURN TO TOP PROCEDURE AT LABEL RETSYS WHERE R14 IS RESTORED * 03353000
**      AND CONTROL IS RETURNED TO ASIR.                              * 03354000
** NOTE:  REGISTER 12 MUST BE SAVED SEPARATELY BECAUSE IT IS THE      * 03355000
**        BASE REGISTER FOR 'LOCKSAVE'                                * 03356000
**********************************************************************/ 03357000
*                                                                  0983 03358000
*   RFY                                                            0983 03359000
*     R8 RSTD;                                                     0983 03360000
*STAEFAIL:                                                         0984 03361000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0984 03362000
STAEFAIL DS    0H                                                  0985 03363000
*   TRPF2SFL=ON;                    /* MARK STAEFAIL PROC AS ENTERED */ 03364000
         LA    @10,TRCURRSW                                        0985 03365000
         OI    TRPF2SFL(@10),B'00100000'                           0985 03366000
*   GEN SETS(LOCKSAVE,R6) REFS(R11,R12,R14);                       0986 03367000
         STM     R11,R14,LOCKSAVE    SAVE REGISTERS ACROSS SETLOCK      03368000
         LR      R6,R12              SAVE DATA REGISTER                 03369000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        X03370000
               RELATED=(SACB,IEECB800(LOCKSOFF))                        03371000
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                          X03372000
               RELATED=(SACB,IEECB800(LOCKSOFF))                        03373000
         LR      R12,R6              RESTORE DATA REGISTER              03374000
         LM      R11,R14,LOCKSAVE    RESTORE REGISTERS                  03375000
*   CALL FINDSACB;                  /* FIND TRACKS SACB              */ 03376000
         BAL   @14,FINDSACB                                        0987 03377000
*   IF SACBPTR^=ZERO&CHCECB^=POSTED THEN/* SACB EXISTS &             */ 03378000
         LTR   SACBPTR,SACBPTR                                     0988 03379000
         BZ    @RF00988                                            0988 03380000
         CLC   CHCECB(4,CSCBPTR),POSTED                            0988 03381000
         BE    @RF00988                                            0988 03382000
*     DO;                           /* DIDOCS DID NOT POST           */ 03383000
*       DCMADFLG=DCMADFLG&CLEAROPT; /* CLEAR ALL BUT DIDOCS DIDOCS 0990 03384000
*                                      BIT                           */ 03385000
         NI    DCMADFLG(SACBPTR),X'20'                             0990 03386000
*       DCMATRCK=OFFBYTE;           /* CLEAR TRACK OPTIONS INDICATORS*/ 03387000
         MVI   DCMATRCK(SACBPTR),X'00'                             0991 03388000
*       DCMAUTME=OFFHALF;           /* CLEAR TRACK TIME INTERVAL     */ 03389000
         SLR   @10,@10                                             0992 03390000
         STH   @10,DCMAUTME(,SACBPTR)                              0992 03391000
*       DCMATECB=CLEARECB;          /* CLEAR TRACK CANCEL ECB ADDR   */ 03392000
         SLR   @10,@10                                             0993 03393000
         ST    @10,DCMATECB(,SACBPTR)                              0993 03394000
*     END;                                                         0994 03395000
*   CMDBUFR(ONE:KEDLGTH)=KED(ONE:KEDLGTH);/* MOVE MESSAGE IN CSCB  0995 03396000
*                                                            @YM4795P*/ 03397000
@RF00988 SLR   @10,@10                                             0995 03398000
         IC    @10,KEDLGTH                                         0995 03399000
         BCTR  @10,0                                               0995 03400000
         EX    @10,@SM03183                                        0995 03401000
*   RFY                                                            0996 03402000
*    (R6,                                                          0996 03403000
*     R7) RSTD;                                                    0996 03404000
*   R7=CHCNID;                      /* PICK UP CONSOLE ID FOR MSG  0997 03405000
*                                      CONVERT               @ZA00073*/ 03406000
         SLR   R7,R7                                               0997 03407000
         IC    R7,CHCNID(,CSCBPTR)                                 0997 03408000
*   R6=ADDR(CNIDAREA);              /* ADDR OF AREA FOR UNPACK     0998 03409000
*                                                            @ZA00073*/ 03410000
         LA    R6,CNIDAREA                                         0998 03411000
*   CALL CVDCOUNT;                  /* PROCESS CONSOLE ID FOR MSG  0999 03412000
*                                      TEXT                  @ZA00073*/ 03413000
         BAL   @14,CVDCOUNT                                        0999 03414000
*   CNSID=CNID;                     /* INIT MSG WITH CONSOLE ID    1000 03415000
*                                                            @ZA00073*/ 03416000
         LA    @10,64                                              1000 03417000
         ALR   @10,CSCBPTR                                         1000 03418000
         MVC   CNSID(2,@10),CNID                                   1000 03419000
*   CNSLAREA=DCMAID;                /* SCREEN AREA ID        @YM4795P*/ 03420000
         MVC   CNSLAREA(1,@10),DCMAID(SACBPTR)                     1001 03421000
*   RFY                                                            1002 03422000
*    (R6,                                                          1002 03423000
*     R7) UNRSTD;                   /*                       @Z40DQSS*/ 03424000
*   CALL LOCKSOFF;                  /* REGARDLESS RELEASE LOCKS      */ 03425000
         BAL   @14,LOCKSOFF                                        1003 03426000
*   RFY                                                            1004 03427000
*    (R0,                                                          1004 03428000
*     R1) RSTD;                     /*                       @Z40DQSS*/ 03429000
*   R0=CHUCMP;                      /* CONSOLE ID                    */ 03430000
         SLR   R0,R0                                               1005 03431000
         IC    R0,CHUCMP(,CSCBPTR)                                 1005 03432000
*   R1=ADDR(CHSWT);                 /* MSG BUFFER IN REG1    @YM4795P*/ 03433000
         LA    R1,CHSWT(,CSCBPTR)                                  1006 03434000
*   GEN SETS(R1)(WTO MF=(E,(R1)));  /* ISSUE WTO             @YM4795P*/ 03435000
         WTO MF=(E,(R1))                                                03436000
*   RESPECIFY                                                      1008 03437000
*     R0 UNRESTRICTED;                                             1008 03438000
*   R1=CSCBPTR;                     /* RESTORE CSCB PTR              */ 03439000
         LR    R1,CSCBPTR                                          1009 03440000
*   CHFC=ON;                        /* INDICATE TO FREE CSCB CORE    */ 03441000
         OI    CHFC(CSCBPTR),B'00000010'                           1010 03442000
*   GEN SETS(R0,R1)(MGCR (1),CHAIN);/* HAVE CSCB CORE FREED BY SVC 34*/ 03443000
         MGCR (1),CHAIN                                                 03444000
*   IF HAVESDWA=ON THEN             /* WAS THERE AN SDWA?    @YM01918*/ 03445000
         TM    HAVESDWA,B'00010000'                                1012 03446000
         BNO   @RF01012                                            1012 03447000
*     DO;                           /* YES, ISSUE SETPR              */ 03448000
*       R1=R8;                      /* REINIT BASE OF SDWA           */ 03449000
         LR    R1,R8                                               1014 03450000
*       DO;                         /* SETRP                       1015 03451000
*                                      RECORD(YES)RECPARM(LRECINF)   */ 03452000
*         RESPECIFY                                                1016 03453000
*          (GPR00P,                                                1016 03454000
*           GPR01P,                                                1016 03455000
*           GPR14P,                                                1016 03456000
*           GPR15P) RSTD;                                          1016 03457000
*         GPR01P->SDWARCDE=0;       /* SAVE RC VALUE                 */ 03458000
         MVI   SDWARCDE(GPR01P),X'00'                              1017 03459000
*         GPR01P->SDWARCRD='1'B;    /* TURN ON RECORD INDICATOR      */ 03460000
         OI    SDWARCRD(GPR01P),B'10000000'                        1018 03461000
*         GPR15P=ADDR(LRECINF);     /* ACCESS RECORD PARAMETER LIST  */ 03462000
         LA    GPR15P,LRECINF                                      1019 03463000
*         GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD      1020 03464000
*                                      PARAMETERS                    */ 03465000
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   1020 03466000
*         RESPECIFY                                                1021 03467000
*          (GPR00P,                                                1021 03468000
*           GPR01P,                                                1021 03469000
*           GPR14P,                                                1021 03470000
*           GPR15P) UNRSTD;                                        1021 03471000
*       END;                                                       1022 03472000
*     END;                                                         1023 03473000
*   ELSE                            /* NO SDWA                       */ 03474000
*     DO;                                                          1024 03475000
         B     @RC01012                                            1024 03476000
@RF01012 DS    0H                                                  1025 03477000
*       RESPECIFY                                                  1025 03478000
*        (R15) RESTRICTED;                                         1025 03479000
*       R15=ZERO;                   /* SET RETURN CODE = 0           */ 03480000
         SLR   R15,R15                                             1026 03481000
*     END;                                                         1027 03482000
*   RETURN TO RETSYS;               /* RETURN TO TOP PROCEDURE AND 1028 03483000
*                                      ISSUE ABEND OR RETURN TO THE     03484000
*                                      SYSTEM.                       */ 03485000
@RC01012 LA    @14,RETSYS                                          1028 03486000
@EL00029 DS    0H                                                  1028 03487000
@EF00029 DS    0H                                                  1028 03488000
@ER00029 BR    @14                                                 1028 03489000
*   RFY                                                            1029 03490000
*    (R1,                                                          1029 03491000
*     R7,                                                          1029 03492000
*     R8,                                                          1029 03493000
*     R15) UNRSTD;                                                 1029 03494000
*   END STAEFAIL;                                                  1030 03495000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 03496000
*     GPR00P PTR(31) REG(0),                                       1031 03497000
*     GPR01P PTR(31) REG(1),                                       1031 03498000
*     GPR14P PTR(31) REG(14),                                      1031 03499000
*     GPR15P PTR(31) REG(15);                                      1031 03500000
*   DECLARE                         /* COMMON VARIABLES              */ 03501000
*     I256C CHAR(256) BASED,                                       1032 03502000
*     I031F FIXED(31) BASED,                                       1032 03503000
*     I031P PTR(31) BASED,                                         1032 03504000
*     I015F FIXED(15) BASED,                                       1032 03505000
*     I015P PTR(15) BASED,                                         1032 03506000
*     I008P PTR(8) BASED,                                          1032 03507000
*     I001C CHAR(1) BASED;                                         1032 03508000
*   END IEECB800                                                   1033 03509000
*                                                                  1033 03510000
