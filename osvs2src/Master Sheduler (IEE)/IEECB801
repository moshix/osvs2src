*       PARMBASE = R1;               /* ESTABLISH PARMLIST BASE      */ 00020000
         LR    PARMBASE,R1                                         0015 00021000
*       CSCBPTR = CSCBADDR;          /* GET THIS TASK'S CSCB ADDRESS */ 00022000
         L     CSCBPTR,CSCBADDR(,PARMBASE)                         0016 00023000
*       WPLPTR = MESSADDR;           /* GET ADDR OF MSG BUFFER       */ 00024000
