         BE    @RT00434                                            0434 00944000
         CH    I,@CH00034                                          0434 00945000
         BNE   @RF00434                                            0434 00946000
@RT00434 DS    0H                                                  0435 00947000
*         DO;                       /* NO                    @G33CPMR*/ 00948000
*           MSGCODE=IEE308I;        /* ERROR CONDITION               */ 00949000
