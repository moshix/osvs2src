         TITLE 'IEECB900 - VARY CN SYNTAX CHECKER                      *00001000
                        '                                               00002000
IEECB900 CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEECB900  74.165'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @11,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@11                                         0001 00012000
         LR    @03,@01                                             0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @12,@01                                             0001 00016000
         USING @DATD,@12                                           0001 00017000
         LR    @01,@03                                             0001 00018000
         ST    @13,@SA00001+4                                      0001 00019000
         LR    @13,@12                                             0001 00020000
         MVC   @PC00001(4),0(@01)                                  0001 00021000
*                                                                  0096 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* THIS IS THE MAIN ENTRY POINT FOR COMMAND PROCESSING.          */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                                  0096 00028000
*   R14SAVE=REG14;                  /* SAVE RETURN ADDRESS           */ 00029000
         ST    REG14,R14SAVE                                       0096 00030000
*   REG0=LENGTH(PARMMAP);           /* PARM LIST LENGTH TO CLEAR     */ 00031000
         LA    REG0,18                                             0097 00032000
*   PARMMAP(1:REG0)=PARMMAP(1:REG0)&&PARMMAP(1:REG0);/* CLEAR AREA   */ 00033000
         LR    @10,REG0                                            0098 00034000
         BCTR  @10,0                                               0098 00035000
         EX    @10,@SX01371                                        0098 00036000
*   CSCBPTR=REG1;                   /* SAVE CSCB PTR                 */ 00037000
         ST    REG1,CSCBPTR                                        0099 00038000
*   MSGCODE=ZERO;                   /* SET ERROR INDICATOR OFF       */ 00039000
         MVI   MSGCODE,X'00'                                       0100 00040000
*   VERBNAME=VARYC;                 /* INIT MESSAGE VARIABLE         */ 00041000
*                                                                  0101 00042000
         MVC   VERBNAME(8),VARYC                                   0101 00043000
*   /*****************************************************************/ 00044000
*   /*                                                               */ 00045000
*   /* SET UP FOR ESTAE ENVIRONMENT FOR THIS MODULE.                 */ 00046000
*   /*                                                               */ 00047000
*   /*****************************************************************/ 00048000
*                                                                  0102 00049000
*   CALL STAERTN;                                                  0102 00050000
         BAL   @14,STAERTN                                         0102 00051000
*   IF REG15=ZERO THEN              /* ESTAE SUCCESSFUL ?            */ 00052000
         LTR   REG15,REG15                                         0103 00053000
         BNZ   @RF00103                                            0103 00054000
*     DO;                                                          0104 00055000
*       GEN(LOAD EP=IEE0503D);      /* GET MSG MODULE ADDRESS        */ 00056000
         LOAD EP=IEE0503D                                               00057000
*       MSGMPTR=REG0;               /* SAVE ADDRESS IN PARM LIST     */ 00058000
         ST    REG0,MSGMPTR                                        0106 00059000
*       MSGAPTR=ADDR(MSGAREA);      /* INIT PARM LIST ADDR           */ 00060000
         LA    @10,MSGAREA                                         0107 00061000
         ST    @10,MSGAPTR                                         0107 00062000
*       CALL EOBRTN;                /* CALCULATE INPUT LENGTH        */ 00063000
*                                                                  0108 00064000
         BAL   @14,EOBRTN                                          0108 00065000
*       /*************************************************************/ 00066000
*       /*                                                           */ 00067000
*       /* THE FOLLOWING RTN, CNCHECK, WILL PROCESS THE VALUES OF THE*/ 00068000
*       /* 'CN(' PARAMETER FOR VALIDITY. IT CALLS SUB-RTN PARENSCN   */ 00069000
*       /* (TO VERIFY BALANCED PARENS AND THAT THERE IS A VALID DATA */ 00070000
*       /* LENGTH WITHIN PARENS), AND SUB-RTN DELIMRTN (TO VERIFY    */ 00071000
*       /* NUMERIC DATA AND VALID CONSOLE ID LENGTH - ONE/TWO        */ 00072000
*       /* DIGITS.) INVALID VALUES RESULT IN FIELD MSGCODE BEING SET */ 00073000
*       /* TO NON-ZERO.                                              */ 00074000
*       /*                                                           */ 00075000
*       /*************************************************************/ 00076000
*                                                                  0109 00077000
*       CALL CNCHECK;               /* VERIFY VALID PARAMETER        */ 00078000
         BAL   @14,CNCHECK                                         0109 00079000
*       IF MSGCODE=ZERO THEN        /* PARAMETER, PARENS CHECK OK    */ 00080000
         CLI   MSGCODE,0                                           0110 00081000
         BNE   @RF00110                                            0110 00082000
*         DO;                                                      0111 00083000
*                                                                  0111 00084000
*           /*********************************************************/ 00085000
*           /*                                                       */ 00086000
*           /* SET UP TO PROCESS COMMAND OPERANDS. THE INPUT LINE    */ 00087000
*           /* WILL BE SCANNED FOR A VALID OPERAND. IF FOUND, THE    */ 00088000
*           /* OPERAND VALUE WILL BE PROCESSED TO COMPLETION. REPEAT */ 00089000
*           /* SCAN UNTIL ALL OPERANDS HAVE BEEN PROCESSED, OR AN    */ 00090000
*           /* ERROR CONDITION IS SET - MSGCODE = NON-ZERO.          */ 00091000
*           /*                                                       */ 00092000
*           /*********************************************************/ 00093000
*                                                                  0112 00094000
*           SPINPTR=ONE;            /* SET LOOP CONDITION            */ 00095000
         MVI   SPINPTR,X'01'                                       0112 00096000
*           DO WHILE(MSGCODE=ZERO&SPINPTR=ONE);/* SET UP LOOP        */ 00097000
         B     @DE00113                                            0113 00098000
@DL00113 DS    0H                                                  0114 00099000
*             IF INPTR->COLSCAN=COMMA THEN/* OPERAND DELIMITER       */ 00100000
         CLC   COLSCAN(1,INPTR),COMMA                              0114 00101000
         BNE   @RF00114                                            0114 00102000
*               DO;                                                0115 00103000
*                                                                  0115 00104000
*                 /***************************************************/ 00105000
*                 /*                                                 */ 00106000
*                 /* SET UP INPUT TO RTN VALIDSCN. INPTR=1ST CHAR    */ 00107000
*                 /* PAST COMMA DELIMITER. REG15 PTR TO THE TABLE OF */ 00108000
*                 /* VALID OPERAND ADDRESSES. RTN VALIDSCN WILL      */ 00109000
*                 /* COMPARE THE INPUT(INPTR) TO THE ENTRIES IN THE  */ 00110000
*                 /* OPERAND TABLE. FOR A VALID MATCH, REG15 WILL    */ 00111000
*                 /* CONTAIN A PTR TO THE OPERAND, AND FIELD TABINDX */ 00112000
*                 /* WILL CONTAIN AN OFFSET VALUE TO BE USED TO      */ 00113000
*                 /* REFERENCE THE OPERAND PROCESSOR VIA A SUBRTN    */ 00114000
*                 /* BRANCH TABLE.                                   */ 00115000
*                 /*                                                 */ 00116000
*                 /***************************************************/ 00117000
*                                                                  0116 00118000
*                 INPTR=INPTR+ONE;  /* BUMP TO OPERAND               */ 00119000
         AL    INPTR,@CF00064                                      0116 00120000
*                 REG15=ADDR(OPADRTAB);/* PTR TO OPERAND ADDRESSES   */ 00121000
         LA    REG15,OPADRTAB                                      0117 00122000
*                 CALL VALIDSCN;    /* CHECK FOR VALID OPERAND       */ 00123000
         BAL   @14,VALIDSCN                                        0118 00124000
*                 IF REG15^=ZERO THEN/* PTR TO VALID OPERAND         */ 00125000
         LTR   REG15,REG15                                         0119 00126000
         BZ    @RF00119                                            0119 00127000
*                   DO;                                            0120 00128000
*                                                                  0120 00129000
*                     /***********************************************/ 00130000
*                     /*                                             */ 00131000
*                     /* VALID OPERAND FOUND. SET INDEX VALUE FOR    */ 00132000
*                     /* BRANCH TABLE AND PROCESS THE OPERAND VALUES.*/ 00133000
*                     /* ON RETURN FROM NORMAL PROCESSING THE OPERAND*/ 00134000
*                     /* PTR - INPTR, IS EXPECTED TO POINT AT OPERAND*/ 00135000
*                     /* DELIMITER - COMMA, OR END OF INPUT ADDRESS  */ 00136000
*                     /*                                             */ 00137000
*                     /***********************************************/ 00138000
*                                                                  0121 00139000
*                     ARRINDX=(TABINDX/FOUR)+ONE;/* ARRAY INDEX PTR  */ 00140000
         SLR   @10,@10                                             0121 00141000
         IC    @10,TABINDX                                         0121 00142000
         SRL   @10,2                                               0121 00143000
         AL    @10,@CF00064                                        0121 00144000
         STC   @10,ARRINDX                                         0121 00145000
*                     CALL RTNPTR;  /* IDENTIFY/PROCESS OPERAND      */ 00146000
         SLR   @10,@10                                             0122 00147000
         IC    @10,ARRINDX                                         0122 00148000
         SLA   @10,2                                               0122 00149000
         L     @15,PTRARR-4(@10)                                   0122 00150000
         BALR  @14,@15                                             0122 00151000
*                   END;                                           0123 00152000
*                 ELSE                                             0124 00153000
*                   MSGCODE=MSG3E;  /* SET ERROR CODE - IEE535I      */ 00154000
         B     @RC00119                                            0124 00155000
@RF00119 MVC   MSGCODE(1),MSG3E                                    0124 00156000
*               END;                                               0125 00157000
*             ELSE                  /* EOB OR INVALID DELIMITER      */ 00158000
*               DO;                                                0126 00159000
         B     @RC00114                                            0126 00160000
@RF00114 DS    0H                                                  0127 00161000
*                 IF INPTR=BUFSIZE THEN/* END OF INPUT               */ 00162000
         CR    INPTR,BUFSIZE                                       0127 00163000
         BNE   @RF00127                                            0127 00164000
*                   SPINPTR=ZERO;   /* SET END OF LOOP INDICATOR     */ 00165000
         MVI   SPINPTR,X'00'                                       0128 00166000
*                 ELSE              /* INVALID DELIMITER             */ 00167000
*                   MSGCODE=MSG7;   /* SET ERROR MSG CODE - IEE307I  */ 00168000
         B     @RC00127                                            0129 00169000
@RF00127 MVC   MSGCODE(1),MSG7                                     0129 00170000
*               END;                                               0130 00171000
@RC00127 DS    0H                                                  0131 00172000
*           END;                                                   0131 00173000
@RC00114 DS    0H                                                  0131 00174000
@DE00113 CLI   MSGCODE,0                                           0131 00175000
         BNE   @DC00113                                            0131 00176000
         CLI   SPINPTR,1                                           0131 00177000
         BE    @DL00113                                            0131 00178000
@DC00113 DS    0H                                                  0132 00179000
*           IF MSGCODE=ZERO THEN    /* SUCCESSFUL PROCESSING         */ 00180000
         CLI   MSGCODE,0                                           0132 00181000
         BNE   @RF00132                                            0132 00182000
*             DO;                                                  0133 00183000
*               REG1=ADDR(PARMMAP); /* SET UP PARM LIST PTR          */ 00184000
         LA    REG1,PARMMAP                                        0134 00185000
*               CALL IEECB901;      /* CALL UCME PROCESSOR           */ 00186000
         L     @15,@CV01165                                        0135 00187000
         BALR  @14,@15                                             0135 00188000
*               IF COMPCODE=ONE THEN/* AUTH FIELD UPDATED            */ 00189000
         CLI   COMPCODE,1                                          0136 00190000
         BNE   @RF00136                                            0136 00191000
*                 DO;                                              0137 00192000
*                   VERBNAME=VARYC; /* INIT MSG VARIABLE FIELD       */ 00193000
         MVC   VERBNAME(8),VARYC                                   0138 00194000
*                   MSGCODE=MSG3;   /* COMPLETION MSG - IEE712I      */ 00195000
         MVC   MSGCODE(1),MSG3                                     0139 00196000
*                 END;                                             0140 00197000
*             END;                                                 0141 00198000
@RF00136 DS    0H                                                  0142 00199000
*         END;                                                     0142 00200000
@RF00132 DS    0H                                                  0143 00201000
*       CALL MSGRTN;                /* SET UP FOR MSG ISSUE          */ 00202000
@RF00110 BAL   @14,MSGRTN                                          0143 00203000
*       GEN( DELETE EP=IEE0503D);   /* FREE MSG MODULE               */ 00204000
          DELETE EP=IEE0503D                                            00205000
*     END;                                                         0145 00206000
*   CALL FREECSCB;                  /* FREE CSCB CORE                */ 00207000
@RF00103 BAL   @14,FREECSCB                                        0146 00208000
*   REG14=R14SAVE;                  /* RESTORE RETURN ADDRESS        */ 00209000
         L     REG14,R14SAVE                                       0147 00210000
*   RETURN;                                                        0148 00211000
@EL00001 L     @13,4(,@13)                                         0148 00212000
@EF00001 L     @00,@SIZDATD                                        0148 00213000
         LR    @01,@12                                             0148 00214000
         FREEMAIN R,LV=(0),A=(1)                                        00215000
@ER00001 BR    @14                                                 0148 00216000
*EOBRTN:                                                           0149 00217000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0149 00218000
*                                                                  0149 00219000
EOBRTN   DS    0H                                                  0150 00220000
*   /*****************************************************************/ 00221000
*   /*                                                               */ 00222000
*   /* ENTRY IS TO DETERMINE THE SIZE OF THE INPUT, AND TO INITIALIZE*/ 00223000
*   /* THE FIELD - BUFSIZE, WITH THE ADDRESS OF END-OF-INPUT         */ 00224000
*   /*                                                               */ 00225000
*   /*****************************************************************/ 00226000
*                                                                  0150 00227000
*   DCL                                                            0150 00228000
*     EOBSIZE PTR(31);              /* END OF BUFFER PRE             */ 00229000
*   INPTR=ADDR(CHBUF);              /* INIT CSCB OPERAND ADDRESS     */ 00230000
         L     INPTR,CSCBPTR                                       0151 00231000
         LA    INPTR,CHBUF(,INPTR)                                 0151 00232000
*   EOBSIZE=INPTR+LENGTH(CHBUF);    /* ADDR OF BUFFER LENGTH         */ 00233000
         LA    EOBSIZE,124                                         0152 00234000
         ALR   EOBSIZE,INPTR                                       0152 00235000
*   DO WHILE(COLSCAN^=BLANK&INPTR<EOBSIZE);                        0153 00236000
         B     @DE00153                                            0153 00237000
@DL00153 DS    0H                                                  0154 00238000
*     INPTR=INPTR+ONE;              /* UPDATE SCAN POINTER           */ 00239000
         AL    INPTR,@CF00064                                      0154 00240000
*   END;                                                           0155 00241000
@DE00153 CLC   COLSCAN(1,INPTR),BLANK                              0155 00242000
         BE    @DC00153                                            0155 00243000
         CR    INPTR,EOBSIZE                                       0155 00244000
         BL    @DL00153                                            0155 00245000
@DC00153 DS    0H                                                  0156 00246000
*   BUFSIZE=INPTR;                  /* INITIALIZE INPUT LENGTH       */ 00247000
         LR    BUFSIZE,INPTR                                       0156 00248000
*   END EOBRTN;                                                    0157 00249000
@EL00002 DS    0H                                                  0157 00250000
@EF00002 DS    0H                                                  0157 00251000
@ER00002 BR    @14                                                 0157 00252000
*CNCHECK:                                                          0158 00253000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0158 00254000
*                                                                  0158 00255000
CNCHECK  DS    0H                                                  0159 00256000
*   /*****************************************************************/ 00257000
*   /*                                                               */ 00258000
*   /* ENTRY HERE IS TO VALIDITY CHECK THE 'CN' PARAMETER. VALID     */ 00259000
*   /* VALUES ARE 'CN('. IF VALUE IS 'CN(', SUBRTN PARENSCN IS CALLED*/ 00260000
*   /* TO VERIFY BALANCED PARENS. VALID VALUES - PTR (INPTR) IS      */ 00261000
*   /* BUMPED TO EXPECTED OPERAND . INDICATOR, PTR TO PARENTHEZIED   */ 00262000
*   /* CONSOLE ID(S) IS STORED IN PARAMETER LIST FIELD - CONIDPTR.   */ 00263000
*   /* INVALID VALUES - ERROR RETURN CODE, ERROR MSG CODE IS SET - . */ 00264000
*   /* INVALID PARAMETER - MSG10 - IEE310I . UNBALANCED PAREN - MSG7 */ 00265000
*   /* - IEE307I. INVALID DELIMITER - COMMA, - MSG3E - IEE535I       */ 00266000
*   /*                                                               */ 00267000
*   /*****************************************************************/ 00268000
*                                                                  0159 00269000
*   SAVE1=REG14;                    /* SAVE RETURN ADDRESS           */ 00270000
         ST    REG14,SAVE1                                         0159 00271000
*   INPTR=ADDR(CHBUF);              /* PTR TO COMMAND OPERAND FIELD  */ 00272000
         L     @10,CSCBPTR                                         0160 00273000
         LA    INPTR,CHBUF(,@10)                                   0160 00274000
*   IF CHBUF(1:3)=CNP THEN          /* VALID PARAMETER?              */ 00275000
         CLC   CHBUF(3,@10),CNP                                    0161 00276000
         BNE   @RF00161                                            0161 00277000
*     DO;                                                          0162 00278000
*       INPTR=INPTR+THREE;          /* BUMP PAST 'CN(' FIELD         */ 00279000
         AL    INPTR,@CF00282                                      0163 00280000
*       CONIDPTR=INPTR;             /* SAVE UCMID PTR IN PARM LIST   */ 00281000
         ST    INPTR,CONIDPTR                                      0164 00282000
*       CALL PARENSCN;              /* CHECK FOR BALANCE PARENS      */ 00283000
         BAL   @14,PARENSCN                                        0165 00284000
*       IF MSGCODE=ZERO THEN        /* PARENS O.K.                   */ 00285000
         CLI   MSGCODE,0                                           0166 00286000
         BNE   @RF00166                                            0166 00287000
*         DO;                                                      0167 00288000
*           INPTR=INPTR+ONE;        /* PTR TO EXPECTED DELIM, COMMA  */ 00289000
         AL    INPTR,@CF00064                                      0168 00290000
*           IF INPTR->COLSCAN=COMMA THEN/* DELIM PRESENT             */ 00291000
         CLC   COLSCAN(1,INPTR),COMMA                              0169 00292000
         BNE   @RF00169                                            0169 00293000
*             CALL DELIMRTN;        /* VERIFY CN VALUES              */ 00294000
         BAL   @14,DELIMRTN                                        0170 00295000
*           ELSE                                                   0171 00296000
*             MSGCODE=MSG3E;        /* SET MSG CODE - IEE535I        */ 00297000
         B     @RC00169                                            0171 00298000
@RF00169 MVC   MSGCODE(1),MSG3E                                    0171 00299000
*         END;                                                     0172 00300000
*     END;                                                         0173 00301000
*   ELSE                            /* INVALID PARAMETER             */ 00302000
*     MSGCODE=MSG10;                /* SET MSG CODE - IEE3103I       */ 00303000
         B     @RC00161                                            0174 00304000
@RF00161 MVC   MSGCODE(1),MSG10                                    0174 00305000
*   REG14=SAVE1;                    /* RESTORE RETURN ADDRESS        */ 00306000
@RC00161 L     REG14,SAVE1                                         0175 00307000
*   END CNCHECK;                                                   0176 00308000
@EL00003 DS    0H                                                  0176 00309000
@EF00003 DS    0H                                                  0176 00310000
@ER00003 BR    @14                                                 0176 00311000
*PARENSCN:                                                         0177 00312000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0177 00313000
*                                                                  0177 00314000
PARENSCN DS    0H                                                  0178 00315000
*   /*****************************************************************/ 00316000
*   /*                                                               */ 00317000
*   /* ENTRY IS TO SCAN FOR A BALANCED PAREN-(RIGHT PAREN). ON ENTRY,*/ 00318000
*   /* INPTR IS POINTING TO THE FIRST CHARACTER PAST A LEFT PAREN.   */ 00319000
*   /* ERROR CONDITIONS ARE - SCANNING A LEFT PAREN, SCANNING TO END */ 00320000
*   /* OF INPUT BEFORE SCANNING A RIGHT PAREN, NO DATA IN PARENS.IF  */ 00321000
*   /* ERROR, SET MSGCODE FIELD. RETURN TO CALLER.                   */ 00322000
*   /*                                                               */ 00323000
*   /*****************************************************************/ 00324000
*                                                                  0178 00325000
*   CNT=ONE;                        /* INIT CHARACTER COUNT FIELD    */ 00326000
         MVI   CNT,X'01'                                           0178 00327000
*   DO WHILE(COLSCAN^=LPAREN&COLSCAN^=RPAREN&MSGCODE=ZERO);        0179 00328000
         B     @DE00179                                            0179 00329000
@DL00179 DS    0H                                                  0180 00330000
*     IF INPTR<BUFSIZE THEN         /* NOT END OF INPUT              */ 00331000
         CR    INPTR,BUFSIZE                                       0180 00332000
         BNL   @RF00180                                            0180 00333000
*       DO;                                                        0181 00334000
*         CNT=CNT+ONE;              /* INCREMENT COUNT FIELD         */ 00335000
         LA    @10,1                                               0182 00336000
         SLR   @08,@08                                             0182 00337000
         IC    @08,CNT                                             0182 00338000
         ALR   @10,@08                                             0182 00339000
         STC   @10,CNT                                             0182 00340000
*         INPTR=INPTR+ONE;          /* BUMP TO NEXT CHARACTER        */ 00341000
         AL    INPTR,@CF00064                                      0183 00342000
*       END;                                                       0184 00343000
*     ELSE                          /* END OF INPUT, NO RPAREN       */ 00344000
*       MSGCODE=MSG7;               /* SET ERROR MSG CODE            */ 00345000
         B     @RC00180                                            0185 00346000
@RF00180 MVC   MSGCODE(1),MSG7                                     0185 00347000
*   END;                                                           0186 00348000
@RC00180 DS    0H                                                  0186 00349000
@DE00179 CLC   COLSCAN(1,INPTR),LPAREN                             0186 00350000
         BE    @DC00179                                            0186 00351000
         CLC   COLSCAN(1,INPTR),RPAREN                             0186 00352000
         BE    @DC00179                                            0186 00353000
         CLI   MSGCODE,0                                           0186 00354000
         BE    @DL00179                                            0186 00355000
@DC00179 DS    0H                                                  0187 00356000
*   IF COLSCAN=LPAREN THEN          /* INVLAID DELIMITER             */ 00357000
         CLC   COLSCAN(1,INPTR),LPAREN                             0187 00358000
         BNE   @RF00187                                            0187 00359000
*     MSGCODE=MSG7;                 /* SET MSG CODE                  */ 00360000
         MVC   MSGCODE(1),MSG7                                     0188 00361000
*   IF COLSCAN=RPAREN THEN          /* BALANCED PARENS FOUND         */ 00362000
@RF00187 CLC   COLSCAN(1,INPTR),RPAREN                             0189 00363000
         BNE   @RF00189                                            0189 00364000
*     IF CNT<TWO THEN               /* NO DATA IN PARENS             */ 00365000
         CLI   CNT,2                                               0190 00366000
         BNL   @RF00190                                            0190 00367000
*       MSGCODE=MSG11;              /* SET ERROR CODE - IEE311I      */ 00368000
         MVC   MSGCODE(1),MSG11                                    0191 00369000
*   END PARENSCN;                                                  0192 00370000
@EL00004 DS    0H                                                  0192 00371000
@EF00004 DS    0H                                                  0192 00372000
@ER00004 BR    @14                                                 0192 00373000
*DELIMRTN:                                                         0193 00374000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0193 00375000
*                                                                  0193 00376000
DELIMRTN DS    0H                                                  0194 00377000
*   /*****************************************************************/ 00378000
*   /*                                                               */ 00379000
*   /* SCAN INPUT FOR A DELIMITER - RPAREN, OR A VALID LENGTH, (1/2  */ 00380000
*   /* DIGITS) OR NUMERIC VALUE - 0/9. ANY OTHER VALUES RESULT IN    */ 00381000
*   /* ERROR MSG BEING ISSUED, AND END OF PROCESSING FOR VARY        */ 00382000
*   /* COMMAND. RETURN TO CALLER .                                   */ 00383000
*   /*                                                               */ 00384000
*   /*****************************************************************/ 00385000
*                                                                  0194 00386000
*   DCL                                                            0194 00387000
*     SCANPTR PTR(31);              /* PTR FOT INPUT SCAN            */ 00388000
*   DCL                                                            0195 00389000
*     CHRSCAN CHAR(1) BASED(SCANPTR);/* SCAN PTR FOR CHAR VALUE      */ 00390000
*   DCL                                                            0196 00391000
*     FOX0 CHAR(1) INIT('0');       /* LOW LIMIT VALUE               */ 00392000
*   DCL                                                            0197 00393000
*     FOX9 CHAR(1) INIT('9');       /* HIGH LIMIT VALUE              */ 00394000
*   SCANPTR=CONIDPTR;               /* WORK PTR FOR SCAN             */ 00395000
         L     SCANPTR,CONIDPTR                                    0198 00396000
*   CNT=ONE;                        /* INIT COUNT FIELD              */ 00397000
         MVI   CNT,X'01'                                           0199 00398000
*   DO WHILE(CHRSCAN^=RPAREN&       /* END OF SCAN INDICATOR         */ 00399000
*         MSGCODE=ZERO);            /* NO ERROR INDICATOR            */ 00400000
         B     @DE00200                                            0200 00401000
@DL00200 DS    0H                                                  0201 00402000
*     IF CHRSCAN>=FOX0&             /* NUMERIC ZERO OR GREATER       */ 00403000
*         CHRSCAN<=FOX9|            /* NUMERIC NINE OR LESS          */ 00404000
*         CHRSCAN=COMMA THEN        /* VALUE DELIMITER COMMA         */ 00405000
         CLC   CHRSCAN(1,SCANPTR),FOX0                             0201 00406000
         BL    @GL00005                                            0201 00407000
         CLC   CHRSCAN(1,SCANPTR),FOX9                             0201 00408000
         BNH   @RT00201                                            0201 00409000
@GL00005 CLC   CHRSCAN(1,SCANPTR),COMMA                            0201 00410000
         BNE   @RF00201                                            0201 00411000
@RT00201 DS    0H                                                  0202 00412000
*       DO;                                                        0202 00413000
*         IF CHRSCAN=COMMA THEN     /* COMMA DELIMITER               */ 00414000
         CLC   CHRSCAN(1,SCANPTR),COMMA                            0203 00415000
         BNE   @RF00203                                            0203 00416000
*           IF CNT<TWO|             /* VALUE LENGTH OF ZERO          */ 00417000
*               CNT>THREE THEN      /* VALUE LENGTH > TWO CHARS      */ 00418000
         CLI   CNT,2                                               0204 00419000
         BL    @RT00204                                            0204 00420000
         CLI   CNT,3                                               0204 00421000
         BNH   @RF00204                                            0204 00422000
@RT00204 DS    0H                                                  0205 00423000
*             MSGCODE=MSG6;         /* SET ERROR CODE - IEE306I      */ 00424000
         MVC   MSGCODE(1),MSG6                                     0205 00425000
*           ELSE                    /* VALID LENGTH VALUE            */ 00426000
*             CNT=ZERO;             /* RE-SET COUNT VALUE            */ 00427000
         B     @RC00204                                            0206 00428000
@RF00204 MVI   CNT,X'00'                                           0206 00429000
*         SCANPTR=SCANPTR+ONE;      /* BUMP SCAN PTR                 */ 00430000
@RC00204 DS    0H                                                  0207 00431000
@RF00203 LA    @10,1                                               0207 00432000
         ALR   SCANPTR,@10                                         0207 00433000
*         CNT=CNT+ONE;              /* BUMP COUNT FIELD              */ 00434000
         SLR   @08,@08                                             0208 00435000
         IC    @08,CNT                                             0208 00436000
         ALR   @10,@08                                             0208 00437000
         STC   @10,CNT                                             0208 00438000
*       END;                                                       0209 00439000
*     ELSE                          /* INVALID NUMERIC               */ 00440000
*       MSGCODE=MSG6;               /* SET ERROR MSG - IEE306I       */ 00441000
         B     @RC00201                                            0210 00442000
@RF00201 MVC   MSGCODE(1),MSG6                                     0210 00443000
*   END;                                                           0211 00444000
@RC00201 DS    0H                                                  0211 00445000
@DE00200 CLC   CHRSCAN(1,SCANPTR),RPAREN                           0211 00446000
         BE    @DC00200                                            0211 00447000
         CLI   MSGCODE,0                                           0211 00448000
         BE    @DL00200                                            0211 00449000
@DC00200 DS    0H                                                  0212 00450000
*   IF CHRSCAN=RPAREN THEN          /* RIGHT PAREN DELIMITER         */ 00451000
         CLC   CHRSCAN(1,SCANPTR),RPAREN                           0212 00452000
         BNE   @RF00212                                            0212 00453000
*     IF CNT<TWO|                   /* ONE DIGIT PLUS DELIMITER      */ 00454000
*         CNT>THREE THEN            /* TWO DIGITS PLUS DELIMITER     */ 00455000
         CLI   CNT,2                                               0213 00456000
         BL    @RT00213                                            0213 00457000
         CLI   CNT,3                                               0213 00458000
         BNH   @RF00213                                            0213 00459000
@RT00213 DS    0H                                                  0214 00460000
*       MSGCODE=MSG6;               /* SET ERROR CODE - IEE306I      */ 00461000
         MVC   MSGCODE(1),MSG6                                     0214 00462000
*     ELSE                                                         0215 00463000
*       IF MSGCODE=ZERO THEN        /* NO ERROR INDICATOR            */ 00464000
         B     @RC00213                                            0215 00465000
@RF00213 CLI   MSGCODE,0                                           0215 00466000
         BNE   @RF00215                                            0215 00467000
*         IF CONIDPTR=SCANPTR THEN  /* NO VALID INPUT                */ 00468000
         C     SCANPTR,CONIDPTR                                    0216 00469000
         BNE   @RF00216                                            0216 00470000
*           MSGCODE=MSG11;          /* SET ERROR CODE - IEE311I      */ 00471000
         MVC   MSGCODE(1),MSG11                                    0217 00472000
*         ELSE                                                     0218 00473000
*           ;                                                      0218 00474000
@RF00216 DS    0H                                                  0219 00475000
*   END DELIMRTN;                                                  0219 00476000
@EL00005 DS    0H                                                  0219 00477000
@EF00005 DS    0H                                                  0219 00478000
@ER00005 BR    @14                                                 0219 00479000
*VALIDSCN:                                                         0220 00480000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0220 00481000
*                                                                  0220 00482000
VALIDSCN DS    0H                                                  0221 00483000
*   /*****************************************************************/ 00484000
*   /*                                                               */ 00485000
*   /* ENTRY HERE IS TO DETERMINE THAT THE OPERAND(S) IS VALID. INPUT*/ 00486000
*   /* IS REG15 PTR TO OPERAND/VALUE TABLE, AS MAPPED BY STRUCTURE - */ 00487000
*   /* TABMAP. INITIALIZE THE TABLE INDEX VALUE - TABINDX AND COMPARE*/ 00488000
*   /* INPUT VALUE TO THE TABLE ENTRY. IF NO MATCH, INCREMENT THE    */ 00489000
*   /* TABINDX VALUE AND COMPARE TO NEXT TABLE ENTRY. IF NO MATCH    */ 00490000
*   /* FOUND IN TABLE, SET ERROR VALUES, RETURN TO CALLER IF A MATCH */ 00491000
*   /* IS FOUND, REG15 CONTAINS THE ADDRESS OF THE MATCH- ING        */ 00492000
*   /* OPERAND. RETURN TO CALLER.                                    */ 00493000
*   /*                                                               */ 00494000
*   /*****************************************************************/ 00495000
*                                                                  0221 00496000
*   DCL                                                            0221 00497000
*     1 TABMAP BASED(APTR),                                        0221 00498000
*                                                                  0221 00499000
*     /***************************************************************/ 00500000
*     /*                                                             */ 00501000
*     /* THIS STRUCTURE IS USED TO MAP THE CONTENTS OF OPERAND AND   */ 00502000
*     /* VALUE TABLES. OFFSETS ARE FOR LENGTH, AND DATA FIELDS.      */ 00503000
*     /*                                                             */ 00504000
*     /***************************************************************/ 00505000
*                                                                  0221 00506000
*      2 DATALEN PTR(8),            /* LENGTH FIELD                  */ 00507000
*      2 DATAWORD CHAR(*);          /* CHARACTER DATA FIELD          */ 00508000
*   DCL                                                            0222 00509000
*     CHAR8 CHAR(*) BASED;          /* CHAR REFERENCE FOR COMPARE    */ 00510000
*   DCL                                                            0223 00511000
*     ADDRINDX PTR(8);              /* ADDRESSING INDEX PTR          */ 00512000
*   ADDRINDX=ZERO;                  /* SET FIELD OFF                 */ 00513000
         SLR   ADDRINDX,ADDRINDX                                   0224 00514000
*   TABINDX=ZERO;                   /* SET TABLE INDEX VALUE         */ 00515000
         MVI   TABINDX,X'00'                                       0225 00516000
*   SPINSW=ONE;                     /* SET LOOP INDICATOR            */ 00517000
         MVI   SPINSW,X'01'                                        0226 00518000
*   DO WHILE(SPINSW=ONE&            /* LOOP INDICATOR = ON           */ 00519000
*         DATAWORD(1:2)^=FOXDATA);  /* END OF TABLE INDICATOR        */ 00520000
         B     @DE00227                                            0227 00521000
@DL00227 DS    0H                                                  0228 00522000
*     REG15=REG15+ADDRINDX;         /* TABLE OFFSET OF OPERAND       */ 00523000
         ALR   REG15,ADDRINDX                                      0228 00524000
*     MASKTAB=DATALEN;              /* OPERAND LENGTH                */ 00525000
         L     @10,APTR(,REG15)                                    0229 00526000
         MVC   MASKTAB(1),DATALEN(@10)                             0229 00527000
*     IF INPTR->CHAR8(1:MASKTAB)^=DATAWORD(1:MASKTAB) THEN         0230 00528000
         L     @10,APTR(,REG15)                                    0230 00529000
         SLR   @03,@03                                             0230 00530000
         IC    @03,MASKTAB                                         0230 00531000
         BCTR  @03,0                                               0230 00532000
         EX    @03,@SC01373                                        0230 00533000
         BE    @RF00230                                            0230 00534000
*       DO;                         /* NO COMPARE MATCH              */ 00535000
*         ADDRINDX=FOUR;            /* SET ADDRESS INCREMENT         */ 00536000
         LA    @10,4                                               0232 00537000
         LR    ADDRINDX,@10                                        0232 00538000
*         TABINDX=TABINDX+FOUR;     /* NO, UPDATE TABLE INDEX.       */ 00539000
         SLR   @03,@03                                             0233 00540000
         IC    @03,TABINDX                                         0233 00541000
         ALR   @10,@03                                             0233 00542000
         STC   @10,TABINDX                                         0233 00543000
*       END;                                                       0234 00544000
*     ELSE                                                         0235 00545000
*       SPINSW=ZERO;                /* YES, EXIT FROM LOOP           */ 00546000
         B     @RC00230                                            0235 00547000
@RF00230 MVI   SPINSW,X'00'                                        0235 00548000
*   END;                                                           0236 00549000
@RC00230 DS    0H                                                  0236 00550000
@DE00227 CLI   SPINSW,1                                            0236 00551000
         BNE   @DC00227                                            0236 00552000
         L     @10,APTR(,REG15)                                    0236 00553000
         CLC   DATAWORD(2,@10),FOXDATA                             0236 00554000
         BNE   @DL00227                                            0236 00555000
@DC00227 DS    0H                                                  0237 00556000
*   IF DATAWORD(1:2)=FOXDATA THEN   /* INVALID OPERAND               */ 00557000
         L     @10,APTR(,REG15)                                    0237 00558000
         CLC   DATAWORD(2,@10),FOXDATA                             0237 00559000
         BNE   @RF00237                                            0237 00560000
*     DO;                                                          0238 00561000
*       MSGCODE=MSG3D;              /* SET MSG CODE - IEE708I        */ 00562000
         MVC   MSGCODE(1),MSG3D                                    0239 00563000
*       REG15=ZERO;                 /* ZERO ADDRESS REG              */ 00564000
         SLR   REG15,REG15                                         0240 00565000
*     END;                                                         0241 00566000
*   END VALIDSCN;                                                  0242 00567000
@EL00006 DS    0H                                                  0242 00568000
@EF00006 DS    0H                                                  0242 00569000
@ER00006 BR    @14                                                 0242 00570000
*AUTHRTN:                                                          0243 00571000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0243 00572000
*                                                                  0243 00573000
AUTHRTN  DS    0H                                                  0244 00574000
*   /*****************************************************************/ 00575000
*   /*                                                               */ 00576000
*   /* ENTRY IS TO VALIDATE AND PROCESS THE VALUES SPECIFIED ON THE  */ 00577000
*   /* 'AUTH=' OPERAND. COMMAND ISSUER MUST BE AUTHORIZED TO USE THIS*/ 00578000
*   /* OPERAND - SYSTEM (CHUCMP = ZERO) OR MASTER CONSOLE (UCMDISPA =*/ 00579000
*   /* ON). IF PARENS ARE PRESENT, THEY ARE TESTED FOR CLOSING PAREN.*/ 00580000
*   /* IF A SINGLE VALUE IS SPECIFIED, A TABLE OF VALID VALUES IS    */ 00581000
*   /* SCANNED AND COMPARED TO INPUT. IF A MATCH IS FOUND THE        */ 00582000
*   /* CORRESPONDING BIT PATTERN IS STORED IN THE PARAMETER LIST TO  */ 00583000
*   /* BE PASSED TO MODULE IEECB901. ELSE, ERROR FIELDS ARE          */ 00584000
*   /* INITIALIZED AND RETURN TO CALLER. PARENTHEZIED VALUES MAY     */ 00585000
*   /* CONTAIN MORE THAN ONE VALUE, SO THE INPUT IS SCANNED FOR A    */ 00586000
*   /* DELIMITER - (',' OR ')'), VALID VALUES ARE CHECKED, AND THE   */ 00587000
*   /* BIT PATTERN IS 'OR'D' INTO THE PARM LIST UNTIL THE INPUT HAS  */ 00588000
*   /* BEEN TOTALLY SCANNED. INVALID VALUES RESULT IN SETTING ERROR  */ 00589000
*   /* FIELDS AND RETURN TO CALLER. ON NORMAL EXIT FROM THIS RTN, THE*/ 00590000
*   /* VALUE - INPTR, SHOULE BE POINTING AT END OF OPERAND DELIMITER */ 00591000
*   /* - COMMA, OR END-OF INPUT                                      */ 00592000
*   /*                                                               */ 00593000
*   /*****************************************************************/ 00594000
*                                                                  0244 00595000
*   DCL                                                            0244 00596000
*     VALUEPTR PTR(31);             /* TEMP PTR TO INPUT, USED ONLY     00597000
*                                      BY THIS RTN                   */ 00598000
*   SAVE2=REG14;                    /* SAVE RETURN ADDRESS           */ 00599000
         ST    REG14,SAVE2                                         0245 00600000
*   IF CHUCMP=HEX80 THEN            /* COMMAND FROM INPUT STREAM?    */ 00601000
         L     @10,CSCBPTR                                         0246 00602000
         CLC   CHUCMP(1,@10),HEX80                                 0246 00603000
         BNE   @RF00246                                            0246 00604000
*     MSGCODE=MSG29;                /* SET MSG CODE - IEE345I        */ 00605000
         MVC   MSGCODE(1),MSG29                                    0247 00606000
*   ELSE                                                           0248 00607000
*     DO;                                                          0248 00608000
         B     @RC00246                                            0248 00609000
@RF00246 DS    0H                                                  0249 00610000
*       RFY                                                        0249 00611000
*         UCMLIST BASED(UCMPTR);                                   0249 00612000
*       UCMPTR=UCMVEA+(UCMVEZ*(CHUCMP-ONE));/* PTR TO ISSUER UCME    */ 00613000
         L     @10,CVTPTR                                          0250 00614000
         L     @10,UCMBASPT(,@10)                                  0250 00615000
         L     @08,CSCBPTR                                         0250 00616000
         SLR   UCMPTR,UCMPTR                                       0250 00617000
         IC    UCMPTR,CHUCMP(,@08)                                 0250 00618000
         BCTR  UCMPTR,0                                            0250 00619000
         LR    @05,UCMPTR                                          0250 00620000
         M     @04,UCMVEZ(,@10)                                    0250 00621000
         AL    @05,UCMVEA(,@10)                                    0250 00622000
         LR    UCMPTR,@05                                          0250 00623000
*       IF CHUCMP=ZERO|             /* THIS A SYSTEM ISSUER?         */ 00624000
*           UCMDISPA='1'B THEN      /* THIS MASTER CONSOLE?          */ 00625000
         CLI   CHUCMP(@08),0                                       0251 00626000
         BE    @RT00251                                            0251 00627000
         TM    UCMDISPA(UCMPTR),B'10000000'                        0251 00628000
         BNO   @RF00251                                            0251 00629000
@RT00251 DS    0H                                                  0252 00630000
*         DO;                                                      0252 00631000
*           VALUEPTR=INPTR+MASKTAB; /* INIT PTR TO OPERAND START     */ 00632000
         LR    VALUEPTR,INPTR                                      0253 00633000
         SLR   @10,@10                                             0253 00634000
         IC    @10,MASKTAB                                         0253 00635000
         ALR   VALUEPTR,@10                                        0253 00636000
*           IF VALUEPTR->CHAR1=LPAREN THEN/* VALUES IN PARENT?       */ 00637000
         CLC   CHAR1(1,VALUEPTR),LPAREN                            0254 00638000
         BNE   @RF00254                                            0254 00639000
*             DO;                                                  0255 00640000
*               INPTR=VALUEPTR+ONE; /* BUMP PAST LEFT PAREN          */ 00641000
         LA    INPTR,1                                             0256 00642000
         ALR   INPTR,VALUEPTR                                      0256 00643000
*               CALL PARENSCN;      /* CHECK PARENS BALANCED         */ 00644000
         BAL   @14,PARENSCN                                        0257 00645000
*               PARENSW=ONE;        /* SET PAREN SWITCH              */ 00646000
         MVI   PARENSW,X'01'                                       0258 00647000
*               INPTR=VALUEPTR+ONE; /* RE-SET VALUE PTR PAST LPAREN  */ 00648000
         LA    INPTR,1                                             0259 00649000
         ALR   INPTR,VALUEPTR                                      0259 00650000
*             END;                                                 0260 00651000
*           ELSE                                                   0261 00652000
*             DO;                                                  0261 00653000
         B     @RC00254                                            0261 00654000
@RF00254 DS    0H                                                  0262 00655000
*               INPTR=VALUEPTR;     /* RESET PTR TO OPERAND VALUE    */ 00656000
         LR    INPTR,VALUEPTR                                      0262 00657000
*               PARENSW=ZERO;       /* SET SWITCH OFF                */ 00658000
         MVI   PARENSW,X'00'                                       0263 00659000
*             END;                                                 0264 00660000
*           IF MSGCODE=ZERO THEN    /* PARENS BALANCED               */ 00661000
@RC00254 CLI   MSGCODE,0                                           0265 00662000
         BNE   @RF00265                                            0265 00663000
*             DO;                                                  0266 00664000
*               IF PARENSW=ZERO THEN/* SINGLE VALUE EXPECTED         */ 00665000
         CLI   PARENSW,0                                           0267 00666000
         BNE   @RF00267                                            0267 00667000
*                 DO;                                              0268 00668000
*                   REG15=ADDR(AUTHTAB);/* PTR TO VALID VALUE TABLE  */ 00669000
         LA    REG15,AUTHTAB                                       0269 00670000
*                   CALL VALIDSCN;  /* CHECK FOR VALID INPUT         */ 00671000
         BAL   @14,VALIDSCN                                        0270 00672000
*                   IF REG15^=ZERO THEN/* VALID INPUT FOUND          */ 00673000
         LTR   REG15,REG15                                         0271 00674000
         BZ    @RF00271                                            0271 00675000
*                     DO;                                          0272 00676000
*                       REG15=ADDR(BITSTAB)+TABINDX;/* PTR TO AUTH'TY   00677000
*                                      BIT MASK                      */ 00678000
         LA    REG15,BITSTAB                                       0273 00679000
         SLR   @10,@10                                             0273 00680000
         IC    @10,TABINDX                                         0273 00681000
         ALR   REG15,@10                                           0273 00682000
*                       AUTHMASK=APTR->CHAR1;/* INIT PARM LIST AREA  */ 00683000
         L     @10,APTR(,REG15)                                    0274 00684000
         MVC   AUTHMASK(1),CHAR1(@10)                              0274 00685000
*                       INPTR=VALUEPTR+MASKTAB;/* RE-SET OPER. PTR   */ 00686000
         LR    INPTR,VALUEPTR                                      0275 00687000
         SLR   @10,@10                                             0275 00688000
         IC    @10,MASKTAB                                         0275 00689000
         ALR   INPTR,@10                                           0275 00690000
*                     END;                                         0276 00691000
*                 END;                                             0277 00692000
*               ELSE                /* PARENTHESIED VALUES EXPECTED  */ 00693000
*                 DO;                                              0278 00694000
         B     @RC00267                                            0278 00695000
@RF00267 DS    0H                                                  0279 00696000
*                   CALL PARMLOOP;  /* PROCESS VALUES                */ 00697000
         BAL   @14,PARMLOOP                                        0279 00698000
*                   PARENSW=ZERO;   /* RE-SET PAREN INDICATOR        */ 00699000
         MVI   PARENSW,X'00'                                       0280 00700000
*                 END;                                             0281 00701000
*             END;                                                 0282 00702000
*         END;                                                     0283 00703000
*       ELSE                                                       0284 00704000
*         MSGCODE=MSG29;            /* SET MSG CODE - IEE345I        */ 00705000
         B     @RC00251                                            0284 00706000
@RF00251 MVC   MSGCODE(1),MSG29                                    0284 00707000
*     END;                                                         0285 00708000
@RC00251 DS    0H                                                  0286 00709000
*   IF MSGCODE=MSG3D THEN           /* INVALID OPERAND VALUE         */ 00710000
@RC00246 CLC   MSGCODE(1),MSG3D                                    0286 00711000
         BNE   @RF00286                                            0286 00712000
*     VERBNAME=AUTHDATA;            /* OPERAND NAME TO MSG VARIABLE  */ 00713000
         MVI   VERBNAME+5,C' '                                     0287 00714000
         MVC   VERBNAME+6(2),VERBNAME+5                            0287 00715000
         MVC   VERBNAME(5),AUTHDATA                                0287 00716000
*   REG14=SAVE2;                    /* RESTORE RETURN ADDRESS        */ 00717000
@RF00286 L     REG14,SAVE2                                         0288 00718000
*   END AUTHRTN;                                                   0289 00719000
@EL00007 DS    0H                                                  0289 00720000
@EF00007 DS    0H                                                  0289 00721000
@ER00007 BR    @14                                                 0289 00722000
*PARMLOOP:                                                         0290 00723000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0290 00724000
*                                                                  0290 00725000
PARMLOOP DS    0H                                                  0291 00726000
*   /*****************************************************************/ 00727000
*   /*                                                               */ 00728000
*   /* PROCESS THE PARENTHEZIED VALUES FOR 'AUTH=' OPERAND. AT ENTRY,*/ 00729000
*   /* SCANPTR IS INITIALIZED WITH THE ADDRESS OF VALUES FOLLOWING   */ 00730000
*   /* THE LEFT PAREN. SCAN FOR VALID OPERAND VALUE VIA-CALL TO VALID*/ 00731000
*   /* SCN. . IF FOUND - REG15 NON-ZERO - SET PTR TO TABLE OF BIT    */ 00732000
*   /* PATTERNS . AND 'OR' APPROPRIATE VALUES IN PARM LIST FIELD     */ 00733000
*   /* AUTHMASK. . COMPARE FOR VALID DELIMITER - COMMA/RIGHT PAREN. .*/ 00734000
*   /* IF NO DELIMITER FOUND, SET ERROR CODES AND RETURN TO CALLER . */ 00735000
*   /* ELSE, IF DELIMITER = COMMA, RE-SET SCAN PTR (INPTR) AND .     */ 00736000
*   /* CONTINUE SCAN. IF DELIMITER = RIGHT PAREN, END OF INPUT HAS . */ 00737000
*   /* BEEN REACHED. SET LOOP INDICATOR OFF, RETURN TO CALLER.       */ 00738000
*   /*                                                               */ 00739000
*   /*****************************************************************/ 00740000
*                                                                  0291 00741000
*   SAVE3=REG14;                    /* SAVE RETURN ADDRESS           */ 00742000
         ST    REG14,SAVE3                                         0291 00743000
*   SPINNER=ONE;                    /* SET UP LOOP INDICATOR         */ 00744000
         MVI   SPINNER,X'01'                                       0292 00745000
*   DO WHILE(MSGCODE=ZERO&          /* NO ERROR CONDITIONS           */ 00746000
*         SPINNER=ONE);             /* NOT END OF INPUT              */ 00747000
         B     @DE00293                                            0293 00748000
@DL00293 DS    0H                                                  0294 00749000
*     REG15=ADDR(PARENPTR);         /* PTR TO VALID PAREN VALUES     */ 00750000
         LA    REG15,PARENPTR                                      0294 00751000
*     CALL VALIDSCN;                /* CHECK FOR VALID VALUE         */ 00752000
         BAL   @14,VALIDSCN                                        0295 00753000
*     IF REG15=ZERO THEN            /* INVALID VALUE                 */ 00754000
         LTR   REG15,REG15                                         0296 00755000
         BNZ   @RF00296                                            0296 00756000
*       SPINNER=ZERO;               /* SET EXIT INDICATOR            */ 00757000
         MVI   SPINNER,X'00'                                       0297 00758000
*     ELSE                          /* SCAN SUCCESSFUL               */ 00759000
*       DO;                                                        0298 00760000
         B     @RC00296                                            0298 00761000
@RF00296 DS    0H                                                  0299 00762000
*         REG15=ADDR(PRNBADR)+TABINDX;/* PTR TO BIT PATTERN TABLE    */ 00763000
         LA    REG15,PRNBADR                                       0299 00764000
         SLR   @10,@10                                             0299 00765000
         IC    @10,TABINDX                                         0299 00766000
         ALR   REG15,@10                                           0299 00767000
*         AUTHMASK=AUTHMASK|APTR->CHAR1;/* 'OR' IN BIT VALUES        */ 00768000
         L     @10,APTR(,REG15)                                    0300 00769000
         SLR   @08,@08                                             0300 00770000
         IC    @08,AUTHMASK                                        0300 00771000
         SLR   @07,@07                                             0300 00772000
         IC    @07,CHAR1(,@10)                                     0300 00773000
         OR    @08,@07                                             0300 00774000
         STC   @08,AUTHMASK                                        0300 00775000
*         REG15=ADDR(PARENPTR)+TABINDX;/* PTR TO VALUE LENGTH        */ 00776000
         LA    REG15,PARENPTR                                      0301 00777000
         SLR   @10,@10                                             0301 00778000
         IC    @10,TABINDX                                         0301 00779000
         ALR   REG15,@10                                           0301 00780000
*         INPTR=INPTR+APTR->CHAR1;  /* BUMP PAST OPERAND VALUE       */ 00781000
         L     @10,APTR(,REG15)                                    0302 00782000
         SLR   @08,@08                                             0302 00783000
         IC    @08,CHAR1(,@10)                                     0302 00784000
         ALR   INPTR,@08                                           0302 00785000
*         IF INPTR->CHAR1^=COMMA&   /* COMMA DELIMITER?              */ 00786000
*             INPTR->CHAR1^=RPAREN THEN/* END OF INPUT DELIMITER     */ 00787000
         CLC   CHAR1(1,INPTR),COMMA                                0303 00788000
         BE    @RF00303                                            0303 00789000
         CLC   CHAR1(1,INPTR),RPAREN                               0303 00790000
         BE    @RF00303                                            0303 00791000
*           MSGCODE=MSG3D;          /* SET ERROR MSG - IEE708I       */ 00792000
         MVC   MSGCODE(1),MSG3D                                    0304 00793000
*         ELSE                                                     0305 00794000
*           DO;                                                    0305 00795000
         B     @RC00303                                            0305 00796000
@RF00303 DS    0H                                                  0306 00797000
*             IF INPTR->CHAR1=RPAREN THEN/* CONTINUE SCAN ?          */ 00798000
         CLC   CHAR1(1,INPTR),RPAREN                               0306 00799000
         BNE   @RF00306                                            0306 00800000
*               SPINNER=ZERO;       /* SET EXIT INDICATOR            */ 00801000
         MVI   SPINNER,X'00'                                       0307 00802000
*             INPTR=INPTR+ONE;      /* BUMP TO NEXT DELIMITER, COMMA/   00803000
*                                      BLANK                         */ 00804000
@RF00306 AL    INPTR,@CF00064                                      0308 00805000
*           END;                                                   0309 00806000
*       END;                                                       0310 00807000
@RC00303 DS    0H                                                  0311 00808000
*   END;                                                           0311 00809000
@RC00296 DS    0H                                                  0311 00810000
@DE00293 CLI   MSGCODE,0                                           0311 00811000
         BNE   @DC00293                                            0311 00812000
         CLI   SPINNER,1                                           0311 00813000
         BE    @DL00293                                            0311 00814000
@DC00293 DS    0H                                                  0312 00815000
*   REG14=SAVE3;                    /* RESTORE RETURN ADDRESS        */ 00816000
         L     REG14,SAVE3                                         0312 00817000
*   END PARMLOOP;                                                  0313 00818000
@EL00008 DS    0H                                                  0313 00819000
@EF00008 DS    0H                                                  0313 00820000
@ER00008 BR    @14                                                 0313 00821000
*MSGRTN:                                                           0314 00822000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0314 00823000
*                                                                  0314 00824000
MSGRTN   DS    0H                                                  0315 00825000
*   /*****************************************************************/ 00826000
*   /*                                                               */ 00827000
*   /* SET UP TO ISSUE MESSAGES VIA MODULE IEE0503D, WHICH MAPS ITS  */ 00828000
*   /* INPUT TO THE XSA MAPPING MACRO. SET WORKAREA BASED ON XSA,    */ 00829000
*   /* CLEAR CORE, AND INITIALIZE THE PARM LIST AND CALL THE MESSAGE */ 00830000
*   /* MODULE. NO RETURN CODE IS EXPECTED - RETURN TO CALLER.        */ 00831000
*   /*                                                               */ 00832000
*   /*****************************************************************/ 00833000
*                                                                  0315 00834000
*   DCL                                                            0315 00835000
*     MSGMOD ENTRY BASED(REG15);    /* ENTRY TO ESTABLISH CALL     0315 00836000
*                                      CAPABILITY TO MSG MODULE ADDR */ 00837000
*   SAVE1=REG14;                    /* SAVE RETURN ADDRESS           */ 00838000
         ST    REG14,SAVE1                                         0316 00839000
*   REG2=ADDR(MSGAREA);             /* PTR TO PARM LIST              */ 00840000
         LA    REG2,MSGAREA                                        0317 00841000
*   REG0=LENGTH(XASVRB);            /* LENGTH OF CORE TO CLEAR       */ 00842000
         LA    REG0,48                                             0318 00843000
*   XSAMAP(1:REG0)=XSAMAP(1:REG0)&&XSAMAP(1:REG0);/* ZERO PARM CORE  */ 00844000
         LR    @10,REG0                                            0319 00845000
         BCTR  @10,0                                               0319 00846000
         EX    @10,@SX01375                                        0319 00847000
*   XAE=MSGCODE;                    /* PTR TO APPROPRIATE MESSAGE    */ 00848000
         MVC   XAE(1,XSAPTR),MSGCODE                               0320 00849000
*   XAV=VERBNAME;                   /* FILLER FOR MESSAGE            */ 00850000
         MVC   XAV(8,XSAPTR),VERBNAME                              0321 00851000
*   IF CHUCMP=HEX80 THEN            /* COMMAND FROM INPUT STREAM     */ 00852000
         L     @10,CSCBPTR                                         0322 00853000
         CLC   CHUCMP(1,@10),HEX80                                 0322 00854000
         BNE   @RF00322                                            0322 00855000
*     XAU=ZERO;                     /* SET MASTER CONSOLE ISSUER     */ 00856000
         MVI   XAU(XSAPTR),X'00'                                   0323 00857000
*   ELSE                                                           0324 00858000
*     XAU=CHUCMP;                   /* CONSOLE ID OF ISSUER          */ 00859000
         B     @RC00322                                            0324 00860000
@RF00322 L     @10,CSCBPTR                                         0324 00861000
         MVC   XAU(1,XSAPTR),CHUCMP(@10)                           0324 00862000
*   REG15=MSGMPTR;                  /* ADDRESS OF IEE0503D           */ 00863000
@RC00322 L     REG15,MSGMPTR                                       0325 00864000
*   CALL MSGMOD;                    /* ISSUE WTO VIA IEE0503D        */ 00865000
         BALR  @14,@15                                             0326 00866000
*   REG14=SAVE1;                    /* RESTORE RETURN ADDRESS        */ 00867000
         L     REG14,SAVE1                                         0327 00868000
*   END MSGRTN;                                                    0328 00869000
@EL00009 DS    0H                                                  0328 00870000
@EF00009 DS    0H                                                  0328 00871000
@ER00009 BR    @14                                                 0328 00872000
*FREECSCB:                                                         0329 00873000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0329 00874000
*                                                                  0329 00875000
FREECSCB DS    0H                                                  0330 00876000
*   /*****************************************************************/ 00877000
*   /*                                                               */ 00878000
*   /* THIS RTN FREE THE CSCB VIA THE MGCR MACRO                     */ 00879000
*   /*                                                               */ 00880000
*   /*****************************************************************/ 00881000
*                                                                  0330 00882000
*   REG1=CSCBPTR;                   /* SET PTR TO CSCB               */ 00883000
         L     @10,CSCBPTR                                         0330 00884000
         LR    REG1,@10                                            0330 00885000
*   CHFC='1'B;                      /* SET FREE CSCB BIT ON          */ 00886000
         OI    CHFC(@10),B'00000010'                               0331 00887000
*   GEN(MGCR (1),CHAIN);            /* ISSUE MACRO                   */ 00888000
         MGCR (1),CHAIN                                                 00889000
*   END FREECSCB;                                                  0333 00890000
@EL00010 DS    0H                                                  0333 00891000
@EF00010 DS    0H                                                  0333 00892000
@ER00010 BR    @14                                                 0333 00893000
*STAERTN:                                                          0334 00894000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0334 00895000
*                                                                  0334 00896000
STAERTN  DS    0H                                                  0335 00897000
*   /*****************************************************************/ 00898000
*   /*                                                               */ 00899000
*   /* ENTRY HERE IS TO ESTABLISH AN ESTAE ENVIRONMENT FOR THIS      */ 00900000
*   /* MODULE. A PARAMETER LIST IS INITIALIZED AND THE ESTAE MODULE  */ 00901000
*   /* IEECB860 IS LOADED, CONTROL IS PASSED TO IT WITH REG1 PTR TO  */ 00902000
*   /* THE PARM LIST. ON RETURN, RETURN CODE - REG15- IS SAVED.      */ 00903000
*   /* IEECB860 IS DELETED, AND RETURN TO CALLER WITH REG15 RETURN   */ 00904000
*   /* CODE.                                                         */ 00905000
*   /*                                                               */ 00906000
*   /*****************************************************************/ 00907000
*                                                                  0335 00908000
*   DCL                                                            0335 00909000
*     1 STAEPARM BASED(REG1),                                      0335 00910000
*                                                                  0335 00911000
*     /***************************************************************/ 00912000
*     /*                                                             */ 00913000
*     /* THIS IS A MAPPING OF THE PARAMETER AREA PASSED TO IEECB860  */ 00914000
*     /*                                                             */ 00915000
*     /***************************************************************/ 00916000
*                                                                  0335 00917000
*      2 CSCBADDR PTR(31),          /* CSCB PTR                      */ 00918000
*      2 CMNDPTR PTR(31),           /* PTR TO COMMAND, LENGTH        */ 00919000
*      2 CMNDBUF,                                                  0335 00920000
*       3 CMNDSIZE CHAR(1),         /* COMMAND LENGTH                */ 00921000
*       3 CMNDWORD CHAR(7),         /* COMMAND WORD                  */ 00922000
*      2 SAVEPTRS,                                                 0335 00923000
*       3 REG11SAV PTR(31),         /* REG11 SAVE PTR                */ 00924000
*       3 REG12SAV PTR(31);         /* REG12 SAVE PTR                */ 00925000
*   DCL                                                            0336 00926000
*     STAEMOD ENTRY BASED(REG15);   /* ENTRY TO ESTABLISH A CALL   0336 00927000
*                                      CAPABILITY TO ESTAE MODULE    */ 00928000
*   SAVE1=REG14;                    /* SAVE RETURN ADDRESS           */ 00929000
         ST    REG14,SAVE1                                         0337 00930000
*   REG1=ADDR(MSGAREA);             /* PTR TO PARM AREA CORE         */ 00931000
         LA    REG1,MSGAREA                                        0338 00932000
*   CSCBADDR=CSCBPTR;               /* PTR TO CSCB                   */ 00933000
         L     @10,CSCBPTR                                         0339 00934000
         ST    @10,CSCBADDR(,REG1)                                 0339 00935000
*   CMNDPTR=ADDR(CMNDBUF);          /* PTR TO COMMAND, LENGTH        */ 00936000
         LA    @10,CMNDBUF(,REG1)                                  0340 00937000
         ST    @10,CMNDPTR(,REG1)                                  0340 00938000
*   CMNDSIZE=FOUR;                  /* SET COMMAND LENGTH            */ 00939000
         MVI   CMNDSIZE(REG1),X'04'                                0341 00940000
*   CMNDWORD=VARYC;                 /* INIT COMMAND WORD             */ 00941000
         MVC   CMNDWORD(7,REG1),VARYC                              0342 00942000
*   REG11SAV=REG11;                 /* SAVE BASE REG                 */ 00943000
         ST    REG11,REG11SAV(,REG1)                               0343 00944000
*   REG12SAV=REG12;                 /* SAVE DATA REG                 */ 00945000
         ST    REG12,REG12SAV(,REG1)                               0344 00946000
*   REG2=REG1;                      /* SAVE PARM PTR                 */ 00947000
         LR    REG2,REG1                                           0345 00948000
*   GEN( LOAD EP=IEECB860);         /* BRING IN ESTAE RTN            */ 00949000
          LOAD EP=IEECB860                                              00950000
*   REG15=REG0;                     /* PTR TO IEECB860               */ 00951000
         LR    REG15,REG0                                          0347 00952000
*   REG1=REG2;                      /* RESTORE PARM PTR              */ 00953000
         LR    REG1,REG2                                           0348 00954000
*   CALL STAEMOD;                   /* CALL ESTAE RTN                */ 00955000
         BALR  @14,@15                                             0349 00956000
*   REG11=REG11SAV;                 /* RESTORE                       */ 00957000
         L     REG11,REG11SAV(,REG1)                               0350 00958000
*   REG12=REG12SAV;                 /* REGISTERS                     */ 00959000
         L     REG12,REG12SAV(,REG1)                               0351 00960000
*   REG2=REG15;                     /* SAVE RETURN CODE              */ 00961000
         LR    REG2,REG15                                          0352 00962000
*   GEN( DELETE EP=IEECB860);       /* FREE ESTAE MODULE             */ 00963000
          DELETE EP=IEECB860                                            00964000
*   REG15=REG2;                     /* RESTORE RETURN CODE REG       */ 00965000
         LR    REG15,REG2                                          0354 00966000
*   REG14=SAVE1;                    /* RESTORE RETURN ADDRESS        */ 00967000
         L     REG14,SAVE1                                         0355 00968000
*   END STAERTN;                                                   0356 00969000
@EL00011 DS    0H                                                  0356 00970000
@EF00011 DS    0H                                                  0356 00971000
@ER00011 BR    @14                                                 0356 00972000
*   END IEECB900                                                   0357 00973000
*                                                                  0357 00974000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00975000
