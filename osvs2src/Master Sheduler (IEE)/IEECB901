         TITLE 'IEECB901 - CN PROCESSOR FOR VARY COMMAND               *00001000
                        '                                               00002000
IEECB901 CSECT ,                                                   0001 00003000
@MAINENT BALR  @15,0                                               0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IEECB901  74.165'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@11                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @12,@01                                             0001 00016000
         USING @DATD,@12                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @12,8(,@13)                                         0001 00020000
         LR    @13,@12                                             0001 00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
*   REGPARM=REG1;                   /* SET UP PARM WORKREG PTR       */ 00023000
         LR    REGPARM,REG1                                        0060 00024000
*   CALL SCANCN;                    /* GO PROCESS IT                 */ 00025000
         BAL   @14,SCANCN                                          0061 00026000
*   RETURN;                                                        0062 00027000
@EL00001 L     @13,4(,@13)                                         0062 00028000
@EF00001 L     @00,@SIZDATD                                        0062 00029000
         LR    @01,@12                                             0062 00030000
         FREEMAIN R,LV=(0),A=(1)                                        00031000
@ER00001 LM    @14,@12,12(@13)                                     0062 00032000
         BR    @14                                                 0062 00033000
*SCANCN:                                                           0063 00034000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0063 00035000
*                                                                  0063 00036000
SCANCN   DS    0H                                                  0064 00037000
*   /*****************************************************************/ 00038000
*   /*                                                               */ 00039000
*   /* ENTRY IS TO PROCESS THE 'CN' PARAMETER VALUE(S) WHICH SPECIFY */ 00040000
*   /* UNIQUE CONSOLE UCME(S). INPUT PTR - CONIDPTR - IS POINTING AT */ 00041000
*   /* THE FIRST CHARACTER PAST THE LEFT PAREN. SUBROUTINES ARE      */ 00042000
*   /* CALLED TO SCAN THE INPUT - AS FOLLOWS: ..DELIMSCN - INPUT     */ 00043000
*   /* DELIMITER SCAN FOR COMMA, RIGHT PAREN. ..CONVRT - CONVERTS    */ 00044000
*   /* VALID INPUT VALUE TO BINARY FOR SCAN OF UCME ARRAY BY         */ 00045000
*   /* SUBROUTINE 'CONSCAN' . ..CONSCAN - LOCATES THE UCME OF THE    */ 00046000
*   /* UNIQUE CONSOLE AND UPDATES THE AUTHORITY FIELD OF THE         */ 00047000
*   /* SUBSYSTEM CONSOLE. NORMAL EXIT IS WHEN A RIGHT PAREN HAS BEEN */ 00048000
*   /* SCANNED. RETURN TO CALLER. ERROR EXIT IS WHEN FIELD - MSGCODE */ 00049000
*   /* - IS NON-ZERO. SET UP PARM LIST FOR MSG, CALL IEE0503D TO     */ 00050000
*   /* ISSUE IT. IF END OF SCAN NOT COMPLETE, CONTINUE. ELSE, RETURN */ 00051000
*   /* TO CALLER                                                     */ 00052000
*   /*                                                               */ 00053000
*   /*****************************************************************/ 00054000
*                                                                  0064 00055000
*   SAVE1=REG14;                    /* SAVE RETURN ADDRESS           */ 00056000
         ST    REG14,SAVE1                                         0064 00057000
*   SCANPTR=CONIDPTR;               /* PARM WORK PTR                 */ 00058000
         L     SCANPTR,CONIDPTR(,REGPARM)                          0065 00059000
*   SPINPTR=ONE;                    /* SET LOOP INDICATOR            */ 00060000
         LA    SPINPTR,1                                           0066 00061000
*   MSGCODE=ZERO;                   /* INIT ERROR CODE FIELD         */ 00062000
         MVI   MSGCODE,X'00'                                       0067 00063000
*   DO WHILE(SPINPTR=ONE);                                         0068 00064000
         B     @DE00068                                            0068 00065000
@DL00068 DS    0H                                                  0069 00066000
*     CALL DELIMSCN;                /* PROCESS FOR COMMA, RPAREN     */ 00067000
         BAL   @14,DELIMSCN                                        0069 00068000
*     CALL CONVRT;                  /* CONVERT VALUE FOR UCME SCAN   */ 00069000
         BAL   @14,CONVRT                                          0070 00070000
*     CALL CONSCAN;                 /* SCAN UCME ARRAY, VALID CONSOLE*/ 00071000
         BAL   @14,CONSCAN                                         0071 00072000
*     IF MSGCODE^=ZERO THEN         /* ERROR CONDITION               */ 00073000
         CLI   MSGCODE,0                                           0072 00074000
         BE    @RF00072                                            0072 00075000
*       DO;                                                        0073 00076000
*         CALL MSGSETUP;            /* SET MSG VARIABLE              */ 00077000
         BAL   @14,MSGSETUP                                        0074 00078000
*         CALL MSGRTN;              /* ISSUE WTO                     */ 00079000
         BAL   @14,MSGRTN                                          0075 00080000
*         MSGCODE=ZERO;             /* RE-SET ERROR CONDITION        */ 00081000
         MVI   MSGCODE,X'00'                                       0076 00082000
*       END;                                                       0077 00083000
*     IF SCANPTR->CHAR1=COMMA THEN  /* CONTINUATION DELIMITER        */ 00084000
@RF00072 CLC   CHAR1(1,SCANPTR),COMMA                              0078 00085000
         BNE   @RF00078                                            0078 00086000
*       SCANPTR=SCANPTR+ONE;        /* BUMP PAST DELIMITER           */ 00087000
         AL    SCANPTR,@CF00063                                    0079 00088000
*     ELSE                          /* END OF INPUT - RIGHT PAREN    */ 00089000
*       SPINPTR=ZERO;               /* SET LOOP EXIT                 */ 00090000
         B     @RC00078                                            0080 00091000
@RF00078 SLR   SPINPTR,SPINPTR                                     0080 00092000
*   END;                                                           0081 00093000
@RC00078 DS    0H                                                  0081 00094000
@DE00068 C     SPINPTR,@CF00063                                    0081 00095000
         BE    @DL00068                                            0081 00096000
*   REG14=SAVE1;                    /* RESTORE RETURN ADDRESS        */ 00097000
         L     REG14,SAVE1                                         0082 00098000
*   END SCANCN;                                                    0083 00099000
@EL00002 DS    0H                                                  0083 00100000
@EF00002 DS    0H                                                  0083 00101000
@ER00002 BR    @14                                                 0083 00102000
*DELIMSCN:                                                         0084 00103000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0084 00104000
*                                                                  0084 00105000
DELIMSCN DS    0H                                                  0085 00106000
*   /*****************************************************************/ 00107000
*   /*                                                               */ 00108000
*   /* THIS SUBRTN DETERMINES THE LENGTH OF A CONSOLE ID, AS DEFINED */ 00109000
*   /* BY THE DELIMITERS - COMMA, RIGHT PAREN. A COUNT FIELD - CNT , */ 00110000
*   /* IS INCREMENTED UNTIL A DELIMITER IS SCANNED. RETURN TO CALLER.*/ 00111000
*   /*                                                               */ 00112000
*   /*****************************************************************/ 00113000
*                                                                  0085 00114000
*   CNT=ZERO;                       /* INIT COUNT FIELD              */ 00115000
         MVI   CNT,X'00'                                           0085 00116000
*   DO WHILE(SCANPTR->CHAR1^=COMMA& /* DELIMITER COMMA               */ 00117000
*         SCANPTR->CHAR1^=RPAREN);  /* DELIMITER RIGHT PAREN         */ 00118000
         B     @DE00086                                            0086 00119000
@DL00086 DS    0H                                                  0087 00120000
*     CNT=CNT+ONE;                  /* INCREMENT COUNT FIELD         */ 00121000
         LA    @09,1                                               0087 00122000
         SLR   @00,@00                                             0087 00123000
         IC    @00,CNT                                             0087 00124000
         ALR   @09,@00                                             0087 00125000
         STC   @09,CNT                                             0087 00126000
*     SCANPTR=SCANPTR+ONE;          /* INCREMENT SCAN PTR            */ 00127000
         AL    SCANPTR,@CF00063                                    0088 00128000
*   END;                                                           0089 00129000
@DE00086 CLC   CHAR1(1,SCANPTR),COMMA                              0089 00130000
         BE    @DC00086                                            0089 00131000
         CLC   CHAR1(1,SCANPTR),RPAREN                             0089 00132000
         BNE   @DL00086                                            0089 00133000
@DC00086 DS    0H                                                  0090 00134000
*   END DELIMSCN;                                                  0090 00135000
@EL00003 DS    0H                                                  0090 00136000
@EF00003 DS    0H                                                  0090 00137000
@ER00003 BR    @14                                                 0090 00138000
*CONVRT:                                                           0091 00139000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0091 00140000
*                                                                  0091 00141000
CONVRT   DS    0H                                                  0092 00142000
*   /*****************************************************************/ 00143000
*   /*                                                               */ 00144000
*   /* CONVERT THE VALUE POINTED TO BY SCANPTR TO BINARY FOR USE IN  */ 00145000
*   /* THE UCME SCAN RTN - CONSCAN.                                  */ 00146000
*   /*                                                               */ 00147000
*   /*****************************************************************/ 00148000
*                                                                  0092 00149000
*   DCL                                                            0092 00150000
*     1 PAKAREA,                    /* AREA FOR PACK INSTRUCTION     */ 00151000
*      2 HIBYTE CHAR(2),            /* TWO-BYTE VALUE FIELD          */ 00152000
*       3 * CHAR(1),                                               0092 00153000
*       3 LOWBYTE CHAR(1);          /* ONE-BYTE VALUE                */ 00154000
*   SAVE2=REG14;                    /* SAVE RETURN ADDRESS           */ 00155000
         ST    REG14,SAVE2                                         0093 00156000
*   PAKAREA=ZERO;                   /* CLEAR AREA FOR PACK           */ 00157000
         SLR   @09,@09                                             0094 00158000
         STCM  @09,3,PAKAREA                                       0094 00159000
*   MOVPTR=SCANPTR-CNT;             /* PTR TO BEGINNING OF VALUE     */ 00160000
         LR    MOVPTR,SCANPTR                                      0095 00161000
         SLR   @09,@09                                             0095 00162000
         IC    @09,CNT                                             0095 00163000
         SLR   MOVPTR,@09                                          0095 00164000
*   IF CNT=ONE THEN                 /* ONE VALUE CONSOLE ID          */ 00165000
         CLI   CNT,1                                               0096 00166000
         BNE   @RF00096                                            0096 00167000
*     LOWBYTE=MOVPTR->CHAR1;        /* INIT LOW BYTE OF PACK AREA    */ 00168000
         MVC   LOWBYTE(1),CHAR1(MOVPTR)                            0097 00169000
*   ELSE                            /* TWO VALUE CONSOLE ID          */ 00170000
*     HIBYTE=MOVPTR->CHAR2;         /* INIT PACK AREA                */ 00171000
         B     @RC00096                                            0098 00172000
@RF00096 MVC   HIBYTE(2),CHAR2(MOVPTR)                             0098 00173000
*   PACK(PAKAREA,PAKAREA);          /* CONVERT TO PACKED NUM         */ 00174000
@RC00096 PACK  PAKAREA(2),PAKAREA(2)                               0099 00175000
*   CVB(CONID,PAKAREA);             /* CONVERT TO BINARY             */ 00176000
         XC    @TS00001(8),@TS00001                                0100 00177000
         MVC   @TS00001+6(2),PAKAREA                               0100 00178000
         CVB   @09,@TS00001                                        0100 00179000
         STC   @09,CONID                                           0100 00180000
*   REG14=SAVE2;                    /* RESTORE RETURN ADDRESS        */ 00181000
         L     REG14,SAVE2                                         0101 00182000
*   END CONVRT;                                                    0102 00183000
@EL00004 DS    0H                                                  0102 00184000
@EF00004 DS    0H                                                  0102 00185000
@ER00004 BR    @14                                                 0102 00186000
*CONSCAN:                                                          0103 00187000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0103 00188000
*                                                                  0103 00189000
CONSCAN  DS    0H                                                  0104 00190000
*   /*****************************************************************/ 00191000
*   /*                                                               */ 00192000
*   /* ENTRY HERE IS TO PROCESS A UNIQUE SUBSYSTEM CONSOLE ID. THE   */ 00193000
*   /* CONSOLE ID IS VALIDITY CHECKED FOR - GREATER THAN ZERO, NOT   */ 00194000
*   /* GREATER THAN THE LAST UCME PTR ON THE CHAIN. IF INVALID, NO   */ 00195000
*   /* UPDATES ARE MADE, MSG CODES ARE SET TO ISSUE APPROPRIATE      */ 00196000
*   /* MESSAGE. IF VALID, THE UPDATE TO THE CONSOLE AUTHORITY FIELD  */ 00197000
*   /* IS MADE. RETURN TO CALLER.                                    */ 00198000
*   /*                                                               */ 00199000
*   /*****************************************************************/ 00200000
*                                                                  0104 00201000
*   IF CONID>ZERO THEN              /* CONSOLE ID NOT ZERO           */ 00202000
         CLI   CONID,0                                             0104 00203000
         BNH   @RF00104                                            0104 00204000
*     DO;                                                          0105 00205000
*       UCMEPTR=UCMVEA+(UCMVEZ*(CONID-ONE));/* OBTAIN CONSOLE UCME 0106 00206000
*                                      ADDRESS                       */ 00207000
         L     @07,CVTPTR                                          0106 00208000
         L     @07,UCMBASPT(,@07)                                  0106 00209000
         SLR   UCMEPTR,UCMEPTR                                     0106 00210000
         IC    UCMEPTR,CONID                                       0106 00211000
         BCTR  UCMEPTR,0                                           0106 00212000
         LR    @05,UCMEPTR                                         0106 00213000
         M     @04,UCMVEZ(,@07)                                    0106 00214000
         AL    @05,UCMVEA(,@07)                                    0106 00215000
         LR    UCMEPTR,@05                                         0106 00216000
*       IF UCMEPTR>UCMVEL THEN      /* INVALID UCME PTR ?            */ 00217000
         C     UCMEPTR,UCMVEL(,@07)                                0107 00218000
         BNH   @RF00107                                            0107 00219000
*         MSGCODE=MSG13;            /* SET UP MSG IEE313I            */ 00220000
         MVI   MSGCODE,X'0D'                                       0108 00221000
*       ELSE                        /* VALID UCME PTR                */ 00222000
*         IF UCMDISPA^='1'B THEN    /* NOT MASTER CONSOLE ?          */ 00223000
         B     @RC00107                                            0109 00224000
@RF00107 TM    UCMDISPA(UCMEPTR),B'10000000'                       0109 00225000
         BO    @RF00109                                            0109 00226000
*           DO;                                                    0110 00227000
*             COMPCODE=ONE;         /* SET SWITCH FOR CALLER ISSUE 0111 00228000
*                                      IEE712I MSG                   */ 00229000
         MVI   COMPCODE(REGPARM),X'01'                             0111 00230000
*             UCMAUTHA=AUTHMASK;    /* UPDATE CONSOLE AUTHORITY      */ 00231000
         MVC   UCMAUTHA(1,UCMEPTR),AUTHMASK(REGPARM)               0112 00232000
*           END;                                                   0113 00233000
*         ELSE                      /* MASTER CONSOLE , ERROR        */ 00234000
*           MSGCODE=MSG13;          /* SET UP MSG IEE313I            */ 00235000
         B     @RC00109                                            0114 00236000
@RF00109 MVI   MSGCODE,X'0D'                                       0114 00237000
*     END;                                                         0115 00238000
*   ELSE                            /* INVALID CONSOLE ID            */ 00239000
*     MSGCODE=MSG13;                /* SET UP MSG IEE313I            */ 00240000
         B     @RC00104                                            0116 00241000
@RF00104 MVI   MSGCODE,X'0D'                                       0116 00242000
*   END CONSCAN;                                                   0117 00243000
@EL00005 DS    0H                                                  0117 00244000
@EF00005 DS    0H                                                  0117 00245000
@ER00005 BR    @14                                                 0117 00246000
*MSGSETUP:                                                         0118 00247000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0118 00248000
*                                                                  0118 00249000
MSGSETUP DS    0H                                                  0119 00250000
*   /*****************************************************************/ 00251000
*   /*                                                               */ 00252000
*   /* ENTRY IS TO PROCESS MESSAGE AREA VARIABLE FIELD - VERBNAME.   */ 00253000
*   /* FOR MSGCODE = IEE313I, VERBNAME = 'CN(XX) ' , WHERE 'XX' IS   */ 00254000
*   /* THE ID OF A UNIQUE CONSOLE. THE FIELD IS FORMATED - CN(00) ,  */ 00255000
*   /* AND THE PARENTHEZIED VALUE(S) ARE OVERLAID WITH THE UNPACKED  */ 00256000
*   /* VERSION OF THE CONSOLE ID.THE PTRS AND VALUES FOR THESE FIELDS*/ 00257000
*   /* ARE SET UP IN SUBRTN CONVRT, AND ARE STILL AVAILABLE TO THIS  */ 00258000
*   /* SUBRTN. INITIALIZE VARIABLE 'VERBNAME', RETURN TO CALLER      */ 00259000
*   /*                                                               */ 00260000
*   /*****************************************************************/ 00261000
*                                                                  0119 00262000
*   VERBNAME=CNFILL;                /* INIT 'CN(0 ) '                */ 00263000
         MVC   VERBNAME(8),CNFILL                                  0119 00264000
*   IF CNT=ONE THEN                 /* ONE VALUE CONSOLE ID          */ 00265000
         CLI   CNT,1                                               0120 00266000
         BNE   @RF00120                                            0120 00267000
*     CNID=MOVPTR->CHAR1;           /* MOVE IN SINGLE VALUE          */ 00268000
         MVC   CNID(1),CHAR1(MOVPTR)                               0121 00269000
*   ELSE                            /* TWO VALUE CONSOLE ID          */ 00270000
*     CNID1=MOVPTR->CHAR2;          /* MOVE IN TWO VALUES            */ 00271000
         B     @RC00120                                            0122 00272000
@RF00120 MVC   CNID1(2),CHAR2(MOVPTR)                              0122 00273000
*   END MSGSETUP;                                                  0123 00274000
@EL00006 DS    0H                                                  0123 00275000
@EF00006 DS    0H                                                  0123 00276000
@ER00006 BR    @14                                                 0123 00277000
*MSGRTN:                                                           0124 00278000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0124 00279000
*                                                                  0124 00280000
MSGRTN   DS    0H                                                  0125 00281000
*   /*****************************************************************/ 00282000
*   /*                                                               */ 00283000
*   /* SET UP TO ISSUE MESSAGES VIA MODULE IEE0503D, WHICH MAPS INPUT*/ 00284000
*   /* TO THE XSA MAPPING MACRO. SET WORKAREA BASED ON XSA, CLEAR    */ 00285000
*   /* CORE, AND INITIALIZE THE PARM LIST AND CALL MESSAGE MODULE. NO*/ 00286000
*   /* RETURN CODE EXPECTED - RETURN TO CALLER.                      */ 00287000
*   /*                                                               */ 00288000
*   /*****************************************************************/ 00289000
*                                                                  0125 00290000
*   DCL                                                            0125 00291000
*     MSGMOD ENTRY BASED(REG15);    /* ENTRY TO ESTABLISH CALL     0125 00292000
*                                      CAPABILITY TO MSG MODULE ADDR */ 00293000
*   DCL                                                            0126 00294000
*     XSAPTR PTR(31) REGISTER(2);   /* XSA MAPPING PTR               */ 00295000
*   RFY                                                            0127 00296000
*    (REG0,                                                        0127 00297000
*     XSAPTR) RESTRICTED;                                          0127 00298000
*   SAVE2=REG14;                    /* SAVE RETURN ADDRESS           */ 00299000
         ST    REG14,SAVE2                                         0128 00300000
*   REG2=MSGAPTR;                   /* PTR TO PARM LIST CORE         */ 00301000
         L     REG2,MSGAPTR(,REGPARM)                              0129 00302000
*   REG0=LENGTH(XASVRB);            /* LENGTH OF CORE TO CLEAR       */ 00303000
         LA    REG0,48                                             0130 00304000
*   XSAMAP(1:REG0)=XSAMAP(1:REG0)&&XSAMAP(1:REG0);/* CLEAR CORE      */ 00305000
         LR    @09,REG0                                            0131 00306000
         BCTR  @09,0                                               0131 00307000
         EX    @09,@SX01248                                        0131 00308000
*   XAE=MSGCODE;                    /* PTR TO APPROPRIATE MSG        */ 00309000
         MVC   XAE(1,XSAPTR),MSGCODE                               0132 00310000
*   XAV=VERBNAME;                   /* FILLER FOR MSG                */ 00311000
         MVC   XAV(8,XSAPTR),VERBNAME                              0133 00312000
*   XAU=CHUCMP;                     /* CONSOLE ID OF ISSUER          */ 00313000
         L     @09,CSCBPTR(,REGPARM)                               0134 00314000
         MVC   XAU(1,XSAPTR),CHUCMP(@09)                           0134 00315000
*   REG15=MSGMPTR;                  /* ADDRESS OF IEE0503D           */ 00316000
         L     REG15,MSGMPTR(,REGPARM)                             0135 00317000
*   GEN( STM   0,5,12(13));         /* SAVE REGS FOR RETURN          */ 00318000
          STM   0,5,12(13)                                              00319000
*   CALL MSGMOD;                    /* ISSUE WTO                     */ 00320000
         BALR  @14,@15                                             0137 00321000
*   GEN( LM    0,5,12(13));         /* RESTORE SAVED REGS            */ 00322000
          LM    0,5,12(13)                                              00323000
*   REG14=SAVE2;                    /* RESTORE RETURN ADDRESS        */ 00324000
         L     REG14,SAVE2                                         0139 00325000
*   RFY                                                            0140 00326000
*    (REG0,                                                        0140 00327000
*     XSAPTR) UNRESTRICTED;                                        0140 00328000
*   END MSGRTN;                                                    0141 00329000
@EL00007 DS    0H                                                  0141 00330000
@EF00007 DS    0H                                                  0141 00331000
@ER00007 BR    @14                                                 0141 00332000
*   END IEECB901                                                   0142 00333000
*                                                                  0142 00334000
