@EL00005 DS    0H                                                  0736 02438000
@EF00005 DS    0H                                                  0736 02439000
@ER00005 LM    @14,@12,@SA00005                                    0736 02440000
         BR    @14                                                 0736 02441000
*                                                                  0737 02442000
*/********************************************************************  02443000
**   CONVHEX TAKES UP TO AN 8 CHAR HEX NUMBER AND CONVERTS IT TO A 0737 02444000
**   4 BYTE BINARY NUMBER. IT IS CALLED FROM ADDRESS, PVTMOD, LPAMOD    02445000
**   AND ASID SEGMENTS.                                            0737 02446000
**********************************************************************/ 02447000
*                                                                  0737 02448000
*CONVHEX:                                                          0737 02449000
*   PROC;                                                          0737 02450000
CONVHEX  STM   @14,@12,12(@13)                                     0737 02451000
*   TEMPADDR=F0S;                   /* INIT AREA TO F0F0F0,ETC       */ 02452000
         MVC   TEMPADDR(8),@CC03182                                0738 02453000
*   DO I=ONE TO DATALN              /* PROCESS EACH CHAR OF ADDR     */ 02454000
*         WHILE WARESP=OFF;         /* UNTIL END OR ERROR            */ 02455000
         LA    I,1                                                 0739 02456000
         B     @DE00739                                            0739 02457000
@DL00739 TM    WARESP,B'00010000'                                  0739 02458000
         BNZ   @DC00739                                            0739 02459000
*     TEMPINDX=(EIGHT-DATALN)+I;    /* CALC POSITION IN TEMP AREA    */ 02460000
         LA    TEMPINDX,8                                          0740 02461000
         SH    TEMPINDX,DATALN                                     0740 02462000
         ALR   TEMPINDX,I                                          0740 02463000
*     IF IN(I:I)<CHARZERO THEN      /* IF CHAR IS NONDECIMAL(1-10)   */ 02464000
         L     @05,TEXTPTR                                         0741 02465000
         LR    @01,@05                                             0741 02466000
         ALR   @01,I                                               0741 02467000
         BCTR  @01,0                                               0741 02468000
         CLI   IN(@01),C'0'                                        0741 02469000
         BNL   @RF00741                                            0741 02470000
*       IF IN(I:I)>LETTERF THEN     /* AND IF NOT A HEX CHAR(A-F)    */ 02471000
         ALR   @05,I                                               0742 02472000
         BCTR  @05,0                                               0742 02473000
         CLI   IN(@05),C'F'                                        0742 02474000
         BNH   @RF00742                                            0742 02475000
*         WARESP=ON;                /* IND SYNTAX IS INVALID         */ 02476000
         OI    WARESP,B'00010000'                                  0743 02477000
*       ELSE                        /* IF CHAR IS A HEX A-F CHAR     */ 02478000
*         DO;                                                      0744 02479000
         B     @RC00742                                            0744 02480000
@RF00742 DS    0H                                                  0745 02481000
*           TEMPNUM=IN(I:I)+PLUS39; /* PREPARE TO PACK               */ 02482000
         LA    @05,57                                              0745 02483000
         L     @01,TEXTPTR                                         0745 02484000
         BCTR  @01,0                                               0745 02485000
         SLR   @00,@00                                             0745 02486000
         IC    @00,IN(I,@01)                                       0745 02487000
         ALR   @05,@00                                             0745 02488000
         STC   @05,TEMPNUM                                         0745 02489000
*           TEMPADDR(TEMPINDX:TEMPINDX)=TEMPNUM;/* COPY CONV CHAR    */ 02490000
         LA    @05,TEMPADDR-1(TEMPINDX)                            0746 02491000
         MVC   0(1,@05),TEMPNUM                                    0746 02492000
*         END;                                                     0747 02493000
*     ELSE                          /* IF CHAR IS A DECIMAL CHAR     */ 02494000
*       TEMPADDR(TEMPINDX:TEMPINDX)=IN(I:I);/* COPY CHAR             */ 02495000
         B     @RC00741                                            0748 02496000
@RF00741 LA    @05,TEMPADDR-1(TEMPINDX)                            0748 02497000
         L     @01,TEXTPTR                                         0748 02498000
         ALR   @01,I                                               0748 02499000
         BCTR  @01,0                                               0748 02500000
         MVC   0(1,@05),IN(@01)                                    0748 02501000
*   END;                                                           0749 02502000
@RC00741 AH    I,@CH00063                                          0749 02503000
@DE00739 CH    I,DATALN                                            0749 02504000
         BNH   @DL00739                                            0749 02505000
@DC00739 DS    0H                                                  0750 02506000
*   IF WARESP=OFF THEN              /* IF SYNTAX OKAY                */ 02507000
         TM    WARESP,B'00010000'                                  0750 02508000
         BNZ   @RF00750                                            0750 02509000
*     DO;                                                          0751 02510000
*       PACK(CONVBUFC,TEMPADDR);    /* PACK NUMBER TO BINARY &SIGN   */ 02511000
         PACK  CONVBUFC(4),TEMPADDR(8)                             0752 02512000
*       SRL(CONVBUFC,FOUR);         /* REMOVE SIGN                   */ 02513000
         L     @05,CONVBUFC                                        0753 02514000
         SRL   @05,4                                               0753 02515000
         ST    @05,CONVBUFC                                        0753 02516000
*     END;                                                         0754 02517000
*   END CONVHEX;                                                   0755 02518000
@EL00006 DS    0H                                                  0755 02519000
@EF00006 DS    0H                                                  0755 02520000
@ER00006 LM    @14,@12,12(@13)                                     0755 02521000
         BR    @14                                                 0755 02522000
*                                                                  0756 02523000
*/********************************************************************  02524000
**  DISABLE KEYWORD PROCESSOR (BOTH DISABLE AND D)                 0756 02525000
**********************************************************************/ 02526000
*                                                                  0756 02527000
*DISABLE:                                                          0756 02528000
*   PROC;                                                          0756 02529000
