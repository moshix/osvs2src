*                                    DO;                           0907 03092000
*                                     WAID=IN(4:I-1);/* PUT INTO WAID*/ 03093000
         MVI   WAID+1,C' '                                         0908 03094000
         MVC   WAID+2(2),WAID+1                                    0908 03095000
         LR    @14,I                                               0908 03096000
         SH    @14,@CH00723                                        0908 03097000
         EX    @14,@SM03432                                        0908 03098000
*                                     BADREPLY=OFF;/* EXIT MSG LOOP  */ 03099000
         NI    BADREPLY,B'01111111'                                0909 03100000
*                                    END;                          0910 03101000
*                                 END;                             0911 03102000
*                               ELSE                               0912 03103000
*                                 ; /* BADREPLY                      */ 03104000
@RF00902 DS    0H                                                  0913 03105000
*                             END;                                 0913 03106000
@RC00902 DS    0H                                                  0914 03107000
*                         RFY                                      0914 03108000
*                           R15 UNRSTD;                            0914 03109000
@RC00895 DS    0H                                                  0914 03110000
@RC00893 DS    0H                                                  0915 03111000
*                         RFY                                      0915 03112000
*                           IN BASED(TEXTPTR);                     0915 03113000
*                       END;        /* END OF BADREPLY DO-WHILE      */ 03114000
