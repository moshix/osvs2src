*   NXTOP=ZERO;                     /* INIT SCAN PTR SAVE AREA       */ 00065000
*                                                                  0140 00066000
         SLR   @14,@14                                             0140 00067000
         ST    @14,NXTOP                                           0140 00068000
*   /*****************************************************************/ 00069000
*   /*                                                               */ 00070000
*   /* THE OPERAND IS CHECKED FOR ONE OF THE THREE POSSIBLE          */ 00071000
*   /* POSITIONAL PARAMETERS, 'SET' OR 'MOD' OR 'DEL'. THEN CONTROL  */ 00072000
*   /* IS PASSED TO THE APPROPRIATE SUBROUTINE.                      */ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*                                                                  0141 00076000
*   IF IN(1:4)=SETC|                /* IF FIRST KEYWORD IS SET       */ 00077000
*       IN(1:4)=SETB THEN                                          0141 00078000
