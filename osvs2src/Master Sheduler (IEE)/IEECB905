         EX    @05,@SM03443                                        1181 04056000
*             TEXTPTR=TEXTPTR+DATALN;/* UPDATE PTR PAST OPERAND      */ 04057000
         ALR   @03,@07                                             1182 04058000
         ST    @03,TEXTPTR                                         1182 04059000
*           END;                                                   1183 04060000
