         BNO   @RF01239                                            1239 04253000
*                     DO;           /* SEARCH FOR THE MAJOR          */ 04254000
*                       R14=ADDR(LPDEMJNM);/* ADDRESS OF MODULE NAME */ 04255000
         LA    R14,LPDEMJNM(,LPDEPTR)                              1241 04256000
*                       R3=CVTPTR;  /* CVT ADDRESS                   */ 04257000
