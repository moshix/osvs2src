*       R0=R0|SP245;                /* ADD SUBPOOL TO HI ORD BYTE    */ 02618000
         O     R0,@CF03117                                         0779 02619000
*       GEN(GETMAIN R,LV=(0));                                     0780 02620000
         GETMAIN R,LV=(0)                                               02621000
*       SCESCVA=R1;                 /* SAVE ADDR IN SCE              */ 02622000
         LA    @09,STAE906                                         0781 02623000
         L     @09,SCEPTR(,@09)                                    0781 02624000
         ST    R1,SCESCVA(,@09)                                    0781 02625000
*       RFY                                                        0782 02626000
*        (R0,                                                      0782 02627000
*         R1) UNRSTD;                                              0782 02628000
*       SCVACBID=IDSCVA;            /* INIT CONTROL BLOCK ID         */ 02629000
         LA    @09,STAE906                                         0783 02630000
         L     @09,SCEPTR(,@09)                                    0783 02631000
         L     @09,SCESCVA(,@09)                                   0783 02632000
         MVC   SCVACBID(4,@09),@CC03161                            0783 02633000
*       SCVALN=WASCVALN+LENGTH(SCVAHDR)+ONE;/* INIT LENGTH OF SCVA 0784 02634000
*                                      FIELD                         */ 02635000
         LA    @05,9                                               0784 02636000
         AH    @05,WASCVALN                                        0784 02637000
         STH   @05,SCVALN(,@09)                                    0784 02638000
*       SCVAPTR=ADDR(SCVADATA);     /* UPDATE PTR TO FIRST           */ 02639000
         LA    SCVAPTR,SCVADATA(,@09)                              0785 02640000
*       IF WACOMPID^=ZERO THEN      /* IF COMP WAS SPECIFIED         */ 02641000
         CLI   WACOMPID,0                                          0786 02642000
         BE    @RF00786                                            0786 02643000
*         DO;                                                      0787 02644000
*           SCVACOMP=WACOMP;        /* COPY INFO FROM WA INTO SCVA   */ 02645000
         MVC   SCVACOMP(8,SCVAPTR),WACOMP                          0788 02646000
*           SCVAPTR=SCVAPTR+LENGTH(SCVACOMP);/* PT PAST THIS         */ 02647000
         AH    SCVAPTR,@CH00061                                    0789 02648000
*         END;                                                     0790 02649000
*       IF WAASIDID^=ZERO THEN      /* IF ASID WAS SPECIFIED         */ 02650000
@RF00786 CLI   WAASIDID,0                                          0791 02651000
         BE    @RF00791                                            0791 02652000
*         DO;                       /* COPY INFO FROM WA INTO SCVA   */ 02653000
*           I=LENGTH(SCVADHDR)+(WAASIDNO*TWO);                     0793 02654000
         SLR   I,I                                                 0793 02655000
         IC    I,WAASIDNO                                          0793 02656000
         ALR   I,I                                                 0793 02657000
         AH    I,@CH00075                                          0793 02658000
*           SCVAASID(1:I)=WAASID(1:I);                             0794 02659000
         LR    @09,I                                               0794 02660000
         BCTR  @09,0                                               0794 02661000
