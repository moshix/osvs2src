         EX    @07,@SM03445                                        1206 04149000
*/********************************************************************  04150000
**   SCAN THE LINK PACK AREA DIRECTORIES FOR THE SPECIFIED MODULE  1207 04151000
**   UTILIZING THE TWO SEARCH ROUTINES IN IEAVLK00 POINTED TO BY   1207 04152000
**   THE CVT (CVTQCDSR - CDE SEARCH, CVTLPDSR - LPDE SEARCH).      1207 04153000
**********************************************************************/ 04154000
*                                                                  1207 04155000
*         DO;                                                      1207 04156000
*           RFY                                                    1208 04157000
*            (R0,                                                  1208 04158000
*             R1,                                                  1208 04159000
*             R3,                                                  1208 04160000
*             R5,                                                  1208 04161000
*             R8,                                                  1208 04162000
*             R9,                                                  1208 04163000
*             R14,                                                 1208 04164000
*             R15) RSTD;                                           1208 04165000
*           R3=CVTPTR;              /* CVT ADDRESS                   */ 04166000
         L     @07,CVTPTR                                          1209 04167000
         LR    R3,@07                                              1209 04168000
*           R5=PSATOLD;             /* POINT TO TCB                  */ 04169000
         L     R5,PSATOLD                                          1210 04170000
*           R9=ADDR(WAPVTLPA);      /* ADDR OF MODULE NAME           */ 04171000
         LA    R9,WAPVTLPA                                         1211 04172000
*           R8=CVTQLPAQ;            /* QUEUE TO BE SEARCHED          */ 04173000
         L     R8,CVTQLPAQ(,@07)                                   1212 04174000
*           R15=CVTQCDSR;           /* ENTRY PT ADDR FOR CDE SRCH    */ 04175000
         L     R15,CVTQCDSR(,@07)                                  1213 04176000
*           RFY                                                    1214 04177000
*            (R7,                                                  1214 04178000
*             R11) RSTD;                                           1214 04179000
*           R7=R11;                 /* SAVE BASE REG                 */ 04180000
         LR    R7,R11                                              1215 04181000
*           GENERATE REFS(CVTLPDSR);                               1216 04182000
      BALR  R14,R15    LINK TO SEARCH ROUTINE                           04183000
      SR    R14,R14    INDICATE CDE FOUND                               04184000
      LR    R0,R11     SAVE CDE ADDRESS                                 04185000
*           R11=R7;                 /* RESTORE BASE REG              */ 04186000
         LR    R11,R7                                              1217 04187000
*           RFY                                                    1218 04188000
*            (R7,                                                  1218 04189000
*             R11) UNRSTD;                                         1218 04190000
*           IF R14=ZERO THEN        /* IF CDE FOUND ON ACTIVE Q      */ 04191000
         LTR   R14,R14                                             1219 04192000
         BNZ   @RF01219                                            1219 04193000
*             DO;                                                  1220 04194000
*               RFY                                                1221 04195000
*                 R14 UNRSTD;                                      1221 04196000
*               CDENTPTR=R0;        /* ADDRESS OF THIS CDE           */ 04197000
         LR    CDENTPTR,R0                                         1222 04198000
*               MODEP=CDENTPT;      /* SAVE ENTRY POINT ADDRESS      */ 04199000
         MVC   MODEP(4),CDENTPT(CDENTPTR)                          1223 04200000
*               IF CDMIN=ON THEN    /* IF CDE IS MINOR, ADDR MAJOR   */ 04201000
         TM    CDMIN(CDENTPTR),B'00000100'                         1224 04202000
         BNO   @RF01224                                            1224 04203000
*                 XTLSTPTR=CDXLMJP->CDXLMJP;/* OBTAIN EXTENT LIST  1225 04204000
*                                      ADDRESS                       */ 04205000
         L     @14,CDXLMJP(,CDENTPTR)                              1225 04206000
         MVC   XTLSTPTR(4),CDXLMJP(@14)                            1225 04207000
*               ELSE                                               1226 04208000
*                 XTLSTPTR=CDXLMJP; /* OBTAIN EXTENT LIST ADDRESS    */ 04209000
         B     @RC01224                                            1226 04210000
@RF01224 MVC   XTLSTPTR(4),CDXLMJP(CDENTPTR)                       1226 04211000
*               MODADDR=XTLMSBAD&HIORDBIT;/* SAVE MODULE START     1227 04212000
*                                      ADDRESS                       */ 04213000
@RC01224 L     @14,@CF03194                                        1227 04214000
         L     @07,XTLSTPTR                                        1227 04215000
         ST    @14,@TF00001                                        1227 04216000
         SLR   @14,@14                                             1227 04217000
         ICM   @14,7,XTLMSBAD(@07)                                 1227 04218000
         N     @14,@TF00001                                        1227 04219000
         ST    @14,MODADDR                                         1227 04220000
*               MODLN=XTLMSBLN&HIORDBIT;/* SAVE MODULE LENGTH        */ 04221000
         SLR   @14,@14                                             1228 04222000
         ICM   @14,7,XTLMSBLN(@07)                                 1228 04223000
         N     @14,@TF00001                                        1228 04224000
         ST    @14,MODLN                                           1228 04225000
*             END;                                                 1229 04226000
*           ELSE                    /* IF NO CDE ON ACTIVE QUEUE,    */ 04227000
*             DO;                   /* SEARCH THE LPA DIRECTORY      */ 04228000
         B     @RC01219                                            1230 04229000
@RF01219 DS    0H                                                  1231 04230000
*               RFY                                                1231 04231000
*                 R14 RSTD;                                        1231 04232000
*               R14=ADDR(WAPVTLPA); /* ADDR OF MODULE NAME           */ 04233000
         LA    R14,WAPVTLPA                                        1232 04234000
*               R15=CVTLPDSR;       /* ENTRY PT ADDR FOR LPDE SRCH   */ 04235000
         L     @07,CVTPTR                                          1233 04236000
         L     R15,CVTLPDSR(,@07)                                  1233 04237000
*               GENERATE;                                          1234 04238000
      LM     R0,R1,0(R14)    LOAD NAME                                  04239000
      BALR   R14,R15                                                    04240000
      SR     R14,R14         INDICATE LPDE FOUND                        04241000
      SR     R14,R14                                                    04242000
*               IF R14=ZERO THEN    /* IF FOUND IN LPA DIRECTORY     */ 04243000
         LTR   R14,R14                                             1235 04244000
         BNZ   @RF01235                                            1235 04245000
*                 DO;                                              1236 04246000
*                   LPDEPTR=R0;     /* ADDRESS OF THIS LPDE          */ 04247000
         LR    LPDEPTR,R0                                          1237 04248000
*                   MODEP=LPDENTP;  /* SAVE ENTRY POINT ADDRESS      */ 04249000
         MVC   MODEP(4),LPDENTP(LPDEPTR)                           1238 04250000
*                   IF LPDEMIN=ON THEN/* IF THIS IS A MINOR LPDE     */ 04251000
