*   GEN(BALR R14,R15);              /* ISSUE MESSAGE                 */ 04845000
         BALR R14,R15                                                   04846000
*   R4=R13->SAVREGB(1:4);           /* GET DATA REG BACK.            */ 04847000
         L     R4,SAVREGB(,R13)                                    1407 04848000
*   GEN(DELETE EP=IEE0503D);                                       1408 04849000
         DELETE EP=IEE0503D                                             04850000
*   RFY                                                            1409 04851000
*    (R0,                                                          1409 04852000
*     R1,                                                          1409 04853000
*     XSAPTR,                                                      1409 04854000
*     R3,                                                          1409 04855000
*     R4,                                                          1409 04856000
*     R5,                                                          1409 04857000
*     R13,                                                         1409 04858000
*     R15) UNRSTD;                                                 1409 04859000
*   END MSGMOD05;                                                  1410 04860000
@EL00023 DS    0H                                                  1410 04861000
@EF00023 DS    0H                                                  1410 04862000
@ER00023 LM    @14,@12,@SA00023                                    1410 04863000
         BR    @14                                                 1410 04864000
*                                                                  1411 04865000
*/********************************************************************  04866000
**   NEXTKEY PREPARES COMMAND PROCESSOR TO PROCESS THE NEXT KEYWORD.    04867000
**   IF A SYNTAX ERROR WAS FOUND PROCESSING THE PREVIOUS KEYWORD,  1411 04868000
**   THE ISSUER IS PROMPTED.                                       1411 04869000
