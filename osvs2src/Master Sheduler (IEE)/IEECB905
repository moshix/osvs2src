         BNZ   @RF00921                                            0921 03155000
*                 CALL GETVTSLP;    /* LOAD IEAVTSLP                 */ 03156000
*                                                                  0922 03157000
         BAL   @14,GETVTSLP                                        0922 03158000
*               /*****************************************************/ 03159000
*               /*                                                   */ 03160000
*               /* IF NO ERROR OR CANCEL, ID IS UNIQUE, ADDSCE TO THE*/ 03161000
*               /* CHAIN                                             */ 03162000
*               /*                                                   */ 03163000
*               /*****************************************************/ 03164000
*                                                                  0923 03165000
*               IF WAFIN=OFF THEN   /* IF CMD NOT BEING TERMINATED   */ 03166000
@RF00921 TM    WAFIN,B'00100000'                                   0923 03167000
         BNZ   @RF00923                                            0923 03168000
*                 DO;                                              0924 03169000
*                   IF SHDRBKWD->SCECBID^=IDSCE&/* VALIDATE POINTERS */ 03170000
*                       SHDRBKWD->SHDRCBID^=IDSHDR THEN            0925 03171000
