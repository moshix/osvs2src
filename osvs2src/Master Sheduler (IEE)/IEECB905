*                   WARESPL=ON;     /* IND MODULE NOT FOUND IN LPA   */ 04307000
         OI    WARESP,B'00011000'                                  1261 04308000
*                 END;                                             1262 04309000
*             END;                                                 1263 04310000
@RC01235 DS    0H                                                  1264 04311000
*           RFY                                                    1264 04312000
*            (R0,                                                  1264 04313000
*             R1,                                                  1264 04314000
*             R3,                                                  1264 04315000
*             R5,                                                  1264 04316000
*             R8,                                                  1264 04317000
