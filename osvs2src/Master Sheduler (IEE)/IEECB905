*         WAALLIND=OFF;             /* FOR MOD, ENSURE ALL IND OFF   */ 03942000
         NI    WAALLIND,B'11111110'                                1147 03943000
*       END;                                                       1148 03944000
*     ELSE                          /* IF SYNTAX IS INVALID          */ 03945000
*       WARESP=ON;                  /* SETUP VARIABLE AREA FOR MSG   */ 03946000
