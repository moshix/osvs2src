*                           WAID=BLANKS;/* ENSURE WAID IND OFF       */ 00432000
         MVC   WAID(4),@CC03150                                    0225 00433000
*                         END;                                     0226 00434000
*                       ELSE        /* IF THE KEYWORD IS NOT ALL     */ 00435000
*                         CALL MKEYSUB;/* PROCESS KEYWORD            */ 00436000
