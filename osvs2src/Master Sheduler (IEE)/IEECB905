*               CALL LPAPVTAD;      /* PROCESS ADDRESSES             */ 04339000
         BAL   @14,LPAPVTAD                                        1272 04340000
*           END;                                                   1273 04341000
*       END;                                                       1274 04342000
*     ELSE                          /* IF LENGTH OF NAME INVALID     */ 04343000
*       WARESP=ON;                  /* IND INVALID SYNTAX            */ 04344000
         B     @RC01202                                            1275 04345000
@RF01202 OI    WARESP,B'00010000'                                  1275 04346000
