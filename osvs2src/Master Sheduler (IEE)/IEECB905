*   DOINFREE=OFF;                   /* NOT A PGFREE.                 */ 03473000
         NI    DOINFREE(R3),B'01111111'                            0993 03474000
*   BADFIX=OFF;                     /* NOT A BADFIX YET.             */ 03475000
         NI    BADFIX,B'10111111'                                  0994 03476000
*   PGSTRT=STRTAD;                  /* IEAVTSLP START ADDR.          */ 03477000
         STCM  STRTAD,7,PGSTRT(R3)                                 0995 03478000
*   PGEND=ENDAD;                    /* IEAVTSLP END ADDR. + 1        */ 03479000
         STCM  ENDAD,7,PGEND(R3)                                   0996 03480000
*   PGLONG=ON;                      /* DO A LONG FIX,NO STEALS     0997 03481000
*                                      PLEASE.                       */ 03482000
         OI    PGLONG(R3),B'00000010'                              0997 03483000
*   LSTFLG=ON;                      /* LAST VSL INDICATOR.           */ 03484000
         OI    LSTFLG(R3),B'10000000'                              0998 03485000
*   FRRCNTRL=OFF;                   /* NO FRR ENTRY SETUP PROCESSING.*/ 03486000
         NI    FRRCNTRL,B'11011111'                                0999 03487000
*   IF FIX=ON THEN                  /* FIX REQUEST ?                 */ 03488000
         TM    FIX,B'10000000'                                     1000 03489000
         BNO   @RF01000                                            1000 03490000
*     PGFIX=ON;                     /* YES, TELL IEAVPSIB.           */ 03491000
         OI    PGFIX(R3),B'01000000'                               1001 03492000
*   ELSE                            /* NOT A FIX, ITS A FREE.        */ 03493000
*     DO;                           /* DO PGFREE(FRR ALSO).          */ 03494000
