@ER00025 LM    @14,@12,@SA00025                                    1460 05050000
         BR    @14                                                 1460 05051000
*                                                                  1461 05052000
*/********************************************************************  05053000
** THE SHDRPFC COUNTER MUST CONTAIN ONE WHICH MEANS THAT IEAVTSLP IS    05054000
** PAGE FIXED BUT THE MODULE IEAVTSLP IS NOT CURRENTLY EXECUTING.  1461 05055000
** IF THIS IS THE CASE, A CS TO ZERO IS DONE TO INDICATE SLIP IS NOT    05056000
** ACTIVE AND IEAVTSLP IS NOT PAGE FIXED.  THEN A PAGE FREE IS     1461 05057000
** DONE.  IF SHDRPFC COUNTER IS GREATER THAN ONE, THEN IEAVTSLP IS 1461 05058000
** EXECUTING AND THE MESSAGE IEE732D (RETRY OR CANCEL) IS WRITTEN. 1461 05059000
**********************************************************************/ 05060000
*                                                                  1461 05061000
*RELVTSLP:                                                         1461 05062000
*   PROC;                                                          1461 05063000
RELVTSLP STM   @14,@12,@SA00026                                    1461 05064000
*   RFY                                                            1462 05065000
*    (R6,                                                          1462 05066000
*     R7) RSTD;                                                    1462 05067000
*   DO WHILE SHDRPFC^=ZERO&         /* ATTEMPT TO OBTAIN PFC CTR     */ 05068000
*         WAFIN=OFF;                /* LOOP AS MANY TIMES AS       1463 05069000
*                                      REQUESTED UNTIL CTR IS      1463 05070000
*                                      OBTAINED OR THE COMMAND IS  1463 05071000
*                                      CANCELED                      */ 05072000
         B     @DE01463                                            1463 05073000
@DL01463 DS    0H                                                  1464 05074000
*     DO I=ONE TO FIVE              /* ATTEMPT FIVE TIMES BEFORE     */ 05075000
*           WHILE SHDRPFC^=ZERO;    /* ISSUING RETRY/CANCEL MSG      */ 05076000
         LA    I,1                                                 1464 05077000
@DL01464 L     @09,CVTPTR                                          1464 05078000
         L     @09,CVTRTMS(,@09)                                   1464 05079000
         ICM   @05,15,SHDRPFC(@09)                                 1464 05080000
         BZ    @DC01464                                            1464 05081000
*       R6=ONE;                     /* VALUE IF LOADED & NOT IN USE  */ 05082000
         LA    R6,1                                                1465 05083000
*       R7=ZERO;                    /* VALUE IND IEAVTSLP NOT LOADED */ 05084000
         SLR   R7,R7                                               1466 05085000
*       CS(R6,R7,SHDRPFC);          /* ATTEMPT TO OBTAIN PFC CTR     */ 05086000
         CS    R6,@07,SHDRPFC(@09)                                 1467 05087000
*       BC(8,PFCZERO);              /* IF OBTAINED, BR PAST    STIMER*/ 05088000
         BC    8,PFCZERO                                           1468 05089000
*       IF I<FIVE THEN              /* ISSUE STIMER BETWEEN TRYS     */ 05090000
         CH    I,@CH00723                                          1469 05091000
         BNL   @RF01469                                            1469 05092000
*         DO;                                                      1470 05093000
*           GEN REFS(PAUSE);                                       1471 05094000
              STIMER WAIT,BINTVL=PAUSE                                  05095000
*         END;                                                     1472 05096000
*PFCZERO:                                                          1473 05097000
*       ;                                                          1473 05098000
@RF01469 DS    0H                                                  1473 05099000
PFCZERO  DS    0H                                                  1474 05100000
*     END;                                                         1474 05101000
         AL    I,@CF00063                                          1474 05102000
         CH    I,@CH00723                                          1474 05103000
         BNH   @DL01464                                            1474 05104000
@DC01464 DS    0H                                                  1475 05105000
*     IF SHDRPFC^=ZERO THEN         /* IF PFC CTR NOT OBTAINED       */ 05106000
         L     @09,CVTPTR                                          1475 05107000
         L     @09,CVTRTMS(,@09)                                   1475 05108000
         ICM   @05,15,SHDRPFC(@09)                                 1475 05109000
         BZ    @RF01475                                            1475 05110000
*       CALL RETRYMSG;              /* ISSUE RETRY/CANCEL MSG -732   */ 05111000
         BAL   @14,RETRYMSG                                        1476 05112000
*   END;                                                           1477 05113000
@RF01475 DS    0H                                                  1477 05114000
@DE01463 L     @09,CVTPTR                                          1477 05115000
         L     @09,CVTRTMS(,@09)                                   1477 05116000
         ICM   @05,15,SHDRPFC(@09)                                 1477 05117000
         BZ    @DC01463                                            1477 05118000
         TM    WAFIN,B'00100000'                                   1477 05119000
         BZ    @DL01463                                            1477 05120000
@DC01463 DS    0H                                                  1478 05121000
*   RFY                                                            1478 05122000
*    (R6,                                                          1478 05123000
*     R7) UNRSTD;                                                  1478 05124000
*   IF WAFIN=OFF THEN               /* IF SHDRPFC CTR OBTAINED       */ 05125000
         TM    WAFIN,B'00100000'                                   1479 05126000
         BNZ   @RF01479                                            1479 05127000
*     DO;                                                          1480 05128000
*       GEN(LOAD EP=IEAVTSLP);      /* OBTAIN ADDR AND LENGTH        */ 05129000
         LOAD EP=IEAVTSLP                                               05130000
*       RFY                                                        1482 05131000
*        (R0,                                                      1482 05132000
*         R1,                                                      1482 05133000
*         R6) RSTD;                                                1482 05134000
*       R6=R1&MASKHI;               /* LOAD REG6 WITH MOD LENGTH     */ 05135000
         LR    R6,R1                                               1483 05136000
         N     R6,@CF03120                                         1483 05137000
*       R6=R6*EIGHT;                /* CONVERT DOUBLEWORD TO BYTE    */ 05138000
         SLL   R6,3                                                1484 05139000
*       ENDAD=R0+R6;                /* CALC ENDING ADDR OF MOD       */ 05140000
         LR    ENDAD,R0                                            1485 05141000
         ALR   ENDAD,R6                                            1485 05142000
*       STRTAD=R0;                  /* MOD ADDRESS                   */ 05143000
         LR    STRTAD,R0                                           1486 05144000
*       FIX=OFF;                    /* DO A PGFREE.                  */ 05145000
         NI    FIX,B'01111111'                                     1487 05146000
*       CALL FIXPAGE;               /* GO FREE FIXED PAGES.          */ 05147000
         BAL   @14,FIXPAGE                                         1488 05148000
*       RFY                                                        1489 05149000
*        (R0,                                                      1489 05150000
*         R1,                                                      1489 05151000
*         R6) UNRSTD;                                              1489 05152000
*       RFY                                                        1490 05153000
*         R15 RSTD;                                                1490 05154000
*       GEN(DELETE EP=IEAVTSLP);                                   1491 05155000
         DELETE EP=IEAVTSLP                                             05156000
*     END;                                                         1492 05157000
*   RFY                                                            1493 05158000
*     R15 UNRSTD;                                                  1493 05159000
*   END RELVTSLP;                                                  1494 05160000
@EL00026 DS    0H                                                  1494 05161000
@EF00026 DS    0H                                                  1494 05162000
@ER00026 LM    @14,@12,@SA00026                                    1494 05163000
         BR    @14                                                 1494 05164000
*                                                                  1495 05165000
*/********************************************************************  05166000
**  THIS SEGMENT UPDATES THE TEXTPTR TO THE NEXT KEYWORD, CALLS    1495 05167000
**  IEECB908 TO WRITE THE 728 (INVALID,PROMPT FOR CORRECTION) MESSAGE,  05168000
**  OR 736 (MODULE NOT IN THE LPA) MESSAGE,                        1495 05169000
**  ANALYZES THE REPLY, AND SAVES AND UPDATES PTRS.                1495 05170000
**********************************************************************/ 05171000
*                                                                  1495 05172000
*RESP:                                                             1495 05173000
*   PROC;                                                          1495 05174000
RESP     STM   @14,@08,@SA00027                                    1495 05175000
         STM   @10,@12,@SA00027+44                                 1495 05176000
*   DO WHILE WARESP=ON;             /* SCAN TO NEXT KEYWORD          */ 05177000
         B     @DE01496                                            1496 05178000
@DL01496 DS    0H                                                  1497 05179000
*     IF TEXTPTR=EOL|               /* IF AT END OF LINE OR THIS     */ 05180000
*         INCHAR=BLANK THEN         /* CHAR IS BLANK                 */ 05181000
         L     @05,TEXTPTR                                         1497 05182000
         C     @05,EOL                                             1497 05183000
         BE    @RT01497                                            1497 05184000
         CLI   INCHAR(@05),C' '                                    1497 05185000
         BNE   @RF01497                                            1497 05186000
@RT01497 DS    0H                                                  1498 05187000
*       WARESP=OFF;                 /* YES, GET OUT OF LOOP          */ 05188000
         NI    WARESP,B'11101111'                                  1498 05189000
*     ELSE                          /* NO,                           */ 05190000
*       IF INCHAR=COMMA THEN        /* IF CHAR IS A COMMA            */ 05191000
         B     @RC01497                                            1499 05192000
@RF01497 L     @05,TEXTPTR                                         1499 05193000
         CLI   INCHAR(@05),C','                                    1499 05194000
         BNE   @RF01499                                            1499 05195000
*         IF WAPAREN=OFF THEN       /* DOES AN OPEN PAREN EXIST      */ 05196000
         TM    WAPAREN,B'00000100'                                 1500 05197000
         BNZ   @RF01500                                            1500 05198000
*           WARESP=OFF;             /* NO OPEN PAREN, EXIT LOOP      */ 05199000
         NI    WARESP,B'11101111'                                  1501 05200000
*         ELSE                      /* OPEN PAREN DOES EXIST,        */ 05201000
*           TEXTPTR=TEXTPTR+ONE;    /* LOOK AT NEXT CHAR             */ 05202000
         B     @RC01500                                            1502 05203000
@RF01500 LA    @05,1                                               1502 05204000
         AL    @05,TEXTPTR                                         1502 05205000
         ST    @05,TEXTPTR                                         1502 05206000
*       ELSE                                                       1503 05207000
*         IF INCHAR=RPAREN THEN     /* IF CHAR IS A RIGHT PAREN      */ 05208000
         B     @RC01499                                            1503 05209000
