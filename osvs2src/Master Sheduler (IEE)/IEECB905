         STH   @07,WASCVALN                                        1191 04098000
*   WAADDRID=ZERO;                  /* CLEAR ENTRY ID                */ 04099000
@RF01190 DS    0H                                                  1192 04100000
@RC01188 MVI   WAADDRID,X'00'                                      1192 04101000
*   CALL SCAN;                      /* SCAN OPERAND TO DELIMITER     */ 04102000
         BAL   @14,SCAN                                            1193 04103000
*   IF INCHAR=LPAREN THEN           /* IF FIRST CHAR IS LEFT PAREN   */ 04104000
         L     @07,TEXTPTR                                         1194 04105000
         CLI   INCHAR(@07),C'('                                    1194 04106000
         BNE   @RF01194                                            1194 04107000
*     DO;                                                          1195 04108000
*       WARESP=OFF;                 /* IND SYNTAX IS VALID           */ 04109000
*       WAPAREN=ON;                 /* IND PAREN OPEN                */ 04110000
