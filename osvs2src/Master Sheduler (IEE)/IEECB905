*                       R15=CVTLPDSR;/* ENTRY PT ADDR FOR LPDE SRCH  */ 04259000
         L     R15,CVTPTR                                          1243 04260000
         L     R15,CVTLPDSR(,R15)                                  1243 04261000
*                       GENERATE;                                  1244 04262000
      LM    R0,R1,0(R14)    LOAD NAME                                   04263000
      BALR  R14,R15                                                     04264000
      SR    R14,R14         INDICATE LPDE FOUND                         04265000
      SR    R14,R14                                                     04266000
*                       IF R14=ZERO THEN/* IF MAJOR LPDE FOUND       */ 04267000
