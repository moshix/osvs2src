SVRBLEN  EQU 224                                                        00030000
*   SAVR14=R14;                     /* SAVE RETURN ADDR              */ 00031000
         LR    SAVR14,R14                                          0127 00032000
*   RFY                                                            0128 00033000
*     SESTAE BASED(ADDR(STAE906));                                 0128 00034000
*   SESTAE=SESTAE&&SESTAE;          /* INIT ESTAE PARMAREA TO ZERO   */ 00035000
         LA    @15,STAE906                                         0129 00036000
         XC    SESTAE(24,@15),SESTAE(@15)                          0129 00037000
*   CSCBPTR=R1;                     /* SAVE CSCB ADDR                */ 00038000
         ST    R1,CSCBPTR(,@15)                                    0130 00039000
*   RFY                                                            0131 00040000
*    (R1,                                                          0131 00041000
*     R14) UNRSTD;                                                 0131 00042000
*   CALL SETESTAE;                  /* SET UP ESTAE ENVIRONMENT      */ 00043000
         BAL   @14,SETESTAE                                        0132 00044000
*   WA=WA&&WA;                      /* INIT WORKAREA TO ZERO         */ 00045000
         XC    WA(86),WA                                           0133 00046000
*   WAUCMP=CHUCMP;                  /* SAVE CONSOLE ID OF ISSUER     */ 00047000
