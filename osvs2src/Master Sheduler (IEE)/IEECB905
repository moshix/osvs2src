         AL    @02,TEXTPTR                                         0221 00424000
         ST    @02,TEXTPTR                                         0221 00425000
*                       IF I=MODALLX THEN/* IF THE KEYWORD IS ALL    */ 00426000
         CH    I,@CH00035                                          0222 00427000
