         BNZ   @DC00854                                            0920 03132000
         TM    DUPID,B'10000000'                                   0920 03133000
         BO    @DL00854                                            0920 03134000
@DC00854 DS    0H                                                  0921 03135000
*               /*****************************************************/ 03136000
*               /*                                                   */ 03137000
*               /* IF NO ERROR OR CANCEL, ID IS UNIQUE, LOAD IEAVTSLP*/ 03138000
*               /* IF NECCESARY                                      */ 03139000
*               /*                                                   */ 03140000
*               /*****************************************************/ 03141000
*                                                                  0921 03142000
*               IF WAFIN=OFF&       /* IF CMD NOT BEING TERMINATED   */ 03143000
*                   SCEDSABL=OFF&   /* IF NEW SCE IS ENABLED AND     */ 03144000
*                   SHDRPFC=ZERO THEN/* IEAVTSLP IS NOT LOADED       */ 03145000
         TM    WAFIN,B'00100000'                                   0921 03146000
         BNZ   @RF00921                                            0921 03147000
