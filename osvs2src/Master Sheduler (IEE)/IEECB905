@RF01126 DS    0H                                                  1132 03888000
*       WAFIN=ON;                   /* TERMINATE THE COMMAND         */ 03889000
         OI    WAFIN,B'00100000'                                   1132 03890000
*       RFY                                                        1133 03891000
*         XSAPTR RSTD;                                             1133 03892000
*       XSAPTR=ADDR(MSGBUF);        /* XSA IS PARM TO IEE0503D       */ 03893000
         LA    XSAPTR,MSGBUF                                       1134 03894000
*       XAE=MSG328;                 /* INFORM USER RESOURCE UNAVAIL  */ 03895000
