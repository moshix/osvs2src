*/********************************************************************  04810000
**   MSGMOD05 SETS UP COMMON PARAMETERS, LOADS AND BALR'S TO       1397 04811000
**   IEE0503D IN ORDER TO SEND A MSG TO THE COMMAND ISSUER         1397 04812000
**********************************************************************/ 04813000
*                                                                  1397 04814000
*MSGMOD05:                                                         1397 04815000
*   PROC;                                                          1397 04816000
MSGMOD05 STM   @14,@12,@SA00023                                    1397 04817000
*   RFY                                                            1398 04818000
*     XSAPTR RSTD;                                                 1398 04819000
*   XAV=SLIP;                       /* VARIABLE TEXT IN MSG          */ 04820000
         MVI   XAV+4(XSAPTR),C' '                                  1399 04821000
         MVC   XAV+5(3,XSAPTR),XAV+4(XSAPTR)                       1399 04822000
         MVC   XAV(4,XSAPTR),@CC03087                              1399 04823000
*   XAU=WAUCMP;                     /* CONSOLE ID OF ISSUER          */ 04824000
         MVC   XAU(1,XSAPTR),WAUCMP                                1400 04825000
*   XAA=ZERO;                       /* INIT TERM ID TO ZERO          */ 04826000
         SLR   @07,@07                                             1401 04827000
         STH   @07,XAA(,XSAPTR)                                    1401 04828000
*   GEN(LOAD EP=IEE0503D);                                         1402 04829000
         LOAD EP=IEE0503D                                               04830000
*   RFY                                                            1403 04831000
*    (R0,                                                          1403 04832000
*     R1,                                                          1403 04833000
*     R3,                                                          1403 04834000
*     R4,                                                          1403 04835000
*     R5,                                                          1403 04836000
*     R13,                                                         1403 04837000
*     R15) RSTD;                    /* IEE0503D DESTROYS CONTENTS OF    04838000
*                                      REGS 0,1,3,4,5 - LM ON EXIT 1403 04839000
*                                      FROM MSGMOD05 RESTORES REGS   */ 04840000
*   R15=R0;                         /* ADDRESS OF IEE0503D           */ 04841000
         LR    R15,R0                                              1404 04842000
*   R13->SAVREGB(1:4)=R4;           /* SAVE THE DATA REG.            */ 04843000
