         MVC   WAUCMP(1),CHUCMP(@15)                               0134 00050000
*   WAID=BLANKS;                    /* INIT WAID TO BLANKS           */ 00051000
         MVC   WAID(4),@CC03150                                    0135 00052000
*   TEXTPTR=ADDR(CHBUF);            /* INIT TO START OF CMD TEXT     */ 00053000
