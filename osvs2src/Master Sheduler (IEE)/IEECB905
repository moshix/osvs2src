         L     @14,CVTRTMS(,@14)                                   1054 03624000
         ICM   @07,7,PGCOMP(@14)                                   1054 03625000
         BZ    @RF01054                                            1054 03626000
*             BADFIX=ON;            /* NO, END COMMAND/GIVE MSG.     */ 03627000
         OI    BADFIX,B'01000000'                                  1055 03628000
*         END;                      /* END NEED WAIT.                */ 03629000
*       ELSE                        /* NO WAIT NEEDED.               */ 03630000
*         IF R15^=0 THEN            /* WAS THE FIX/FREE OK ?         */ 03631000
         B     @RC01041                                            1057 03632000
@RF01041 LTR   R15,R15                                             1057 03633000
         BZ    @RF01057                                            1057 03634000
*           BADFIX=ON;              /* NO IT WAS NOT.                */ 03635000
         OI    BADFIX,B'01000000'                                  1058 03636000
*     END;                          /* END PGFIX/FREE.               */ 03637000
@RF01057 DS    0H                                                  1059 03638000
@RC01041 DS    0H                                                  1060 03639000
*   IF FRRCNTRL=OFF THEN            /* NORMAL CALL(NOT IN THE FRR) ? */ 03640000
@RF01027 TM    FRRCNTRL,B'00100000'                                1060 03641000
         BNZ   @RF01060                                            1060 03642000
*     DO;                                                          1061 03643000
*       GEN REFS(PSA,PSACSTK,FRRS,FRRSCURR,FRRSELEN,FRRSEMP);      1062 03644000
*                                                     /*YES,DEL FRR*/   03645000
        SETFRR D,WRKREGS=(R5,R6)                                        03646000
*     END;                                                         1063 03647000
*   IF FRLOCK=ON THEN               /* DID WE GET THE LOCK ?         */ 03648000
@RF01060 TM    FRLOCK,B'00010000'                                  1064 03649000
         BNO   @RF01064                                            1064 03650000
*     DO;                           /* YES, BETTER RELEASE IT.       */ 03651000
*       GEN REFS(PSALITA,FLC);                                     1066 03652000
*                       /* FREE THE LOCK. */                            03653000
FL SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,RELATED=GL                       03654000
*     END;                                                         1067 03655000
*   IF FRRCNTRL=ON THEN             /* IN THE FRR                    */ 03656000
@RF01064 TM    FRRCNTRL,B'00100000'                                1068 03657000
         BNO   @RF01068                                            1068 03658000
*     DO;                                                          1069 03659000
*       R14=RETADD;                 /* GET RETURN ADDRESS.           */ 03660000
         L     R14,RETADD                                          1070 03661000
*       GEN(BR R14);                /* RETURN TO RTM OR IEECB905.    */ 03662000
         BR R14                                                         03663000
*     END;                                                         1072 03664000
*   RFY                                                            1073 03665000
*    (R0,                                                          1073 03666000
*     R1,                                                          1073 03667000
*     R2,                                                          1073 03668000
*     R3,                                                          1073 03669000
*     R6,                                                          1073 03670000
*     R15) UNRSTD;                                                 1073 03671000
*   END FIXPAGE;                    /* END SUBROUTINE.               */ 03672000
