*                       ELSE        /* IF NOT FOUND, ISSUE 732 MSG   */ 04272000
*                         DO;                                      1247 04273000
         B     @RC01245                                            1247 04274000
@RF01245 DS    0H                                                  1248 04275000
*                           WARESP=ON;/* IND ERROR                   */ 04276000
*                           WARESPL=ON;/* IND MODULE NOT FOUND IN LPA*/ 04277000
