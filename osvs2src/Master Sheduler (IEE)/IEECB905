         BNZ   @DC00888                                            0916 03116000
         TM    BADREPLY,B'10000000'                                0916 03117000
         BO    @DL00888                                            0916 03118000
@DC00888 DS    0H                                                  0917 03119000
*                       IF WAFIN=OFF THEN/* ISSUER SPECIFIED NEW ID  */ 03120000
         TM    WAFIN,B'00100000'                                   0917 03121000
         BNZ   @RF00917                                            0917 03122000
*                         CALL GETSEQ;/* OBTAIN CMD-SEQ BYTE AGAIN   */ 03123000
         BAL   @14,GETSEQ                                          0918 03124000
*                     END;                                         0919 03125000
