*       DO;                         /* (NXTOP HOLDS RESUME ADDR)     */ 04888000
*         TEXTPTR=NXTOP;            /* RESTORE PTR TO NEXT KEYWORD   */ 04889000
         ST    @08,TEXTPTR                                         1416 04890000
*         NXTOP=ZERO;               /* RESET                         */ 04891000
