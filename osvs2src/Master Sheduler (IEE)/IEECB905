*         DO;                                                      0798 02669000
*           SCVAJOBN=WAJOBNAM;      /* COPY INFO FROM WA INTO SCVA   */ 02670000
         MVC   SCVAJOBN(9,SCVAPTR),WAJOBNAM                        0799 02671000
*           SCVAPTR=SCVAPTR+LENGTH(SCVAJOBN);/* PT PAST THIS         */ 02672000
         AH    SCVAPTR,@CH02322                                    0800 02673000
*         END;                                                     0801 02674000
*       IF WAJSPID^=ZERO THEN       /* IF JSPGM WAS SPECIFIED        */ 02675000
@RF00797 CLI   WAJSPID,0                                           0802 02676000
         BE    @RF00802                                            0802 02677000
*         DO;                                                      0803 02678000
*           SCVAJOBN=WAJSPGM;       /* COPY INFO FROM WA INTO SCVA   */ 02679000
         MVC   SCVAJOBN(9,SCVAPTR),WAJSPGM                         0804 02680000
*           SCVAPTR=SCVAPTR+LENGTH(SCVAJOBN);/* PT PAST THIS         */ 02681000
         AH    SCVAPTR,@CH02322                                    0805 02682000
*         END;                                                     0806 02683000
*       IF WAADDRID^=ZERO THEN      /* IF PVT|LPAMOD|ADDRESS SPEC    */ 02684000
@RF00802 CLI   WAADDRID,0                                          0807 02685000
         BE    @RF00807                                            0807 02686000
*         IF WAADDRID=SCVIDPVT|     /* IF PVTMOD OR LPAMOD SPEC      */ 02687000
*             WAADDRID=SCVIDLPA THEN                               0808 02688000
         CLI   WAADDRID,6                                          0808 02689000
         BE    @RT00808                                            0808 02690000
         CLI   WAADDRID,7                                          0808 02691000
         BNE   @RF00808                                            0808 02692000
@RT00808 DS    0H                                                  0809 02693000
*           DO;                                                    0809 02694000
*             SCVAMOD=WAADDR;       /* COPY INFO FROM WA INTO SCVA   */ 02695000
         MVC   SCVAMOD(17,SCVAPTR),WAADDR                          0810 02696000
*             SCVAPTR=SCVAPTR+LENGTH(SCVAMOD);/* PT PAST IT          */ 02697000
         AH    SCVAPTR,@CH02963                                    0811 02698000
*           END;                                                   0812 02699000
*         ELSE                      /* IF NOT PVTMOD OR LPAMOD       */ 02700000
*           DO;                     /* THEN ADDRESS WAS SPECIFIED    */ 02701000
         B     @RC00808                                            0813 02702000
@RF00808 DS    0H                                                  0814 02703000
*             SCVAADDR=WAADDR;      /* COPY INFO FROM WA INTO SCVA   */ 02704000
         MVC   SCVAADDR(9,SCVAPTR),WAADDR                          0814 02705000
*             SCVAPTR=SCVAPTR+LENGTH(SCVAADDR);/* PT PAST            */ 02706000
         AH    SCVAPTR,@CH02322                                    0815 02707000
*           END;                                                   0816 02708000
*       SCVCCID=SCVIDLST;           /* SET AN ID OF 0 TO IND END     */ 02709000
@RC00808 DS    0H                                                  0817 02710000
@RF00807 MVI   SCVCCID(SCVAPTR),X'01'                              0817 02711000
*     END;                                                         0818 02712000
*   IF WAFIN=OFF THEN               /* IF ALL OKAY                   */ 02713000
@RF00775 TM    WAFIN,B'00100000'                                   0819 02714000
         BNZ   @RF00819                                            0819 02715000
*     DO;                                                          0820 02716000
*                                                                  0820 02717000
*/********************************************************************  02718000
**   ADD ONE SCE ONTO THE SCE CHAIN. THE FLOW IS AS FOLLOWS:       0821 02719000
**         - IF SLIP IS NOT ACTIVE AND NO SHDR EXISTS (I.E., THIS IS    02720000
**           THE FIRST SLIP TRAP SET), CORE IS OBTAINED AND THE SHDR    02721000
**           IS INITIALIZED. THE CVT IS UPDATED (VIA CS) WITH THE  0821 02722000
**           ADDRESS OF THE SHDR.                                  0821 02723000
**         - ATTEMPTS ARE MADE TO OBTAIN (TS INSTRUCTION) THE COMMAND   02724000
**           SEQUENCING BYTE IN THE SHDR. IF UNSUCCESSFUL, A MESSAGE    02725000
**           IS WRITTEN ASKING THE ISSUER IF HE WANTS TO RETRY THE 0821 02726000
**           TS INSTRUCTION OR CANCEL.                             0821 02727000
**         - WHEN THE CMD-SEQ BYTE IS OBTAINED, THE WORKAREA IS    0821 02728000
**           CHECKED TO SEE IF AN ID WAS SUPPLIED AND IF NOT, ONE IS    02729000
**           GENERATED.                                            0821 02730000
**         - THE SCE'S ON THE CHAIN ARE EACH CHECKED TO ENSURE THE NEW  02731000
**           SCE'S ID IS UNIQUE. IF THE ID IS NOT UNIQUE AND WAS A 0821 02732000
**           GENERATED ID, ANOTHER ID IS GENERATED AND THE COMPARISON   02733000
**           WITH THE SCE'S ON THE CHAIN IS REPEATED. IF THE ID IS NOT  02734000
**           UNIQUE AND WAS A SPECIFIED ID, THE CMD-SEQ BYTE IS    0821 02735000
**           RELEASED AND A MESSAGE IS WRITTEN ASKING THE ISSUER TO     02736000
**           SPECIFY A NEW ID OR CANCEL. IF HE REPLIES WITH ID,    0821 02737000
**           CONTROL RETURNS TO THE CODE WHICH ATTEMPTS TO OBTAIN THE   02738000
**           CMD-SEQ BYTE.                                         0821 02739000
**         - IF THE SLIP PROCESSOR (IEAVTSLP) HAS NOT BEEN LOADED YET   02740000
**           AND THIS TRAP IS ENABLED, IT ATTEMPTS TO LOAD AND PAGEFIX  02741000
**           IEAVTSLP. IF UNSUCCESSFUL, IT ISSUES THE 328 MESSAGE AND   02742000
**           TERMINATES THE COMMAND.                               0821 02743000
**         - IF SUCCESSFUL TO HERE, THE SCE IS ADDED ONTO THE END OF    02744000
**           THE CHAIN. THE CMD-SEQ BYTE IS THEN RELEASED AND A MESSAGE 02745000
**           SENT INFORMING THE ISSUER THE TRAP IS SET.            0821 02746000
**********************************************************************/ 02747000
*                                                                  0821 02748000
*       IF CVTRTMS^=ZERO&SHDRCBID^=IDSHDR THEN                     0821 02749000
         L     @09,CVTPTR                                          0821 02750000
         L     @09,CVTRTMS(,@09)                                   0821 02751000
         LTR   @09,@09                                             0821 02752000
         BZ    @RF00821                                            0821 02753000
         CLC   SHDRCBID(4,@09),@CC03157                            0821 02754000
         BE    @RF00821                                            0821 02755000
*         GEN(ABEND 102);                                          0822 02756000
         ABEND 102                                                      02757000
*       IF CVTRTMS=ZERO THEN        /* IF SLIP HAS NEVER BEEN ACTIV  */ 02758000
*                                                                  0823 02759000
@RF00821 L     @09,CVTPTR                                          0823 02760000
         ICM   @05,15,CVTRTMS(@09)                                 0823 02761000
         BNZ   @RF00823                                            0823 02762000
*         /***********************************************************/ 02763000
*         /*                                                         */ 02764000
*         /* PREPARE SLIP TO BECOME ACTIVE BY CREATING AN SHDR       */ 02765000
*         /*                                                         */ 02766000
*         /***********************************************************/ 02767000
*                                                                  0824 02768000
*         DO;                                                      0824 02769000
*           RFY                                                    0825 02770000
*            (R0,                                                  0825 02771000
*             R1) RSTD;                                            0825 02772000
*           R0=SIZESHDR|SP245;      /* GET SHDR FROM SUBPOOL 245     */ 02773000
         L     R0,SIZESHDR                                         0826 02774000
         O     R0,@CF03117                                         0826 02775000
*           GEN(GETMAIN R,LV=(0));                                 0827 02776000
         GETMAIN R,LV=(0)                                               02777000
*           SAVSHDR=R1;             /* SAVE ADDR OF SHDR             */ 02778000
         LA    @09,STAE906                                         0828 02779000
         ST    R1,SAVSHDR(,@09)                                    0828 02780000
*           RFY                                                    0829 02781000
*            (R0,                                                  0829 02782000
*             R1) UNRSTD;                                          0829 02783000
*           RFY                                                    0830 02784000
*             SHDR BASED(SAVSHDR);  /* ACCESS SHDR                   */ 02785000
*           SHDR=SHDR&&SHDR;        /* CLEAR SHDR                    */ 02786000
         LA    @09,STAE906                                         0831 02787000
         L     @09,SAVSHDR(,@09)                                   0831 02788000
         XC    SHDR(28,@09),SHDR(@09)                              0831 02789000
*           SHDRCBID=IDSHDR;        /* PUT ID IN                     */ 02790000
         MVC   SHDRCBID(4,@09),@CC03157                            0832 02791000
*           SHDRBKWD=SAVSHDR;       /* INIT BKWD PTR                 */ 02792000
         ST    @09,SHDRBKWD(,@09)                                  0833 02793000
*           RFY                                                    0834 02794000
*             SHDR BASED(CVTRTMS);  /* RESET SHDR BASE               */ 02795000
*           RFY                                                    0835 02796000
*            (R6,                                                  0835 02797000
*             R7) RSTD;                                            0835 02798000
*           R6=ZERO;                /* INIT COMPARING VALUE TO 0     */ 02799000
         SLR   R6,R6                                               0836 02800000
*           R7=SAVSHDR;             /* INIT REPLACE VALUE            */ 02801000
         LR    R7,@09                                              0837 02802000
*           CS(R6,R7,CVTRTMS);      /* FILL IN CVT WITH SHDR ADDR    */ 02803000
         L     @09,CVTPTR                                          0838 02804000
         CS    R6,@07,CVTRTMS(@09)                                 0838 02805000
*           BC(8,HDROKAY);          /* IF CS FAILED, FREE STORAGE    */ 02806000
         BC    8,HDROKAY                                           0839 02807000
*           RFY                                                    0840 02808000
*            (R6,                                                  0840 02809000
*             R7) UNRSTD;                                          0840 02810000
*                                                                  0840 02811000
*           /*********************************************************/ 02812000
*           /*                                                       */ 02813000
*           /* FREE THE SHDR (SUBPOOL=245)                           */ 02814000
*           /*                                                       */ 02815000
*           /*********************************************************/ 02816000
*                                                                  0841 02817000
*           RFY                                                    0841 02818000
*            (R0,                                                  0841 02819000
*             R1) RSTD;                                            0841 02820000
*           R0=SIZESHDR|SP245;      /* SETUP SIZE AND SUBPOOL        */ 02821000
         L     R0,SIZESHDR                                         0842 02822000
         O     R0,@CF03117                                         0842 02823000
*           R1=SAVSHDR;             /* ADDRESS OF SHDR               */ 02824000
         LA    @09,STAE906                                         0843 02825000
         L     R1,SAVSHDR(,@09)                                    0843 02826000
*           GEN(FREEMAIN R,LV=(0),A=(1));/* FREE THE SHDR            */ 02827000
         FREEMAIN R,LV=(0),A=(1)                                        02828000
*           RFY                                                    0845 02829000
*            (R0,                                                  0845 02830000
*             R1) UNRSTD;                                          0845 02831000
*HDROKAY:                                                          0846 02832000
*           SAVSHDR=ZERO;           /* IND SHDR CONNECTED OR FREED   */ 02833000
HDROKAY  LA    @09,STAE906                                         0846 02834000
         SLR   @05,@05                                             0846 02835000
         ST    @05,SAVSHDR(,@09)                                   0846 02836000
*         END;                                                     0847 02837000
*       IF WAFIN=OFF THEN           /* IF NO ERROR, CONTINUE         */ 02838000
@RF00823 TM    WAFIN,B'00100000'                                   0848 02839000
         BNZ   @RF00848                                            0848 02840000
*         DO;                                                      0849 02841000
*                                                                  0849 02842000
*           /*********************************************************/ 02843000
*           /*                                                       */ 02844000
*           /* ATTEMPT TO OBTAIN THE COMMAND-SEQUENCING BYTE IN THE  */ 02845000
*           /* SHDR                                                  */ 02846000
*           /*                                                       */ 02847000
*           /*********************************************************/ 02848000
*                                                                  0850 02849000
*           CALL GETSEQ;            /* ATTEMPT TO GET CMD-SEQ BYTE   */ 02850000
         BAL   @14,GETSEQ                                          0850 02851000
*           IF SESTSEQ=ON THEN      /* IF SEQ BYTE WAS OBTAINED      */ 02852000
         LA    @09,STAE906                                         0851 02853000
         TM    SESTSEQ(@09),B'00100000'                            0851 02854000
         BNO   @RF00851                                            0851 02855000
*             DO;                                                  0852 02856000
*                                                                  0852 02857000
*               /*****************************************************/ 02858000
*               /*                                                   */ 02859000
*               /* CHECK FOR DUPLICATE SCE ID. IF ID NOT PSECIFIED,  */ 02860000
*               /* ONE IS GENERATED. IF A DUPLICATE OF THE ID        */ 02861000
*               /* (SPECIFIED OR GENERATED) IS FOUND IN THE SCE      */ 02862000
*               /* CHAIN, THEN 1) IF THE ID WAS SPECIFIED, A MESSAGE */ 02863000
*               /* IS SENT TO THE ISSUER ASKING FOR ANOTHER WHICH    */ 02864000
*               /* WILL BE CHECKED FOR DUPLICATE, 2) IF THE ID WAS   */ 02865000
*               /* GENERATED, ANOTHER ONE IS GENERATED AND CHECKED.  */ 02866000
*               /*                                                   */ 02867000
*               /*****************************************************/ 02868000
*                                                                  0853 02869000
*               DUPID=ON;           /* CHECK ID FOR DUPLICATE        */ 02870000
         OI    DUPID,B'10000000'                                   0853 02871000
*               DO WHILE WAFIN=OFF& /* KEEP CHECKING FOR DUP UNITL   */ 02872000
*                     DUPID=ON;     /* A UNIQUE ID IS FOUND OR THE 0854 02873000
*                                      ISSUER CANCELS THE CMD        */ 02874000
         B     @DE00854                                            0854 02875000
@DL00854 DS    0H                                                  0855 02876000
*                 IF WAID=BLANKS THEN/* IF AN ID MUST BE GENERATED   */ 02877000
         CLC   WAID(4),@CC03150                                    0855 02878000
         BNE   @RF00855                                            0855 02879000
*                   DO;                                            0856 02880000
*                     SHDRIDCT=SHDRIDCT+ONE;/* INCR ID CTR IN SHDR   */ 02881000
         L     @09,CVTPTR                                          0857 02882000
         L     @09,CVTRTMS(,@09)                                   0857 02883000
         LA    @05,1                                               0857 02884000
         AH    @05,SHDRIDCT(,@09)                                  0857 02885000
         STH   @05,SHDRIDCT(,@09)                                  0857 02886000
*                     IF SHDRIDCT>NINES THEN/* IF CTR OVER MAX OF  0858 02887000
*                                      9999                          */ 02888000
         CH    @05,@CH03113                                        0858 02889000
         BNH   @RF00858                                            0858 02890000
*                       SCEID=ZERO; /* THEN DO NOT FILL IN SCE       */ 02891000
         LA    @09,STAE906                                         0859 02892000
         L     @09,SCEPTR(,@09)                                    0859 02893000
         SLR   @05,@05                                             0859 02894000
         ST    @05,SCEID(,@09)                                     0859 02895000
*                     ELSE                                         0860 02896000
*                       DO;                                        0860 02897000
         B     @RC00858                                            0860 02898000
@RF00858 DS    0H                                                  0861 02899000
*                         CVD(SHDRIDCT,TEMPID);/* CONVERT ID HEX TO     02900000
*                                      DEC                           */ 02901000
         L     @09,CVTPTR                                          0861 02902000
         L     @09,CVTRTMS(,@09)                                   0861 02903000
         LH    @09,SHDRIDCT(,@09)                                  0861 02904000
         CVD   @09,@TS00001                                        0861 02905000
         MVC   TEMPID(4),@TS00001+4                                0861 02906000
*                         TEMPID=TEMPID|CHPOS;/* CHANGE SIGN C TO F  */ 02907000
         OC    TEMPID(4),@CB03192                                  0862 02908000
*                         UNPK(SCEID,TEMPID);/* CONVERT TO CHAR INTO    02909000
*                                      SCE                           */ 02910000
         LA    @09,STAE906                                         0863 02911000
         L     @09,SCEPTR(,@09)                                    0863 02912000
         UNPK  SCEID(4,@09),TEMPID(4)                              0863 02913000
*                       END;                                       0864 02914000
*                   END;                                           0865 02915000
*                 ELSE              /* IF AN ID WAS SPECIFIED        */ 02916000
*                   SCEID=WAID;     /* FILL IN SCE                   */ 02917000
         B     @RC00855                                            0866 02918000
@RF00855 LA    @09,STAE906                                         0866 02919000
         L     @09,SCEPTR(,@09)                                    0866 02920000
         MVC   SCEID(4,@09),WAID                                   0866 02921000
*                 IF SCEID=ZERO&    /* IF ID WAS GENERATED BUT WAS   */ 02922000
*                     WAID=BLANKS THEN                             0867 02923000
@RC00855 LA    @09,STAE906                                         0867 02924000
         L     @09,SCEPTR(,@09)                                    0867 02925000
         ICM   @05,15,SCEID(@09)                                   0867 02926000
         BNZ   @RF00867                                            0867 02927000
         CLC   WAID(4),@CC03150                                    0867 02928000
         BE    @RT00867                                            0867 02929000
*                   ;               /* OVER MAX, SKIP SCE SEARCH     */ 02930000
*                 ELSE              /* COMPARE THIS ID TO CHAINSCE   */ 02931000
*                   DO;                                            0869 02932000
@RF00867 DS    0H                                                  0870 02933000
*                     TEMPID=SCEID; /* MOVE ID IN ORDER TO ADDR IT   */ 02934000
         LA    @09,STAE906                                         0870 02935000
         L     @05,SCEPTR(,@09)                                    0870 02936000
         MVC   TEMPID(4),SCEID(@05)                                0870 02937000
*                     SCANSCE=SHDRFWD;/* ADDR OF FIRST SCE           */ 02938000
         L     @05,CVTPTR                                          0871 02939000
         L     @05,CVTRTMS(,@05)                                   0871 02940000
         MVC   SCANSCE(4,@09),SHDRFWD(@05)                         0871 02941000
*                     RFY                                          0872 02942000
*                       SCE BASED(SCANSCE);                        0872 02943000
*                     DO WHILE SCANSCE^=ZERO&/* SCAN SCE CHAIN UNTIL */ 02944000
*                           SCECBID=IDSCE&TEMPID^=SCEID;/* AT END OR    02945000
*                                      DUP FOUND                     */ 02946000
         B     @DE00873                                            0873 02947000
@DL00873 DS    0H                                                  0874 02948000
*                       SCANSCE=SCEFWD;/* NEXT SCE ON THE CHAIN      */ 02949000
         LA    @09,STAE906                                         0874 02950000
         L     @05,SCANSCE(,@09)                                   0874 02951000
         L     @05,SCEFWD(,@05)                                    0874 02952000
         ST    @05,SCANSCE(,@09)                                   0874 02953000
*                     END;                                         0875 02954000
@DE00873 LA    @09,STAE906                                         0875 02955000
         L     @09,SCANSCE(,@09)                                   0875 02956000
         LTR   @09,@09                                             0875 02957000
         BZ    @DC00873                                            0875 02958000
         CLC   SCECBID(4,@09),@CC03159                             0875 02959000
         BNE   @DC00873                                            0875 02960000
         CLC   TEMPID(4),SCEID(@09)                                0875 02961000
         BNE   @DL00873                                            0875 02962000
@DC00873 DS    0H                                                  0876 02963000
*                     IF SCANSCE=ZERO THEN/* IF END OF CHAIN REACHED */ 02964000
         LA    @09,STAE906                                         0876 02965000
         ICM   @05,15,SCANSCE(@09)                                 0876 02966000
         BNZ   @RF00876                                            0876 02967000
*                       DUPID=OFF;  /* THEN SCE ID IS NOT A DUP      */ 02968000
         NI    DUPID,B'01111111'                                   0877 02969000
*                     ELSE                                         0878 02970000
*                       IF SCECBID^=IDSCE THEN                     0878 02971000
         B     @RC00876                                            0878 02972000
@RF00876 LA    @09,STAE906                                         0878 02973000
         L     @09,SCANSCE(,@09)                                   0878 02974000
         CLC   SCECBID(4,@09),@CC03159                             0878 02975000
         BE    @RF00878                                            0878 02976000
*                         GEN(ABEND 102);                          0879 02977000
         ABEND 102                                                      02978000
*                     RFY                                          0880 02979000
*                       SCE BASED(SCEPTR);                         0880 02980000
@RF00878 DS    0H                                                  0880 02981000
@RC00876 DS    0H                                                  0881 02982000
*                   END;                                           0881 02983000
*                 IF DUPID=ON THEN  /* IF A UNIQUE ID NOT FOUND      */ 02984000
@RT00867 TM    DUPID,B'10000000'                                   0882 02985000
         BNO   @RF00882                                            0882 02986000
*                   IF WAID^=BLANKS|/* AND IF ID WAS SPECIFIED OR    */ 02987000
*                       SCEID=ZERO THEN/* THE GENERATED ONE WAS > MAX*/ 02988000
         CLC   WAID(4),@CC03150                                    0883 02989000
         BNE   @RT00883                                            0883 02990000
         LA    @09,STAE906                                         0883 02991000
         L     @09,SCEPTR(,@09)                                    0883 02992000
         ICM   @05,15,SCEID(@09)                                   0883 02993000
         BNZ   @RF00883                                            0883 02994000
@RT00883 DS    0H                                                  0884 02995000
*                     DO;                                          0884 02996000
*                       SHDRSEQ=ZEROB;/* RELEASE CMD-SEQ BYTE        */ 02997000
         L     @09,CVTPTR                                          0885 02998000
         L     @09,CVTRTMS(,@09)                                   0885 02999000
         MVI   SHDRSEQ(@09),X'00'                                  0885 03000000
*                       SESTSEQ=OFF;/* IND CMD-SEQ BYTE NOT HELD     */ 03001000
         LA    @09,STAE906                                         0886 03002000
         NI    SESTSEQ(@09),B'11011111'                            0886 03003000
*                       BADREPLY=ON;/* INIT CONTROL BIT              */ 03004000
         OI    BADREPLY,B'10000000'                                0887 03005000
*                       DO WHILE WAFIN=OFF&/* ASK ISSUER FOR ANOTHER */ 03006000
*                             BADREPLY=ON;/* ID UNITL OR REPLY       */ 03007000
         B     @DE00888                                            0888 03008000
@DL00888 DS    0H                                                  0889 03009000
*                         REPLYBUF=BLANK;/* CLEAR BUF                */ 03010000
         MVI   REPLYBUF+1,C' '                                     0889 03011000
         MVC   REPLYBUF+2(117),REPLYBUF+1                          0889 03012000
         MVI   REPLYBUF,C' '                                       0889 03013000
*                         CALL IEECB908(MSG729,WAUCMP,SCEID,MSGVAR2,    03014000
*                             REPLYBUF,ECB1);                      0890 03015000
         LA    @09,@CH00996                                        0890 03016000
         ST    @09,@AL00001                                        0890 03017000
         LA    @09,WAUCMP                                          0890 03018000
         ST    @09,@AL00001+4                                      0890 03019000
         LA    @09,STAE906                                         0890 03020000
         L     @09,SCEPTR(,@09)                                    0890 03021000
         LA    @09,SCEID(,@09)                                     0890 03022000
         ST    @09,@AL00001+8                                      0890 03023000
         LA    @09,MSGVAR2                                         0890 03024000
         ST    @09,@AL00001+12                                     0890 03025000
         LA    @09,REPLYBUF                                        0890 03026000
         ST    @09,@AL00001+16                                     0890 03027000
         LA    @09,ECB1                                            0890 03028000
         ST    @09,@AL00001+20                                     0890 03029000
         MVI   @AL00001+20,X'80'                                   0890 03030000
         L     @15,@CV02993                                        0890 03031000
         LA    @01,@AL00001                                        0890 03032000
         BALR  @14,@15                                             0890 03033000
*                         RFY                                      0891 03034000
*                           IN BASED(REPLY);                       0891 03035000
*                         RFY                                      0892 03036000
*                           R15 RSTD;                              0892 03037000
*                         IF R15^=ZERO THEN/* IF MSG MOD RC BAD      */ 03038000
         LTR   R15,R15                                             0893 03039000
         BZ    @RF00893                                            0893 03040000
*                           WAFIN=ON;/* TERMINATE CMD, EXIT LOOP     */ 03041000
         OI    WAFIN,B'00100000'                                   0894 03042000
*                         ELSE      /* MSG MOD RC OKAY               */ 03043000
*                           IF IN(1:6)=CANCEL THEN                 0895 03044000
         B     @RC00893                                            0895 03045000
@RF00893 CLC   IN(6,REPLY),@CC03154                                0895 03046000
         BNE   @RF00895                                            0895 03047000
*                             DO;                                  0896 03048000
*                               CALL IEECB908(MSG756,WAUCMP,SLIP); 0897 03049000
         MVC   @AL00001(12),@AL00897                               0897 03050000
         LA    @14,WAUCMP                                          0897 03051000
         ST    @14,@AL00001+4                                      0897 03052000
         L     @15,@CV02993                                        0897 03053000
         LA    @01,@AL00001                                        0897 03054000
         BALR  @14,@15                                             0897 03055000
*                               WAFIN=ON;                          0898 03056000
         OI    WAFIN,B'00100000'                                   0898 03057000
*                             END;                                 0899 03058000
*                           ELSE                                   0900 03059000
*                             DO;                                  0900 03060000
         B     @RC00895                                            0900 03061000
@RF00895 DS    0H                                                  0901 03062000
*                               UNKNOWN8=IN(1:3);/* COPY FOR TABLE 0901 03063000
*                                      TEST                          */ 03064000
         MVI   UNKNOWN8+3,C' '                                     0901 03065000
         MVC   UNKNOWN8+4(4),UNKNOWN8+3                            0901 03066000
         MVC   UNKNOWN8(3),IN(REPLY)                               0901 03067000
*                               IF UNKNOWN8=KTABKEY(KTIDX) THEN    0902 03068000
         L     @14,@CV02893                                        0902 03069000
         CLC   UNKNOWN8(8),KTABKEY+100(@14)                        0902 03070000
