*                               WAFIN=ON;/* TERMINATE THE COMMANDS   */ 00512000
         OI    WAFIN,B'00100000'                                   0235 00513000
*                             END;                                 0236 00514000
*                           ELSE                                   0237 00515000
*                             DO;                                  0237 00516000
