*       GEN(BALR R14,R15);          /* USE THE PGFIX/FREE SERVICE.   */ 03584000
         BALR R14,R15                                                   03585000
*       R4=R6;                      /* GET DATA REG BACK.            */ 03586000
         LR    R4,R6                                               1039 03587000
*       RFY                                                        1040 03588000
*         R4 UNRSTD;                                               1040 03589000
*       IF R15=8 THEN               /* NEED A WAIT ?                 */ 03590000
         CH    R15,@CH00061                                        1041 03591000
         BNE   @RF01041                                            1041 03592000
*         DO;                       /* YES.                          */ 03593000
*           RFY                                                    1043 03594000
*             TCB BASED(PSATOLD);                                  1043 03595000
*           RFY                                                    1044 03596000
*             RBBASIC BASED(TCBRBP);                               1044 03597000
*           PHALF1=UPPSW;           /* SETUP RBOPSW                  */ 03598000
         L     @14,PSATOLD                                         1045 03599000
         L     @07,TCBRBP(,@14)                                    1045 03600000
         MVC   PHALF1(4,@07),@CB03075                              1045 03601000
*           PHALF2=ADDR(PGFXWAIT);  /* PUT RESUME ADDR IN            */ 03602000
         LA    @05,PGFXWAIT                                        1046 03603000
         ST    @05,PHALF2(,@07)                                    1046 03604000
*           STM(R0,R15,TCBGRS);     /* SAVE REGS IN TCB              */ 03605000
         STM   R0,R15,TCBGRS(@14)                                  1047 03606000
*           R1=ADDR(PGECB);         /* ADDR OF ECB FOR WAIT          */ 03607000
         L     @14,CVTPTR                                          1048 03608000
         L     R1,CVTRTMS(,@14)                                    1048 03609000
         LA    R1,PGECB(,R1)                                       1048 03610000
*           R0=ONE;                 /* WAIT FOR ONE EVENT            */ 03611000
         LA    R0,1                                                1049 03612000
*           R15=CVTVWAIT;           /* GET ADDR OF WAIT RTN          */ 03613000
         L     R15,CVTVWAIT(,@14)                                  1050 03614000
*           GEN(BALR R14,R15);                                     1051 03615000
         BALR R14,R15                                                   03616000
*PGFXWAIT:                                                         1052 03617000
*           ;                                                      1052 03618000
PGFXWAIT DS    0H                                                  1053 03619000
*           FRLOCK=OFF;             /* IND LOCK FREED BY WAIT        */ 03620000
         NI    FRLOCK,B'11101111'                                  1053 03621000
*           IF PGCOMP^=0 THEN       /* FIX/FREE OK ?                 */ 03622000
