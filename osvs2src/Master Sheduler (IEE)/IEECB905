         ST    @05,SCEFWD(,@14)                                    0929 03192000
*                   SHDRBKWD=SCEPTR;/* UPDATE SHDR TO POINT TO SCE   */ 03193000
         ST    @05,SHDRBKWD(,@15)                                  0930 03194000
*                   SESTCHN=OFF;    /* IND CHAINING FINISHED         */ 03195000
         NI    SESTCHN(@09),B'11111101'                            0931 03196000
*                   WAID=SCEID;     /* SAVE SCE'S ID                 */ 03197000
         MVC   WAID(4),SCEID(@05)                                  0932 03198000
*                   SCEPTR=ZERO;    /* IND NOT TO FREE SCE ON ERR    */ 03199000
         SLR   @05,@05                                             0933 03200000
         ST    @05,SCEPTR(,@09)                                    0933 03201000
*                   SHDRSEQ=ZERO;   /* RELEASE CMD-SEQ BYTE          */ 03202000
         MVI   SHDRSEQ(@15),X'00'                                  0934 03203000
*                   SESTSEQ=OFF;    /* IND CMD-SEQ BYTE NOT HELD     */ 03204000
         NI    SESTSEQ(@09),B'11011111'                            0935 03205000
*                   CALL IEECB908(MSG727S,WAUCMP,WAID);/* TRAP SET 0936 03206000
*                                      MSG                           */ 03207000
         LA    @09,@CH00075                                        0936 03208000
         ST    @09,@AL00001                                        0936 03209000
         LA    @09,WAUCMP                                          0936 03210000
         ST    @09,@AL00001+4                                      0936 03211000
         LA    @09,WAID                                            0936 03212000
