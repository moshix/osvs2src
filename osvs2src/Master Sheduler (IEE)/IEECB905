*                                 SHDRSEQ=ZEROB;/* RELEASE CMD-SEQ 0496 01652000
*                                      BYTE                          */ 01653000
         L     @01,CVTPTR                                          0496 01654000
         L     @01,CVTRTMS(,@01)                                   0496 01655000
         MVI   SHDRSEQ(@01),X'00'                                  0496 01656000
*                                 SESTSEQ=OFF;/* IND CMD-SEQ BYTE NOT   01657000
*                                      HELD                          */ 01658000
         LA    @15,STAE906                                         0497 01659000
         NI    SESTSEQ(@15),B'11011111'                            0497 01660000
*                               END;                               0498 01661000
*                           END;    /* END DEL,ALL PROCESSING        */ 01662000
*                   END;                                           0500 01663000
*                 ELSE              /* IF INVALID SYNTAX             */ 01664000
*                   CALL NEXTKEY;   /* PROMPT ISSUER                 */ 01665000
         B     @RC00362                                            0501 01666000
@RF00362 BAL   @14,NEXTKEY                                         0501 01667000
*               END;                                               0502 01668000
@RC00362 DS    0H                                                  0503 01669000
*           END;                    /* END OF DO-WHILE               */ 01670000
@RF00332 DS    0H                                                  0503 01671000
@DE00330 TM    WAFIN,B'00100000'                                   0503 01672000
         BZ    @DL00330                                            0503 01673000
*         END;                                                     0504 01674000
*       ELSE                                                       0505 01675000
*         DO;                       /* IF KEYWORD IS NONE OF ABOVE   */ 01676000
         B     @RC00324                                            0505 01677000
@RF00324 DS    0H                                                  0506 01678000
*           RFY                                                    0506 01679000
*             XSAPTR RSTD;                                         0506 01680000
*           XSAPTR=ADDR(MSGBUF);    /* MAP XSA OVER MSG BUFFER       */ 01681000
         LA    XSAPTR,MSGBUF                                       0507 01682000
*           XAE=MSG310;             /* SETUP FOR MSG IEE310I         */ 01683000
         MVI   XAE(XSAPTR),X'0A'                                   0508 01684000
*           CALL MSGMOD05;          /* INFORM ISSUER KEYWORD MISSNG  */ 01685000
         BAL   @14,MSGMOD05                                        0509 01686000
*           RFY                                                    0510 01687000
*             XSAPTR UNRSTD;                                       0510 01688000
*         END;                                                     0511 01689000
*   IF CSCBPTR^=ZERO THEN           /* IF CSCB HASN'T BEEN FREED     */ 01690000
@RC00324 DS    0H                                                  0512 01691000
@RC00194 DS    0H                                                  0512 01692000
@RC00141 LA    @15,STAE906                                         0512 01693000
         ICM   @14,15,CSCBPTR(@15)                                 0512 01694000
         BZ    @RF00512                                            0512 01695000
*     CALL FREECSCB;                /* FREE IT                       */ 01696000
         BAL   @14,FREECSCB                                        0513 01697000
*   R14=SAVR14;                     /* RESTORE RETURN ADDRESS        */ 01698000
@RF00512 LR    R14,SAVR14                                          0514 01699000
*   RETURN;                                                        0515 01700000
@EL00001 L     @13,4(,@13)                                         0515 01701000
@EF00001 L     @00,@SIZDATD                                        0515 01702000
         LR    @01,@04                                             0515 01703000
         FREEMAIN R,LV=(0),A=(1)                                        01704000
@ER00001 BR    @14                                                 0515 01705000
*                                                                  0516 01706000
*/********************************************************************  01707000
**  THIS SEGMENT HANDLES THE KEYWORD ACTION= AND IT'S ABBREV A=.   0516 01708000
**  THERE ARE THREE AVAILABLE OPTIONS: WAIT, SVCD, AND IGNORE.     0516 01709000
**********************************************************************/ 01710000
*                                                                  0516 01711000
*ACTION:                                                           0516 01712000
*   PROC;                                                          0516 01713000
ACTION   STM   @14,@12,@SA00002                                    0516 01714000
*   CALL SCAN;                      /* SCAN FOR NEXT DELIMITER       */ 01715000
         BAL   @14,SCAN                                            0517 01716000
*   IF WAFIN=OFF THEN               /* IF CMD HAS NOT BEEN CANCELED  */ 01717000
         TM    WAFIN,B'00100000'                                   0518 01718000
         BNZ   @RF00518                                            0518 01719000
*     DO;                                                          0519 01720000
*       IF WARESP=OFF THEN          /* IF SCAN HAD NO PROBLEMS       */ 01721000
         TM    WARESP,B'00010000'                                  0520 01722000
         BNZ   @RF00520                                            0520 01723000
*         IF DATALN=FOUR THEN       /* IF LENGTH OF OPERAND IS 4     */ 01724000
         CLC   DATALN(2),@CH00035                                  0521 01725000
         BNE   @RF00521                                            0521 01726000
*           IF IN(1:4)=WAIT THEN    /* CHECK IF OPERAND IS WAIT      */ 01727000
         L     @05,TEXTPTR                                         0522 01728000
         CLC   IN(4,@05),@CC03134                                  0522 01729000
         BNE   @RF00522                                            0522 01730000
*             DO;                                                  0523 01731000
*               SCEWAIT=ON;         /* IF SO, IND IN SCE             */ 01732000
*               SCESVCD=OFF;        /* AND SET SVCD AND              */ 01733000
*               SCEIGNOR=OFF;       /* IGNORE INDS OFF               */ 01734000
*               SCENODMP=OFF;       /* NODUMP INDS OFF       @G33NPHE*/ 01735000
         LA    @05,STAE906                                         0527 01736000
         L     @05,SCEPTR(,@05)                                    0527 01737000
         OI    SCEWAIT(@05),B'00001000'                            0527 01738000
         NI    SCESVCD(@05),B'11101001'                            0527 01739000
*             END;                                                 0528 01740000
*           ELSE                    /* IF NOT WAIT,                  */ 01741000
*             IF IN(1:4)=SVCD THEN  /* CHECK IF OPERAND IS SVCD      */ 01742000
         B     @RC00522                                            0529 01743000
@RF00522 L     @05,TEXTPTR                                         0529 01744000
         CLC   IN(4,@05),@CC03136                                  0529 01745000
         BNE   @RF00529                                            0529 01746000
*               DO;                                                0530 01747000
*                 SCESVCD=ON;       /* IF SO, IND IN SCE             */ 01748000
*                 SCEWAIT=OFF;      /* AND SET WAIT AND              */ 01749000
*                 SCEIGNOR=OFF;     /* IGNORE INDS OFF               */ 01750000
*                 SCENODMP=OFF;     /* NODUMP INDS OFF       @G33NPHE*/ 01751000
         LA    @05,STAE906                                         0534 01752000
         L     @05,SCEPTR(,@05)                                    0534 01753000
         OI    SCESVCD(@05),B'00010000'                            0534 01754000
         NI    SCEWAIT(@05),B'11110001'                            0534 01755000
*               END;                                               0535 01756000
*             ELSE                  /* IF NOT WAIT OR SVCD,          */ 01757000
*               WARESP=ON;          /* IND INVALID SYNTAX            */ 01758000
         B     @RC00529                                            0536 01759000
@RF00529 OI    WARESP,B'00010000'                                  0536 01760000
*         ELSE                      /* OPERAND'S LENGTH IS NOT 4     */ 01761000
*           IF DATALN=SIX THEN      /* CHECK IF IT'S 6               */ 01762000
         B     @RC00521                                            0537 01763000
@RF00521 CLC   DATALN(2),@CH00737                                  0537 01764000
         BNE   @RF00537                                            0537 01765000
*             IF IN(1:6)=IGNORE THEN/* IF SO, IS OPERAND IGNORE      */ 01766000
         L     @05,TEXTPTR                                         0538 01767000
         CLC   IN(6,@05),@CC03138                                  0538 01768000
         BNE   @RF00538                                            0538 01769000
*               DO;                                                0539 01770000
*                 SCEIGNOR=ON;      /* IF SO, IND IN SCE             */ 01771000
*                 SCESVCD=OFF;      /* AND SET SVCD AND              */ 01772000
*                 SCEWAIT=OFF;      /* WAIT INDS OFF                 */ 01773000
*                 SCENODMP=OFF;     /* NODUMP INDS OFF       @G33NPHE*/ 01774000
