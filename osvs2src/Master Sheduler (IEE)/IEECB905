*           DO;                     /* IF SO THEN                    */ 03980000
*             IF WAJOBID=ZEROB THEN /* IF JOBNAME NOT PREVIOUSLY     */ 03981000
         CLI   WAJOBID,0                                           1158 03982000
         BNE   @RF01158                                            1158 03983000
*               DO;                 /* STORED IN WA                  */ 03984000
*                 WAJOBID=SCVIDJN;  /* INIT ENTRY ID                 */ 03985000
         MVI   WAJOBID,X'04'                                       1160 03986000
*                 WASCVALN=WASCVALN+LENGTH(SCVAJOBN);/* ADD IN LNGTH */ 03987000
         AH    @05,WASCVALN                                        1161 03988000
         STH   @05,WASCVALN                                        1161 03989000
*               END;                                               1162 03990000
*             WAJOBD=IN(1:DATALN);  /* COPY JOBNAME INTO WA          */ 03991000
@RF01158 MVI   WAJOBD+1,C' '                                       1163 03992000
         MVC   WAJOBD+2(6),WAJOBD+1                                1163 03993000
         LH    @07,DATALN                                          1163 03994000
         LR    @05,@07                                             1163 03995000
         BCTR  @05,0                                               1163 03996000
