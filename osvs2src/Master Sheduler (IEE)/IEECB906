*             DO;                                                  0147 00409000
*               FIN=OFF;            /* RESET ERR IND                 */ 00410000
         NI    FIN(R2),B'11110111'                                 0148 00411000
*               BCKSCE1=SHDRBKWD;   /* INIT PTR TO LAST ELEMENT      */ 00412000
         L     @15,CVTPTR                                          0149 00413000
         L     @01,CVTRTMS(,@15)                                   0149 00414000
         L     BCKSCE1,SHDRBKWD(,@01)                              0149 00415000
*               LSTSCEB=ZERO;       /* INIT PTR                      */ 00416000
         SLR   LSTSCEB,LSTSCEB                                     0150 00417000
*               DO WHILE BCKSCE1^=ADDR(SHDR)&/* LOOP THRU CHAIN      */ 00418000
*                     FIN=OFF;      /* UNTIL END OF CHAIN OR ERROR   */ 00419000
         B     @DE00151                                            0151 00420000
@DL00151 DS    0H                                                  0152 00421000
*                 IF BCKSCE1^=ZERO&BCKSCE1->SCECBID='SCE '&BCKSCE1->    00422000
*                     SCEFWD=LSTSCEB THEN                          0152 00423000
         LTR   BCKSCE1,BCKSCE1                                     0152 00424000
         BZ    @RF00152                                            0152 00425000
         CLC   SCECBID(4,BCKSCE1),@CC01623                         0152 00426000
         BNE   @RF00152                                            0152 00427000
         C     LSTSCEB,SCEFWD(,BCKSCE1)                            0152 00428000
         BNE   @RF00152                                            0152 00429000
*                   DO;                                            0153 00430000
*                     LSTSCEB=BCKSCE1;/* SAVE ADDR OF SCE            */ 00431000
         LR    LSTSCEB,BCKSCE1                                     0154 00432000
*                     BCKSCE1=BCKSCE1->SCEBKWD;/* UPDATE PTR TO NXT  */ 00433000
         L     BCKSCE1,SCEBKWD(,BCKSCE1)                           0155 00434000
*                   END;                                           0156 00435000
*                 ELSE              /* IF PTRS BAD                   */ 00436000
*                   FIN=ON;         /* IND ERROR                     */ 00437000
         B     @RC00152                                            0157 00438000
@RF00152 OI    FIN(R2),B'00001000'                                 0157 00439000
*               END;                                               0158 00440000
@RC00152 DS    0H                                                  0158 00441000
@DE00151 L     @15,CVTPTR                                          0158 00442000
         C     BCKSCE1,CVTRTMS(,@15)                               0158 00443000
         BE    @DC00151                                            0158 00444000
         TM    FIN(R2),B'00001000'                                 0158 00445000
         BZ    @DL00151                                            0158 00446000
@DC00151 DS    0H                                                  0159 00447000
*               LSTSCEF->SCEFWD=LSTSCEB;/* CONNECT TWO SCES          */ 00448000
         ST    LSTSCEB,SCEFWD(,LSTSCEF)                            0159 00449000
*               IF LSTSCEB=ZERO THEN/* IF AT END OF CHAIN            */ 00450000
         LTR   LSTSCEB,LSTSCEB                                     0160 00451000
         BNZ   @RF00160                                            0160 00452000
*                 SHDRBKWD=LSTSCEF; /* MOVE INTO SHDR                */ 00453000
         L     @15,CVTPTR                                          0161 00454000
         L     @01,CVTRTMS(,@15)                                   0161 00455000
         ST    LSTSCEF,SHDRBKWD(,@01)                              0161 00456000
*               ELSE                /* IF NOT AT END                 */ 00457000
*                 LSTSCEB->SCEBKWD=LSTSCEF;/* MOVE ADDR INTO SCE     */ 00458000
         B     @RC00160                                            0162 00459000
@RF00160 ST    LSTSCEF,SCEBKWD(,LSTSCEB)                           0162 00460000
*             END;                                                 0163 00461000
@RC00160 DS    0H                                                  0164 00462000
*           RFY                                                    0164 00463000
*            (LSTSCEF,                                             0164 00464000
*             BCKSCE1,                                             0164 00465000
*             LSTSCEB) UNRSTD;                                     0164 00466000
@RF00146 DS    0H                                                  0165 00467000
*           SHDRSEQ=ZEROB;          /* RELEASE CMD-SEQ BYTE          */ 00468000
         L     @15,CVTPTR                                          0165 00469000
         L     @01,CVTRTMS(,@15)                                   0165 00470000
         MVI   SHDRSEQ(@01),X'00'                                  0165 00471000
*         END;                                                     0166 00472000
*       ELSE                                                       0167 00473000
*         ;                                                        0167 00474000
@RF00130 DS    0H                                                  0168 00475000
*     ELSE                          /* CVT POINTS NOT TO AN SHDR     */ 00476000
*       CVTRTMS=0;                  /* REINIT PTR                    */ 00477000
         B     @RC00129                                            0168 00478000
@RF00129 L     @15,CVTPTR                                          0168 00479000
         SLR   @06,@06                                             0168 00480000
         ST    @06,CVTRTMS(,@15)                                   0168 00481000
*   IF SESTCSCB^=0                  /* IF A CSCB IS HELD             */ 00482000
*     THEN                                                         0169 00483000
@RC00129 DS    0H                                                  0169 00484000
@RF00128 ICM   @15,15,SESTCSCB(R2)                                 0169 00485000
         BZ    @RF00169                                            0169 00486000
*     DO;                           /* FREE IT                       */ 00487000
*       CHFC=ON;                    /* INDICATE THIS CSCB TO BE FREED*/ 00488000
         L     @15,CSCBPTR(,R2)                                    0171 00489000
         OI    CHFC(@15),B'00000010'                               0171 00490000
*       R1=ADDR(CHAIN);             /* LOAD THE CSCB ADDR IN R1 FOR     00491000
*                                      THE SVC                       */ 00492000
         LR    R1,@15                                              0172 00493000
*       GEN REFS(R1) CODE(MGCR (1),CHAIN);                         0173 00494000
         MGCR (1),CHAIN                                                 00495000
*     END;                                                         0174 00496000
*   RFY                                                            0175 00497000
*    (R14,                                                         0175 00498000
*     R15) RSTD;                                                   0175 00499000
@RF00169 DS    0H                                                  0176 00500000
*   IF SEST503D=ON                  /* SHOULD IEE0503D BE DELETED    */ 00501000
*     THEN                          /* YES                           */ 00502000
