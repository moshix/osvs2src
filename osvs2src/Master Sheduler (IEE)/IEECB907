         MVI   WPLMLTXT(@10),C' '                               02S0324 00398000
         MVC   WPLMLTXT+1(65,@10),WPLMLTXT(@10)                 02S0324 00399000
*                       DO IDS=1 TO MAXIDS/* PUT UP TO 5 IDS PER DATA*/ 00400000
*                             WHILE(SCEEND=NO);/* LINE               */ 00401000
