         AL    @06,@CF01941                                     02S0326 00411000
         MVC   ID(4,@06),SCEID(SCEPTR)                          02S0326 00412000
*                         IF SCEDSABL=YES THEN/* IF THIS TRAP IS        00413000
*                                      DISABLED                      */ 00414000
         TM    SCEDSABL(SCEPTR),B'10000000'                     02S0327 00415000
         BNO   @RF00327                                         02S0327 00416000
*                           DO;     /* SET LENGTH BIT AND DISABLED   */ 00417000
*                             ENBL=OFF;/* SET ENABLE BIT FOR LENGTH. */ 00418000
         NI    ENBL,B'01111111'                                 02S0329 00419000
*                             STATE(IDS)=DISABLED;/* INDICATE IN        00420000
*                                      DISPLAY LINE                  */ 00421000
         AL    @10,@TF00001                                     02S0330 00422000
         AL    @10,@CF01943                                     02S0330 00423000
         MVC   STATE-5(8,@10),DISABLED                          02S0330 00424000
*                           END;    /* D ALL DISABLED.               */ 00425000
*                         ELSE      /* NOT DISABLED,                 */ 00426000
*                           DO;     /* SET LENGTH BIT AND ENABLED    */ 00427000
         B     @RC00327                                         02S0332 00428000
@RF00327 DS    0H                                               02S0333 00429000
*                             ENBL=ON;/* LENGTH BIT,SUBTRACT 1       */ 00430000
         OI    ENBL,B'10000000'                                 02S0333 00431000
*                             STATE(IDS)=ENABLED;/* INDICATE TRAP IS    00432000
*                                      ENABLED                       */ 00433000
         LR    @10,IDS                                          02S0334 00434000
         MH    @10,@CH00155                                     02S0334 00435000
         L     @06,WPTR                                         02S0334 00436000
         ALR   @06,@10                                          02S0334 00437000
         AL    @06,@CF01943                                     02S0334 00438000
         MVC   STATE-5(8,@06),ENABLED                           02S0334 00439000
*                           END;    /* D ALL ENABLED.                */ 00440000
*                         IF SCEFWD=0 THEN/* SCE LAST ON CHAIN,      */ 00441000
@RC00327 L     @10,SCEFWD(,SCEPTR)                              02S0336 00442000
         LTR   @10,@10                                          02S0336 00443000
         BNZ   @RF00336                                         02S0336 00444000
*                           SCEEND=YES;/* INDICATE ALL TRAPS DONE    */ 00445000
         OI    SCEEND,B'00010000'                               02S0337 00446000
*                         ELSE      /* NOT LAST TRAP                 */ 00447000
*                           SCEPTR=SCEFWD;/* GET PTR TO NEXT TRAP    */ 00448000
         B     @RC00336                                         02S0338 00449000
@RF00336 L     SCEPTR,SCEFWD(,SCEPTR)                           02S0338 00450000
*                       END;        /* END FILL UP DATA LINE         */ 00451000
@RC00336 AL    IDS,@CF00040                                     02S0339 00452000
         C     IDS,@CF00117                                     02S0339 00453000
         BNH   @DL00325                                         02S0339 00454000
@DC00325 DS    0H                                               02S0340 00455000
*                       IF WTO1ST=NO THEN/* 1ST LINE AFTER SVC35 ?   */ 00456000
         TM    WTO1ST,B'01000000'                               02S0340 00457000
         BNZ   @RF00340                                         02S0340 00458000
*                         DO;       /* NO,DO MLWTO DATA LINE    02S0341 00459000
*                                      PROCESSING.                   */ 00460000
*                                                               02S0341 00461000
*                           /*****************************************/ 00462000
*                           /*                                       */ 00463000
*                           /* LENGTH IS NO. IDS TIMES LENGTH OF EACH*/ 00464000
*                           /* ID SLOT(IE. ID PLUS STATE) PLUS WTO   */ 00465000
*                           /* FLAGS. SUBTRACT ONE FOR THE EXTRA     */ 00466000
*                           /* BLANK AFTER THE STATE.                */ 00467000
*                           /*                                       */ 00468000
*                           /*****************************************/ 00469000
*                                                               02S0342 00470000
*                           WPLMLLEN=(IDS-1)*ENTLEN+3;/* LENGTH OF      00471000
*                                      THIS DATA LINE, #IDS*LENGTH OF   00472000
*                                      ID+STATE PLUS WTO HEADER.     */ 00473000
         L     @10,WPLPTR                                       02S0342 00474000
         LR    @06,IDS                                          02S0342 00475000
         BCTR  @06,0                                            02S0342 00476000
         MH    @06,@CH00155                                     02S0342 00477000
         AL    @06,@CF00088                                     02S0342 00478000
         STC   @06,WPLMLLEN(,@10)                               02S0342 00479000
*                           IF ENBL=YES THEN/* IF ENABLED TRAP       */ 00480000
         TM    ENBL,B'10000000'                                 02S0343 00481000
         BNO   @RF00343                                         02S0343 00482000
*                             WPLMLLEN=WPLMLLEN-1;/* DECREMENT LINE     00483000
*                                      LENGTH                        */ 00484000
         BCTR  @06,0                                            02S0344 00485000
         STC   @06,WPLMLLEN(,@10)                               02S0344 00486000
*                           ELSE                                02S0345 00487000
*                             ;     /* NOT DISABLED.                 */ 00488000
@RF00343 DS    0H                                               02S0346 00489000
*                           WPLML0=0;/* FIRST MLWTO BYTE MUST BE        00490000
*                                      ZERO.                         */ 00491000
         L     @10,WPLPTR                                       02S0346 00492000
         MVI   WPLML0(@10),X'00'                                02S0346 00493000
*                           WPLMLLTF=0;/* MAKE SURE NO EXTRA BITS ARE   00494000
*                                      ON.                           */ 00495000
