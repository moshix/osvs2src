*                           WPTR->WID=SCEID;/* GET THE ID IN MSG.    */ 00736000
         MVC   WID(4,@10),SCEID(SCEPTR)                         03S0408 00737000
*                           DO BPTR=WPTR TO WPTR+3 WHILE(BPTR->CHARCK^= 00738000
*                                 ' ');/* UPDATE POINTER PAST NON       00739000
*                                      BLANK ID PART                 */ 00740000
         LR    BPTR,@10                                         03S0409 00741000
         B     @DE00409                                         03S0409 00742000
@DL00409 CLI   CHARCK(BPTR),C' '                                03S0409 00743000
         BE    @DC00409                                         03S0409 00744000
*                           END;    /* END UPDATE PAST ID            */ 00745000
         AL    BPTR,@CF00040                                    03S0410 00746000
@DE00409 LA    @10,3                                            03S0410 00747000
         AL    @10,WPTR                                         03S0410 00748000
         CR    BPTR,@10                                         03S0410 00749000
         BNH   @DL00409                                         03S0410 00750000
@DC00409 DS    0H                                               03S0411 00751000
*                           WPTR=BPTR;/* UPDATE AFTER BLANK SCAN.    */ 00752000
         ST    BPTR,WPTR                                        03S0411 00753000
*                           WPTR->WCOMMA=COMMA;/* NEED A        03S0412 00754000
*                                      COMMA-ID=XXXX,                */ 00755000
         L     @10,WPTR                                         03S0412 00756000
         MVC   WCOMMA(1,@10),COMMA                              03S0412 00757000
*                           WPTR=WPTR+1;/* GET STATE                 */ 00758000
         AL    @10,@CF00040                                     03S0413 00759000
         ST    @10,WPTR                                         03S0413 00760000
*                           IF SCEDSABL=YES THEN/* IS THE TRAP  03S0414 00761000
*                                      DISABLED?                     */ 00762000
         TM    SCEDSABL(SCEPTR),B'10000000'                     03S0414 00763000
         BNO   @RF00414                                         03S0414 00764000
*                             DO;   /* YES, ID=XXXX,DISABLED,        */ 00765000
*                               WPTR->L1STATE=DISABLED;/* SHOW  03S0416 00766000
*                                      DISABLED.                     */ 00767000
         MVC   L1STATE(8,@10),DISABLED                          03S0416 00768000
*                               WPTR=WPTR+LENGTH(DISABLED);/* GET       00769000
*                                      ACTION PART.                  */ 00770000
         AL    @10,@CF00084                                     03S0417 00771000
         ST    @10,WPTR                                         03S0417 00772000
*                             END;  /* END DISABLED                  */ 00773000
*                           ELSE    /* ITS ENABLED                   */ 00774000
*                             DO;   /* INDICATE ENABLED              */ 00775000
         B     @RC00414                                         03S0419 00776000
@RF00414 DS    0H                                               03S0420 00777000
*                               WPTR->L1STATE=IDENABL;/*        03S0420 00778000
*                                      ID=XXXX,ENABLED,              */ 00779000
         L     @10,WPTR                                         03S0420 00780000
         MVI   L1STATE+7(@10),C' '                              03S0420 00781000
         MVC   L1STATE(7,@10),IDENABL                           03S0420 00782000
*                               WPTR=WPTR+LENGTH(IDENABL);/* GET TO     00783000
*                                      ACTION                        */ 00784000
         AL    @10,@CF00032                                     03S0421 00785000
         ST    @10,WPTR                                         03S0421 00786000
*                             END;  /* END ENABLED                   */ 00787000
*                           WPTR->WACTION=ACTIONEQ;/*           03S0423 00788000
*                                      ID=XXXX,ENABLED,ACTION=.      */ 00789000
@RC00414 L     @10,WPTR                                         03S0423 00790000
         MVC   WACTION(8,@10),ACTIONEQ                          03S0423 00791000
*                           WPTR=WPTR+LENGTH(ACTIONEQ);/* GET TO        00792000
*                                      ACTION DATA.                  */ 00793000
         AL    @10,@CF00084                                     03S0424 00794000
         ST    @10,WPTR                                         03S0424 00795000
*                           IF SCEIGNOR=YES THEN/* ACTION IGNORE?    */ 00796000
         TM    SCEIGNOR(SCEPTR),B'00000100'                     03S0425 00797000
         BNO   @RF00425                                         03S0425 00798000
*                             DO;   /* YES,                          */ 00799000
*                               WPTR->WIGNR=IGNOR;/* PUT IN     03S0427 00800000
*                                      ACTION=IGNORE                 */ 00801000
         MVC   WIGNR(6,@10),IGNOR                               03S0427 00802000
*                               WPTR=WPTR+LENGTH(IGNOR);/* INCREMENT    00803000
*                                      FOR MSGTXT LENGTH             */ 00804000
         AL    @10,@CF00119                                     03S0428 00805000
         ST    @10,WPTR                                         03S0428 00806000
*                             END;  /* END FOR IGNORE                */ 00807000
*                           ELSE    /* NOT IGNORE                    */ 00808000
*                             IF SCEWAIT=YES THEN/* IS IT WAIT?      */ 00809000
         B     @RC00425                                         03S0430 00810000
@RF00425 TM    SCEWAIT(SCEPTR),B'00001000'                      03S0430 00811000
         BNO   @RF00430                                         03S0430 00812000
*                               DO; /* YES,                          */ 00813000
*                                 WPTR->WWAIT=WAIT;/* PUT IN    03S0432 00814000
*                                      ACTION=WAIT,                  */ 00815000
         L     @10,WPTR                                         03S0432 00816000
         MVC   WWAIT(4,@10),WAIT                                03S0432 00817000
*                                 WPTR=WPTR+LENGTH(WAIT);/* UPDATE      00818000
*                                      FOR LENGTH                    */ 00819000
         AL    @10,@CF00053                                     03S0433 00820000
         ST    @10,WPTR                                         03S0433 00821000
*                               END;/* END FOR WAIT                  */ 00822000
*                             ELSE  /* NOT WAIT              @G33NPHE*/ 00823000
*                               IF SCENODMP=YES THEN/* IS IT NODUMP?    00824000
*                                                            @G33NPHE*/ 00825000
         B     @RC00430                                         03S0435 00826000
@RF00430 TM    SCENODMP(SCEPTR),B'00000010'                     03S0435 00827000
         BNO   @RF00435                                         03S0435 00828000
*                                 DO;/* YES,                 @G33NPHE*/ 00829000
*                                   WPTR->WNODMP=NODMP;/* PUT IN        00830000
*                                      ACTION=NODUMP         @G33NPHE*/ 00831000
         L     @10,WPTR                                         03S0437 00832000
         MVC   WNODMP(6,@10),NODMP                              03S0437 00833000
*                                   WPTR=WPTR+LENGTH(NODMP);/* UPDATE   00834000
*                                      FOR LENGTH            @G33NPHE*/ 00835000
         AL    @10,@CF00119                                     03S0438 00836000
         ST    @10,WPTR                                         03S0438 00837000
*                                 END;/* END FOR NODUMP      @G33NPHE*/ 00838000
*                               ELSE/* NOT IGNORE,WAIT OR NODUMP        00839000
*                                                            @G33NPHE*/ 00840000
*                                 DO;/* ITS SVCD                     */ 00841000
         B     @RC00435                                         03S0440 00842000
@RF00435 DS    0H                                               03S0441 00843000
*                                   WPTR->WSVCD=SVCD;/* ACTION-SVCD  */ 00844000
         L     @10,WPTR                                         03S0441 00845000
         MVC   WSVCD(4,@10),SVCD                                03S0441 00846000
*                                   WPTR=WPTR+LENGTH(SVCD);/* UPDATE    00847000
*                                      FOR TEXT LENGTH               */ 00848000
         AL    @10,@CF00053                                     03S0442 00849000
         ST    @10,WPTR                                         03S0442 00850000
*                                 END;/* END FOR SVCD                */ 00851000
*                           WPLMLLTB=ON;/* INDICATE TITLE LINE       */ 00852000
