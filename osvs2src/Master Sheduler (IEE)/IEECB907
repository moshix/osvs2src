         STH   @06,WPLMLLTF(,@10)                               02S0347 00497000
*                           IF SCEEND=YES THEN/* ALL SCES SEARCHED   */ 00498000
         TM    SCEEND,B'00010000'                               02S0348 00499000
         BNO   @RF00348                                         02S0348 00500000
*                             WPLMLLTD=ON;/* YES,INDICATE END LINE   */ 00501000
         OI    WPLMLLTD(@10),B'00010000'                        02S0349 00502000
*                           WPLMLLTC=ON;/* INDICATE DATA LINE        */ 00503000
@RF00348 L     @10,WPLPTR                                       02S0350 00504000
         OI    WPLMLLTC(@10),B'00100000'                        02S0350 00505000
*                           WPLPTR=WPLPTR+WPLMLLEN;/* GET TO NEXT       00506000
*                                      DATA LINE                     */ 00507000
