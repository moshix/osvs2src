         ALR   @10,@06                                          02S0803 02190000
         C     @10,@CF00152                                     02S0803 02191000
         BNH   @RF00803                                         02S0803 02192000
*     CALL MAKELINE;                /* LINE NO MAKE NEW LINE         */ 02193000
         BAL   @14,MAKELINE                                     02S0804 02194000
*   ELSE                                                        02S0805 02195000
*     ;                             /* IT WILL FIT                   */ 02196000
@RF00803 DS    0H                                               02S0806 02197000
*   SAVWPTR=WPTR;                   /* SAVE CURRENT MSG PTR          */ 02198000
         L     @10,WPTR                                         02S0806 02199000
         ST    @10,SAVWPTR                                      02S0806 02200000
*   IF NEWLINE^=YES THEN            /* NEW LINE CREATED?             */ 02201000
         TM    NEWLINE,B'00000001'                              02S0807 02202000
         BO    @RF00807                                         02S0807 02203000
*     DO;                           /* NO                            */ 02204000
*       WPTR->WCOMMA=COMMA;         /* NEED A COMMA                  */ 02205000
         MVC   WCOMMA(1,@10),COMMA                              02S0809 02206000
*       WPTR=WPTR+1;                /* UPDATE PAST COMMA             */ 02207000
         AL    @10,@CF00040                                     02S0810 02208000
         ST    @10,WPTR                                         02S0810 02209000
*     END;                          /* END OLD LINE                  */ 02210000
*   ELSE                            /* IT IS A NEW LINE.             */ 02211000
*     NEWLINE=OFF;                  /* TURN OFF NEW INDICATOR.       */ 02212000
         B     @RC00807                                         02S0812 02213000
