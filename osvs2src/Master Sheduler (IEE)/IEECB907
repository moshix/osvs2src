@RC00430 DS    0H                                               03S0444 00854000
@RC00425 L     @10,WPLPTR                                       03S0444 00855000
         OI    WPLMLLTB(@10),B'01000000'                        03S0444 00856000
*                           WPLMLLEN=WPTR-ADDR(WPLMLTXT)+4;/* LENGTH    00857000
*                                      OF TEXT + WTO FLAGS           */ 00858000
