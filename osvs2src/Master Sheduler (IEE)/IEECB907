         LA    @15,WPLMLTXT(,@04)                               03S0498 01070000
         LR    @14,@06                                          03S0498 01071000
         SLR   @14,@15                                          03S0498 01072000
         C     @14,@CF00216                                     03S0498 01073000
         BNH   @RF00498                                         03S0498 01074000
         CR    SHIFTER,@10                                      03S0498 01075000
         BE    @RF00498                                         03S0498 01076000
*                                   DO;/* A NEW LINE IF MORE MODES.  */ 01077000
*                                    WPLMLLEN=WPLMLLEN+WPTR-ADDR(       01078000
*                                        WPLMLTXT);/* GET LINE LENGTH*/ 01079000
