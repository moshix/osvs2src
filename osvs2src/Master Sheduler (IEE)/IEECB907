*                           RFY                                 03S0492 01041000
*                             R1 UNRSTD;                        03S0492 01042000
*                           DO MCTR=1 TO ALLMODES;/* DO ALL MODES    */ 01043000
         LA    MCTR,1                                           03S0493 01044000
@DL00493 DS    0H                                               03S0494 01045000
*                             IF SHIFTER<0 THEN/* IS THIS MODE  03S0494 01046000
*                                      SELECTED                      */ 01047000
         SLR   @10,@10                                          03S0494 01048000
         CR    SHIFTER,@10                                      03S0494 01049000
         BNL   @RF00494                                         03S0494 01050000
*                               DO; /* YES PUT IT IN THE LINE        */ 01051000
*                                 WPTR->WMODE=MODE(MCTR);/* GET MODE    01052000
*                                      FROM LIST.                    */ 01053000
         L     @06,WPTR                                         03S0496 01054000
         LR    @04,MCTR                                         03S0496 01055000
         MH    @04,@CH00032                                     03S0496 01056000
         LA    @01,MODE-7(@04)                                  03S0496 01057000
         MVC   WMODE(7,@06),0(@01)                              03S0496 01058000
*                                 WPTR=WPTR+MLN(MCTR);/* UPDATE MODE    01059000
*                                      LENGTH                        */ 01060000
