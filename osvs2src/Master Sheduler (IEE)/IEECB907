*                 DO;               /* THEN DO THE SUMMARY           */ 00337000
*                                                               02S0313 00338000
*/********************************************************************  00339000
*                                                               02S0313 00340000
* DSPLYALL- THIS SEGMENT CONSTRUCTS THE SUMMARY DISPLAY IN RESPONSE TO  00341000
*           OPERATOR COMMAND DISPLAY SLIP.                      02S0313 00342000
*                                                               02S0313 00343000
*  OPERATION- CLEAR THE WTO WORKAREA AND SETUP CONTROL LINE VIA 02S0313 00344000
*             INTERNAL SUBROUTINE SETCNTRL.                     02S0313 00345000
*             CONSTRUCT THE LABEL LINE                          02S0313 00346000
*             ADD UP TO 8 DATA LINES THEN ISSUE MLWTO           02S0313 00347000
*               IF 10 LINES(INCLUDING CONTROL AND LABEL) WERE   02S0313 00348000
*               WRITTEN, SETUP AND WRITE UP TO 10 MORE LINES. THIS      00349000
*               IS REPEATED UNTIL ALL SLIP TRAPS(SCES) HAVE     02S0313 00350000
*               BEEN DISPLAYED.                                 02S0313 00351000
**********************************************************************/ 00352000
*                                                               02S0313 00353000
*                   CALL SETCNTRL;  /* SETUP CNTROL LINE AND RETURN     00354000
*                                      LINPTR,SAVEIDS AND SPLPTR     */ 00355000
         BAL   @14,SETCNTRL                                     02S0313 00356000
*                   WPLMLLEN=LENGTH(LABEL)+4;/* LABEL LINE+FLAG AREA */ 00357000
         L     @10,WPLPTR                                       02S0314 00358000
         LA    @06,70                                           02S0314 00359000
         STC   @06,WPLMLLEN(,@10)                               02S0314 00360000
*                   WPLMLLTB=ON;    /* INDICATE LABEL LINE           */ 00361000
         OI    WPLMLLTB(@10),B'01000000'                        02S0315 00362000
*                   WPLMLTXT(1:LENGTH(LABEL))=LABEL;/* MOVE IN LABEL    00363000
*                                      LINE                          */ 00364000
         MVC   WPLMLTXT(66,@10),LABEL                           02S0316 00365000
*                   LINPTR->LINENUM=LINPTR->LINENUM+1;/* UPDATE # OF    00366000
*                                      LINES                         */ 00367000
         LA    @04,1                                            02S0317 00368000
         SLR   @02,@02                                          02S0317 00369000
         IC    @02,LINENUM(,LINPTR)                             02S0317 00370000
         ALR   @04,@02                                          02S0317 00371000
         STC   @04,LINENUM(,LINPTR)                             02S0317 00372000
*                   WPLPTR=WPLPTR+WPLMLLEN;/* POINT 1ST DATA LINE    */ 00373000
         ALR   @10,@06                                          02S0318 00374000
         ST    @10,WPLPTR                                       02S0318 00375000
*                   SCEPTR=SHDRFWD; /* ADDRESS LAST TRAP SET         */ 00376000
         L     @06,CVTPTR                                       02S0319 00377000
         L     @06,CVTRTMS(,@06)                                02S0319 00378000
         L     SCEPTR,SHDRFWD(,@06)                             02S0319 00379000
*                   WPTR=ADDR(WPLMLTXT);/* SETWORK POINTER.          */ 00380000
         LA    @10,WPLMLTXT(,@10)                               02S0320 00381000
         ST    @10,WPTR                                         02S0320 00382000
*                   NUMLIN=8;       /* WRITE 8 DATA LINES            */ 00383000
         LA    NUMLIN,8                                         02S0321 00384000
*                   DO WHILE(SCEEND=NO&WTOERR=NO);/* WRITE UNTIL ALL    00385000
*                                      SCES ARE SCANNED OR WTO FAILS */ 00386000
