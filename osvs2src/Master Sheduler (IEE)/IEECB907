         LCR   @06,@06                                          03S0445 00860000
         AL    @06,WPTR                                         03S0445 00861000
         AL    @06,@CF00053                                     03S0445 00862000
         STC   @06,WPLMLLEN(,@10)                               03S0445 00863000
*                                                               03S0446 00864000
*/********************************************************************  00865000
* L2ERRTYP-THIS SEGMENT DISPLAYS THE ERROR TYPE,REQUESTED ON A PREVIOUS 00866000
*          SLIP COMMAND, FOR DISPLAY SLIP=.                     03S0446 00867000
*                                                               03S0446 00868000
*   OPERATION-THIS SEGMENT IS POSITIONALLY DEPENDENT UPON BIT SETTINGS  00869000
*             IN THE SCE THAT CORRESPOND TO THE ERRTYPS CHOSEN. 03S0446 00870000
*             CHECK REGISTER FOR NEGATIVE, IF SO MOVE THE APPROPRIATE   00871000
*             ERRTYPE TO THE MESSAGE.                           03S0446 00872000
*             SHIFT AND DO ABOVE CHECK UNTIL ALL FIELDS ARE CHECKED.    00873000
*                                                               03S0446 00874000
**********************************************************************/ 00875000
*                                                               03S0446 00876000
*                           LINPTR->LINENUM=LINPTR->LINENUM+1;/*        00877000
*                                      UPDATE MLWTO LINE #.          */ 00878000
         LA    @04,1                                            03S0446 00879000
         SLR   @15,@15                                          03S0446 00880000
         IC    @15,LINENUM(,LINPTR)                             03S0446 00881000
         ALR   @04,@15                                          03S0446 00882000
         STC   @04,LINENUM(,LINPTR)                             03S0446 00883000
*                           WPLPTR=WPLPTR+WPLMLLEN;/* GET TO NEXT       00884000
*                                      SLOT IN WTO AREA.             */ 00885000
         ALR   @10,@06                                          03S0447 00886000
         ST    @10,WPLPTR                                       03S0447 00887000
*                           WPLMLTXT='';/* BLANK OUT TEXT.           */ 00888000
         MVI   WPLMLTXT(@10),C' '                               03S0448 00889000
         MVC   WPLMLTXT+1(125,@10),WPLMLTXT(@10)                03S0448 00890000
*                           WPLML0=0;/* ZERO HI ORDER BYTE OF LENGTH.*/ 00891000
         MVI   WPLML0(@10),X'00'                                03S0449 00892000
*                           WPLMLLTF=0;/* ZERO LINE TYPE FLAGS.      */ 00893000
         SLR   @06,@06                                          03S0450 00894000
         STH   @06,WPLMLLTF(,@10)                               03S0450 00895000
*                           WPTR=ADDR(WPLMLTXT);/* GET WORK POINTER. */ 00896000
         LA    @10,WPLMLTXT(,@10)                               03S0451 00897000
         ST    @10,WPTR                                         03S0451 00898000
*                           WPTR->WERRTYP=ERRTYPEQ;/* MOVE IN   03S0452 00899000
*                                      ERRORTYP=                     */ 00900000
         MVC   WERRTYP(7,@10),ERRTYPEQ                          03S0452 00901000
*                           WPTR=WPTR+LENGTH(WERRTYP);/* UPDATE TO      00902000
*                                      VARIABLE PART.                */ 00903000
         AL    @10,@CF00032                                     03S0453 00904000
         ST    @10,WPTR                                         03S0453 00905000
*                           RFY                                 03S0454 00906000
*                             SHIFTER RSTD;/* NEED A REGISTER.       */ 00907000
*                           SHIFTER=0;/* CLEAR SHIFT REG.            */ 00908000
         SLR   SHIFTER,SHIFTER                                  03S0455 00909000
*                           RFY                                 03S0456 00910000
*                             R1 RSTD;                          03S0456 00911000
*                           R1=SCEPTR;/* ICM BASE.                   */ 00912000
         LR    R1,SCEPTR                                        03S0457 00913000
*                           GEN REFS(SCERRTYP,SHIFTER)/* GET ERRTYP     00914000
*                                      FLAGS                         */ 00915000
*                               (ICM SHIFTER,8,SCERRTYP(R1));/* PUT     00916000
*                                      IN HI ORDER BYTE              */ 00917000
         ICM SHIFTER,8,SCERRTYP(R1)                                     00918000
*                           RFY                                 03S0459 00919000
*                             R1 UNRSTD;                        03S0459 00920000
*                           DO ECTR=1 TO ENUM;/* LOOP THRU TO GET       00921000
*                                      ERRTYPS                       */ 00922000
         LA    ECTR,1                                           03S0460 00923000
@DL00460 DS    0H                                               03S0461 00924000
*                             IF SHIFTER<0 THEN/* IF THE BIT IS ON      00925000
*                                      THEN                          */ 00926000
         LTR   SHIFTER,SHIFTER                                  03S0461 00927000
         BNM   @RF00461                                         03S0461 00928000
*                               DO; /* MOVE IN PROPER ERRTYPE        */ 00929000
*                                 WPTR->WETYP=ETYP(ECTR);/* GET THE     00930000
*                                      ERROR TYPE.                   */ 00931000
         L     @10,WPTR                                         03S0463 00932000
         LR    @06,ECTR                                         03S0463 00933000
         SLA   @06,3                                            03S0463 00934000
         LA    @04,ETYP-8(@06)                                  03S0463 00935000
         MVC   WETYP(8,@10),0(@04)                              03S0463 00936000
*                                 WPTR=WPTR+ELEN(ECTR);/* UPDATE MSG    00937000
*                                      TEXT PTR.                     */ 00938000
         SLR   @06,@06                                          03S0464 00939000
         IC    @06,ELEN-1(ECTR)                                 03S0464 00940000
         ALR   @10,@06                                          03S0464 00941000
         ST    @10,WPTR                                         03S0464 00942000
*                               END;/* END ERRTYP SPECIFIED.         */ 00943000
*                             ELSE                              03S0466 00944000
*                               ;   /* NO ERRTYP SPECIFIED.          */ 00945000
@RF00461 DS    0H                                               03S0467 00946000
*                             SLL(SHIFTER,1);/* GETUP FOR NEXT ERRTYP   00947000
*                                      CHK                           */ 00948000
         SLL   SHIFTER,1                                        03S0467 00949000
*                           END;    /* END ERRORTYPE CHECK LOOP      */ 00950000
         AL    ECTR,@CF00040                                    03S0468 00951000
         C     ECTR,@CF00084                                    03S0468 00952000
         BNH   @DL00460                                         03S0468 00953000
*                           RFY                                 03S0469 00954000
*                             SHIFTER UNRSTD;/* DON'T NEED REG. ANY     00955000
*                                      MORE                          */ 00956000
*                           WPLMLLEN=WPTR-ADDR(WPLMLTXT)-1+4;/* GET     00957000
*                                      THE LENGTH OF TEXT,THERE IS AN   00958000
*                                      EXTRA COMMA.                  */ 00959000
         L     @10,WPLPTR                                       03S0470 00960000
         LA    @06,WPLMLTXT(,@10)                               03S0470 00961000
         L     @04,WPTR                                         03S0470 00962000
         SLR   @04,@06                                          03S0470 00963000
         AL    @04,@CF00088                                     03S0470 00964000
         STC   @04,WPLMLLEN(,@10)                               03S0470 00965000
*                           ETXT=WPLMLLEN-4-LENGTH(WERRTYP)-1;/* GET    00966000
*                                      LENGTH OF DATA WITHOUT THE       00967000
*                                      EXTRA COMMA                   */ 00968000
         SL    @04,@CF00033                                     03S0471 00969000
         LR    ETXT,@04                                         03S0471 00970000
*                           WPTR=ADDR(WPLMLTXT)+LENGTH(WERRTYP);/*      00971000
*                                      GET PTR TO EXTRA COMMA        */ 00972000
         AL    @06,@CF00032                                     03S0472 00973000
         ST    @06,WPTR                                         03S0472 00974000
*                           WPTR->OVRLY(1:ETXT+1)=(WPTR+1)->OVRLY(1:    00975000
*                               ETXT+1);/* MOVE DATA OVER COMMA      */ 00976000
         EX    ETXT,@SM01949                                    03S0473 00977000
*                                                               03S0474 00978000
*/********************************************************************  00979000
*                                                               03S0474 00980000
* L3MODE-THIS SEGMENT WRITES THE MODE PORTION OF DISPLAY SLIP=. 03S0474 00981000
*                                                               03S0474 00982000
* OPERATION-IN ORDER TO REMAIN TABLE DRIVEN, THIS SUBROUTINE IS 03S0474 00983000
*           DEPENDENT UPON POSITIONAL PLACEMENT OF BITS WITHIN THE      00984000
*           SCE MODE BYTES.                                     03S0474 00985000
*           SAME TECHNIQUE AS ERRTYPE (IE. LOAD A REGISTER, SHIFT IT    00986000
*           THEN TEST FOR NEGATIVE.)                            03S0474 00987000
**********************************************************************/ 00988000
*                                                               03S0474 00989000
*                           CALL MAKELINE;/* GET TO NEXT MLWTO LINE  */ 00990000
         BAL   @14,MAKELINE                                     03S0474 00991000
*                           NEWLINE=OFF;/* TURN IT OFF FOR SCVA PROCS*/ 00992000
         NI    NEWLINE,B'11111110'                              03S0475 00993000
*                           WPTR->WMODEQ=MODEQ;/* PUT IN MODE=       */ 00994000
         L     @10,WPTR                                         03S0476 00995000
         MVC   WMODEQ(5,@10),MODEQ                              03S0476 00996000
*                           WPTR=WPTR+LENGTH(WMODEQ);/* GET TO  03S0477 00997000
*                                      VARIABLE AREA                 */ 00998000
         AL    @10,@CF00117                                     03S0477 00999000
         ST    @10,WPTR                                         03S0477 01000000
*                           IF SCEEVERY=YES THEN/* EVERY,            */ 01001000
         TM    SCEEVERY(SCEPTR),B'00100000'                     03S0478 01002000
         BNO   @RF00478                                         03S0478 01003000
*                             DO;   /* YES, PUT IT IN THE MSG.       */ 01004000
*                               WPTR->WEVERY=EVERY;/* NOW ITS   03S0480 01005000
*                                      'MODE=EVERY,'.                */ 01006000
         MVC   WEVERY(6,@10),EVERY                              03S0480 01007000
*                               WPTR=WPTR+LENGTH(WEVERY);/* GET TO      01008000
*                                      NEXT PART.                    */ 01009000
         AL    @10,@CF00119                                     03S0481 01010000
         ST    @10,WPTR                                         03S0481 01011000
*                             END;  /* END PUT IN EVERY.             */ 01012000
*                           ELSE    /* NOT EVERY,                    */ 01013000
*                             DO;   /* ITS ANY                       */ 01014000
         B     @RC00478                                         03S0483 01015000
@RF00478 DS    0H                                               03S0484 01016000
*                               WPTR->WANY=ANY;/* NOW ITS'MODE=ANY,' */ 01017000
         L     @10,WPTR                                         03S0484 01018000
         MVC   WANY(4,@10),ANY                                  03S0484 01019000
*                               WPTR=WPTR+LENGTH(WANY);/* GET TO NEXT   01020000
*                                      SLOT.                         */ 01021000
         AL    @10,@CF00053                                     03S0485 01022000
         ST    @10,WPTR                                         03S0485 01023000
*                             END;  /* END PUT IN ANY.               */ 01024000
*                           RFY                                 03S0487 01025000
*                             SHIFTER RSTD;/* NEED A REG.            */ 01026000
@RC00478 DS    0H                                               03S0488 01027000
*                           SHIFTER=0;                          03S0488 01028000
         SLR   SHIFTER,SHIFTER                                  03S0488 01029000
*                           RFY                                 03S0489 01030000
*                             R1 RSTD;                          03S0489 01031000
*                           R1=SCEPTR;/* ICM BASE REG.               */ 01032000
         LR    R1,SCEPTR                                        03S0490 01033000
*                           GEN REFS(SCESUPR,SCEDIS,SCEGLSP,SCEGLSD,    01034000
*                               SCELLOC,SCETYP1,SCESRB,SCETCB,SCERECV,  01035000
*                               SCEPP,SCESUPER,SCESKEY,SCEPKEY,SCEGLOC, 01036000
*                               SCELOCK,SCEMODE,SCEMODE1,SCEMODE2)      01037000
*                               (ICM   SHIFTER,12,SCEMODE(R1));/* GET   01038000
*                                      MODES FOR SHIFT               */ 01039000
