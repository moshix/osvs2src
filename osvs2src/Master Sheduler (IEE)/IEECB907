*                           WPLMCSF=WPLMCSF&&WPLMCSF;/* CLEAR UNUSED    00534000
*                                      BITS.                         */ 00535000
@RF00355 L     @10,WPLPTR                                       02S0357 00536000
         XC    WPLMCSF(2,@10),WPLMCSF(@10)                      02S0357 00537000
