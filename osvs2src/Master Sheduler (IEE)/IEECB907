         LR    @09,IDS                                          02S0354 00521000
         BCTR  @09,0                                            02S0354 00522000
         MH    @09,@CH00155                                     02S0354 00523000
         AL    @09,@CF00088                                     02S0354 00524000
         STH   @09,WPLLGH(,@10)                                 02S0354 00525000
*                           IF ENBL=YES THEN/* IF ENABLED GET RID OF    00526000
*                                      BLANK.                        */ 00527000
         TM    ENBL,B'10000000'                                 02S0355 00528000
         BNO   @RF00355                                         02S0355 00529000
*                             WPLLGH=WPLLGH-1;/* THERES AN EXTRA        00530000
*                                      BLANK.                        */ 00531000
