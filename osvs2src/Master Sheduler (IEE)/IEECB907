         IC    @06,WPLMLLEN(,@10)                               02S0351 00509000
         ALR   @06,@10                                          02S0351 00510000
         ST    @06,WPLPTR                                       02S0351 00511000
*                         END;      /* END MLWTO DATA LINE      02S0352 00512000
*                                      PROCESSING.                   */ 00513000
*                       ELSE        /* IT IS 1ST LINE AFTER SVC35.   */ 00514000
*                         DO;       /* MAKE A LINE WITH CONTROL INFO.*/ 00515000
         B     @RC00340                                         02S0353 00516000
@RF00340 DS    0H                                               02S0354 00517000
*                           WPLLGH=3+(IDS-1)*ENTLEN;/* LENGTH OF THIS   00518000
*                                      LINE.                         */ 00519000
