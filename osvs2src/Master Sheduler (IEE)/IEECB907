         IC    @10,WPLMLLEN(,@04)                               03S0500 01081000
         ALR   @06,@10                                          03S0500 01082000
         SLR   @06,@15                                          03S0500 01083000
         STC   @06,WPLMLLEN(,@04)                               03S0500 01084000
*                                    CALL MAKELINE;/* GET A NEWLINE. */ 01085000
         BAL   @14,MAKELINE                                     03S0501 01086000
*                                    WPTR=WPTR+LENGTH(MODEQ);/* START   01087000
*                                      OF TEXT                       */ 01088000
         LA    @10,5                                            03S0502 01089000
         AL    @10,WPTR                                         03S0502 01090000
         ST    @10,WPTR                                         03S0502 01091000
*                                   END;/* END NEW LINE.             */ 01092000
*                                 ELSE                          03S0504 01093000
*                                   ;/* NO NEW LINE NEEDED.          */ 01094000
@RF00498 DS    0H                                               03S0505 01095000
*                               END;/* END MODE SPECIFIED.           */ 01096000
*                             ELSE                              03S0506 01097000
*                               ;   /* NO MODE SPECIFIED.            */ 01098000
@RF00494 DS    0H                                               03S0507 01099000
*                             SLL(SHIFTER,1);/* GET TO NEXT MODE.    */ 01100000
         SLL   SHIFTER,1                                        03S0507 01101000
*                           END;    /* END ALL MODES.                */ 01102000
         AL    MCTR,@CF00040                                    03S0508 01103000
         C     MCTR,@CF00149                                    03S0508 01104000
         BNH   @DL00493                                         03S0508 01105000
*                           NEWLINE=OFF;/* TURN OFF NEW LINE    03S0509 01106000
*                                      INDICATOR                     */ 01107000
         NI    NEWLINE,B'11111110'                              03S0509 01108000
*                           RFY                                 03S0510 01109000
*                             SHIFTER UNRSTD;/* FREE UP REG.         */ 01110000
*                           WPLMLLEN=WPLMLLEN+WPTR-ADDR(WPLMLTXT)-1;    01111000
         L     @10,WPLPTR                                       03S0511 01112000
         SLR   @06,@06                                          03S0511 01113000
         IC    @06,WPLMLLEN(,@10)                               03S0511 01114000
         AL    @06,WPTR                                         03S0511 01115000
         LA    @04,WPLMLTXT(,@10)                               03S0511 01116000
         SLR   @06,@04                                          03S0511 01117000
         BCTR  @06,0                                            03S0511 01118000
         STC   @06,WPLMLLEN(,@10)                               03S0511 01119000
*                                   /* LENTH LESS COMMA.             */ 01120000
*                           WPLMLLTC=ON;/* THIS IS A DATA LINE.      */ 01121000
         OI    WPLMLLTC(@10),B'00100000'                        03S0512 01122000
*                           LINPTR->LINENUM=LINPTR->LINENUM+1;/*        01123000
*                                      UPDATE MLWTO LINE COUNT.      */ 01124000
         LA    @10,1                                            03S0513 01125000
         SLR   @06,@06                                          03S0513 01126000
         IC    @06,LINENUM(,LINPTR)                             03S0513 01127000
         ALR   @10,@06                                          03S0513 01128000
         STC   @10,LINENUM(,LINPTR)                             03S0513 01129000
*                           IF SCESCVA^=0 THEN/* ANY OPTIONAL   02S0514 01130000
