         ST    @09,WPLPTR                                       02S0359 00545000
*                           WPLLTF=WPLLTF&&WPLLTF;/* CLEAR OUT LINE     00546000
*                                      TYPE FLAGS.                   */ 00547000
         XC    WPLLTF(2,@09),WPLLTF(@09)                        02S0360 00548000
*                           WPLLTFC=ON;/* ONLY WANT DATA AND MAYBE      00549000
*                                      END.                          */ 00550000
         OI    WPLLTFC(@09),B'00100000'                         02S0361 00551000
*                           IF SCEEND=YES THEN/* ALL SCES DONE ?     */ 00552000
         TM    SCEEND,B'00010000'                               02S0362 00553000
         BNO   @RF00362                                         02S0362 00554000
*                             WPLLTFD=ON;/* YES, END THE WTO.        */ 00555000
         OI    WPLLTFD(@09),B'00010000'                         02S0363 00556000
