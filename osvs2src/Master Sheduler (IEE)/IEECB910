         LR    @09,@10                                             0360 01188000
         BCTR  @09,0                                               0360 01189000
         EX    @09,@SM02260                                        0360 01190000
*                             NEWPTR=NEWPTR+TXTLEN;/* MOVE NEWTXT TO    01191000
*                                      THE NEXT EMPTY POSITION       */ 01192000
         ALR   NEWPTR,@10                                          0361 01193000
*                           END;                                   0362 01194000
