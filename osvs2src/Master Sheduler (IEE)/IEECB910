         LR    @07,@09                                             0309 00959000
         BCTR  @07,0                                               0309 00960000
         EX    @07,@SM02260                                        0309 00961000
*                       NEWPTR=NEWPTR+TXTLEN;/* MOVE TO THE NEXT SLOT*/ 00962000
         ALR   NEWPTR,@09                                          0310 00963000
*                       IF BITREG=16|BITREG=32/* CHECK TO SEE IF THE    00964000
*                                      NAME SHOULD BE MOVED IN       */ 00965000
*                         THEN                                     0311 00966000
         CH    BITREG,@CH00037                                     0311 00967000
         BE    @RT00311                                            0311 00968000
         CH    BITREG,@CH00400                                     0311 00969000
