         OI    0(@05),B'10000000'                                  0168 00322000
*                     ELSE          /* IF THE RTCT DOES NOT SHOW THE    00323000
*                                      DATASET AVAILABLE             */ 00324000
*                       DO;         /* CHECK IT OUT DIRECTLY         */ 00325000
         B     @RC00167                                            0169 00326000
@RF00167 DS    0H                                                  0170 00327000
*                                                                  0170 00328000
*                         /*******************************************/ 00329000
*                         /*                                         */ 00330000
*                         /* DYNAMIC ALLOC OF THE SYS1.DUMP DATASET  */ 00331000
*                         /*                                         */ 00332000
*                         /*******************************************/ 00333000
*                                                                  0170 00334000
*                         S99RBPTR=ADDR(S99RB);/* INIT THE PARMLIST     00335000
*                                      TO POINT TO THE REQUEST BLOCK */ 00336000
         LA    @10,S99RB                                           0170 00337000
         ST    @10,S99RBPTR                                        0170 00338000
*                         S99RBPND=ON;/* SET THE VARIABLE LENGTH   0171 00339000
*                                      PARMLIST END FLAG             */ 00340000
         OI    S99RBPND,B'10000000'                                0171 00341000
*                         S99RB=''B;/* INIT THE REQUEST BLOCK TO   0172 00342000
*                                      ZEROS                         */ 00343000
         XC    S99RB(20),S99RB                                     0172 00344000
*                         S99RBLN=LENGTH(S99RB);/* INIT THE LENGTH 0173 00345000
*                                      FIELD                         */ 00346000
         MVI   S99RBLN,X'14'                                       0173 00347000
*                         S99VERB=S99VRBAL;/* SET THE VERB CODE TO 0174 00348000
*                                      ALLOCATE                      */ 00349000
         MVI   S99VERB,X'01'                                       0174 00350000
*                         S99TXTPP=ADDR(TEXTLIST);/* POINT TO THE  0175 00351000
*                                      TEXT UNITS PARMLIST           */ 00352000
         LA    @10,TEXTLIST                                        0175 00353000
