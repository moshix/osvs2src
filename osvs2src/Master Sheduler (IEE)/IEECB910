@RF00394 L     TXTPTR,@CA02052                                     0396 01382000
*                               NEWTXT(1:TXTLEN)=TXT(1:TXTLEN);/*  0397 01383000
*                                      MOVE IN KEYWORD               */ 01384000
@RC00394 SLR   @09,@09                                             0397 01385000
         IC    @09,TXTLEN(,TXTPTR)                                 0397 01386000
         LR    @07,@09                                             0397 01387000
         BCTR  @07,0                                               0397 01388000
         EX    @07,@SM02260                                        0397 01389000
*                               NEWPTR=NEWPTR+TXTLEN;/* MOVE NEWTXT     01390000
*                                      TO NEXT SLOT                  */ 01391000
         ALR   NEWPTR,@09                                          0398 01392000
*                               TXTPTR=TXTPTR+TXTLEN+1;/* POINT TO 0399 01393000
*                                      THE NEXT TEXT IN THE LIST     */ 01394000
         ALR   @09,TXTPTR                                          0399 01395000
         AL    @09,@CF00267                                        0399 01396000
         LR    TXTPTR,@09                                          0399 01397000
*                             END;                                 0400 01398000
*                                                                  0401 01399000
*                           /*****************************************/ 01400000
*                           /*                                       */ 01401000
*                           /* BIT INTERPRETATION. EACH BIT IN AN OPT*/ 01402000
*                           /* BYTE INDICATES A PORTION OF TEXT TO BE*/ 01403000
*                           /* ADDED TO THE LINE TO CONSTRUCT THE    */ 01404000
*                           /* TOTAL LINE. EACH BIT IS TESTED AND IF */ 01405000
*                           /* IT IS ON THE CORRESPONDING TEXT IS    */ 01406000
*                           /* MOVED INTO THE LINE. CSECT IEECB911   */ 01407000
*                           /* CONTAINS A STRING OF LENGTH&TEXT      */ 01408000
*                           /* ELEMENTS IN THE SAME ORDER AS THE     */ 01409000
*                           /* BITS.                                 */ 01410000
*                           /*                                       */ 01411000
*                           /*****************************************/ 01412000
*                                                                  0401 01413000
*                           DO WHILE(BITREG>1&TXTLEN^=0);/* TEST EACH   01414000
*                                      BIT IN THE BYTE               */ 01415000
@RF00390 B     @DE00401                                            0401 01416000
@DL00401 DS    0H                                                  0402 01417000
*                             GEN SETS(BITREG) REFS(BYTEREG,TMINST)     01418000
*                                 CODE;                            0402 01419000
         SRA   BITREG,1              SHIFT TO THE NEXT BIT POSITION     01420000
         EX    BITREG,TMINST                                            01421000
*                             BC(8,NOOPT);/* IF THE TM SHOWED THE BIT   01422000
*                                      TO BE OFF SKIP THE CODE THAT     01423000
*                                      MOVES IN THE TEXT FOR THE BIT    01424000
*                                      BEING ON                      */ 01425000
         BC    8,NOOPT                                             0403 01426000
*                             NEWTXT(1:TXTLEN)=TXT(1:TXTLEN);/* IF 0404 01427000
*                                      BIT IS ON MOVE IN ITS TEXT    */ 01428000
         SLR   @09,@09                                             0404 01429000
         IC    @09,TXTLEN(,TXTPTR)                                 0404 01430000
         LR    @07,@09                                             0404 01431000
         BCTR  @07,0                                               0404 01432000
         EX    @07,@SM02260                                        0404 01433000
*                             NEWPTR=NEWPTR+TXTLEN;/* MOVE TO THE  0405 01434000
*                                      NEXT SLOT                     */ 01435000
         ALR   NEWPTR,@09                                          0405 01436000
*NOOPT:                             /* BRANCH HERE TO AVOID ADDING 0406 01437000
*                                      TEXT TO THE LINE              */ 01438000
*                             TXTPTR=TXTPTR+TXTLEN+1;/* MOVE TO THE     01439000
*                                      TEXT FOR THE NEXT OPTION BIT  */ 01440000
NOOPT    LR    @09,TXTPTR                                          0406 01441000
         SLR   @07,@07                                             0406 01442000
         IC    @07,TXTLEN(,TXTPTR)                                 0406 01443000
         ALR   @09,@07                                             0406 01444000
         AL    @09,@CF00267                                        0406 01445000
         LR    TXTPTR,@09                                          0406 01446000
*                           END;    /* IF THE NEXT TXTLEN IS 0 OR THE   01447000
*                                      BIT MASK IN BITREG IS 1,    0407 01448000
*                                      INDICATING THE LAST BIT OF THE   01449000
*                                      BYTE HAS BEEN TESTED, END THE    01450000
*                                      LOOP                          */ 01451000
@DE00401 CH    BITREG,@CH00267                                     0407 01452000
         BNH   @DC00401                                            0407 01453000
         CLI   TXTLEN(TXTPTR),0                                    0407 01454000
         BNE   @DL00401                                            0407 01455000
@DC00401 DS    0H                                                  0408 01456000
*                           BYTEREG=BYTEREG+1;/* MOVE TO THE NEXT  0408 01457000
*                                      BYTE OF OPTIONS               */ 01458000
         AL    BYTEREG,@CF00267                                    0408 01459000
*                         END;      /* OF BIT INTERPRETATION BYTE  0409 01460000
*                                      LOOP                          */ 01461000
         AL    BYTE,@CF00267                                       0409 01462000
         CH    BYTE,@CH00280                                       0409 01463000
         BNH   @DL00388                                            0409 01464000
*                         RFY                                      0410 01465000
*                          (BITREG,                                0410 01466000
*                           BYTEREG) UNRSTD;                       0410 01467000
*                         IF(DT=SATYPE|DT=SUTYPE)/* FOR THE DUMPTYPES   01468000
*                                      THAT HAVE PDATA               */ 01469000
*                             &PDATOPTS(DT)=PDATONLY/* IF THERE WERE    01470000
*                                      NO PDATA OPTIONS              */ 01471000
*                           THEN                                   0411 01472000
         CH    DT,@CH00267                                         0411 01473000
         BE    @GL00010                                            0411 01474000
         CH    DT,@CH00272                                         0411 01475000
         BNE   @RF00411                                            0411 01476000
@GL00010 LR    @10,DT                                              0411 01477000
         SLA   @10,2                                               0411 01478000
         LA    @09,PDATOPTS-4(@10)                                 0411 01479000
         MVC   @TS00001(2),0(@09)                                  0411 01480000
