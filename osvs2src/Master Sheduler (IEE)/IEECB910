         BNZ   @RF00350                                            0350 01152000
*                       DO;                                        0351 01153000
*                         TXTPTR=ADDR(ADDNOOPT);/* GET THE TXT ADDR     01154000
*                                      IN IEECB911                   */ 01155000
         L     TXTPTR,@CA02048                                     0352 01156000
*                         NEWTXT(1:TXTLEN)=TXT(1:TXTLEN);/* MOVE IN     01157000
*                                      THE TEXT                      */ 01158000
         SLR   @10,@10                                             0353 01159000
         IC    @10,TXTLEN(,TXTPTR)                                 0353 01160000
         LR    @09,@10                                             0353 01161000
