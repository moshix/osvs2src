*                         TUP1=ADDR(TU1);/* INIT THE TEXT UNITS    0176 00355000
*                                      PARMLIST                      */ 00356000
         LA    @10,TU1                                             0176 00357000
         ST    @10,TUP1                                            0176 00358000
*                         TUP2=ADDR(TU2);                          0177 00359000
         LA    @10,TU2                                             0177 00360000
         ST    @10,TUP2                                            0177 00361000
*                         TUP3=ADDR(TU3);                          0178 00362000
         LA    @10,TU3                                             0178 00363000
         ST    @10,TUP3                                            0178 00364000
*                         TULSTEND=ON;                             0179 00365000
         OI    TULSTEND,B'10000000'                                0179 00366000
*                         TU1=TU1LIST;/* INIT THE TEXT UNITS         */ 00367000
         MVC   TU1(17),TU1LIST                                     0180 00368000
*                         TU1DSN(LENGTH(TU1DSN)-1:LENGTH(TU1DSN))= 0181 00369000
