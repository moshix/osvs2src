         LCR   @10,@10                                             0326 01032000
         ALR   @10,NEWPTR                                          0326 01033000
         STC   @10,BLDLEN                                          0326 01034000
*                     IF ENT=LASTENT/* IS THIS THE LAST LINE         */ 01035000
*                       THEN        /* IT IS THE LAST                */ 01036000
