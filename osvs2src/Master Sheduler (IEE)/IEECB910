@RT00311 DS    0H                                                  0312 00971000
*                         DO;                                      0312 00972000
*                           NEWTXT(1:LENGTH(NAME))=NAME(ENT);/* COPY    00973000
*                                      IN THE NAME FROM THE DSTABLE  */ 00974000
         LR    @09,ENT                                             0313 00975000
         MH    @09,@CH02234                                        0313 00976000
         LA    @07,NAME-105(@09)                                   0313 00977000
         MVC   NEWTXT(3,NEWPTR),0(@07)                             0313 00978000
*                           NEWPTR=NEWPTR+LENGTH(NAME);            0314 00979000
         AH    NEWPTR,@CH00582                                     0314 00980000
*                         END;                                     0315 00981000
*NOTEXT:                            /* BRANCH HERE TO AVOID ADDING 0316 00982000
*                                      TEXT TO THE LINE              */ 00983000
*                       TXTPTR=ADDR(TXT)+TXTLEN;/* MOVE TO THE TEXT     00984000
*                                      FOR THE NEXT OPTION BIT       */ 00985000
@RF00311 DS    0H                                                  0316 00986000
NOTEXT   LA    @09,TXT(,TXTPTR)                                    0316 00987000
         SLR   @07,@07                                             0316 00988000
         IC    @07,TXTLEN(,TXTPTR)                                 0316 00989000
         ALR   @09,@07                                             0316 00990000
         LR    TXTPTR,@09                                          0316 00991000
*                     END;          /* IF THE NEXT TXTLEN IS 0 OR THE   00992000
*                                      BIT MASK IN BITREG IS 1,    0317 00993000
*                                      INDICATING THE LAST BIT OF THE   00994000
*                                      BYTE HAS BEEN TESTED, END THE    00995000
*                                      LOOP                          */ 00996000
@DE00306 CH    BITREG,@CH00267                                     0317 00997000
         BH    @DL00306                                            0317 00998000
*                     RFY                                          0318 00999000
*                      (BITREG,                                    0318 01000000
*                       BYTEREG) UNRSTD;                           0318 01001000
*                     IF TITLEOK(ENT)=ON/* CHECK FOR A TITLE IN    0319 01002000
*                                      DSTABLE                       */ 01003000
*                       THEN        /* IF THERE IS ONE               */ 01004000
