         LCR   @10,@10                                             0416 01507000
         ALR   @10,NEWPTR                                          0416 01508000
         STC   @10,BLDLEN                                          0416 01509000
*                 IF DT=4           /* CHECK TO SEE IF THE MSG     0417 01510000
*                                      BUILDING IS AT AN END         */ 01511000
*                   THEN                                           0417 01512000
         CH    DT,@CH00280                                         0417 01513000
