         ST    @15,@TF00001                                        0159 00284000
         ALR   @15,@14                                             0159 00285000
         MVC   0(3,@05),RTCTDSNM-12(@15)                           0159 00286000
*                 IF RTCTDETP(ENT)=OFF/* CHECK THE DUMP DATASET TYPE */ 00287000
*                   THEN            /* FOR TAPE TYPES                */ 00288000
         AL    @14,@TF00001                                        0160 00289000
         TM    RTCTDETP-12(@14),B'00100000'                        0160 00290000
         BNZ   @RF00160                                            0160 00291000
*                   DO;                                            0161 00292000
*                     TAPE(ENT)=ON; /* INDICATE TAPE TYPE IN TABLE   */ 00293000
*                     AVAIL(ENT)=ON;/* TAPES ARE ALWAYS CONSIDERED TO   00294000
*                                      BE AVAILABLE                  */ 00295000
         LA    @05,TAPE-105(@10)                                   0163 00296000
         OI    0(@05),B'10100000'                                  0163 00297000
*                   END;                                           0164 00298000
*                 ELSE              /* IF IT IS A DASD TYPE DATASET  */ 00299000
*                   DO;                                            0165 00300000
         B     @RC00160                                            0165 00301000
@RF00160 DS    0H                                                  0166 00302000
*                     DASD(ENT)=ON; /* INDICATE DASD TYPE IN TABLE   */ 00303000
