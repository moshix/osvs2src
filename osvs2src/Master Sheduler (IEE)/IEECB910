         BNE   @RF00126                                            0126 00169000
*                   OPTIONS=YES;                                   0127 00170000
         OI    OPTIONS,B'01000000'                                 0127 00171000
*                                                                  0128 00172000
*           /*********************************************************/ 00173000
*           /*                                                       */ 00174000
*           /* TERMINATE WITH ERROR MSG IEE708I BECAUSE NO VALID     */ 00175000
*           /* COMMAND TEXT COULD BE FOUND                           */ 00176000
*           /*                                                       */ 00177000
*           /*********************************************************/ 00178000
*                                                                  0128 00179000
*                 ELSE                                             0128 00180000
*                   PARSERR=ON;     /* TERMINATE WITH AN ERROR       */ 00181000
         B     @RC00126                                            0128 00182000
@RF00126 OI    PARSERR,B'00100000'                                 0128 00183000
*         END;                      /* OF OPERAND NOT D OR DUMP      */ 00184000
@RC00126 DS    0H                                                  0129 00185000
@RC00124 DS    0H                                                  0129 00186000
@RC00122 DS    0H                                                  0129 00187000
@RC00120 DS    0H                                                  0130 00188000
*       IF PARSERR=ON               /* CHECK FOR A PARSE ERROR       */ 00189000
*         THEN                      /* IF THERE WAS ONE              */ 00190000
@RC00114 TM    PARSERR,B'00100000'                                 0130 00191000
         BNO   @RF00130                                            0130 00192000
*         DO;                                                      0131 00193000
*           RFY                                                    0132 00194000
*             R13 RSTD;                                            0132 00195000
