         EX    @09,@SM02260                                        0353 01163000
*                         NEWPTR=NEWPTR+TXTLEN;/* MOVE NEWTXT TO THE    01164000
*                                      NEXT EMPTY POSITION           */ 01165000
         ALR   NEWPTR,@10                                          0354 01166000
*                       END;                                       0355 01167000
*                     ELSE          /* IF THERE ARE OPTIONS          */ 01168000
*                       DO;         /* A LIST WILL NEED TO BE      0356 01169000
*                                      GENERATED                     */ 01170000
         B     @RC00350                                            0356 01171000
@RF00350 DS    0H                                                  0357 01172000
*                         IF OVEROPT(DT)=ON/* DETERMINE THE MODE     */ 01173000
