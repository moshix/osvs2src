*                                                                  0094 00037000
*   /*****************************************************************/ 00038000
*   /*                                                               */ 00039000
*   /* USE IEECB860 TO PROVIDE ESTAE COVERAGE. THE ESTAE AND SYSTEM  */ 00040000
*   /* RESOURCES OBTAINED BY IEECB860 WILL BE CLEANED UP BY TASK     */ 00041000
*   /* TERMINATION                                                   */ 00042000
*   /*                                                               */ 00043000
*   /*****************************************************************/ 00044000
*                                                                  0095 00045000
*   RFY                                                            0095 00046000
*    (R0,                                                          0095 00047000
*     R13) RSTD;                    /*                       @ZA25262*/ 00048000
*   STM(R0,R14,SAVEAREA);           /* SAVE REGS                     */ 00049000
         STM   R0,R14,SAVEAREA(R13)                                0096 00050000
*   DO;                             /* LOAD DCB(0) EP(NAME860)       */ 00051000
*     RESPECIFY                                                    0098 00052000
*      (GPR00P,                                                    0098 00053000
*       GPR01P) RESTRICTED;                                        0098 00054000
*     GPR00P=ADDR(NAME860);         /* LOAD ENTRY POINT NAME ADDRESS */ 00055000
