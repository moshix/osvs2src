*                           DO;                                    0358 01180000
*                             TXTPTR=ADDR(OVERMODE);/* GET THE TXT 0359 01181000
*                                      ADDR IN IEECB911              */ 01182000
         L     TXTPTR,@CA02045                                     0359 01183000
*                             NEWTXT(1:TXTLEN)=TXT(1:TXTLEN);/* MOVE    01184000
*                                      IN THE TEXT                   */ 01185000
         SLR   @10,@10                                             0360 01186000
