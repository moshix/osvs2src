         SLA   @09,2                                               0377 01277000
         LA    @07,SDATOPTS-4(@09)                                 0377 01278000
         CLC   0(2,@07),@CB02071                                   0377 01279000
         BNE   @RF00377                                            0377 01280000
*                               TXTPTR=ADDR(NOSDATA);/* POINT TO TEXT   01281000
*                                      IN IEECB911                   */ 01282000
         L     TXTPTR,@CA02051                                     0378 01283000
*                             ELSE  /* BEGIN THE OPTIONS LIST        */ 01284000
*                               TXTPTR=ADDR(SDATA);/* POINT TO TEXT     01285000
*                                      IN IEECB911                   */ 01286000
         B     @RC00377                                            0379 01287000
@RF00377 L     TXTPTR,@CA02053                                     0379 01288000
*                           END;                                   0380 01289000
*                         ELSE                                     0381 01290000
*                           IF DT=SVCTYPE/* CHECK FOR THE ONLY OTHER    01291000
*                                      TWO DUMP TYPES                */ 01292000
*                             THEN                                 0381 01293000
         B     @RC00375                                            0381 01294000
@RF00375 CH    DT,@CH00280                                         0381 01295000
         BNE   @RF00381                                            0381 01296000
*                             TXTPTR=ADDR(SVCDATA);/* SVC DUMP       */ 01297000
         L     TXTPTR,@CA02054                                     0382 01298000
*                           ELSE    /* ASSUME                        */ 01299000
*                             TXTPTR=ADDR(SMDATA);/* SYSMDUMP        */ 01300000
         B     @RC00381                                            0383 01301000
@RF00381 L     TXTPTR,@CA02044                                     0383 01302000
*                         NEWTXT(1:TXTLEN)=TXT(1:TXTLEN);/* MOVE IN     01303000
*                                      LIST BEGINNING                */ 01304000
@RC00381 DS    0H                                                  0384 01305000
@RC00375 SLR   @09,@09                                             0384 01306000
         IC    @09,TXTLEN(,TXTPTR)                                 0384 01307000
         LR    @07,@09                                             0384 01308000
         BCTR  @07,0                                               0384 01309000
         EX    @07,@SM02260                                        0384 01310000
*                         NEWPTR=NEWPTR+TXTLEN;/* MOVE NEWTXT TO NEXT   01311000
*                                      SLOT                          */ 01312000
         ALR   NEWPTR,@09                                          0385 01313000
*                         TXTPTR=TXTPTR+TXTLEN+1;/* POINT TO THE NEXT   01314000
*                                      TEXT IN THE LIST              */ 01315000
         LA    @07,1                                               0386 01316000
         ALR   @09,TXTPTR                                          0386 01317000
         ALR   @09,@07                                             0386 01318000
         LR    TXTPTR,@09                                          0386 01319000
*                         BYTEREG=ADDR(OPTWORD(DT));/* INIT THE BYTE    01320000
*                                      PTR FOR THE BIT ANALYSIS LOOP */ 01321000
         LR    BYTEREG,DT                                          0387 01322000
         SLA   BYTEREG,2                                           0387 01323000
         LA    BYTEREG,OPTWORD-4(BYTEREG)                          0387 01324000
*                                                                  0388 01325000
*                         /*******************************************/ 01326000
*                         /*                                         */ 01327000
*                         /* LOOP THROUGH THE OPTION BYTES TO BUILD  */ 01328000
*                         /* THE OPTIONS LIST                        */ 01329000
*                         /*                                         */ 01330000
*                         /*******************************************/ 01331000
*                                                                  0388 01332000
*                         DO BYTE=1 TO 4;/* DO EACH BYTE OF DUMP   0388 01333000
*                                      OITIONS                       */ 01334000
         LR    BYTE,@07                                            0388 01335000
@DL00388 DS    0H                                                  0389 01336000
*                           BITREG='00000100'X;/* INIT BIT SELECTOR     01337000
*                                      MASK FOR EX OF TM INST        */ 01338000
         LA    BITREG,256                                          0389 01339000
*                           IF(DT=SATYPE|DT=SUTYPE)/* CHECK FOR SDATA   01340000
*                                      PDATA PROCESSING              */ 01341000
*                               &BYTE=3/* SDATA TEXT ENDING AND PDATA   01342000
*                                      TEXT BEGINNING                */ 01343000
*                             THEN                                 0390 01344000
         CH    DT,@CH00267                                         0390 01345000
         BE    @GL00007                                            0390 01346000
         CH    DT,@CH00272                                         0390 01347000
         BNE   @RF00390                                            0390 01348000
@GL00007 CH    BYTE,@CH00582                                       0390 01349000
         BNE   @RF00390                                            0390 01350000
*                             DO;   /* SET UP FOR PDATA              */ 01351000
*                               IF SDATOPTS(DT)^=SDATONLY/* IF THERE    01352000
*                                      WERE SDATA OPTIONS            */ 01353000
*                                 THEN/* THEN CLOSE UP THE LIST TEXT */ 01354000
         LR    @09,DT                                              0392 01355000
         SLA   @09,2                                               0392 01356000
         LA    @07,SDATOPTS-4(@09)                                 0392 01357000
         CLC   0(2,@07),@CB02071                                   0392 01358000
         BE    @RF00392                                            0392 01359000
*                                 EVAL(NEWPTR-1)->NEWTXT(1)=')';/* 0393 01360000
*                                      OVERLAY THE LAST COMMA IN THE    01361000
*                                      LIST OF OPTIONS WITH A CLOSE     01362000
*                                      PAREN                         */ 01363000
         LR    @09,NEWPTR                                          0393 01364000
         BCTR  @09,0                                               0393 01365000
         MVI   NEWTXT(@09),C')'                                    0393 01366000
*                               IF PDATOPTS(DT)=PDATONLY/* IF THERE     01367000
*                                      ARE NO PDATA OPTIONS          */ 01368000
*                                 THEN/* THEN JUST SAY SO            */ 01369000
@RF00392 LR    @09,DT                                              0394 01370000
         SLA   @09,2                                               0394 01371000
         LA    @07,PDATOPTS-4(@09)                                 0394 01372000
         MVC   @TS00001(2),0(@07)                                  0394 01373000
         NC    @TS00001(2),@CB02263                                0394 01374000
         BNZ   @RF00394                                            0394 01375000
*                                 TXTPTR=ADDR(NOPDATA);            0395 01376000
