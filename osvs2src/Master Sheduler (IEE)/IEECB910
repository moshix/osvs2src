         LA    @01,@AL00001                                        0330 01060000
         BALR  @14,@15                                             0330 01061000
*                   END;            /* OF LOOP TO BUILD IEE856I DATA    01062000
*                                      LINES                         */ 01063000
         AL    ENT,@CF00267                                        0331 01064000
@DE00300 C     ENT,LASTENT                                         0331 01065000
         BNH   @DL00300                                            0331 01066000
*                   RFY                                            0332 01067000
*                    (NEWPTR,                                      0332 01068000
*                     TXTPTR) UNRSTD;                              0332 01069000
*                 END;              /* OF DATA LINES FOLLOWING THE 0333 01070000
*                                      LABEL LINE                    */ 01071000
*             END;                  /* OF BUILDING MSG IEE856I       */ 01072000
*                                                                  0335 01073000
*           /*********************************************************/ 01074000
*           /*                                                       */ 01075000
*           /* GENERATE MSG IEE857I DISPLAY DUMP OPTIONS DATA LINES  */ 01076000
*           /*                                                       */ 01077000
*           /*********************************************************/ 01078000
*                                                                  0335 01079000
*           ELSE                    /* ASSUME THAT OPTIONS WAS     0335 01080000
*                                      REQUESTED IF NONE OF THE OTHER   01081000
