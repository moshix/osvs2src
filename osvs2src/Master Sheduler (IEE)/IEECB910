*                         S99NOMNT=ON;/* INDICATE VOLUMES ARE NOT TO    00384000
*                                      BE MOUNTED TO SATISFY THIS  0184 00385000
*                                      REQUEST                       */ 00386000
         OI    S99NOMNT,B'00100000'                                0184 00387000
*                         RFY                                      0185 00388000
*                          (R1,                                    0185 00389000
*                           RETCODE) RSTD;                         0185 00390000
*                         R1=ADDR(S99RBPTR);/* SET UP THE PARM REG 0186 00391000
*                                      FOR SVC99                     */ 00392000
         LA    R1,S99RBPTR                                         0186 00393000
*/* MACDATE Y-2 73082                                                */ 00394000
*                                                                  0187 00395000
*                         SVC(99);                                 0187 00396000
         SVC   99                                                  0187 00397000
*                         RFY                                      0188 00398000
*                           R1 UNRSTD;                             0188 00399000
*                         IF RETCODE^=0/* CHECK FOR DYNAM ALLOC    0189 00400000
*                                      FAILURE                       */ 00401000
*                           THEN    /* IF IT DIDN'T GO               */ 00402000
         LTR   RETCODE,RETCODE                                     0189 00403000
         BZ    @RF00189                                            0189 00404000
*                           DO;                                    0190 00405000
*                             RFY                                  0191 00406000
*                               RETCODE UNRSTD;                    0191 00407000
*                             FULL(ENT)=ON;/* IF THE DATASET CANNOT     00408000
*                                      BE ALLOC TO SEE IF IT IS FULL    00409000
*                                      ASSUME THE RTCT FLAGS ARE   0192 00410000
*                                      CORRECT                       */ 00411000
         LR    @10,ENT                                             0192 00412000
