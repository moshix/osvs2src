         ST    @10,@AL00001                                        0297 00859000
         LA    @10,@CF00267                                        0297 00860000
         ST    @10,@AL00001+4                                      0297 00861000
         LA    @10,@CF00267                                        0297 00862000
         ST    @10,@AL00001+8                                      0297 00863000
         LA    @10,@CB02003                                        0297 00864000
         ST    @10,@AL00001+12                                     0297 00865000
         LA    @10,MLWTOID                                         0297 00866000
         ST    @10,@AL00001+16                                     0297 00867000
         LA    @10,WTOCONTA                                        0297 00868000
         ST    @10,@AL00001+20                                     0297 00869000
         L     @15,@CV01998                                        0297 00870000
         LA    @01,@AL00001                                        0297 00871000
         BALR  @14,@15                                             0297 00872000
*                                   /* WRITE OUT THE LINE. SEE     0298 00873000
*                                      IEECB912 FOR PARM EXPLANATION */ 00874000
*               ELSE                                               0298 00875000
*                 DO;               /* THERE WILL BE DATA LINES TO 0298 00876000
*                                      FOLLOW                        */ 00877000
         B     @RC00296                                            0298 00878000
@RF00296 DS    0H                                                  0299 00879000
*                   CALL IEECB912(BLDAREA,1,1,LABLTYPE,MLWTOID,WTOCONTA 00880000
*                       );          /* WRITE OUT THE LINE. SEE     0299 00881000
*                                      WTOCOUNT FOR PARM EXPLANATION */ 00882000
         LA    @10,BLDAREA                                         0299 00883000
         ST    @10,@AL00001                                        0299 00884000
         LA    @10,@CF00267                                        0299 00885000
         ST    @10,@AL00001+4                                      0299 00886000
         LA    @10,@CF00267                                        0299 00887000
         ST    @10,@AL00001+8                                      0299 00888000
         LA    @10,@CB00302                                        0299 00889000
         ST    @10,@AL00001+12                                     0299 00890000
         LA    @10,MLWTOID                                         0299 00891000
         ST    @10,@AL00001+16                                     0299 00892000
         LA    @10,WTOCONTA                                        0299 00893000
         ST    @10,@AL00001+20                                     0299 00894000
         L     @15,@CV01998                                        0299 00895000
         LA    @01,@AL00001                                        0299 00896000
         BALR  @14,@15                                             0299 00897000
*                                                                  0300 00898000
*                   /*************************************************/ 00899000
*                   /*                                               */ 00900000
*                   /* BUILD THE DATA LINES FOR MSG IEE856I          */ 00901000
*                   /*                                               */ 00902000
*                   /*************************************************/ 00903000
*                                                                  0300 00904000
*                   DO ENT=1 TO LASTENT;/* GENERATE A DATA LINE FOR     00905000
*                                      EACH ENTRY IN DSTABLE         */ 00906000
         LA    ENT,1                                               0300 00907000
         B     @DE00300                                            0300 00908000
@DL00300 DS    0H                                                  0301 00909000
*                     NEWPTR=ADDR(BLDLINE);                        0301 00910000
         LA    NEWPTR,BLDLINE                                      0301 00911000
*                     TXTPTR=ADDR(DATA856);/* LOCATE THE TEXT FOR THE   00912000
*                                      DATA SET LINE                 */ 00913000
*                                                                  0302 00914000
         L     TXTPTR,@CA02056                                     0302 00915000
*                     /***********************************************/ 00916000
*                     /*                                             */ 00917000
*                     /* BIT INTERPRETATION. EACH BIT IN MSGBITS     */ 00918000
*                     /* INDICATES A PORTION OF TEXT TO BE ADDED TO  */ 00919000
*                     /* THE LINE TO CONSTRUCT THE TOTAL LINE. EACH  */ 00920000
*                     /* BIT IS TESTED AND IF IT IS ON THE           */ 00921000
*                     /* CORRESPONDING TEXT IS MOVED INTO THE LINE.  */ 00922000
*                     /* CSECT IEECB911 CONTAINS A STRING OF         */ 00923000
*                     /* LENGTH&TEXT ELEMENTS IN THE SAME ORDER AS   */ 00924000
*                     /* THE BITS.                                   */ 00925000
*                     /*                                             */ 00926000
*                     /***********************************************/ 00927000
*                                                                  0303 00928000
*                     RFY                                          0303 00929000
*                      (BYTEREG,                                   0303 00930000
*                       BITREG) RSTD;                              0303 00931000
*                     BYTEREG=ADDR(MSGBITS(ENT));/* LOCATE THE MSG 0304 00932000
*                                      BITS THAT DESCRIBE THE      0304 00933000
*                                      CONDITION OF THE DATASET AND     00934000
*                                      CAN BE USED TO BUILD THE TEXT */ 00935000
         LR    BYTEREG,ENT                                         0304 00936000
         MH    BYTEREG,@CH02234                                    0304 00937000
         LA    BYTEREG,MSGBITS-105(BYTEREG)                        0304 00938000
*                     BITREG='00000100'X;/* INIT THE BIT MASK WHICH     00939000
*                                      CONTROLS WHICH BIT IN THE BYTE   00940000
*                                      POINTED TO BY BYTEREG IS TO BE   00941000
*                                      TESTED                        */ 00942000
         LA    BITREG,256                                          0305 00943000
*                     DO WHILE BITREG>1;/* TEST EACH BIT IN THE BYTE */ 00944000
