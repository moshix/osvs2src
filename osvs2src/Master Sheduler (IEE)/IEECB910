         LR    @09,@10                                             0366 01225000
         BCTR  @09,0                                               0366 01226000
         EX    @09,@SM02260                                        0366 01227000
*                               NEWPTR=NEWPTR+TXTLEN;/* MOVE NEWTXT     01228000
*                                      TO THE NEXT EMPTY POSITION    */ 01229000
         ALR   NEWPTR,@10                                          0367 01230000
*                             END;                                 0368 01231000
*                           ELSE    /* FOR DUMP TYPES WHERE THE    0369 01232000
*                                      EFFECTIVE OPTIONS MATCH THE 0369 01233000
*                                      PARMLIB OPTIONS               */ 01234000
*                             DO;                                  0369 01235000
         B     @RC00363                                            0369 01236000
@RF00363 DS    0H                                                  0370 01237000
*                               TXTPTR=ADDR(ADDPLIB);/* GET THE TXT     01238000
