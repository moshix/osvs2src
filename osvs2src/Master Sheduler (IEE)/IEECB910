         MH    @10,@CH02234                                        0319 01006000
         LA    @09,TITLEOK-105(@10)                                0319 01007000
         TM    0(@09),B'10000000'                                  0319 01008000
         BNO   @RF00319                                            0319 01009000
*                       DO;                                        0320 01010000
*                         NEWTXT(1:TXTLEN)=TXT(1:TXTLEN);/* COPY IN     01011000
*                                      TITLE HEADER WHICH WILL BE THE   01012000
*                                      TEXT POINTED TO AFTER THE BIT    01013000
*                                      LOOP IS DONE                  */ 01014000
         SLR   @09,@09                                             0321 01015000
         IC    @09,TXTLEN(,TXTPTR)                                 0321 01016000
         LR    @05,@09                                             0321 01017000
         BCTR  @05,0                                               0321 01018000
         EX    @05,@SM02260                                        0321 01019000
*                         NEWPTR=NEWPTR+TXTLEN;                    0322 01020000
         ALR   NEWPTR,@09                                          0322 01021000
