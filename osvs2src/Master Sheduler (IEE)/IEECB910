         LR    @15,R0                                              0105 00074000
         LA    @01,@AL00001                                        0105 00075000
         BALR  @14,@15                                             0105 00076000
*   LM(R0,R14,SAVEAREA);            /* RESTORE REGS                  */ 00077000
         LM    R0,R14,SAVEAREA(R13)                                0106 00078000
*   RFY                                                            0107 00079000
*    (R0,                                                          0107 00080000
*     R13) UNRSTD;                  /*                       @ZA25262*/ 00081000
*   IF RETCODE=0                    /* CHECK THE RETURN CODE FROM  0108 00082000
*                                      IEECB860                      */ 00083000
*     THEN                          /* IF IT IS OK GO AHEAD          */ 00084000
         LTR   RETCODE,RETCODE                                     0108 00085000
         BNZ   @RF00108                                            0108 00086000
*     DO;                                                          0109 00087000
*       RFY                                                        0110 00088000
*         RETCODE UNRSTD;                                          0110 00089000
*                                                                  0111 00090000
*       /*************************************************************/ 00091000
*       /*                                                           */ 00092000
*       /* PARSE THE DISPLAY COMMAND OPERAND                         */ 00093000
*       /*                                                           */ 00094000
*       /*************************************************************/ 00095000
*                                                                  0111 00096000
*       FCFLAGS=''B;                /* INIT                          */ 00097000
         MVI   FCFLAGS,X'00'                                       0111 00098000
*       RFY                                                        0112 00099000
*         OPTPTR RSTD;                                             0112 00100000
*       OPTPTR=ADDR(CHBUF);         /* BEGIN PARSE AT THE BEGINNING     00101000
*                                      OF THE OPERAND FIELD PASSED IN   00102000
*                                      THE CSCB                      */ 00103000
*                                                                  0113 00104000
         L     OPTPTR,CSCBPTR                                      0113 00105000
         LA    OPTPTR,CHBUF(,OPTPTR)                               0113 00106000
*       /*************************************************************/ 00107000
*       /*                                                           */ 00108000
*       /* THE FIRST PARM MUST HAVE BEEN ONE OF THE VALID FORMS OF   */ 00109000
*       /* DUMP OR THIS RTN WOULD NOT BE ATTACHED                    */ 00110000
*       /*                                                           */ 00111000
*       /*************************************************************/ 00112000
*                                                                  0114 00113000
*       IF OPERAND(1:5)='DUMP '|OPERAND(1:2)='D '/* CHECK FOR JUST 0114 00114000
*                                      THE FIRST POSITIONAL WITH THE    00115000
*                                      SECOND PARM DEFAULTED         */ 00116000
*         THEN                                                     0114 00117000
         CLC   OPERAND(5,OPTPTR),@CC02196                          0114 00118000
         BE    @RT00114                                            0114 00119000
         CLC   OPERAND(2,OPTPTR),@CC02197                          0114 00120000
         BNE   @RF00114                                            0114 00121000
@RT00114 DS    0H                                                  0115 00122000
*         STATUS=YES;                                              0115 00123000
         OI    STATUS,B'10000000'                                  0115 00124000
*       ELSE                        /* IF IT WASN'T DUMP WITH A    0116 00125000
*                                      DEFAULT THEN IT MUST BE     0116 00126000
*                                      FOLLOWED BY A 2ND PARM        */ 00127000
*         DO;                                                      0116 00128000
         B     @RC00114                                            0116 00129000
@RF00114 DS    0H                                                  0117 00130000
*           IF OPERAND(1:5)='DUMP,' /* DETERMINE WHICH OF THE ONLY 0117 00131000
*                                      TWO OTHER POSSIBLITIES IT IS,    00132000
