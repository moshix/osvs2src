         TM    STATUS,B'10000000'                                  0154 00257000
         BNO   @RF00154                                            0154 00258000
*             DO;                   /* GATHER STATUS DATA            */ 00259000
*               FULLCNT=0;          /* INIT COUNT OF FULL DUMP     0156 00260000
*                                      DATASETS                      */ 00261000
         SLR   @10,@10                                             0156 00262000
         ST    @10,FULLCNT                                         0156 00263000
*               DO ENT=1 BY 1       /* GATHER THE DATA FOR DSTABLE   */ 00264000
*                     WHILE(ENT<=DIM(RTCTSDDS)&RTCTDSNM(ENT)^=0);/*     00265000
*                                      CHECK TO SEE IF THIS ENTRY IN    00266000
