*       DO;                                                        0148 00289000
*                                                                  0148 00290000
         B     @RC00133                                            0148 00291000
@RF00133 DS    0H                                                  0149 00292000
*         /***********************************************************/ 00293000
*         /*                                                         */ 00294000
*         /* DETERMINE IF COMMAND WAS ONLINE OR OFFLINE AND BACKOUT  */ 00295000
*         /*                                                         */ 00296000
*         /***********************************************************/ 00297000
*                                                                  0149 00298000
*         IF ONLINE='1'B THEN       /* ONLINE COMMAND                */ 00299000
         TM    ONLINE(WORKPTR),B'00100000'                         0149 00300000
         BNO   @RF00149                                            0149 00301000
*           CALL CHONLIN;           /* CALL CHANNEL ONLINE PROCESSOR */ 00302000
         BAL   @14,CHONLIN                                         0150 00303000
