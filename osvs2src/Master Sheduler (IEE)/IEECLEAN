*CHONLIN:                                                          0172 00399000
*   PROC;                                                          0172 00400000
*                                                                  0172 00401000
CHONLIN  STM   @14,@12,@SA00002                                    0172 00402000
*/*****************************************************************  */ 00403000
*/* CHONLINE ROUTINE WILL MONITOR THE CLEANUP OPERATION OF A VARY    */ 00404000
*/* CHANNEL ONLINE OPERATION. ANY NECESSARY MESSAGES WILL BE WRITTEN */ 00405000
*/* AND IF NECESSARY WILL BACK OUT OF THE VARY OPERATION             */ 00406000
*/*****************************************************************  */ 00407000
*                                                                  0173 00408000
*   /*****************************************************************/ 00409000
*   /*                                                               */ 00410000
*   /* CHECK IF OPERATION WENT TO SUCESSFUL COMPLETION               */ 00411000
*   /*                                                               */ 00412000
*   /*****************************************************************/ 00413000
*                                                                  0173 00414000
*   IF CHONLINE^='1'B&CHNTOPER='1'B THEN/* SUCCESSFUL?               */ 00415000
         TM    CHONLINE(WORKPTR),B'00000010'                       0173 00416000
         BO    @RF00173                                            0173 00417000
         TM    CHNTOPER(WORKPTR),B'00000001'                       0173 00418000
         BNO   @RF00173                                            0173 00419000
*     CALL WTORTN(17);              /* ISSUE NOT OPERATIONAL MESSAGE */ 00420000
         LA    @01,@AL00174                                        0174 00421000
         BAL   @14,WTORTN                                          0174 00422000
*   ELSE                                                           0175 00423000
*     DO;                                                          0175 00424000
*                                                                  0175 00425000
         B     @RC00173                                            0175 00426000
@RF00173 DS    0H                                                  0176 00427000
*       /*************************************************************/ 00428000
*       /*                                                           */ 00429000
*       /* CHECK IF CHANNEL WAS ONLINE WHEN ENTERED. IF SO WRITE     */ 00430000
*       /* CHANNEL ONLINE MESSAGE AND EXIT                           */ 00431000
*       /*                                                           */ 00432000
*       /*************************************************************/ 00433000
*                                                                  0176 00434000
*       IF CHISON='1'B THEN         /* WAS CHANNEL ON?               */ 00435000
         TM    CHISON(WORKPTR),B'00001000'                         0176 00436000
         BNO   @RF00176                                            0176 00437000
*         DO;                                                      0177 00438000
*           CALL WTORTN(3);         /* ISSUE ONLINE MESSAGE          */ 00439000
         LA    @01,@AL00178                                        0178 00440000
         BAL   @14,WTORTN                                          0178 00441000
*           GOTO CHONEXIT;          /* COMPLETE. EXIT                */ 00442000
         B     CHONEXIT                                            0179 00443000
*         END;                                                     0180 00444000
*                                                                  0181 00445000
*       /*************************************************************/ 00446000
*       /*                                                           */ 00447000
*       /* CHECK FOR ERROR TRYING TO ACTIVATE CRH HARDWARE           */ 00448000
*       /*                                                           */ 00449000
*       /*************************************************************/ 00450000
*                                                                  0181 00451000
*       IF CRHFAIL='1'B THEN        /* ERROR?                @Y30CQXA*/ 00452000
@RF00176 TM    CRHFAIL(WORKPTR),B'10000000'                        0181 00453000
         BNO   @RF00181                                            0181 00454000
*         DO;                       /*                       @Y30CQXA*/ 00455000
*           CALL WTORTN(29);        /* ISSUE CRH NOT ACTIVATED     0183 00456000
*                                                            @Y30CQXA*/ 00457000
*                                                                  0183 00458000
         LA    @01,@AL00183                                        0183 00459000
         BAL   @14,WTORTN                                          0183 00460000
*           /*********************************************************/ 00461000
*           /*                                                       */ 00462000
*           /* ACTIVATION HAS FAILED, INSURE CHANNEL IS MARKED       */ 00463000
*           /* OFFLINE.                                              */ 00464000
*           /*                                                       */ 00465000
*           /*********************************************************/ 00466000
*                                                                  0184 00467000
*           PCCAPTR=PCCAT00P(HEXCPUAD+1);/* PCCA ADDRESS     @Y30CQXA*/ 00468000
         SLR   @12,@12                                             0184 00469000
         ICM   @12,3,HEXCPUAD(WORKPTR)                             0184 00470000
         SLA   @12,2                                               0184 00471000
