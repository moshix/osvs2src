*           PCCAPTR=PCCAT00P(HEXCPUAD+1);/* ADDR OF PCCA             */ 03066000
         SLR   @15,@15                                             0749 03067000
         ICM   @15,3,HEXCPUAD(WORKPTR)                             0749 03068000
         SLA   @15,2                                               0749 03069000
         L     @12,CVTPCCAT(,@12)                                  0749 03070000
         L     PCCAPTR,PCCAT00P(@15,@12)                           0749 03071000
*           CATPTR=ADDR(PCCACAT);   /* CAT TABLE ADDRESS             */ 03072000
*                                                                  0750 03073000
         LA    CATPTR,PCCACAT(,PCCAPTR)                            0750 03074000
*           /*********************************************************/ 03075000
*           /*                                                       */ 03076000
*           /* SET UP DO LOOP TO FILL IN CHANNEL ELEMENT PORTIONS OF */ 03077000
*           /* SMF RECORDS                                           */ 03078000
*           /*                                                       */ 03079000
*           /*********************************************************/ 03080000
*                                                                  0751 03081000
*           DO LOOPCNT=1 TO 16;     /* SCAN CONTROL                  */ 03082000
*                                                                  0751 03083000
         LA    LOOPCNT,1                                           0751 03084000
@DL00751 DS    0H                                                  0752 03085000
*             /*******************************************************/ 03086000
*             /*                                                     */ 03087000
*             /* USE PCCA OF SUBJECT CPU TO FILL IN RECORDS IF THIS  */ 03088000
*             /* IS A VARY CPU ONLINE THEN USE PCCA TO DETERMINE IF  */ 03089000
*             /* SMF RECORD SHOULD BE BUILT IF THIS IS A OFFLINE THEN*/ 03090000
*             /* USE COPY OF CAT TABLE IN WORK AREA TO DETERMINE IF  */ 03091000
*             /* RECORD SHOULD BE BUILT                              */ 03092000
*             /*                                                     */ 03093000
*             /*******************************************************/ 03094000
*                                                                  0752 03095000
*             IF(ONLINE='1'B&CATNOP='0'B&CATNGEN^='1'B)|(ONLINE^='1'B&  03096000
*                 CHANBIT(LOOPCNT)^='1'B) THEN/* SHOULD WE WRITE A 0752 03097000
*                                      RECORD?                       */ 03098000
         TM    ONLINE(WORKPTR),B'00100000'                         0752 03099000
         BNO   @GL00010                                            0752 03100000
         TM    CATNOP(CATPTR),B'01100000'                          0752 03101000
         BZ    @RT00752                                            0752 03102000
@GL00010 TM    ONLINE(WORKPTR),B'00100000'                         0752 03103000
         BO    @RF00752                                            0752 03104000
         LR    @12,WORKPTR                                         0752 03105000
         ALR   @12,LOOPCNT                                         0752 03106000
         TM    CHANBIT-1(@12),B'01000000'                          0752 03107000
         BO    @RF00752                                            0752 03108000
@RT00752 DS    0H                                                  0753 03109000
*               DO;                                                0753 03110000
*                 SMFRECNT=SMFRECNT+1;/* COUNT NUMBER OF ELEMENTS    */ 03111000
         AH    SMFRECNT,@CH00049                                   0754 03112000
*                 SMF22CID=2;       /* CHANNEL INDICATOR             */ 03113000
         MVI   SMF22CID(SMF22PTR),X'02'                            0755 03114000
*                 SMF22CPD=HEXCPU1; /* CPU ADDRESS OF CHANNEL        */ 03115000
         MVC   SMF22CPD(1,SMF22PTR),HEXCPU1(WORKPTR)               0756 03116000
*                 SMF22CHA=LOOPCNT-1;/* CHANNEL ADDRESS              */ 03117000
         LR    @12,LOOPCNT                                         0757 03118000
         BCTR  @12,0                                               0757 03119000
         STC   @12,SMF22CHA(,SMF22PTR)                             0757 03120000
*                 SMF22CHA=CATCHID(1:2);/* CHANNEL MODEL AND TYPE    */ 03121000
         MVC   SMF22CHA(1,SMF22PTR),CATCHID(CATPTR)                0758 03122000
*                 SMF22PTR=SMF22PTR+LENGTH(SMF22CHL);/* INCREMENT SMF   03123000
*                                      POINTER                       */ 03124000
         AL    SMF22PTR,@CF01135                                   0759 03125000
*               END;                                               0760 03126000
*             CATPTR=CATPTR+LENGTH(CAT);/* INCREMENT TO NEXT ENTRY   */ 03127000
@RF00752 AL    CATPTR,@CF00035                                     0761 03128000
*           END;                    /* END OF SCAN LOOP              */ 03129000
         AH    LOOPCNT,@CH00049                                    0762 03130000
         CH    LOOPCNT,@CH00114                                    0762 03131000
         BNH   @DL00751                                            0762 03132000
*         END;                      /* END NOT CPU,CRH VARY  @G557P2L*/ 03133000
*     END;                          /* END OF CPU PROCESSING         */ 03134000
*   ELSE                                                           0765 03135000
*     DO;                                                          0765 03136000
*                                                                  0765 03137000
         B     @RC00740                                            0765 03138000
@RF00740 DS    0H                                                  0766 03139000
*       /*************************************************************/ 03140000
*       /*                                                           */ 03141000
*       /* WAS A CHANNEL COMMAND                                     */ 03142000
*       /*                                                           */ 03143000
*       /*************************************************************/ 03144000
*                                                                  0766 03145000
*       PCCAPTR=PCCAT00P(HEXCPUAD+1);/* PCCA ADDRESS                 */ 03146000
         SLR   @12,@12                                             0766 03147000
         ICM   @12,3,HEXCPUAD(WORKPTR)                             0766 03148000
         SLA   @12,2                                               0766 03149000
         L     @01,CVTPTR                                          0766 03150000
         L     @01,CVTPCCAT(,@01)                                  0766 03151000
         L     PCCAPTR,PCCAT00P(@12,@01)                           0766 03152000
*       CATPTR=ADDR(PCCACAT)+(HEXCHNAD*8);/* CAT TABLE ENTRY         */ 03153000
*                                                                  0767 03154000
         LA    CATPTR,PCCACAT(,PCCAPTR)                            0767 03155000
         SLR   @12,@12                                             0767 03156000
         ICM   @12,3,HEXCHNAD(WORKPTR)                             0767 03157000
         LR    @15,@12                                             0767 03158000
         SLA   @15,3                                               0767 03159000
         ALR   CATPTR,@15                                          0767 03160000
*       /*************************************************************/ 03161000
*       /*                                                           */ 03162000
*       /* FILL IN SMF RECORD ENTRY                                  */ 03163000
*       /*                                                           */ 03164000
*       /*************************************************************/ 03165000
*                                                                  0768 03166000
*       SMF22CID=2;                 /* CHANNEL IDENTIFIER            */ 03167000
         MVI   SMF22CID(SMF22PTR),X'02'                            0768 03168000
*       SMF22CHN=CATCHID(1:2);      /* TYPE AND MODEL                */ 03169000
         MVC   SMF22CHN(2,SMF22PTR),CATCHID(CATPTR)                0769 03170000
*       SMF22CHA=HEXCHNAD;          /* CHANNEL ADDRESS               */ 03171000
         STC   @12,SMF22CHA(,SMF22PTR)                             0770 03172000
*       SMF22CPD=HEXCPU1;           /* CPU ADDRESS OF CHANNEL        */ 03173000
         MVC   SMF22CPD(1,SMF22PTR),HEXCPU1(WORKPTR)               0771 03174000
*     END;                                                         0772 03175000
*                                                                  0772 03176000
*   /*****************************************************************/ 03177000
*   /*                                                               */ 03178000
*   /* SMF RECORDS HAVE BEEN BUILT WRITE RECORDS RESET SMF POINTER   */ 03179000
*   /* BACK TO BEGINNING OF SMF RECORD AND FILL IN ELEMENT COUNT     */ 03180000
*   /*                                                               */ 03181000
*   /*****************************************************************/ 03182000
*                                                                  0773 03183000
*   SMF22PTR=COREPTR;               /* RESET POINTER BACK TO       0773 03184000
*                                      BEGINNING                     */ 03185000
*                                                                  0773 03186000
@RC00740 L     SMF22PTR,COREPTR(,WORKPTR)                          0773 03187000
*   /*****************************************************************/ 03188000
*   /*                                                               */ 03189000
*   /* FILL IN ELEMENT COUNT PORTION OF RECORD HEADER                */ 03190000
*   /*                                                               */ 03191000
*   /*****************************************************************/ 03192000
*                                                                  0774 03193000
*   IF CHNIND='1'B THEN             /* CHANNEL COMMAND?              */ 03194000
         TM    CHNIND(WORKPTR),B'10000000'                         0774 03195000
         BNO   @RF00774                                            0774 03196000
*     SMF22ECT=1;                   /* ONE ELEMENT FOR VARY CHANNEL  */ 03197000
*                                                                  0775 03198000
         MVC   SMF22ECT(2,SMF22PTR),@CH00049                       0775 03199000
*   /*****************************************************************/ 03200000
*   /*                                                               */ 03201000
*   /* COUNT OF ELEMENTS PLUS CPU ELEMENT                            */ 03202000
*   /*                                                               */ 03203000
*   /*****************************************************************/ 03204000
*                                                                  0776 03205000
*   ELSE                                                           0776 03206000
*     SMF22ECT=SMFRECNT+1;          /* NUMBER OF ELEMENTS COUNTED    */ 03207000
         B     @RC00774                                            0776 03208000
@RF00774 LA    @12,1                                               0776 03209000
         ALR   @12,SMFRECNT                                        0776 03210000
         STH   @12,SMF22ECT(,SMF22PTR)                             0776 03211000
*   RFY                                                            0777 03212000
*    (R1) RSTD;                     /* RESTRICT REGISTERS            */ 03213000
@RC00774 DS    0H                                                  0778 03214000
*   R1=SMF22PTR;                    /* LOAD ADDRESS OF SMF RECORD    */ 03215000
         LR    R1,SMF22PTR                                         0778 03216000
*   GEN(SMFWTM  (1));               /* WRITE SMF RECORD              */ 03217000
         SMFWTM  (1)                                                    03218000
*   RFY                                                            0780 03219000
*    (R1) UNRSTD;                   /* RELEASE REGISTERS             */ 03220000
*CHSMFEXT:                                                         0781 03221000
*   END;                                                           0781 03222000
@EL00013 DS    0H                                                  0781 03223000
@EF00013 DS    0H                                                  0781 03224000
@ER00013 LM    @14,@01,@SA00013                                    0781 03225000
         LM    @03,@12,@SA00013+16                                 0781 03226000
         BR    @14                                                 0781 03227000
*FREECORE:                                                         0782 03228000
*   PROC;                                                          0782 03229000
*                                                                  0782 03230000
FREECORE STM   @14,@12,@SA00014                                    0782 03231000
*/******* FREECORE ************************************************  */ 03232000
*/* FREECORE ROUTINE WILL FREE ANY STORAGE AREAS CHAINED TOGETHER    */ 03233000
*/* BY GETCORE. ADDITIONALLY ANY AREAS POINTED TO BY OLDPCCA AND     */ 03234000
*/* OLDLCCA WILL ALSO BE FREED.                                      */ 03235000
*/*****************************************************************  */ 03236000
*                                                                  0783 03237000
*   DO;                                                            0783 03238000
*                                                                  0783 03239000
*     /***************************************************************/ 03240000
*     /*                                                             */ 03241000
*     /* CHECK IF ANY STORAGE EXISTS CHAINED OFF OF BEGINPTR         */ 03242000
*     /*                                                             */ 03243000
*     /***************************************************************/ 03244000
*                                                                  0784 03245000
*     COREPTR=BEGINPTR;             /* ADDRESSABILITY TO FIRST BLOCK */ 03246000
         MVC   COREPTR(4,WORKPTR),BEGINPTR(WORKPTR)                0784 03247000
*     DO WHILE(COREPTR^=0);         /* LOOP TO END OF CHAIN          */ 03248000
         B     @DE00785                                            0785 03249000
@DL00785 DS    0H                                                  0786 03250000
*       RFY                                                        0786 03251000
*        (R12) RSTD;                /* RESTRICT REG 12               */ 03252000
*       SUBPOOL=SPNUM;              /* SUBPOOL TO FREE               */ 03253000
         L     @15,COREPTR(,WORKPTR)                               0787 03254000
         MVC   SUBPOOL(1,WORKPTR),SPNUM+1(@15)                     0787 03255000
*       GETLENG=LNGTH;              /* LENGTH TO FREE                */ 03256000
         MVC   GETLENG(2,WORKPTR),LNGTH(@15)                       0788 03257000
*       R12=FRWRDPTR;               /* SAVE NEXT BLOCK ADDRESS FOR 0789 03258000
*                                      FREE                          */ 03259000
         L     R12,FRWRDPTR(,@15)                                  0789 03260000
*       CALL FREEMAIN;              /* FREE STORAGE                  */ 03261000
*                                                                  0790 03262000
         BAL   @14,FREEMAIN                                        0790 03263000
*       /*************************************************************/ 03264000
*       /*                                                           */ 03265000
*       /* BLOCK HAS BEEN FREEDED. UPDATE COREPTR AND FREE NEXT BLOCK*/ 03266000
*       /*                                                           */ 03267000
*       /*************************************************************/ 03268000
*                                                                  0791 03269000
*       COREPTR=R12;                /* UPDATE TO NEXT BLOCK          */ 03270000
         ST    R12,COREPTR(,WORKPTR)                               0791 03271000
*       RFY                                                        0792 03272000
*        (R12) UNRSTD;              /* RELEASE REGISTER 12           */ 03273000
*     END;                          /* END OF FREECORE LOOP          */ 03274000
*                                                                  0793 03275000
@DE00785 SLR   @12,@12                                             0793 03276000
         C     @12,COREPTR(,WORKPTR)                               0793 03277000
         BNE   @DL00785                                            0793 03278000
*     /***************************************************************/ 03279000
*     /*                                                             */ 03280000
*     /* CHECK IF PSA EXISTS TO BE FREED BY CLEANUP                  */ 03281000
*     /*                                                             */ 03282000
*     /***************************************************************/ 03283000
*                                                                  0794 03284000
*     IF CPUPSA^=0 THEN             /* OLD PSA EXISTS                */ 03285000
         L     @15,CPUPSA(,WORKPTR)                                0794 03286000
         CR    @15,@12                                             0794 03287000
         BE    @RF00794                                            0794 03288000
*       DO;                                                        0795 03289000
*                                                                  0795 03290000
*         /***********************************************************/ 03291000
*         /*                                                         */ 03292000
*         /* ESTABLISH ADDRESSABILITY TO PSA TO FIND PCCA            */ 03293000
*         /*                                                         */ 03294000
*         /***********************************************************/ 03295000
*                                                                  0796 03296000
*         PSAPTR=CPUPSA;            /* ADDRESSABILITY TO PSA         */ 03297000
         LR    PSAPTR,@15                                          0796 03298000
*         PCCAPTR=PSAPCCAV;         /* PCCA ADDRESS                  */ 03299000
*                                                                  0797 03300000
         L     PCCAPTR,PSAPCCAV(,PSAPTR)                           0797 03301000
*         /***********************************************************/ 03302000
*         /*                                                         */ 03303000
*         /* DISCONNECT THIS PSA FROM THE PCCA FOR THIS CPU          */ 03304000
*         /*                                                         */ 03305000
*         /***********************************************************/ 03306000
*                                                                  0798 03307000
*         PCCAPSAV=0;               /* ZERO OUT PSA POINTER          */ 03308000
         ST    @12,PCCAPSAV(,PCCAPTR)                              0798 03309000
*         PCCAPSAR=0;               /* ZERO OUT PSA POINTER          */ 03310000
         ST    @12,PCCAPSAR(,PCCAPTR)                              0799 03311000
*/* CHECK FOR FRR STACKS TO BE FREED         @ZA05955                */ 03312000
*                                                                  0800 03313000
*         IF PSACSTK^=0 THEN        /* FRR STACKS EXIST?     @ZA05955*/ 03314000
         C     @12,PSACSTK(,PSAPTR)                                0800 03315000
         BE    @RF00800                                            0800 03316000
*           DO;                     /* ROUND DOWN TO DOUBLEWORD    0801 03317000
*                                      BOUNDARY TO FREE      @ZA05955*/ 03318000
*             COREPTR=(PSASSTK&'FFFFFFF8'X);/* FREE AREA ADDR      0802 03319000
*                                                            @ZA05955*/ 03320000
         L     @12,PSASSTK(,PSAPTR)                                0802 03321000
         N     @12,@CF02933                                        0802 03322000
         ST    @12,COREPTR(,WORKPTR)                               0802 03323000
*             GETLENG=YSTAKLEN;     /* LENGTH OF STACKS AREA @ZA05955*/ 03324000
         MVC   GETLENG(2,WORKPTR),@CH01615                         0803 03325000
*             SUBPOOL=245;          /* SUBPOOL OF STACKS AREA      0804 03326000
*                                                            @ZA05955*/ 03327000
         MVI   SUBPOOL(WORKPTR),X'F5'                              0804 03328000
*             CALL FREEMAIN;        /* FREE FRR STACKS AREA  @ZA05955*/ 03329000
         BAL   @14,FREEMAIN                                        0805 03330000
*           END;                    /* END FREE FRR STACKS   @ZA05955*/ 03331000
*                                                                  0806 03332000
*         /***********************************************************/ 03333000
*         /*                                                         */ 03334000
*         /* SET UP PARMS TO FREE OLD PSA                            */ 03335000
*         /*                                                         */ 03336000
*         /***********************************************************/ 03337000
*                                                                  0807 03338000
*         COREPTR=CPUPSA;           /* ADDRESS TO FREE               */ 03339000
@RF00800 MVC   COREPTR(4,WORKPTR),CPUPSA(WORKPTR)                  0807 03340000
*         GETLENG=LENGTH(PSA);      /* LENGTH TO FREE                */ 03341000
         MVC   GETLENG(2,WORKPTR),@CH03262                         0808 03342000
*         SUBPOOL=245;              /* SUBPOOL NUMBER                */ 03343000
         MVI   SUBPOOL(WORKPTR),X'F5'                              0809 03344000
*         CALL FREEMAIN;            /* FREE STORAGE                  */ 03345000
         BAL   @14,FREEMAIN                                        0810 03346000
*       END;                                                       0811 03347000
*                                                                  0811 03348000
*     /***************************************************************/ 03349000
*     /*                                                             */ 03350000
*     /* CHECK IF PCCA'S OR LCCA'S EXIST TO BE FREED AND IF SO FREE  */ 03351000
*     /*                                                             */ 03352000
*     /***************************************************************/ 03353000
*                                                                  0812 03354000
*     IF OLDPCCA^=0 THEN            /* OLD PCCA TO FREE?             */ 03355000
@RF00794 L     @12,OLDPCCA(,WORKPTR)                               0812 03356000
         LTR   @12,@12                                             0812 03357000
         BZ    @RF00812                                            0812 03358000
*       DO;                                                        0813 03359000
*         COREPTR=OLDPCCA;          /* MOVE ADDRESS TO FREE          */ 03360000
         ST    @12,COREPTR(,WORKPTR)                               0814 03361000
*         SUBPOOL=245;              /* SUBPOOL NUMBER TO FREE        */ 03362000
         MVI   SUBPOOL(WORKPTR),X'F5'                              0815 03363000
*         GETLENG=LENGTH(PCCA);     /* LENGTH TO FREE                */ 03364000
         MVC   GETLENG(2,WORKPTR),@CH03263                         0816 03365000
*         CALL FREEMAIN;            /* FREE STORAGE                  */ 03366000
         BAL   @14,FREEMAIN                                        0817 03367000
*       END;                                                       0818 03368000
*                                                                  0818 03369000
*     /***************************************************************/ 03370000
*     /*                                                             */ 03371000
*     /* CHECK IF A LCCA EXISTS THAT NEEDS TO BE FREEDED             */ 03372000
*     /*                                                             */ 03373000
*     /***************************************************************/ 03374000
*                                                                  0819 03375000
*     IF OLDLCCA^=0 THEN            /* LCCA TO FREE?                 */ 03376000
@RF00812 L     @12,OLDLCCA(,WORKPTR)                               0819 03377000
         LTR   @12,@12                                             0819 03378000
         BZ    @RF00819                                            0819 03379000
*       DO;                                                        0820 03380000
*         COREPTR=OLDLCCA;          /* ADDRESS OF LCCA TO FREE       */ 03381000
         ST    @12,COREPTR(,WORKPTR)                               0821 03382000
*         SUBPOOL=245;              /* SUBPOOL NUMBER TO FREE        */ 03383000
         MVI   SUBPOOL(WORKPTR),X'F5'                              0822 03384000
*         GETLENG=LENGTH(LCCA);     /* LENGTH TO FREE                */ 03385000
         MVC   GETLENG(2,WORKPTR),@CH03264                         0823 03386000
*         CALL FREEMAIN;            /* FREE STORAGE                  */ 03387000
         BAL   @14,FREEMAIN                                        0824 03388000
*       END;                                                       0825 03389000
*   END;                            /* END OF FREECORE PROC          */ 03390000
*   END;                                                           0827 03391000
@EL00014 DS    0H                                                  0827 03392000
@EF00014 DS    0H                                                  0827 03393000
@ER00014 LM    @14,@12,@SA00014                                    0827 03394000
         BR    @14                                                 0827 03395000
*FREEMAIN:                                                         0828 03396000
*   PROC;                                                          0828 03397000
*                                                                  0828 03398000
FREEMAIN STM   @14,@12,12(@13)                                     0828 03399000
*/*****************************************************************  */ 03400000
*/* FREEMAIN ROUTINE WILL FREE STORAGE REQUESTED BY CALLER. INPUT    */ 03401000
*/* IS COREPTR, SUBPOOL, AND GETLENG FIELDS IN WORKAREA. NO ERROR    */ 03402000
*/* INDICATORS ARE SET IN ROUTINE                                    */ 03403000
*/*****************************************************************  */ 03404000
*                                                                  0829 03405000
*   DO;                             /* FREEMAIN (E) LV(GETLENG)    0829 03406000
*                                      A(COREPTR) SP(SUBPOOL)        */ 03407000
*     RESPECIFY                                                    0830 03408000
*      (GPR01P) RESTRICTED;                                        0830 03409000
*     I005=I005&&I005;              /* INIT LIST                     */ 03410000
         XC    I005(10),I005                                       0831 03411000
*     I00505='00'X;                 /* SET TYPE                      */ 03412000
         MVI   I00505,X'00'                                        0832 03413000
*     I00502=GETLENG;               /* LENGTH                        */ 03414000
         SLR   @12,@12                                             0833 03415000
         ICM   @12,3,GETLENG(WORKPTR)                              0833 03416000
         STCM  @12,7,I00502                                        0833 03417000
*     I00504=ADDR(COREPTR);         /* ADDR OF ADDR LIST             */ 03418000
         LA    @12,COREPTR(,WORKPTR)                               0834 03419000
         STCM  @12,7,I00504                                        0834 03420000
*     I00506=SUBPOOL;               /* SUBPOOL VALUE                 */ 03421000
         MVC   I00506(1),SUBPOOL(WORKPTR)                          0835 03422000
*     GPR01P=ADDR(I005);            /* REG1 POINTS TO LIST           */ 03423000
         LA    GPR01P,I005                                         0836 03424000
*     SVC(5);                       /* STANDARD-FORM OF GETMAIN      */ 03425000
         SVC   5                                                   0837 03426000
*     RESPECIFY                                                    0838 03427000
*      (GPR01P) UNRESTRICTED;                                      0838 03428000
*   END;                            /* FREEMAIN (E) LV(GETLENG)    0839 03429000
*                                      A(COREPTR) SP(SUBPOOL)        */ 03430000
*   END;                                                           0840 03431000
@EL00015 DS    0H                                                  0840 03432000
@EF00015 DS    0H                                                  0840 03433000
@ER00015 LM    @14,@12,12(@13)                                     0840 03434000
         BR    @14                                                 0840 03435000
*GETCORE:                                                          0841 03436000
*   PROC;                                                          0841 03437000
*                                                                  0841 03438000
GETCORE  STM   @14,@12,@SA00016                                    0841 03439000
*/****************************************************************** */ 03440000
*/* GETCORE ROUTINE WILL GET AND CHAIN STORAGE REQUESTED BY CALLER   */ 03441000
*/* INPUT TO ROUTINE IS LENGTH AND SUBPOOL SPECIFIED IN WORKAREA     */ 03442000
*/* STORAGE BLOCKS WILL BE CHAINED TOGETHER AND ANCHORED OUT OF      */ 03443000
*/* WORKAREA FIELD BEGINPTR. SUBPOOL AND LENGTH IS RECORDED IN       */ 03444000
*/* CHAIN BLOCKS FOR FREEING LATER.                                  */ 03445000
*/* POSSIBLE ERROR INDICATORS ARE: NOCORE                            */ 03446000
*/****************************************************************** */ 03447000
*                                                                  0842 03448000
*   IF NOCHAIN='0'B THEN            /* CHECK FOR NOCHAIN INDICATOR   */ 03449000
*                                                                  0842 03450000
         TM    NOCHAIN(WORKPTR),B'10000000'                        0842 03451000
         BNZ   @RF00842                                            0842 03452000
*     /***************************************************************/ 03453000
*     /*                                                             */ 03454000
*     /* IF CALLER HAS REQUESTED TO CHAIN THIS BLOCK WITH ALL OTHER  */ 03455000
*     /* GETMAINS THEN ADD EIGHT TO REQUESTED LENGTH FOR CHAINING    */ 03456000
*     /* BLOCKS                                                      */ 03457000
*     /*                                                             */ 03458000
*     /***************************************************************/ 03459000
*                                                                  0843 03460000
*     GETLENG=GETLENG+8;            /* ADD EIGHT TO LENGTH           */ 03461000
         LA    @12,8                                               0843 03462000
         SLR   @15,@15                                             0843 03463000
         ICM   @15,3,GETLENG(WORKPTR)                              0843 03464000
         ALR   @12,@15                                             0843 03465000
         STH   @12,GETLENG(,WORKPTR)                               0843 03466000
*   RFY                                                            0844 03467000
*    (RETCODE) RSTD;                /* RESTRICT RETURN CODE REG      */ 03468000
*                                                                  0844 03469000
@RF00842 DS    0H                                                  0845 03470000
*   /*****************************************************************/ 03471000
*   /*                                                               */ 03472000
*   /* ISSUE CONDITIONAL GETMAIN FOR STORAGE REQUESTED               */ 03473000
*   /*                                                               */ 03474000
*   /*****************************************************************/ 03475000
*                                                                  0845 03476000
*   DO;                             /* GETMAIN (EC) LV(GETLENG)    0845 03477000
*                                      A(COREPTR) SP(SUBPOOL)      0845 03478000
*                                      RTCD(RETCODE)                 */ 03479000
*     RESPECIFY                                                    0846 03480000
*      (GPR01P,                                                    0846 03481000
*       GPR15F) RESTRICTED;                                        0846 03482000
*     I004=I004&&I004;              /* INIT LIST                     */ 03483000
         XC    I004(10),I004                                       0847 03484000
*     I00404='20'X;                 /* TYPE                          */ 03485000
