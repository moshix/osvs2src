         L     @02,CVTPCCAT(,@02)                                  0184 00473000
         L     PCCAPTR,PCCAT00P(@12,@02)                           0184 00474000
*           CATPTR=ADDR(PCCACAT)+(HEXCHNAD*8);/* CAT TABLE ADDRESS 0185 00475000
*                                                            @Y30CQXA*/ 00476000
         LA    CATPTR,PCCACAT(,PCCAPTR)                            0185 00477000
