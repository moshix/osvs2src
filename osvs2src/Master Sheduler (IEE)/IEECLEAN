*     I00405=SUBPOOL;               /* SUBPOOL VALUE                 */ 03494000
         MVC   I00405(1),SUBPOOL(WORKPTR)                          0851 03495000
*     GPR01P=ADDR(I004);            /* REG1 POINTS TO LIST           */ 03496000
         LA    GPR01P,I004                                         0852 03497000
*     SVC(4);                       /* STANDARD-FORM OF GETMAIN      */ 03498000
         SVC   4                                                   0853 03499000
*     RETCODE=GPR15F;               /* SET RETURN CODE               */ 03500000
*     RESPECIFY                                                    0855 03501000
*      (GPR01P,                                                    0855 03502000
*       GPR15F) UNRESTRICTED;                                      0855 03503000
*   END;                            /* GETMAIN (EC) LV(GETLENG)    0856 03504000
*                                      A(COREPTR) SP(SUBPOOL)      0856 03505000
*                                      RTCD(RETCODE)                 */ 03506000
*                                                                  0856 03507000
*   /*****************************************************************/ 03508000
*   /*                                                               */ 03509000
*   /* CHECK RETURN CODE AND SET NOCORE ERROR FLAG IF ERROR          */ 03510000
*   /*                                                               */ 03511000
*   /*****************************************************************/ 03512000
*                                                                  0857 03513000
*   IF RETCODE^=0 THEN              /* WAS GETMAIN SUCCESSFUL        */ 03514000
         LTR   RETCODE,RETCODE                                     0857 03515000
         BZ    @RF00857                                            0857 03516000
*     DO;                                                          0858 03517000
*       NOCORE='1'B;                /* SET ERROR FLAG                */ 03518000
         OI    NOCORE(WORKPTR),B'00000001'                         0859 03519000
*       GOTO GETCOREX;              /* RETURN TO CALLER              */ 03520000
         B     GETCOREX                                            0860 03521000
*     END;                                                         0861 03522000
*   RFY                                                            0862 03523000
*    (RETCODE) UNRSTD;                                             0862 03524000
*                                                                  0862 03525000
@RF00857 DS    0H                                                  0863 03526000
*   /*****************************************************************/ 03527000
*   /*                                                               */ 03528000
*   /* ZERO OUT STORAGE OBTAINED                                     */ 03529000
*   /*                                                               */ 03530000
*   /*****************************************************************/ 03531000
*                                                                  0863 03532000
*   RFY                                                            0863 03533000
