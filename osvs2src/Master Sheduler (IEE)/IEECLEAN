*                                                                  0133 00216000
*     IF CHNIND^='1'B THEN          /* CPU COMMAND?                  */ 00217000
@RF00128 TM    CHNIND(WORKPTR),B'10000000'                         0133 00218000
         BO    @RF00133                                            0133 00219000
*       DO;                                                        0134 00220000
*                                                                  0134 00221000
*         /***********************************************************/ 00222000
*         /*                                                         */ 00223000
*         /* WAS A VARY CPU COMMAND. DETERMINE IF A ONLINE OR OFFLINE*/ 00224000
*         /* COMMAND                                                 */ 00225000
*         /*                                                         */ 00226000
*         /***********************************************************/ 00227000
*                                                                  0135 00228000
*         IF ONLINE='1'B THEN       /* ONLINE COMMAND ?              */ 00229000
         TM    ONLINE(WORKPTR),B'00100000'                         0135 00230000
         BNO   @RF00135                                            0135 00231000
*           CALL CPUONLIN;          /* CALL ONLINE PROCESSOR         */ 00232000
         BAL   @14,CPUONLIN                                        0136 00233000
*         ELSE                                                     0137 00234000
*           CALL CPUOFFLN;          /* CALL OFFLINE PROCESSOR        */ 00235000
*                                                                  0137 00236000
         B     @RC00135                                            0137 00237000
@RF00135 BAL   @14,CPUOFFLN                                        0137 00238000
*         /***********************************************************/ 00239000
*         /*                                                         */ 00240000
*         /* SET UP DO LOOP TO DETECT SHARED DEVICES         @ZA00805*/ 00241000
*         /*                                                         */ 00242000
*         /***********************************************************/ 00243000
*                                                                  0138 00244000
*         DO UCBLKPTR=CVTILK2 BY 2 WHILE(UCBTAB^='FFFF'X);/* @ZA00805*/ 00245000
@RC00135 L     @12,CVTPTR                                          0138 00246000
         L     UCBLKPTR,CVTILK2(,@12)                              0138 00247000
         B     @DE00138                                            0138 00248000
@DL00138 DS    0H                                                  0139 00249000
*           IF UCBTAB^='0000'X THEN /* CHECK FOR DUMMY ENTRY @ZA00805*/ 00250000
         SLR   @12,@12                                             0139 00251000
         ICM   @12,3,UCBTAB(UCBLKPTR)                              0139 00252000
         LTR   @12,@12                                             0139 00253000
         BZ    @RF00139                                            0139 00254000
*             DO;                   /* CONTINUE PROCESSING IF NOT  0140 00255000
*                                      DUMMY                 @ZA00805*/ 00256000
*               RFY                                                0141 00257000
*                (R10,                                             0141 00258000
*                 R11,                                             0141 00259000
*                 R12) RSTD;        /* RESTRICT REGISTERS    @ZA00805*/ 00260000
*               R10=UCBTAB;         /* PICK UP UCB ADDRESS   @ZA00805*/ 00261000
         SLR   @03,@03                                             0142 00262000
         ICM   @03,3,UCBTAB(UCBLKPTR)                              0142 00263000
         LR    R10,@03                                             0142 00264000
*               GEN REFS(UCBOB,UCBTBYT2,UCBTBYT3,UCBSTAT,UCBSQC,UCBFLP1 00265000
*                   ,CSDCPUOL)                                     0143 00266000
*                   (IOSGEN DETECTOR,UCB=(10),REG=(11,12),VAR=2);/*     00267000
*                                                            @ZA00805*/ 00268000
         IOSGEN DETECTOR,UCB=(10),REG=(11,12),VAR=2                     00269000
*               RFY                                                0144 00270000
