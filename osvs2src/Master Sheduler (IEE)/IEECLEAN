*           GO TO CHONEXIT;         /* RETURN TO CALLER      @G47AN2O*/ 00484000
         B     CHONEXIT                                            0187 00485000
*         END;                                                     0188 00486000
*                                                                  0188 00487000
*       /*************************************************************/ 00488000
*       /*                                                           */ 00489000
*       /* CHECK IF SS/1 ASSOCIATION FAILED AND IF SO INFORM OPERATOR*/ 00490000
*       /*                                                           */ 00491000
*       /*************************************************************/ 00492000
*                                                                  0189 00493000
*       IF SS1FAIL='1'B THEN        /* ASSOCIATE FAIL?       @Y30LPXA*/ 00494000
@RF00181 TM    SS1FAIL(WORKPTR),B'00010000'                        0189 00495000
         BNO   @RF00189                                            0189 00496000
*         CALL WTORTN(30);          /* ISSUE FAILED MESSAGE  @Y30LPXA*/ 00497000
*                                                                  0190 00498000
