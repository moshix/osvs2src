*     /*                                                             */ 00314000
*     /***************************************************************/ 00315000
*                                                                  0153 00316000
*RECONEXT:                                                         0153 00317000
*     GEN(DEQ  (RCNENQQ,RCNENQR,3,SYSTEM));/* RELEASE RESOURCE       */ 00318000
@RC00133 DS    0H                                                  0153 00319000
RECONEXT DEQ  (RCNENQQ,RCNENQR,3,SYSTEM)                                00320000
*     RFY                                                          0154 00321000
*      (R1,                                                        0154 00322000
*       R10,                                                       0154 00323000
*       R11,                                                       0154 00324000
*       R12) RSTD;                  /* RESTRICT REGISTERS            */ 00325000
*                                                                  0154 00326000
*     /***************************************************************/ 00327000
*     /*                                                             */ 00328000
*     /* RELEASE FRAMES FIXED IN MAINLINE PROCESSOR                  */ 00329000
*     /*                                                             */ 00330000
*     /***************************************************************/ 00331000
*                                                                  0155 00332000
*     R10=ADDR(IEEVWKUP);           /* MAIN ENTRY ADDRESS            */ 00333000
         L     R10,@CV00085                                        0155 00334000
*     R11=ADDR(IEECLEAN);           /* END ADDRESS FOR FREE          */ 00335000
         L     R11,@CA00028                                        0156 00336000
*     R12=ADDR(PGFXECB);            /* ECB ADDRESS FOR FREE          */ 00337000
         LA    R12,PGFXECB(,WORKPTR)                               0157 00338000
*     GEN(PGFREE R,A=(10),EA=(11),ECB=(12));/* FREE FRAMES           */ 00339000
         PGFREE R,A=(10),EA=(11),ECB=(12)                               00340000
