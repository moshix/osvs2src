         BAL   @14,FREECORE                                        0165 00377000
*MAINTERM:                                                         0166 00378000
*     R1=CSCBPTR;                   /* GET CSCB ADDRESS              */ 00379000
MAINTERM L     @15,CSCBPTR(,WORKPTR)                               0166 00380000
         LR    R1,@15                                              0166 00381000
*     CHFC='1'B;                    /* SET RELEASE BIT               */ 00382000
         OI    CHFC(@15),B'00000010'                               0167 00383000
*     GEN(MGCR  (1),CHAIN);         /* FREE CSCB                     */ 00384000
         MGCR  (1),CHAIN                                                00385000
*     RETURN;                       /* RETURN TO MAINLINE            */ 00386000
@EL00001 L     @13,4(,@13)                                         0169 00387000
@EF00001 L     @00,@SIZDATD                                        0169 00388000
         LR    @01,@07                                             0169 00389000
         FREEMAIN R,LV=(0),A=(1)                                        00390000
@ER00001 LM    @14,@12,12(@13)                                     0169 00391000
         BR    @14                                                 0169 00392000
*     RFY                                                          0170 00393000
*      (R1,                                                        0170 00394000
