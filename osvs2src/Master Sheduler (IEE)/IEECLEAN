         BR    @14                                                 0271 00867000
*BACKOUT:                                                          0272 00868000
*   PROC;                                                          0272 00869000
*                                                                  0272 00870000
BACKOUT  STM   @14,@12,@SA00005                                    0272 00871000
*/****************************************************************** */ 00872000
*/* BACKOUT ROUTINE WILL BACK OUT OF A VARY CPU OR VARY CHANNEL      */ 00873000
*/* OFFLINE COMMAND. CHANNELS WILL BE RETURNED TO THERE FORMER STATUS*/ 00874000
*/* AND ANY DEVICES THAT HAVE BEEN TAKEN OFFLINE WILL BE BROUGHT BACK*/ 00875000
*/* ONLINE.                                                          */ 00876000
*/****************************************************************** */ 00877000
*                                                                  0273 00878000
*   /*****************************************************************/ 00879000
*   /*                                                               */ 00880000
*   /* CHECK IF ANY RECORDER BLOCKS WERE BUILT. THIS WILL INDICATE IF*/ 00881000
*   /* ANY DEVICES WERE TAKEN OFFLINE                                */ 00882000
*   /*                                                               */ 00883000
*   /*****************************************************************/ 00884000
*                                                                  0273 00885000
*   IF ENTRYPTR=0 THEN              /* ANY RECORDER BLOCKS?          */ 00886000
         ICM   @12,15,ENTRYPTR(WORKPTR)                            0273 00887000
         BZ    @RT00273                                            0273 00888000
*     GOTO BACKSKIP;                /* SKIP DEVICE PROCESSING        */ 00889000
*                                                                  0274 00890000
*   /*****************************************************************/ 00891000
*   /*                                                               */ 00892000
*   /* RECORDER BLOCKS EXIST BRING DEVICES BACK ONLINE IF NECESSARY  */ 00893000
*   /* CHECK IF UCB ENQ IS HELD AND IF NOT ENQ AGAIN                 */ 00894000
*   /*                                                               */ 00895000
*   /*****************************************************************/ 00896000
*                                                                  0275 00897000
*   IF UCBENQ^='1'B THEN            /* DO WE HAVE UCB RESOURCE ?     */ 00898000
         TM    UCBENQ(WORKPTR),B'00000010'                         0275 00899000
         BO    @RF00275                                            0275 00900000
*     GEN(ENQ  (UCBENQQ,UCBENQR,S,2,SYSTEM));/* ENQ                  */ 00901000
         ENQ  (UCBENQQ,UCBENQR,S,2,SYSTEM)                              00902000
*   UCBENQ='1'B;                    /* SET FOOTPRINT                 */ 00903000
@RF00275 OI    UCBENQ(WORKPTR),B'00000010'                         0277 00904000
*   CALL CNSLBACK;                  /* BACK OUT CONSOLES IF NECESSARY*/ 00905000
*                                                                  0278 00906000
         BAL   @14,CNSLBACK                                        0278 00907000
*   /*****************************************************************/ 00908000
*   /*                                                               */ 00909000
*   /* CHECK IF ANY UCB'S HAVE BEEN ALTERED AND IF SO BRING DEVICES  */ 00910000
*   /* BACK ONLINE                                                   */ 00911000
*   /*                                                               */ 00912000
*   /*****************************************************************/ 00913000
*                                                                  0279 00914000
*   IF UCBALTER='1'B THEN           /* UCB'S BEEN ALTERED?           */ 00915000
         TM    UCBALTER(WORKPTR),B'10000000'                       0279 00916000
         BNO   @RF00279                                            0279 00917000
*     DO;                                                          0280 00918000
*                                                                  0280 00919000
*       /*************************************************************/ 00920000
*       /*                                                           */ 00921000
*       /* SET UP DO LOOP TO SCAN ALL RECORDER BLOCKS AND MARK       */ 00922000
*       /* DEVICES BACK ONLINE                                       */ 00923000
*       /*                                                           */ 00924000
*       /*************************************************************/ 00925000
*                                                                  0281 00926000
*       RCRDRPTR=ENTRYPTR;          /* ADDRESSABILITY TO FIRST     0281 00927000
*                                      RECORDER BLOCK                */ 00928000
         L     RCRDRPTR,ENTRYPTR(,WORKPTR)                         0281 00929000
*       DO WHILE(RCRDRPTR^=0);      /* SCAN ALL RECORDER BLOCKS      */ 00930000
*                                                                  0282 00931000
         B     @DE00282                                            0282 00932000
@DL00282 DS    0H                                                  0283 00933000
*         /***********************************************************/ 00934000
*         /*                                                         */ 00935000
*         /* RESET HIER REASON INDICATOR IN UCB IF NOT A CONSOLE     */ 00936000
*         /* ENTRY AND MARK UCB ONLINE FOR DEVOFF ENTRIES            */ 00937000
*         /*                                                         */ 00938000
*         /***********************************************************/ 00939000
*                                                                  0283 00940000
*         UCBPTR=UCBADDR;           /* ADDRESSABILITY FOR UCB        */ 00941000
         SLR   UCBPTR,UCBPTR                                       0283 00942000
         ICM   UCBPTR,7,UCBADDR(RCRDRPTR)                          0283 00943000
*         IF HIERONLY='1'B|MKDEVOFF='1'B THEN/* HIER ONLY ENTRY?     */ 00944000
         TM    HIERONLY(RCRDRPTR),B'10010000'                      0284 00945000
         BZ    @RF00284                                            0284 00946000
*           UCBVHRSN='0'B;          /* RESET HIERARCHY REASON      0285 00947000
*                                      INDICATOR                     */ 00948000
*                                                                  0285 00949000
         NI    UCBVHRSN(UCBPTR),B'11111101'                        0285 00950000
*         /***********************************************************/ 00951000
*         /*                                                         */ 00952000
*         /* CHECK FOR DEVOFF ENTRY AND IF SO MARK DEVICE BACK ONLINE*/ 00953000
*         /*                                                         */ 00954000
*         /***********************************************************/ 00955000
*                                                                  0286 00956000
*         IF MKDEVOFF='1'B THEN     /* DEVICE TAKEN OFFLINE?         */ 00957000
@RF00284 TM    MKDEVOFF(RCRDRPTR),B'00010000'                      0286 00958000
         BNO   @RF00286                                            0286 00959000
*           UCBONLI='1'B;           /* MARK DEVICE BACK ONLINE       */ 00960000
         OI    UCBONLI(UCBPTR),B'10000000'                         0287 00961000
*         RCRDRPTR=NEXTPTR;         /* ADDRESSABILITY FOR NEXT BLOCK */ 00962000
@RF00286 L     RCRDRPTR,NEXTPTR(,RCRDRPTR)                         0288 00963000
*       END;                        /* END OF DO LOOP                */ 00964000
@DE00282 LTR   RCRDRPTR,RCRDRPTR                                   0289 00965000
         BNZ   @DL00282                                            0289 00966000
*     END;                          /* END OF RECORDER BLOCK       0290 00967000
*                                      PROCESSING                    */ 00968000
*                                                                  0290 00969000
*   /*****************************************************************/ 00970000
*   /*                                                               */ 00971000
*   /* ALL DEVICES HAVE BEEN BROUGHT BACK ONLINE. NOW CHECK IF       */ 00972000
*   /* CHANNELS HAVE BEEN MARKED OFFLINE                             */ 00973000
*   /*                                                               */ 00974000
*   /*****************************************************************/ 00975000
*                                                                  0291 00976000
*BACKSKIP:                                                         0291 00977000
*   IF CHNLSMRK='1'B THEN           /* HAVE CHANNELS BEEN MARKED?    */ 00978000
@RF00279 DS    0H                                                  0291 00979000
BACKSKIP TM    CHNLSMRK(WORKPTR),B'00100000'                       0291 00980000
         BNO   @RF00291                                            0291 00981000
*     DO;                                                          0292 00982000
*       PCCAPTR=PCCAT00P(HEXCPUAD+1);/* SET UP PCCA POINTER          */ 00983000
*                                                                  0293 00984000
         SLR   @12,@12                                             0293 00985000
         ICM   @12,3,HEXCPUAD(WORKPTR)                             0293 00986000
         SLA   @12,2                                               0293 00987000
         L     @01,CVTPTR                                          0293 00988000
         L     @01,CVTPCCAT(,@01)                                  0293 00989000
         L     PCCAPTR,PCCAT00P(@12,@01)                           0293 00990000
*       /*************************************************************/ 00991000
*       /*                                                           */ 00992000
*       /* CHANNELS HAVE BEEN MARKED OFFLINE. IF THIS IS A VARY CPU  */ 00993000
*       /* COMMAND THEN MARK CHANNELS BACK ONLINE FROM COPY OF CAT   */ 00994000
*       /* TABLE IN WORK AREA. ELSE JUST MARK THE SINGLE CHANNEL ON  */ 00995000
*       /*                                                           */ 00996000
*       /*************************************************************/ 00997000
*                                                                  0294 00998000
*       IF CHNIND='1'B THEN         /* CHANNEL COMMAND?              */ 00999000
         TM    CHNIND(WORKPTR),B'10000000'                         0294 01000000
         BNO   @RF00294                                            0294 01001000
*         DO;                                                      0295 01002000
*                                                                  0295 01003000
*           /*********************************************************/ 01004000
*           /*                                                       */ 01005000
*           /* INDEX INTO CAT TABLE AND MARK ONLY ONE CHANNEL ONLINE */ 01006000
*           /*                                                       */ 01007000
*           /*********************************************************/ 01008000
*                                                                  0296 01009000
*           CATPTR=ADDR(PCCACAT)+(8*HEXCHNAD);/* SET UP CAT POINTER  */ 01010000
         LA    CATPTR,PCCACAT(,PCCAPTR)                            0296 01011000
         SLR   @12,@12                                             0296 01012000
         ICM   @12,3,HEXCHNAD(WORKPTR)                             0296 01013000
         SLA   @12,3                                               0296 01014000
         ALR   CATPTR,@12                                          0296 01015000
*           CATNOP='0'B;            /* RESET OFFLINE INDICATOR       */ 01016000
         NI    CATNOP(CATPTR),B'10111111'                          0297 01017000
*           PCCAIO=HASIO;           /* CPU STILL HAS         @G47AN2O   01018000
*                                      I/O ABILITY           @G47AN2O*/ 01019000
         OI    PCCAIO(PCCAPTR),B'01000000'                         0298 01020000
*         END;                                                     0299 01021000
*       ELSE                                                       0300 01022000
*                                                                  0300 01023000
*         /***********************************************************/ 01024000
*         /*                                                         */ 01025000
*         /* VARY CPU COMMAND. CHECK IF A COPY OF THE CAT IS         */ 01026000
*         /* AVAILABLE AND IF SO SET ALL CHANNELS TO THERE ORIGINAL  */ 01027000
*         /* CONDITION                                               */ 01028000
*         /*                                                         */ 01029000
*         /***********************************************************/ 01030000
*                                                                  0300 01031000
*         IF COPYCAT='1'B THEN      /* CAT AVAILABLE?                */ 01032000
         B     @RC00294                                            0300 01033000
@RF00294 TM    COPYCAT(WORKPTR),B'00000010'                        0300 01034000
         BNO   @RF00300                                            0300 01035000
*           DO;                                                    0301 01036000
*             CATPTR=ADDR(PCCACAT); /* ADDRESSABILITY TO CAT TABLE   */ 01037000
         LA    CATPTR,PCCACAT(,PCCAPTR)                            0302 01038000
*             PCCAIO=NOIO;          /* CPU TENTATIVELY       @G47AN2O   01039000
*                                      HAS NO IO ABILITY     @G47AN2O*/ 01040000
         NI    PCCAIO(PCCAPTR),B'10111111'                         0303 01041000
*             DO LOOPCNT=1 TO 16;   /* SCAN ALL CHANNEL ENTRIES      */ 01042000
*                                                                  0304 01043000
         LA    LOOPCNT,1                                           0304 01044000
@DL00304 DS    0H                                                  0305 01045000
*               /*****************************************************/ 01046000
*               /*                                                   */ 01047000
*               /* CHECK IF THIS CHANNEL WAS ORIGINALLY OFFLINE OR   */ 01048000
*               /* NOT                                               */ 01049000
*               /*                                                   */ 01050000
*               /*****************************************************/ 01051000
*                                                                  0305 01052000
*               IF CHANBIT(LOOPCNT)='0'B/* WAS CHANNEL OFFLINE?      */ 01053000
*                 THEN                                             0305 01054000
         LR    @12,WORKPTR                                         0305 01055000
         ALR   @12,LOOPCNT                                         0305 01056000
         TM    CHANBIT-1(@12),B'01000000'                          0305 01057000
         BNZ   @RF00305                                            0305 01058000
*                 DO;               /*                       @G47AN2O*/ 01059000
*                   CATNOP='0'B;    /* RESET OFFLINE BIT             */ 01060000
         NI    CATNOP(CATPTR),B'10111111'                          0307 01061000
*                   PCCAIO=HASIO;   /* CPU STILL HAS         @G47AN2O   01062000
*                                      I/O CAPABILITY        @G47AN2O*/ 01063000
         OI    PCCAIO(PCCAPTR),B'01000000'                         0308 01064000
*                 END;              /*                       @G47AN2O*/ 01065000
*               CATPTR=CATPTR+8;    /* STEP TO NEXT CAT ENTRY        */ 01066000
@RF00305 AH    CATPTR,@CH00035                                     0310 01067000
*             END;                  /* END OF CHANNEL RESTORE LOOP   */ 01068000
         AH    LOOPCNT,@CH00049                                    0311 01069000
         CH    LOOPCNT,@CH00114                                    0311 01070000
         BNH   @DL00304                                            0311 01071000
*           END;                    /* END OF VARY CPU CHANNEL     0312 01072000
*                                      PROCESSING                    */ 01073000
*     END;                          /* END OF CHANNEL MARK PROCESSING*/ 01074000
*                                                                  0313 01075000
@RF00300 DS    0H                                                  0313 01076000
@RC00294 DS    0H                                                  0314 01077000
*   /*****************************************************************/ 01078000
*   /*                                                               */ 01079000
*   /* ALL CHANNELS ARE NOW ONLINE. RESET ACR INDICATOR IN LCCA OF   */ 01080000
*   /* SUBJECT CPU                                                   */ 01081000
*   /*                                                               */ 01082000
*   /*****************************************************************/ 01083000
*                                                                  0314 01084000
*   LCCAPTR=LCCAT00P(HEXCPUAD+1);   /* ADDRESSIBILITY FOR SUBJECT CPU*/ 01085000
@RF00291 SLR   @12,@12                                             0314 01086000
         ICM   @12,3,HEXCPUAD(WORKPTR)                             0314 01087000
         SLA   @12,2                                               0314 01088000
         L     @01,CVTPTR                                          0314 01089000
         L     @01,CVTLCCAT(,@01)                                  0314 01090000
         L     LCCAPTR,LCCAT00P(@12,@01)                           0314 01091000
*   LCCAVARY='0'B;                  /* RESET ACR FLAG BIT            */ 01092000
*                                                                  0315 01093000
         NI    LCCAVARY(LCCAPTR),B'11111110'                       0315 01094000
*   /*****************************************************************/ 01095000
*   /*                                                               */ 01096000
*   /* RELEASE UCB RESOURCE IF HELD                                  */ 01097000
*   /*                                                               */ 01098000
*   /*****************************************************************/ 01099000
*                                                                  0316 01100000
*   IF UCBENQ='1'B THEN             /* DO WE HAVE UCB RESOURCE?      */ 01101000
         TM    UCBENQ(WORKPTR),B'00000010'                         0316 01102000
         BNO   @RF00316                                            0316 01103000
*     GEN(DEQ  (UCBENQQ,UCBENQR,2,SYSTEM));/* DEQ UCB'S              */ 01104000
         DEQ  (UCBENQQ,UCBENQR,2,SYSTEM)                                01105000
*   UCBENQ='0'B;                    /* RESET FOOTPRINT               */ 01106000
*                                                                  0318 01107000
@RF00316 NI    UCBENQ(WORKPTR),B'11111101'                         0318 01108000
*   /*****************************************************************/ 01109000
*   /*                                                               */ 01110000
*   /* CHECK IF TIMER SUPERVISOR HAS BEEN NOTIFIED THAT A CPU IS     */ 01111000
*   /* BEING REMOVED AND IF SO CANCEL THE REQUEST                    */ 01112000
*   /*                                                               */ 01113000
*   /*****************************************************************/ 01114000
*                                                                  0319 01115000
*   IF CLOCKSW='1'B THEN            /* HAS TIMER EXIT BEEN TAKEN?    */ 01116000
         TM    CLOCKSW(WORKPTR),B'00000100'                        0319 01117000
         BNO   @RF00319                                            0319 01118000
*     DO;                                                          0320 01119000
*       RFY                                                        0321 01120000
*        (R1) RSTD;                 /* RESTRICT INTERFACE REG        */ 01121000
*       R1=ADDR(PCCAT00P(HEXCPUAD+1));/* ADDRESS OF PCCA POINTER     */ 01122000
         SLR   @12,@12                                             0322 01123000
         ICM   @12,3,HEXCPUAD(WORKPTR)                             0322 01124000
         SLA   @12,2                                               0322 01125000
         L     @15,CVTPTR                                          0322 01126000
         L     R1,CVTPCCAT(,@15)                                   0322 01127000
         LA    R1,PCCAT00P(@12,R1)                                 0322 01128000
*       GEN(LINK  EP=IEAVRCAN);     /* CANCEL TIMER SWITCH           */ 01129000
         LINK  EP=IEAVRCAN                                              01130000
*       RFY                                                        0324 01131000
*        (R1) UNRSTD;               /* RELEASE REG ONE               */ 01132000
*     END;                          /* *** END OF BACKOUT PROCESSING */ 01133000
*   END;                            /* ** END OF PROC                */ 01134000
@EL00005 DS    0H                                                  0326 01135000
@EF00005 DS    0H                                                  0326 01136000
@ER00005 LM    @14,@12,@SA00005                                    0326 01137000
         BR    @14                                                 0326 01138000
*CNSLBACK:                                                         0327 01139000
*   PROC;                                                          0327 01140000
*                                                                  0327 01141000
CNSLBACK STM   @14,@12,12(@13)                                     0327 01142000
*/*****************************************************************  */ 01143000
*/* CNSLBACK ROUTINE WILL DETERMINE IF A EXIT HAS BEEN TAKEN TO      */ 01144000
*/* CONSOLE SWITCH AND IF SO ISSUE INTERNAL VARY COMMANDS TO         */ 01145000
*/* RESTORE ALL CONSOLES TO CONSOLE STATUS. THE ROUT AND DESCRIPTOR  */ 01146000
*/* FLAGS, AS WELL AS CONSOLE AUTHORITY SHOULD REMAIN AS IT WAS      */ 01147000
*/* BEFORE THE VARY CPU/CHANNEL COMMAND.                             */ 01148000
*/*****************************************************************  */ 01149000
*                                                                  0328 01150000
*   /*****************************************************************/ 01151000
*   /*                                                               */ 01152000
*   /* CHECK IF ANY CONSOLES REQUIRED SWITCHING                      */ 01153000
*   /*                                                               */ 01154000
*   /*****************************************************************/ 01155000
*                                                                  0328 01156000
*   IF CNSLSW='1'B THEN             /* ANY CONSOLES FOUND?           */ 01157000
         TM    CNSLSW(WORKPTR),B'00000001'                         0328 01158000
         BNO   @RF00328                                            0328 01159000
*     DO;                                                          0329 01160000
*                                                                  0329 01161000
*       /*************************************************************/ 01162000
*       /*                                                           */ 01163000
*       /* SET UP DO LOOP TO SCAN ALL DEVICES THAT HAVE BEEN RECORDED*/ 01164000
*       /* BY UCBRCRDR ROUTINE BRING BACK ONLINE AS CONSOLES ALL     */ 01165000
*       /* CONSOLES THAT HAVE BEEN MARKED OFFLINE BECAUSE OF THE     */ 01166000
*       /* VARY.                                                     */ 01167000
*       /*                                                           */ 01168000
*       /*************************************************************/ 01169000
*                                                                  0330 01170000
*       RCRDRPTR=ENTRYPTR;          /* POINTER TO FIRST RECORDER   0330 01171000
*                                      BLOCK                 @ZA01563*/ 01172000
         L     RCRDRPTR,ENTRYPTR(,WORKPTR)                         0330 01173000
*       DO WHILE(RCRDRPTR^=0);      /* LOOP TO END OF CHAIN          */ 01174000
*                                                                  0331 01175000
         B     @DE00331                                            0331 01176000
@DL00331 DS    0H                                                  0332 01177000
*         /***********************************************************/ 01178000
*         /*                                                         */ 01179000
*         /* CHECK IF THIS ENTRY REPRESENTS A CONSOLE                */ 01180000
*         /*                                                         */ 01181000
*         /***********************************************************/ 01182000
*                                                                  0332 01183000
*         IF CONSLFLG='1'B THEN     /* CONSOLE?                      */ 01184000
         TM    CONSLFLG(RCRDRPTR),B'01000000'                      0332 01185000
         BNO   @RF00332                                            0332 01186000
*           DO;                                                    0333 01187000
*             UCBPTR=UCBADDR;       /* UCB ADDRESS                   */ 01188000
         SLR   UCBPTR,UCBPTR                                       0334 01189000
         ICM   UCBPTR,7,UCBADDR(RCRDRPTR)                          0334 01190000
*             UCBVHRSN='0'B;        /* RESET CONSOLE SWITCH FLAG     */ 01191000
*                                                                  0335 01192000
         NI    UCBVHRSN(UCBPTR),B'11111101'                        0335 01193000
*             /*******************************************************/ 01194000
*             /*                                                     */ 01195000
*             /* IF CONSOLE EXIT HAS BEEN TAKEN, THEN ISSUE INTERNAL */ 01196000
*             /* VARY                                                */ 01197000
*             /*                                                     */ 01198000
*             /*******************************************************/ 01199000
*                                                                  0336 01200000
*             IF CNSLEXIT='1'B THEN /* CONSOLE EXIT TAKEN?           */ 01201000
         TM    CNSLEXIT(WORKPTR),B'00000001'                       0336 01202000
         BNO   @RF00336                                            0336 01203000
*               DO;                                                0337 01204000
*                                                                  0337 01205000
*                 /***************************************************/ 01206000
*                 /*                                                 */ 01207000
*                 /* SET UP PARAMETER TO ISSUE AN INTERNAL VARY      */ 01208000
*                 /* CONSOLE COMMAND                                 */ 01209000
*                 /*                                                 */ 01210000
*                 /***************************************************/ 01211000
*                                                                  0338 01212000
*                 VCONCMD=VARYCON;  /* MOVE COMMAND TO BUFFER        */ 01213000
         MVC   VCONCMD(38),VARYCON                                 0338 01214000
*                 VCONDEV=UCBNAME;  /* MOVE CONSOLE ADDRESS TO TEXT  */ 01215000
         MVC   VCONDEV(3),UCBNAME(UCBPTR)                          0339 01216000
*                 RFY                                              0340 01217000
*                  (R0,                                            0340 01218000
*                   R1) RSTD;       /* RESTRICT REGS FOR INTERFACE   */ 01219000
*                 R0=0;             /* ZERO OUT REGISTER             */ 01220000
         SLR   R0,R0                                               0341 01221000
*                 R1=ADDR(VCONCMD); /* POINT TO COMMAND              */ 01222000
         LA    R1,VCONCMD                                          0342 01223000
*                 SVC(34);          /* ISSUE COMMAND                 */ 01224000
         SVC   34                                                  0343 01225000
*                 RFY                                              0344 01226000
*                  (R0,                                            0344 01227000
*                   R1) UNRSTD;     /* RELEASE REGS                  */ 01228000
*               END;                /* END OF RESTORE                */ 01229000
*           END;                                                   0346 01230000
@RF00336 DS    0H                                                  0347 01231000
*         RCRDRPTR=NEXTPTR;         /* ADDRESSABILITY TO NEXT BLOCK  */ 01232000
@RF00332 L     RCRDRPTR,NEXTPTR(,RCRDRPTR)                         0347 01233000
*       END;                        /* END OF DO LOOP                */ 01234000
@DE00331 LTR   RCRDRPTR,RCRDRPTR                                   0348 01235000
         BNZ   @DL00331                                            0348 01236000
*     END;                          /* END OF CONSOLE SWITCH       0349 01237000
*                                      PROCESSING                    */ 01238000
*CONSEXIT:                                                         0350 01239000
*   END;                            /* END OF PROC                   */ 01240000
@EL00006 DS    0H                                                  0350 01241000
@EF00006 DS    0H                                                  0350 01242000
@ER00006 LM    @14,@12,12(@13)                                     0350 01243000
         BR    @14                                                 0350 01244000
*CPUONLIN:                                                         0351 01245000
*   PROC;                                                          0351 01246000
*                                                                  0351 01247000
CPUONLIN STM   @14,@12,@SA00007                                    0351 01248000
*/*****************************************************************  */ 01249000
*/* CPUONLIN ROUTINE WILL MONITOR THE VARY CPU ONLINE PROCESSOR AND  */ 01250000
*/* ISSUE ANY MESSAGES THAT MAY BE NEEDED. IF NECESSARY BACKOUT      */ 01251000
*/* PROCESSING WILL BE PERFORMED.                                    */ 01252000
*/*****************************************************************  */ 01253000
*                                                                  0352 01254000
*   /*****************************************************************/ 01255000
*   /*                                                               */ 01256000
*   /* CHECK IF CPU WAS ONLINE WHEN VARY ONLINE COMMAND WAS ISSUED IF*/ 01257000
*   /* SO. CPU MAY HAVE PREVIOUSLY BEEN MARKED UNAVAILABLE FOR JOB   */ 01258000
*   /* SCHEDULING. SET CPU AVAILABLE FOR JOB SCHED. IN CSD AND ISSUE */ 01259000
*   /* CPU ONLINE MESSAGE                                            */ 01260000
*   /*                                                               */ 01261000
*   /*****************************************************************/ 01262000
*                                                                  0352 01263000
*   IF CPUISON='1'B THEN            /* WAS CPU ALREADY ONLINE?       */ 01264000
         TM    CPUISON(WORKPTR),B'00010000'                        0352 01265000
         BNO   @RF00352                                            0352 01266000
*     DO;                                                          0353 01267000
*       JOBSHED='1'B;               /* SET BIT FOR JOBSCHED ROUTINE  */ 01268000
         OI    JOBSHED(WORKPTR),B'00000001'                        0354 01269000
*       CALL JOBSCHED;              /* GO MARK CPU AVAILABLE         */ 01270000
         BAL   @14,JOBSCHED                                        0355 01271000
*       IF SKIPWTO='1'B THEN        /* UNEXPECTED CPU STATUS CHANGE?    01272000
*                                                            @ZA10796*/ 01273000
         TM    SKIPWTO(WORKPTR),B'00000100'                        0356 01274000
         BO    @RT00356                                            0356 01275000
*         GOTO CPUONXT;             /* YES, BR TO EXIT ROUTINE     0357 01276000
*                                                            @ZA10796*/ 01277000
*       CALL WTORTN(4);             /* ISSUE CPU ONLINE MESSAGE      */ 01278000
         LA    @01,@AL00358                                        0358 01279000
         BAL   @14,WTORTN                                          0358 01280000
*       GOTO CPUONXT;               /* PROCESSING IS COMPLETE LEAVE  */ 01281000
         B     CPUONXT                                             0359 01282000
*     END;                                                         0360 01283000
*                                                                  0360 01284000
*   /*****************************************************************/ 01285000
*   /*                                                               */ 01286000
*   /* CHECK IF CPU IS ONLINE SUCCESSFUL AND WRITE SMF RECORDS       */ 01287000
*   /*                                                               */ 01288000
*   /*****************************************************************/ 01289000
*                                                                  0361 01290000
*   IF CPUONOK='1'B THEN            /* CPU ONLINE SUCCESSFUL ?       */ 01291000
@RF00352 TM    CPUONOK(WORKPTR),B'01000000'                        0361 01292000
         BNO   @RF00361                                            0361 01293000
*     DO;                                                          0362 01294000
*       JOBSHED='1'B;               /* SET JOB SCHEDULING FOOTPRINT  */ 01295000
         OI    JOBSHED(WORKPTR),B'00000001'                        0363 01296000
*       CALL JOBSCHED;              /* RESET JOB SCHEDULING BIT IN 0364 01297000
*                                      CSD                           */ 01298000
*                                                                  0364 01299000
         BAL   @14,JOBSCHED                                        0364 01300000
*       /*************************************************************/ 01301000
*       /*                                                           */ 01302000
*       /* INSURE THAT CPU HAS BEEN RELEASED FROM TIMER SYNC LOOP    */ 01303000
*       /*                                                           */ 01304000
*       /*************************************************************/ 01305000
*                                                                  0365 01306000
*       LCCAPTR=LCCAT00P(HEXCPUAD+1);/* POINT TO LCCA OF CPU @Y30CQXA*/ 01307000
         SLR   @12,@12                                             0365 01308000
         ICM   @12,3,HEXCPUAD(WORKPTR)                             0365 01309000
         SLA   @12,2                                               0365 01310000
         L     @01,CVTPTR                                          0365 01311000
         L     @01,CVTLCCAT(,@01)                                  0365 01312000
         L     LCCAPTR,LCCAT00P(@12,@01)                           0365 01313000
*       LCCATIMR='0'B;              /* RESET TIMER SPIN      @Y30CQXA*/ 01314000
*                                                                  0366 01315000
         NI    LCCATIMR(LCCAPTR),B'11101111'                       0366 01316000
*       /*************************************************************/ 01317000
*       /*                                                           */ 01318000
*       /* CHECK IF CPU ONLINE PROCESSING IS COMPLETE AND IF SO ISSUE*/ 01319000
*       /* SMF RECORDS                                               */ 01320000
*       /*                                                           */ 01321000
*       /*************************************************************/ 01322000
*                                                                  0367 01323000
*       IF ONLINMSG='1'B THEN       /* ONLINE PROCESSING COMPLETE?   */ 01324000
         TM    ONLINMSG(WORKPTR),B'00100000'                       0367 01325000
         BNO   @RF00367                                            0367 01326000
*         CALL BACKOFF;             /* GO ISSUE SMF RECORDS          */ 01327000
*                                                                  0368 01328000
         BAL   @14,BACKOFF                                         0368 01329000
*       /*************************************************************/ 01330000
*       /*                                                           */ 01331000
*       /* CHECK IF SS/1 ASSOCIATION WAS SUCESSFUL, IF NOT THEN      */ 01332000
*       /* INFORM OPERATOR OF FAILURE                                */ 01333000
*       /*                                                           */ 01334000
*       /*************************************************************/ 01335000
*                                                                  0369 01336000
*       IF SS1FAIL='1'B THEN        /* SS/1 ASSOC FAIL?      @Y30LPXA*/ 01337000
@RF00367 TM    SS1FAIL(WORKPTR),B'00010000'                        0369 01338000
         BNO   @RF00369                                            0369 01339000
