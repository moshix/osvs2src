         TITLE 'IEECLEAN CPU/CHANNEL CLEANUP AND ESTAE ROUTINE         *00001000
                        '                                               00002000
IEECLEAN CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(19)                                             0001 00008000
         DC    C'IEECLEANR0300077125'                              0001 00009000
WTORT    DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00954-WTORT)                                      00013000
         ENTRY WTORT                                                    00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @08,0                                               0001 00017000
@PSTART  LA    @09,4095(,@08)                                      0001 00018000
         USING @PSTART,@08                                         0001 00019000
         USING @PSTART+4095,@09                                    0001 00020000
         L     @00,@SIZDATD                                        0001 00021000
         GETMAIN  R,LV=(0)                                              00022000
         LR    @07,@01                                             0001 00023000
         USING @DATD,@07                                           0001 00024000
         ST    @13,@SA00001+4                                      0001 00025000
         LM    @15,@01,16(@13)                                     0001 00026000
         ST    @07,8(,@13)                                         0001 00027000
         LR    @13,@07                                             0001 00028000
         AH    @15,4(,@15)                                         0001 00029000
         BR    @15                                                 0001 00030000
@EP00001 MVC   @PC00001(4),0(@01)                                  0001 00031000
*                                      (MAX 126 CHARS)               */ 00032000
*/* ************************* END OF WPL *****************************/ 00033000
