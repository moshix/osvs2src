*   LCCAVCPU='0'B;                  /* RESET DISPATCHER HOOK         */ 02568000
*                                                                  0661 02569000
         NI    LCCAVCPU(LCCAPTR),B'10111111'                       0661 02570000
*   /*****************************************************************/ 02571000
*   /*                                                               */ 02572000
*   /* WAIT ONE SECOND TO INSURE THAT DISPATCHER HAS NOT SEEN HOOK   */ 02573000
*   /*                                                               */ 02574000
*   /*****************************************************************/ 02575000
*                                                                  0662 02576000
*   GEN(STIMER  WAIT,BINTVL=SEC1);  /* WAIT ONE SECOND               */ 02577000
*                                                                  0662 02578000
         STIMER  WAIT,BINTVL=SEC1                                       02579000
*   /*****************************************************************/ 02580000
*   /*                                                               */ 02581000
*   /* CHECK IF CPU IS STILL ALIVE IN CSDCPUAL MASK AND IF SO SET    */ 02582000
*   /* ERROR FLAG TO CAUSE A BACKOUT OF THE VARY                     */ 02583000
*   /*                                                               */ 02584000
*   /*****************************************************************/ 02585000
*                                                                  0663 02586000
*   IF(CSDCPUAL&CPUMSK)^=0 THEN     /* CPU STILL ALIVE?              */ 02587000
         L     @12,CVTPTR                                          0663 02588000
