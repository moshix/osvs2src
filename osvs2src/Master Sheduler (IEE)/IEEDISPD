         TITLE 'DISPLAY DOMAIN PROCESSOR - IEEDISPD                    *00001000
                        '                                               00002000
IEEDISPD CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(24)                                             0001 00007000
         DC    C'IEEDISPD 78024  UZ16178 '                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         LR    @03,@01                                             0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         LR    @01,@03                                             0001 00018000
         ST    @13,@SA00001+4                                      0001 00019000
         LR    @13,@11                                             0001 00020000
         XC    @ZTEMPS(@ZLEN),@ZTEMPS                                   00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
*                                                                  0031 00023000
*   /*****************************************************************/ 00024000
*   /*                                                               */ 00025000
*   /* WHEN THIS MODULE IS ATTACHED,REGISTER 1 POINTS TO THE DISPLAY */ 00026000
*   /* PROCESSOR'S CSCB. THE DMN KEYWORD IS LOCATED IN CHBUF IN THIS */ 00027000
*   /* CSCB. IEEBUFSC WILL BE USED TO SCAN FOR THIS KEYWORD AND ITS  */ 00028000
*   /* ASSOCIATED PARAMETER. THIS TOP SEGMENT DETERMINES THE FLOW OF */ 00029000
*   /* CONTROL THRU THE MODULE                                       */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                                  0031 00033000
*   REG14SAV=R14;                   /* SAVE RETURN ADDRESS           */ 00034000
         ST    R14,REG14SAV                                        0031 00035000
*   CALL SETSTAE;                   /* CREATE STAE ENVIRONMENT       */ 00036000
         BAL   @14,SETSTAE                                         0032 00037000
*   IF R15=ZERO THEN                /* CHECK IF STAE CREATED         */ 00038000
         LTR   R15,R15                                             0033 00039000
         BNZ   @RF00033                                            0033 00040000
*     DO;                           /* PROCESSING TO DISPLAY DMN   0034 00041000
*                                      TABLE                         */ 00042000
*       FLAGS=ZERO;                 /* ZERO FLAGS                    */ 00043000
         MVI   FLAGS,X'00'                                         0035 00044000
*       ORIGID=CHCNID;              /* SAVE ORIGIN. CONSOLE ID       */ 00045000
         SLR   @03,@03                                             0036 00046000
         IC    @03,CHCNID(,CSCBPTR)                                0036 00047000
         STH   @03,ORIGID                                          0036 00048000
*       R0SAVE=ORIGID;              /* SET ROSAVE FOR 1ST WTO        */ 00049000
         ST    @03,R0SAVE                                          0037 00050000
*       ORIGAREA=CHARID;            /* SAVE SCREEN AREA ID           */ 00051000
*                                                                  0038 00052000
         MVC   ORIGAREA(1),CHARID(CSCBPTR)                         0038 00053000
*       /*************************************************************/ 00054000
*       /*                                                           */ 00055000
*       /* GET STORAGE FOR WORKAREAS AND PARAMETERS                  */ 00056000
*       /*                                                           */ 00057000
*       /*************************************************************/ 00058000
*                                                                  0039 00059000
*       CALL GETSTOR;                                              0039 00060000
*                                                                  0039 00061000
         BAL   @14,GETSTOR                                         0039 00062000
*       /*************************************************************/ 00063000
*       /*                                                           */ 00064000
*       /* CALL ROUTINES TO BUILD 1ST TWO LINES OF THE MULTI-LINE WTO*/ 00065000
*       /* IN THE PARAMETER LIST                                     */ 00066000
*       /*                                                           */ 00067000
*       /*************************************************************/ 00068000
*                                                                  0040 00069000
*       CALL TITLEBLD;              /* BUILD CONTROL LINE            */ 00070000
         BAL   @14,TITLEBLD                                        0040 00071000
*       CALL LABELBLD;              /* BUILD LABEL LINE              */ 00072000
*                                                                  0041 00073000
         BAL   @14,LABELBLD                                        0041 00074000
*       /*************************************************************/ 00075000
*       /*                                                           */ 00076000
*       /* INITIALIZE FIRST DATA LINE                        @ZA25176*/ 00077000
*       /*                                                           */ 00078000
*       /*************************************************************/ 00079000
*                                                                  0042 00080000
*       WPLML0=ZERO;                /*                       @ZA25176*/ 00081000
         MVI   WPLML0(WPLPTR),X'00'                                0042 00082000
*       WPLMLLEN=LENGTH(DLINE);     /* SET LENGTH IN WPL     @ZA25176*/ 00083000
         MVI   WPLMLLEN(WPLPTR),X'34'                              0043 00084000
*       WPLMLLTF=ZERO;              /* ZERO LINETYPE FLAGS   @ZA25176*/ 00085000
         SLR   @08,@08                                             0044 00086000
         STH   @08,WPLMLLTF(,WPLPTR)                               0044 00087000
*       WPLMLLTC='1'B;              /* SIGNAL DATALINE       @ZA25176*/ 00088000
         OI    WPLMLLTC(WPLPTR),B'00100000'                        0045 00089000
*       TEXTPTR=WPLPTR;             /* SET TEXTPTR FOR NEXT LINE     */ 00090000
         LR    TEXTPTR,WPLPTR                                      0046 00091000
*       MSGTXT=BLANK;               /* BLANK TEXT FIELD              */ 00092000
*                                                                  0047 00093000
         MVI   MSGTXT+5(TEXTPTR),C' '                              0047 00094000
         MVC   MSGTXT+6(46,TEXTPTR),MSGTXT+5(TEXTPTR)              0047 00095000
         MVC   MSGTXT+4(1,TEXTPTR),BLANK                           0047 00096000
*       /*************************************************************/ 00097000
*       /*                                                           */ 00098000
*       /* DECIDE WHETHER TO DISPLAY WHOLE DOMAIN TABLE,OR ONE OF ITS*/ 00099000
*       /* ENTRIES.                                                  */ 00100000
*       /*                                                           */ 00101000
*       /*************************************************************/ 00102000
*                                                                  0048 00103000
*       CALL DMNSCAN;               /* IS 'DMN ' IN CHBUF?           */ 00104000
         BAL   @14,DMNSCAN                                         0048 00105000
*       IF R15=ZERO THEN            /* YES,THEN SET COUNT FOR NO. OF */ 00106000
         LTR   R15,R15                                             0049 00107000
         BNZ   @RF00049                                            0049 00108000
*         NODMNWRT=NODMNS;          /* DOMAINS TO BE DISPLAYED.      */ 00109000
         L     @08,AREAPTR                                         0050 00110000
         LH    @08,NODMNS(,@08)                                    0050 00111000
         ST    @08,NODMNWRT                                        0050 00112000
*       ELSE                                                       0051 00113000
*         DO;                       /* NO,CHECK FOR OTHER OPTION,    */ 00114000
         B     @RC00049                                            0051 00115000
@RF00049 DS    0H                                                  0052 00116000
*           CALL DMNVSCAN;          /* DMN=VALUE                     */ 00117000
         BAL   @14,DMNVSCAN                                        0052 00118000
*           IF R15=ZERO THEN        /* OPTION IS FOUND               */ 00119000
         LTR   R15,R15                                             0053 00120000
         BNZ   @RF00053                                            0053 00121000
*             DO;                                                  0054 00122000
*               CALL VALDMN;        /* VERIFY VALUE IN CORRECT RANGE */ 00123000
         BAL   @14,VALDMN                                          0055 00124000
*               IF R15=ZERO THEN    /* VALUE IS ACCEPTABLE           */ 00125000
         LTR   R15,R15                                             0056 00126000
         BNZ   @RF00056                                            0056 00127000
*                 DO;               /* SET COUNT FOR # DMNS TO     0057 00128000
*                                      DISPLAY                       */ 00129000
*                   IF BUFCHAR^=BLANK THEN/* MORE OPERANDS EXIST     */ 00130000
         L     @08,ENDFLD                                          0058 00131000
         CLC   BUFCHAR(1,@08),BLANK                                0058 00132000
         BE    @RF00058                                            0058 00133000
*                     CALL ERRMSG(PI,DISPLAY);/* ISSUE ERROR MSG     */ 00134000
         LA    @01,@AL00059                                        0059 00135000
         BAL   @14,ERRMSG                                          0059 00136000
*                   ELSE                                           0060 00137000
*                     NODMNWRT=ONE; /* DISPLAY ONLY ONE DOMAIN       */ 00138000
         B     @RC00058                                            0060 00139000
@RF00058 MVC   NODMNWRT(4),@CF00072                                0060 00140000
*                 END;                                             0061 00141000
*               ELSE                                               0062 00142000
*                 CALL ERRMSG(KVI,DOMAIN);/* VALUE IS INVLAID,ISSUE     00143000
*                                      MSG                           */ 00144000
         B     @RC00056                                            0062 00145000
@RF00056 LA    @01,@AL00062                                        0062 00146000
         BAL   @14,ERRMSG                                          0062 00147000
*             END;                                                 0063 00148000
*           ELSE                    /* VALUE IS INVALID              */ 00149000
*             CALL ERRMSG(KVI,DOMAIN);/* ISSUE ERROR MSG             */ 00150000
         B     @RC00053                                            0064 00151000
@RF00053 LA    @01,@AL00064                                        0064 00152000
         BAL   @14,ERRMSG                                          0064 00153000
*         END;                                                     0065 00154000
*                                                                  0065 00155000
@RC00053 DS    0H                                                  0066 00156000
*       /*************************************************************/ 00157000
*       /*                                                           */ 00158000
*       /* DISPLAY THE DOMAIN TABLE                                  */ 00159000
*       /*                                                           */ 00160000
*       /*************************************************************/ 00161000
*                                                                  0066 00162000
*       DO COUNT=FIRST TO NODMNWRT; /* PROCESS FOR EACH DOMAIN       */ 00163000
@RC00049 LA    @08,1                                               0066 00164000
         B     @DE00066                                            0066 00165000
@DL00066 DS    0H                                                  0067 00166000
*         CALL DATABLD;             /* BUILD A DATA LINE FOR WTO     */ 00167000
         BAL   @14,DATABLD                                         0067 00168000
*         IF COUNT=NODMNWRT THEN                                   0068 00169000
         CLC   COUNT(4),NODMNWRT                                   0068 00170000
         BNE   @RF00068                                            0068 00171000
*           DONE='1'B;              /* SIGNAL LAST LINE TO MSGSET  0069 00172000
*                                                            @ZA25176*/ 00173000
         OI    DONE,B'10000000'                                    0069 00174000
*         CALL MSGSET;              /* WRITE OUT TEN LINES           */ 00175000
@RF00068 BAL   @14,MSGSET                                          0070 00176000
*       END;                                                       0071 00177000
*                                                                  0071 00178000
         LA    @08,1                                               0071 00179000
         AL    @08,COUNT                                           0071 00180000
@DE00066 ST    @08,COUNT                                           0071 00181000
         C     @08,NODMNWRT                                        0071 00182000
         BNH   @DL00066                                            0071 00183000
*       /*************************************************************/ 00184000
*       /*                                                           */ 00185000
*       /* AFTER COMPLETING LOOP,IF ALL LINES HAVE BEEN WRITTEN, FREE*/ 00186000
*       /* UP STORAGE AND RETURN.                            @ZA25176*/ 00187000
*       /*                                                           */ 00188000
*       /*************************************************************/ 00189000
*                                                                  0072 00190000
*       CALL FREESTOR;              /* FREE STORAGE                  */ 00191000
         BAL   @14,FREESTOR                                        0072 00192000
*     END;                                                         0073 00193000
*   CALL FREECSCB;                  /* FREE CSCB                     */ 00194000
@RF00033 BAL   @14,FREECSCB                                        0074 00195000
*   R14=REG14SAV;                   /* RESTORE REG14                 */ 00196000
         L     R14,REG14SAV                                        0075 00197000
*   RETURN;                                                        0076 00198000
@EL00001 L     @13,4(,@13)                                         0076 00199000
@EF00001 L     @00,@SIZDATD                                        0076 00200000
         LR    @01,@11                                             0076 00201000
         FREEMAIN R,LV=(0),A=(1)                                        00202000
@ER00001 BR    @14                                                 0076 00203000
*SETSTAE:                                                          0077 00204000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0077 00205000
*                                                                  0077 00206000
SETSTAE  DS    0H                                                  0078 00207000
*   /*****************************************************************/ 00208000
*   /*                                                               */ 00209000
*   /* THIS ROUTINE CREATES A STAE ENVIRONMENT THROUGH THE MODULE    */ 00210000
*   /* IEECB860. A PARAMETER LIST IS BUILT AND PASSED TO MODULE      */ 00211000
*   /* IEECB860 VIA REGISTER 1.                                      */ 00212000
*   /*                                                               */ 00213000
*   /*****************************************************************/ 00214000
*                                                                  0078 00215000
*   R6=R1;                          /* SAVE SETS CSCB PTR            */ 00216000
         LR    R6,R1                                               0078 00217000
*   W1=R14;                         /* SAVE RETURN ADDR              */ 00218000
         LR    W1,R14                                              0079 00219000
*   R1=ADDR(WORKAREA);              /* GET CORE FOR PARM LIST        */ 00220000
         LA    R1,WORKAREA                                         0080 00221000
*   R2=R1;                          /* SAVE CORE ADDR                */ 00222000
         LR    R2,R1                                               0081 00223000
*   CSCBADDR=R6;                    /* STORE CSCB PTR IN LIST        */ 00224000
         ST    R6,CSCBADDR(,R1)                                    0082 00225000
*   W2=ADDR(STAEBUF);               /* GET PTR TO CMD BUF            */ 00226000
         LA    W2,STAEBUF(,R1)                                     0083 00227000
*   BUFPTR=W2;                      /* STORE CMD BUF PTR IN PARAMETER   00228000
*                                      LIST                          */ 00229000
         ST    W2,BUFPTR(,R1)                                      0084 00230000
*   CMDSIZE=CMDLEN;                 /* STORE LENGTH OF COMMAND       */ 00231000
         MVI   CMDSIZE(R1),X'07'                                   0085 00232000
*   COMMAND=DISPLAY;                /* STORE VERB                    */ 00233000
         MVI   COMMAND+8(R1),C' '                                  0086 00234000
         MVC   COMMAND+9(2,R1),COMMAND+8(R1)                       0086 00235000
         MVC   COMMAND(8,R1),DISPLAY                               0086 00236000
*   RFY                                                            0087 00237000
*    (R11,                                                         0087 00238000
*     R12,                                                         0087 00239000
*     R13) RSTD;                                                   0087 00240000
*   RETADDR=W1;                     /* SAVE RETURN ADDRESS           */ 00241000
         ST    W1,RETADDR(,R1)                                     0088 00242000
*   REG6SAV=R6;                     /* SAVE CSCB ADDRESS             */ 00243000
         ST    R6,REG6SAV(,R1)                                     0089 00244000
*   REG11SAV=R11;                   /* SAVE DATA REG PTR             */ 00245000
         ST    R11,REG11SAV(,R1)                                   0090 00246000
*   REG12SAV=R12;                   /* SAVE BASE REG PTR             */ 00247000
         ST    R12,REG12SAV(,R1)                                   0091 00248000
*   REG13SAV=R13;                   /* SAVE SAVE AREA PTR            */ 00249000
         ST    R13,REG13SAV(,R1)                                   0092 00250000
*   GEN(LOAD EP=IEECB860);          /* LOAD STAE CREATING MOD        */ 00251000
         LOAD EP=IEECB860                                               00252000
*   R15=R0;                         /* PUT ENTRY ADDR IN REG 15      */ 00253000
         LR    R15,R0                                              0094 00254000
*   R1=R2;                          /* RESTORE PARM PTR              */ 00255000
         LR    R1,R2                                               0095 00256000
*   GEN(BALR R14,R15);              /* BRANCH TO STAE ROUTINE        */ 00257000
         BALR R14,R15                                                   00258000
*   W1=RETADDR;                     /* RESTORE RETURN ADDRESS        */ 00259000
         L     W1,RETADDR(,R1)                                     0097 00260000
*   R6=REG6SAV;                     /* RESTORE CSCB ADDR             */ 00261000
         L     R6,REG6SAV(,R1)                                     0098 00262000
*   R11=REG11SAV;                   /* RESTORE DATA REGISTER         */ 00263000
         L     R11,REG11SAV(,R1)                                   0099 00264000
*   R12=REG12SAV;                   /* RESTORE BASE REGISTER         */ 00265000
         L     R12,REG12SAV(,R1)                                   0100 00266000
*   R13=REG13SAV;                   /* RESTORE SAVE AREA PTR         */ 00267000
         L     R13,REG13SAV(,R1)                                   0101 00268000
*   RFY                                                            0102 00269000
*    (R11,                                                         0102 00270000
*     R12,                                                         0102 00271000
*     R13) UNRSTD;                                                 0102 00272000
*   W2=R15;                         /* SAVE RETURN CODE              */ 00273000
         LR    W2,R15                                              0103 00274000
*   GEN(DELETE EP=IEECB860);        /* DELETE STAE CREATE MOD        */ 00275000
         DELETE EP=IEECB860                                             00276000
*   R15=W2;                         /* PUT RETURN CODE IN REGISTER 15*/ 00277000
         LR    R15,W2                                              0105 00278000
*   R14=W1;                         /* RESTORE RETURN ADDR           */ 00279000
         LR    R14,W1                                              0106 00280000
*   END SETSTAE;                                                   0107 00281000
@EL00002 DS    0H                                                  0107 00282000
@EF00002 DS    0H                                                  0107 00283000
@ER00002 BR    @14                                                 0107 00284000
*GETSTOR:                                                          0108 00285000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0108 00286000
*                                                                  0108 00287000
GETSTOR  DS    0H                                                  0109 00288000
*   /*****************************************************************/ 00289000
*   /*                                                               */ 00290000
*   /* THIS ROUTINE OBTAINS STORAGE FOR A COPY OF THE DOMAIN TABLE   */ 00291000
*   /* FROM SUBPOOL 253. THE COPY IS OBTAINED VIA SYSEVENT #40       */ 00292000
*   /* COPYDMDT. STORAGE IS ALSO OBTAINED FOR WTO WORKAREA.          */ 00293000
*   /*                                                               */ 00294000
*   /*****************************************************************/ 00295000
*                                                                  0109 00296000
*                                                                  0109 00297000
*   /*****************************************************************/ 00298000
*   /*                                                               */ 00299000
*   /* GET STORAGE FOR WTO PARAMETER LIST AND STORAGE FOR SYSEVENT   */ 00300000
*   /* PROCESSOR PARAMETERS                                          */ 00301000
*   /*                                                               */ 00302000
*   /*****************************************************************/ 00303000
*                                                                  0109 00304000
*   REGISTER=R14;                   /* SAVE RETURN ADDR              */ 00305000
         LR    REGISTER,R14                                        0109 00306000
*   SIZE=MAXSIZE+WTOSIZE+FOUR;      /* TABLE SIZE AND COUNT FIELD    */ 00307000
         LA    @09,904                                             0110 00308000
         AL    @09,MAXSIZE                                         0110 00309000
         ST    @09,SIZE                                            0110 00310000
*   R0=SIZE|SUBPL253;               /* SUBPOOL NUMBER AND STORAGE  0111 00311000
*                                      SIZE                          */ 00312000
         O     @09,SUBPL253                                        0111 00313000
         LR    R0,@09                                              0111 00314000
*   GEN(GETMAIN R,LV=(0));                                         0112 00315000
*                                                                  0112 00316000
         GETMAIN R,LV=(0)                                               00317000
*   /*****************************************************************/ 00318000
*   /*                                                               */ 00319000
*   /* CLEAR OBTAINED AREA                                           */ 00320000
*   /*                                                               */ 00321000
*   /*****************************************************************/ 00322000
*                                                                  0113 00323000
*   AREAPTR=R1;                     /* SAVE ADDRESS OF PAR AREA      */ 00324000
         ST    R1,AREAPTR                                          0113 00325000
*   WPLBASE=R1+MAXSIZE+FOUR;        /* SAVE ADDR OF WTO WORKAREA     */ 00326000
         LR    @09,R1                                              0114 00327000
         AL    @09,MAXSIZE                                         0114 00328000
         AL    @09,@CF00049                                        0114 00329000
         ST    @09,WPLBASE                                         0114 00330000
*   WPLPTR=WPLBASE;                 /* ADDR OF FIRST WTO LINE        */ 00331000
         LR    WPLPTR,@09                                          0115 00332000
*   DMDTPTR=ADDR(DMNTAB);           /* SAVE ADDRESS OF DOMAIN COPY   */ 00333000
         L     @09,AREAPTR                                         0116 00334000
         LA    DMDTPTR,DMNTAB(,@09)                                0116 00335000
*   R0=R1;                          /* ADDRESS OF AREA TO BE CLEARED */ 00336000
         LR    R0,R1                                               0117 00337000
*   R1=SIZE;                        /* LENGTH OF AREA        @ZM43101*/ 00338000
         L     R1,SIZE                                             0118 00339000
*   R14=ADDR(CLEAR);                /* ADDR OF LOCATION WITH 0       */ 00340000
         LA    R14,CLEAR                                           0119 00341000
*   R15=ZERO;                       /* PADDING CHAR IS 0             */ 00342000
         SLR   R15,R15                                             0120 00343000
*   GEN REFS(R0,R14)(MVCL R0,R14);                                 0121 00344000
*                                                                  0121 00345000
         MVCL R0,R14                                                    00346000
*   /*****************************************************************/ 00347000
*   /*                                                               */ 00348000
*   /* GET COPY OF DOMAIN TABLE AND THE COUNT OF THE NO. OF ENTRIES  */ 00349000
*   /* IN THE TABLE.                                                 */ 00350000
*   /*                                                               */ 00351000
*   /*****************************************************************/ 00352000
*                                                                  0122 00353000
*   R1=AREAPTR;                     /* ADDR OF PARM AREA             */ 00354000
         L     R1,AREAPTR                                          0122 00355000
*   GEN(SYSEVENT COPYDMDT,ENTRY=BRANCH);                           0123 00356000
         SYSEVENT COPYDMDT,ENTRY=BRANCH                                 00357000
*   R14=REGISTER;                   /* RESTORE R14                   */ 00358000
         LR    R14,REGISTER                                        0124 00359000
*   END GETSTOR;                                                   0125 00360000
@EL00003 DS    0H                                                  0125 00361000
@EF00003 DS    0H                                                  0125 00362000
@ER00003 BR    @14                                                 0125 00363000
*TITLEBLD:                                                         0126 00364000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0126 00365000
*                                                                  0126 00366000
TITLEBLD DS    0H                                                  0127 00367000
*   /*****************************************************************/ 00368000
*   /*                                                               */ 00369000
*   /* BUILDS CONTROL LINE FOR DOMAIN DISPLAY TEXTPTR PTS TO         */ 00370000
*   /* BEGINNING OF WTO PARAMETER LIST.                              */ 00371000
*   /*                                                               */ 00372000
*   /*****************************************************************/ 00373000
*                                                                  0127 00374000
*   WPLMCSFA='1'B;                  /* ROUT/DESC CODES EXIST         */ 00375000
*   WPLMCSFB='1'B;                  /* REG0 - Q TO CONSOLE IF ACTIVE */ 00376000
*   WPLMCSFC='1'B;                  /* COMMAND RESPONSE IND          */ 00377000
         OI    WPLMCSFA(WPLPTR),B'11100000'                        0129 00378000
*   WPLMCSFJ='1'B;                  /* TURN ON MLWTO INDICATOR       */ 00379000
         OI    WPLMCSFJ(WPLPTR),B'01000000'                        0130 00380000
*   MCSFSAVE=WPLMCSF;               /* SAVE MCS FLAGS                */ 00381000
         LH    @03,WPLMCSF(,WPLPTR)                                0131 00382000
         N     @03,@CF01644                                        0131 00383000
         STH   @03,MCSFSAVE                                        0131 00384000
*   WPLTXT(1:LENGTH(CNTLINE))=CNTLINE;/* FILL IN HEADER              */ 00385000
         MVC   WPLTXT(31,WPLPTR),CNTLINE                           0132 00386000
*TIME:                                                             0133 00387000
*   GEN(TIME  DEC,ERRET=NOTIME);    /* GET TIME                      */ 00388000
TIME     TIME  DEC,ERRET=NOTIME                                         00389000
*   CONVERT1=R0;                    /* SAVE TIME FROM REG 0          */ 00390000
         LA    @03,CONVERT                                         0134 00391000
         ST    R0,CONVERT1(,@03)                                   0134 00392000
*   GEN(UNPK  UNPAK(7),CONVERT(4)); /* CONV TO EBCDIC                */ 00393000
         UNPK  UNPAK(7),CONVERT(4)                                      00394000
*   WPLTXT(9:10)=UNPAK(1:2);        /* HOURS                         */ 00395000
         MVC   WPLTXT+8(2,WPLPTR),UNPAK                            0136 00396000
*   WPLTXT(12:13)=UNPAK(3:4);       /* MINUTES                       */ 00397000
         MVC   WPLTXT+11(2,WPLPTR),UNPAK+2                         0137 00398000
*   WPLTXT(15:16)=UNPAK(5:6);       /* SECONDS                       */ 00399000
         MVC   WPLTXT+14(2,WPLPTR),UNPAK+4                         0138 00400000
*NOTIME:                                                           0139 00401000
*   WPLLGH=LENGTH(CNTLINE)+FOUR;    /* SET CNTL LINE LENGTH          */ 00402000
NOTIME   LA    @03,35                                              0139 00403000
         STH   @03,WPLLGH(,WPLPTR)                                 0139 00404000
*   WPLPTR=WPLPTR+WPLLGH;           /* UPDATE PAST TEXT              */ 00405000
         ALR   WPLPTR,@03                                          0140 00406000
*   WPLDESCE='1'B;                  /* DESC CODE 5                   */ 00407000
*   WPLDESCH='1'B;                  /* DESC CODE 8                   */ 00408000
         OI    WPLDESCE(WPLPTR),B'00001001'                        0142 00409000
*   WPLDESCI='1'B;                  /* DESC CODE 9                   */ 00410000
         OI    WPLDESCI(WPLPTR),B'10000000'                        0143 00411000
*   DESCSAVE=WPLDESC;               /* SAVE DESCRIPTOR FLAGS         */ 00412000
         MVC   DESCSAVE(2),WPLDESC(WPLPTR)                         0144 00413000
*   WPLROUT=ZERO;                   /* ZERO ROUTE CODES              */ 00414000
         SLR   @03,@03                                             0145 00415000
