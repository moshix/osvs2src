*DMNVSCAN:                                                         0172 00512000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0172 00513000
DMNVSCAN DS    0H                                                  0173 00514000
*                                                                  0173 00515000
*/*                                                                0173 00516000
********************************************************************    00517000
**                                                                 0173 00518000
**     ROUTINE NAME        IEEBUFSC                                0173 00519000
**                                                                 0173 00520000
**     FUNCTION            TO SCAN A BUFFER FOR A GIVEN KEYWORD OF 0173 00521000
**                         A GIVEN LENGTH AND TO RETURN A POINTER  0173 00522000
**                         TO THE KEYWORD VALUE AND THE LENGTH OF  0173 00523000
**                         THE VALUE                               0173 00524000
**                                                                 0173 00525000
**     ENTRY POINT         IEEBUFSC                                0173 00526000
**                                                                 0173 00527000
**     ATTRIBUTES          REFRESHABLE                             0173 00528000
**                                                                 0173 00529000
**     OPERATION     0     1. SET UP PTR TO BUFFER -1 AND PTR      0173 00530000
**                            TO KEYWORD LENGTH                    0173 00531000
**                         2. IF BUFFER PTR = SPECIFIED KEYWORD    0173 00532000
**                            GO TO FOUND ROUTINE (3).             0173 00533000
**                            A. IF NOT, INCREMENT BUFFER PTR      0173 00534000
**                               BY ONE.                           0173 00535000
**                            B. IF PTR IS AT END OF BUFFER SET    0173 00536000
**                               UP NOT FOUND RETURN CODE (8) IN   0173 00537000
**                               R15 AND EXIT.                     0173 00538000
**                            C. IF PTR IS AT A COMMA, GO BACK TO  0173 00539000
**                               OPERATION 2.                      0173 00540000
**                            D. IF PTR IS AT A BLANK SET UP NOT   0173 00541000
**                               FOUND RETURN CODE (8) IN R15 AND  0173 00542000
**                               EXIT.                             0173 00543000
**                            E. OTHERWISE GO TO OPERATION 2.A.    0173 00544000
**                         3. AT FOUND ROUTINE SET UP R14 TO POINT 0173 00545000
**                            TO KEYWORD VALUE                     0173 00546000
**                         4. SCAN KEYWORD VALUE TO THE END OF     0173 00547000
**                            THE BUFFER, A COMMA OR A BLANK       0173 00548000
**                         5. CALCULATE LENGTH OF KEYWORD VALUE.   0173 00549000
**                         6. IF LENGTH IS < OR = ZERO. SET UP     0173 00550000
**                            AN INVALID KEYWORD VALUE LENGTH      0173 00551000
**                            RETURN CODE (4) IN R15 AND EXIT.     0173 00552000
**                         7. OTHERWISE, SET GOOD RETURN CODE      0173 00553000
**                            OF ZERO IN R15 AND EXIT.             0173 00554000
**                                                                 0173 00555000
**     EXTERNAL REFS       NONE                                    0173 00556000
**                                                                 0173 00557000
**     SVC                 NONE                                    0173 00558000
**                                                                 0173 00559000
**     INPUT               R1 - PTR TO BEGINNING OF BUFFER         0173 00560000
**                         R0 - PTR TO LAST BYTE OF BUFFER + 1     0173 00561000
**                         R15- PTR TO KEYWORD WITH THE LENGTH     0173 00562000
**                              OF THE KEYWORD IN THE HIGH ORDER   0173 00563000
**                              BYTE.                              0173 00564000
**                              EG. KEYWORD FORMAT 'DSNAME='       0173 00565000
**                              R15 WOULD POINT TO THE 'D'         0173 00566000
**                              WITH A 6 IN THE HIGH ORDER         0173 00567000
**                              BYTE.                              0173 00568000
**                                                                 0173 00569000
**     OUTPUT              R14  PTR TO FIRST BYTE OF KEYWORD       0173 00570000
**                              VALUE IN BUFFER OR WORKAREA        0173 00571000
**                         R1   LENGTH OF KEYWORD VALUE            0173 00572000
**                         R15  RETURN CODE OF ZERO.               0173 00573000
**                                                                 0173 00574000
**     ERROR OUTPUT        R15  CONTAINS A FOUR IF KEYWORD         0173 00575000
**                              HAD AN INVALID LENGTH              0173 00576000
**                         R15  CONTAINS AN EIGHT IF THE KEYWORD   0173 00577000
**                              WAS NOT FOUND IN THE BUFFER        0173 00578000
**                                                                 0173 00579000
**     EXIT                TO NEXT SEQUENTIAL INSTRUCTION AFTER    0173 00580000
**                         MACRO CALL                              0173 00581000
**                                                                 0173 00582000
**     NOTES               LISTS OF OPERANDS IN PARENTHESES ARE    0173 00583000
**                         NOT PROCESSED.  THIS MACRO IS INTENDED  0173 00584000
**                         TO PROCESS ONLY A SINGLE VALUE FOR      0173 00585000
**                         A GIVEN KEYWORD                         0173 00586000
********************************************************************    00587000
*                                                                    */ 00588000
*                                                                  0173 00589000
*   REGISTER=R14;                                                  0173 00590000
         LR    REGISTER,R14                                        0173 00591000
*   R0=ADDR(CHTYPE);                /* PTR LAST BYTE BUFFER +1       */ 00592000
         LA    R0,CHTYPE(,CSCBPTR)                                 0174 00593000
*   R15=ADDR(KEYWORD);              /* PTR TO DESIRED KEYWORD        */ 00594000
         LA    R15,KEYWORD                                         0175 00595000
*   R15=R15|KEYWDLEN;               /* GET KEYWORD LENGTH            */ 00596000
         O     R15,KEYWDLEN                                        0176 00597000
*   R1=ADDR(CHBUF);                 /* PTR BEGINNING OF BUFFER       */ 00598000
         LA    R1,CHBUF(,CSCBPTR)                                  0177 00599000
*   GEN(IEEBUFSC);                                                 0178 00600000
         IEEBUFSC                                                       00601000
*   VALLEN=R1;                      /* SAVE LENGTH OF VALUE          */ 00602000
         ST    R1,VALLEN                                           0179 00603000
*   VALPTR=R14;                     /* SAVE PTR TO VALUE             */ 00604000
         ST    R14,VALPTR                                          0180 00605000
*   ENDFLD=VALPTR+VALLEN;           /* PT TO DELIMITER               */ 00606000
         L     @03,VALPTR                                          0181 00607000
         AL    @03,VALLEN                                          0181 00608000
         ST    @03,ENDFLD                                          0181 00609000
*   R14=REGISTER;                   /* RESTORE REG 14                */ 00610000
         LR    R14,REGISTER                                        0182 00611000
*   END DMNVSCAN;                                                  0183 00612000
@EL00007 DS    0H                                                  0183 00613000
@EF00007 DS    0H                                                  0183 00614000
@ER00007 BR    @14                                                 0183 00615000
