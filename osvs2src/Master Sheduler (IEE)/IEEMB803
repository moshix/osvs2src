*   REG1=ADDR(MSGPARM);             /* PARM ADDRESS TO REG1          */ 01981000
         LA    REG1,MSGPARM                                        0802 01982000
*   CALL IEEMB807;                  /* ISSUE WTO                     */ 01983000
         L     @15,@CV02192                                        0803 01984000
         BALR  @14,@15                                             0803 01985000
*   REG15=0000;                     /* CLEAR RETURN CODE REG         */ 01986000
