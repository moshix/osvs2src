*   REG14=MAINSAV;                  /* RESTORE RETURN ADDRESS        */ 01371000
         L     REG14,MAINSAV                                       0626 01372000
*   END NEWLOG;                                                    0627 01373000
@EL00016 DS    0H                                                  0627 01374000
@EF00016 DS    0H                                                  0627 01375000
@ER00016 BR    @14                                                 0627 01376000
*CLOSRTN:                                                          0628 01377000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0628 01378000
*                                                                  0628 01379000
CLOSRTN  DS    0H                                                  0629 01380000
*   /*****************************************************************/ 01381000
*   /*                                                               */ 01382000
*   /* PROCESS THE OLD LOG DATA SET FOR CLOSE. ON ENTRY THE ACB PTR  */ 01383000
*   /* IS CONTAINED IN FIELD LCACBTWO , AND THE DDNAME IS CONTAINED  */ 01384000
*   /* IN FIELD LCADDTWO.                                            */ 01385000
*   /*                                                               */ 01386000
*   /*****************************************************************/ 01387000
*                                                                  0629 01388000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 01389000
         LR    REGSAV,REG14                                        0629 01390000
*   REG2=LCACBTWO;                  /* ACB PTR TO CLOSE              */ 01391000
         L     REG2,LCACBTWO(,LCAPTR)                              0630 01392000
*   REG1=ADDR(CLOSCORE);            /* CLOSE MACRO PARM CORE         */ 01393000
         LA    REG1,CLOSCORE                                       0631 01394000
*   CLOSCORE(1:CLOSLEN2)=CLOSLIST(1:CLOSLEN2);/* MOVE MACRO LIST FORM*/ 01395000
         L     @09,CLOSLEN2                                        0632 01396000
         BCTR  @09,0                                               0632 01397000
         EX    @09,@SM02476                                        0632 01398000
*   GEN( CLOSE ((2)),MF=(E,(1)));   /* CLOSE THE ACB                 */ 01399000
          CLOSE ((2)),MF=(E,(1))                                        01400000
*   LCACBTWO=0000;                  /* ZERO ACB PTR                  */ 01401000
         SLR   @09,@09                                             0634 01402000
         ST    @09,LCACBTWO(,LCAPTR)                               0634 01403000
*   REG14=REGSAV;                   /* RESTORE RETURN ADDRESS        */ 01404000
         LR    REG14,REGSAV                                        0635 01405000
*   END CLOSRTN;                                                   0636 01406000
@EL00017 DS    0H                                                  0636 01407000
@EF00017 DS    0H                                                  0636 01408000
@ER00017 BR    @14                                                 0636 01409000
*LOGERROR:                                                         0637 01410000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0637 01411000
*                                                                  0637 01412000
LOGERROR DS    0H                                                  0638 01413000
*   /*****************************************************************/ 01414000
*   /*                                                               */ 01415000
*   /* ENTRY HERE IS TO CLEAN UP LOG CORE BECAUSE OF ABNORMAL        */ 01416000
*   /* PROCESSING OF THE LOG FUNCTION. THIS RTN IS CALLED BY THE     */ 01417000
*   /* ESTAE EXIT RTN AND BY LOG INITIALIZATION OPEN FAILURE. THE LOG*/ 01418000
*   /* DATA SET(S) ARE CLOSED AND DYNAMICALLY UNALLOCATED AND RETURN */ 01419000
*   /* TO CALLER.                                                    */ 01420000
*   /*                                                               */ 01421000
*   /*****************************************************************/ 01422000
*                                                                  0638 01423000
*   MAINSAV=REG14;                  /* SAVE RETURN ADDRESS           */ 01424000
         ST    REG14,MAINSAV                                       0638 01425000
*   IF LCADDONE^=ZEROS THEN         /* A DATA SET IS ALLOCATED       */ 01426000
         CLC   LCADDONE(8,LCAPTR),ZEROS                            0639 01427000
         BE    @RF00639                                            0639 01428000
*     DO;                                                          0640 01429000
*       DO WHILE(LCACBONE^=0000);                                  0641 01430000
         B     @DE00641                                            0641 01431000
@DL00641 DS    0H                                                  0642 01432000
*         IF LCACBTWO^=0000 THEN    /* TWO DATA SETS OPEN            */ 01433000
         L     @03,LCACBTWO(,LCAPTR)                               0642 01434000
         LTR   @03,@03                                             0642 01435000
         BZ    @RF00642                                            0642 01436000
*           DO;                                                    0643 01437000
*             ACBPTR=LCACBTWO;      /* PTR TO SECOND ACB TO FREE     */ 01438000
         ST    @03,ACBPTR                                          0644 01439000
*             CALL CLOSRTN;         /* CLOSE THE DATA SET            */ 01440000
         BAL   @14,CLOSRTN                                         0645 01441000
*             CALL FREEACB;         /* FREE THE ACB CORE             */ 01442000
         BAL   @14,FREEACB                                         0646 01443000
*           END;                                                   0647 01444000
*         ELSE                                                     0648 01445000
*           DO;                                                    0648 01446000
         B     @RC00642                                            0648 01447000
@RF00642 DS    0H                                                  0649 01448000
*             ACBPTR=LCACBONE;      /* PTR TO FIRST ACB TO FREE      */ 01449000
         L     @03,LCACBONE(,LCAPTR)                               0649 01450000
         ST    @03,ACBPTR                                          0649 01451000
*             LCACBTWO=LCACBONE;    /* SET FIELD FOR CLOSE RTN       */ 01452000
         ST    @03,LCACBTWO(,LCAPTR)                               0650 01453000
*             CALL CLOSRTN;         /* CLOSE THE DATA SET            */ 01454000
         BAL   @14,CLOSRTN                                         0651 01455000
*             CALL FREEACB;         /* FREE ACB CORE                 */ 01456000
         BAL   @14,FREEACB                                         0652 01457000
*             LCACBONE=0000;        /* SET LOOP EXIT VALUE           */ 01458000
         SLR   @03,@03                                             0653 01459000
         ST    @03,LCACBONE(,LCAPTR)                               0653 01460000
*           END;                                                   0654 01461000
*         LCAACBF='0'B;             /* SET CORE FLAG OFF             */ 01462000
@RC00642 NI    LCAACBF(LCAPTR),B'11111011'                         0655 01463000
*       END;                                                       0656 01464000
@DE00641 L     @03,LCACBONE(,LCAPTR)                               0656 01465000
         LTR   @03,@03                                             0656 01466000
         BNZ   @DL00641                                            0656 01467000
*       DO WHILE(LCADDONE^=ZEROS);                                 0657 01468000
         B     @DE00657                                            0657 01469000
@DL00657 DS    0H                                                  0658 01470000
*         IF LCADDTWO^=ZEROS THEN   /* TWO DATA SETS ALLOCATED       */ 01471000
         CLC   LCADDTWO(8,LCAPTR),ZEROS                            0658 01472000
         BE    @RF00658                                            0658 01473000
*           CALL UNALLOC;           /* UNALLOCATE THE DATA SET       */ 01474000
         BAL   @14,UNALLOC                                         0659 01475000
*         ELSE                                                     0660 01476000
*           DO;                                                    0660 01477000
         B     @RC00658                                            0660 01478000
@RF00658 DS    0H                                                  0661 01479000
*             LCADDTWO=LCADDONE;    /* SET PARM FOR UNALLOC RTN      */ 01480000
         MVC   LCADDTWO(8,LCAPTR),LCADDONE(LCAPTR)                 0661 01481000
*             CALL UNALLOC;         /* UNALLOCATE THE DATA SET       */ 01482000
         BAL   @14,UNALLOC                                         0662 01483000
*             LCADDONE=ZEROS;       /* EXIT FROM LOOP                */ 01484000
         MVC   LCADDONE(8,LCAPTR),ZEROS                            0663 01485000
*           END;                                                   0664 01486000
*       END;                                                       0665 01487000
@RC00658 DS    0H                                                  0665 01488000
@DE00657 CLC   LCADDONE(8,LCAPTR),ZEROS                            0665 01489000
         BNE   @DL00657                                            0665 01490000
*     END;                                                         0666 01491000
*   REG14=MAINSAV;                  /* RESTORE RETURN ADDRESS        */ 01492000
@RF00639 L     REG14,MAINSAV                                       0667 01493000
*   END LOGERROR;                                                  0668 01494000
@EL00018 DS    0H                                                  0668 01495000
@EF00018 DS    0H                                                  0668 01496000
@ER00018 BR    @14                                                 0668 01497000
*ALLOCRTN:                                                         0669 01498000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0669 01499000
*                                                                  0669 01500000
ALLOCRTN DS    0H                                                  0670 01501000
*   /*****************************************************************/ 01502000
*   /*                                                               */ 01503000
*   /* ENTRY IS TO DYNAMICALLY ALLOCATE A SYSOUT DATA SET FOR THE    */ 01504000
*   /* SYSTEM LOG. THE DYNAM ALLOC INTERFACE IS SET UP , CLASS VALUE */ 01505000
*   /* IS INITIALIZED FROM BASEA(MSLOGCLS) FIELD , AND THE SVC IS    */ 01506000
*   /* INVOKED - ?DYNALLOC - . REG15 RETURN CODE WILL BE CHECKED BY  */ 01507000
*   /* CALLER. RETURN TO CALLER.                                     */ 01508000
*   /*                                                               */ 01509000
*   /*****************************************************************/ 01510000
*                                                                  0670 01511000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 01512000
         LR    REGSAV,REG14                                        0670 01513000
*   S99RBPTR=ADDR(S99RB);           /* PTR TO DYNAM ALLOC BLOCK      */ 01514000
         LA    @09,S99RB                                           0671 01515000
         ST    @09,S99RBPTR                                        0671 01516000
*   S99RBPND='1'B;                  /* SET HI-BIT ON FOR DYNAM ALLOC */ 01517000
         OI    S99RBPND,B'10000000'                                0672 01518000
*   S99RB=S99RB&&S99RB;             /* CLEAR CONTROL BLOCK CORE      */ 01519000
         XC    S99RB(20),S99RB                                     0673 01520000
*   S99RBLN=LENGTH(S99RB);          /* LENGTH OF CONTROL BLOCK       */ 01521000
         MVI   S99RBLN,X'14'                                       0674 01522000
*   S99VERB=S99VRBAL;               /* SET ALLOCATION VERB VALUE     */ 01523000
         MVI   S99VERB,X'01'                                       0675 01524000
*   S99TXTPP=ADDR(ALLOCPTR);        /* PTR TO TEXT UNIT PTRS         */ 01525000
         LA    @09,ALLOCPTR                                        0676 01526000
         ST    @09,S99TXTPP                                        0676 01527000
*   DATU1=DATXT1;                   /* MOVE IN SYSOUT TEXT UNIT FLDS */ 01528000
         MVC   DATU1(7),DATXT1                                     0677 01529000
*   OUTCLS=MSLOGCLS;                /* INITIALIZE OUTPUT CLASS VALUE */ 01530000
         L     @09,CVTPTR                                          0678 01531000
         L     @09,CVTMSER(,@09)                                   0678 01532000
         MVC   OUTCLS(1),MSLOGCLS(@09)                             0678 01533000
*   DATU2=DATXT2;                   /* MOVE IN DDNAME TEXT UNIT FLDS */ 01534000
         MVC   DATU2(14),DATXT2                                    0679 01535000
*   DATU3=DATXT3;                   /* INITIALIZ LRECL FIELD @YA00804*/ 01536000
         MVC   DATU3(8),DATXT3                                     0680 01537000
*   DATU4=DATXT4;                   /* INITIALIZ RECFM FIELD @YA00804*/ 01538000
         MVC   DATU4(7),DATXT4                                     0681 01539000
*   DATP1=ADDR(DATU1);              /* PTR TO SYSOUT TEXT UNIT       */ 01540000
         LA    @09,DATU1                                           0682 01541000
         ST    @09,DATP1                                           0682 01542000
