@DL00427 DS    0H                                                  0428 00743000
*     CALL WTLREC;                  /* PUT WTL QUEUE TO LOG DATA SET */ 00744000
         BAL   @14,WTLREC                                          0428 00745000
*   END;                                                           0429 00746000
@DE00427 LTR   REG15,REG15                                         0429 00747000
         BNZ   @DL00427                                            0429 00748000
*   CALL ALLOCRTN;                  /* GET NEW LOG DATA SET          */ 00749000
         BAL   @14,ALLOCRTN                                        0430 00750000
*   IF REG15=0000 THEN              /* ALLOCATION SUCCESSFUL         */ 00751000
         LTR   REG15,REG15                                         0431 00752000
         BNZ   @RF00431                                            0431 00753000
*     DO;                                                          0432 00754000
*       CALL NEWACB;                /* GET, INITIALIZE ACB           */ 00755000
         BAL   @14,NEWACB                                          0433 00756000
*       IF REG15=0000 THEN          /* CORE GOTTEN FOR ACB           */ 00757000
         LTR   REG15,REG15                                         0434 00758000
         BNZ   @RF00434                                            0434 00759000
*         DO;                                                      0435 00760000
*           CALL OPENRTN;           /* OPEN NEW LOG DATA SET         */ 00761000
         BAL   @14,OPENRTN                                         0436 00762000
*           IF REG15=0000 THEN      /* OPEN SUCCESSFUL               */ 00763000
         LTR   REG15,REG15                                         0437 00764000
         BNZ   @RF00437                                            0437 00765000
*             DO;                                                  0438 00766000
*               LCACBTWO=LCACBONE;  /* OLD ACB PTR FOR CLOSE         */ 00767000
         L     @03,LCACBONE(,LCAPTR)                               0439 00768000
         ST    @03,LCACBTWO(,LCAPTR)                               0439 00769000
*               LCACBONE=ACBPTR;    /* CURRENT ACB PTR               */ 00770000
         L     @09,ACBPTR                                          0440 00771000
         ST    @09,LCACBONE(,LCAPTR)                               0440 00772000
*               LCADDTWO=LCADDONE;  /* OLD DDNAME PTR FOR UNALLOC    */ 00773000
         MVC   LCADDTWO(8,LCAPTR),LCADDONE(LCAPTR)                 0441 00774000
*               LCADDONE=DDALNM;    /* CURRENT DDNAME PTR            */ 00775000
         MVC   LCADDONE(8,LCAPTR),DDALNM                           0442 00776000
*               ACBPTR=LCACBTWO;    /* SET OLD ACB PTR FOR FREE      */ 00777000
         ST    @03,ACBPTR                                          0443 00778000
*               CALL CLOSRTN;       /* CLOSE OLD DATA SET            */ 00779000
         BAL   @14,CLOSRTN                                         0444 00780000
*               CALL UNALLOC;       /* UNALLOCATE OLD DATA SET       */ 00781000
         BAL   @14,UNALLOC                                         0445 00782000
*               IF REG15=0000 THEN  /* UNALLOCATION SUCCESSFUL       */ 00783000
         LTR   REG15,REG15                                         0446 00784000
         BNZ   @RF00446                                            0446 00785000
*                 MSGONE=MSG1;      /* 'Q' TO SYSOUT MSG             */ 00786000
         IC    @03,MSG1                                            0447 00787000
         STC   @03,MSGONE                                          0447 00788000
*               ELSE                                               0448 00789000
*                 MSGONE=MSG2;      /* UNALLOC FAILED MSG            */ 00790000
         B     @RC00446                                            0448 00791000
@RF00446 IC    @03,MSG2                                            0448 00792000
         STC   @03,MSGONE                                          0448 00793000
*               MSGCLS=OUTCLS;      /* SET OUTPUT CLASS VALUE        */ 00794000
@RC00446 IC    @03,OUTCLS                                          0449 00795000
         STC   @03,MSGCLS                                          0449 00796000
*               LOGLOOP='00'X;      /* END POST/PUT LOOP             */ 00797000
         MVI   LOGLOOP,X'00'                                       0450 00798000
*             END;                                                 0451 00799000
*           ELSE                    /* OPEN FAILURE                  */ 00800000
*             DO;                                                  0452 00801000
         B     @RC00437                                            0452 00802000
@RF00437 DS    0H                                                  0453 00803000
*               LCADDTWO=DDALNM;    /* UNALLOCATE DDNAME             */ 00804000
         MVC   LCADDTWO(8,LCAPTR),DDALNM                           0453 00805000
*               CALL UNALLOC;       /* UNALLOCATE NEW LOG DATA SET   */ 00806000
         BAL   @14,UNALLOC                                         0454 00807000
*               MSGONE=MSG6;        /* D/S SWITCH MSG                */ 00808000
         IC    @03,MSG6                                            0455 00809000
         STC   @03,MSGONE                                          0455 00810000
*               LOGLOOP='00'X;      /* END POST/PUT LOOP             */ 00811000
         MVI   LOGLOOP,X'00'                                       0456 00812000
*             END;                                                 0457 00813000
*           CALL FREEACB;           /* FREE ACB CORE                 */ 00814000
@RC00437 BAL   @14,FREEACB                                         0458 00815000
*         END;                                                     0459 00816000
*       ELSE                        /* GET ACB CORE FAILED           */ 00817000
*         DO;                                                      0460 00818000
         B     @RC00434                                            0460 00819000
@RF00434 DS    0H                                                  0461 00820000
*           LCADDTWO=DDALNM;        /* ALLOCATED DDNAME              */ 00821000
         MVC   LCADDTWO(8,LCAPTR),DDALNM                           0461 00822000
*           CALL UNALLOC;           /* UNALLOCATE NEW DATA SET       */ 00823000
         BAL   @14,UNALLOC                                         0462 00824000
*           MSGONE=MSG6;            /* D/S SWITCH FAILED MSG         */ 00825000
         IC    @03,MSG6                                            0463 00826000
         STC   @03,MSGONE                                          0463 00827000
*           LOGLOOP='00'X;          /* END POST/PUR LOOP             */ 00828000
         MVI   LOGLOOP,X'00'                                       0464 00829000
*         END;                                                     0465 00830000
*     END;                                                         0466 00831000
*   ELSE                            /* ALLOCATION FAILED             */ 00832000
*     DO;                                                          0467 00833000
         B     @RC00431                                            0467 00834000
@RF00431 DS    0H                                                  0468 00835000
*       MSGONE=MSG6;                /* D/S SWITCH FAILED MSG         */ 00836000
         IC    @03,MSG6                                            0468 00837000
         STC   @03,MSGONE                                          0468 00838000
*       LOGLOOP='00'X;              /* END POST/PUT LOOP             */ 00839000
         MVI   LOGLOOP,X'00'                                       0469 00840000
*     END;                                                         0470 00841000
*   POSTWLOG='0'B;                  /* ZERO POSTED BIT IN LCA        */ 00842000
@RC00431 L     @03,POSTWLOG(,LCAPTR)                               0471 00843000
@GL00001 LR    @09,@03                                             0471 00844000
         N     @09,@CF02461                                        0471 00845000
         CS    @03,@09,POSTWLOG(LCAPTR)                            0471 00846000
         BNE   @GL00001                                            0471 00847000
*   IF MSGONE^=MSG6 THEN            /* UNSUCCESSFUL D/S SWITCH       */ 00848000
         CLC   MSGONE(1),MSG6                                      0472 00849000
         BE    @RF00472                                            0472 00850000
*     DO;                                                          0473 00851000
*       RFY                                                        0474 00852000
*        (WREG1,                                                   0474 00853000
*         WREG2) RESTRICTED;                                       0474 00854000
*       WREG1=LCALIMCT;             /* WTL COUNT FOR WRITELOG        */ 00855000
         L     WREG1,LCALIMCT(,LCAPTR)                             0475 00856000
*REDO:                                                             0476 00857000
*       WREG2=0000;                 /* SET ZERO VALUE FOR WTL COUNT  */ 00858000
REDO     SLR   WREG2,WREG2                                         0476 00859000
*       CS(WREG1,WREG2,LCALIMCT);   /* SWAP IN ZERO VALUE            */ 00860000
         CS    WREG1,@07,LCALIMCT(LCAPTR)                          0477 00861000
*       BC(7,REDO);                 /* RE-PROCESS IF COMPARE FAILS   */ 00862000
         BC    7,REDO                                              0478 00863000
*       RFY                                                        0479 00864000
*        (WREG1,                                                   0479 00865000
*         WREG2) UNRESTRICTED;                                     0479 00866000
*     END;                                                         0480 00867000
*   NUMMSGS=1;                      /* WTOS TO ISSUE                 */ 00868000
@RF00472 MVI   NUMMSGS,X'01'                                       0481 00869000
*   REG1=ADDR(MSGPARM);             /* SET PARM REG                  */ 00870000
         LA    REG1,MSGPARM                                        0482 00871000
*   CALL IEEMB807;                  /* ISSUE WTO MSG                 */ 00872000
         L     @15,@CV02192                                        0483 00873000
         BALR  @14,@15                                             0483 00874000
*   REG14=REGSAV2;                  /* RESTORE RETURN ADDRESS        */ 00875000
         L     REG14,REGSAV2                                       0484 00876000
*   END SWITCHDS;                                                  0485 00877000
@EL00007 DS    0H                                                  0485 00878000
@EF00007 DS    0H                                                  0485 00879000
@ER00007 BR    @14                                                 0485 00880000
*LOGTERM:                                                          0486 00881000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0486 00882000
*                                                                  0486 00883000
LOGTERM  DS    0H                                                  0487 00884000
*   /*****************************************************************/ 00885000
*   /*                                                               */ 00886000
*   /* IF AT LOG CLOSE, THE ECB (MSLGCLOZ) CONTAINS          @YM06135*/ 00887000
*   /* A COMPLETION CODE OF FOUR, A MESSAGE (IEE775I)        @YM06135*/ 00888000
*   /* IS ISSUED TO INFORM THE OPERATOR THE LOG              @YM06135*/ 00889000
*   /* TERMINATION IS IN PROCESS BECAUSE BUFFER CORE         @YM06135*/ 00890000
*   /* IS NOT AVAILABLE.                                     @YM06135*/ 00891000
*   /* LOG TASK INACTIVE ECB POSTED - CALL SHUTDOWN TO SET LOG TASK  */ 00892000
*   /* NOT ACTIVE. CALL WTLREC RTN TO WRITE THE WTL QUEUE TO THE LOG */ 00893000
*   /* DATA SET. UPDATE ACB AND DDNAME PTRS AND CLOSE AND UNALLOCATE */ 00894000
*   /* THE LOG DATA SET. SET MSG CODE TO REFLECT RETURN CODE FROM    */ 00895000
*   /* UNALLOC RTN. CALL WTLCLOSE RTN TO FREE ALL CONTROL BLOCKS AND */ 00896000
*   /* AREAS CREATED BY LOG TASK. RETURN TO TOP OF MAIN-LINE RTN TO  */ 00897000
*   /* WAIT ON ECB MSLGSTRT FOR A WRITELOG START COMMAND TO BE       */ 00898000
*   /* ISSUED.                                                       */ 00899000
*   /*                                                               */ 00900000
*   /*****************************************************************/ 00901000
*                                                                  0487 00902000
*   DCL                                                            0487 00903000
*     1 ECBMAP DEF(MSLGCLOZ),       /* COMPLETION CODE MAPP  @YM06135*/ 00904000
*      2 * CHAR(3),                                                0487 00905000
*      2 ECBCODE CHAR(1);                                          0487 00906000
*   REGSAV2=REG14;                  /* SAVE RETURN ADDRESS           */ 00907000
         ST    REG14,REGSAV2                                       0488 00908000
*   IF ECBCODE=4 THEN               /* SVC BLDCPOOL FAIL     @YM06135*/ 00909000
         L     @03,CVTPTR                                          0489 00910000
         L     @03,CVTMSER(,@03)                                   0489 00911000
         CLI   ECBCODE(@03),4                                      0489 00912000
         BNE   @RF00489                                            0489 00913000
*     DO;                                                          0490 00914000
*       MSGONE=MSG8;                /* CORE UNAVAILABLE MSG  @YM06135*/ 00915000
         IC    @03,MSG8                                            0491 00916000
         STC   @03,MSGONE                                          0491 00917000
*       NUMMSGS=1;                  /* WTOS TO ISSUE         @YM06135*/ 00918000
         MVI   NUMMSGS,X'01'                                       0492 00919000
*       REG1=ADDR(MSGPARM);         /* SET PARM REG          @YM06135*/ 00920000
         LA    REG1,MSGPARM                                        0493 00921000
*       CALL IEEMB807;              /* ISSUE WTO MSG         @YM06135*/ 00922000
         L     @15,@CV02192                                        0494 00923000
         BALR  @14,@15                                             0494 00924000
*     END;                                                         0495 00925000
*   MSLGCLOZ=0000;                  /* CLEAR CLOSE ECB               */ 00926000
@RF00489 L     @03,CVTPTR                                          0496 00927000
         L     @03,CVTMSER(,@03)                                   0496 00928000
         SLR   @09,@09                                             0496 00929000
         ST    @09,MSLGCLOZ(,@03)                                  0496 00930000
*   CALL SHUTDOWN;                  /* SET LOG NOT ACTIVE            */ 00931000
         BAL   @14,SHUTDOWN                                        0497 00932000
*   REG15=4;                        /* SET CONTROL SW FOR PUT RTN    */ 00933000
         LA    REG15,4                                             0498 00934000
*   DO WHILE(REG15^=0000);                                         0499 00935000
         B     @DE00499                                            0499 00936000
@DL00499 DS    0H                                                  0500 00937000
*     CALL WTLREC;                  /* PUT WTL QUEUE TO LOG DATA SET */ 00938000
         BAL   @14,WTLREC                                          0500 00939000
*   END;                                                           0501 00940000
@DE00499 LTR   REG15,REG15                                         0501 00941000
         BNZ   @DL00499                                            0501 00942000
*   LCADDTWO=LCADDONE;              /* SET DD PTR FOR UNALLOCATE     */ 00943000
         MVC   LCADDTWO(8,LCAPTR),LCADDONE(LCAPTR)                 0502 00944000
*   LCACBTWO=LCACBONE;              /* SET ACB PTR FOR CLOSERTN      */ 00945000
         L     @03,LCACBONE(,LCAPTR)                               0503 00946000
         ST    @03,LCACBTWO(,LCAPTR)                               0503 00947000
*   ACBPTR=LCACBONE;                /* SET ACB PTR FOR FREE          */ 00948000
         ST    @03,ACBPTR                                          0504 00949000
*   CALL CLOSRTN;                   /* CLOSE LOG DATA SET            */ 00950000
         BAL   @14,CLOSRTN                                         0505 00951000
*   CALL UNALLOC;                   /* UNALLOCATE LOG DATA SET       */ 00952000
         BAL   @14,UNALLOC                                         0506 00953000
*   IF REG15=0000 THEN              /* UNALLOCATE SUCCESSFUL ?       */ 00954000
         LTR   REG15,REG15                                         0507 00955000
         BNZ   @RF00507                                            0507 00956000
*     MSGONE=MSG1;                  /* SET QUEUED DATA MSG CODE      */ 00957000
         IC    @03,MSG1                                            0508 00958000
         STC   @03,MSGONE                                          0508 00959000
*   ELSE                                                           0509 00960000
*     MSGONE=MSG2;                  /* NO, SET 'DATA LOSS' MSG CODE  */ 00961000
         B     @RC00507                                            0509 00962000
@RF00507 IC    @03,MSG2                                            0509 00963000
         STC   @03,MSGONE                                          0509 00964000
*   MSGCLS=MSLOGCLS;                /* SET OUTPUT CLASS VALUE        */ 00965000
@RC00507 L     @03,CVTPTR                                          0510 00966000
         L     @03,CVTMSER(,@03)                                   0510 00967000
         IC    @09,MSLOGCLS(,@03)                                  0510 00968000
         STC   @09,MSGCLS                                          0510 00969000
*   CALL WTLCLOSE;                  /* CLEAN UP LOG CORE             */ 00970000
         BAL   @14,WTLCLOSE                                        0511 00971000
*   LOGLOOP='00'X;                  /* EXIT FROM CHCKPOST LOOP       */ 00972000
         MVI   LOGLOOP,X'00'                                       0512 00973000
*   LOGWAIT='00'X;                  /* EXIT FROM MAIN-LINE WAIT LOOP */ 00974000
         MVI   LOGWAIT,X'00'                                       0513 00975000
*   REG14=REGSAV2;                  /* RESTORE RETURN ADDRESS        */ 00976000
         L     REG14,REGSAV2                                       0514 00977000
*   END LOGTERM;                                                   0515 00978000
@EL00008 DS    0H                                                  0515 00979000
@EF00008 DS    0H                                                  0515 00980000
@ER00008 BR    @14                                                 0515 00981000
*SHUTDOWN:                                                         0516 00982000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0516 00983000
*                                                                  0516 00984000
SHUTDOWN DS    0H                                                  0517 00985000
*   /*****************************************************************/ 00986000
*   /*                                                               */ 00987000
*   /* THE SYSTEM LOG FUNCTION IS BEING MADE INACTIVE DUE TO A       */ 00988000
*   /* WRITELOG CLOSE COMMAND -(NORMAL) , OR AN ABEND IN THE LOG TASK*/ 00989000
*   /* -(ABNORMAL,VIA ESTAE PROCESSING). SET LCA PTR IN BASEA(BALOG) */ 00990000
*   /* TO ZERO , SET COMM-TASK INTERFACE FIELDS , POST COMM-TASK THAT*/ 00991000
*   /* THE LOG IS INACTIVE. IF ANY LOG SVC(36)S ARE STILL EXECUTING, */ 00992000
*   /* ISSUE A WAIT ON MULTIPLE LOG ECBS . THE LAST SVC(36) WILL POST*/ 00993000
*   /* THE LOG TO CLOSE. RETURN TO CALLER.                           */ 00994000
*   /*                                                               */ 00995000
*   /*****************************************************************/ 00996000
*                                                                  0517 00997000
*   DCL                                                            0517 00998000
*     SVCWAIT PTR(8);               /* SWITCH FOR WAIT LOOP          */ 00999000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 01000000
         LR    REGSAV,REG14                                        0518 01001000
*   BALOG=0000;                     /* ZERO LCA PTR                  */ 01002000
         L     @09,CVTPTR                                          0519 01003000
         L     @08,CVTMSER(,@09)                                   0519 01004000
         SLR   @07,@07                                             0519 01005000
         ST    @07,BALOG(,@08)                                     0519 01006000
*   MSLOGTHD='1'B;                  /* SET COMM-TASK INDICATOR ON    */ 01007000
         OI    MSLOGTHD(@08),B'01000000'                           0520 01008000
*   IF UCMSYSG='1'B THEN            /* LOG = HARDCOPY                */ 01009000
         L     @09,UCMBASPT(,@09)                                  0521 01010000
         LR    @08,@09                                             0521 01011000
         AL    @08,@CF02465                                        0521 01012000
         L     @08,UCMMCSPT(,@08)                                  0521 01013000
         TM    UCMSYSG(@08),B'00000010'                            0521 01014000
         BNO   @RF00521                                            0521 01015000
*     DO;                                                          0522 01016000
*                                                                  0522 01017000
*       /*************************************************************/ 01018000
*       /*                                                           */ 01019000
*       /* COMM-TASK MUST BE MADE AWARE THAT THE LOG IS CLOSING SET  */ 01020000
*       /* UP AND POST COMM-TASK ATTENTION ECB.                      */ 01021000
*       /*                                                           */ 01022000
*       /*************************************************************/ 01023000
*                                                                  0523 01024000
*       POSTCORE=POSTLIST;          /* MOVE LIST TO GOTTEN CORE      */ 01025000
         MVC   POSTCORE(12),POSTLIST                               0523 01026000
*       UCMSYSO='1'B;               /* SET LOG ATTENTION BIT         */ 01027000
         OI    UCMSYSO(@08),B'00000010'                            0524 01028000
*       REG1=ADDR(POSTCORE);        /* ADDR OF POST PARM LIST        */ 01029000
         LA    REG1,POSTCORE                                       0525 01030000
*       REG2=UCMASCB;               /* COMM-TASK ASCB TO REG         */ 01031000
         L     REG2,UCMASCB(,@09)                                  0526 01032000
*       ECBAD=ADDR(UCMAECB);        /* COMM-TASK ECB ADDR            */ 01033000
         LA    @09,UCMAECB(,@09)                                   0527 01034000
         ST    @09,ECBAD                                           0527 01035000
*       GEN( POST ASCB=(2),ERRET=CVTBRET,MF=(E,(1)));              0528 01036000
          POST ASCB=(2),ERRET=CVTBRET,MF=(E,(1))                        01037000
*     END;                                                         0529 01038000
*   IF MSLOGSVC^=0000 THEN          /* SVCS STILL EXECUTING          */ 01039000
@RF00521 L     @09,CVTPTR                                          0530 01040000
         L     @09,CVTMSER(,@09)                                   0530 01041000
         L     @09,MSLOGSVC(,@09)                                  0530 01042000
         LTR   @09,@09                                             0530 01043000
         BZ    @RF00530                                            0530 01044000
*     DO;                                                          0531 01045000
*       SVCWAIT=1;                  /* SET LOOP VALUE                */ 01046000
         LA    SVCWAIT,1                                           0532 01047000
*       DO WHILE(SVCWAIT=1);                                       0533 01048000
         B     @DE00533                                            0533 01049000
@DL00533 DS    0H                                                  0534 01050000
*         REG1=ADDR(LCAELST);       /* ADDR OF LOG ECB LIST          */ 01051000
         LA    REG1,LCAELST(,LCAPTR)                               0534 01052000
*         GEN( WAIT  ECBLIST=(1));  /* WAIT ON ALL ECBS              */ 01053000
          WAIT  ECBLIST=(1)                                             01054000
*         IF MSWTRP^='1'B THEN      /* NOT POSTED FOR PUT RTN        */ 01055000
         L     @05,CVTPTR                                          0536 01056000
         L     @05,CVTMSER(,@05)                                   0536 01057000
         TM    MSWTRP(@05),B'01000000'                             0536 01058000
         BO    @RF00536                                            0536 01059000
*           SVCWAIT='00'X;          /* EXIT FROM LOOP                */ 01060000
         SLR   SVCWAIT,SVCWAIT                                     0537 01061000
*         ELSE                                                     0538 01062000
*           MSWTRSW=MSWTRSW&'00111111'B;/* CLEAR POST BIT , RE-WAIT  */ 01063000
         B     @RC00536                                            0538 01064000
@RF00536 L     @05,CVTPTR                                          0538 01065000
         L     @05,CVTMSER(,@05)                                   0538 01066000
         LA    @09,63                                              0538 01067000
         SLR   @08,@08                                             0538 01068000
         IC    @08,MSWTRSW(,@05)                                   0538 01069000
         NR    @09,@08                                             0538 01070000
         STC   @09,MSWTRSW(,@05)                                   0538 01071000
*       END;                                                       0539 01072000
@RC00536 DS    0H                                                  0539 01073000
@DE00533 C     SVCWAIT,@CF00078                                    0539 01074000
         BE    @DL00533                                            0539 01075000
*     END;                                                         0540 01076000
*   REG14=REGSAV;                   /* RESTORE RETURN ADDRESS        */ 01077000
@RF00530 LR    REG14,REGSAV                                        0541 01078000
*   END SHUTDOWN;                                                  0542 01079000
@EL00009 DS    0H                                                  0542 01080000
@EF00009 DS    0H                                                  0542 01081000
@ER00009 BR    @14                                                 0542 01082000
*POOLFREE:                                                         0543 01083000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0543 01084000
*                                                                  0543 01085000
POOLFREE DS    0H                                                  0544 01086000
*   /*****************************************************************/ 01087000
*   /*                                                               */ 01088000
*   /* ENTRY HERE IS TO FREE CORE GOTTEN VIA BLDCPOOL MACRO FOR THE  */ 01089000
*   /* WTL QUEUE POOL.                                               */ 01090000
*   /*                                                               */ 01091000
*   /*****************************************************************/ 01092000
*                                                                  0544 01093000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 01094000
         LR    REGSAV,REG14                                        0544 01095000
*   REG1=LCACPID;                   /* SET UNIQUE POOL ID            */ 01096000
         L     REG1,LCACPID(,LCAPTR)                               0545 01097000
*   GEN( DELCPOOL  CPID=(1),BRANCH=YES);                           0546 01098000
          DELCPOOL  CPID=(1),BRANCH=YES                                 01099000
*   REG14=REGSAV;                   /* RESTORE RETURN ADDRESS        */ 01100000
         LR    REG14,REGSAV                                        0547 01101000
