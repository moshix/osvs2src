*   REG2=UCMASCB;                   /* COMM-TASK ASCB PTR            */ 01363000
         L     REG2,UCMASCB(,@09)                                  0622 01364000
*   BALOG=LCAPTR;                   /* LCA PTR INTO BASEA    @YM08121*/ 01365000
