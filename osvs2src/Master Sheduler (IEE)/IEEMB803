*SWITCHDS:                                                         0422 00711000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0422 00712000
*                                                                  0422 00713000
SWITCHDS DS    0H                                                  0423 00714000
*   /*****************************************************************/ 00715000
*   /*                                                               */ 00716000
*   /* DATA SET SWITCH ECB POSTED- CALL ALLOCRTN TO GET A NEW LOG    */ 00717000
*   /* DATA SET. IF NOT SUCCESSFUL,SET 'SWITCH FAILED' MSG           */ 00718000
*   /* CODE,RETURN TO MULTIPLE WAIT IN MAIN-LINE. CALL NEWACB TO GET */ 00719000
*   /* ACB CORE ,CALL OPENRTN TO OPEN NEW LOG DATA SET. IF OPEN      */ 00720000
*   /* FAILS,FREE ACB CORE, CALL UNALLOC RTN TO FREE NEW DATA SET,   */ 00721000
*   /* SET 'SW FAIL' MSG CODE,RETURN TO MULTI WAIT IN MAIN-LINE.     */ 00722000
*   /* UPDATE ACB AND DATA SET PTRS, CALL CLOSRTN TO CLOSE 'OLD' LOG */ 00723000
*   /* DATA SET, UNALLOCATE 'OLD' DATA SET. IF UNALLOCATE FAILS SET  */ 00724000
*   /* 'DATA LOSS' MSG CODE, ELSE 'Q'ED MSG CODE,RETURN TO MULTI-WAIT*/ 00725000
*   /* IN MAIN-LINE.                                                 */ 00726000
*   /*                                                               */ 00727000
*   /*****************************************************************/ 00728000
*                                                                  0423 00729000
*   DCL                                                            0423 00730000
*     REDO LABEL;                   /* LABEL FOR 'CS' INSTRUCTION    */ 00731000
*   REGSAV2=REG14;                  /* SAVE RETURN ADDRESS           */ 00732000
         ST    REG14,REGSAV2                                       0424 00733000
*   MSLGWLOG=0000;                  /* CLEAR WRITELOG ECB            */ 00734000
         L     @03,CVTPTR                                          0425 00735000
         L     @03,CVTMSER(,@03)                                   0425 00736000
