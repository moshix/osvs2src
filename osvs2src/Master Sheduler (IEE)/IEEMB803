         SLR   @09,@09                                             0783 01923000
         ST    @09,ZEROPTR(,@03)                                   0783 01924000
*   LCAELCNT=LCAELCNT-(PREVCNT-1);  /* RE-SET WTL BUFFER COUNT       */ 01925000
         L     @03,PREVCNT(,@03)                                   0784 01926000
         BCTR  @03,0                                               0784 01927000
         LCR   @03,@03                                             0784 01928000
         AL    @03,LCAELCNT(,LCAPTR)                               0784 01929000
         ST    @03,LCAELCNT(,LCAPTR)                               0784 01930000
*   CALL WTLREC;                    /* PUT BUFFER TO LOG DATA SET    */ 01931000
         BAL   @14,WTLREC                                          0785 01932000
*   CALL POOLFREE;                  /* FREE LOG BUFFER POOL          */ 01933000
         BAL   @14,POOLFREE                                        0786 01934000
*   GEN( BLDCPOOL SP=231,CSIZE=140,POOLSIZ=4,AUTODEL=YES,BRANCH=YES);   01935000
          BLDCPOOL SP=231,CSIZE=140,POOLSIZ=4,AUTODEL=YES,BRANCH=YES    01936000
*                                   /* GET NEW LOG BUFFER POOL       */ 01937000
*   IF REG15=0000 THEN              /* GET SUCCESSFUL                */ 01938000
         LTR   REG15,REG15                                         0788 01939000
         BNZ   @RF00788                                            0788 01940000
*     LCACPID=REG0;                 /* SET POOL ID IN LCA            */ 01941000
         ST    REG0,LCACPID(,LCAPTR)                               0789 01942000
*   ELSE                            /* NOT SUCCESSFUL, ABEND LOG     */ 01943000
*     DO;                                                          0790 01944000
         B     @RC00788                                            0790 01945000
@RF00788 DS    0H                                                  0791 01946000
*       MSWTL='0'B;                 /* TURN OFF BUFFER RECURSION     */ 01947000
         L     @09,CVTPTR                                          0791 01948000
         L     @09,CVTMSER(,@09)                                   0791 01949000
         NI    MSWTL+136(@09),B'11011111'                          0791 01950000
*       LCASTAE='1'B;               /* SET LOG RECURSION ON          */ 01951000
         MVI   LCASTAE(LCAPTR),X'01'                               0792 01952000
*       LCAPOOLF='0'B;              /* INDICATE NO POOL TO FREE      */ 01953000
         NI    LCAPOOLF(LCAPTR),B'10111111'                        0793 01954000
*       BALOG=LCAPTR;               /* RE-SET LCA PTR FOR CLEAN-UP   */ 01955000
         ST    LCAPTR,BALOG(,@09)                                  0794 01956000
*       CALL ABENDRTN;              /* GO ISSUE ABEND. NO RETURN FROM   01957000
*                                      THIS CALLED RTN.      @ZA07784*/ 01958000
         BAL   @14,ABENDRTN                                        0795 01959000
*     END;                                                         0796 01960000
*   MSWTL='0'B;                     /* TURN OFF RECURSION            */ 01961000
@RC00788 L     @09,CVTPTR                                          0797 01962000
         L     @09,CVTMSER(,@09)                                   0797 01963000
         NI    MSWTL+136(@09),B'11011111'                          0797 01964000
*   BALOG=LCAPTR;                   /* RESTORE LCA PTR               */ 01965000
         ST    LCAPTR,BALOG(,@09)                                  0798 01966000
*   BALOGINT='1'B;                  /* SET ACTIVE BIT ON             */ 01967000
*                                                                  0799 01968000
         OI    BALOGINT(@09),B'10000000'                           0799 01969000
*   /*****************************************************************/ 01970000
*   /*                                                               */ 01971000
*   /* SET UP TO ISSUE WTO IEE041I LOG ACTIVE MSG                    */ 01972000
*   /*                                                               */ 01973000
*   /*****************************************************************/ 01974000
*                                                                  0800 01975000
*   MSGONE=MSG4;                    /* MSG CODE TO PARM LIST         */ 01976000
         IC    @09,MSG4                                            0800 01977000
