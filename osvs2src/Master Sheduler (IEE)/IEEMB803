*CHCKPOST:                                                         0400 00625000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0400 00626000
*                                                                  0400 00627000
CHCKPOST DS    0H                                                  0401 00628000
*   /*****************************************************************/ 00629000
*   /*                                                               */ 00630000
*   /* ENTRY HERE WHEN THE MULTIPLE ECB WAIT HAS BEEN POSTED TO - .  */ 00631000
*   /* SET THE LOG TASK INACTIVE - MSLGCLOZ ECB . . SWITCH THE LOG   */ 00632000
*   /* DATA SETS - MSLGWLOG ECB . . PUT WTL RECORDS TO LOG DATA SET -*/ 00633000
*   /* MSLGWTR ECB THE POST BIT OF THE ECBS IS TESTED AND THE        */ 00634000
*   /* REQUIRED PROCESSING IS PERFORMED TO SATISFY THE REQUEST. MSG  */ 00635000
*   /* MODULE IEEMB807 IS CALLED TO PROCESS MSG CODES AND ISSUE WTOS.*/ 00636000
*   /*                                                               */ 00637000
*   /*****************************************************************/ 00638000
*                                                                  0401 00639000
*   NUMMSGS='00'X;                  /* PRE-SET NULL MSG CODE         */ 00640000
         MVI   NUMMSGS,X'00'                                       0401 00641000
*   MAINSAV=REG14;                  /* SAVE RETURN ADDRESS           */ 00642000
         ST    REG14,MAINSAV                                       0402 00643000
*   LOGLOOP=1;                      /* SET SW FOR POST/PUT LOOP      */ 00644000
         MVI   LOGLOOP,X'01'                                       0403 00645000
*   DO WHILE(LOGLOOP=1);                                           0404 00646000
         B     @DE00404                                            0404 00647000
@DL00404 DS    0H                                                  0405 00648000
*     IF MSCLOSP^='1'B THEN         /* NOT WRITELOG CLOSE ECB        */ 00649000
         L     @03,CVTPTR                                          0405 00650000
