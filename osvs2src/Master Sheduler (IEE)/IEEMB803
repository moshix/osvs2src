*               END;                                               0271 00210000
*             ELSE                  /* DYNAMIC ALLOCATION FAILURE    */ 00211000
*               DO;                                                0272 00212000
         B     @RC00243                                            0272 00213000
@RF00243 DS    0H                                                  0273 00214000
*                 CALL SHUTDOWN;    /* SET LOG INACTIVE              */ 00215000
