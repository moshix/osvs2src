         LA    REG1,MSGPARM                                        0290 00253000
*     CALL IEEMB807;                /* ISSUE WTO                     */ 00254000
         L     @15,@CV02192                                        0291 00255000
         BALR  @14,@15                                             0291 00256000
*   END;                                                           0292 00257000
@DE00206 CLI   SPINSW,1                                            0292 00258000
         BE    @DL00206                                            0292 00259000
*WTLREC:                                                           0293 00260000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0293 00261000
*                                                                  0293 00262000
         B     @PB00002                                            0293 00263000
WTLREC   DS    0H                                                  0294 00264000
*   /*****************************************************************/ 00265000
*   /*                                                               */ 00266000
*   /* ENTRY IS TO SET UP THE WTL RECORD ELEMENTS IN CORRECT ORDER   */ 00267000
*   /* (FIRST IN / FIRST OUT -(FIFO)) FROM THE WTL QUEUE POOL WHICH  */ 00268000
*   /* WAS SET UP IN (LAST IN / FIRST OUT-(LIFO)) ORDER. EACH RECORD */ 00269000
*   /* ELEMENT HAS A BACKWARD PTR TO THE PREVIOUS ELEMENT ON THE WTL */ 00270000
*   /* POOL CHAIN, EXCEPT THE FIRST RECORD ELEMENT, WHICH HAS A ZERO */ 00271000
*   /* PTR FIELD (CHAINPTR). THE PTR TO THE WTL QUEUE CHAIN AND COUNT*/ 00272000
*   /* OF RECORD ELEMENTS ON THE CHAIN (LCAQHEAD) IS LOADED INTO AN  */ 00273000
*   /* EVEN/ODD PAIR OF REGS, AND THE VALUES ARE PROPAGATED TO       */ 00274000
*   /* ANOTHER EVEN/ODD PAIR OF REGS WITH THE COUNT VALUE DECREMENTED*/ 00275000
*   /* BY ONE. THE NEW VALUES ARE STORED BACK TO LCAQHEAD USING      */ 00276000
*   /* COMPARE DOUBLE AND SWAP (CDS) INSTRUCTION FOR SERIALIZATION OF*/ 00277000
*   /* THE FIELD. THE WTL POOL CHAIN IS SCANNED FOR A ZERO PTR TO    */ 00278000
*   /* NEXT ELEMENT. THIS IS THE RECORD ELEMENT THAT WILL BE PUT TO  */ 00279000
*   /* THE SYSTEM LOG DATA SET. THE BACKWARD PTR OF THE PREVIOUS     */ 00280000
*   /* RECORD ELEMENT IS SET TO ZERO AND THE PUT RTN PUTREC IS CALLED*/ 00281000
*   /* TO WRITE THE RECORD OUT. RETURN CODES (REG15) ARE DETERMINED  */ 00282000
*   /* BY THE VALUE OF THE LCAQHEAD COUNT FIELD. IF THE COUNT IS     */ 00283000
*   /* ZERO, REG15 IS SET ZERO, IF THE COUNT IS NOT ZERO, REG15 IS   */ 00284000
*   /* SET TO FOUR. RETURN CODES ARE PROCESSED BY CALLER.            */ 00285000
*   /*                                                               */ 00286000
*   /*****************************************************************/ 00287000
*                                                                  0294 00288000
*   DCL                                                            0294 00289000
*     AGAN LABEL;                   /* USED BY 'CDS'                 */ 00290000
*   RFY                                                            0295 00291000
*    (PR1ELCNT,                                                    0295 00292000
*     PR1ELAD,                                                     0295 00293000
*     PR2ELCNT,                                                    0295 00294000
*     PR2ELAD) RESTRICTED;                                         0295 00295000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 00296000
         LR    REGSAV,REG14                                        0296 00297000
*   RETCODE=4;                      /* SET LOOP SWITCH       @ZA07784*/ 00298000
         MVI   RETCODE,X'04'                                       0297 00299000
*   DO WHILE(RETCODE=4);            /* SET PUT LOOP          @ZA07784*/ 00300000
         B     @DE00298                                            0298 00301000
@DL00298 DS    0H                                                  0299 00302000
*     GEN( LM    PR1ELCNT,PR1ELAD,LCAQHEAD(LCAPTR));/* SET UP      0299 00303000
*                                      'CDS'REGS                     */ 00304000
          LM    PR1ELCNT,PR1ELAD,LCAQHEAD(LCAPTR)                       00305000
*     IF PR1ELCNT^=0000 THEN        /* ANY QUEUE ELEMENTS TO PUT OUT */ 00306000
         LTR   PR1ELCNT,PR1ELCNT                                   0300 00307000
         BZ    @RF00300                                            0300 00308000
*       DO;                                                        0301 00309000
*                                                                  0301 00310000
*         /***********************************************************/ 00311000
*         /*                                                         */ 00312000
*         /* RECORD ELEMENT CHAIN IS NOT EMPTY. SET UP TO DECREMENT  */ 00313000
*         /* THE ELEMENT COUNT -LCAQHEAD(LCAELCNT) IN REG6 (PR1ELCNT)*/ 00314000
*         /* USING 'CDS' FOR SERIALIZATION ON THE WTL QUEUE CHAIN PTR*/ 00315000
*         /* (LCAQHEAD)                                              */ 00316000
*         /*                                                         */ 00317000
*         /***********************************************************/ 00318000
*                                                                  0302 00319000
*AGAN:                                                             0302 00320000
*         PR2ELCNT=PR1ELCNT-1;      /* DECREMENT ELEMENT COUNT       */ 00321000
AGAN     LR    PR2ELCNT,PR1ELCNT                                   0302 00322000
         BCTR  PR2ELCNT,0                                          0302 00323000
*         PR2ELAD=PR1ELAD|REGMASK;  /* SET HI-BIT FLAG ON            */ 00324000
         LR    PR2ELAD,PR1ELAD                                     0303 00325000
         O     PR2ELAD,REGMASK                                     0303 00326000
*         IF PR2ELCNT=0000 THEN     /* THIS LAST ELEMENT ON QUEUE    */ 00327000
         LTR   PR2ELCNT,PR2ELCNT                                   0304 00328000
         BNZ   @RF00304                                            0304 00329000
*           PR2ELAD=0000;           /* ZERO CHAIN PTR                */ 00330000
         SLR   PR2ELAD,PR2ELAD                                     0305 00331000
*         CDS(PR1ELCNT,PR2ELCNT,LCAQHEAD);/* SWAP IN NEW VALUES      */ 00332000
@RF00304 CDS   PR1ELCNT,PR2ELCNT,LCAQHEAD(LCAPTR)                  0306 00333000
*         BC(7,AGAN);               /* RE-PROCESS IF COMPARE FAILED  */ 00334000
         BC    7,AGAN                                              0307 00335000
*         IF PR2ELCNT^=0000 THEN    /* NOT LAST ELEMENT ON QUEUE     */ 00336000
         LTR   PR2ELCNT,PR2ELCNT                                   0308 00337000
         BZ    @RF00308                                            0308 00338000
*           DO;                                                    0309 00339000
*                                                                  0309 00340000
*             /*******************************************************/ 00341000
*             /*                                                     */ 00342000
*             /* SCAN THE WTL QUEUE CHAIN FOR A ZERO PTR TO NEXT     */ 00343000
*             /* ELEMENT- (CHAINPTR). THE RECORD ELEMENT WITH THIS   */ 00344000
*             /* PTR IS THE LAST ELEMENT ON THE CHAIN AND WILL BE PUT*/ 00345000
*             /* TO THE LOG DATA SET THE (CHAINPTR) FIELD OF THE     */ 00346000
*             /* PREVIOUS ELEMENT WILL BE SET TO ZERO FOR THE NEXT   */ 00347000
*             /* QUEUE SCAN                                          */ 00348000
*             /*                                                     */ 00349000
*             /*******************************************************/ 00350000
*                                                                  0310 00351000
*             DO WHILE(CHAINPTR^=0000);/* SCAN FOR LAST ELEMENT      */ 00352000
         B     @DE00310                                            0310 00353000
@DL00310 DS    0H                                                  0311 00354000
*               PREVREG=PR1ELAD;    /* SAVE PREVIOUS ELEMENT PTR     */ 00355000
         ST    PR1ELAD,PREVREG                                     0311 00356000
*               PR1ELAD=CHAINPTR;   /* GET NEXT ELEMENT PTR          */ 00357000
         L     PR1ELAD,CHAINPTR(,PR1ELAD)                          0312 00358000
*               IF PR1ELAD^=0000 THEN/* NOT LAST WTL ELEMENT @ZA07784*/ 00359000
         LTR   PR1ELAD,PR1ELAD                                     0313 00360000
         BZ    @RF00313                                            0313 00361000
*                 DO;                                              0314 00362000
*                   IF PREVCNT^=SWITCHFD+1 THEN/* BAD CNT    @ZA07784*/ 00363000
         L     @09,PREVREG                                         0315 00364000
         LA    @08,1                                               0315 00365000
         AL    @08,SWITCHFD(,PR1ELAD)                              0315 00366000
         CL    @08,PREVCNT(,@09)                                   0315 00367000
         BE    @RF00315                                            0315 00368000
*                     IF MSWTL='1'B THEN/* IN RECURSION      @ZA07784*/ 00369000
         LA    @09,136                                             0316 00370000
         L     @08,CVTPTR                                          0316 00371000
         AL    @09,CVTMSER(,@08)                                   0316 00372000
         TM    MSWTL(@09),B'00100000'                              0316 00373000
         BNO   @RF00316                                            0316 00374000
*                       DO;                                        0317 00375000
*                         LCASTAE='1'B;/* SET LOG RECURS BIT @ZA07784*/ 00376000
         MVI   LCASTAE(LCAPTR),X'01'                               0318 00377000
*                         MSWTL='0'B;/* ZERO BUFFER RECURS   @ZA07784*/ 00378000
         NI    MSWTL(@09),B'11011111'                              0319 00379000
*                         CALL ABENDRTN;/* GO ISSUE ABEND. NO RETURN    00380000
*                                      FROM THIS CALLED RTN. @ZA07784*/ 00381000
         BAL   @14,ABENDRTN                                        0320 00382000
*                       END;                                       0321 00383000
*                     ELSE                                         0322 00384000
*                       CALL CHAINRTN;/* GO REPAIR CHAIN     @ZA07784*/ 00385000
         B     @RC00316                                            0322 00386000
@RF00316 BAL   @14,CHAINRTN                                        0322 00387000
*                   ELSE                                           0323 00388000
*                     ;                                            0323 00389000
@RF00315 DS    0H                                                  0324 00390000
*                 END;                                             0324 00391000
@RC00315 DS    0H                                                  0325 00392000
*             END;                                                 0325 00393000
@RF00313 DS    0H                                                  0325 00394000
@DE00310 SLR   @09,@09                                             0325 00395000
         C     @09,CHAINPTR(,PR1ELAD)                              0325 00396000
         BNE   @DL00310                                            0325 00397000
*             ZEROPTR=0000;         /* PREVIOUS ELEMENT PTR FIELD    */ 00398000
         L     @08,PREVREG                                         0326 00399000
         ST    @09,ZEROPTR(,@08)                                   0326 00400000
*           END;                                                   0327 00401000
*         ELSE                                                     0328 00402000
*           RETCODE='00'X;          /* SET LAST WTL INDICATOR        */ 00403000
         B     @RC00308                                            0328 00404000
