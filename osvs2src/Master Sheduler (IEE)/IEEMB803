         L     REG1,ACBPTR                                         0561 01154000
*   GEN( FREEMAIN R,LV=(0),A=(1));  /* FREE CORE                     */ 01155000
          FREEMAIN R,LV=(0),A=(1)                                       01156000
*   REG14=REGSAV;                   /* RESTORE RETURN ADDRESS        */ 01157000
         LR    REG14,REGSAV                                        0563 01158000
*   END FREEACB;                                                   0564 01159000
@EL00012 DS    0H                                                  0564 01160000
@EF00012 DS    0H                                                  0564 01161000
@ER00012 BR    @14                                                 0564 01162000
*ACBINIT:                                                          0565 01163000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0565 01164000
*                                                                  0565 01165000
ACBINIT  DS    0H                                                  0566 01166000
*   /*****************************************************************/ 01167000
*   /*                                                               */ 01168000
*   /* THE ACCESS CONTROL BLOCK -ACB- IS USED BY THE OPEN MACRO TO   */ 01169000
*   /* OPEN AN ALLOCATED LOG DATA SET. ENTRY IS TO GET AND INITIALIZE*/ 01170000
*   /* CORE FOR AN ACB CONTROL BLOCK                                 */ 01171000
*   /*                                                               */ 01172000
*   /*****************************************************************/ 01173000
*                                                                  0566 01174000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 01175000
         LR    REGSAV,REG14                                        0566 01176000
*   REG0=LENACB2;                   /* LENGTH OF CORE NEEDED         */ 01177000
         L     REG0,LENACB2                                        0567 01178000
*   REG0=REG0|SPNFLD;               /* SUBPOOL = 231                 */ 01179000
         O     REG0,SPNFLD                                         0568 01180000
*   GEN( GETMAIN R,LV=(0));         /* GET CORE                      */ 01181000
          GETMAIN R,LV=(0)                                              01182000
*   ACBPTR=REG1;                    /* SAVE ACB ADDRESS              */ 01183000
         ST    REG1,ACBPTR                                         0570 01184000
*   LCAACBF='1'B;                   /* SET ACB CORE GOTTEN BIT       */ 01185000
         OI    LCAACBF(LCAPTR),B'00000100'                         0571 01186000
*   ACBCORE(1:LENACB2)=ACBLIST(1:LENACB2);/* MOVE IN ACB             */ 01187000
         L     @09,ACBPTR                                          0572 01188000
         L     @08,LENACB2                                         0572 01189000
         BCTR  @08,0                                               0572 01190000
         EX    @08,@SM02467                                        0572 01191000
*   REG14=REGSAV;                   /* RESTORE RETURN ADDRESS        */ 01192000
         LR    REG14,REGSAV                                        0573 01193000
*   END ACBINIT;                                                   0574 01194000
@EL00013 DS    0H                                                  0574 01195000
@EF00013 DS    0H                                                  0574 01196000
@ER00013 BR    @14                                                 0574 01197000
*NEWACB:                                                           0575 01198000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0575 01199000
*                                                                  0575 01200000
NEWACB   DS    0H                                                  0576 01201000
*   /*****************************************************************/ 01202000
*   /*                                                               */ 01203000
*   /* ENTRY HERE IS FROM SUB-RTN SWITCHDS TO GET CORE FOR A NEW ACB */ 01204000
*   /* CONTROL BLOCK REQUIRED TO PERFORM A LOG DATA SET SWITCH. THE  */ 01205000
*   /* CORE IS GOTTEN VIA A CONDITIONAL GETMAIN MACRO IN SP 231 IF   */ 01206000
*   /* GETMAIN IS SUCCESSFUL (REG15 = ZERO), THE ACBPTR FIELD IS     */ 01207000
*   /* INITIALIZED WITH THE GOTTEN CORE ADDRESS AND THE LIST FORM OF */ 01208000
*   /* THE ACB IS MOVED INTO CORE . IF GETMAIN FAILS (REG15 = FOUR), */ 01209000
*   /* NO FURTHER PROCESSING TAKES PLACE IN THIS RTN. RETURN TO      */ 01210000
*   /* CALLER TO PROCESS THE REG15 RETURN CODE.                      */ 01211000
*   /*                                                               */ 01212000
*   /*****************************************************************/ 01213000
*                                                                  0576 01214000
*   DCL                                                            0576 01215000
*     GETLIST CHAR(*) GEND;         /* LIST FORM OF GETMAIN          */ 01216000
*   DCL                                                            0577 01217000
*     GETEND GEND;                  /* END OF LIST INDICATOR         */ 01218000
*   DCL                                                            0578 01219000
*     GETLEN PTR(8) INIT(ADDR(GETEND)-ADDR(GETLIST));/* LIST LENGTH  */ 01220000
*   DCL                                                            0579 01221000
*     GETADPTR PTR(31);             /* GOTTEN CORE ADDRESS           */ 01222000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 01223000
         LR    REGSAV,REG14                                        0580 01224000
*   RFY                                                            0581 01225000
*    (REG4) RESTRICTED;                                            0581 01226000
*   REG1=ADDR(WORKAREA);            /* CORE FOR GETMAIN LIST FORM    */ 01227000
         LA    REG1,WORKAREA                                       0582 01228000
*   WORKAREA(1:GETLEN)=GETLIST(1:GETLEN);/* MOVE IN LIST FORM        */ 01229000
         SLR   @05,@05                                             0583 01230000
         IC    @05,GETLEN                                          0583 01231000
         BCTR  @05,0                                               0583 01232000
         EX    @05,@SM02469                                        0583 01233000
*   REG4=LENACB2;                   /* LENGTH OF CORE NEEDED         */ 01234000
         L     REG4,LENACB2                                        0584 01235000
*   GEN( GETMAIN  LV=(4),A=GETADPTR,MF=(E,(1)));/* ISSUE GETMAIN     */ 01236000
          GETMAIN  LV=(4),A=GETADPTR,MF=(E,(1))                         01237000
*   IF REG15=0000 THEN              /* GETMAIN SUCCESSFUL            */ 01238000
         LTR   REG15,REG15                                         0586 01239000
         BNZ   @RF00586                                            0586 01240000
*     DO;                                                          0587 01241000
*       ACBPTR=GETADPTR;            /* INITIALIZE NEW CORE ADDRESS   */ 01242000
         L     @05,GETADPTR                                        0588 01243000
         ST    @05,ACBPTR                                          0588 01244000
*       ACBCORE(1:LENACB2)=ACBLIST(1:LENACB2);/* INITIALIZE NEW ACB     01245000
*                                      FROM LIST                     */ 01246000
         L     @09,LENACB2                                         0589 01247000
         BCTR  @09,0                                               0589 01248000
         EX    @09,@SM02471                                        0589 01249000
*     END;                                                         0590 01250000
*   REG14=REGSAV;                   /* RESTORE RETURN ADDRESS        */ 01251000
@RF00586 LR    REG14,REGSAV                                        0591 01252000
*   RFY                                                            0592 01253000
*    (REG4) UNRESTRICTED;                                          0592 01254000
*   END NEWACB;                                                    0593 01255000
@EL00014 DS    0H                                                  0593 01256000
@EF00014 DS    0H                                                  0593 01257000
@ER00014 BR    @14                                                 0593 01258000
*OPENRTN:                                                          0594 01259000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0594 01260000
*                                                                  0594 01261000
OPENRTN  DS    0H                                                  0595 01262000
*   /*****************************************************************/ 01263000
*   /*                                                               */ 01264000
*   /* PROCESS THE ALLOCATED LOG DATA SET FOR OPEN VIA ACB . ACB CORE*/ 01265000
*   /* IS INITIALIZED FROM ITS LIST FORM , ACB PTR IS PUT IN REG2 ,  */ 01266000
*   /* THE DATA SET DDNAME -(DDALNM)- IS STORED IN ACB , AND THE OPEN*/ 01267000
*   /* MACRO IS ISSUED. IF OPEN FAILS , SET REG15 RETURN CODE = FOUR.*/ 01268000
*   /* ELSE INITIALIZE THE RPLDACB FIELD IN THE RPL CONTROL BLK.     */ 01269000
*   /*                                                               */ 01270000
*   /*****************************************************************/ 01271000
*                                                                  0595 01272000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 01273000
         LR    REGSAV,REG14                                        0595 01274000
*   REG1=ADDR(OPENCORE);            /* PTR TO OPEN MACRO             */ 01275000
         LA    REG1,OPENCORE                                       0596 01276000
*   REG2=ACBPTR;                    /* PTR TO ACB TO OPEN            */ 01277000
         L     @09,ACBPTR                                          0597 01278000
         LR    REG2,@09                                            0597 01279000
*   OPENCORE(1:OPENLEN2)=OPENLIST(1:OPENLEN2);/* MOVE IN LIST FORM   */ 01280000
         L     @08,OPENLEN2                                        0598 01281000
         BCTR  @08,0                                               0598 01282000
         EX    @08,@SM02473                                        0598 01283000
*   ACBDDNM=DDALNM;                 /* LOG DATA SET DDNAME TO ACB    */ 01284000
         MVC   ACBDDNM(8,@09),DDALNM                               0599 01285000
*   GEN( OPEN ((2)),MF=(E,(1)));    /* OPEN THE ACB                  */ 01286000
          OPEN ((2)),MF=(E,(1))                                         01287000
*   IF ACBOPEN^='1'B THEN           /* OPEN FAILED                   */ 01288000
         L     @09,ACBPTR                                          0601 01289000
         TM    ACBOPEN(@09),B'00010000'                            0601 01290000
         BO    @RF00601                                            0601 01291000
*     REG15=4;                      /* SET FAILED RETURN CODE        */ 01292000
         LA    REG15,4                                             0602 01293000
*   ELSE                                                           0603 01294000
*     DO;                                                          0603 01295000
         B     @RC00601                                            0603 01296000
@RF00601 DS    0H                                                  0604 01297000
*       RPLDACB=ACBPTR;             /* ACB PTR INTO RPL FIELD        */ 01298000
         L     @09,LCARPLP(,LCAPTR)                                0604 01299000
         L     @08,ACBPTR                                          0604 01300000
         ST    @08,RPLDACB(,@09)                                   0604 01301000
*       REG15=0000;                 /* SET SUCCESSFUL RETURN CODE    */ 01302000
         SLR   REG15,REG15                                         0605 01303000
*     END;                                                         0606 01304000
*   REG14=REGSAV;                   /* RESTORE RETURN ADDRESS        */ 01305000
@RC00601 LR    REG14,REGSAV                                        0607 01306000
*   END OPENRTN;                                                   0608 01307000
@EL00015 DS    0H                                                  0608 01308000
@EF00015 DS    0H                                                  0608 01309000
@ER00015 BR    @14                                                 0608 01310000
*NEWLOG:                                                           0609 01311000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0609 01312000
*                                                                  0609 01313000
NEWLOG   DS    0H                                                  0610 01314000
*   /*****************************************************************/ 01315000
*   /*                                                               */ 01316000
*   /* ENTRY HERE IS FOR A ZERO LCA PTR IN BASEA(BALOG) FIELD. FOR   */ 01317000
*   /* ENTRY AT IPL , IF THE LOG IS HARDCOPY THE POSTED BIT IS TURNED*/ 01318000
*   /* ON TO ALLOW COMM-TASK TO ISSUE WTLS TO LOG. REQUIRED BITS ARE */ 01319000
*   /* SET , COMM-TASK ATTENTION ECB IS POSTED , AND THE SYSTEM LOG  */ 01320000
*   /* LCA PTR IS INITIALIZED - IEEBASEA(BALOG). RETURN TO CALLER    */ 01321000
*   /*                                                               */ 01322000
*   /*****************************************************************/ 01323000
*                                                                  0610 01324000
*   MAINSAV=REG14;                  /* SAVE RETURN ADDRESS           */ 01325000
         ST    REG14,MAINSAV                                       0610 01326000
*   IF UCMRQLM1^='00'X THEN         /* THIS IS IPL TIME              */ 01327000
         L     @03,CVTPTR                                          0611 01328000
         L     @03,UCMBASPT(,@03)                                  0611 01329000
         CLI   UCMRQLM1(@03),0                                     0611 01330000
         BE    @RF00611                                            0611 01331000
*     DO;                                                          0612 01332000
*       UCMRQLM1='00'X;             /* ZERO FIELD                    */ 01333000
         MVI   UCMRQLM1(@03),X'00'                                 0613 01334000
*       IF UCMSYSG='1'B THEN        /* LOG = HARDCOPY                */ 01335000
         AL    @03,@CF02465                                        0614 01336000
         L     @03,UCMMCSPT(,@03)                                  0614 01337000
         TM    UCMSYSG(@03),B'00000010'                            0614 01338000
         BNO   @RF00614                                            0614 01339000
*         UCMNPECB=UCMNPECB|NIPSW;  /* SET POST BIT IN DUMMY ECB     */ 01340000
         L     @09,UCMNPECB(,@03)                                  0615 01341000
         O     @09,NIPSW                                           0615 01342000
         ST    @09,UCMNPECB(,@03)                                  0615 01343000
*     END;                                                         0616 01344000
@RF00614 DS    0H                                                  0617 01345000
*   UCMSYSO='1'B;                   /* LOG ATTENTION BIT             */ 01346000
@RF00611 L     @03,CVTPTR                                          0617 01347000
         L     @09,UCMBASPT(,@03)                                  0617 01348000
         LR    @08,@09                                             0617 01349000
         AL    @08,@CF02465                                        0617 01350000
         L     @08,UCMMCSPT(,@08)                                  0617 01351000
         OI    UCMSYSO(@08),B'00000010'                            0617 01352000
*   MSLOGTHD='0'B;                  /* LOG ACTIVE BIT                */ 01353000
         L     @03,CVTMSER(,@03)                                   0618 01354000
         NI    MSLOGTHD(@03),B'10111111'                           0618 01355000
*   POSTCORE=POSTLIST;              /* MOVE LIST TO GOTTEN CORE      */ 01356000
         MVC   POSTCORE(12),POSTLIST                               0619 01357000
*   REG1=ADDR(POSTCORE);            /* ADDR OF POST PARM LIST        */ 01358000
         LA    REG1,POSTCORE                                       0620 01359000
*   ECBAD=ADDR(UCMAECB);            /* COMM-TASK ECB ADDR TO LIST    */ 01360000
         LA    @08,UCMAECB(,@09)                                   0621 01361000
