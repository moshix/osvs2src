*   REG14=CHAINSAV;                 /* RESTORE CALLERS ADDRESS       */ 01988000
         L     REG14,CHAINSAV                                      0805 01989000
*   END CHAINRTN;                   /* RETURN                        */ 01990000
@EL00024 DS    0H                                                  0806 01991000
@EF00024 DS    0H                                                  0806 01992000
@ER00024 BR    @14                                                 0806 01993000
*ABENDRTN:                                                         0807 01994000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0807 01995000
*                                                                  0807 01996000
ABENDRTN DS    0H                                                  0808 01997000
*   /*****************************************************************/ 01998000
*   /*                                                               */ 01999000
*   /* ENTRY HERE IS TO ISSUE AN ABEND FOR THE LOG FUNCTION. CALLERS */ 02000000
*   /* OF THIS RTN ARE SUB-RTNS WTLREC FOR RECURSIVE BAD CHAIN       */ 02001000
*   /* PROBLEM, PUTREC FOR NON-ZERO RETURN CODE FROM PUT MACRO, AND  */ 02002000
*   /* CHAINRTN WHEN IT FAILS TO RE-ACQUIRE A LOG BUFFER POOL VIA THE*/ 02003000
*   /* BLDCPOOL MACRO. THIS SUB-RTN WAS ADDED AS A RESULT OF APAR    */ 02004000
*   /* OZ07784.                                                      */ 02005000
*   /*                                                               */ 02006000
*   /*****************************************************************/ 02007000
*                                                                  0808 02008000
*   MSLGWTR=0000;                   /* CLEAR WRITER ECB              */ 02009000
         L     @07,CVTPTR                                          0808 02010000
         L     @07,CVTMSER(,@07)                                   0808 02011000
         SLR   @09,@09                                             0808 02012000
         ST    @09,MSLGWTR(,@07)                                   0808 02013000
*   LOGLOOP='00'X;                  /* CLEAR CALLER LOOP SWITCH      */ 02014000
         MVI   LOGLOOP,X'00'                                       0809 02015000
*   MSLOGDAR='1'B;                  /* SET LOG ABEND SWITCH          */ 02016000
         OI    MSLOGDAR(@07),B'00010000'                           0810 02017000
*   CALL DUMPRTN;                   /* DUMP LOG CORE AREAS           */ 02018000
         BAL   @14,DUMPRTN                                         0811 02019000
*   GEN( ABEND 3085,,,SYSTEM);      /* RECOVER LOG IN ESTAE PROCESSOR*/ 02020000
          ABEND 3085,,,SYSTEM                                           02021000
*   END ABENDRTN;                                                  0813 02022000
@EL00025 DS    0H                                                  0813 02023000
@EF00025 DS    0H                                                  0813 02024000
