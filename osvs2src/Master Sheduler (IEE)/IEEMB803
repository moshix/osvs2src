@EL00010 DS    0H                                                  0548 01103000
@EF00010 DS    0H                                                  0548 01104000
@ER00010 BR    @14                                                 0548 01105000
*FREELCA:                                                          0549 01106000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0549 01107000
*                                                                  0549 01108000
FREELCA  DS    0H                                                  0550 01109000
*   /*****************************************************************/ 01110000
*   /*                                                               */ 01111000
*   /* ENTRY TO THIS RTN IS TO FREE CORE GOTTEN FOR THE LCA AND RPL  */ 01112000
*   /* CONTROL BLOCKS                                                */ 01113000
*   /*                                                               */ 01114000
*   /*****************************************************************/ 01115000
*                                                                  0550 01116000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 01117000
         LR    REGSAV,REG14                                        0550 01118000
*   REG0=LCASIZE+LENRPL;            /* LENGTH TO FREE , LCA AND RPL  */ 01119000
         SLR   REG0,REG0                                           0551 01120000
         IC    REG0,LCASIZE                                        0551 01121000
         SLR   @05,@05                                             0551 01122000
         IC    @05,LENRPL                                          0551 01123000
         ALR   REG0,@05                                            0551 01124000
*   REG0=REG0|SPNFLD;               /* SUBPOOL = 231                 */ 01125000
         O     REG0,SPNFLD                                         0552 01126000
*   REG1=LCASAVE;                   /* ADDRESS OF GOTTEN CORE        */ 01127000
         L     REG1,LCASAVE                                        0553 01128000
*   GEN( FREEMAIN R,LV=(0),A=(1));  /* FREE LCA , RPL CORE           */ 01129000
          FREEMAIN R,LV=(0),A=(1)                                       01130000
*   REG14=REGSAV;                   /* RESTORE RETURN ADDRESS        */ 01131000
         LR    REG14,REGSAV                                        0555 01132000
*   END FREELCA;                                                   0556 01133000
@EL00011 DS    0H                                                  0556 01134000
@EF00011 DS    0H                                                  0556 01135000
@ER00011 BR    @14                                                 0556 01136000
*FREEACB:                                                          0557 01137000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0557 01138000
*                                                                  0557 01139000
FREEACB  DS    0H                                                  0558 01140000
*   /*****************************************************************/ 01141000
*   /*                                                               */ 01142000
*   /* ENTRY HERE IS TO FREE CORE GOTTEN FOR THE ACB CONTROL BLOCK   */ 01143000
*   /*                                                               */ 01144000
*   /*****************************************************************/ 01145000
*                                                                  0558 01146000
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */ 01147000
         LR    REGSAV,REG14                                        0558 01148000
*   REG0=LENACB2;                   /* LENGTH TO FREE                */ 01149000
         L     REG0,LENACB2                                        0559 01150000
