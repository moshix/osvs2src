*           END;                                                   0415 00689000
*         ELSE                                                     0416 00690000
*           ;                                                      0416 00691000
@RF00407 DS    0H                                                  0417 00692000
*       ELSE                        /* LOG DATA SET SWITCH POSTED    */ 00693000
*         CALL SWITCHDS;            /* PROCESS TO SWITCH DATA SETS   */ 00694000
         B     @RC00406                                            0417 00695000
@RF00406 BAL   @14,SWITCHDS                                        0417 00696000
*     ELSE                          /* LOG TERMINATION ECB POSTED    */ 00697000
*       CALL LOGTERM;               /* PROCESS TO CLOSE LOG          */ 00698000
         B     @RC00405                                            0418 00699000
@RF00405 BAL   @14,LOGTERM                                         0418 00700000
*   END;                                                           0419 00701000
