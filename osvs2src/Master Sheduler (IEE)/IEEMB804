@EL00007 DS    0H                                                  0259 00497000
@EF00007 DS    0H                                                  0259 00498000
@ER00007 BR    @14                                                 0259 00499000
*WTOCNVRT:                                                         0260 00500000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0260 00501000
*                                                                  0260 00502000
WTOCNVRT DS    0H                                                  0261 00503000
*   /*****************************************************************/ 00504000
*   /*                                                               */ 00505000
*   /* ENTRY TO THIS RTN IS TO CONVERT THE USERS WTL TO A WTO WHEN   */ 00506000
*   /* THE LOG IS NOT ACTIVE OR ESTAE FAILED .                       */ 00507000
*   /*                                                               */ 00508000
*   /*****************************************************************/ 00509000
*                                                                  0261 00510000
*   SAVE14=REG14;                   /* SAVE RETURN ADDRESS           */ 00511000
         LR    SAVE14,REG14                                        0261 00512000
*   RFY                                                            0262 00513000
*    (REG5,                                                        0262 00514000
*     REG8) RESTRICTED;                                            0262 00515000
*   REG8=USERSIZE-4;                /* RECORD LENGTH - HEADER        */ 00516000
         SLR   REG8,REG8                                           0263 00517000
         ICM   REG8,3,USERSIZE(REG2)                               0263 00518000
         SL    REG8,@CF00054                                       0263 00519000
*   IF REG8>126 THEN                /* MAXIMUM WTL LENGTH    @YM05484*/ 00520000
         LA    @10,126                                             0264 00521000
