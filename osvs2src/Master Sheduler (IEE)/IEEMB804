*CHKQOUT:                                                          0227 00377000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0227 00378000
*                                                                  0227 00379000
CHKQOUT  DS    0H                                                  0228 00380000
*   /*****************************************************************/ 00381000
*   /*                                                               */ 00382000
*   /* THE SVC USE COUNT IS DECREMENTED VIA ( CS ) . IF LCA PTR AND  */ 00383000
*   /* THE SVC USE COUNT IS ZERO , A RETURN CODE OF FOUR IS SET TO   */ 00384000
*   /* CAUSE A POST OF LOG TERMINATION ECB . THE ESTAE IS CANCELLED  */ 00385000
*   /* UNLESS CALL IS FROM ESTAE EXIT RTN STAERTN .                  */ 00386000
*   /*                                                               */ 00387000
*   /*****************************************************************/ 00388000
*                                                                  0228 00389000
*   DCL                                                            0228 00390000
*     LABEL2 LABEL;                 /* 'CS' RETURN POINT             */ 00391000
*   SAVE14=REG14;                   /* SAVE RETURN ADDRESS           */ 00392000
         LR    SAVE14,REG14                                        0229 00393000
*   RFY                                                            0230 00394000
*    (PR1SVC,                                                      0230 00395000
*     PR2SVC,                                                      0230 00396000
*     PR1LCA) RESTRICTED;                                          0230 00397000
*   RETCODE='00'X;                  /* PRE-SET GOOD RETURN CODE      */ 00398000
         MVI   RETCODE,X'00'                                       0231 00399000
*   PR1SVC=MSLOGSVC;                /* SVC USE COUNT TO REG          */ 00400000
         L     @10,CVTPTR                                          0232 00401000
         L     @10,CVTMSER(,@10)                                   0232 00402000
