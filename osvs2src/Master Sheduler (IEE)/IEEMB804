         ALR   PR2ELCNT,@10                                        0433 01102000
*   CHAINPTR=PR1ELAD;               /* SET BACKWARD POINTER          */ 01103000
         ST    PR1ELAD,CHAINPTR(,REG1)                             0434 01104000
*   PR2ELAD=REG1;                   /* SET CURRENT ELEMENT PTR       */ 01105000
         LR    PR2ELAD,REG1                                        0435 01106000
*   IF PR1ELAD^=0 THEN              /* NOT FIRST ELEMENT     @ZA07784*/ 01107000
         LTR   PR1ELAD,PR1ELAD                                     0436 01108000
