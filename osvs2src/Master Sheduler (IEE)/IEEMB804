@RT00438 DS    0H                                                  0439 01121000
*     PR2ELAD=PR2ELAD|REGMASK;      /* SET THIS 'S' BIT ON           */ 01122000
         O     PR2ELAD,REGMASK                                     0439 01123000
*   CDS(PR1ELCNT,PR2ELCNT,LCAQHEAD);/* SWAP FIELD                    */ 01124000
@RF00438 CDS   PR1ELCNT,PR2ELCNT,LCAQHEAD(LCAPTR)                  0440 01125000
*   BC(7,LABEL4);                   /* RE-PROCESS IF COMPARE FAILED  */ 01126000
         BC    7,LABEL4                                            0441 01127000
*   REG14=SAVE14;                   /* RESTORE RETURN ADDRESS        */ 01128000
         LR    REG14,SAVE14                                        0442 01129000
*   END UPDATEQ;                                                   0443 01130000
@EL00016 DS    0H                                                  0443 01131000
@EF00016 DS    0H                                                  0443 01132000
@ER00016 BR    @14                                                 0443 01133000
*CHKPUT:                                                           0444 01134000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0444 01135000
*                                                                  0444 01136000
CHKPUT   DS    0H                                                  0445 01137000
*   /*****************************************************************/ 01138000
*   /*                                                               */ 01139000
*   /* ENTRY HERE IS TO DETERMINE IF THE WTL QUEUE LIMIT FOR POSTING */ 01140000
*   /* OF THE PUT RTN HAS BEEN REAXHED . THE HI-ORDER BIT ('S' BIT)  */ 01141000
*   /* OF THE PREVIOUS ELEMENT POINTER IS TESTED FOR (OFF) . . IF    */ 01142000
*   /* (OFF), THE HI-ORDER BIT OF THE NEW ELEMENT POINTER IS TESTED  */ 01143000
*   /* FOR (ON) . . IF (ON) , THE WTL QUEUE LIMIT HAS BEEN REACHED BY*/ 01144000
*   /* THIS EXECUTION OF THE WTL SVC - . THE POSTING RTN IS CALLED TO*/ 01145000
*   /* POST THE LOG TASK PUT RTN ECB (MSLGWTR) , TO WRITE THE WTL    */ 01146000
*   /* QUEUE ELEMENTS TO THE LOG DATA SET . . ELSE , THE PUT RTN HAS */ 01147000
*   /* BEEN PREVIOUSLY POSTED AND IS STILL WRITING THE QUEUE ELEMENTS*/ 01148000
*   /* TO THE LOG DATA SET SO THE POST OF ECB MSLGWTR IS BYPASSED .  */ 01149000
*   /* RETURN TO CALLER .                                            */ 01150000
*   /*                                                               */ 01151000
*   /*****************************************************************/ 01152000
*                                                                  0445 01153000
*   SAVE14=REG14;                   /* SAVE RETURN ADDRESS           */ 01154000
         LR    SAVE14,REG14                                        0445 01155000
*   REG6=PR1ELAD|REGMASK;           /* SET 'S' BIT ON IN REG         */ 01156000
         LR    REG6,PR1ELAD                                        0446 01157000
         O     REG6,REGMASK                                        0446 01158000
*   IF PR1ELAD^=REG6 THEN           /* 'S' BIT OFF                   */ 01159000
         CR    PR1ELAD,REG6                                        0447 01160000
         BE    @RF00447                                            0447 01161000
*     IF PR2ELAD<0 THEN             /* 'S' BIT ON                    */ 01162000
         LTR   PR2ELAD,PR2ELAD                                     0448 01163000
         BNM   @RF00448                                            0448 01164000
*       DO;                                                        0449 01165000
*         PUTSW='1'B;               /* SET POST OF PUT RTN           */ 01166000
         OI    PUTSW,B'10000000'                                   0450 01167000
*         CALL POSTRTN;             /* POST PUT RTN ECB              */ 01168000
         BAL   @14,POSTRTN                                         0451 01169000
*       END;                                                       0452 01170000
*     ELSE                                                         0453 01171000
*       ;                                                          0453 01172000
@RF00448 DS    0H                                                  0454 01173000
*   ELSE                            /* CHECK FOR POST MISSED @ZA06797*/ 01174000
*     IF PR2ELCNT>WTLPLIM THEN      /* COUNT GREATER THAN 20 @ZA06797*/ 01175000
         B     @RC00447                                            0454 01176000
