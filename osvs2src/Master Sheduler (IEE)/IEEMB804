@RC00293 DS    0H                                                  0310 00674000
*   REG14=REGSAVE;                  /* RESTORE RETURN ADDRESS        */ 00675000
         L     REG14,REGSAVE                                       0310 00676000
*   END SCANCHK;                                                   0311 00677000
@EL00010 DS    0H                                                  0311 00678000
@EF00010 DS    0H                                                  0311 00679000
@ER00010 BR    @14                                                 0311 00680000
*POSTRTN:                                                          0312 00681000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0312 00682000
*                                                                  0312 00683000
POSTRTN  DS    0H                                                  0313 00684000
*   /*****************************************************************/ 00685000
*   /*                                                               */ 00686000
*   /* ENTRY IS TO POST THE LOG TASK TO PERFORM A PARTICULAR FUNCTION*/ 00687000
*   /* WHICH IS DEFINED BY A SWITCH SET BY THE CALLER OF THIS        */ 00688000
*   /* ROUTINE. ONLY ONE OF THE THREE POSSIBLE BIT SETTINGS CAN BE ON*/ 00689000
*   /* WHEN THIS RTN GETS CONTROL .                                  */ 00690000
*   /*                                                               */ 00691000
*   /*****************************************************************/ 00692000
*                                                                  0313 00693000
*   DCL                                                            0313 00694000
*     1 POSTCORE BASED(REG1),       /* ECBLIST STRUCTURE             */ 00695000
*      2 ECBAD PTR(31),             /* ECB ADDRESS                   */ 00696000
*      2 ASCBAD PTR(31),            /* ASCB ADDRESS                  */ 00697000
*      2 ERRAD PTR(31);             /* RETURN ADDR FOR POST FAILURE  */ 00698000
*   DCL                                                            0314 00699000
*     POSTLIST GEND CHAR(12);       /* LIST FORM OF POST             */ 00700000
*   REGSAVE=REG14;                  /* SAVE RETURN ADDRESS           */ 00701000
         ST    REG14,REGSAVE                                       0315 00702000
*   REG1=ADDR(WORKAREA);            /* SET UP LIST ADDRESS           */ 00703000
         LA    REG1,WORKAREA                                       0316 00704000
*   POSTCORE=POSTLIST;              /* MOVE IN LIST FORM OF POST     */ 00705000
         MVC   POSTCORE(12,REG1),POSTLIST                          0317 00706000
*   ASCBAD=LCAASCB;                 /* ASCB ADDR TO LIST             */ 00707000
         L     @10,LCAASCB(,LCAPTR)                                0318 00708000
         ST    @10,ASCBAD(,REG1)                                   0318 00709000
*   ERRAD=ADDR(CVTBRET);            /* EXIT RTN ADDR TO LIST         */ 00710000
         L     @10,CVTPTR                                          0319 00711000
         LA    @06,CVTBRET(,@10)                                   0319 00712000
         ST    @06,ERRAD(,REG1)                                    0319 00713000
*   IF PUTSW='1'B THEN              /* POST LOG TASK PUT RTN         */ 00714000
         TM    PUTSW,B'10000000'                                   0320 00715000
         BNO   @RF00320                                            0320 00716000
*     ECBAD=ADDR(MSLGWTR);                                         0321 00717000
         L     @10,CVTMSER(,@10)                                   0321 00718000
         LA    @10,MSLGWTR(,@10)                                   0321 00719000
         ST    @10,ECBAD(,REG1)                                    0321 00720000
*   IF WTLOGSW='1'B THEN            /* POST LOG TASK DATA SET SWITCH */ 00721000
@RF00320 TM    WTLOGSW,B'01000000'                                 0322 00722000
         BNO   @RF00322                                            0322 00723000
*     IF POSTWLOG='0'B THEN         /* WRITELOG NOT PROCESSING       */ 00724000
         TM    POSTWLOG(LCAPTR),B'01000000'                        0323 00725000
         BNZ   @RF00323                                            0323 00726000
*       DO;                                                        0324 00727000
*         POSTWLOG='1'B;            /* SET PROCESSING BIT ON         */ 00728000
         L     @10,POSTWLOG(,LCAPTR)                               0325 00729000
@GL00002 LR    @06,@10                                             0325 00730000
         O     @06,@CF02229                                        0325 00731000
         CS    @10,@06,POSTWLOG(LCAPTR)                            0325 00732000
         BNE   @GL00002                                            0325 00733000
*         ECBAD=ADDR(MSLGWLOG);     /* DATA SET SWITCH ECB ADDR      */ 00734000
         L     @10,CVTPTR                                          0326 00735000
         L     @10,CVTMSER(,@10)                                   0326 00736000
         LA    @10,MSLGWLOG(,@10)                                  0326 00737000
         ST    @10,ECBAD(,REG1)                                    0326 00738000
*       END;                                                       0327 00739000
*     ELSE                          /* WRITELOG IN PROCESS           */ 00740000
*       ECBAD=0;                    /* SET FOR POST BYPASS           */ 00741000
         B     @RC00323                                            0328 00742000
@RF00323 SLR   @10,@10                                             0328 00743000
         ST    @10,ECBAD(,REG1)                                    0328 00744000
*   IF CLSLOGSW='1'B THEN           /* POST LOG TASK TERMINATION     */ 00745000
@RC00323 DS    0H                                                  0329 00746000
@RF00322 TM    CLSLOGSW,B'00010000'                                0329 00747000
         BNO   @RF00329                                            0329 00748000
*     IF POSTCLOZ='0'B THEN         /* TERMINATION NOT PROCESSING    */ 00749000
         TM    POSTCLOZ(LCAPTR),B'10000000'                        0330 00750000
         BNZ   @RF00330                                            0330 00751000
*       DO;                                                        0331 00752000
*         POSTCLOZ='1'B;            /* SET TERMINATION BIT ON        */ 00753000
         L     @10,POSTCLOZ(,LCAPTR)                               0332 00754000
@GL00003 LR    @06,@10                                             0332 00755000
         O     @06,@CF02232                                        0332 00756000
         CS    @10,@06,POSTCLOZ(LCAPTR)                            0332 00757000
         BNE   @GL00003                                            0332 00758000
*         IF POSTERR='1'B THEN      /* BUFFER CORE GET FAIL  @YM06135*/ 00759000
         TM    POSTERR,B'00100000'                                 0333 00760000
         BNO   @RF00333                                            0333 00761000
*           DO;                                                    0334 00762000
*             ECBAD=ADDR(MSLGCLOZ); /* TERMINATION ECB ADDR  @YM06135*/ 00763000
         L     @10,CVTPTR                                          0335 00764000
         L     @10,CVTMSER(,@10)                                   0335 00765000
         LA    @10,MSLGCLOZ(,@10)                                  0335 00766000
         ST    @10,ECBAD(,REG1)                                    0335 00767000
*             GEN( POST ,4,MF=(E,(1)));/* POST CLOSE         @YM06135*/ 00768000
          POST ,4,MF=(E,(1))                                            00769000
*             ECBAD=0;              /* BYPASS SECOND POST    @YM06135*/ 00770000
         SLR   @10,@10                                             0337 00771000
         ST    @10,ECBAD(,REG1)                                    0337 00772000
*           END;                                                   0338 00773000
*         ELSE                                                     0339 00774000
*           ECBAD=ADDR(MSLGCLOZ);   /* TERMINATION ECB ADDR  @YM06135*/ 00775000
         B     @RC00333                                            0339 00776000
@RF00333 L     @10,CVTPTR                                          0339 00777000
