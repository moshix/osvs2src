         BNZ   @RF00455                                            0455 01185000
*         DO;                       /*                       @ZA06797*/ 01186000
*           PUTSW='1'B;             /* SET POST WRITER ECB   @ZA06797*/ 01187000
         OI    PUTSW,B'10000000'                                   0457 01188000
*           CALL POSTRTN;           /* POST PUT RTN ECB      @ZA06797*/ 01189000
         BAL   @14,POSTRTN                                         0458 01190000
*         END;                                                     0459 01191000
*   REG14=SAVE14;                   /* RESTORE RETURN ADDRESS        */ 01192000
@RF00455 DS    0H                                                  0460 01193000
@RF00454 DS    0H                                                  0460 01194000
@RC00447 LR    REG14,SAVE14                                        0460 01195000
*   END CHKPUT;                                                    0461 01196000
@EL00017 DS    0H                                                  0461 01197000
@EF00017 DS    0H                                                  0461 01198000
@ER00017 BR    @14                                                 0461 01199000
*AUTHTEST:                                                         0462 01200000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0462 01201000
*                                                                  0462 01202000
AUTHTEST DS    0H                                                  0463 01203000
*   /*****************************************************************/ 01204000
*   /*                                                               */ 01205000
*   /* ENTRY HERE IS TO VERIFY THE CALLER AUTHORITY TO WRITE DATA    */ 01206000
*   /* FROM THE ADDRESS OF THE INPUT AREA. THE TESTAUTH SVC ( 119 )  */ 01207000
*   /* IS ISSUED , POINTING TO THE USER KEY IN THE R/B OLD PSW . IF  */ 01208000
*   /* AUTHORIZED, REG15 = ZERO , RETURN TO NORMAL PROCESSING. ELSE ,*/ 01209000
*   /* CALL VALIDTST RTN TO VERIFY THAT THE DATA IS IN CALLERS CORE. */ 01210000
*   /* VALID VERIFY RESULTS IN NORMAL RETURN TO PROCESSING. INVALID  */ 01211000
*   /* VERIFY RESULTS IN AN ABEND OCCURING IN VALIDTST.              */ 01212000
*   /*                                                               */ 01213000
*   /*****************************************************************/ 01214000
*                                                                  0463 01215000
*   REGSAVE=REG14;                  /* SAVE RETURN ADDR      @YM05484*/ 01216000
         ST    REG14,REGSAVE                                       0463 01217000
*   GEN( TESTAUTH KEY=YES,RBLEVEL=2);/* TEST USER STORAGE KEY.     0464 01218000
*                                                            @YM05484*/ 01219000
          TESTAUTH KEY=YES,RBLEVEL=2                                    01220000
*   IF REG15^=0 THEN                /* NOT AUTHORIZED USER   @YM05484*/ 01221000
         LTR   REG15,REG15                                         0465 01222000
         BZ    @RF00465                                            0465 01223000
*     CALL VALIDTST;                /* VERIFY DATA ADDRESS INTEGRITY    01224000
*                                                            @YM05484*/ 01225000
         BAL   @14,VALIDTST                                        0466 01226000
*   REG14=REGSAVE;                  /* RESTORE RETURN ADDR   @YM05484*/ 01227000
@RF00465 L     REG14,REGSAVE                                       0467 01228000
*   END AUTHTEST;                                                  0468 01229000
@EL00018 DS    0H                                                  0468 01230000
@EF00018 DS    0H                                                  0468 01231000
@ER00018 BR    @14                                                 0468 01232000
*SUBSYS:                                                           0469 01233000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);/*                       @ZA04791*/ 01234000
*                                                                  0469 01235000
SUBSYS   DS    0H                                                  0470 01236000
*   /*****************************************************************/ 01237000
*   /*                                                               */ 01238000
*   /* ENTRY HERE IS TO TAKE A SUB-SYSTEM EXIT FOR ALL WTL SVC'S. THE*/ 01239000
*   /* SSOB AND SSWT CONTROL BLOCKS ARE INITIALIZED WITH THE WTL     */ 01240000
*   /* FUNCTION ID AND A POINTER TO THE WTL TEXT BUFFER. THE         */ 01241000
*   /* SUB-SYSTEM WILL EITHER PROCESS THE WTL ITSELF, OR ALLOW THIS  */ 01242000
*   /* MODULE TO CONTINUE PROCESSING THE WTL. ON RETURN FROM THE     */ 01243000
*   /* EXIT, RETURN CODES ARE CHECKED AND PROCESSING CONTINUES       */ 01244000
*   /* ACCORDINGLY. THIS ROUTINE WAS ADDED FOR APAR ZANNNNN.         */ 01245000
*   /*                                                               */ 01246000
*   /*****************************************************************/ 01247000
*                                                                  0470 01248000
*   DCL                                                            0470 01249000
*     SSOBNAME CHAR(4) INIT('SSOB');/* SSOB NAME ID          @ZA04791*/ 01250000
*   REGSAVE=REG14;                  /* SAVE RETURN PTR       @ZA04791*/ 01251000
         ST    REG14,REGSAVE                                       0471 01252000
*   SSOB=SSOB&&SSOB;                /* CLEAR SSOB AREA       @ZA04791*/ 01253000
         XC    SSOB(20),SSOB                                       0472 01254000
*   SSOBID=SSOBNAME;                /* CONTROL BLOCK ID      @ZA04791*/ 01255000
         MVC   SSOBID(4),SSOBNAME                                  0473 01256000
*   SSOBLEN=LENGTH(SSOB);           /* SSOB LENGTH           @ZA04791*/ 01257000
         MVC   SSOBLEN(2),@CH00804                                 0474 01258000
*   SSOBFUNC=SSOBWTL;               /* WTL FUNCTION ID       @ZA04791*/ 01259000
         MVC   SSOBFUNC(2),@CH02000                                0475 01260000
*   SSOBINDV=ADDR(SSWT);            /* PARM LIST ADDRESS     @ZA04791*/ 01261000
         LA    @10,SSWT                                            0476 01262000
         ST    @10,SSOBINDV                                        0476 01263000
*   SSWT=SSWT&&SSWT;                /* CONTROL BLOCK LENGTH  @ZA04791*/ 01264000
         XC    SSWT(16),SSWT                                       0477 01265000
*   SSWTLEN=LENGTH(SSWT);           /* SET SSWT LENGTH       @ZA04791*/ 01266000
         MVC   SSWTLEN(2),@CH01240                                 0478 01267000
*   SSWTMIN=REG2;                   /* PTR TO WTL RECORD     @ZA04791*/ 01268000
         ST    REG2,SSWTMIN                                        0479 01269000
*/*  ? IEFSSREQ  (SSOB);                                             */ 01270000
*/*                                                                  */ 01271000
*/*  PASS CONTROL TO JOB ENTRY SUBSYSTEM TO                          */ 01272000
*/*  PROCESS REQUEST -                                               */ 01273000
*/*                                                                  */ 01274000
*/*    INPUT - REGISTER 1 POINTS TO A ONE-                           */ 01275000
*/*            WORD PARAMETER LIST WHICH                             */ 01276000
*/*            POINTS TO THE SSOB                                    */ 01277000
*/*                                                                  */ 01278000
*                                                                  0480 01279000
*   CALL SSREQ(SSOB);               /* INVOKE SUB-SYSTEM     @ZA04791*/ 01280000
         LA    @10,SSOB                                            0480 01281000
         ST    @10,@AL00001                                        0480 01282000
         MVI   @AL00001,X'80'                                      0480 01283000
         L     @10,CVTPTR                                          0480 01284000
         L     @10,CVTJESCT(,@10)                                  0480 01285000
         L     @15,JESSSREQ(,@10)                                  0480 01286000
         LA    @01,@AL00001                                        0480 01287000
         BALR  @14,@15                                             0480 01288000
*   REG14=REGSAVE;                  /* RESTORE RETURN ADDR   @ZA04791*/ 01289000
         L     REG14,REGSAVE                                       0481 01290000
*   IF SSOBRETN=0 THEN              /* SUB-SYSTEM HANDLE WTL @ZA04791*/ 01291000
         L     @10,SSOBRETN                                        0482 01292000
         LTR   @10,@10                                             0482 01293000
         BNZ   @RF00482                                            0482 01294000
*     REG15=0;                      /* NO, CONTINUE          @ZA04791*/ 01295000
         SLR   REG15,REG15                                         0483 01296000
*   ELSE                                                           0484 01297000
*     REG15=4;                      /* YES, SET CODE TO BYPASS WTL 0484 01298000
*                                      ISSUE                 @ZA04791*/ 01299000
         B     @RC00482                                            0484 01300000
@RF00482 LA    REG15,4                                             0484 01301000
*   END SUBSYS;                     /* RETURN TO CALLER      @ZA04791*/ 01302000
@EL00019 DS    0H                                                  0485 01303000
@EF00019 DS    0H                                                  0485 01304000
@ER00019 BR    @14                                                 0485 01305000
