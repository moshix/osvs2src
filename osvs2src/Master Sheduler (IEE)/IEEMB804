@EL00008 DS    0H                                                  0274 00547000
@EF00008 DS    0H                                                  0274 00548000
@ER00008 BR    @14                                                 0274 00549000
*WTORTN:                                                           0275 00550000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0275 00551000
*                                                                  0275 00552000
WTORTN   DS    0H                                                  0276 00553000
*   /*****************************************************************/ 00554000
*   /*                                                               */ 00555000
*   /* THIS RTN MOVES THE USER MESSAGE INTO THE WTO BUFFER , ISSUES  */ 00556000
*   /* THE WTO , AND UPDATES THE USER MESSAGE PTR .                  */ 00557000
*   /*                                                               */ 00558000
*   /*****************************************************************/ 00559000
*                                                                  0276 00560000
*   RFY                                                            0276 00561000
*    (REG6,                                                        0276 00562000
*     REG5,                                                        0276 00563000
*     REG8) RESTRICTED;                                            0276 00564000
*   REGSAVE=REG14;                  /* SAVE RETURN ADDR              */ 00565000
         ST    REG14,REGSAVE                                       0277 00566000
*   REG1=ADDR(WORKAREA);            /* GET MSG BUFFER ADDR           */ 00567000
         LA    REG1,WORKAREA                                       0278 00568000
*   LOGMSG(1:LEN)=WTOMAP(1:LEN);    /* MOVE IN WTO LIST FORM         */ 00569000
