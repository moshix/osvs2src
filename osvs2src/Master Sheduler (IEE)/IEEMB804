*     DO;                                                          0294 00629000
*       REG6=REG4+REG5;             /* SET MAX SCAN FOR BREAKPOINT   */ 00630000
         LR    REG6,REG4                                           0295 00631000
         ALR   REG6,REG5                                           0295 00632000
*       DO WHILE(REG6->MSGFLD(1)^=' '&REG6^=REG4);/* SCAN RECORD   0296 00633000
*                                      BACKWARD FOR BLANK            */ 00634000
         B     @DE00296                                            0296 00635000
@DL00296 DS    0H                                                  0297 00636000
*         REG6=REG6-1;              /* DECREMENT SCAN PTR            */ 00637000
         BCTR  REG6,0                                              0297 00638000
*       END;                                                       0298 00639000
@DE00296 CLI   MSGFLD(REG6),C' '                                   0298 00640000
