         LA    @10,MSLGCLOZ(,@10)                                  0339 00779000
         ST    @10,ECBAD(,REG1)                                    0339 00780000
*       END;                                                       0340 00781000
*     ELSE                          /* TERMINATION IN PROCESS        */ 00782000
*       ECBAD=0;                    /* SET FOR BYPASS OF POST        */ 00783000
         B     @RC00330                                            0341 00784000
@RF00330 SLR   @10,@10                                             0341 00785000
         ST    @10,ECBAD(,REG1)                                    0341 00786000
*   IF ECBAD^=0 THEN                /* PROCESS THE POST              */ 00787000
@RC00330 DS    0H                                                  0342 00788000
@RF00329 L     @10,ECBAD(,REG1)                                    0342 00789000
         LTR   @10,@10                                             0342 00790000
         BZ    @RF00342                                            0342 00791000
*     GEN( POST MF=(E,(1)));        /* POST SYSTEM LOG               */ 00792000
          POST MF=(E,(1))                                               00793000
*   SVCBITS='00'X;                  /* RE-SET POST SWITCHES          */ 00794000
@RF00342 MVI   SVCBITS,X'00'                                       0344 00795000
*   REG14=REGSAVE;                  /* RESTORE RETURN ADDRESS        */ 00796000
         L     REG14,REGSAVE                                       0345 00797000
*   END POSTRTN;                                                   0346 00798000
@EL00011 DS    0H                                                  0346 00799000
@EF00011 DS    0H                                                  0346 00800000
@ER00011 BR    @14                                                 0346 00801000
*STAERTN:                                                          0347 00802000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0347 00803000
*                                                                  0347 00804000
STAERTN  DS    0H                                                  0348 00805000
*   /*****************************************************************/ 00806000
*   /*                                                               */ 00807000
*   /* ENTRY TO THIS RTN IS VIA ABEND/ESTAE INTERFACE WHEN THE SVC   */ 00808000
*   /* HAS ABENDED UNDER THE USER TASK . CLEAN-UP IS PERFORMED AND A */ 00809000
*   /* DUMP MAY BE ISSUED . ABEND PROCESSING IS CONTINUED            */ 00810000
*   /*                                                               */ 00811000
*   /*****************************************************************/ 00812000
*                                                                  0348 00813000
*   DCL                                                            0348 00814000
*     LGRECINF CHAR GEND;                                          0348 00815000
*   RFY                                                            0349 00816000
*    (REG8,                                                        0349 00817000
*     REG11,                                                       0349 00818000
*     REG12) RESTRICTED;                                           0349 00819000
*   REG11=12;                       /* SET UP FOR STAE PARMLIST TEST */ 00820000
         LA    REG11,12                                            0350 00821000
*   GEN;                                                           0351 00822000
