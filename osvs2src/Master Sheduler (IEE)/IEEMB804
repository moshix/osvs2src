         CR    REG6,REG4                                           0298 00642000
         BNE   @DL00296                                            0298 00643000
@DC00296 DS    0H                                                  0299 00644000
*       IF REG6=REG4 THEN           /* NO BREAKPOINT FOUND           */ 00645000
         CR    REG6,REG4                                           0299 00646000
         BNE   @RF00299                                            0299 00647000
*         REG6=REG4+REG5;           /* SET MAX VALUE FOR RECORD MOVE */ 00648000
         LR    REG6,REG4                                           0300 00649000
         ALR   REG6,REG5                                           0300 00650000
*       ELSE                                                       0301 00651000
*         REG6=REG6+1;              /* SET LENGTH ADDR TO BLANK CHAR */ 00652000
         B     @RC00299                                            0301 00653000
@RF00299 AL    REG6,@CF00062                                       0301 00654000
*       REG6=REG6-REG4;             /* COMPUTE SCAN LENGTH           */ 00655000
@RC00299 SLR   REG6,REG4                                           0302 00656000
*       REG8=REG8-REG6;             /* COMPUTE BALANCE TO SCAN       */ 00657000
         SLR   REG8,REG6                                           0303 00658000
*     END;                                                         0304 00659000
*   ELSE                                                           0305 00660000
*     DO;                                                          0305 00661000
         B     @RC00293                                            0305 00662000
@RF00293 DS    0H                                                  0306 00663000
*       REG6=REG8;                  /* SET LENGTH FOR RECORD MOVE    */ 00664000
         LR    REG6,REG8                                           0306 00665000
*       REG8=0;                     /* SET DATA END CODE FOR CALLER  */ 00666000
         SLR   REG8,REG8                                           0307 00667000
*     END;                                                         0308 00668000
*   RFY                                                            0309 00669000
*    (REG6,                                                        0309 00670000
