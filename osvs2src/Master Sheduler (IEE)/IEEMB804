*   END WTORTN;                                                    0288 00600000
@EL00009 DS    0H                                                  0288 00601000
@EF00009 DS    0H                                                  0288 00602000
@ER00009 BR    @14                                                 0288 00603000
*SCANCHK:                                                          0289 00604000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0289 00605000
*                                                                  0289 00606000
SCANCHK  DS    0H                                                  0290 00607000
*   /*****************************************************************/ 00608000
*   /*                                                               */ 00609000
*   /* THIS RTN SEGMENTS THE USER RECORD BY SCANNING BACKWARD FOR A  */ 00610000
*   /* LOGICAL BREAKPOINT (' '). IT IS CALLED FOR WTL TO WTO         */ 00611000
*   /* CONVERSION , AND TO TRUNCATE A USER RECORD GREATER THAN       */ 00612000
*   /* ONE-THIRTY BYTES .                                            */ 00613000
*   /*                                                               */ 00614000
*   /*****************************************************************/ 00615000
*                                                                  0290 00616000
*   REGSAVE=REG14;                  /* SAVE RETURN ADDRESS           */ 00617000
         ST    REG14,REGSAVE                                       0290 00618000
*   RFY                                                            0291 00619000
*    (REG6,                                                        0291 00620000
*     REG4,                                                        0291 00621000
*     REG5,                                                        0291 00622000
*     REG8) RESTRICTED;                                            0291 00623000
*   REG4=REG2;                      /* MSG ADDR TO WORK REG          */ 00624000
         LR    REG4,REG2                                           0292 00625000
*   IF REG8>REG5 THEN               /* REC SIZE GREATER THAN MAX SIZE*/ 00626000
