*                                                            @YM03371*/ 00532000
         LA    REG5,64                                             0268 00533000
*     CALL SCANCHK;                 /* SEGMENT WTO IF LONGER THAN MAX*/ 00534000
         BAL   @14,SCANCHK                                         0269 00535000
*     CALL WTORTN;                  /* BUILD , ISSUE WTO MSG         */ 00536000
         BAL   @14,WTORTN                                          0270 00537000
*   END;                                                           0271 00538000
@DE00267 LTR   REG8,REG8                                           0271 00539000
