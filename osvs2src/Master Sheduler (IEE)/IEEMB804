*LIMITCHK:                                                         0247 00449000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0247 00450000
*                                                                  0247 00451000
LIMITCHK DS    0H                                                  0248 00452000
*   /*****************************************************************/ 00453000
*   /*                                                               */ 00454000
*   /* THE COUNT OF WTLS IS INCREMENTED VIA ( CS ) . THE NEW VALUE IS*/ 00455000
*   /* TESTED FOR EQUAL THE NIP/IPL SPECIFIED VALUE . IF EQUAL, A    */ 00456000
*   /* RETURN CODE OF FOUR IS SET FOR A POST OF THE DATA SET SWITCH  */ 00457000
*   /* ECB .                                                         */ 00458000
*   /*                                                               */ 00459000
*   /*****************************************************************/ 00460000
*                                                                  0248 00461000
*   RFY                                                            0248 00462000
*    (WREG1,                                                       0248 00463000
*     WREG2) RESTRICTED;                                           0248 00464000
*   SAVE14=REG14;                   /* SAVE RETURN ADDRESS           */ 00465000
         LR    SAVE14,REG14                                        0249 00466000
*   WREG1=LCALIMCT;                 /* CNT OF WTLS PROCESSED         */ 00467000
         L     WREG1,LCALIMCT(,LCAPTR)                             0250 00468000
*LABEL5:                                                           0251 00469000
*   WREG2=WREG1+1;                  /* BUMP CNT FOR THIS WTL         */ 00470000
LABEL5   LA    WREG2,1                                             0251 00471000
         ALR   WREG2,WREG1                                         0251 00472000
*   CS(WREG1,WREG2,LCALIMCT);       /* SWAP IN NEW VALUE (WREG2)     */ 00473000
         CS    WREG1,@04,LCALIMCT(LCAPTR)                          0252 00474000
*   BC(7,LABEL5);                   /* RE-PROCESS IF COMPARE FAILED  */ 00475000
         BC    7,LABEL5                                            0253 00476000
*   IF WREG2=MSLOGLMT THEN          /* DATA SET LIMIT REACHED        */ 00477000
