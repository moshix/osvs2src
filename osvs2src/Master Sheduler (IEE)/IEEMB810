         BNZ   @RF00063                                            0063 00102000
*                   DO;             /* IF LENGTH OK AND A NUMERIC  0064 00103000
*                                      VALUE THEN CONTINUE.          */ 00104000
*                                                                  0064 00105000
*                     /***********************************************/ 00106000
*                     /*                                             */ 00107000
*                     /* THE PERFORM VALUE IS PASSED TO THE SYSTEM   */ 00108000
*                     /* RESOURCE MANAGER THRU THE SYSEVENT MACRO.   */ 00109000
*                     /* THE RETURN CODE IN REG 1 IS CHECKED TO      */ 00110000
*                     /* DETERMINE IF THE MANAGER ACCEPTED THE NEW   */ 00111000
*                     /* PERFORM VALUE.                              */ 00112000
*                     /*                                             */ 00113000
*                     /***********************************************/ 00114000
*                                                                  0065 00115000
*                     CALL SYSEVENT;                               0065 00116000
         BAL   @14,SYSEVENT                                        0065 00117000
*                     CALL CHECKRC2;                               0066 00118000
         BAL   @14,CHECKRC2                                        0066 00119000
*                   END;                                           0067 00120000
*               END;                                               0068 00121000
*             ELSE                                                 0069 00122000
*               CALL CHECKRC1;      /* CHECK IEEBUFSC RC             */ 00123000
         B     @RC00060                                            0069 00124000
@RF00060 BAL   @14,CHECKRC1                                        0069 00125000
*           END;                                                   0070 00126000
@RC00060 DS    0H                                                  0071 00127000
*         IF XAE=0 THEN             /* WAS THE JOBNAME FOUND         */ 00128000
@RF00057 CLI   XAE(XSAPTR),0                                       0071 00129000
         BNE   @RF00071                                            0071 00130000
*           REG3=CHPTR;             /* IF NOT, GET NEXT CSCB         */ 00131000
         L     REG3,CHPTR(,CSCBPTR)                                0072 00132000
*       END;                                                       0073 00133000
*                                                                  0073 00134000
