         ST    REG8,REG8SAV(,REG1)                                 0098 00219000
*   REG12SAV=REG12;                 /* SAVE BASE REGISTER            */ 00220000
         ST    REG12,REG12SAV(,REG1)                               0099 00221000
*   REG11SAV=REG11;                 /* SAVE DATA REG ADDRESS         */ 00222000
         ST    REG11,REG11SAV(,REG1)                               0100 00223000
*   REG13SAV=REG13;                 /* SAVE SAVE AREA PTR            */ 00224000
         ST    REG13,REG13SAV(,REG1)                               0101 00225000
*   GEN(LOAD EP=IEECB860);          /* LOAD STAE CREATING MOD        */ 00226000
         LOAD EP=IEECB860                                               00227000
*   REG15=REG0;                     /* PUT ENTRY ADDR IN R15         */ 00228000
         LR    REG15,REG0                                          0103 00229000
