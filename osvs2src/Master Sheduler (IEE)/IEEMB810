*   END;                                                           0151 00407000
         B     @EL00006                                            0151 00408000
*GETBUFSC:                                                         0152 00409000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0152 00410000
*                                                                  0152 00411000
GETBUFSC DS    0H                                                  0153 00412000
*/*                                                                0153 00413000
********************************************************************    00414000
**                                                                 0153 00415000
**     ROUTINE NAME        IEEBUFSC                                0153 00416000
**                                                                 0153 00417000
**     FUNCTION            TO SCAN A BUFFER FOR A GIVEN KEYWORD OF 0153 00418000
**                         A GIVEN LENGTH AND TO RETURN A POINTER  0153 00419000
**                         TO THE KEYWORD VALUE AND THE LENGTH OF  0153 00420000
**                         THE VALUE                               0153 00421000
**                                                                 0153 00422000
**     ENTRY POINT         IEEBUFSC                                0153 00423000
**                                                                 0153 00424000
**     ATTRIBUTES          REFRESHABLE                             0153 00425000
**                                                                 0153 00426000
**     OPERATION           1. SET UP PTR TO BUFFER -1 AND PTR      0153 00427000
**                            TO KEYWORD LENGTH                    0153 00428000
**                         2. IF BUFFER PTR = SPECIFIED KEYWORD    0153 00429000
**                            GO TO FOUND ROUTINE (3).             0153 00430000
**                            A. IF NOT, INCREMENT BUFFER PTR      0153 00431000
**                               BY ONE.                           0153 00432000
**                            B. IF PTR IS AT END OF BUFFER SET    0153 00433000
**                               UP NOT FOUND RETURN CODE (8) IN   0153 00434000
**                               R15 AND EXIT.                     0153 00435000
**                            C. IF PTR IS AT A COMMA, GO BACK TO  0153 00436000
**                               OPERATION 2.                      0153 00437000
**                            D. IF PTR IS AT A BLANK SET UP NOT   0153 00438000
**                               FOUND RETURN CODE (8) IN R15 AND  0153 00439000
**                               EXIT.                             0153 00440000
**                            E. OTHERWISE GO TO OPERATION 2.A.    0153 00441000
**                         3. AT FOUND ROUTINE SET UP R14 TO POINT 0153 00442000
**                            TO KEYWORD VALUE                     0153 00443000
**                         4. SCAN KEYWORD VALUE TO THE END OF     0153 00444000
**                            THE BUFFER, A COMMA OR A BLANK       0153 00445000
**                         5. CALCULATE LENGTH OF KEYWORD VALUE.   0153 00446000
**                         6. IF LENGTH IS < OR = ZERO, SET UP     0153 00447000
**                            AN INVALID KEYWORD VALUE LENGTH      0153 00448000
**                            RETURN CODE (4) IN R15 AND EXIT.     0153 00449000
**                         7. OTHERWISE, SET GOOD RETURN CODE      0153 00450000
**                            OF ZERO IN R15 AND EXIT.             0153 00451000
**                                                                 0153 00452000
**     EXTERNAL REFS       NONE                                    0153 00453000
**                                                                 0153 00454000
**     SVCS                NONE                                    0153 00455000
**                                                                 0153 00456000
**     INPUT               R1 - PTR TO BEGINNING OF BUFFER         0153 00457000
**                         R0 - PTR TO LAST BYTE OF BUFFER + 1     0153 00458000
**                         R15- PTR TO KEYWORD WITH THE LENGTH     0153 00459000
**                              OF THE KEYWORD IN THE HIGH ORDER   0153 00460000
**                              BYTE.                              0153 00461000
**                              EG. KEYWORD FORMAT 'DSNAME='       0153 00462000
**                                  R15 WOULD POINT TO THE 'D'     0153 00463000
**                                  WITH A 6 IN THE HIGH ORDER     0153 00464000
**                                  BYTE.                          0153 00465000
**                                                                 0153 00466000
**     OUTPUT              R14  PTR TO FIRST BYTE OF KEYWORD       0153 00467000
**                              VALUE IN BUFFER OR WORKAREA        0153 00468000
**                         R1   LENGTH OF KEYWORD VALUE            0153 00469000
**                         R15  RETURN CODE OF ZERO.               0153 00470000
**                                                                 0153 00471000
**     ERROR OUTPUT        R15  CONTAINS A FOUR IF KEYWORD         0153 00472000
**                              HAD AN INVALID LENGTH              0153 00473000
**                         R15  CONTAINS AN EIGHT IF THE KEYWORD   0153 00474000
**                              WAS NOT FOUND IN THE BUFFER        0153 00475000
**                                                                 0153 00476000
**     EXIT                TO NEXT SEQUENTIAL INSTRUCTION AFTER    0153 00477000
**                         MACRO CALL                              0153 00478000
**                                                                 0153 00479000
**     NOTES               LISTS OF OPERANDS IN PARENTHESES ARE    0153 00480000
**                         NOT PROCESSED. THIS MACRO IS INTENDED   0153 00481000
**                         TO PROCESS ONLY A SINGLE VALUE  FOR     0153 00482000
**                         A GIVEN KEYWORD                         0153 00483000
********************************************************************    00484000
*                                                                    */ 00485000
*                                                                  0153 00486000
*   REG4=REG14;                     /* SAVE RETURN ADDRESS           */ 00487000
         LR    REG4,REG14                                          0153 00488000
*   REG1=ADDR(REG8->CHBUF);         /* GET ADDR OF BUFFER            */ 00489000
         LA    REG1,CHBUF(,REG8)                                   0154 00490000
*   REG0=ADDR(REG8->CHTYPE);        /* GET ADDR OF BUF END           */ 00491000
         LA    REG0,CHTYPE(,REG8)                                  0155 00492000
*   REG15=ADDR(KEYWORD);            /* GET PTR TO KEYWORD            */ 00493000
         LA    REG15,KEYWORD                                       0156 00494000
*   REG15=REG15|KEYWDLEN;           /* GET LENGTH OF KEYWD           */ 00495000
         O     REG15,KEYWDLEN                                      0157 00496000
*   GEN(IEEBUFSC);                                                 0158 00497000
         IEEBUFSC                                                       00498000
*   REG5=REG14;                     /* SAVE PTR TO VALUE             */ 00499000
         LR    REG5,REG14                                          0159 00500000
*   REG14=REG4;                     /* RESTORE RETURN ADDR           */ 00501000
         LR    REG14,REG4                                          0160 00502000
*   END;                                                           0161 00503000
@EL00007 DS    0H                                                  0161 00504000
@EF00007 DS    0H                                                  0161 00505000
@ER00007 BR    @14                                                 0161 00506000
*CHECKRC1:                                                         0162 00507000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0162 00508000
*                                                                  0162 00509000
CHECKRC1 DS    0H                                                  0163 00510000
*   /*****************************************************************/ 00511000
*   /*                                                               */ 00512000
*   /* THE RETURN CODES FROM IEEBUFSC ARE CHECKED. IF RC = 4 THEN THE*/ 00513000
*   /* ERROR MSG IEE308I 'RESET TERM LENGTH ERROR IS ISSUED'. IF THE */ 00514000
*   /* RC = 8 THEN THE KEYWORD WAS MISSING AND THE ERROR MSG IEE309I */ 00515000
*   /* 'RESET UNIDENTIFIABLE KEYWORD' ISSUED.                        */ 00516000
*   /*                                                               */ 00517000
*   /*****************************************************************/ 00518000
*                                                                  0163 00519000
*   IF REG15=4 THEN                 /* RC=4 VALUE MISSING            */ 00520000
         CH    REG15,@CH00061                                      0163 00521000
         BNE   @RF00163                                            0163 00522000
*     XAE='08'X;                                                   0164 00523000
         MVI   XAE(XSAPTR),X'08'                                   0164 00524000
*   ELSE                                                           0165 00525000
*     DO;                                                          0165 00526000
         B     @RC00163                                            0165 00527000
@RF00163 DS    0H                                                  0166 00528000
*       IF REG15=8 THEN             /* RC=8 KEYWORD MISSING          */ 00529000
         CH    REG15,@CH00040                                      0166 00530000
         BNE   @RF00166                                            0166 00531000
*         XAE='09'X;                                               0167 00532000
         MVI   XAE(XSAPTR),X'09'                                   0167 00533000
*       ELSE                                                       0168 00534000
*         XAE='FF'X;                /* CANT IDENTIFY RC              */ 00535000
         B     @RC00166                                            0168 00536000
@RF00166 MVI   XAE(XSAPTR),X'FF'                                   0168 00537000
*     END;                                                         0169 00538000
*   END;                                                           0170 00539000
@EL00008 DS    0H                                                  0170 00540000
@EF00008 DS    0H                                                  0170 00541000
@ER00008 BR    @14                                                 0170 00542000
*CKVALUE:                                                          0171 00543000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0171 00544000
*                                                                  0171 00545000
CKVALUE  DS    0H                                                  0172 00546000
*   /*****************************************************************/ 00547000
*   /*                                                               */ 00548000
*   /* IEEBUFSC HAS LOCATED THE KEYWORD AND REG 5 POINTS TO THE      */ 00549000
*   /* VALUE. A CHECK IS NOW MADE THAT THE VALUE HAS THE CORRECT     */ 00550000
*   /* LENGTH AND IS ALL NUMERIC.                                    */ 00551000
*   /*                                                               */ 00552000
*   /*****************************************************************/ 00553000
*                                                                  0172 00554000
*   REG7=REG5;                      /* SAVE VALUE PTR                */ 00555000
         LR    REG7,REG5                                           0172 00556000
*   REG4=REG1;                      /* SAVE LENTH OF VALUE           */ 00557000
         LR    REG4,REG1                                           0173 00558000
*   IF REG1>3 THEN                  /* COMPARE SCANNED VALUE'S LENGTH   00559000
*                                      WITH MAX LENGTH               */ 00560000
         CH    REG1,@CH00431                                       0174 00561000
         BNH   @RF00174                                            0174 00562000
*     XAE='08'X;                    /* FLAG FOR ERROR MSG IF TOO   0175 00563000
*                                      LARGE                         */ 00564000
         MVI   XAE(XSAPTR),X'08'                                   0175 00565000
*   ELSE                            /* IF LENGTH CORRECT THEN CK IF     00566000
*                                      NUMERIC                       */ 00567000
*     DO WHILE(REG1>0);             /* SETUP DO LOOP TO CK ALL     0176 00568000
*                                      NUMBERS                       */ 00569000
         B     @RC00174                                            0176 00570000
@RF00174 B     @DE00176                                            0176 00571000
@DL00176 DS    0H                                                  0177 00572000
*       IF PERVALUE(1)>'9'|PERVALUE(1)<'0' THEN/* IS IT A NUMERIC  0177 00573000
*                                      VALUE?                        */ 00574000
         CLI   PERVALUE(REG5),C'9'                                 0177 00575000
         BH    @RT00177                                            0177 00576000
         CLI   PERVALUE(REG5),C'0'                                 0177 00577000
         BNL   @RF00177                                            0177 00578000
@RT00177 DS    0H                                                  0178 00579000
*         XAE='06'X;                /* NO, SETUP ERROR MSG           */ 00580000
         MVI   XAE(XSAPTR),X'06'                                   0178 00581000
*       REG5=REG5+1;                /* ADVANCE COL PTR REG           */ 00582000
@RF00177 AH    REG5,@CH00047                                       0179 00583000
*       REG1=REG1-1;                /* DECREASE LENGTH               */ 00584000
         BCTR  REG1,0                                              0180 00585000
*     END;                                                         0181 00586000
@DE00176 LTR   REG1,REG1                                           0181 00587000
         BP    @DL00176                                            0181 00588000
*   IF XAE=0 THEN                   /* ALL ARE NUMERIC               */ 00589000
@RC00174 CLI   XAE(XSAPTR),0                                       0182 00590000
         BNE   @RF00182                                            0182 00591000
*     DO;                           /* THEN MOVE VALUE               */ 00592000
*       REG5=REG7;                  /* RESTORE STARTING PTR          */ 00593000
         LR    REG5,REG7                                           0184 00594000
*       REG7=5-REG4;                /* GET CORRECT BYTE LOCATION TO     00595000
*                                      MOVE VALUE INTO               */ 00596000
         LA    REG7,5                                              0185 00597000
         SLR   REG7,REG4                                           0185 00598000
*       ZONEAREA(REG7:4)=PERVALUE(1:REG4);/* MOVE TO HOLDAREA FOR  0186 00599000
*                                      CONVERT TO BINARY             */ 00600000
         LA    @10,ZONEAREA-1(REG7)                                0186 00601000
         LA    @09,4                                               0186 00602000
         SLR   @09,REG7                                            0186 00603000
         EX    @09,@SM01255                                        0186 00604000
*       IF ZONEAREA(2:4)^>'255' THEN/* VALUE GREATER THAN 255        */ 00605000
         CLC   ZONEAREA+1(3),@CC01237                              0187 00606000
         BH    @RF00187                                            0187 00607000
*         REG15=0;                  /* YES,PUT OK RC IN REG15        */ 00608000
         SLR   REG15,REG15                                         0188 00609000
*       ELSE                                                       0189 00610000
*         DO;                                                      0189 00611000
         B     @RC00187                                            0189 00612000
@RF00187 DS    0H                                                  0190 00613000
*           REG15=4;                /* NO, PUT NO GOOD RC IN 15      */ 00614000
         LA    REG15,4                                             0190 00615000
*           XAE='3D'X;              /* XAE = UNACCEPTABLE VALUE      */ 00616000
         MVI   XAE(XSAPTR),X'3D'                                   0191 00617000
*           XAV=PERFORM;            /* MOVE IN PERFORM KEYWD         */ 00618000
         MVC   XAV(8,XSAPTR),PERFORM                               0192 00619000
*         END;                                                     0193 00620000
*     END;                                                         0194 00621000
*   ELSE                                                           0195 00622000
*     REG15=4;                      /* PUT BAD RC IN R15             */ 00623000
         B     @RC00182                                            0195 00624000
@RF00182 LA    REG15,4                                             0195 00625000
*   END;                                                           0196 00626000
@EL00009 DS    0H                                                  0196 00627000
@EF00009 DS    0H                                                  0196 00628000
@ER00009 BR    @14                                                 0196 00629000
*SYSEVENT:                                                         0197 00630000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0197 00631000
*                                                                  0197 00632000
SYSEVENT DS    0H                                                  0198 00633000
*   /*****************************************************************/ 00634000
*   /*                                                               */ 00635000
*   /* BEFORE ISSUING THE SYSEVENT MACRO THE PERFORM VALUE IS        */ 00636000
*   /* CONVERTED TO BINARY AND THE CONVERTED VALUE MOVED INTO THE 3RD*/ 00637000
*   /* BYTE OF REGISTER 1. THE ASID FROM THE CSCB IS PLACED INTO THE */ 00638000
*   /* TWO LOW ORDER BYTES OF REGISTER 0.                            */ 00639000
*   /*                                                               */ 00640000
*   /*****************************************************************/ 00641000
*                                                                  0198 00642000
*   REG0=CHASID;                    /* GET ASID FROM CSCB            */ 00643000
         LH    REG0,CHASID(,CSCBPTR)                               0198 00644000
*   GEN;                                                           0199 00645000
     PACK PACKAREA,ZONEAREA            PACK PERFORM VALUE               00646000
     CVB  REG1,PACKAREA                CONVERT TO BINARY                00647000
     SYSEVENT  RESETPG,ASID=(0)        OF REG1.                         00648000
     SLL REG1,16                       MOVE OUT ANY EXTRA               00649000
     SRL REG1,24                       DATA BEFORE DOING COMPARE        00650000
*   END;                                                           0200 00651000
@EL00010 DS    0H                                                  0200 00652000
@EF00010 DS    0H                                                  0200 00653000
@ER00010 BR    @14                                                 0200 00654000
*GET0503D:                                                         0201 00655000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0201 00656000
*                                                                  0201 00657000
GET0503D DS    0H                                                  0202 00658000
*   /*****************************************************************/ 00659000
*   /*                                                               */ 00660000
*   /* THIS ROUTINE LOADS MSG MODULE IEE0503D. BRANCHES TO IT TO     */ 00661000
*   /* ISSUE ALL MESSAGES THEN DELETES IT UPON RETURN.               */ 00662000
*   /*                                                               */ 00663000
*   /*****************************************************************/ 00664000
*                                                                  0202 00665000
*   REG6=REG14;                     /* SAVE RETURN ADDRESS           */ 00666000
         LR    REG6,REG14                                          0202 00667000
*   GEN(LOAD EP=IEE0503D);          /* LOAD IN IEE0503D              */ 00668000
         LOAD EP=IEE0503D                                               00669000
*   REG15=REG0;                     /* GET ENTRY ADDRESS             */ 00670000
         LR    REG15,REG0                                          0204 00671000
*   GEN(BALR REG14,REG15);          /* BRANCH TO MSG MODULE          */ 00672000
         BALR REG14,REG15                                               00673000
*   GEN(DELETE EP=IEE0503D);        /* DELETE IEE0503D               */ 00674000
