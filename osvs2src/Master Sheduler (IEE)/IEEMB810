         ST    REG5,BUFPTR(,REG1)                                  0093 00205000
*   CMDSIZE=5;                      /* STORE LENGTH OF CMD           */ 00206000
         MVI   CMDSIZE(REG1),X'05'                                 0094 00207000
*   COMMAND=RESET;                  /* STORE VERB                    */ 00208000
         MVI   COMMAND+5(REG1),C' '                                0095 00209000
         MVC   COMMAND+6(5,REG1),COMMAND+5(REG1)                   0095 00210000
         MVC   COMMAND(5,REG1),RESET                               0095 00211000
*   RESPECIFY                                                      0096 00212000
*    (REG11,                                                       0096 00213000
*     REG12,                                                       0096 00214000
*     REG13) RESTRICTED;                                           0096 00215000
*   REG4SAV=REG4;                   /* SAVE RETURN ADDRESS           */ 00216000
         ST    REG4,REG4SAV(,REG1)                                 0097 00217000
