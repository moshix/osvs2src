*                                                                  0047 00023000
*   /*****************************************************************/ 00024000
*   /*                                                               */ 00025000
*   /* WHEN ENTERED REG 1 POINTS TO RESET CSCB. A STAE ENVIRONMENT IS*/ 00026000
*   /* CREATED AND A DUMMY XSA IS BUILT TO BE PASSED TO MESSAGE      */ 00027000
*   /* MODULE IEE0503D. THE JOBNAME ENTERED WITH THE RESET VERB IS   */ 00028000
*   /* SCANNED OFF AND MOVED INTO A BLANKED OUT AREA FOR LATER       */ 00029000
*   /* COMPARISON.                                                   */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                                  0047 00033000
*   REG14SAV=REG14;                 /* SAVE RETURN ADDRESS           */ 00034000
         ST    REG14,REG14SAV                                      0047 00035000
*   CALL ISSUSTAE;                  /* CREATE STAE ENVIRONMENT       */ 00036000
         BAL   @14,ISSUSTAE                                        0048 00037000
*   IF REG15=0 THEN                 /* IF STAE CREATED CONTINUE      */ 00038000
         LTR   REG15,REG15                                         0049 00039000
         BNZ   @RF00049                                            0049 00040000
*     DO;                                                          0050 00041000
*       CALL BUILDXSA;              /* BUILD DUMMY XSA FOR ACCESS BY    00042000
*                                      MSG MOD                       */ 00043000
         BAL   @14,BUILDXSA                                        0051 00044000
*       CALL SCANINIT;              /* INITIALIZE FOR THE SCAN OF THE   00045000
*                                      JOBNAME                       */ 00046000
         BAL   @14,SCANINIT                                        0052 00047000
*       CALL SCANNAME;              /* SCAN THE JOBNAME              */ 00048000
*                                                                  0053 00049000
         BAL   @14,SCANNAME                                        0053 00050000
*       /*************************************************************/ 00051000
*       /*                                                           */ 00052000
*       /* THE POINTER TO THE CSCB IS PLACED INTO REG3 AND A         */ 00053000
*       /* COMPARISON MADE OF THE JOBNAME FOUND IN THE CSCB WITH THE */ 00054000
*       /* JOBNAME FOUND IN THE INPUT BUFFER. IF NOT EQUAL THE NEXT  */ 00055000
*       /* CSCB ON THE CHAIN IS ACCESSED AND THE JOBNAME IN IT       */ 00056000
*       /* COMPARED ALL ERRORS ARE FLAGGED BY MOVING A VALUE INTO THE*/ 00057000
*       /* XAE FIELD OF THE XSA. ANY ERRORS TERMINATE THE DO LOOP.   */ 00058000
*       /* WHEN ALL CSCBS HAVE BEEN ACCESSED THE LOOP WILL ALSO      */ 00059000
*       /* TERMINATE.                                                */ 00060000
*       /*                                                           */ 00061000
*       /*************************************************************/ 00062000
*                                                                  0054 00063000
*       GEN(ENQ  (QN,RN,S,3,SYSTEM));/* SERIALIZE CSCB USE           */ 00064000
         ENQ  (QN,RN,S,3,SYSTEM)                                        00065000
*       REG3=BACHN;                 /* GET THE POINTER TO THE CSCB 0055 00066000
*                                      CHAIN.                        */ 00067000
         L     @10,CVTPTR                                          0055 00068000
         L     @10,CVTMSER(,@10)                                   0055 00069000
         L     REG3,BACHN(,@10)                                    0055 00070000
*       DO WHILE(XAE=0&REG3^=0);                                   0056 00071000
         B     @DE00056                                            0056 00072000
@DL00056 DS    0H                                                  0057 00073000
*         IF CHKEY(1:8)=BUFFER1 THEN/* COMPARE THE JOBNAME FOUND IN     00074000
*                                      THE INPUT BUFFER WITH THE NAME   00075000
*                                      FOUND IN THE CSCB.            */ 00076000
         CLC   CHKEY(8,CSCBPTR),BUFFER1                            0057 00077000
         BNE   @RF00057                                            0057 00078000
*           DO;                                                    0058 00079000
*                                                                  0058 00080000
*             /*******************************************************/ 00081000
*             /*                                                     */ 00082000
*             /* IF THE NAMES COMPARE EQUAL, THEN THE JOB IS ACTIVE  */ 00083000
*             /* IEEBUFSC MACRO IS USED TO SCAN OFF THE 'PERFORM'    */ 00084000
*             /* KEYWORD. THE RETURN CODES FROM IEEBUFSC ARE CHECKED.*/ 00085000
*             /* IF ZERO THEN THE PERFORM VALUE IS CHECKED TO MAKE   */ 00086000
*             /* SURE IT IS A NUMERIC VALUE OF THE CORRECT LENGTH.   */ 00087000
*             /*                                                     */ 00088000
*             /*******************************************************/ 00089000
*                                                                  0059 00090000
*             CALL GETBUFSC;        /* THE IEEBUFSC ROUTINE          */ 00091000
         BAL   @14,GETBUFSC                                        0059 00092000
*             IF REG15=0 THEN       /* IF ALL WENT WELL              */ 00093000
         LTR   REG15,REG15                                         0060 00094000
         BNZ   @RF00060                                            0060 00095000
*               DO;                 /* THEN CONTINUE.                */ 00096000
*                 CALL CKVALUE;     /* CK LENGTH AND WHETHER NUMERIC    00097000
*                                      OR NOT.                       */ 00098000
         BAL   @14,CKVALUE                                         0062 00099000
*                 IF REG15=0 THEN                                  0063 00100000
