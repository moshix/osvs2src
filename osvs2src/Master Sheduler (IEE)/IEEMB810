*   REG12=REG12SAV;                 /* RESTORE BASE REG              */ 00240000
         L     REG12,REG12SAV(,REG1)                               0109 00241000
*   REG13=REG13SAV;                 /* RESTORE SAVE AREA PTR         */ 00242000
         L     REG13,REG13SAV(,REG1)                               0110 00243000
*   RESPECIFY                                                      0111 00244000
*    (REG11,                                                       0111 00245000
*     REG12,                                                       0111 00246000
*     REG13) UNRESTRICTED;                                         0111 00247000
*   REG5=REG15;                     /* SAVE RETURN CODE              */ 00248000
         LR    REG5,REG15                                          0112 00249000
*   GEN(DELETE EP=IEECB860);        /* DELETE STAE CREATE MOD        */ 00250000
         DELETE EP=IEECB860                                             00251000
*   REG15=REG5;                     /* PUT RETURN CODE INTO R15      */ 00252000
         LR    REG15,REG5                                          0114 00253000
*   REG14=REG4;                     /* RESTORE THE RETURN ADDR       */ 00254000
         LR    REG14,REG4                                          0115 00255000
*   END;                                                           0116 00256000
