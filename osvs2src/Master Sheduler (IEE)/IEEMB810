*BUILDXSA:                                                         0117 00260000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0117 00261000
*                                                                  0117 00262000
BUILDXSA DS    0H                                                  0118 00263000
*   /*****************************************************************/ 00264000
*   /*                                                               */ 00265000
*   /* THIS ROUTINE BUILDS A DUMMY XSA TO PASS TO MESSAGE MODULE     */ 00266000
*   /* IEE0503D. THE XSA WILL BE USED FOR ALL MESSAGES.              */ 00267000
*   /*                                                               */ 00268000
*   /*****************************************************************/ 00269000
*                                                                  0118 00270000
*   REG2=ADDR(WORKAREA);            /* GET ADDR FOR XSA              */ 00271000
         LA    REG2,WORKAREA                                       0118 00272000
*   REG0=LENGTH(XSAMAP);            /* GET SIZE OF CORE              */ 00273000
         LA    REG0,136                                            0119 00274000
*   XSAMAP(1:REG0)=XSAMAP(1:REG0)&&XSAMAP(1:REG0);/* ZERO OUT THE  0120 00275000
*                                      CORE                          */ 00276000
         LR    @10,REG0                                            0120 00277000
         BCTR  @10,0                                               0120 00278000
         EX    @10,@SX01250                                        0120 00279000
*   XAV=RESET;                      /* PLACE RESET VERB IN XSA FOR 0121 00280000
*                                      MSG MOD                       */ 00281000
         MVI   XAV+5(XSAPTR),C' '                                  0121 00282000
         MVC   XAV+6(2,XSAPTR),XAV+5(XSAPTR)                       0121 00283000
         MVC   XAV(5,XSAPTR),RESET                                 0121 00284000
*   XAU=CHUCMP;                     /* STORE ISSUING CONSOLE ID      */ 00285000
         MVC   XAU(1,XSAPTR),CHUCMP(CSCBPTR)                       0122 00286000
*   END;                                                           0123 00287000
@EL00003 DS    0H                                                  0123 00288000
