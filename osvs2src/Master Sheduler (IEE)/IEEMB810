@ER00003 BR    @14                                                 0123 00290000
*FREECORE:                                                         0124 00291000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0124 00292000
*                                                                  0124 00293000
FREECORE DS    0H                                                  0125 00294000
*   /*****************************************************************/ 00295000
*   /*                                                               */ 00296000
*   /* THIS ROUTINE ISSUES THE MGCR MACRO TO FREE THE CSCB           */ 00297000
*   /*                                                               */ 00298000
*   /*****************************************************************/ 00299000
*                                                                  0125 00300000
*   REG8->CHFC='1'B;                /* TURN ON FREE CSCB SWITCH IN 0125 00301000
*                                      CSCB                          */ 00302000
         OI    CHFC(REG8),B'00000010'                              0125 00303000
*   REG1=REG8;                      /* GET RESET CSCB PTR            */ 00304000
         LR    REG1,REG8                                           0126 00305000
*   GEN(MGCR (1),CHAIN);            /* REMOVE CSCB FROM CHAIN AND  0127 00306000
*                                      FREE CORE                     */ 00307000
         MGCR (1),CHAIN                                                 00308000
*   END;                                                           0128 00309000
@EL00004 DS    0H                                                  0128 00310000
@EF00004 DS    0H                                                  0128 00311000
@ER00004 BR    @14                                                 0128 00312000
*SCANINIT:                                                         0129 00313000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0129 00314000
*                                                                  0129 00315000
SCANINIT DS    0H                                                  0130 00316000
*   /*****************************************************************/ 00317000
*   /*                                                               */ 00318000
*   /* THIS ROUTINE BLANKS OUT THE BUFFER THAT WILL HOLD THE JOBNAME */ 00319000
*   /* AND INITIALIZES REG 1 TO POINT TO THE BEGINNING OF THE INPUT  */ 00320000
*   /* BUFFER AND REG 0 TO POINT TO THE END OF THE BUFFER            */ 00321000
*   /*                                                               */ 00322000
*   /*****************************************************************/ 00323000
*                                                                  0130 00324000
*   REG1=ADDR(REG8->CHBUF);         /* GET POINTER TO KEYWORD        */ 00325000
         LA    REG1,CHBUF(,REG8)                                   0130 00326000
*   BUFFER1(1:8)=' ';               /* PUT BLANKS IN HOLD AREA       */ 00327000
         MVI   BUFFER1+1,C' '                                      0131 00328000
         MVC   BUFFER1+2(6),BUFFER1+1                              0131 00329000
         MVI   BUFFER1,C' '                                        0131 00330000
*   SIZE=0;                         /* LENGTH OF JOBNAME SAVED IN  0132 00331000
*                                      SIZE FIELD                    */ 00332000
         MVI   SIZE,X'00'                                          0132 00333000
*   REG0=ADDR(REG8->CHTYPE);        /* GET END OF INPUT BUFFER + 1   */ 00334000
         LA    REG0,CHTYPE(,REG8)                                  0133 00335000
*   END;                                                           0134 00336000
@EL00005 DS    0H                                                  0134 00337000
@EF00005 DS    0H                                                  0134 00338000
@ER00005 BR    @14                                                 0134 00339000
*SCANNAME:                                                         0135 00340000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0135 00341000
*                                                                  0135 00342000
SCANNAME DS    0H                                                  0136 00343000
*   /*****************************************************************/ 00344000
*   /*                                                               */ 00345000
*   /* THIS ROUTINE SCANS THE JOBNAME FROM THE COMMAND INPUT BUFFER. */ 00346000
*   /* ONCE SCANNED IT IS MOVED TO THE HOLDAREA FOR LATER COMPARISON */ 00347000
*   /* WITH THE NAMES FOUND IN THE CSCBS. IF NO NAME IS FOUND THEN WE*/ 00348000
*   /* TERMINATE AND ISSUE ERROR MSG IEE308I 'RESET TERM LENGTH      */ 00349000
*   /* ERROR'.                                                       */ 00350000
*   /*                                                               */ 00351000
*   /*****************************************************************/ 00352000
*                                                                  0136 00353000
*   DO WHILE(SCANCOL^=','&REG1^=REG0);/* SCAN FOR COMMA OR END OF  0136 00354000
*                                      BUFFER.                       */ 00355000
         B     @DE00136                                            0136 00356000
@DL00136 DS    0H                                                  0137 00357000
*     REG1=REG1+1;                  /* INCREMENT SCAN PTR            */ 00358000
         LA    @10,1                                               0137 00359000
         ALR   REG1,@10                                            0137 00360000
*     SIZE=SIZE+1;                  /* INCREMENT LENGTH COUNTER.     */ 00361000
         MVC   @ZT00003+3(1),SIZE                                  0138 00362000
         AL    @10,@ZT00003                                        0138 00363000
         STC   @10,SIZE                                            0138 00364000
*   END;                                                           0139 00365000
@DE00136 CLI   SCANCOL(REG1),C','                                  0139 00366000
         BE    @DC00136                                            0139 00367000
         CR    REG1,REG0                                           0139 00368000
         BNE   @DL00136                                            0139 00369000
@DC00136 DS    0H                                                  0140 00370000
*   IF REG1^=REG0 THEN              /* END OF BUFFER?                */ 00371000
         CR    REG1,REG0                                           0140 00372000
         BE    @RF00140                                            0140 00373000
*     DO;                           /* IF NOT THEN PROCEED           */ 00374000
*       IF SIZE=0|SIZE>8 THEN       /* IF THE LENGTH IS 0 OR >8 THEN    00375000
*                                      NO JOBNAME WAS FOUND          */ 00376000
         CLI   SIZE,0                                              0142 00377000
         BE    @RT00142                                            0142 00378000
         CLI   SIZE,8                                              0142 00379000
         BNH   @RF00142                                            0142 00380000
@RT00142 DS    0H                                                  0143 00381000
*         XAE='08'X;                /* PLACE ERROR MSG ID INTO XAE 0143 00382000
*                                      FIELD                         */ 00383000
         MVI   XAE(XSAPTR),X'08'                                   0143 00384000
*       ELSE                                                       0144 00385000
*         DO;                                                      0144 00386000
         B     @RC00142                                            0144 00387000
@RF00142 DS    0H                                                  0145 00388000
*           REG1=ADDR(REG8->CHBUF); /* POINT TO BEGINNING OF       0145 00389000
*                                      PARAMETER                     */ 00390000
         LA    REG1,CHBUF(,REG8)                                   0145 00391000
*           BUFFER1(1:SIZE)=INBUFFER(1:SIZE);                      0146 00392000
         SLR   @10,@10                                             0146 00393000
         IC    @10,SIZE                                            0146 00394000
         BCTR  @10,0                                               0146 00395000
         EX    @10,@SM01253                                        0146 00396000
*         END;                                                     0147 00397000
*     END;                                                         0148 00398000
*   ELSE                                                           0149 00399000
*     XAE='08'X;                    /* EXCEEDED BUFFER               */ 00400000
         B     @RC00140                                            0149 00401000
@RF00140 MVI   XAE(XSAPTR),X'08'                                   0149 00402000
*   RETURN;                         /* RETURN TO CALLER              */ 00403000
@EL00006 DS    0H                                                  0150 00404000
@EF00006 DS    0H                                                  0150 00405000
