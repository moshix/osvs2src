*ISSUSTAE:                                                         0086 00179000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0086 00180000
*                                                                  0086 00181000
ISSUSTAE DS    0H                                                  0087 00182000
*   /*****************************************************************/ 00183000
*   /*                                                               */ 00184000
*   /* THIS ROUTINE CREATES A STAE ENVIRONMENT THRU MODULE IEECB860. */ 00185000
*   /* A PARAMETER LIST IS BUILT AND PASSED TO MODULE IEECB860 VIA   */ 00186000
*   /* REGISTER 1.                                                   */ 00187000
*   /*                                                               */ 00188000
*   /*****************************************************************/ 00189000
*                                                                  0087 00190000
*   REG8=REG1;                      /* SAVE SETS CSCB PTR            */ 00191000
         LR    REG8,REG1                                           0087 00192000
*   REG4=REG14;                     /* SAVE RETURN ADDR              */ 00193000
         LR    REG4,REG14                                          0088 00194000
