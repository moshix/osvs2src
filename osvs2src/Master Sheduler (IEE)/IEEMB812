*                                                                  0127 00464000
*IRARMWTR:                                                         0127 00465000
*   ENTRY                           /*                       @Z40BPCH*/ 00466000
*       (MBWTLIST,MBIPDATP);        /*                       @Z40BPCH*/ 00467000
@EP00127 MVC   @PC00001+16(4),0(@01)                               0127 00468000
         MVC   @PC00001+12(4),4(@01)                               0127 00469000
*   WTLPARML(5:92)=MBWTLIST;                                       0128 00470000
         L     @12,@PC00001+12                                     0128 00471000
         L     @12,MBMSGLTP(,@12)                                  0128 00472000
         L     @11,@PC00001+16                                     0128 00473000
         MVC   WTLPARML+4(88,@12),MBWTLIST(@11)                    0128 00474000
*   GPR01P=ADDR(WTLPARML);          /* ADDR OF WTL PARM LIST         */ 00475000
         LR    GPR01P,@12                                          0129 00476000
*   GEN( WTL MF=(E,(1)) );                                         0130 00477000
          WTL MF=(E,(1))                                                00478000
*   RETURN;                         /*                       @Z40BPCH*/ 00479000
