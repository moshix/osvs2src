*                                                                  0104 00337000
*/********************************************************************/ 00338000
*/*                                                                  */ 00339000
*/*  NAME = SET IPS PROCESSOR READ ROUTINE                           */ 00340000
*/*  PURPOSE = GET A BUFFER FROM SUBPOOL 0 AND READ A RECORD FROM    */ 00341000
*/*            SYS1.PARMLIB MEMBER                                   */ 00342000
*/*  LINKAGE = BALR                                                  */ 00343000
*/*  INPUT = NONE                                                    */ 00344000
*/*  OUTPUT = REG 15 - RETURN CODE=0 IF READ IS SUCCESSFUL  @Z40BPCH */ 00345000
*/*                               =4 IF READ IS UNSUCCESSFU @Z40BPCH */ 00346000
*/*           IF RETURN CODE=0 :                            @Z40BPCH */ 00347000
*/*           (1) ADDRESS OF BUFFER                         @Z40BPCH */ 00348000
*/*           (2) LENGTH OF DATA AREA IN BUFFER                      */ 00349000
*/*           (3) SUBPOOL NO. AND LENGTH OF BUFFER                   */ 00350000
*/*  EXIT-NORMAL = RETURN TO CALLER                                  */ 00351000
*/*   RETURN CODE- 0 IF READ IS SUCCESSFUL                           */ 00352000
*/*                4 IF READ IS UNSUCCESSFUL                         */ 00353000
*/*  EXIT-ERROR = MB812INE                                  @Z40BPCH */ 00354000
*/*                                                                  */ 00355000
*/********************************************************************/ 00356000
*                                                                  0104 00357000
*IRARMRDR:                                                         0104 00358000
*   ENTRY                           /*                       @Z40BPCH*/ 00359000
*       (MBRDBUFP,                  /*                       @Z40BPCH*/ 00360000
*       MBRDBUFL,MBRDSPLN,MBIPDATP);/*                       @Z40BPCH*/ 00361000
@EP00104 MVC   @PC00001(16),0(@01)                                 0104 00362000
*   IF IOERSTAT='1'B|EODSTAT='1'B   /*                       @Z40BPCH*/ 00363000
*     THEN                          /* I/O ERROR OR EOF              */ 00364000
         L     @12,@PC00001+12                                     0105 00365000
         L     @12,MBSTATP(,@12)                                   0105 00366000
         TM    IOERSTAT(@12),B'01100000'                           0105 00367000
         BZ    @RF00105                                            0105 00368000
*     RETURN CODE(FAIL);            /* RETURN UNSUCCESSFUL           */ 00369000
         L     @13,4(,@13)                                         0106 00370000
         L     @00,@SIZDATD                                        0106 00371000
         LR    @01,@09                                             0106 00372000
         FREEMAIN R,LV=(0),A=(1)                                        00373000
         LA    @15,4                                               0106 00374000
         L     @14,12(,@13)                                        0106 00375000
         LM    @00,@12,20(@13)                                     0106 00376000
         BR    @14                                                 0106 00377000
*   ELSE                            /* READ SUCCESSFUL               */ 00378000
*     DO;                                                          0107 00379000
@RF00105 DS    0H                                                  0108 00380000
*       RESPECIFY                                                  0108 00381000
*         MBBUFPTR RSTD;                                           0108 00382000
*                                                                  0108 00383000
*/*  GET A BUFFER OF 88 BYTES FROM SUBPOOL 0                         */ 00384000
*                                                                  0109 00385000
*       GPR00P=RECORDSZ;            /* SUBPOOL NO=0, SIZE=88         */ 00386000
         LA    GPR00P,88                                           0109 00387000
*       GEN( GETMAIN R,LV=(0) );                                   0110 00388000
          GETMAIN R,LV=(0)                                              00389000
*       MBBUFPTR=GPR01P;            /* ADDR OF BUFFER                */ 00390000
         LR    MBBUFPTR,GPR01P                                     0111 00391000
*       MBRDBUFP=MBBUFPTR;          /* ADDR OF BUFFER                */ 00392000
         L     @12,@PC00001                                        0112 00393000
         ST    MBBUFPTR,MBRDBUFP(,@12)                             0112 00394000
*       MBRDBUFL=RECORDSZ;          /* LENGTH OF BUFFER              */ 00395000
         LA    @12,88                                              0113 00396000
         L     @11,@PC00001+4                                      0113 00397000
         ST    @12,MBRDBUFL(,@11)                                  0113 00398000
*       MBRDSPLN=RECORDSZ;          /* SUBPOOL INFORMATION           */ 00399000
         L     @11,@PC00001+8                                      0114 00400000
         ST    @12,MBRDSPLN(,@11)                                  0114 00401000
*       MBBLANK=BLANK;              /* INSERT BLANK IN 1ST BYTE      */ 00402000
         MVI   MBBLANK(MBBUFPTR),C' '                              0115 00403000
*       MBBUFPTR=MBBUFPTR+ONE;      /* POINT TO 2ND BYTE OF BUFFER   */ 00404000
         AL    MBBUFPTR,@CF00062                                   0116 00405000
*       RFY                                                        0117 00406000
*        (R7,                                                      0117 00407000
*         R8) RSTD;                 /*                       @Z40BPCH*/ 00408000
*       R7=MBREADLP;                /* ADDR OR READ AREA     @Z40BPCH*/ 00409000
         L     @12,@PC00001+12                                     0118 00410000
         L     R7,MBREADLP(,@12)                                   0118 00411000
*       R8=MBPLDCBP;                /* ADDR OF PARMLB DCB    @Z40BPCH*/ 00412000
         L     R8,MBPLDCBP(,@12)                                   0119 00413000
*/*  READ A RECORD FROM SYS1.PARMLIB.                                */ 00414000
*/*  CHECK FOR I/O ERROR AND END OF DATA SET CONDITIONS              */ 00415000
*                                                                  0120 00416000
*       GEN REFS(R7,R8,MBBUFPTR);                                  0120 00417000
*                                     /*                    @Z40BPCH*/  00418000
         READ  (R7),SF,(R8),(MBBUFPTR),'S',MF=E            @Z40BPCH     00419000
         CHECK (R7)                                         @Z40BPCH    00420000
*       RFY                                                        0121 00421000
*        (R7,                                                      0121 00422000
*         R8) UNRSTD;               /*                       @Z40BPCH*/ 00423000
*       IF IOERSTAT='1'B            /* I/O ERROR HAS OCCURRED        */ 00424000
*           |EODSTAT='1'B           /* END OF DATA SET HAS BEEN    0122 00425000
*                                      REACHED                       */ 00426000
*         THEN                      /* READ UNSUCCESSFUL             */ 00427000
         L     @12,@PC00001+12                                     0122 00428000
         L     @12,MBSTATP(,@12)                                   0122 00429000
         TM    IOERSTAT(@12),B'01100000'                           0122 00430000
         BZ    @RF00122                                            0122 00431000
*         RETURN CODE(FAIL);        /* RETURN UNSUCCESSFUL           */ 00432000
         L     @13,4(,@13)                                         0123 00433000
         L     @00,@SIZDATD                                        0123 00434000
         LR    @01,@09                                             0123 00435000
         FREEMAIN R,LV=(0),A=(1)                                        00436000
         LA    @15,4                                               0123 00437000
         L     @14,12(,@13)                                        0123 00438000
         LM    @00,@12,20(@13)                                     0123 00439000
         BR    @14                                                 0123 00440000
*       ELSE                        /* READ SUCCESSFUL               */ 00441000
*         RETURN CODE(SUCCESS);     /* RETURN SUCCESSFUL             */ 00442000
@RF00122 L     @13,4(,@13)                                         0124 00443000
         L     @00,@SIZDATD                                        0124 00444000
         LR    @01,@09                                             0124 00445000
         FREEMAIN R,LV=(0),A=(1)                                        00446000
         SLR   @15,@15                                             0124 00447000
         L     @14,12(,@13)                                        0124 00448000
         LM    @00,@12,20(@13)                                     0124 00449000
         BR    @14                                                 0124 00450000
*     END;                                                         0125 00451000
*   RESPECIFY                                                      0126 00452000
*     MBBUFPTR UNRSTD;                                             0126 00453000
*                                                                  0127 00454000
*/********************************************************************/ 00455000
