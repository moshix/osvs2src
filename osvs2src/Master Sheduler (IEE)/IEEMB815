*   END CHDCOMMA;                                                  0303 00932000
         B     @EL00005                                            0303 00933000
*CHDEOCMD:                                                         0304 00934000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0304 00935000
*                                                                  0304 00936000
CHDEOCMD DS    0H                                                  0305 00937000
*/*      THE END OF COMMAND TRANSLATE ROUTINE HAS TWO ENTRY          */ 00938000
*/*     SITUATIONS WHEN TRT ENCOUNTERS A BLANK                       */ 00939000
*/*    - THIS BLANK MAY BE THE FIRST BLANK AFTER THE END OF          */ 00940000
*/*       THE OPERAND IN THE BUFFER                                  */ 00941000
*/*       THIS BLANK MAY BE THE STOPPER VALUE WHCIH ISOLATED         */ 00942000
*/*       THE LAST PARAMETER OF THE OPERAND                          */ 00943000
*/*                                                                  */ 00944000
*/*    IF WE HAVE JUST ISOLATED A PARMAMETER :                       */ 00945000
*/*      OUTLINE OF PROCESSING  - BY NESTED  IF-THEN LOGIC IT CHECKS */ 00946000
*/*    FOR  - OPTIONS (OF SDATA,PDATA OR SDUMP)                      */ 00947000
*/*      OR - TEST 'KEY31' FOR INDICATION OF 'ALL' OR 'NODUMP'       */ 00948000
*/*      OR - TEST 'YESNO' FOR SPECIFICATION OF QUIESCE CHANGES      */ 00949000
*/*      OR - TEST 'PKEY2' FOR 2ND POSITION KEYWORD                  */ 00950000
*/*      OR - TEST 'PKEY1' FOR 1ST POSITION KEYWORD                  */ 00951000
*/*                                                                  */ 00952000
*                                                                  0305 00953000
*   /*****************************************************************/ 00954000
*   /*                                                               */ 00955000
*   /* TEST TO SEE IF THIS IS THE END OF THE COMMAND. IF THE PREVIOUS*/ 00956000
*   /* STOPPER VALUE FROM TRANSLATE AND TEST WAS THE FUNCTION CODE   */ 00957000
*   /* FOR A RIGHT PARAN, THEN WE ARE PROCESSING THE FIRST BLANK     */ 00958000
*   /* AFTER THE END OF THE COMMAND                                  */ 00959000
*   /*                                                               */ 00960000
*   /*****************************************************************/ 00961000
*                                                                  0305 00962000
*   MSGID=IEE712;                   /* TO INDICATE NOTHING IS LEFT TO   00963000
*                                      BE SCANNED SET MSG INDEX FOR     00964000
*                                      'PROCESSING COMPLETE' @G33CPHE*/ 00965000
         MVI   MSGID,X'03'                                         0305 00966000
*   IF STOPSAVE=RPARNCDE&LEN=0      /* IS THIS THE 1ST BLANK AFTER 0306 00967000
*                                      THE END OF THE OPERAND LIST ?    00968000
*                                      (PREVIOUS STOPPER MUST HAVE 0306 00969000
*                                      BEEN RIGHT PARAN AND LENGTH OF   00970000
*                                      PARAMETER SHOULD BE ZERO)     */ 00971000
*     THEN                                                         0306 00972000
         CLI   STOPSAVE,X'05'                                      0306 00973000
         BNE   @RF00306                                            0306 00974000
         LTR   LEN,LEN                                             0306 00975000
         BZ    @RT00306                                            0306 00976000
*     ;                                                            0307 00977000
*   ELSE                            /* NO - WE HAVE A PARM TO PROCESS*/ 00978000
*     IF OPTIONX='1'B               /* IS THIS A DUMP OPTION ?       */ 00979000
*       THEN                        /* YES                           */ 00980000
@RF00306 TM    OPTIONX,B'00000100'                                 0308 00981000
         BNO   @RF00308                                            0308 00982000
*       DO;                                                        0309 00983000
*         R14SAVE=R14;              /* SAVE RETURN ADDRESS           */ 00984000
         ST    R14,R14SAVE                                         0310 00985000
*         CALL OPSET;               /* HANDLE THIS DUMP OPTION       */ 00986000
         LR    @15,OPSETPTR                                        0311 00987000
         BALR  @14,@15                                             0311 00988000
*         R14=R14SAVE;              /* RESTORE RETURN ADDRESS TO   0312 00989000
*                                      MAINLINE                      */ 00990000
         L     R14,R14SAVE                                         0312 00991000
*       END;                                                       0313 00992000
*     ELSE                          /* NOT AN OPTION- CHECK FOR    0314 00993000
*                                      QUIESCE                       */ 00994000
*       IF YESNO=ON                 /* IS THE YES OR NO OPTION FOR Q    00995000
*                                      EXPECTED?             @G33CPHE*/ 00996000
*         THEN                      /* YES IT IS             @G33CPHE*/ 00997000
         B     @RC00308                                            0314 00998000
@RF00308 TM    YESNO,B'00001000'                                   0314 00999000
         BNO   @RF00314                                            0314 01000000
*         IF LEN=2&OPERAND(1:2)='NO'/* IS IT NO              @G33CPHE*/ 01001000
*           THEN                                                   0315 01002000
         CH    LEN,@CH00077                                        0315 01003000
         BNE   @RF00315                                            0315 01004000
         CLC   OPERAND(2,START),@CC02103                           0315 01005000
         BNE   @RF00315                                            0315 01006000
*           IF ADDR(CHNGWORK)->RTCTSDPG=OFF/* CHECK FOR A CONFLICTING   01007000
*                                      VALUE ALREADY SPECIFIED     0316 01008000
*                                                            @G33CPHE*/ 01009000
*             THEN                  /* Q=NO SPECIFIED        @G33CPHE*/ 01010000
         LA    @15,CHNGWORK                                        0316 01011000
         TM    RTCTSDPG(@15),B'10000000'                           0316 01012000
         BNZ   @RF00316                                            0316 01013000
*             ADDR(CHNGWORK)->RTCTSDPH=ON;/* TURN ON THE OPTION IN 0317 01014000
*                                      THE WORK AREA         @G33CPHE*/ 01015000
         OI    RTCTSDPH(@15),B'01000000'                           0317 01016000
*           ELSE                                                   0318 01017000
*             MSGID=IEE312;         /* INDICATE THE ERROR    @G33CPHE*/ 01018000
         B     @RC00316                                            0318 01019000
@RF00316 MVI   MSGID,X'0C'                                         0318 01020000
*         ELSE                                                     0319 01021000
*           IF LEN=3&OPERAND(1:3)='YES'/* IS IT Q=YES        @G33CPHE*/ 01022000
*             THEN                                                 0319 01023000
         B     @RC00315                                            0319 01024000
@RF00315 CH    LEN,@CH00290                                        0319 01025000
         BNE   @RF00319                                            0319 01026000
         CLC   OPERAND(3,START),@CC02104                           0319 01027000
         BNE   @RF00319                                            0319 01028000
*             IF ADDR(CHNGWORK)->RTCTSDPH=OFF/* CHECK FOR A        0320 01029000
*                                      CONFLICTING VALUE ALREADY   0320 01030000
*                                      SPECIFIED             @G33CPHE*/ 01031000
*               THEN                /* YES                   @G33CPHE*/ 01032000
         LA    @15,CHNGWORK                                        0320 01033000
         TM    RTCTSDPH(@15),B'01000000'                           0320 01034000
         BNZ   @RF00320                                            0320 01035000
*               ADDR(CHNGWORK)->RTCTSDPG=ON;/* TURN ON THE OPTION IN    01036000
*                                      THE WORK AREA         @G33CPHE*/ 01037000
         OI    RTCTSDPG(@15),B'10000000'                           0321 01038000
*             ELSE                                                 0322 01039000
*               MSGID=IEE312;       /* INDICATE THE ERROR    @G33CPHE*/ 01040000
         B     @RC00320                                            0322 01041000
@RF00320 MVI   MSGID,X'0C'                                         0322 01042000
*           ELSE                    /* NOT A VALID VALUE     @G33CPHE*/ 01043000
*             MSGID=IEE708;         /* INDICATE THE ERROR    @G33CPHE*/ 01044000
         B     @RC00319                                            0323 01045000
@RF00319 MVI   MSGID,X'3D'                                         0323 01046000
*       ELSE                                                       0324 01047000
*         IF CDSET='0'B             /* IS FUNCTION NOT A SET (DEL OR    01048000
*                                      RESET) ?                      */ 01049000
*           THEN                                                   0324 01050000
         B     @RC00314                                            0324 01051000
@RF00314 TM    CDSET,B'10000000'                                   0324 01052000
         BNZ   @RF00324                                            0324 01053000
*           DO;                     /*                       @G33CPHE*/ 01054000
*             CDRESET=ON;           /* THIS MUST BE A RESET OPERATION   01055000
*                                      OR AN INVALID COMMAND, BECAUSE   01056000
*                                      ANYTHING ELSE WOULD HAVE HAD     01057000
*                                      OPTIONS               @G33CPHE*/ 01058000
         OI    CDRESET,B'01000000'                                 0326 01059000
*             IF(PKEY1=ON&((LEN=3&OPERAND(1:3)='DEL')|(LEN=5&OPERAND(1: 01060000
*                 5)='RESET')))|((PKEY2=ON|KEY31=ON)&(LEN=3&OPERAND(1:3 01061000
*                 )='ALL'))         /* CHECK FOR ONE OF THE VALID  0327 01062000
