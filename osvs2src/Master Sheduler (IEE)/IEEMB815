*                                                            @G33CPHE*/ 00830000
         LA    DT,4                                                0272 00831000
*               ELSE                /* ERROR - KEYWORD (LENGTH 5) IS    00832000
*                                      NOT SDUMP                     */ 00833000
*                 MSGID=IEE309;     /*                       @G33CPHE*/ 00834000
         B     @RC00271                                            0273 00835000
@RF00271 MVI   MSGID,X'09'                                         0273 00836000
*           END;                    /*                       @G33CPHE*/ 00837000
*       END;                                                       0275 00838000
*     ELSE                          /* KEY POSITION 2 - NOT EXPECTED */ 00839000
*       IF PKEY1='1'B               /* EXPECTING 1ST POSITION KEYWORD*/ 00840000
*         THEN                                                     0276 00841000
         B     @RC00251                                            0276 00842000
@RF00251 TM    PKEY1,B'10000000'                                   0276 00843000
         BNO   @RF00276                                            0276 00844000
*         DO;                                                      0277 00845000
*           PKEY1='0'B;             /* NO LONGER EXPECTING 1ST     0278 00846000
