@RC00282 DS    0H                                                  0287 00882000
@RC00280 DS    0H                                                  0288 00883000
*   /*****************************************************************/ 00884000
*   /*                                                               */ 00885000
*   /* 'COMMA' PROCESSOR -- RESET TRANSLATE TABLE FOR NEXT SCAN      */ 00886000
*   /*                                                               */ 00887000
*   /*****************************************************************/ 00888000
*                                                                  0288 00889000
*   TRANTBL('('+1)=UNEXPCDE;        /*                       @G33CPHE*/ 00890000
@RF00276 DS    0H                                                  0288 00891000
@RC00251 DS    0H                                                  0288 00892000
@RC00212 MVI   TRANTBL+77,X'06'                                    0288 00893000
*   TRANTBL(','+1)=COMMACDE;        /*                       @G33CPHE*/ 00894000
         MVI   TRANTBL+107,X'02'                                   0289 00895000
*   IF OPTIONX='1'B                 /* OPTIONS EXPECTED NEXT ?       */ 00896000
*     THEN                          /* YES                           */ 00897000
