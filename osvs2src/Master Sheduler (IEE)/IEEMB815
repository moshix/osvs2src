         TITLE 'IEEMB815 - CHNGDUMP COMMAND PROCESSOR                  *00001000
                        '                                               00002000
IEEMB815 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(19)                                             0001 00007000
         DC    C'IEEMB815R0300077047'                              0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  LA    @10,4095(,@12)                                      0001 00011000
         USING @PSTART,@12                                         0001 00012000
         USING @PSTART+4095,@10                                    0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @08,@01                                             0001 00016000
         USING @DATD,@08                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LR    @13,@08                                             0001 00019000
*                                                                  0082 00020000
*   /*****************************************************************/ 00021000
*   /*                                                               */ 00022000
*   /* GET AND INITIALIZE WORKAREA                                   */ 00023000
*   /*                                                               */ 00024000
*   /*****************************************************************/ 00025000
*                                                                  0082 00026000
*   PART1=''B;                      /* CLEAR FIRST PART OF WORKAREA  */ 00027000
         XC    PART1(88),PART1                                     0082 00028000
*   XSASAVE=XSAPTR;                 /* SAVE PTR TO XSA               */ 00029000
         ST    XSAPTR,XSASAVE                                      0083 00030000
*   START=XAL;                      /* INITIALIZE PTR TO OPERAND     */ 00031000
*                                                                  0084 00032000
         SLR   START,START                                         0084 00033000
         ICM   START,7,XAL(XSAPTR)                                 0084 00034000
*   /*****************************************************************/ 00035000
*   /*                                                               */ 00036000
*   /* INITIALIZE TRANSLATE TABLE FOR FIRST SCAN INITIALIZE TRANSLATE*/ 00037000
*   /* TABLE TO ALL INVALIDS                                         */ 00038000
*   /*                                                               */ 00039000
*   /*****************************************************************/ 00040000
*                                                                  0085 00041000
*   TRANTBL(1)=INVLDCDE;            /* INITIALIZE FIRST BYTE OF    0085 00042000
*                                      TRANSLATE TABLE TO FUNCTION 0085 00043000
*                                      CODE FOR 'INVALIDS'   @G33CPHE*/ 00044000
         MVI   TRANTBL,X'07'                                       0085 00045000
*   MVC(TRANTBL(2:256),TRANTBL(1:255));/* PROPAGATE INVALIDS @G33CPHE*/ 00046000
         MVC   TRANTBL+1(255),TRANTBL                              0086 00047000
*   TRANTBL(','+1)=COMMACDE;        /* STOPPER CHARACTER AFTER 'SET'    00048000
*                                      OR 'DEL' CAN BE COMMA @G33CPHE*/ 00049000
         MVI   TRANTBL+107,X'02'                                   0087 00050000
*   TRANTBL(' '+1)=EOCMDCDE;        /* STOPPER CHARACTER CAN ALSO BE    00051000
*                                      A BLANK AFTER 'DEL'   @G33CPHE*/ 00052000
         MVI   TRANTBL+64,X'03'                                    0088 00053000
*   TRANTBL('='+1)=UNEXPCDE;        /* A VALID BUT CURRENTLY       0089 00054000
*                                      UNEXPECTED DELIMITER  @G33CPHE*/ 00055000
         MVI   TRANTBL+126,X'06'                                   0089 00056000
*   TRANTBL('('+1)=UNEXPCDE;        /* A VALID BUT CURRENTLY       0090 00057000
*                                      UNEXPECTED DELIMITER  @G33CPHE*/ 00058000
         MVI   TRANTBL+77,X'06'                                    0090 00059000
*   TRANTBL(')'+1)=UNEXPCDE;        /* A VALID BUT CURRENTLY       0091 00060000
*                                      UNEXPECTED DELIMITER  @G33CPHE*/ 00061000
*                                                                  0091 00062000
         MVI   TRANTBL+93,X'06'                                    0091 00063000
*   /*****************************************************************/ 00064000
*   /*                                                               */ 00065000
*   /* DON'T STOP TRANSLATE ON ALPHABETIC CHARACTERS                 */ 00066000
*   /*                                                               */ 00067000
*   /*****************************************************************/ 00068000
*                                                                  0092 00069000
*   TRANTBL('A'+1:'I'+1)=TRANTBL('A'+1:'I'+1)&&TRANTBL('A'+1:'I'+1);    00070000
         XC    TRANTBL+193(9),TRANTBL+193                          0092 00071000
*   TRANTBL('J'+1:'R'+1)=TRANTBL('J'+1:'R'+1)&&TRANTBL('J'+1:'R'+1);    00072000
         XC    TRANTBL+209(9),TRANTBL+209                          0093 00073000
*   TRANTBL('S'+1:'Z'+1)=TRANTBL('S'+1:'Z'+1)&&TRANTBL('S'+1:'Z'+1);    00074000
         XC    TRANTBL+226(8),TRANTBL+226                          0094 00075000
*   TRANTBL('0'+1:'9'+1)=TRANTBL('0'+1:'9'+1)&&TRANTBL('0'+1:'9'+1);    00076000
         XC    TRANTBL+240(10),TRANTBL+240                         0095 00077000
*   ENP=0;                          /* INITIALIZE PTR TO LAST CHAR 0096 00078000
*                                      TRANSLATED                    */ 00079000
         SLR   ENP,ENP                                             0096 00080000
*   PKEY1='1'B;                     /* INDICATE 'SET', 'DEL', OR   0097 00081000
*                                      'RESET' IS EXPECTED (FIRST  0097 00082000
*                                      POSITION KEYWORD)     @G33CPHE*/ 00083000
         OI    PKEY1,B'10000000'                                   0097 00084000
*   DT=0;                           /* CLEAR DUMP TYPE INDICATOR   0098 00085000
*                                                            @G33CPHE*/ 00086000
         SLR   DT,DT                                               0098 00087000
*   MSGID='00'X;                    /* CLEAR MESSAGE INDEX   @G33CPHE*/ 00088000
         MVI   MSGID,X'00'                                         0099 00089000
*   R14SVC34=R14;                   /* RET ADDRESS OF SVC 34 ROUTER  */ 00090000
         ST    R14,R14SVC34                                        0100 00091000
*   IF START=0                      /* IS THERE ANY OPERAND ?        */ 00092000
*     THEN                          /* NO                            */ 00093000
         LTR   START,START                                         0101 00094000
         BNZ   @RF00101                                            0101 00095000
*     MSGID=IEE305;                 /* MSG INDEX FOR KEYWORD MISSING    00096000
*                                                            @G33CPHE*/ 00097000
         MVI   MSGID,X'05'                                         0102 00098000
*   ELSE                                                           0103 00099000
*     DO;                                                          0103 00100000
         B     @RC00101                                            0103 00101000
@RF00101 DS    0H                                                  0104 00102000
*       OPLNTH=10;                  /* INITIALIZE TO ONE MORE THAN 0104 00103000
*                                      MAXIMUM KEYWORD LENGTH        */ 00104000
         LA    OPLNTH,10                                           0104 00105000
*       STOPPER=0;                  /* INITIALIZE REG USED TO HOLD 0105 00106000
*                                      TRANSLATE STOPPER VALUE       */ 00107000
         SLR   STOPPER,STOPPER                                     0105 00108000
*                                                                  0106 00109000
*       /*************************************************************/ 00110000
*       /*                                                           */ 00111000
*       /* THIS IS THE CONTROLLER SEGMENT OF THE CHNGDUMP COMMAND    */ 00112000
*       /* PROCESSOR. IT CALCULATES THE LENGTH OF THE KEYWORD OR     */ 00113000
*       /* PARAMETER ENCOUNTERED, THE LENGTH OF THE OPERAND REMAINING*/ 00114000
*       /* TO BE TRANSLATED. IT MAINTAINS POINTERS TO THE CURRENT    */ 00115000
*       /* KEYWORD, CALLS TRANSLATE AND CALLS INTERNAL PROCEDURES TO */ 00116000
*       /* PROCESS THE DELIMITERS ENCOUNTERED.                       */ 00117000
*       /*                                                           */ 00118000
*       /*************************************************************/ 00119000
*                                                                  0106 00120000
*       DO                          /* CONTROLS TRT OF OPERAND       */ 00121000
*             WHILE(MSGID='00'X);   /* TRT UNTIL END OF OPERAND OR 0106 00122000
*                                      ERROR                 @G33CPHE*/ 00123000
         B     @DE00106                                            0106 00124000
@DL00106 DS    0H                                                  0107 00125000
*         ERRTXTP=START;            /* POINT TO THE TEXT TO BE     0107 00126000
*                                      SCANNED NEXT AS THE TEXT TO BE   00127000
*                                      PUT INTO AN ERROR MSG @G33CPHE*/ 00128000
         ST    START,ERRTXTP                                       0107 00129000
*         GEN REFS(OPLNTH,TRTIN,TRANTBL,OPERAND) SETS(ENP,STOPPER);     00130000
        EX   OPLNTH,TRTIN          EXECUTE TRT INSTRUCTION FOR FULL     00131000
*                                  LENGTH OF OPERAND NOT YET            00132000
*                                  TRANSLATED                           00133000
*         BC(8,NOSTOPER);           /* UNINTERUPTED TRT SCAN         */ 00134000
         BC    8,NOSTOPER                                          0109 00135000
*/* AFTER THE TRT ENP=ADDR OF CHAR THAT CAUSED TRT TO STOP AND     0110 00136000
*   STOPPER=FUNCTION CODE FROM TRANTBL THAT CAUSED THE STOP   @G33CPHE  00137000
*                                                                    */ 00138000
*                                                                  0110 00139000
*         LEN=ENP-START;            /* DETERMINE LENGTH OF KEYWORD OR   00140000
*                                      PARAMETER JUST ISOLATED       */ 00141000
         LR    LEN,ENP                                             0110 00142000
         SLR   LEN,START                                           0110 00143000
*         CALL BRTABLE(STOPPER);    /* GO TO PROPER DELIMITER      0111 00144000
*                                      SUBROUTINE TO PROCESS THIS  0111 00145000
*                                      PART OF OPERAND               */ 00146000
         LR    @06,STOPPER                                         0111 00147000
         SLA   @06,2                                               0111 00148000
         L     @15,BRTABLE-4(@06)                                  0111 00149000
         BALR  @14,@15                                             0111 00150000
*         STOPSAVE=STOPPER;         /* SAVE TO DETERMINE PREVIOUS  0112 00151000
*                                      STOPPER CHARACTER             */ 00152000
         STC   STOPPER,STOPSAVE                                    0112 00153000
*         START=ENP+1;                                             0113 00154000
         LA    START,1                                             0113 00155000
         ALR   START,ENP                                           0113 00156000
*       END;                                                       0114 00157000
@DE00106 CLI   MSGID,X'00'                                         0114 00158000
         BE    @DL00106                                            0114 00159000
*       GO TO CONTINUE;             /* BRANCH AROUND MAX LENGTH PARM    00160000
*                                      ERROR HANDLING                */ 00161000
         B     CONTINUE                                            0115 00162000
*NOSTOPER:                                                         0116 00163000
*       MSGID=IEE308;               /* MSG INDEX FOR TERM LENGTH   0116 00164000
*                                      ERROR -- ISOLATED PARM EXCEEDS   00165000
*                                      MAXIMUM ALLOWABLE     @G33CPHE*/ 00166000
NOSTOPER MVI   MSGID,X'08'                                         0116 00167000
*CONTINUE:                          /* RESUME NORMAL PROCESSING      */ 00168000
*     END;                                                         0117 00169000
CONTINUE DS    0H                                                  0118 00170000
*   RESPECIFY                                                      0118 00171000
*    (OPSETPTR,                                                    0118 00172000
*     STOPPER,                                                     0118 00173000
*     ENP) UNRESTRICTED;                                           0118 00174000
*                                                                  0118 00175000
@RC00101 DS    0H                                                  0119 00176000
*   /*****************************************************************/ 00177000
*   /*                                                               */ 00178000
*   /* UPDATE THE RTCT WITH THE NEW EFFECTIVE DUMP OPTIONS IF THE    */ 00179000
*   /* MSGID IS NOT AN ERROR ONE.                            @G33CPHE*/ 00180000
*   /*                                                               */ 00181000
*   /*****************************************************************/ 00182000
*                                                                  0119 00183000
*   IF MSGID=IEE712                 /* IS THIS THE PROCESSING      0119 00184000
*                                      COMPLETE MSG?         @G33CPHE*/ 00185000
*     THEN                          /* YES SO GO AHEAD WITH THE RTCT    00186000
*                                      UPDATE                @G33CPHE*/ 00187000
         CLI   MSGID,X'03'                                         0119 00188000
         BNE   @RF00119                                            0119 00189000
*     IF DT=ALL                     /* CHECK TO SEE IF ALL THE DUMP     00190000
*                                      TYPES MUST BE UPDATED OR JUST    00191000
*                                      ONE                   @G33CPHE*/ 00192000
*       THEN                                                       0120 00193000
         LTR   DT,DT                                               0120 00194000
         BNZ   @RF00120                                            0120 00195000
*       DO DT=1 TO DIM(RTCTOPTS);   /* IF ALL THE DUMP TYPES ARE   0121 00196000
*                                      INVOLVED LOOP DOING EACH ONE     00197000
*                                      AT A TIME             @G33CPHE*/ 00198000
         LA    DT,1                                                0121 00199000
@DL00121 DS    0H                                                  0122 00200000
*         CALL UPDTERTN;            /* THIS SUBROUTINE WILL UPDATE 0122 00201000
*                                      THE RTCT EFFECTIVE DUMP OPTION   00202000
*                                      CORRESPONDING TO THE DT     0122 00203000
*                                                            @G33CPHE*/ 00204000
         BAL   @14,UPDTERTN                                        0122 00205000
*       END;                        /*                       @G33CPHE*/ 00206000
         AH    DT,@CH00065                                         0123 00207000
         CH    DT,@CH00057                                         0123 00208000
         BNH   @DL00121                                            0123 00209000
*     ELSE                                                         0124 00210000
*       CALL UPDTERTN;              /* UPDATE THE EFFECTIVE RTCT   0124 00211000
*                                      OPTIONS FOR THE DUMP TYPE   0124 00212000
*                                      SPECIFIED (DT)        @G33CPHE*/ 00213000
         B     @RC00120                                            0124 00214000
@RF00120 BAL   @14,UPDTERTN                                        0124 00215000
*   XSAPTR=XSASAVE;                 /* RESTORE PTR TO XSA            */ 00216000
@RC00120 DS    0H                                                  0125 00217000
@RF00119 L     XSAPTR,XSASAVE                                      0125 00218000
*   XAE=MSGID;                      /* PUT MSG INDEX INTO XSA      0126 00219000
*                                                            @G33CPHE*/ 00220000
         MVC   XAE(1,XSAPTR),MSGID                                 0126 00221000
*   IF MSGID^=IEE712&MSGID^=IEE305  /* IS THE MSG ONE THAT NEEDS THE    00222000
*                                      ERROR TEXT INSERTED   @G33CPHE*/ 00223000
*       &START^=0                   /* AND IS THERE TEXT TO BE     0127 00224000
*                                      INSERTED INTO THE MSG TEXT  0127 00225000
*                                                            @G33CPHE*/ 00226000
*     THEN                          /* YES                   @G33CPHE*/ 00227000
         CLI   MSGID,X'03'                                         0127 00228000
         BE    @RF00127                                            0127 00229000
         CLI   MSGID,X'05'                                         0127 00230000
         BE    @RF00127                                            0127 00231000
         LTR   START,START                                         0127 00232000
         BZ    @RF00127                                            0127 00233000
*     DO;                           /*                       @G33CPHE*/ 00234000
*       IF LEN<LENGTH(XAV)          /* IS THE LENGTH OF THE ERROR  0129 00235000
*                                      PARM TEXT LESS THAN THE     0129 00236000
*                                      VARIABLE SECTION OF THE MSG 0129 00237000
*                                                            @G33CPHE*/ 00238000
*         THEN                      /* YES IT IS             @G33CPHE*/ 00239000
         CH    LEN,@CH00063                                        0129 00240000
         BNL   @RF00129                                            0129 00241000
*         LEN=LEN+1;                /* SO INCREASE IT BY ONE TO    0130 00242000
*                                      INCLUDE THE DELIMITER THAT  0130 00243000
*                                      FOLLOWS IT            @G33CPHE*/ 00244000
         AH    LEN,@CH00065                                        0130 00245000
*       ELSE                        /* OTHERWISE IF IT IS TOO LONG 0131 00246000
*                                                            @G33CPHE*/ 00247000
*         LEN=LENGTH(XAV);          /* TRUNCATE IT           @G33CPHE*/ 00248000
         B     @RC00129                                            0131 00249000
@RF00129 LA    LEN,8                                               0131 00250000
*       XAV='        ';             /* INIT THE VARIABLE AREA TO   0132 00251000
*                                      BLANKS                @G33CPHE*/ 00252000
@RC00129 MVC   XAV(8,XSAPTR),@CC02081                              0132 00253000
*       XAV(1:LEN)=ERRTXT(1:LEN);   /* AND MOVE IN THE OFFENDING PARM   00254000
*                                                            @G33CPHE*/ 00255000
         LR    @15,LEN                                             0133 00256000
         BCTR  @15,0                                               0133 00257000
         L     @05,ERRTXTP                                         0133 00258000
         EX    @15,@SM02259                                        0133 00259000
*     END;                          /* OR ERROR TEXT INSERTION     0134 00260000
*                                                            @G33CPHE*/ 00261000
*   CALL IEE2103D;                  /* ISSUE MESSAGE TO INDICATE   0135 00262000
*                                      SUCCESS OR FAILURE OF COMMAND */ 00263000
@RF00127 L     @15,@CV02052                                        0135 00264000
         BALR  @14,@15                                             0135 00265000
*   R14=R14SVC34;                   /* RESTORE RETURN ADDRESS OF   0136 00266000
*                                      CHNGDUMP CALLER       @G33CPHE*/ 00267000
         L     R14,R14SVC34                                        0136 00268000
*   RETURN;                                                        0137 00269000
@EL00001 L     @13,4(,@13)                                         0137 00270000
@EF00001 L     @00,@SIZDATD                                        0137 00271000
         LR    @01,@08                                             0137 00272000
         FREEMAIN R,LV=(0),A=(1)                                        00273000
@ER00001 BR    @14                                                 0137 00274000
*   RESPECIFY                                                      0138 00275000
*    (OPSETPTR,                                                    0138 00276000
*     STOPPER,                                                     0138 00277000
*     ENP) RESTRICTED;                                             0138 00278000
*CHDLPARN:                                                         0139 00279000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0139 00280000
*                                                                  0139 00281000
CHDLPARN DS    0H                                                  0140 00282000
*   /*****************************************************************/ 00283000
*   /*                                                               */ 00284000
*   /* THIS PROC HANDLES PROCESSING OF THE LEFT PARANTHESIS DELIMITER*/ 00285000
*   /*                                                               */ 00286000
*   /*****************************************************************/ 00287000
*                                                                  0140 00288000
*   LPARAN='1'B;                    /* INDICATE LEFT PARAN         0140 00289000
*                                      ENCOUNTERED                   */ 00290000
*                                                                  0140 00291000
         OI    LPARAN,B'00001000'                                  0140 00292000
*   /*****************************************************************/ 00293000
*   /*                                                               */ 00294000
*   /* RESET TRANSLATE TABLE FOR NEXT SCAN                           */ 00295000
*   /*                                                               */ 00296000
*   /*****************************************************************/ 00297000
*                                                                  0141 00298000
*   TRANTBL(')'+1)=RPARNCDE;        /* STOP ON RIGHT PARAN   @G33CPHE*/ 00299000
         MVI   TRANTBL+93,X'05'                                    0141 00300000
*   TRANTBL(','+1)=COMMACDE;        /* STOP ON COMMA         @G33CPHE*/ 00301000
         MVI   TRANTBL+107,X'02'                                   0142 00302000
*   TRANTBL('='+1)=UNEXPCDE;        /* EQUAL SIGN UNEXPECTED @G33CPHE*/ 00303000
         MVI   TRANTBL+126,X'06'                                   0143 00304000
*   TRANTBL(' '+1)=UNEXPCDE;        /* BLANK IS UNEXPECTED   @G33CPHE*/ 00305000
         MVI   TRANTBL+64,X'06'                                    0144 00306000
*   TRANTBL('('+1)=UNEXPCDE;        /* LEFT PARAN IS UNEXPECTED    0145 00307000
*                                                            @G33CPHE*/ 00308000
         MVI   TRANTBL+77,X'06'                                    0145 00309000
*   RETURN;                                                        0146 00310000
@EL00002 DS    0H                                                  0146 00311000
@EF00002 DS    0H                                                  0146 00312000
@ER00002 BR    @14                                                 0146 00313000
*   END CHDLPARN;                                                  0147 00314000
*CHDRPARN:                                                         0148 00315000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0148 00316000
*                                                                  0148 00317000
CHDRPARN DS    0H                                                  0149 00318000
*   /*****************************************************************/ 00319000
*   /*                                                               */ 00320000
*   /* HANDLES PROCESSING OF RIGHT PARANTHESIS. VALIDITY CHECKS FOR  */ 00321000
*   /* PREVIOUS LEFT PARANTHESIS AND OPTIONS EXPECTED. CALLS OPTION  */ 00322000
*   /* HANDLER TO PROCESS THE OPTION                                 */ 00323000
*   /*                                                               */ 00324000
*   /*****************************************************************/ 00325000
*                                                                  0149 00326000
*   R14SAVE=R14;                    /* SAVE RETURN ADDRESS TO      0149 00327000
*                                      MAINLINE PROCESSING BEFORE  0149 00328000
*                                      CALLING OPTION HANDLING     0149 00329000
*                                      SUBROUTINE                    */ 00330000
         ST    R14,R14SAVE                                         0149 00331000
*   CALL OPSET;                     /* GO TO OPTION HANDLER          */ 00332000
         LR    @15,OPSETPTR                                        0150 00333000
         BALR  @14,@15                                             0150 00334000
*   R14=R14SAVE;                    /* RESTORE RETURN ADDRESS TO   0151 00335000
*                                      MAINLINE PROCESSING           */ 00336000
         L     R14,R14SAVE                                         0151 00337000
*   LPARAN='0'B;                    /* REINITIALIZE LEFT PARANTHESIS    00338000
*                                      INDICATOR SINCE WE HAVE     0152 00339000
*                                      ENCOUNTERED THE MATCHING RIGHT   00340000
*                                      PARAN.                        */ 00341000
*   OPTIONX='0'B;                   /* NO MORE OPTIONS EXPECTED      */ 00342000
*                                                                  0153 00343000
         NI    LPARAN,B'11110011'                                  0153 00344000
*   /*****************************************************************/ 00345000
*   /*                                                               */ 00346000
*   /* RIGHT PARAN PROCESSOR - RESET TRANSLATE TABLE FOR NEXT SCAN   */ 00347000
*   /*                                                               */ 00348000
*   /*****************************************************************/ 00349000
*                                                                  0154 00350000
*   TRANTBL(','+1)=COMMACDE;        /* STOP ON COMMA         @G33CPHE*/ 00351000
         MVI   TRANTBL+107,X'02'                                   0154 00352000
*   TRANTBL(' '+1)=EOCMDCDE;        /* STOP ON BLANK         @G33CPHE*/ 00353000
         MVI   TRANTBL+64,X'03'                                    0155 00354000
*   TRANTBL('='+1)=UNEXPCDE;        /* EQUAL SIGN IS UNEXPECTED    0156 00355000
*                                                            @G33CPHE*/ 00356000
         MVI   TRANTBL+126,X'06'                                   0156 00357000
*   TRANTBL('('+1)=UNEXPCDE;        /* LEFT PARAN IS UNEXPECTED    0157 00358000
*                                                            @G33CPHE*/ 00359000
         MVI   TRANTBL+77,X'06'                                    0157 00360000
*   TRANTBL(')'+1)=UNEXPCDE;        /* RIGHT PARAN IS UNEXPECTED   0158 00361000
*                                                            @G33CPHE*/ 00362000
         MVI   TRANTBL+93,X'06'                                    0158 00363000
*   RETURN;                                                        0159 00364000
@EL00003 DS    0H                                                  0159 00365000
@EF00003 DS    0H                                                  0159 00366000
@ER00003 BR    @14                                                 0159 00367000
*   END CHDRPARN;                                                  0160 00368000
*CHDEQUAL:                                                         0161 00369000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0161 00370000
*                                                                  0161 00371000
CHDEQUAL DS    0H                                                  0162 00372000
*   /*****************************************************************/ 00373000
*   /*                                                               */ 00374000
*   /* THIS PROCESSES THE KEYWORDS WHICH ARE FOLLOWED BY THE EQUAL   */ 00375000
*   /* SIGN DELIMITER. POSSIBLE KEYWORDS IN THIS POSITION ARE        */ 00376000
*   /* PDATA,SDATA,Q OR SDUMP. THE OPTIONS EXPECTED FLAG IS TURNED ON*/ 00377000
*   /* AND THE KEYWORDS ARE SYNTAX CHECKED. IF AN ERROR IS DETECTED, */ 00378000
*   /* IT WILL BE FLAGGED AS UNIDENTIFIABLE KEYWORD.                 */ 00379000
*   /*                                                               */ 00380000
*   /*****************************************************************/ 00381000
*                                                                  0162 00382000
*   IF OPTIONX='0'B THEN                                           0162 00383000
         TM    OPTIONX,B'00000100'                                 0162 00384000
         BNZ   @RF00162                                            0162 00385000
*     IF KEY3='1'B                  /* EXPECTING 3RD POSITION      0163 00386000
*                                      KEYWORD?                      */ 00387000
*       THEN                                                       0163 00388000
         TM    KEY3,B'00100000'                                    0163 00389000
         BNO   @RF00163                                            0163 00390000
*       DO;                         /* YES- MUST BE PDATA,SDATA OR Q    00391000
*                                                             @G33CPH*/ 00392000
*         KEY31=OFF;                /* AN EXCLUSIVE 3RD POS KEYWORD     00393000
*                                      IS NO LONGER POSSIBLE @G33CPHE*/ 00394000
         NI    KEY31,B'11101111'                                   0165 00395000
*         IF DT=SADINDEX|DT=SUDINDEX/* SYSABEND OR SYSUDUMP SPECIFIED   00396000
*                                                            @G33CPHE*/ 00397000
*             &LEN=5                /*                       @G33CPHE*/ 00398000
*           THEN                    /* YES                           */ 00399000
         CH    DT,@CH00065                                         0166 00400000
         BE    @RT00166                                            0166 00401000
         CH    DT,@CH00077                                         0166 00402000
         BNE   @RF00166                                            0166 00403000
         CH    LEN,@CH00770                                        0166 00404000
         BNE   @RF00166                                            0166 00405000
@RT00166 DS    0H                                                  0167 00406000
*           DO;                     /*                       @G33CPHE*/ 00407000
*             OPTIONX=ON;           /*                       @G33CPHE*/ 00408000
         OI    OPTIONX,B'00000100'                                 0168 00409000
*             IF OPERAND(1:5)='PDATA'/*                      @G33CPHE*/ 00410000
*               THEN                                               0169 00411000
         CLC   OPERAND(5,START),@CC02091                           0169 00412000
         BNE   @RF00169                                            0169 00413000
*               DO;                                                0170 00414000
*                 PDATAKEY='1'B;                                   0171 00415000
         OI    PDATAKEY,B'00010000'                                0171 00416000
*                 OPSETPTR=ADDR(CHDPDATA);/* SET PTR TO OPTION     0172 00417000
*                                      HANDLER FOR PDATA OPTIONS     */ 00418000
         LA    OPSETPTR,CHDPDATA                                   0172 00419000
*               END;                                               0173 00420000
*             ELSE                                                 0174 00421000
*               IF OPERAND(1:5)='SDATA'/* SDATA KEYWORD ?    @G33CPHE*/ 00422000
*                 THEN              /* YES                           */ 00423000
         B     @RC00169                                            0174 00424000
@RF00169 CLC   OPERAND(5,START),@CC02093                           0174 00425000
         BNE   @RF00174                                            0174 00426000
*                 DO;                                              0175 00427000
*                   SDATAKEY='1'B;  /* INDICATE SDATA SPECIFIED      */ 00428000
         OI    SDATAKEY,B'00100000'                                0176 00429000
*                   OPSETPTR=ADDR(CHDSDATA);/* SET PTR TO OPTION   0177 00430000
*                                      HANDLER FOR SDATA OPTIONS     */ 00431000
         LA    OPSETPTR,CHDSDATA                                   0177 00432000
*                 END;                                             0178 00433000
*               ELSE                                               0179 00434000
*                 MSGID=IEE309;     /* KEYWORD NOT VALID FOR 3ND   0179 00435000
*                                      POSITIONAL FOLLOWED BY AN = 0179 00436000
*                                                            @G33CPHE*/ 00437000
         B     @RC00174                                            0179 00438000
@RF00174 MVI   MSGID,X'09'                                         0179 00439000
*           END;                    /*                       @G33CPHE*/ 00440000
*         ELSE                                                     0181 00441000
*           IF DT=SVDINDEX&LEN=1&OPERAND(1)='Q'/* Q= KEYWORD @G33CPHE*/ 00442000
*             THEN                                                 0181 00443000
         B     @RC00166                                            0181 00444000
@RF00166 CH    DT,@CH00057                                         0181 00445000
         BNE   @RF00181                                            0181 00446000
         CH    LEN,@CH00065                                        0181 00447000
         BNE   @RF00181                                            0181 00448000
         CLI   OPERAND(START),C'Q'                                 0181 00449000
         BNE   @RF00181                                            0181 00450000
*             YESNO='1'B;           /* EXPECTING 'YES' OR'NO'        */ 00451000
         OI    YESNO,B'00001000'                                   0182 00452000
*           ELSE                                                   0183 00453000
*             MSGID=IEE309;         /* KEYWORD NOT VALID FOR 3ND   0183 00454000
*                                      POSITIONAL FOLLOWED BY AN = 0183 00455000
*                                                            @G33CPHE*/ 00456000
         B     @RC00181                                            0183 00457000
@RF00181 MVI   MSGID,X'09'                                         0183 00458000
*       END;                        /*                       @G33CPHE*/ 00459000
*     ELSE                                                         0185 00460000
*       IF PKEY2='1'B&CDRESET=OFF   /* EXPECTING 2ND POS. PARM     0185 00461000
*                                                            @G33CPHE*/ 00462000
*         THEN                      /* MUST BE SDUMP OR SYSMDUMP   0185 00463000
*                                                            @G33CPHE*/ 00464000
         B     @RC00163                                            0185 00465000
@RF00163 TM    PKEY2,B'01000000'                                   0185 00466000
         BNO   @RF00185                                            0185 00467000
         TM    CDRESET,B'01000000'                                 0185 00468000
         BNZ   @RF00185                                            0185 00469000
*         DO;                       /*                       @G33CPHE*/ 00470000
*           PKEY2='0'B;             /* NO LONGER EXPECTING PKEY2     */ 00471000
*           KEY3='1'B;              /* INDICATE -NOW EXPECTING     0188 00472000
*                                      3RD-4TH POSITION PARAMETERS   */ 00473000
*           OPTIONX='1'B;           /* OPTIONS NOW EXPECTED          */ 00474000
         OI    KEY3,B'00100100'                                    0189 00475000
         NI    PKEY2,B'10111111'                                   0189 00476000
*           IF LEN=5&OPERAND(1:5)='SDUMP' THEN                     0190 00477000
         CH    LEN,@CH00770                                        0190 00478000
         BNE   @RF00190                                            0190 00479000
         CLC   OPERAND(5,START),@CC02096                           0190 00480000
         BNE   @RF00190                                            0190 00481000
*             DO;                                                  0191 00482000
*               OPSETPTR=ADDR(CHDSDUMP);/* SET PTR TO OPTION HANDLER    00483000
*                                      FOR SDUMP OPTIONS             */ 00484000
         LA    OPSETPTR,CHDSDUMP                                   0192 00485000
*               DT=SVDINDEX;        /* SET DUMP TYPE         @G33CPHE*/ 00486000
         LA    DT,4                                                0193 00487000
*             END;                                                 0194 00488000
*           ELSE                                                   0195 00489000
*             IF LEN=8&OPERAND(1:8)='SYSMDUMP'/*             @G33CPHE*/ 00490000
*               THEN                /*                       @G33CPHE*/ 00491000
         B     @RC00190                                            0195 00492000
@RF00190 CH    LEN,@CH00063                                        0195 00493000
         BNE   @RF00195                                            0195 00494000
         CLC   OPERAND(8,START),@CC02098                           0195 00495000
         BNE   @RF00195                                            0195 00496000
*               DO;                 /*                       @G33CPHE*/ 00497000
*                 DT=SMDINDEX;      /* SET DUMP TYPE         @G33CPHE*/ 00498000
         LA    DT,3                                                0197 00499000
*                 OPSETPTR=ADDR(CHDSYSM);/* POINT TO SYSMDUMP OPTION    00500000
*                                      RTN                   @G33CPHE*/ 00501000
         LA    OPSETPTR,CHDSYSM                                    0198 00502000
*               END;                /*                       @G33CPHE*/ 00503000
*             ELSE                  /*                       @G33CPHE*/ 00504000
*               MSGID=IEE309;       /* KEYWORD NOT VALID FOR 2ND   0200 00505000
*                                      POSITIONAL FOLLOWED BY AN = 0200 00506000
*                                                            @G33CPHE*/ 00507000
         B     @RC00195                                            0200 00508000
@RF00195 MVI   MSGID,X'09'                                         0200 00509000
*         END;                      /*                       @G33CPHE*/ 00510000
*       ELSE                                                       0202 00511000
*         MSGID=IEE307;             /* EQUAL SIGN FOUND IN OPTION  0202 00512000
*                                      LIST                  @G33CPHE*/ 00513000
         B     @RC00185                                            0202 00514000
@RF00185 MVI   MSGID,X'07'                                         0202 00515000
*   ELSE                            /*                       @G33CPHE*/ 00516000
*     MSGID=IEE307;                 /* MSG INDEX FOR INVALID       0203 00517000
*                                      DELIMITER             @G33CPHE*/ 00518000
*                                                                  0203 00519000
         B     @RC00162                                            0203 00520000
@RF00162 MVI   MSGID,X'07'                                         0203 00521000
*   /*****************************************************************/ 00522000
*   /*                                                               */ 00523000
*   /* EQUAL PROCESSOR -- RESET TRANSLATE TABLE FOR NEXT SCAN        */ 00524000
*   /*                                                               */ 00525000
*   /*****************************************************************/ 00526000
*                                                                  0204 00527000
*   TRANTBL(','+1)=COMMACDE;        /* STOP ON COMMA         @G33CPHE*/ 00528000
@RC00162 MVI   TRANTBL+107,X'02'                                   0204 00529000
*   TRANTBL('('+1)=LPARNCDE;        /* STOP ON LEFT PARAN    @G33CPHE*/ 00530000
         MVI   TRANTBL+77,X'04'                                    0205 00531000
*   TRANTBL('='+1)=UNEXPCDE;        /* EQUAL SIGN IS UNEXPECTED    0206 00532000
*                                                            @G33CPHE*/ 00533000
         MVI   TRANTBL+126,X'06'                                   0206 00534000
*   TRANTBL(' '+1)=EOCMDCDE;        /* BLANK IS VALID        @G33CPHE*/ 00535000
         MVI   TRANTBL+64,X'03'                                    0207 00536000
*   TRANTBL(')'+1)=UNEXPCDE;        /* RIGHT PARAN IS UNEXPECTED   0208 00537000
*                                                            @G33CPHE*/ 00538000
         MVI   TRANTBL+93,X'06'                                    0208 00539000
*   RETURN;                                                        0209 00540000
@EL00004 DS    0H                                                  0209 00541000
@EF00004 DS    0H                                                  0209 00542000
@ER00004 BR    @14                                                 0209 00543000
*   END CHDEQUAL;                                                  0210 00544000
*CHDCOMMA:                                                         0211 00545000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0211 00546000
*                                                                  0211 00547000
CHDCOMMA DS    0H                                                  0212 00548000
*/*    *****  THE COMMA SUBROUTINE  ****                             */ 00549000
*/*    IF OPTIONS ARE EXPECTED, THIS ROUTINE CALLS THE OPTION        */ 00550000
*/*    HANDLER FOR SDATA OR PDATA OR SDUMP.                          */ 00551000
*/*    IF THE SECOND POSITION KEYWORD IS EXPECTED, THEN A CHECK IS   */ 00552000
*/*    MADE FOR SYSABEND, SYSUDUMP, OR SDUMP, AND THE PROPER         */ 00553000
*/*    INDICATORS ARE SET ACCORDINGLY.                               */ 00554000
*/*                                                                  */ 00555000
*/*    IF THE FIRST POSITION KEYWORD IS EXPECTED, THEN A CHECK IS    */ 00556000
*/*    MADE FOR SET OR DEL.                                          */ 00557000
*/*                                                                  */ 00558000
*/*    FINALLY IF NONE OF THE ABOVE CONDITIONS ARE FOUND, A CHECK IS */ 00559000
*/*    MADE TO SEE IF THIS IS A COMMA FOLLOWING THE RIGHT PARAN. IF  */ 00560000
*/*    THIS IS THE CASE,WE DO NOT HAVE A KEYWORD OR SUBPARAMETER     */ 00561000
*/*    ISOLATED, GO TO CONTINUE THE SCAN.                            */ 00562000
*/*                                                                  */ 00563000
*/*    A MESSAGE INDEX FOR AN APPROPRIATE ERROR MESSAGE IS SET       */ 00564000
*/*    WHEN AN UNEXPECTED CONDITION IS FOUND.                        */ 00565000
*/*                                                                  */ 00566000
*                                                                  0212 00567000
*   IF KEY3=ON                      /* CHECK TO SEE IF 3RD POSITION     00568000
*                                      KEYWORDS ARE IN PROCESS       */ 00569000
*     THEN                                                         0212 00570000
         TM    KEY3,B'00100000'                                    0212 00571000
         BNO   @RF00212                                            0212 00572000
*     DO;                           /*                       @G33CPHE*/ 00573000
*       KEY31=OFF;                  /* YES. THIS MEANS THAT MORE THAN   00574000
*                                      ONE 3RD POS IS BEING        0214 00575000
*                                      SPECIFIED, HENCE THE EXCUSIVE    00576000
*                                      ONES CAN NO LONGER POSSIBLY BE   00577000
*                                      VALID                 @G33CPHE*/ 00578000
         NI    KEY31,B'11101111'                                   0214 00579000
*       IF OPTIONX='1'B             /* EXPECTING OPTIONS ??          */ 00580000
*         THEN                                                     0215 00581000
         TM    OPTIONX,B'00000100'                                 0215 00582000
         BNO   @RF00215                                            0215 00583000
*         DO;                                                      0216 00584000
*           R14SAVE=R14;            /* SAVE RETURN ADDRESS           */ 00585000
         ST    R14,R14SAVE                                         0217 00586000
*           CALL OPSET;             /* GO TO OPTION HANDLER          */ 00587000
         LR    @15,OPSETPTR                                        0218 00588000
         BALR  @14,@15                                             0218 00589000
*           R14=R14SAVE;            /* RESTORE R14                   */ 00590000
         L     R14,R14SAVE                                         0219 00591000
*           IF LPARAN^='1'B         /* IF A LEFT PARAN HAS NOT BEEN     00592000
*                                      ENCOUNTERED, THEN ALLOW ONLY     00593000
*                                      ONE                           */ 00594000
*             THEN                                                 0220 00595000
         TM    LPARAN,B'00001000'                                  0220 00596000
         BO    @RF00220                                            0220 00597000
*             OPTIONX='0'B;         /* NO MORE DUMP OPTIONS ARE    0221 00598000
*                                      EXPECTED FOR THIS KEYWORD   0221 00599000
*                                      BECAUSE PARENTHESIS WERE NOT     00600000
*                                      USED                          */ 00601000
         NI    OPTIONX,B'11111011'                                 0221 00602000
*         END;                                                     0222 00603000
*       ELSE                        /*                       @G33CPHE*/ 00604000
*         IF STOPSAVE=RPARNCDE&LEN=0/* IF THIS COMMA FOLLOWS A RIGHT    00605000
*                                      PAREN WITH NO KEYWORD @G33CPHE*/ 00606000
*           THEN                                                   0223 00607000
         B     @RC00215                                            0223 00608000
@RF00215 CLI   STOPSAVE,X'05'                                      0223 00609000
         BNE   @RF00223                                            0223 00610000
         LTR   LEN,LEN                                             0223 00611000
         BZ    @RT00223                                            0223 00612000
*           ;                       /* THERE IS NOTHING TO PROCESS 0224 00613000
*                                      YET                   @G33CPHE*/ 00614000
*         ELSE                      /* CHECK FOR VALID 3RD POSITION     00615000
*                                      NONEXCLUSIVE KEYWORDS @G33CPHE*/ 00616000
*           IF YESNO=ON             /* IS THE YES OR NO OPTION FOR Q    00617000
*                                      EXPECTED?             @G33CPHE*/ 00618000
*             THEN                  /* YES IT IS             @G33CPHE*/ 00619000
@RF00223 TM    YESNO,B'00001000'                                   0225 00620000
         BNO   @RF00225                                            0225 00621000
*             IF LEN=2&OPERAND(1:2)='NO'/* IS IT NO          @G33CPHE*/ 00622000
*               THEN                                               0226 00623000
         CH    LEN,@CH00077                                        0226 00624000
         BNE   @RF00226                                            0226 00625000
         CLC   OPERAND(2,START),@CC02103                           0226 00626000
         BNE   @RF00226                                            0226 00627000
*               IF ADDR(CHNGWORK)->RTCTSDPG=OFF/* CHECK FOR A      0227 00628000
*                                      CONFLICTING VALUE ALREADY   0227 00629000
*                                      SPECIFIED             @G33CPHE*/ 00630000
*                 THEN              /* Q=NO SPECIFIED        @G33CPHE*/ 00631000
         LA    @15,CHNGWORK                                        0227 00632000
         TM    RTCTSDPG(@15),B'10000000'                           0227 00633000
         BNZ   @RF00227                                            0227 00634000
*                 DO;                                              0228 00635000
*                   YESNO=OFF;      /* SHOW THAT THE YESNO OPTION IS    00636000
*                                      NO LONGER BEING EXPECTED      */ 00637000
         NI    YESNO,B'11110111'                                   0229 00638000
*                   ADDR(CHNGWORK)->RTCTSDPH=ON;/* TURN ON THE OPTION   00639000
*                                      IN THE WORK AREA      @G33CPHE*/ 00640000
         OI    RTCTSDPH(@15),B'01000000'                           0230 00641000
*                 END;                                             0231 00642000
*               ELSE                                               0232 00643000
*                 MSGID=IEE312;     /* INDICATE THE ERROR    @G33CPHE*/ 00644000
         B     @RC00227                                            0232 00645000
@RF00227 MVI   MSGID,X'0C'                                         0232 00646000
*             ELSE                                                 0233 00647000
*               IF LEN=3&OPERAND(1:3)='YES'/* IS IT Q=YES    @G33CPHE*/ 00648000
*                 THEN                                             0233 00649000
         B     @RC00226                                            0233 00650000
@RF00226 CH    LEN,@CH00290                                        0233 00651000
         BNE   @RF00233                                            0233 00652000
         CLC   OPERAND(3,START),@CC02104                           0233 00653000
         BNE   @RF00233                                            0233 00654000
*                 IF ADDR(CHNGWORK)->RTCTSDPH=OFF/* CHECK FOR A    0234 00655000
*                                      CONFLICTING VALUE ALREADY   0234 00656000
*                                      SPECIFIED             @G33CPHE*/ 00657000
*                   THEN            /* YES                   @G33CPHE*/ 00658000
         LA    @15,CHNGWORK                                        0234 00659000
         TM    RTCTSDPH(@15),B'01000000'                           0234 00660000
         BNZ   @RF00234                                            0234 00661000
*                   DO;                                            0235 00662000
*                     YESNO=OFF;    /* SHOW THAT THE YESNO OPTION IS    00663000
*                                      NO LONGER BEING EXPECTED      */ 00664000
         NI    YESNO,B'11110111'                                   0236 00665000
*                     ADDR(CHNGWORK)->RTCTSDPG=ON;/* TURN ON THE   0237 00666000
*                                      OPTION IN THE WORK AREA     0237 00667000
*                                                            @G33CPHE*/ 00668000
         OI    RTCTSDPG(@15),B'10000000'                           0237 00669000
*                   END;                                           0238 00670000
*                 ELSE                                             0239 00671000
*                   MSGID=IEE312;   /* INDICATE THE ERROR    @G33CPHE*/ 00672000
         B     @RC00234                                            0239 00673000
@RF00234 MVI   MSGID,X'0C'                                         0239 00674000
*               ELSE                /* NOT A VALID VALUE     @G33CPHE*/ 00675000
*                 MSGID=IEE708;     /* INDICATE THE ERROR    @G33CPHE*/ 00676000
         B     @RC00233                                            0240 00677000
@RF00233 MVI   MSGID,X'3D'                                         0240 00678000
*           ELSE                    /* YES NO NOT EXPECTED   @G33CPHE*/ 00679000
*             IF LEN=3&OPERAND(1:3)='ADD'/* IS IT ADD?       @G33CPHE*/ 00680000
*               THEN                                               0241 00681000
         B     @RC00225                                            0241 00682000
@RF00225 CH    LEN,@CH00290                                        0241 00683000
         BNE   @RF00241                                            0241 00684000
         CLC   OPERAND(3,START),@CC02105                           0241 00685000
         BNE   @RF00241                                            0241 00686000
*               IF OVERMODE=OFF     /* CHECK FOR A CONFLICTING VALUE    00687000
*                                      ALREADY SPECIFIED     @G33CPHE*/ 00688000
*                 THEN              /* YES                   @G33CPHE*/ 00689000
         TM    OVERMODE,B'00000010'                                0242 00690000
         BNZ   @RF00242                                            0242 00691000
*                 ADDMODE=ON;       /* SET FLAG FOR CHDSAQ SEGMENT 0243 00692000
*                                      WHICH WILL SET BITS IN THE  0243 00693000
*                                      UPDATE AREA           @G33CPHE*/ 00694000
         OI    ADDMODE,B'00000100'                                 0243 00695000
*               ELSE                                               0244 00696000
*                 MSGID=IEE312;     /* INDICATE THE ERROR    @G33CPHE*/ 00697000
         B     @RC00242                                            0244 00698000
@RF00242 MVI   MSGID,X'0C'                                         0244 00699000
*             ELSE                  /* NOT ADD               @G33CPHE*/ 00700000
*               IF LEN=4&OPERAND(1:4)='OVER'/* IS IT OVER?   @G33CPHE*/ 00701000
*                 THEN                                             0245 00702000
         B     @RC00241                                            0245 00703000
@RF00241 CH    LEN,@CH00057                                        0245 00704000
         BNE   @RF00245                                            0245 00705000
         CLC   OPERAND(4,START),@CC02106                           0245 00706000
         BNE   @RF00245                                            0245 00707000
*                 IF ADDMODE=OFF    /* CHECK FOR A CONFLICTING VALUE    00708000
*                                      ALREADY SPECIFIED     @G33CPHE*/ 00709000
*                   THEN            /* YES IT IS             @G33CPHE*/ 00710000
         TM    ADDMODE,B'00000100'                                 0246 00711000
         BNZ   @RF00246                                            0246 00712000
*                   OVERMODE=ON;    /* SET FLAG FOR CHDSAQ SEGMENT 0247 00713000
*                                      WHICH WILL SET BITS IN THE  0247 00714000
*                                      UPDATE AREA           @G33CPHE*/ 00715000
         OI    OVERMODE,B'00000010'                                0247 00716000
*                 ELSE                                             0248 00717000
*                   MSGID=IEE312;   /* INDICATE THE ERROR    @G33CPHE*/ 00718000
         B     @RC00246                                            0248 00719000
@RF00246 MVI   MSGID,X'0C'                                         0248 00720000
*               ELSE                /* THERE ARE NO MORE VALID     0249 00721000
*                                      POSSIBILITIES         @G33CPHE*/ 00722000
*                 MSGID=IEE309;     /* INDICATE THE ERROR    @G33CPHE*/ 00723000
         B     @RC00245                                            0249 00724000
@RF00245 MVI   MSGID,X'09'                                         0249 00725000
*     END;                                                         0250 00726000
*   ELSE                                                           0251 00727000
*     IF PKEY2='1'B                 /* IS THE SECOND POSITION KEYWORD   00728000
*                                      EXPECTED ??                   */ 00729000
*       THEN                        /* THEN THIS MUST BE IT.         */ 00730000
         B     @RC00212                                            0251 00731000
@RF00212 TM    PKEY2,B'01000000'                                   0251 00732000
         BNO   @RF00251                                            0251 00733000
*       DO;                                                        0252 00734000
*         IF CDRESET=ON             /* WAS RESET SPECIFIED?  @G33CPHE*/ 00735000
*           THEN                    /* YES IT WAS            @G33CPHE*/ 00736000
         TM    CDRESET,B'01000000'                                 0253 00737000
         BNO   @RF00253                                            0253 00738000
*           DO;                     /*                       @G33CPHE*/ 00739000
*             MSGID=IEE535;         /* INDICATE THE ERROR. RESET MAY    00740000
*                                      NOT HAVE ANY 3RD POS PARMS  0255 00741000
*                                      HENCE THE 2ND POS PARM MAY NOT   00742000
*                                      BE FOLLOWED BY A COMMA      0255 00743000
*                                                            @G33CPHE*/ 00744000
         MVI   MSGID,X'3E'                                         0255 00745000
*             ERRTXTP=START+LEN;    /* CHANGE THE POINTER TO THE   0256 00746000
*                                      ERROR TEXT TO INCLUDE THE   0256 00747000
*                                      COMMA THAT FOLLOWS THE PORTION   00748000
*                                      JUST SCANNED AND WHATEVER   0256 00749000
*                                      KEYWORD FOLLOWS. THE REASON 0256 00750000
*                                      FOR THIS IS THAT WE KNOW IT IS   00751000
*                                      WRONG BUT WE HAVEN'T CHECKED     00752000
*                                      THE PART JUST SCANNED, SO IT     00753000
*                                      MAY BE RIGHT.         @G33CPHE*/ 00754000
         LR    @15,START                                           0256 00755000
         ALR   @15,LEN                                             0256 00756000
         ST    @15,ERRTXTP                                         0256 00757000
*             LEN=LENGTH(XAV);      /* ALSO CHANGE THE LEN TO THE MAX   00758000
*                                      SINCE WHAT FOLLOWS IS UNKNOWN    00759000
*                                                            @G33CPHE*/ 00760000
         LA    LEN,8                                               0257 00761000
*           END;                                                   0258 00762000
*         ELSE                                                     0259 00763000
*           DO;                     /* RESET NOT SPECIFIED   @G33CPHE*/ 00764000
         B     @RC00253                                            0259 00765000
@RF00253 DS    0H                                                  0260 00766000
*             PKEY2='0'B;           /* NO LONGER EXPECTING THE SECOND   00767000
*                                      POSITION KEYWORD              */ 00768000
*             KEY3='1'B;            /* NOW EXPECTING 3RD POSITION  0261 00769000
*                                      KEYWORDS              @G33CPHE*/ 00770000
*             KEY31='1'B;           /* AN EXCLUSIVE 3RD POS KEYWORD     00771000
*                                      CAN VALIDLY BE SPECIFIED AT 0262 00772000
*                                      THIS TIME             @G33CPHE*/ 00773000
*                                                                  0262 00774000
         OI    KEY3,B'00110000'                                    0262 00775000
         NI    PKEY2,B'10111111'                                   0262 00776000
*             /*******************************************************/ 00777000
*             /*                                                     */ 00778000
*             /* CHECK FOR SYSABEND OR SYSUDUMP                      */ 00779000
*             /*                                                     */ 00780000
*             /*******************************************************/ 00781000
*                                                                  0263 00782000
*             IF LEN=8 THEN         /* CHECK OUT ELIGIBLE 8 CHARACTER   00783000
*                                      KEYWORDS                      */ 00784000
         CH    LEN,@CH00063                                        0263 00785000
         BNE   @RF00263                                            0263 00786000
*               IF OPERAND(1:8)='SYSABEND' THEN                    0264 00787000
         CLC   OPERAND(8,START),@CC02107                           0264 00788000
         BNE   @RF00264                                            0264 00789000
*                 DT=SADINDEX;      /* SET DUMP TYPE INDICATOR     0265 00790000
*                                                            @G33CPHE*/ 00791000
         LA    DT,1                                                0265 00792000
*               ELSE                                               0266 00793000
*                 IF OPERAND(1:8)='SYSUDUMP' THEN                  0266 00794000
         B     @RC00264                                            0266 00795000
@RF00264 CLC   OPERAND(8,START),@CC02108                           0266 00796000
         BNE   @RF00266                                            0266 00797000
*                   DT=SUDINDEX;    /* SET DUMP TYPE INDICATOR     0267 00798000
*                                                            @G33CPHE*/ 00799000
         LA    DT,2                                                0267 00800000
*                 ELSE              /*                       @G33CPHE*/ 00801000
*                   IF OPERAND(1:8)='SYSMDUMP'/*             @G33CPHE*/ 00802000
*                     THEN          /*                       @G33CPHE*/ 00803000
         B     @RC00266                                            0268 00804000
@RF00266 CLC   OPERAND(8,START),@CC02098                           0268 00805000
         BNE   @RF00268                                            0268 00806000
*                     DT=SMDINDEX;  /* SET DUMP TYPE INDICATOR     0269 00807000
*                                                            @G33CPHE*/ 00808000
         LA    DT,3                                                0269 00809000
*                   ELSE            /* ERROR - KEYWORD IS NEITHER  0270 00810000
*                                      SYSABEND NOR SYSUDUMP         */ 00811000
*                     MSGID=IEE309; /*                       @G33CPHE*/ 00812000
         B     @RC00268                                            0270 00813000
@RF00268 MVI   MSGID,X'09'                                         0270 00814000
*             ELSE                  /* LENGTH IS NOT 8               */ 00815000
*                                                                  0271 00816000
*               /*****************************************************/ 00817000
*               /*                                                   */ 00818000
*               /* CHECK FOR SDUMP                                   */ 00819000
*               /*                                                   */ 00820000
*               /*****************************************************/ 00821000
*                                                                  0271 00822000
*               IF LEN=5&OPERAND(1:5)='SDUMP' THEN                 0271 00823000
         B     @RC00263                                            0271 00824000
