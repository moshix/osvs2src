*                                                            @G29ANSS*/ 01369000
         MVC   MSG859IM(8,@14),JSSNTINT(JSSNTPTR)                  0412 01370000
*               DO;                 /* WTOP MF(E,PARMAREA)           */ 01371000
*                 RFY                                              0414 01372000
*                   GPR01P RSTD;    /* RESTRICT REG 1                */ 01373000
*                 GPR01P=ADDR(PARMAREA);/* ADDRESS CONTROL PROGRAM 0415 01374000
