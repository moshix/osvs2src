         LH    @10,LEN859                                          0410 01361000
         BCTR  @10,0                                               0410 01362000
         EX    @10,@SM02236                                        0410 01363000
*               MSG859SS=JSSNTNAM;  /* PUT SUBSYSTEM NAME IN MSG   0411 01364000
*                                                            @G29ANSS*/ 01365000
         MVC   MSG859SS(4,@14),JSSNTNAM(JSSNTPTR)                  0411 01366000
*               MSG859IM=JSSNTINT;  /* PUT SUBSYSTEM INITIALIZATION     01367000
