         WTO MF=(E,WTOPARML)                                            03128000
*         END;                                                     0668 03129000
*       GO TO WTOEXIT;              /* RETURN TO CALLER              */ 03130000
         B     WTOEXIT                                             0669 03131000
*     END;                          /* END PROCESSING OF END MLWTO 0670 03132000
*                                      REQUEST                       */ 03133000
*                                                                  0670 03134000
*   /*****************************************************************/ 03135000
*   /*                                                               */ 03136000
*   /* FIND MESSAGE IN MESSAGE TABLE USING MSGID AS SEARCH ARGUMENT  */ 03137000
*   /*                                                               */ 03138000
*   /*****************************************************************/ 03139000
*                                                                  0671 03140000
*   MSGPTR=ADDR(MSGTBLE);           /* SET PTR TO FIRST MSG          */ 03141000
@RF00650 LA    MSGPTR,MSGTBLE                                      0671 03142000
*   DO WHILE((LENELEM^=0)&(IDELEM^=MSGID));                        0672 03143000
         B     @DE00672                                            0672 03144000
@DL00672 DS    0H                                                  0673 03145000
*     MSGPTR=MSGPTR+LENELEM;        /* INCR TO NEXT MESSAGE          */ 03146000
         SLR   @14,@14                                             0673 03147000
         IC    @14,LENELEM(,MSGPTR)                                0673 03148000
         ALR   MSGPTR,@14                                          0673 03149000
*   END;                                                           0674 03150000
*                                                                  0674 03151000
