*   RETURN;                         /* RETURN TO SYSTEM              */ 00316000
@EL00001 L     @13,4(,@13)                                         0167 00317000
@EF00001 L     @00,@SIZDATD                                        0167 00318000
         LR    @01,@10                                             0167 00319000
         FREEMAIN R,LV=(0),A=(1)                                        00320000
@ER00001 BR    @14                                                 0167 00321000
*                                                                  0168 00322000
*   /*****************************************************************/ 00323000
*   /*                                                               */ 00324000
*   /* PARSE ROUTINE                                                 */ 00325000
*   /*                                                               */ 00326000
*   /*****************************************************************/ 00327000
*                                                                  0168 00328000
*PARSE:                                                            0168 00329000
*   PROC;                                                          0168 00330000
*                                                                  0168 00331000
PARSE    STM   @14,@02,@SA00002                                    0168 00332000
         STM   @04,@12,@SA00002+20                                 0168 00333000
*/*                                                                  */ 00334000
*/*   FUNCTION - PARSE COMMAND TEXT IN CSCB AND SET APPROPIATE       */ 00335000
*/*    GLOBAL FUNCTION SWITCHES. IF ANY ERROR ARE DETECTED THEN      */ 00336000
*/*    WRITE APPROPIATE ERROR MESSAGE TO OPERATOR AND EXIT.          */ 00337000
*/*                                                                  */ 00338000
*/*   INPUT-                                                         */ 00339000
*/*    .COMMAND TEXT                                                 */ 00340000
*/*    .REQUESTING CONSOLE ID                                        */ 00341000
*/*                                                                  */ 00342000
*/*   OUTPUT-                                                        */ 00343000
*/*    .RC                                                           */ 00344000
*/*     0  - SUCCESSFUL                                              */ 00345000
*/*     12 - DELIMITER ERROR                                         */ 00346000
*/*     13 - UNIDENTIFIABLE KEYWORD                                  */ 00347000
*/*     14 - PARAMETERS CONFLICT                                     */ 00348000
*/*     15 - IMPROPER CHANNEL NUMBER                                 */ 00349000
*/*    .GLOBAL FUNCTION FLAGS                                        */ 00350000
*/*                                                                  */ 00351000
*/*   COMMAND SYNTAX                                                 */ 00352000
*/*  FORM 1    M                     ALL OPTIONS                     */ 00353000
*/*  FORM 2    M=OPRND               SINGLE OPERAND                  */ 00354000
*/*  FORM 3    M=(OPRND ...,OPRND)   LIST OF OPERANDS                */ 00355000
*/*                                                                  */ 00356000
*/*   SEE POPRND SUBROUTINE FOR LIST OF POSSIBLE OPERANDS            */ 00357000
*/*                                                                  */ 00358000
*/*                                                                  */ 00359000
*                                                                  0169 00360000
*   DCL                                                            0169 00361000
*     PPTR PTR,                     /* PARSE PTR                     */ 00362000
*     CHAR CHAR(1) BASED(PPTR),     /* 1 CHAR IN TEXT                */ 00363000
*     CHARS CHAR(*) BASED(PPTR),    /* MULTIPLE CHARS IN TEXT        */ 00364000
*     LCLFF PTR,                    /* PTR TO LOCAL COPY OF FUNCTION    00365000
*                                      FLAGS                         */ 00366000
*     LCLBUF CHAR(LENGTH(FUNCFLGS));/* LOCAL COPY OF FUNC FLAGS      */ 00367000
*                                                                  0169 00368000
*   /*****************************************************************/ 00369000
*   /*                                                               */ 00370000
*   /* INITIALIZE PARSE VARIABLES                                    */ 00371000
*   /*                                                               */ 00372000
*   /*****************************************************************/ 00373000
*                                                                  0170 00374000
*   RC=0;                           /* SET RETURN CODE               */ 00375000
         SLR   @14,@14                                             0170 00376000
         ST    @14,RC                                              0170 00377000
*   GLBFF=ADDR(GLBBUF);             /* SET MAP PTR TO GLOBAL COPY OF    00378000
*                                      FUNC FLAGS                    */ 00379000
         LA    GLBFF,GLBBUF                                        0171 00380000
*   FUNCFLGS=''B;                   /* ZERO OUT FLAG FIELD           */ 00381000
         XC    FUNCFLGS(17,GLBFF),FUNCFLGS(GLBFF)                  0172 00382000
*   LCLFF=ADDR(LCLBUF);             /* SET MAP PTR TO LOCAL COPY OF     00383000
*                                      FUNC FLAGS                    */ 00384000
*                                                                  0173 00385000
         LA    LCLFF,LCLBUF                                        0173 00386000
*   /*****************************************************************/ 00387000
*   /*                                                               */ 00388000
*   /* SET PARSE PTR PAST 'M'                                        */ 00389000
*   /*                                                               */ 00390000
*   /*****************************************************************/ 00391000
*                                                                  0174 00392000
*   PPTR=ADDR(CHBUF)+1;                                            0174 00393000
*                                                                  0174 00394000
         L     PPTR,CSCBPTR                                        0174 00395000
         LA    PPTR,CHBUF(,PPTR)                                   0174 00396000
         AH    PPTR,@CH00068                                       0174 00397000
*   /*****************************************************************/ 00398000
*   /*                                                               */ 00399000
*   /* CHECK IF DEFAULT CODED                                        */ 00400000
*   /*                                                               */ 00401000
*   /*****************************************************************/ 00402000
*                                                                  0175 00403000
*   IF CHAR=' ' THEN                                               0175 00404000
*                                                                  0175 00405000
         CLI   CHAR(PPTR),C' '                                     0175 00406000
         BNE   @RF00175                                            0175 00407000
*     /***************************************************************/ 00408000
*     /*                                                             */ 00409000
*     /* DEFAULT OPTIONS CODED, SET DEFAULT VALUES                   */ 00410000
*     /*                                                             */ 00411000
*     /***************************************************************/ 00412000
*                                                                  0176 00413000
*     DO;                                                          0176 00414000
*       CCDHSF='F0'X;               /* REQ. ALL FUNCTIONS            */ 00415000
         MVI   CCDHSF(GLBFF),X'F0'                                 0177 00416000
*       DO CHANID=0 TO CSDCHAD;     /* REQUEST STATUS FOR / DEVICES     00417000
*                                      ON ALL VAILD CHANNELS         */ 00418000
         B     @DE00178                                            0178 00419000
@DL00178 DS    0H                                                  0179 00420000
*         DEVONCHF(CHANID+1)='1'B;  /* SET CHAN FLAG ON              */ 00421000
         ALR   @14,GLBFF                                           0179 00422000
         OI    DEVONCHF(@14),B'10000000'                           0179 00423000
*       END;                                                       0180 00424000
         L     @07,CHANID                                          0180 00425000
         AH    @07,@CH00068                                        0180 00426000
         LR    @14,@07                                             0180 00427000
@DE00178 ST    @14,CHANID                                          0180 00428000
         L     @07,CVTPTR                                          0180 00429000
         L     @07,CVTCSD(,@07)                                    0180 00430000
         CH    @14,CSDCHAD(,@07)                                   0180 00431000
         BNH   @DL00178                                            0180 00432000
*     END;                                                         0181 00433000
*                                                                  0181 00434000
*   /*****************************************************************/ 00435000
*   /*                                                               */ 00436000
*   /* DEFAULT NOT CODED, PARSE EACH INDIVIDUAL OPERAND              */ 00437000
*   /*                                                               */ 00438000
*   /*****************************************************************/ 00439000
*                                                                  0182 00440000
*   ELSE                                                           0182 00441000
*     DO;                                                          0182 00442000
*                                                                  0182 00443000
         B     @RC00175                                            0182 00444000
@RF00175 DS    0H                                                  0183 00445000
*       /*************************************************************/ 00446000
*       /*                                                           */ 00447000
*       /* CHECK FOR '='                                             */ 00448000
*       /*                                                           */ 00449000
*       /*************************************************************/ 00450000
*                                                                  0183 00451000
*       IF CHAR^='=' THEN                                          0183 00452000
*                                                                  0183 00453000
         CLI   CHAR(PPTR),C'='                                     0183 00454000
         BE    @RF00183                                            0183 00455000
*         /***********************************************************/ 00456000
*         /*                                                         */ 00457000
*         /* SET UP DELIMITER ERROR EXIT                             */ 00458000
*         /*                                                         */ 00459000
*         /***********************************************************/ 00460000
*                                                                  0184 00461000
*         DO;                                                      0184 00462000
*           RC=12;                  /* DELIMITER ERROR               */ 00463000
         MVC   RC(4),@CF00037                                      0185 00464000
*           GO TO ERROREXT;         /* TAKE ERROR EXIT               */ 00465000
         B     ERROREXT                                            0186 00466000
*         END;                                                     0187 00467000
*                                                                  0187 00468000
*       /*************************************************************/ 00469000
*       /*                                                           */ 00470000
*       /* INCR PAST '='                                             */ 00471000
*       /*                                                           */ 00472000
*       /*************************************************************/ 00473000
*                                                                  0188 00474000
*       PPTR=PPTR+1;                                               0188 00475000
*                                                                  0188 00476000
@RF00183 AH    PPTR,@CH00068                                       0188 00477000
*       /*************************************************************/ 00478000
*       /*                                                           */ 00479000
