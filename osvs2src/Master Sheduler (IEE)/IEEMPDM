         OI    CHFC(@14),B'00000010'                               0163 00301000
*   R1=CSCBPTR;                     /* SET PTR TO CSCB               */ 00302000
         LR    R1,@14                                              0164 00303000
*   GEN(MGCR (1),CHAIN);            /* FREE CSCB                     */ 00304000
