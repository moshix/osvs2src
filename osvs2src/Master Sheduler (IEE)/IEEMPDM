         SVC   120                                                 0564 02535000
*     RESPECIFY                                                    0565 02536000
*      (GPR01F,                                                    0565 02537000
*       GPR15F,                                                    0565 02538000
*       GPR00F) UNRESTRICTED;                                      0565 02539000
*   END;                            /* GETMAIN (RC) LV(RSRWAL)     0566 02540000
*                                      SP(253)                       */ 02541000
*                                                                  0566 02542000
*   /*****************************************************************/ 02543000
*   /*                                                               */ 02544000
*   /* CHECK IF GETMAIN FAILED, IF SO THEN WRITE ERROR MESSAGE, SET  */ 02545000
*   /* ERROR RC AND RETURN                                           */ 02546000
*   /*                                                               */ 02547000
*   /*****************************************************************/ 02548000
*                                                                  0567 02549000
*   IF R15^=0 THEN                                                 0567 02550000
         LTR   R15,R15                                             0567 02551000
         BZ    @RF00567                                            0567 02552000
*     DO;                                                          0568 02553000
*       CALL WTORTN(16);            /* WORKING STORAGE NOT AVAIL     */ 02554000
         LA    @01,@AL00569                                        0569 02555000
         BAL   @14,WTORTN                                          0569 02556000
*       RC=4;                       /* SET ERROR RETURN CODE         */ 02557000
         MVC   RC(4),@CF00109                                      0570 02558000
*       RETURN;                     /* RETURN TO CALLER              */ 02559000
@EL00009 DS    0H                                                  0571 02560000
@EF00009 DS    0H                                                  0571 02561000
@ER00009 LM    @14,@12,@SA00009                                    0571 02562000
         BR    @14                                                 0571 02563000
*     END;                                                         0572 02564000
*   RSRWAP=R1;                      /* SAVE PTR TO GOOD RSR WA       */ 02565000
*                                                                  0573 02566000
@RF00567 ST    R1,RSRWAP(,JBWKPTR)                                 0573 02567000
*   /*****************************************************************/ 02568000
*   /*                                                               */ 02569000
*   /* SET UP WORKAREA AND ISSUE CALL TO RSR TO GET STATUS           */ 02570000
*   /*                                                               */ 02571000
*   /*****************************************************************/ 02572000
*                                                                  0574 02573000
*   DO RSRSTATP=RSRWAP TO RSRWAP+RSRWAL-1;/* FOR EACH WA BYTE        */ 02574000
         L     @14,RSRWAP(,JBWKPTR)                                0574 02575000
         B     @DE00574                                            0574 02576000
@DL00574 DS    0H                                                  0575 02577000
*     RSRSTAT2=''B;                 /* ZERO ENTIRE WA                */ 02578000
         L     @14,RSRSTATP(,JBWKPTR)                              0575 02579000
         MVI   RSRSTAT2(@14),X'00'                                 0575 02580000
*   END;                                                           0576 02581000
*                                                                  0576 02582000
         LA    @14,1                                               0576 02583000
         AL    @14,RSRSTATP(,JBWKPTR)                              0576 02584000
@DE00574 ST    @14,RSRSTATP(,JBWKPTR)                              0576 02585000
         L     @07,RSRWAP(,JBWKPTR)                                0576 02586000
         AL    @07,RSRWAL                                          0576 02587000
         BCTR  @07,0                                               0576 02588000
         CR    @14,@07                                             0576 02589000
         BNH   @DL00574                                            0576 02590000
*   /*****************************************************************/ 02591000
*   /*                                                               */ 02592000
*   /* SET UP RSR PARM LIST                                          */ 02593000
*   /*                                                               */ 02594000
*   /*****************************************************************/ 02595000
*                                                                  0577 02596000
*   RSRSTRBN=0;                     /* ADDR IN FORM SSP0             */ 02597000
         L     @14,RSRWAP(,JBWKPTR)                                0577 02598000
         SLR   @07,@07                                             0577 02599000
         STH   @07,RSRSTRBN(,@14)                                  0577 02600000
*   RSRFRCT=(CVTEORM+4096-1)/4096;  /* NUM OF PAGES                  */ 02601000
         L     @07,CVTPTR                                          0578 02602000
         L     @02,CVTEORM(,@07)                                   0578 02603000
         SH    @02,@CH03337                                        0578 02604000
         SRDA  @02,32                                              0578 02605000
         D     @02,@CF03027                                        0578 02606000
         STH   @03,RSRFRCT(,@14)                                   0578 02607000
*   RSRSTUS='1'B;                   /* REQUEST STATUS FUNC           */ 02608000
         OI    RSRSTUS(@14),B'00001000'                            0579 02609000
*   RSRSLP=RSRWAP+LENGTH(RSRPARML); /* @ STATUS BUFFER               */ 02610000
         LA    @09,20                                              0580 02611000
         ALR   @09,@14                                             0580 02612000
         ST    @09,RSRSLP(,@14)                                    0580 02613000
*   R1=RSRWAP;                      /* SET @ RSR PARM LIST           */ 02614000
*                                                                  0581 02615000
         LR    R1,@14                                              0581 02616000
*   /*****************************************************************/ 02617000
*   /*                                                               */ 02618000
*   /* CALL RSR TO GET PAGE STATUS                                   */ 02619000
*   /*                                                               */ 02620000
*   /*****************************************************************/ 02621000
*                                                                  0582 02622000
*   CALL RSR;                                                      0582 02623000
*                                                                  0582 02624000
         L     @07,PVTPTR(,@07)                                    0582 02625000
         L     @15,PVTPRCF(,@07)                                   0582 02626000
         BALR  @14,@15                                             0582 02627000
*   /*****************************************************************/ 02628000
*   /*                                                               */ 02629000
*   /* CHECK RETURN CODE FROM RSR IN REG 15                          */ 02630000
*   /*                                                               */ 02631000
*   /*****************************************************************/ 02632000
*                                                                  0583 02633000
*   IF R15^=0 THEN                                                 0583 02634000
*                                                                  0583 02635000
         LTR   R15,R15                                             0583 02636000
         BZ    @RF00583                                            0583 02637000
*     /***************************************************************/ 02638000
*     /*                                                             */ 02639000
*     /* IF NOT SUCCESSFUL THEN ABEND ROUTINE AND EXIT THRU ESTAE    */ 02640000
*     /* ROUTINE                                                     */ 02641000
*     /*                                                             */ 02642000
*     /***************************************************************/ 02643000
*                                                                  0584 02644000
*     DO;                                                          0584 02645000
*       INTERROR=R15;               /* SET ERROR CODE                */ 02646000
         ST    R15,INTERROR                                        0585 02647000
*       DO;                         /* ABEND(183) SYSTEM             */ 02648000
*         RESPECIFY                                                0587 02649000
*          (GPR01F) RESTRICTED;                                    0587 02650000
*         GPR01F=((183)&'00000FFF'X)*4096;/* COMP CODE IN BITS 8-19  */ 02651000
         L     GPR01F,@CF03338                                     0588 02652000
*         SVC(13);                  /* ISSUE ABEND SVC               */ 02653000
         SVC   13                                                  0589 02654000
*         RESPECIFY                                                0590 02655000
*          (GPR01F) UNRESTRICTED;                                  0590 02656000
*       END;                        /* ABEND(183) SYSTEM ABEND     0591 02657000
*                                      COMMAND PROCESSOR     @YM03534*/ 02658000
*     END;                                                         0592 02659000
*                                                                  0592 02660000
*   /*****************************************************************/ 02661000
*   /*                                                               */ 02662000
*   /* RETURN TO CALLER                                              */ 02663000
*   /*                                                               */ 02664000
*   /*****************************************************************/ 02665000
*                                                                  0593 02666000
*   RETURN;                                                        0593 02667000
         B     @EL00009                                            0593 02668000
*   END RSRSTAT;                                                   0594 02669000
         B     @EL00009                                            0594 02670000
*                                                                  0595 02671000
*/********************************************************************/ 02672000
*/*                                                                  */ 02673000
*/* PROCEDURE NAME - RCONSUB                                         */ 02674000
*/* DESCRIPTIVE NAME - RECONFIGURABLE STORAGE UNIT STATUS SUBROUTINE */ 02675000
*/* FUNCTION -                                                       */ 02676000
*/*    THIS SUBROUTINE WILL DETERMINE THE RECONFIGURABLE STORAGE     */ 02677000
*/*    UNITS AVAILABLE TO THE SYSTEM AND PUT OUT ONE MESSAGE OF THE  */ 02678000
*/*    FOLLOWING FORMAT FOR EACH RECONFIGURABLE CONTIGUOUS STORAGE   */ 02679000
*/*    RANGE:                                                        */ 02680000
*/*        R XXXXXK - YYYYYK                                         */ 02681000
*/*                                                                  */ 02682000
*/*    A RECONFIGURABLE STORAGE UNIT (RSU) IS DEFINED TO BE ONE      */ 02683000
*/*    STORAGE BOX WHICH CONTAINS NO LONG-TERM FIXED PAGES, AND WHICH*/ 02684000
*/*    WILL THUS BE ABLE TO BE BROUGHT ONLINE OR OFFLINE USING THE   */ 02685000
*/*    VARY STORAGE COMMAND.  THE RECONFIGURABLE UNIT MAY CURRENTLY  */ 02686000
*/*    BE EITHER ONLINE OR OFFLINE.                                  */ 02687000
*/*                                                                  */ 02688000
*/*    THE MINIMUM NUMBER OF RSU'S AVAILABLE TO THE SYSTEM IS        */ 02689000
*/*    INITIALLY SET BY THE RSU= KEYWORD WHEN RESPONDING TO THE      */ 02690000
*/*    SPECIFY SYSTEM PARAMETERS MESSAGE DURING IPL.  THIS NUMBER MAY*/ 02691000
*/*    SUBSEQUENTLY BE LOWERED BY RSM DEPENDING UPON SYSTEM DEMAND   */ 02692000
*/*    FOR STORAGE IN WHICH TO PLACE LONG-TERM FIXED PAGES.          */ 02693000
*/*                                                                  */ 02694000
*/* OPERATION -                                                      */ 02695000
*/*    (1) GET THE TOP ADDRESS OF THE NUCLEUS FROM CVTNUCB, AND THE  */ 02696000
*/*        TOP POSSIBLE REAL ADDRESS IN THE SYSTEM FROM CVTEORM.     */ 02697000
*/*    (2) GET THE NUMBER OF 4K FRAMES IN A STORAGE BOX FROM PVTRSUS */ 02698000
*/*        AND CALCULATE THE NUMBER OF BYTES IN A BOX.               */ 02699000
*/*    (3) ROUND THE TOP NUCLEUS ADDRESS UP TO A BOX BOUNDARY.       */ 02700000
*/*    (4) TAKE THE ADDRESS OF THE FIRST BYTE IN THIS FIRST INTEGRAL */ 02701000
*/*        BOX ABOVE THE NUCLEUS AND SHIFT IT RIGHT 8 BITS TO GET    */ 02702000
*/*        A RBN.  ADD THE RBN TO THE APPARENT ORIGIN OF THE PFT     */ 02703000
*/*        (PVTPFTP) TO GET THE ADDRESS OF THE PFTE FOR THE FIRST    */ 02704000
*/*        FRAME IN THIS BOX.                                        */ 02705000
*/*    (5) CHECK PFTPREF BIT FOR ON.  IF IT IS ON, THEN THIS BOX IS  */ 02706000
*/*        A PART OF THE PREFERRED AREA (FOR LONG-TERM FIXED PAGES), */ 02707000
*/*        AND THUS IS NOT RECONFIGURABLE.  IF THE BIT IS OFF, SAVE  */ 02708000
*/*        THE ADDR OF THIS BOX AS THE BEGINNING OF A CONTIGUOUS     */ 02709000
*/*        RECONFIGURABLE RANGE.                                     */ 02710000
*/*    (6) GO TO THE FIRST FRAME OF THE NEXT HIGHER BOX AND REPEAT   */ 02711000
*/*        STEPS 4 AND 5.  WE NEED ONLY CHECK THE FIRST FRAME IN     */ 02712000
*/*        EACH BOX SINCE RSM WILL SET THE PFTPREF BIT IN ONE WHOLE  */ 02713000
*/*        BOX AT A TIME.  THUS IF ONE FRAME IS RECONFIGURABLE,      */ 02714000
*/*        THEN THE ENTIRE BOX IS RECONFIGURABLE.                    */ 02715000
*/*    (7) PUT OUT ONE MESSAGE FOR EACH CONTIGUOUS RECONFIGURABLE    */ 02716000
*/*        RANGE, CONVERTING THE ADDRESSES TO DECIMAL K ADDRESSES.   */ 02717000
*/*    (8) STOP THE LOOP AND EXIT WHEN THE HIGH POSSIBLE REAL ADDRESS*/ 02718000
*/*        IS REACHED.                                               */ 02719000
*/* RETURN -  TO CALLER                                              */ 02720000
*/*                                                                  */ 02721000
*/********************************************************************/ 02722000
*                                                                  0595 02723000
*RCONSUB:                                                          0595 02724000
*   PROC;                                                          0595 02725000
RCONSUB  STM   @14,@12,@SA00010                                    0595 02726000
*   BOXSIZE=PVTRSUS*4096;           /* SIZE OF BOX IN BYTE   @YD03005*/ 02727000
         L     @14,CVTPTR                                          0596 02728000
