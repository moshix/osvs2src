*   /*****************************************************************/ 04450000
*   /*                                                               */ 04451000
*   /* PAGEFIX ROUTINE WILL FIX STORAGE FOR CALLER TO RUN DISABLED.  */ 04452000
*   /* CALLER WILL SET UP REGS BEFORE CALLING ROUTINE. IF NECESSARY  */ 04453000
*   /* CONTROL WILL NOT BE RETURNED TO CALLER UNTIL FUNCTION HAS BEEN*/ 04454000
*   /* PERFORMED. IF AN ERROR OCCURS A MESSAGE WILL BE WRITTEN AND   */ 04455000
*   /* RETURN MADE TO CALLER.                                @ZA20381*/ 04456000
*   /*                                                               */ 04457000
*   /*****************************************************************/ 04458000
*                                                                  0965 04459000
*   RFY                                                            0965 04460000
*    (R7,                                                          0965 04461000
*     R8,                                                          0965 04462000
*     R9) RESTRICTED;               /*                       @ZA20381*/ 04463000
*   PGFXECB=0;                      /* ZERO OUT THE ECB      @ZA20381*/ 04464000
         SLR   @14,@14                                             0966 04465000
         ST    @14,PGFXECB                                         0966 04466000
*   R9=ADDR(PGFXECB);               /* PAGE FIX ECB          @ZA20381*/ 04467000
         LA    R9,PGFXECB                                          0967 04468000
*   GEN(PGFIX R,A=(7),EA=(8),ECB=(9),LONG=N);/* PAGEFIX @ZA20381W    */ 04469000
*                                                                  0968 04470000
         PGFIX R,A=(7),EA=(8),ECB=(9),LONG=N                            04471000
*   /*****************************************************************/ 04472000
*   /*                                                               */ 04473000
*   /* CHECK IF WAIT SHOULD BE ISSUED.. CHECK POST BIT IN ECB        */ 04474000
*   /*                                                       @ZA20381*/ 04475000
*   /*                                                               */ 04476000
*   /*****************************************************************/ 04477000
*                                                                  0969 04478000
*   IF POSTED^='1'B THEN            /* ECB POSTED YET        @ZA20381*/ 04479000
         TM    POSTED,B'01000000'                                  0969 04480000
         BO    @RF00969                                            0969 04481000
*     GEN(WAIT ECB=PGFXECB);        /* WAIT ON ECB           @ZA20381*/ 04482000
*                                                                  0970 04483000
         WAIT ECB=PGFXECB                                               04484000
*   /*****************************************************************/ 04485000
*   /*                                                               */ 04486000
*   /* AFTER WE COME OUT OF WAIT CHECK POST CODE TO SEE IF FIX WAS   */ 04487000
*   /* SUCCESSFUL @ZA20381W                                          */ 04488000
*   /*                                                               */ 04489000
*   /*****************************************************************/ 04490000
*                                                                  0971 04491000
*   IF PGFXCODE^=0 THEN                                            0971 04492000
@RF00969 ICM   @14,7,PGFXCODE                                      0971 04493000
         BZ    @RF00971                                            0971 04494000
*     DO;                           /* PAGEFIX FAILED        @ZA20381*/ 04495000
*       CALL WTORTN(17);            /* WRITE MSG CMD PROCESSOR FAILED   04496000
*                                                            @ZA20381*/ 04497000
