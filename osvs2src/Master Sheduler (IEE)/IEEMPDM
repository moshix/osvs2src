*         RSRSTATP=RSRSTATP+1;      /* INCR MAP PTR                  */ 02306000
         LA    @14,1                                               0509 02307000
         AL    @14,RSRSTATP(,JBWKPTR)                              0509 02308000
         ST    @14,RSRSTATP(,JBWKPTR)                              0509 02309000
*       END;                                                       0510 02310000
*   END;                            /* END OF MAP SCAN LOOP          */ 02311000
*                                                                  0511 02312000
@RC00434 DS    0H                                                  0511 02313000
@DE00433 L     @14,RSRWAP(,JBWKPTR)                                0511 02314000
         SLR   @07,@07                                             0511 02315000
         ICM   @07,3,RSRFRCT(@14)                                  0511 02316000
         AL    @07,RSRSLP(,@14)                                    0511 02317000
         C     @07,RSRSTATP(,JBWKPTR)                              0511 02318000
         BH    @DL00433                                            0511 02319000
*   /*****************************************************************/ 02320000
*   /*                                                               */ 02321000
*   /* CHECK IF ANY PAGES WERE FOUND TO BE OFFLINE                   */ 02322000
*   /*                                                               */ 02323000
*   /*****************************************************************/ 02324000
*                                                                  0512 02325000
*   IF ANYOFFLN='0'B THEN                                          0512 02326000
*                                                                  0512 02327000
         TM    ANYOFFLN,B'10000000'                                0512 02328000
         BNZ   @RF00512                                            0512 02329000
*     /***************************************************************/ 02330000
*     /*                                                             */ 02331000
*     /* NO, WRITE NO STORAGE OFFLINE MESSAGE                        */ 02332000
*     /*                                                             */ 02333000
*     /***************************************************************/ 02334000
*                                                                  0513 02335000
*     DO;                                                          0513 02336000
*       CALL WTORTN(11);            /* NO STORAGE OFFLINE            */ 02337000
         LA    @01,@AL00514                                        0514 02338000
         BAL   @14,WTORTN                                          0514 02339000
*     END;                                                         0515 02340000
*                                                                  0515 02341000
*/* DETERMINE IF THERE ARE ANY RECONFIGURABLE STORAGE UNITS  @ZA20381*/ 02342000
*                                                                  0516 02343000
