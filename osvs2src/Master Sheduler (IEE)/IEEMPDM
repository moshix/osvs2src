         B     @EL00012                                            0819 03928000
*                                                                  0820 03929000
*   /*****************************************************************/ 03930000
*   /*                                                               */ 03931000
*   /* ESTAE ROUTINE TO CLEANUP RESOURCES                            */ 03932000
*   /*                                                               */ 03933000
*   /*****************************************************************/ 03934000
*                                                                  0820 03935000
*ESTAERTN:                                                         0820 03936000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0820 03937000
*                                                                  0820 03938000
ESTAERTN DS    0H                                                  0821 03939000
*/*     ROUTINE WILL BE GIVEN CONROL BY RTM WHEN THIS TASK IS        */ 03940000
*/*  SCHEDULED FOR AN ABEND.                                         */ 03941000
*/*                                                                  */ 03942000
*                                                                  0821 03943000
*   /*****************************************************************/ 03944000
*   /*                                                               */ 03945000
*   /* SUBROUTINE FLAGS                                              */ 03946000
*   /*                                                               */ 03947000
*   /*****************************************************************/ 03948000
*                                                                  0821 03949000
*   DCL                                                            0821 03950000
*     1 * BIT(8),                   /* LOCAL FLAGS                   */ 03951000
*      2 GOODRTCA BIT(1);           /* GOOD RTCA AVAILABLE           */ 03952000
*                                                                  0821 03953000
*   /*****************************************************************/ 03954000
*   /*                                                               */ 03955000
*   /* ESTAE ENTRY CODE                                              */ 03956000
*   /*                                                               */ 03957000
*   /*****************************************************************/ 03958000
*                                                                  0822 03959000
*   DO;                             /* ENTESTAE (R10,R11,R12)SAVEAREA   03960000
*                                      (ESTAESA)ERREXIT(NORTCA)PARMRE   03961000
*                                      G(R15)                        */ 03962000
*/*  ENTRY CODE FOR ESTAE ROUTINE.                                   */ 03963000
*/*  IF RTCA ALLOCATED THEN RESTORE CODE AND                         */ 03964000
*/*  DATA REGS FROM START OF USERS PARM LIST,                        */ 03965000
*/*  AND ESTABLISH ADDR TO ESTAE SAVE AREA                           */ 03966000
*/*  IF REQUIRED.                                                    */ 03967000
*/*  IF NO RTCA AVAILABLE THEN BRANCH TO ERROR                       */ 03968000
*/*  CODE AFTER ESTABLISHING ADDRESSABILITY                          */ 03969000
*                                                                  0823 03970000
*     GEN(USING *,GPR15P);                                         0823 03971000
         USING *,GPR15P                                                 03972000
*     GEN(CH    GPR00F,*+30);       /* CHECK IF RTCA ALLOC FAILED    */ 03973000
         CH    GPR00F,*+30                                              03974000
*     GEN(BNE   *+28);              /* NO,SAVE RTM REGS              */ 03975000
         BNE   *+28                                                     03976000
*     GEN(DROP  GPR15P);            /* DROP LOCAL ADDR               */ 03977000
         DROP  GPR15P                                                   03978000
*     GEN(LR    R15,GPR02P);        /* PTR TO USER PARM LIST         */ 03979000
         LR    R15,GPR02P                                               03980000
*     GEN(L     R10,00(R15));                                      0828 03981000
         L     R10,00(R15)                                              03982000
*     GEN(L     R11,04(R15));                                      0829 03983000
         L     R11,04(R15)                                              03984000
*     GEN(L     R12,08(R15));                                      0830 03985000
         L     R12,08(R15)                                              03986000
*     GEN(LA    GPR13P,ESTAESA);    /* SET PTR TO ESTAE SA           */ 03987000
         LA    GPR13P,ESTAESA                                           03988000
*     GEN(B     NORTCA);            /* BRANCH TO ERROR EXIT          */ 03989000
         B     NORTCA                                                   03990000
*     GEN(DC    H'12');             /* NO RTCA AVAIL RC              */ 03991000
         DC    H'12'                                                    03992000
*     GEN(STM   GPR14P,GPR12F,12(GPR13P));/* GOOD R13, SAVE RTM'S  0834 03993000
*                                      REGS                          */ 03994000
         STM   GPR14P,GPR12F,12(GPR13P)                                 03995000
*     GEN(L     R15,0(GPR01P));     /* PTR TO USER PARM LIST         */ 03996000
         L     R15,0(GPR01P)                                            03997000
*     GEN(L     R10,00(R15));                                      0836 03998000
         L     R10,00(R15)                                              03999000
*     GEN(L     R11,04(R15));                                      0837 04000000
         L     R11,04(R15)                                              04001000
*     GEN(L     R12,08(R15));                                      0838 04002000
         L     R12,08(R15)                                              04003000
*     GEN(ST    GPR13P,ESTAESA+4);  /* SET PTR TO RTM SA             */ 04004000
         ST    GPR13P,ESTAESA+4                                         04005000
*     GEN(LA    GPR13P,ESTAESA);    /* SET PTR TO ESTAE SA           */ 04006000
         LA    GPR13P,ESTAESA                                           04007000
*   END;                            /* ENTESTAE (R10,R11,R12)SAVEAREA   04008000
*                                      (ESTAESA)ERREXIT(NORTCA)PARMRE   04009000
*                                      G(R15)                        */ 04010000
*   RFY                                                            0842 04011000
*    (R2,                                                          0842 04012000
*     R3,                                                          0842 04013000
