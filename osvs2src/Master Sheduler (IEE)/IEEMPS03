         LA    @12,@CC02246                                        0154 00366000
         ST    @12,I05606                                          0154 00367000
*     I05607(1)=ADDR(RNAME);        /* ADDRESS OF MINOR RESOURCE NAME*/ 00368000
         LA    @12,@CC02244                                        0155 00369000
         ST    @12,I05607                                          0155 00370000
*     I05603(1)='01000000'B;        /* SET BIT STRING 1 FOR RESOURCE    00371000
*                                      ELEMENT                       */ 00372000
         MVI   I05603,B'01000000'                                  0156 00373000
*     I05605(1)='01000000'B;        /* SET BIT STRING 2 FOR RESOURCE    00374000
*                                      ELEMENT                       */ 00375000
         MVI   I05605,B'01000000'                                  0157 00376000
*     I05603(1,1)='1'B;             /* SET END OF LIST BIT           */ 00377000
         OI    I05603,B'10000000'                                  0158 00378000
*     GPR01P=ADDR(I05602);          /* LOAD LIST ADDRESS             */ 00379000
         LA    GPR01P,I05602                                       0159 00380000
*     SVC(56);                      /* ISSUE ENQ SVC                 */ 00381000
         SVC   56                                                  0160 00382000
*     RESPECIFY                                                    0161 00383000
*      (GPR01P) UNRESTRICTED;                                      0161 00384000
*   END;                            /* ENQ (QNAME,RNAME,E,3,SYSTEM)  */ 00385000
*   VRYENQ=ON;                      /* SET ENQ FOOT PRINT ON         */ 00386000
         OI    VRYENQ,B'01000000'                                  0163 00387000
*   SRWALN=CSDCPUOL*208;            /* IEESTPRS REQUIRES 208 BYTES OF   00388000
*                                      WORK AREA FOR EACH ALIVE CPU  */ 00389000
*                                                                  0164 00390000
         L     @12,CVTPTR                                          0164 00391000
         L     @12,CVTCSD(,@12)                                    0164 00392000
         LH    @12,CSDCPUOL(,@12)                                  0164 00393000
         MH    @12,@CH02336                                        0164 00394000
         ST    @12,SRWALN                                          0164 00395000
*   /*****************************************************************/ 00396000
*   /*                                                               */ 00397000
*   /* GET FIXED WORK AREA FROM LSQA                                 */ 00398000
*   /*                                                               */ 00399000
*   /*****************************************************************/ 00400000
*                                                                  0165 00401000
*   DO;                             /* GETMAIN (RC) LV(SRWALN)     0165 00402000
*                                      SP(253)                       */ 00403000
*     RESPECIFY                                                    0166 00404000
*      (GPR01F,                                                    0166 00405000
*       GPR15F,                                                    0166 00406000
*       GPR00F) RESTRICTED;                                        0166 00407000
*     GPR01F=0;                     /* REG 1 MUST BE ZERO            */ 00408000
         SLR   GPR01F,GPR01F                                       0167 00409000
*     GPR15F=0;                     /* RC-TYPE GETMAIN               */ 00410000
         SLR   GPR15F,GPR15F                                       0168 00411000
*     GPR00F=SRWALN;                /* LENGTH REQUESTED              */ 00412000
         LR    GPR00F,@12                                          0169 00413000
*     GPR15F=GPR15F|((253)*256);    /* SP IN BYTE 2                  */ 00414000
         O     GPR15F,@CF02449                                     0170 00415000
*     SVC(120);                     /* RC/RU-FORM OF GETMAIN         */ 00416000
         SVC   120                                                 0171 00417000
*     RESPECIFY                                                    0172 00418000
*      (GPR01F,                                                    0172 00419000
*       GPR15F,                                                    0172 00420000
*       GPR00F) UNRESTRICTED;                                      0172 00421000
*   END;                            /* GETMAIN (RC) LV(SRWALN)     0173 00422000
*                                      SP(253)                       */ 00423000
*                                                                  0173 00424000
*   /*****************************************************************/ 00425000
*   /*                                                               */ 00426000
*   /* DETERMINE WHETHER GETMAIN WAS SUCCESSFUL                      */ 00427000
*   /*                                                               */ 00428000
*   /*****************************************************************/ 00429000
*                                                                  0174 00430000
*   IF R15^=GOODRC THEN                                            0174 00431000
         LTR   R15,R15                                             0174 00432000
         BZ    @RF00174                                            0174 00433000
*     DO;                                                          0175 00434000
*       MSGID=GFMI;                 /* GETMAIN FAILED MSG            */ 00435000
         MVC   MSGID(2),@CH00059                                   0176 00436000
*       GOTO BADSURTN;              /* RETURN CODE 4 EXIT            */ 00437000
         B     BADSURTN                                            0177 00438000
*     END;                                                         0178 00439000
*   GTMAIN=ON;                      /* SET GETMAIN FOOT PRINT        */ 00440000
@RF00174 OI    GTMAIN,B'00100000'                                  0179 00441000
*   SRWAAD=R1;                      /* SAVE WORK AREA ADDR           */ 00442000
         ST    R1,SRWAAD                                           0180 00443000
*   R3=SRWALN;                      /* WORK AREA LENGTH IN ODD REG   */ 00444000
         L     R3,SRWALN                                           0181 00445000
*   R15=NULL;                       /* ZERO PAD CHAR                 */ 00446000
         SLR   R15,R15                                             0182 00447000
*   R14=NULL;                       /* ZERO LENGTH                   */ 00448000
         SLR   R14,R14                                             0183 00449000
*   R2=R1;                          /* AREA ADDR IN EVEN REG         */ 00450000
         LR    R2,R1                                               0184 00451000
*   GEN(MVCL R2,R14);               /* ZERO WORK AREA                */ 00452000
         MVCL R2,R14                                                    00453000
*   RETURN CODE(GOODRC);            /* NORMAL RETURN ----------------*/ 00454000
*                                                                  0186 00455000
         SLR   @15,@15                                             0186 00456000
         L     @13,4(,@13)                                         0186 00457000
         L     @14,12(,@13)                                        0186 00458000
         LM    @00,@12,20(@13)                                     0186 00459000
         BR    @14                                                 0186 00460000
*   /*****************************************************************/ 00461000
*   /*                                                               */ 00462000
*   /* ERROR RETURN TO MAIN LINE                                     */ 00463000
*   /*                                                               */ 00464000
*   /*****************************************************************/ 00465000
*                                                                  0187 00466000
*BADSURTN:                                                         0187 00467000
*   RETURN CODE(BADRC);             /* ISSUE MSG AND EXIT -----------*/ 00468000
BADSURTN LA    @15,4                                               0187 00469000
