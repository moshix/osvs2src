*                                                                  0135 00267000
         BAL   @14,STPRSTRT                                        0135 00268000
*   /*****************************************************************/ 00269000
*   /*                                                               */ 00270000
*   /* THE EXIT LABEL CODE CONCLUDES IEEMPS03 PROCESSING             */ 00271000
*   /*                                                               */ 00272000
*   /*****************************************************************/ 00273000
*                                                                  0136 00274000
*EXIT:                                                             0136 00275000
*                                                                  0136 00276000
*   /*****************************************************************/ 00277000
*   /*                                                               */ 00278000
*   /* ISSUE MSG ACCORDING TO MSGID LAST SET BY SUBROUTINES AND FREE */ 00279000
*   /* SYSTEM RESOURCES                                              */ 00280000
*   /*                                                               */ 00281000
*   /*****************************************************************/ 00282000
*                                                                  0136 00283000
*   CALL CLEANUP;                                                  0136 00284000
EXIT     BAL   @14,CLEANUP                                         0136 00285000
*   R14=RETADD;                     /* MASTER SCHED RETURN ADDR      */ 00286000
         L     R14,RETADD                                          0137 00287000
*   RETURN;                         /* RETURN TO CALLER--------------*/ 00288000
@EL00001 L     @13,4(,@13)                                         0138 00289000
@EF00001 L     @00,@SIZDATD                                        0138 00290000
         LR    @01,@10                                             0138 00291000
         FREEMAIN R,LV=(0),A=(1)                                        00292000
@ER00001 BR    @14                                                 0138 00293000
*   RFY                                                            0139 00294000
*    (R11,                                                         0139 00295000
*     R10) UNRSTD;                  /* FREE CODE AND DATA REGS       */ 00296000
*                                                                  0140 00297000
*/********************************************************************/ 00298000
*/*                                                                  */ 00299000
*/* SETUP PROCEDURE                                                  */ 00300000
*/*                                                                  */ 00301000
*/* FUNCTION: - ESTABLISH ESTAE ENVIRONMENT                          */ 00302000
*/*           - ENQ ON SYSZVARY,CPU                                  */ 00303000
*/*           - GETMAIN IEESTPRS WORK AREA                           */ 00304000
*/*                                                                  */ 00305000
*/********************************************************************/ 00306000
*                                                                  0140 00307000
*SETUP:                                                            0140 00308000
*   PROC OPTIONS(SAVE,SAVEAREA);                                   0140 00309000
SETUP    STM   @14,@12,12(@13)                                     0140 00310000
         ST    @13,@SA00002+4                                      0140 00311000
         LA    @12,@SA00002                                        0140 00312000
         ST    @12,8(,@13)                                         0140 00313000
         LR    @13,@12                                             0140 00314000
*   R2=ADDR(ESTAEPRM);              /* ESTAE PARM AREA USED BY     0141 00315000
*                                      ENTESTAE MACRO TO RESTORE CODE   00316000
*                                      AND DATA REGS                 */ 00317000
         LA    R2,ESTAEPRM                                         0141 00318000
*   R1=ADDR(ESTAELST);              /* CONTROL BLOCK AREA NORMALLY 0142 00319000
*                                      FILLED IN BY LIST VERSION OF     00320000
*                                      ESTAE MACRO                   */ 00321000
*                                                                  0142 00322000
         LA    R1,ESTAELST                                         0142 00323000
*   /*****************************************************************/ 00324000
*   /*                                                               */ 00325000
*   /* ESTABLISH ESTAE EXIT                                          */ 00326000
*   /*                                                               */ 00327000
*   /*****************************************************************/ 00328000
*                                                                  0143 00329000
*   GENERATE REFS(ESTAERTN,R1,R2) SETS(R0,R1,R14,R15);             0143 00330000
*                                                                  0143 00331000
   ESTAE ESTAERTN,PARAM=(2),MF=(E,(1))                                  00332000
*   /*****************************************************************/ 00333000
*   /*                                                               */ 00334000
*   /* DETERMINE WHETHER ESTAE EXIT ESTABLISHED                      */ 00335000
*   /*                                                               */ 00336000
*   /*****************************************************************/ 00337000
*                                                                  0144 00338000
*   IF R15^=GOODRC THEN                                            0144 00339000
         LTR   R15,R15                                             0144 00340000
         BZ    @RF00144                                            0144 00341000
*     DO;                                                          0145 00342000
*       MSGID=SEMI;                 /* SYSTEM ERROR MSG              */ 00343000
         MVC   MSGID(2),@CH01001                                   0146 00344000
*       GOTO BADSURTN;              /* RETURN CODE 4 EXIT            */ 00345000
         B     BADSURTN                                            0147 00346000
*     END;                                                         0148 00347000
*   ESTENV=ON;                      /* SET ESTAE EXIT ESTABLISHED  0149 00348000
*                                      FOOT PRINT ON                 */ 00349000
*                                                                  0149 00350000
@RF00144 OI    ESTENV,B'10000000'                                  0149 00351000
*   /*****************************************************************/ 00352000
*   /*                                                               */ 00353000
*   /* SERIALIZE MP COMMAND PROCESSING                               */ 00354000
*   /*                                                               */ 00355000
*   /*****************************************************************/ 00356000
*                                                                  0150 00357000
*   DO;                             /* ENQ (QNAME,RNAME,E,3,SYSTEM)  */ 00358000
*     RESPECIFY                                                    0151 00359000
*      (GPR01P) RESTRICTED;                                        0151 00360000
*     I056(1:16)=I056(1:16)&&I056(1:16);/* ZERO OUT THE TABLE        */ 00361000
         XC    I056(16),I056                                       0152 00362000
*     I05604(1)=3;                  /* LENGTH OF MINOR RESOURCE NAME */ 00363000
