*   FRRENTRY=ON;                    /* FOOT PRINT                    */ 01414000
*                                                                  0394 01415000
         OI    FRRENTRY,B'01000000'                                0394 01416000
*   /*****************************************************************/ 01417000
*   /*                                                               */ 01418000
*   /* DETERMINE WHETHER QUIESCE OWNS RESTART RESOURCE WORD          */ 01419000
*   /*                                                               */ 01420000
*   /*****************************************************************/ 01421000
*                                                                  0395 01422000
*   IF RSTWD=ON                     /* TEST FOOT PRINT               */ 01423000
*     THEN                                                         0395 01424000
         TM    RSTWD,B'00000010'                                   0395 01425000
