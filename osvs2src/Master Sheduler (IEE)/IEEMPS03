         AL    @12,@CF02450                                        0092 00096000
         L     @12,UCMMCSPT(,@12)                                  0092 00097000
         L     @12,UCMDVEPT(,@12)                                  0092 00098000
         MVC   CONSLID(1),UCMID(@12)                               0092 00099000
*       ELSE                                                       0093 00100000
*         CONSLID=CHUCMP;           /* ISSUING CONSOLE               */ 00101000
         B     @RC00091                                            0093 00102000
@RF00091 L     @12,CSCBPTR                                         0093 00103000
         MVC   CONSLID(1),CHUCMP(@12)                              0093 00104000
*       MSGID=CRMI;                 /* COMMAND REJECT MSG ID         */ 00105000
@RC00091 MVC   MSGID(2),@CH00038                                   0094 00106000
*       GOTO EXIT;                  /* ISSUE MSG AND RETURN CONTROL  */ 00107000
         B     EXIT                                                0095 00108000
*     END;                                                         0096 00109000
*                                                                  0096 00110000
*   /*****************************************************************/ 00111000
*   /*                                                               */ 00112000
*   /* SET UP ESTAE ENVIRONMENT, ENQ ON SYSZVARY AND GETMAIN PAGE    */ 00113000
*   /* FIXED WORK AREA FOR IEESTPRS                                  */ 00114000
*   /*                                                               */ 00115000
*   /*****************************************************************/ 00116000
*                                                                  0097 00117000
*   CALL SETUP;                                                    0097 00118000
*                                                                  0097 00119000
@RC00088 BAL   @14,SETUP                                           0097 00120000
*   /*****************************************************************/ 00121000
*   /*                                                               */ 00122000
*   /* IF AN ERROR WAS ENCOUNTERED IN SETUP, ISSUE MESSAGE AND EXIT  */ 00123000
*   /*                                                               */ 00124000
*   /*****************************************************************/ 00125000
*                                                                  0098 00126000
*   IF R15=BADRC THEN                                              0098 00127000
         C     R15,@CF00059                                        0098 00128000
