         LR    @13,@12                                             0240 00790000
*   RFY                                                            0241 00791000
*    (R4,                                                          0241 00792000
*     R10) RSTD;                    /* RESTRICT ADDITIONAL WORK REG     00793000
*                                      AND RESTRICT CODE REG         */ 00794000
*                                                                  0241 00795000
*   /*****************************************************************/ 00796000
*   /*                                                               */ 00797000
*   /* TEST FOOT PRINT FOR RESTART RESOURCE WORD OWNERSHIP           */ 00798000
*   /*                                                               */ 00799000
*   /*****************************************************************/ 00800000
*                                                                  0242 00801000
*   IF RSTWD=ON THEN                                               0242 00802000
         TM    RSTWD,B'00000010'                                   0242 00803000
         BNO   @RF00242                                            0242 00804000
*     DO;                                                          0243 00805000
*       CVTRSTWD=NULL;              /* RELEASE RESOURCE              */ 00806000
         L     @12,CVTPTR                                          0244 00807000
         SLR   @05,@05                                             0244 00808000
         ST    @05,CVTRSTWD(,@12)                                  0244 00809000
*       RSTWD=OFF;                  /* ERASE FOOT PRINT              */ 00810000
         NI    RSTWD,B'11111101'                                   0245 00811000
*     END;                                                         0246 00812000
*                                                                  0246 00813000
*   /*****************************************************************/ 00814000
*   /*                                                               */ 00815000
*   /* TEST FOOT PRINT FOR LCCARSTR OWNERSHIP                        */ 00816000
*   /*                                                               */ 00817000
*   /*****************************************************************/ 00818000
*                                                                  0247 00819000
*   IF EXFLIHFG=ON THEN                                            0247 00820000
@RF00242 TM    EXFLIHFG,B'00000100'                                0247 00821000
         BNO   @RF00247                                            0247 00822000
*     DO;                                                          0248 00823000
*       LCCARSTR=OFF;               /* RELEASE CVTRSTWD SPIN BIT     */ 00824000
         L     @12,LCCAPTR                                         0249 00825000
         NI    LCCARSTR(@12),B'11110111'                           0249 00826000
*       EXFLIHFG=OFF;               /* ERASE FOOT PRINT              */ 00827000
         NI    EXFLIHFG,B'11111011'                                0250 00828000
*     END;                                                         0251 00829000
*                                                                  0251 00830000
*   /*****************************************************************/ 00831000
*   /*                                                               */ 00832000
*   /* TEST FOOT PRINT FOR DISP LOCK OWNERSHIP                       */ 00833000
*   /*                                                               */ 00834000
*   /*****************************************************************/ 00835000
*                                                                  0252 00836000
*   IF DSABLE=ON THEN                                              0252 00837000
@RF00247 TM    DSABLE,B'00001000'                                  0252 00838000
         BNO   @RF00252                                            0252 00839000
*     DO;                                                          0253 00840000
*                                                                  0253 00841000
*       /*************************************************************/ 00842000
*       /*                                                           */ 00843000
*       /* RELEASE RESOURCES AND ENABLE FOR INTERRUPTS               */ 00844000
*       /*                                                           */ 00845000
*       /*************************************************************/ 00846000
*                                                                  0254 00847000
*       GENERATE SETS(R0,R1,R14,R15) DEFS(ENABLE) REFS(DISABLE,PSACSTK, 00848000
*           PSA,PSALITA,FLC,FRRSCURR,FRRS,FRRSEMP,FRRSELEN);       0254 00849000
