         TITLE 'IEEMPS03 - QUIESCE COMMAND PROCESSOR                   *00001000
                        '                                               00002000
IEEMPS03 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(19)                                             0001 00007000
         DC    C'IEEMPS03R0300075294'                              0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @11,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@11                                         0001 00012000
         LR    @12,@01                                             0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @10,@01                                             0001 00016000
         USING @DATD,@10                                           0001 00017000
         LR    @01,@12                                             0001 00018000
         ST    @13,@SA00001+4                                      0001 00019000
         LR    @13,@10                                             0001 00020000
         MVC   @PC00001(4),0(@01)                                  0001 00021000
*   WORKAREA=''B;                   /* ZERO DYNAMIC WORK AREA        */ 00022000
         XC    WORKAREA(242),WORKAREA                              0080 00023000
*   CSCBPTR=R1;                     /* SAVE QUIESCE CSCB ADDR        */ 00024000
         ST    R1,CSCBPTR                                          0081 00025000
*   RETADD=R14;                     /* SAVE RETURN ADDR              */ 00026000
         ST    R14,RETADD                                          0082 00027000
*   CODEREG1=R11;                   /* SAVE CODE REG                 */ 00028000
         ST    R11,CODEREG1                                        0083 00029000
*   DATAREG1=R10;                   /* SAVE DATA REG                 */ 00030000
         ST    R10,DATAREG1                                        0084 00031000
*   DATAEND=R10+@SIZDATD;           /* END OF DATA AREA              */ 00032000
