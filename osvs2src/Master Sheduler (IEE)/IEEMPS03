*   GENERATE SETS(R0,R1,R14,R15) REFS(CVTRSTWD,ENABLE) DEFS(DISABLE);   00213000
@RF00116 DS    0H                                                  0125 00214000
DISABLE SETLOCK OBTAIN,TYPE=DISP,MODE=UNCOND,REGS=USE,                XX00215000
               RELATED=(CVTRSTWD,IEEMPS03(ENABLE))                      00216000
*   DSABLE=ON;                      /* SET DISABLED FOOT PRINT       */ 00217000
         OI    DSABLE,B'00001000'                                  0126 00218000
*   R2=ADDR(FRRRTN);                /* ADDR FUNCTIONAL RECOVERY RTN  */ 00219000
