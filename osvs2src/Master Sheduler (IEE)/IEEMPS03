*/* SWAPWORD SUBROUTINE                                              */ 00587000
*/*                                                                  */ 00588000
*/* FUNCTION: - SERIALIZE USE OF THE STOP/RESTART RESOURCE WORD      */ 00589000
*/*                                                                  */ 00590000
*/********************************************************************/ 00591000
*                                                                  0207 00592000
*SWAPWORD:                                                         0207 00593000
*   PROC OPTIONS(SAVE,SAVEAREA);                                   0207 00594000
SWAPWORD STM   @14,@12,12(@13)                                     0207 00595000
         ST    @13,@SA00004+4                                      0207 00596000
         LA    @12,@SA00004                                        0207 00597000
         ST    @12,8(,@13)                                         0207 00598000
         LR    @13,@12                                             0207 00599000
*   R3=(LCCACPUA*65536)+            /* PLACE CPU LOGICAL ADDR,     0208 00600000
*                                      LCCACPUA, IN HIGH ORDER 2   0208 00601000
*                                      BYTES OF R3 BY MULTIPLYING IT    00602000
*                                      WITH 1 GREATER THAN THE MAX 0208 00603000
*                                      NUMBER THAT CAN BE CONTAINED     00604000
*                                      IN THE LOW ORDER TWO BYTES    */ 00605000
*       QS;                         /* EBCDIC QS FOR QUIESCE ID      */ 00606000
         L     @12,LCCAPTR                                         0208 00607000
         LH    R3,LCCACPUA(,@12)                                   0208 00608000
         SLA   R3,16                                               0208 00609000
         LH    @09,QS                                              0208 00610000
         N     @09,@CF02455                                        0208 00611000
         ALR   R3,@09                                              0208 00612000
*   EXFLIHFG=ON;                    /* LCCARSTR ON FOOT PRINT        */ 00613000
         OI    EXFLIHFG,B'00000100'                                0209 00614000
*   LCCARSTR=ON;                    /* WARN FLIH OF CVTRSTWD SPIN    */ 00615000
         OI    LCCARSTR(@12),B'00001000'                           0210 00616000
*/* ---------------  SWAP LOOP FOR CVTRSTWD  ----------------------- */ 00617000
*                                                                  0211 00618000
*SWAPLOOP:                                                         0211 00619000
*   R2=NULL;                        /* CVTRSTWD MUST BE ZERO FOR SWAP*/ 00620000
SWAPLOOP SLR   R2,R2                                               0211 00621000
*   CS(R2,R3,CVTRSTWD);             /* COMPARE AND SWAP              */ 00622000
*                                                                  0212 00623000
         L     @12,CVTPTR                                          0212 00624000
         CS    R2,@03,CVTRSTWD(@12)                                0212 00625000
*   /*****************************************************************/ 00626000
*   /*                                                               */ 00627000
*   /* TEST CONDITION CODE FOR EQUAL ON SWAP                         */ 00628000
*   /*                                                               */ 00629000
*   /*****************************************************************/ 00630000
*                                                                  0213 00631000
*   BC(8,GOODSWAP);                 /* ------ NORMAL LOOP EXIT ----- */ 00632000
*                                                                  0213 00633000
         BC    8,GOODSWAP                                          0213 00634000
*   /*****************************************************************/ 00635000
*   /*                                                               */ 00636000
*   /* ALLOW ACR TO PROCESS EMERGENCY SIGNAL OR MALFUNCTION ALERT    */ 00637000
*   /*                                                               */ 00638000
*   /*****************************************************************/ 00639000
*                                                                  0214 00640000
*   GENERATE SETS(R0,R14,R15,R1,WNDWSAV,PSACR0);                   0214 00641000
*                                                                  0214 00642000
   WINDOW WNDWSAV                                                       00643000
*   /*****************************************************************/ 00644000
*   /*                                                               */ 00645000
*   /* REPEAT LOOP UNTIL EMS/MFA OR GOOD SWAP OCCURS                 */ 00646000
*   /*                                                               */ 00647000
*   /*****************************************************************/ 00648000
*                                                                  0215 00649000
*   IF LCCAACR=OFF THEN                                            0215 00650000
         L     @12,LCCAPTR                                         0215 00651000
         TM    LCCAACR(@12),B'10000000'                            0215 00652000
         BZ    @RT00215                                            0215 00653000
*     GOTO SWAPLOOP;                                               0216 00654000
*                                                                  0216 00655000
*/* ---------------- END OF CVTRSTWD SWAP LOOP ----------------------*/ 00656000
*                                                                  0217 00657000
*   /*****************************************************************/ 00658000
*   /*                                                               */ 00659000
*   /* RECEIVE CONTROL WHEN MFA/EMS IS ISSUED BY ANOTHER CPU         */ 00660000
*   /*                                                               */ 00661000
*   /*****************************************************************/ 00662000
*                                                                  0217 00663000
*   R1=PSALCCAV;                    /* LCCA VIRTUAL ADDR             */ 00664000
*                                                                  0217 00665000
         L     R1,PSALCCAV                                         0217 00666000
*   /*****************************************************************/ 00667000
*   /*                                                               */ 00668000
*   /* ALLOW MACHINE CHECK ERROR RECOVERY PROCESSING                 */ 00669000
*   /*                                                               */ 00670000
*   /*****************************************************************/ 00671000
*                                                                  0218 00672000
*   GENERATE SETS(R0,R1,R14,R15,CVTBTERM,CVTMAP,LCCACPUS,LCCA,WSACACR,  00673000
*       WSAC);                                                     0218 00674000
   CALLRTM TYPE=ACRLM,CCA=(1)                                           00675000
*   MSGID=SEMI;                     /* SYSTEM ERROR MSG              */ 00676000
         MVC   MSGID(2),@CH01001                                   0219 00677000
*   RETURN CODE(BADRC);             /* ABNORMAL EXIT --------------- */ 00678000
*                                                                  0220 00679000
         LA    @15,4                                               0220 00680000
         L     @13,4(,@13)                                         0220 00681000
         L     @14,12(,@13)                                        0220 00682000
         LM    @00,@12,20(@13)                                     0220 00683000
         BR    @14                                                 0220 00684000
*   /*****************************************************************/ 00685000
*   /*                                                               */ 00686000
*   /* RECEIVE CONTROL AFTER SUCCESSFUL SWAP                         */ 00687000
*   /*                                                               */ 00688000
*   /*****************************************************************/ 00689000
*                                                                  0221 00690000
*GOODSWAP:                                                         0221 00691000
*   RSTWD=ON;                       /* CVTRSTWD HELD FOOT PRINT      */ 00692000
GOODSWAP OI    RSTWD,B'00000010'                                   0221 00693000
*   LCCARSTR=OFF;                   /* CVTRSTWD SPIN BIT             */ 00694000
         L     @12,LCCAPTR                                         0222 00695000
         NI    LCCARSTR(@12),B'11110111'                           0222 00696000
*   EXFLIHFG=OFF;                   /* FOOT PRINT INDICATING SPIN BIT   00697000
*                                      IS ON                         */ 00698000
         NI    EXFLIHFG,B'11111011'                                0223 00699000
*   RETURN CODE(GOODRC);            /* NORMAL EXIT -----------       */ 00700000
         SLR   @15,@15                                             0224 00701000
         L     @13,4(,@13)                                         0224 00702000
         L     @14,12(,@13)                                        0224 00703000
         LM    @00,@12,20(@13)                                     0224 00704000
         BR    @14                                                 0224 00705000
*   END;                            /* END SWAPWORD PROC             */ 00706000
