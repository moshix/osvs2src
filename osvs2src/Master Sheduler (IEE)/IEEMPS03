*/*                                                                  */ 00709000
*/* STPRSTRT SUBROUTINE                                              */ 00710000
*/*                                                                  */ 00711000
*/* FUNCTION: - PROVIDE THE INTERFACE TO THE STOP/RESTART SUBROUTINE */ 00712000
*/*                                                                  */ 00713000
*/********************************************************************/ 00714000
*                                                                  0226 00715000
*STPRSTRT:                                                         0226 00716000
*   PROC OPTIONS(SAVE,SAVEAREA);                                   0226 00717000
STPRSTRT STM   @14,@12,12(@13)                                     0226 00718000
         ST    @13,@SA00005+4                                      0226 00719000
         LA    @12,@SA00005                                        0226 00720000
         ST    @12,8(,@13)                                         0226 00721000
         LR    @13,@12                                             0226 00722000
*   R0='CCC'X;                      /* WAIT STATE CODE FOR         0227 00723000
*                                      STOP-RESTART SUBROUTINE       */ 00724000
         LA    R0,3276                                             0227 00725000
*   R1=SRWAAD;                      /* ADDR OF WORK AREA             */ 00726000
         L     R1,SRWAAD                                           0228 00727000
*   STPRS=ON;                       /* FOOT PRINT INDICATING THAT  0229 00728000
*                                      IEESTPRS WAS CALLED           */ 00729000
*                                                                  0229 00730000
         OI    STPRS,B'00000001'                                   0229 00731000
*   /*****************************************************************/ 00732000
*   /*                                                               */ 00733000
*   /* PLACE CPU'S IN MANUAL STATE - RETURN AFTER RESTART            */ 00734000
*   /*                                                               */ 00735000
*   /*****************************************************************/ 00736000
*                                                                  0230 00737000
*   CALL IEESTPRS;                                                 0230 00738000
*                                                                  0230 00739000
         L     @12,CVTPTR                                          0230 00740000
         L     @15,CVTSTPRS(,@12)                                  0230 00741000
         BALR  @14,@15                                             0230 00742000
*   /*****************************************************************/ 00743000
*   /*                                                               */ 00744000
*   /* DETERMINE WHETHER IEESTPRS WAS SUCCESSFUL                     */ 00745000
*   /*                                                               */ 00746000
*   /*****************************************************************/ 00747000
*                                                                  0231 00748000
*   IF R15=GOODRC THEN                                             0231 00749000
         SLR   @12,@12                                             0231 00750000
         CR    R15,@12                                             0231 00751000
         BNE   @RF00231                                            0231 00752000
*     DO;                                                          0232 00753000
*       MSGID=QSMI;                 /* QUIESCE SUCCESSFUL MSG        */ 00754000
         MVC   MSGID(2),@CH00078                                   0233 00755000
*       RETURN CODE(GOODRC);        /* NORMAL EXIT ----------        */ 00756000
         LR    @15,@12                                             0234 00757000
         L     @13,4(,@13)                                         0234 00758000
         L     @14,12(,@13)                                        0234 00759000
         LM    @00,@12,20(@13)                                     0234 00760000
         BR    @14                                                 0234 00761000
*     END;                                                         0235 00762000
*   STPRSRC=R15;                    /* SAVE IEESTPRS RETURN CODE     */ 00763000
@RF00231 ST    R15,STPRSRC                                         0236 00764000
*   MSGID=QUMI;                     /* QUIESCE UNSUCCESSFUL MSG      */ 00765000
         MVC   MSGID(2),@CH00284                                   0237 00766000
*   RETURN CODE(BADRC);             /* ABNORMAL EXIT ----------      */ 00767000
         LA    @15,4                                               0238 00768000
         L     @13,4(,@13)                                         0238 00769000
         L     @14,12(,@13)                                        0238 00770000
         LM    @00,@12,20(@13)                                     0238 00771000
         BR    @14                                                 0238 00772000
*   END;                            /* END STPRSTRT PROC             */ 00773000
*                                                                  0240 00774000
*/********************************************************************/ 00775000
*/*                                                                  */ 00776000
*/* CLEANUP PROCEDURE                                                */ 00777000
*/*                                                                  */ 00778000
*/* FUNCTION: - FREE SYSTEM RESOURCES ALLOCATED TO IEEMPS03          */ 00779000
*/*           - CALL WTOPROC TO ISSUE A STATUS MSG TO THE OPERATOR   */ 00780000
*/*                                                                  */ 00781000
*/********************************************************************/ 00782000
*                                                                  0240 00783000
*CLEANUP:                                                          0240 00784000
*   PROC OPTIONS(SAVE,SAVEAREA);                                   0240 00785000
