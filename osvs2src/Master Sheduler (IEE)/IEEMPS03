         ST    @12,ENTRYPT                                         0086 00038000
*   MSGID=SEMI;                     /* ESTABLISH MSG DEFAULT         */ 00039000
*                                                                  0087 00040000
         MVC   MSGID(2),@CH01001                                   0087 00041000
*   /*****************************************************************/ 00042000
*   /*                                                               */ 00043000
*   /* TEST FOR AUTHORIZED SOURCE OF COMMAND                         */ 00044000
*   /*                                                               */ 00045000
*   /*****************************************************************/ 00046000
*                                                                  0088 00047000
*   IF CHUCMP=UCMID|                /* MASTER CONSOLE                */ 00048000
*       ((CHUCMP=RDR)&(AUTHFLG=ON)) /* AUTHORIZED RDR                */ 00049000
*     THEN                                                         0088 00050000
         L     @12,CSCBPTR                                         0088 00051000
         L     @09,CVTPTR                                          0088 00052000
         L     @09,UCMBASPT(,@09)                                  0088 00053000
         AL    @09,@CF02450                                        0088 00054000
         L     @09,UCMMCSPT(,@09)                                  0088 00055000
         L     @09,UCMDVEPT(,@09)                                  0088 00056000
         CLC   CHUCMP(1,@12),UCMID(@09)                            0088 00057000
         BE    @RT00088                                            0088 00058000
         CLI   CHUCMP(@12),X'80'                                   0088 00059000
         BNE   @RF00088                                            0088 00060000
         TM    AUTHFLG(@12),B'10000000'                            0088 00061000
         BNO   @RF00088                                            0088 00062000
@RT00088 DS    0H                                                  0089 00063000
*     CONSLID=UCMID;                /* MSTR CNSL RECEIVES ALL MSG'S     00064000
*                                      FOR AUTHORIZED COMMAND SOURCES*/ 00065000
*                                                                  0089 00066000
         L     @12,CVTPTR                                          0089 00067000
         L     @12,UCMBASPT(,@12)                                  0089 00068000
         AL    @12,@CF02450                                        0089 00069000
         L     @12,UCMMCSPT(,@12)                                  0089 00070000
         L     @12,UCMDVEPT(,@12)                                  0089 00071000
         MVC   CONSLID(1),UCMID(@12)                               0089 00072000
*   /*****************************************************************/ 00073000
*   /*                                                               */ 00074000
*   /* DETERMINE MSG CONSOLE WHEN SOURCE OF COMMAND IS UNAUTHORIZED, */ 00075000
*   /* THEN ISSUE MSG AND EXIT                                       */ 00076000
*   /*                                                               */ 00077000
*   /*****************************************************************/ 00078000
*                                                                  0090 00079000
*   ELSE                                                           0090 00080000
*     DO;                                                          0090 00081000
         B     @RC00088                                            0090 00082000
@RF00088 DS    0H                                                  0091 00083000
*       IF CHUCMP=RDR|              /* UNAUTHORIZED RDR              */ 00084000
*           CHUCMP=NULL             /* INTERNAL COMMAND              */ 00085000
*         THEN                                                     0091 00086000
