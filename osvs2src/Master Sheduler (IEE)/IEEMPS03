*/* PAGEFIX SUBROUTINE                                               */ 00478000
*/*                                                                  */ 00479000
*/* FUNCTION: - FIX STORAGE TO PREVENT PAGE FAULT WHILE DISABLED     */ 00480000
*/*                                                                  */ 00481000
*/********************************************************************/ 00482000
*                                                                  0189 00483000
*PAGEFIX:                                                          0189 00484000
*   PROC OPTIONS(SAVE,SAVEAREA);                                   0189 00485000
PAGEFIX  STM   @14,@12,12(@13)                                     0189 00486000
         ST    @13,@SA00003+4                                      0189 00487000
         LA    @12,@SA00003                                        0189 00488000
         ST    @12,8(,@13)                                         0189 00489000
         LR    @13,@12                                             0189 00490000
*   RFY                                                            0190 00491000
*    (R4) RSTD;                     /* RESTRICT ADDITIONAL WORK REG  */ 00492000
*   PGFXECB=''B;                    /* ZERO ECB                      */ 00493000
         SLR   @12,@12                                             0191 00494000
         ST    @12,PGFXECB                                         0191 00495000
*   R4=ADDR(PGFXECB);               /* LOAD ADDR ECB                 */ 00496000
*                                                                  0192 00497000
         LA    R4,PGFXECB                                          0192 00498000
*   /*****************************************************************/ 00499000
*   /*                                                               */ 00500000
*   /* FIX PAGES SHORT TERM                                          */ 00501000
*   /*                                                               */ 00502000
*   /*****************************************************************/ 00503000
*                                                                  0193 00504000
*   GENERATE REFS(R2,R3,R4) SETS(PGFXECB,R14,R15,R0,R1);           0193 00505000
   PGFIX R,A=(2),EA=(3),ECB=(4),LONG=N                                  00506000
*   RFY                                                            0194 00507000
*    (R4) UNRSTD;                   /* RELEASE LOCAL WORK REG        */ 00508000
*                                                                  0194 00509000
*   /*****************************************************************/ 00510000
*   /*                                                               */ 00511000
*   /* DETERMINE IF PAGE IS FIXED                                    */ 00512000
*   /*                                                               */ 00513000
*   /*****************************************************************/ 00514000
*                                                                  0195 00515000
*   IF R15=GOODRC THEN                                             0195 00516000
         LTR   R15,R15                                             0195 00517000
         BZ    @RT00195                                            0195 00518000
*     GOTO GOODPF;                  /* NORMAL RETURN                 */ 00519000
*                                                                  0196 00520000
*   /*****************************************************************/ 00521000
*   /*                                                               */ 00522000
*   /* WAIT FOR POST IF AN ERROR WAS NOT ENCOUNTERED                 */ 00523000
*   /*                                                               */ 00524000
*   /*****************************************************************/ 00525000
*                                                                  0197 00526000
*   IF R15=8 THEN                                                  0197 00527000
         C     R15,@CF00065                                        0197 00528000
         BNE   @RF00197                                            0197 00529000
*     DO;                                                          0198 00530000
*                                                                  0198 00531000
*       /*************************************************************/ 00532000
*       /*                                                           */ 00533000
*       /* WAIT FOR POSTING OF ECB                                   */ 00534000
*       /*                                                           */ 00535000
*       /*************************************************************/ 00536000
*                                                                  0199 00537000
*       GENERATE REFS(PGFXECB) SETS(R0,R1,R14,R15);                0199 00538000
*                                                                  0199 00539000
          WAIT ECB=PGFXECB         /* WAIT FOR POSTING OF ECB       */  00540000
*       /*************************************************************/ 00541000
*       /*                                                           */ 00542000
*       /* DETERMINE WHETHER FIX WAS POSTED                          */ 00543000
*       /*                                                           */ 00544000
*       /*************************************************************/ 00545000
*                                                                  0200 00546000
*       IF PGFXCODE=GOODRC THEN                                    0200 00547000
         L     @12,PGFXCODE-1                                      0200 00548000
         LA    @12,0(,@12)                                         0200 00549000
         LTR   @12,@12                                             0200 00550000
         BZ    @RT00200                                            0200 00551000
*         GOTO GOODPF;              /* NORMAL RETURN                 */ 00552000
*     END;                                                         0202 00553000
*   BADCODE=R15;                    /* SAVE UNEXPECTED RETURN CODE   */ 00554000
*                                                                  0203 00555000
@RF00197 ST    R15,BADCODE                                         0203 00556000
*   /*****************************************************************/ 00557000
*   /*                                                               */ 00558000
*   /* PAGE NOT FIXED - TERMINATE PROCESSING ------------            */ 00559000
*   /*                                                               */ 00560000
*   /*****************************************************************/ 00561000
*                                                                  0204 00562000
*   RETURN CODE(BADRC);                                            0204 00563000
*                                                                  0204 00564000
         LA    @15,4                                               0204 00565000
         L     @13,4(,@13)                                         0204 00566000
