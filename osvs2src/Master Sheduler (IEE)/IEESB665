*     END;                                                         0149 00423000
*   ELSE                                                           0150 00424000
*     IF CHCLS=ZERO THEN                                           0150 00425000
         B     @RC00144                                            0150 00426000
@RF00144 L     @12,SAVEASCB(,STAEPTR)                              0150 00427000
         L     @12,ASCBCSCB(,@12)                                  0150 00428000
         CLC   CHCLS(8,@12),ZERO                                   0150 00429000
         BNE   @RF00150                                            0150 00430000
*       DO;                         /* IF NO NAME IN CSCB            */ 00431000
*         STCNAME=STARTED;          /* PUT STC NAME IN MESSAGE       */ 00432000
