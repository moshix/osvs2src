*   IF ASCBCSCB=0 THEN              /* IF NO CSCB USE STC NAME       */ 00410000
         L     @06,SAVEASCB(,STAEPTR)                              0144 00411000
         ICM   @05,15,ASCBCSCB(@06)                                0144 00412000
         BNZ   @RF00144                                            0144 00413000
*     DO;                                                          0145 00414000
*       STCNAME=STARTED;            /* PUT STC NAME IN MESSAGE       */ 00415000
