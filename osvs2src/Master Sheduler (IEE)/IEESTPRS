         TITLE 'IEESTPRS--STOP/RESTART SUBROUTINE                      *00001000
                        '                                               00002000
IEESTPRS CSECT                                                          00003000
         USING *,15                                                     00004000
IEESTPRS MODID BR=YES                                                   00005000
         L     REG15,16           LOAD R15 WITH ADDR OF CVT             00006000
         L     REG15,CVTSPSA(REG15)    LOAD R15 WITH ADDR OF GLOBAL WA  00007000
*                                       VECTOR TABLE                    00008000
         L     REG15,WSAGREST(REG15)   LOAD R15 WITH ADDR OF MY         00009000
*                                       GLOBAL WORK AREA                00010000
         STM   REG0,REG14,0(REG15)  STORE CALLERS REGS                  00011000
*                                       IN MY GLOBAL WORK AREA          00012000
MAINCODE BALR  BASEREG,0            ESTABLISH                           00013000
         DROP 15                                                        00014000
         USING *,BASEREG               ADDRESSIBILITY (R12)             00015000
*   GLOSAPTR=REG15;                 /* LOAD R11 WITH ADDR OF MY    0052 00016000
*                                      GLOBAL SA                     */ 00017000
         LR    GLOSAPTR,REG15                                      0052 00018000
*   RFY                                                            0053 00019000
*     REG15 UNRSTD;                 /* FREE R15 FOR USE              */ 00020000
*   RETREG=0;                       /* STORE RETURN CODE 0 IN THE  0054 00021000
*                                      CALLER'S R15, NOW SAVED IN  0054 00022000
*                                      GLOBALSA              @ZD08005*/ 00023000
*                                                                  0054 00024000
         SLR   @10,@10                                             0054 00025000
         ST    @10,RETREG(,GLOSAPTR)                               0054 00026000
*   /*****************************************************************/ 00027000
*   /*                                                               */ 00028000
*   /* TEST THE CALLER'S R1 TO DETERMINE IF IT CONTAINS A PTR TO A   */ 00029000
*   /* STATUS SAVE AREA. IF THE CALLER'S R1 (STATSAVE) IS ZERO, NO   */ 00030000
*   /* STATUS IS SAVED                                               */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                                  0055 00034000
*   IF STATSAVE^=0 THEN             /* IF CALLER'S R1 NOT = ZERO     */ 00035000
         L     @07,STATSAVE(,GLOSAPTR)                             0055 00036000
