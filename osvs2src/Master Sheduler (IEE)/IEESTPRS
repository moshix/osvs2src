*                                                                  0400 01647000
*   IF REG15^=0 THEN                                               0400 01648000
         LTR   REG15,REG15                                         0400 01649000
         BZ    @RF00400                                            0400 01650000
*     DO;                                                          0401 01651000
*       FLCRNPSW=SAVRESTN;          /* RESTORE ORIGINAL RESTART NEW     01652000
*                                      PSW                           */ 01653000
         L     @14,LCCACPUS(,LCCAPTR)                              0402 01654000
         L     @14,WSACREST(,@14)                                  0402 01655000
         MVC   FLCRNPSW(8,PSAPTR),SAVRESTN(@14)                    0402 01656000
*       RETCODE=12;                 /* SET RETURN CODE 12            */ 01657000
         MVI   RETCODE(GLOSAPTR),X'0C'                             0403 01658000
*       GOTO RTNSETUP;              /* GO SET UP FOR RETURN TO     0404 01659000
*                                      MAINLINE                      */ 01660000
         B     RTNSETUP                                            0404 01661000
*     END;                                                         0405 01662000
*   ELSE                                                           0406 01663000
*     ;                             /* NULL ELSE                     */ 01664000
@RF00400 DS    0H                                                  0407 01665000
*                                                                  0407 01666000
*   /*****************************************************************/ 01667000
*   /*                                                               */ 01668000
*   /* THE FOLLOWING SIGP START SERVES ONLY ONE PURPOSE. IT WILL     */ 01669000
*   /* ASSURE ME THAT THE PREVIOUS SIGP RESTART HAS COMPLETED AND IT */ 01670000
*   /* IS NOW SAFE FOR ME TO CHANGE THE RESTART NEW PSW. IPC WILL    */ 01671000
*   /* RECEIVE CONDITION CODE 2 (BUSY) UNTIL THE PENDING SIGP RESTART*/ 01672000
*   /* HAS BEEN EXECUTED.                                            */ 01673000
*   /*                                                               */ 01674000
*   /*****************************************************************/ 01675000
*                                                                  0407 01676000
*   REG1=PCCAPTR;                   /* ADDRESS OF PCCA FOR IPC       */ 01677000
         LR    REG1,PCCAPTR                                        0407 01678000
*   GEN(DSGNL START,CPU=(1));       /* USED TO DETERMINE IF SIGP   0408 01679000
*                                      RESTART HAS COMPLETED.        */ 01680000
*                                                                  0408 01681000
         DSGNL START,CPU=(1)                                            01682000
*   /*****************************************************************/ 01683000
*   /*                                                               */ 01684000
*   /* IF NOT RETURN CODE ZERO, SET UP TO BACK OUT                   */ 01685000
*   /*                                                               */ 01686000
*   /*****************************************************************/ 01687000
*                                                                  0409 01688000
*   IF REG15^=0 THEN                                               0409 01689000
         LTR   REG15,REG15                                         0409 01690000
         BZ    @RF00409                                            0409 01691000
*     DO;                           /* DO IF                         */ 01692000
*       RETCODE=12;                 /* UNEXPECTED RETURN CODE FROM 0411 01693000
*                                      IPC                           */ 01694000
         MVI   RETCODE(GLOSAPTR),X'0C'                             0411 01695000
*       GOTO RTNSETUP;              /* GO SETUP FOR RETURN TO      0412 01696000
*                                      MAINLINE                      */ 01697000
         B     RTNSETUP                                            0412 01698000
*     END;                                                         0413 01699000
*                                                                  0413 01700000
*   /*****************************************************************/ 01701000
*   /*                                                               */ 01702000
*   /* DETERMINE IF THE CALLER'S R1 IS ZERO. IF NON-ZERO WE MUST SAVE*/ 01703000
*   /* TOTAL STATUS AWAY                                             */ 01704000
*   /*                                                               */ 01705000
*   /*****************************************************************/ 01706000
*                                                                  0414 01707000
*   IF STATSAVE=0 THEN                                             0414 01708000
@RF00409 SLR   @14,@14                                             0414 01709000
         C     @14,STATSAVE(,GLOSAPTR)                             0414 01710000
         BNE   @RF00414                                            0414 01711000
*     DO;                                                          0415 01712000
*       PSASRSA=0;                  /* CLEAR STATUS SA PTR IN PSA. 0416 01713000
*                                      THIS PTR IS TESTED FOR ZERO BY   01714000
*                                      MY FLIH                       */ 01715000
         ST    @14,PSASRSA(,PSAPTR)                                0416 01716000
*     END;                                                         0417 01717000
*   ELSE                                                           0418 01718000
*     DO;                                                          0418 01719000
*                                                                  0418 01720000
         B     @RC00414                                            0418 01721000
@RF00414 DS    0H                                                  0419 01722000
*       /*************************************************************/ 01723000
*       /*                                                           */ 01724000
*       /* STATUS SA POINTER IS NON-ZERO, SAVE TOTAL STATUS AWAY     */ 01725000
*       /*                                                           */ 01726000
*       /*************************************************************/ 01727000
*                                                                  0419 01728000
*       REG1=PCCAPTR;               /* ADDRESS OF CURRCPUA'S PCCA FOR   01729000
*                                      SIGP                          */ 01730000
         LR    REG1,PCCAPTR                                        0419 01731000
*       GEN(DSGNL STOP,CPU=(1));    /* ISSUE STOP AND IGNORE RETURN     01732000
*                                      CODE                          */ 01733000
         DSGNL STOP,CPU=(1)                                             01734000
*       RFY                                                        0421 01735000
*         WORKREG6 RSTD;            /* RESTRICT R6                   */ 01736000
*       GEN(LRA WORKREG6,0(STATUSAD));/* LOAD REAL ADDR OF STATUS SA    01737000
*                                      FROM R3 INTO R6               */ 01738000
         LRA WORKREG6,0(STATUSAD)                                       01739000
*       PSASRSA=WORKREG6;           /* STORE REAL ADDR OF STATUS SA     01740000
*                                      INTO PSA TO BE USED FOR     0423 01741000
*                                      RESTORING STATUS BY THE     0423 01742000
*                                      RESTART FLIH                  */ 01743000
         ST    WORKREG6,PSASRSA(,PSAPTR)                           0423 01744000
*       RFY                                                        0424 01745000
*         WORKREG6 UNRSTD;          /* RELEASE R6 FOR USE            */ 01746000
*                                                                  0424 01747000
*       /*************************************************************/ 01748000
*       /*                                                           */ 01749000
*       /* THE FOLLOWING INFORMATION WILL BE SAVED IN THE STATUS SA  */ 01750000
*       /* FROM ABSOLUTE LO CORE                                     */ 01751000
*       /*                                                           */ 01752000
*       /*************************************************************/ 01753000
*                                                                  0425 01754000
*       REG15=0;                    /* ZERO VALUE            @ZD03005*/ 01755000
         SLR   REG15,REG15                                         0425 01756000
*       PCCAPTR=REG15->PSAPCCAV;    /* ADDRESSABILITY TO MY PCCA   0426 01757000
*                                                            @ZD03005*/ 01758000
         L     PCCAPTR,PSAPCCAV(,REG15)                            0426 01759000
*       PSAPTR=PCCAPSAV;            /* BASE PSA MAPPING ON THE     0427 01760000
*                                      VIRTUAL ADDRESS OF MY PSA FOR    01761000
*                                      REVERSE PREFIXING.            */ 01762000
         L     PSAPTR,PCCAPSAV(,PCCAPTR)                           0427 01763000
*       CPUTIMER=TIMERVAL;          /* SAVE CPU TIMER                */ 01764000
         MVC   CPUTIMER(8,STATUSAD),TIMERVAL(PSAPTR)               0428 01765000
*       CLKCOMP=CMPRATOR;           /* SAVE CLOCK COMPARATOR         */ 01766000
         MVC   CLKCOMP(8,STATUSAD),CMPRATOR(PSAPTR)                0429 01767000
*       FLPTREGS=FPREGS;            /* SAVE FLOATING POINT REGS      */ 01768000
         MVC   FLPTREGS(32,STATUSAD),FPREGS(PSAPTR)                0430 01769000
*       CONTREGS=CREGS;             /* SAVE CONTROL REGISTERS        */ 01770000
*                                                                  0431 01771000
         MVC   CONTREGS(64,STATUSAD),CREGS(PSAPTR)                 0431 01772000
*       /*************************************************************/ 01773000
*       /*                                                           */ 01774000
*       /* RE-ESTABLISH ADDRESSABILITY FOR THE CURRENT CPU BEING     */ 01775000
*       /* PROCESSED                                                 */ 01776000
*       /*                                                           */ 01777000
*       /*************************************************************/ 01778000
*                                                                  0432 01779000
*       PCCAPTR=WORKREG5;           /* ESTABLISH PCCA MAPPING FOR  0432 01780000
*                                      CURRCPUA                      */ 01781000
         LR    PCCAPTR,WORKREG5                                    0432 01782000
*       PSAPTR=PCCAPSAV;            /* ESTABLISH PSA MAPPING FOR   0433 01783000
*                                      CURRCPUA                      */ 01784000
*                                                                  0433 01785000
         L     PSAPTR,PCCAPSAV(,PCCAPTR)                           0433 01786000
*       /*************************************************************/ 01787000
*       /*                                                           */ 01788000
*       /* THE FOLLOWING INFORMATION WILL BE SAVED IN THE STATUS SA  */ 01789000
*       /* FROM THE CURRENT CPU'S PSA.                               */ 01790000
*       /*                                                           */ 01791000
*       /*************************************************************/ 01792000
*                                                                  0434 01793000
*       LOWSAVE=LOWCORE;            /* SAVE LOW CORE INFO    @ZD03005*/ 01794000
         MVC   LOWSAVE(24,STATUSAD),LOWCORE(PSAPTR)                0434 01795000
*       NEWPSWS=MACHNEW;            /* MCK NEW, SVC NEW, PROG CK NEW */ 01796000
         MVC   NEWPSWS(24,STATUSAD),MACHNEW(PSAPTR)                0435 01797000
*       OLDPSWS=MACHOLD;            /* MCK OLD, SVC OLD, PROG CK OLD */ 01798000
         MVC   OLDPSWS(24,STATUSAD),MACHOLD(PSAPTR)                0436 01799000
*       FLCSNPSW=FLCRNPSW;          /* SET SVC NEW POINTING TO MY  0437 01800000
*                                      FLIH                          */ 01801000
         MVC   FLCSNPSW(8,PSAPTR),FLCRNPSW(PSAPTR)                 0437 01802000
*       FLCPNPSW=FLCRNPSW;          /* SET PROG CK NEW POINTING TO MY   01803000
*                                      FLIH                          */ 01804000
         MVC   FLCPNPSW(8,PSAPTR),FLCRNPSW(PSAPTR)                 0438 01805000
*       FLCMNPSW=FLCRNPSW;          /* SET MACHINE CK NEW POINTING TO   01806000
*                                      MY FLIH                       */ 01807000
         MVC   FLCMNPSW(8,PSAPTR),FLCRNPSW(PSAPTR)                 0439 01808000
*       STATUSAD=STATUSAD+208;      /* UPDATE THE STATUS SA POINTER     01809000
*                                      TO POINT TO THE NEXT CPU'S  0440 01810000
*                                      SAVE AREA                     */ 01811000
         AH    STATUSAD,@CH02353                                   0440 01812000
*     END;                                                         0441 01813000
*   RSTRMPS='000C0000'X;            /* ENABLED FOR MACHINE CHECKS  0442 01814000
*                                                            @ZD03005*/ 01815000
@RC00414 MVC   RSTRMPS(4,PSAPTR),@CB02295                          0442 01816000
*   RSTRTIC=ADDR(RESTFLIH);         /* IC POINT TO RESTART FLIH    0443 01817000
*                                                            @ZD03005*/ 01818000
         LA    @14,RESTFLIH                                        0443 01819000
         ST    @14,RSTRTIC(,PSAPTR)                                0443 01820000
*   RESTMASK=(RESTMASK|PCCACAFM);   /* SET FLAG FOR THIS CPU IN MASK */ 01821000
         OC    RESTMASK(2,GLOSAPTR),PCCACAFM(PCCAPTR)              0444 01822000
*   RFY                                                            0445 01823000
*     REG14 RSTD;                   /* RESTRICT R14                  */ 01824000
*RTNSETUP:                                                         0446 01825000
*   REG14=REG14SAV;                 /* LOAD R14 WITH RETURN ADDRESS     01826000
*                                      TO MAINLINE                   */ 01827000
RTNSETUP L     REG14,REG14SAV(,GLOSAPTR)                           0446 01828000
*   RFY                                                            0447 01829000
*     REG14 UNRSTD;                 /* RELEASE R14 FOR USE           */ 01830000
*   RETURN;                         /* RETURN TO MAINLINE            */ 01831000
@EL00002 DS    0H                                                  0448 01832000
@EF00002 DS    0H                                                  0448 01833000
@ER00002 BR    @14                                                 0448 01834000
*   END STOPSTOR;                   /* END STOPSTOR INTERNAL PROC    */ 01835000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 01836000
*     GPR00P PTR(31) REG(0),                                       0450 01837000
*     GPR01P PTR(31) REG(1),                                       0450 01838000
*     GPR14P PTR(31) REG(14),                                      0450 01839000
