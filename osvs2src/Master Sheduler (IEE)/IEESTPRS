*/*** START OF RESTFLIH **********************************************/ 00417000
*/*                                                                  */ 00418000
*/*  THE FIRST CPU THROUGH THIS ROUTINE WILL ISSUE RESTART TO ALL OF */ 00419000
*/*  THE OTHER ALIVE CPU'S. EACH OF THEM WILL RUN A PORTION OF THE   */ 00420000
*/*  INTERRUPT HANDLER AS THEY ARE RESTARTED. FIELDS ARE MODIFIED TO */ 00421000
*/*  PRESERVE JOB STEP TIMING, SAVED STATUS IS RESTORED AND CONTROL  */ 00422000
*/*  IS GIVEN BACK TO THE PROGRAMS INTERRUPTED BY THE STOP ON EACH   */ 00423000
*/*  OF THE CPU'S. THIS FLIH IS ENTERED WITH DAT OFF.                */ 00424000
*/*                                                                  */ 00425000
*/********************************************************************/ 00426000
*                                                                  0135 00427000
*RESTFLIH:                                                         0135 00428000
*   GENERATE;                                                      0135 00429000
RESTFLIH DS    0H                                                  0135 00430000
         BALR  REG15,0                  ESTABLISH                       00431000
         USING *,REG15                     ADDRESSABILITY               00432000
         B     *+6                                                      00433000
         DC    AL2(RESTFLIH-MAINCODE)      ADJUST LOC COUNTER           00434000
         SH    REG15,*-2                                                00435000
         DROP  REG15                                                    00436000
         LR    BASEREG,REG15                                            00437000
*   PSAPTR=0;                       /* BASE PSA ON ZERO TO ALLOW EACH   00438000
*                                      CPU TO ADDRESS ITS OWN PSA    */ 00439000
*                                                                  0136 00440000
         SLR   PSAPTR,PSAPTR                                       0136 00441000
*   /*****************************************************************/ 00442000
*   /*                                                               */ 00443000
*   /* DETERMINE IF STATUS WAS SAVED BY TESTING THE PSASRSA FIELD. IF*/ 00444000
*   /* NON ZERO IT CONTAINS THE REAL ADDRESS OF THE STATUS SA.       */ 00445000
*   /*                                                               */ 00446000
*   /*****************************************************************/ 00447000
*                                                                  0137 00448000
*   IF PSASRSA^=0 THEN                                             0137 00449000
         ICM   @10,15,PSASRSA(PSAPTR)                              0137 00450000
         BZ    @RF00137                                            0137 00451000
*RESTORE:                                                          0138 00452000
*     DO;                           /* IF STATUS WAS SAVED THEN    0138 00453000
*                                      RESTORE IT                    */ 00454000
RESTORE  DS    0H                                                  0139 00455000
*       STATUSAD=PSASRSA;           /* ESTABLISH ADDRESSABILITY TO 0139 00456000
*                                      STATUS SA                     */ 00457000
         L     STATUSAD,PSASRSA(,PSAPTR)                           0139 00458000
*       GEN(LCTL 0,15,CONTREGS(STATUSAD));/* RESTORE CONTROL REGS    */ 00459000
         LCTL 0,15,CONTREGS(STATUSAD)                                   00460000
*       LOWCORE=LOWSAVE;            /* RESTORE LOW CORE      @ZD03005*/ 00461000
         MVC   LOWCORE(24,PSAPTR),LOWSAVE(STATUSAD)                0141 00462000
*       MACHNEW=NEWPSWS;            /* RESTORE MCK NEW, SVC NEW, PROG   00463000
*                                      CK NEW                        */ 00464000
         MVC   MACHNEW(24,PSAPTR),NEWPSWS(STATUSAD)                0142 00465000
*       MACHOLD=OLDPSWS;            /* RESTORE MCK OLD, SVC OLD, PROG   00466000
*                                      CK OLD                        */ 00467000
         MVC   MACHOLD(24,PSAPTR),OLDPSWS(STATUSAD)                0143 00468000
*       GENERATE;                                                  0144 00469000
         LD    0,FLPTREG0(STATUSAD)      RESTORE FLOATING POINT REG0    00470000
         LD    2,FLPTREG2(STATUSAD)      RESTORE FLOATING POINT REG2    00471000
         LD    4,FLPTREG4(STATUSAD)      RESTORE FLOATING POINT REG4    00472000
         LD    6,FLPTREG6(STATUSAD)      RESTORE FLOATING POINT REG6    00473000
         SCKC  CLKCOMP(STATUSAD)         RESTORE CLOCK COMPARATOR       00474000
         SPT   CPUTIMER(STATUSAD)        RESTORE CPU TIMER              00475000
*     END RESTORE;                  /* END RESTORE DO GROUP          */ 00476000
*   ELSE                                                           0146 00477000
*     ;                             /* NULL ELSE                     */ 00478000
@RF00137 DS    0H                                                  0147 00479000
*   GEN(STOSM RSTRMSK,X'04');       /* (BIT 5 OF SYSTEM MASK) TURN 0147 00480000
*                                      DAT ON                        */ 00481000
         STOSM RSTRMSK,X'04'                                            00482000
*   GLOSAPTR=WSAGREST;              /* LOAD R11 WITH ADDR OF MY    0148 00483000
*                                      GLOBAL SA                     */ 00484000
         L     @09,CVTPTR                                          0148 00485000
         L     @09,CVTSPSA(,@09)                                   0148 00486000
         L     GLOSAPTR,WSAGREST(,@09)                             0148 00487000
*   LCCAPTR=PSALCCAV;               /* ESTABLISH ADDRESSABILITY TO 0149 00488000
*                                      LCCA                          */ 00489000
         L     LCCAPTR,PSALCCAV(,PSAPTR)                           0149 00490000
*   PCCAPTR=PSAPCCAV;               /* ESTABLISH ADDRESSABILITY TO 0150 00491000
*                                      PCCA                          */ 00492000
         L     PCCAPTR,PSAPCCAV(,PSAPTR)                           0150 00493000
*   FLCRNPSW=SAVRESTN;              /* RESTORE RESTART NEW PSW       */ 00494000
*                                                                  0151 00495000
         L     @10,LCCACPUS(,LCCAPTR)                              0151 00496000
         L     @10,WSACREST(,@10)                                  0151 00497000
         MVC   FLCRNPSW(8,PSAPTR),SAVRESTN(@10)                    0151 00498000
*   /*****************************************************************/ 00499000
*   /*                                                               */ 00500000
*   /* RESTART DONE IF ON INDICATES THERE ARE NO REMAINING CPUS TO   */ 00501000
*   /* RESTART THE FLIH. THE FIRST ONE THRU WILL RESTART THE OTHERS, */ 00502000
*   /* OR ELSE THEY WERE RESTARTED IN THE ERRCLNUP SEGMENT IF IT WAS */ 00503000
*   /* ENTERED.                                                      */ 00504000
*   /*                                                               */ 00505000
*   /*****************************************************************/ 00506000
*                                                                  0152 00507000
*   IF RESTDONE='1'B THEN                                          0152 00508000
         TM    RESTDONE(GLOSAPTR),B'00100000'                      0152 00509000
         BO    @RT00152                                            0152 00510000
*     GOTO STORTODA;                /* IF NOT FIRST CPU, GO STORE TOD   00511000
*                                      AFTER                         */ 00512000
*   ELSE                                                           0154 00513000
*     ;                             /* NULL ELSE                     */ 00514000
*   RESTDONE='1'B;                  /* SET SWITCH ON TO INDICATE A 0155 00515000
*                                      CPU HAS BEEN THRU AND       0155 00516000
*                                      RESTARTED THE OTHERS          */ 00517000
         OI    RESTDONE(GLOSAPTR),B'00100000'                      0155 00518000
*   RFY                                                            0156 00519000
*     REGSAPTR RSTD;                /* RESTRICT R13                  */ 00520000
*   REGSAPTR=ADDR(BEREGSA);         /* LOAD R13 WITH ADDRESS OF 18 WD   00521000
*                                      SAVE AREA FOR BRANCH ENTRY  0157 00522000
*                                      INTO SRM                      */ 00523000
         LA    REGSAPTR,BEREGSA(,GLOSAPTR)                         0157 00524000
*   GEN(SYSEVENT SYQSCCMP,ENTRY=BRANCH);/* NOTIFY SRM THAT ALL CPU'S    00525000
*                                      ARE ABOUT TO BE RESTARTED     */ 00526000
         SYSEVENT SYQSCCMP,ENTRY=BRANCH                                 00527000
*   RFY                                                            0159 00528000
*     REGSAPTR UNRSTD;              /* FREE R13 FOR USE              */ 00529000
*   SYSEV2FG='1'B;                  /* INDICATE SRM EVENT HAS BEEN 0160 00530000
*                                      ISSUED IN CASE ERRCLNUP IS  0160 00531000
*                                      ENTERED.                      */ 00532000
*                                                                  0160 00533000
         OI    SYSEV2FG(GLOSAPTR),B'00000010'                      0160 00534000
*   /*****************************************************************/ 00535000
*   /*                                                               */ 00536000
*   /* DETERMINE IF THE CLOCK WAS BAD WHEN I TRIED TO STORE TOD      */ 00537000
*   /* BEFORE. IF BAD BYPASS TIME ADJUSTMENT                         */ 00538000
*   /*                                                               */ 00539000
*   /*****************************************************************/ 00540000
*                                                                  0161 00541000
*   IF BADCLOCK='0'B THEN           /* IF CLOCK IS GOOD THEN STOR CLK*/ 00542000
         TM    BADCLOCK(GLOSAPTR),B'00010000'                      0161 00543000
         BNZ   @RF00161                                            0161 00544000
*CLOKGOOD:                          /* CLOCK IS GOOD, STORE CLOCK    */ 00545000
*     DO;                                                          0162 00546000
CLOKGOOD DS    0H                                                  0163 00547000
*       RFY                                                        0163 00548000
*         WORKREG5 RSTD;            /* RESTRICT R5                   */ 00549000
*       WORKREG5=ADDR(TODAFTER);    /* SET UP ADDRESSABILITY FOR   0164 00550000
*                                      STORE CLOCK IN CPU/REL SA     */ 00551000
         L     @10,LCCACPUS(,LCCAPTR)                              0164 00552000
         L     WORKREG5,WSACREST(,@10)                             0164 00553000
         LA    WORKREG5,TODAFTER(,WORKREG5)                        0164 00554000
*       GEN(STCK 0(WORKREG5));      /* STORE CLOCK IN TODAFTER TO BE    00555000
*                                      USED IN THE CALCULATION OF THE   00556000
*                                      PERIOD OF TIME THE CPU'S WERE    00557000
*                                      STOPPED.                      */ 00558000
         STCK 0(WORKREG5)                                               00559000
*       BC(8,ASCBLOOP);             /* BRANCH ON CC 0 TO ADJUST ASCB    00560000
*                                      WAIT TIME                     */ 00561000
         BC    8,ASCBLOOP                                          0166 00562000
*       BADCLOCK='1'B;              /* IF NOT CC 0 SET SWITCH TO   0167 00563000
*                                      INDICATE TOD CLOCK VALUE IS 0167 00564000
*                                      NOT VALID                     */ 00565000
         OI    BADCLOCK(GLOSAPTR),B'00010000'                      0167 00566000
*       GOTO RESTCPU;               /* CLOCK IS BAD, BYPASS TIME   0168 00567000
*                                      ADJUSTMENT AND GO TO RESTART     00568000
*                                      LOOP                          */ 00569000
         B     RESTCPU                                             0168 00570000
*ASCBLOOP:                          /* LOOP THROUGH ASCBS            */ 00571000
*       RFY                                                        0169 00572000
*         ASCBPTR RSTD;             /* RESTRICT R9                   */ 00573000
*                                                                  0169 00574000
ASCBLOOP DS    0H                                                  0170 00575000
*       /*************************************************************/ 00576000
*       /*                                                           */ 00577000
*       /* TEST EVERY ENTRY IN THE ASVT TO DETERMINE WHAT ASCB'S ARE */ 00578000
*       /* ACTIVE. IF THE HI ORDER BIT OF THE ENTRY IS ZERO, THEN THE*/ 00579000
*       /* ENTRY CONTAINS THE ADDRESS OF AN ACTIVE ASCB. IF THE HI   */ 00580000
*       /* ORDER BIT IS A ONE, THE ENTRY CONTAINS THE ADDRESS OF THE */ 00581000
*       /* NEXT AVAILABLE ASCB. ONLY THE ACTIVE ASCB'S WILL HAVE THE */ 00582000
*       /* WAIT TIME ADJUSTED                                        */ 00583000
*       /*                                                           */ 00584000
*       /*************************************************************/ 00585000
*                                                                  0170 00586000
*       DO TEMPWORK=1 TO ASVTMAXU;  /* ASVTMAXU-MAX. NUM. OF ADDR  0170 00587000
*                                      SPACES                        */ 00588000
*                                                                  0170 00589000
         LA    @10,1                                               0170 00590000
         B     @DE00170                                            0170 00591000
@DL00170 DS    0H                                                  0171 00592000
*         /***********************************************************/ 00593000
*         /*                                                         */ 00594000
*         /* TEST ENTRY FOR NEGATIVE OR ZERO                         */ 00595000
*         /*                                                         */ 00596000
*         /***********************************************************/ 00597000
*                                                                  0171 00598000
*         IF(ASVTENTY(TEMPWORK)^=0)&(ASVTAVAL(TEMPWORK)='0'B) THEN 0171 00599000
         L     @10,TEMPWORK(,GLOSAPTR)                             0171 00600000
         SLL   @10,2                                               0171 00601000
         L     @06,CVTPTR                                          0171 00602000
         L     @06,ASVTPTR(,@06)                                   0171 00603000
         L     @04,ASVTENTY-4(@10,@06)                             0171 00604000
         LTR   @04,@04                                             0171 00605000
         BZ    @RF00171                                            0171 00606000
         ALR   @06,@10                                             0171 00607000
         TM    ASVTAVAL-4(@06),B'10000000'                         0171 00608000
         BNZ   @RF00171                                            0171 00609000
*           DO;                     /* IF THE ENTRY IS NOT NEGATIVE     00610000
*                                      AND IS NOT ZERO THEN IT IS AN    00611000
*                                      ACTIVE ASCB                   */ 00612000
*             ASCBPTR=ASVTENTY(TEMPWORK);/* ESTABLISH ADDRESSABILITY    00613000
*                                      TO THIS ASCB                  */ 00614000
         LR    ASCBPTR,@04                                         0173 00615000
*             ASCBEWST=TODAFTER;    /* ADJUST WAIT TIME WITH TODAFTER*/ 00616000
         L     @10,LCCACPUS(,LCCAPTR)                              0174 00617000
         L     @10,WSACREST(,@10)                                  0174 00618000
         MVC   ASCBEWST(8,ASCBPTR),TODAFTER(@10)                   0174 00619000
*           END;                                                   0175 00620000
*         ELSE                                                     0176 00621000
*           ;                       /* NULL ELSE                     */ 00622000
@RF00171 DS    0H                                                  0177 00623000
*       END;                        /* END WAIT TIME ADJUSTMENT      */ 00624000
         LA    @10,1                                               0177 00625000
         AL    @10,TEMPWORK(,GLOSAPTR)                             0177 00626000
@DE00170 ST    @10,TEMPWORK(,GLOSAPTR)                             0177 00627000
         L     @06,CVTPTR                                          0177 00628000
         L     @06,ASVTPTR(,@06)                                   0177 00629000
         C     @10,ASVTMAXU(,@06)                                  0177 00630000
         BNH   @DL00170                                            0177 00631000
*       RFY                                                        0178 00632000
*         ASCBPTR UNRSTD;           /* FREE R9 FOR USE               */ 00633000
*     END CLOKGOOD;                                                0179 00634000
*   ELSE                                                           0180 00635000
*     ;                             /* NULL ELSE                     */ 00636000
@RF00161 DS    0H                                                  0181 00637000
*                                                                  0181 00638000
*   /*****************************************************************/ 00639000
*   /*                                                               */ 00640000
*   /* THIS RESTART LOOP WILL RESTART ALL ALIVE CPU'S THAT HAVE THEIR*/ 00641000
*   /* BIT SET ON IN THE RESTART MASK                                */ 00642000
*   /*                                                               */ 00643000
*   /*****************************************************************/ 00644000
*                                                                  0181 00645000
*RESTCPU:                                                          0181 00646000
*   RESTMASK=(RESTMASK&&PCCACAFM);  /* CLEAR BIT IN RESTART MASK FOR    00647000
*                                      THIS CPU                      */ 00648000
RESTCPU  XC    RESTMASK(2,GLOSAPTR),PCCACAFM(PCCAPTR)              0181 00649000
*   CURRCPUA=0;                     /* SET CURRCPUA TO ZERO          */ 00650000
         SLR   @10,@10                                             0182 00651000
         STH   @10,CURRCPUA(,GLOSAPTR)                             0182 00652000
*   RFY                                                            0183 00653000
*    (REG0,                                                        0183 00654000
*     WORKREG5) RSTD;               /* RESTRICT R0 AND R5            */ 00655000
*RESTLOOP:                                                         0184 00656000
*   DO WHILE RESTMASK^=0;           /* CONTINUE UNTIL ALL CPU'S HAVE    00657000
*                                      BEEN RESTARTED                */ 00658000
*                                                                  0184 00659000
RESTLOOP B     @DE00184                                            0184 00660000
@DL00184 DS    0H                                                  0185 00661000
*     /***************************************************************/ 00662000
*     /*                                                             */ 00663000
*     /* IF CURRCPUA IS NOT IN THE RESTART MASK GO INCREMENT TO THE  */ 00664000
*     /* NEXT CPU                                                    */ 00665000
*     /*                                                             */ 00666000
*     /***************************************************************/ 00667000
*                                                                  0185 00668000
*     IF(RESTMASK&BITMASK(CPUID+1))^=0 THEN                        0185 00669000
         SLR   @10,@10                                             0185 00670000
         SLR   @09,@09                                             0185 00671000
         IC    @09,CPUID(,GLOSAPTR)                                0185 00672000
         ALR   @09,@09                                             0185 00673000
         LA    @06,BITMASK(@09)                                    0185 00674000
         SLR   @04,@04                                             0185 00675000
         ICM   @04,3,0(@06)                                        0185 00676000
         SLR   @06,@06                                             0185 00677000
         ICM   @06,3,RESTMASK(GLOSAPTR)                            0185 00678000
         NR    @06,@04                                             0185 00679000
         CR    @06,@10                                             0185 00680000
         BE    @RF00185                                            0185 00681000
*       DO;                         /* DO IF CPU IS IN RESTART MASK  */ 00682000
*         RESTMASK=(RESTMASK&&BITMASK(CPUID+1));/* CLEAR BIT IN    0187 00683000
*                                      RESTART MASK FOR THE CURRENT     00684000
*                                      CPU BEING PROCESSED           */ 00685000
*                                                                  0187 00686000
         LA    @06,BITMASK(@09)                                    0187 00687000
         XC    RESTMASK(2,GLOSAPTR),0(@06)                         0187 00688000
*         /***********************************************************/ 00689000
*         /*                                                         */ 00690000
*         /* DETERMINE IF CPU IS STILL ALIVE BY TESTING THE ALIVE    */ 00691000
*         /* MASK IN THE CSD. IF IT IS NOT ALIVE GO INCREMENT TO NEXT*/ 00692000
*         /* CPU                                                     */ 00693000
*         /*                                                         */ 00694000
*         /***********************************************************/ 00695000
*                                                                  0188 00696000
*         IF(CSDCPUAL&BITMASK(CPUID+1))^=0 THEN                    0188 00697000
         L     @09,CVTPTR                                          0188 00698000
         L     @01,CVTCSD(,@09)                                    0188 00699000
         SLR   @06,@06                                             0188 00700000
         ICM   @06,3,CSDCPUAL(@01)                                 0188 00701000
         NR    @04,@06                                             0188 00702000
         CR    @04,@10                                             0188 00703000
         BE    @RF00188                                            0188 00704000
*           DO;                                                    0189 00705000
*             PCCAT00P(CURRCPUA+1)->/* PCCA FOR CURRCPUA     @G51BP2O*/ 00706000
*                 PCCAPSAV->        /* PSA FOR CURRCPUA      @G51BP2O*/ 00707000
*                 PSAACTCD=         /* ACTION CODE           @G51BP2O*/ 00708000
*                 PSAACTCD;         /* ACTION CODE FROM THE PSA OF 0190 00709000
*                                      THE EXECUTING CPU     @G51BP2O*/ 00710000
         LH    @10,CURRCPUA(,GLOSAPTR)                             0190 00711000
         LR    @06,@10                                             0190 00712000
         SLA   @06,2                                               0190 00713000
         L     @01,CVTPCCAT(,@09)                                  0190 00714000
         L     @01,PCCAT00P(@06,@01)                               0190 00715000
         L     @01,PCCAPSAV(,@01)                                  0190 00716000
         MVC   PSAACTCD(1,@01),PSAACTCD(PSAPTR)                    0190 00717000
*             WORKREG5=CURRCPUA;    /* LOAD ADDRESS OF CPU FOR SIGP  */ 00718000
         LR    WORKREG5,@10                                        0191 00719000
*             GEN(SIGP REG0,WORKREG5,6(REG0));/* SIGP RESTART        */ 00720000
         SIGP REG0,WORKREG5,6(REG0)                                     00721000
*             BC(8,INCRACPU);       /* BRANCH ON CONDITION CODE ZERO    00722000
*                                      TO INCREMENT TO THE NEXT CPU  */ 00723000
         BC    8,INCRACPU                                          0193 00724000
*             RETCODE=20;           /* SET RETURN CODE 20 IN CALLER'S   00725000
*                                      R15                           */ 00726000
         MVI   RETCODE(GLOSAPTR),X'14'                             0194 00727000
*           END;                                                   0195 00728000
*         ELSE                                                     0196 00729000
*           ;                       /* NULL ELSE                     */ 00730000
@RF00188 DS    0H                                                  0197 00731000
*       END;                                                       0197 00732000
*     ELSE                                                         0198 00733000
*       ;                           /* NULL ELSE                     */ 00734000
@RF00185 DS    0H                                                  0199 00735000
*INCRACPU:                                                         0199 00736000
*     CURRCPUA=CURRCPUA+1;          /* INCREMENT TO NEXT CPU         */ 00737000
INCRACPU LA    @10,1                                               0199 00738000
         AH    @10,CURRCPUA(,GLOSAPTR)                             0199 00739000
         STH   @10,CURRCPUA(,GLOSAPTR)                             0199 00740000
*   END RESTLOOP;                   /* END RESTART LOOP              */ 00741000
@DE00184 ICM   @10,3,RESTMASK(GLOSAPTR)                            0200 00742000
         BNZ   @DL00184                                            0200 00743000
*   RFY                                                            0201 00744000
*     REG0 UNRSTD;                  /* FREE R0 FOR USE               */ 00745000
*                                                                  0202 00746000
*   /*****************************************************************/ 00747000
*   /*                                                               */ 00748000
*   /* THE OTHER CPU'S HAVE NOW BEEN RESTARTED. ESTABLISH            */ 00749000
*   /* ADDRESSABILITY AND FINISH PROCESSING THIS CPU.                */ 00750000
*   /*                                                               */ 00751000
*   /*****************************************************************/ 00752000
*                                                                  0202 00753000
*   LCCAPTR=PSALCCAV;               /* ADDRESSABILITY TO LCCA        */ 00754000
         L     LCCAPTR,PSALCCAV(,PSAPTR)                           0202 00755000
*   PCCAPTR=PSAPCCAV;               /* ADDRESSABILITY TO PCCA        */ 00756000
*                                                                  0203 00757000
         L     PCCAPTR,PSAPCCAV(,PSAPTR)                           0203 00758000
*   /*****************************************************************/ 00759000
*   /*                                                               */ 00760000
*   /* IF BADCLOCK SWITCH IS OFF, ADJUST JOB STEP TIMING             */ 00761000
*   /*                                                               */ 00762000
*   /*****************************************************************/ 00763000
*                                                                  0204 00764000
*STORTODA:                                                         0204 00765000
*   IF BADCLOCK='0'B THEN                                          0204 00766000
STORTODA TM    BADCLOCK(GLOSAPTR),B'00010000'                      0204 00767000
         BNZ   @RF00204                                            0204 00768000
*     DO;                                                          0205 00769000
*       WORKREG5=ADDR(TODAFTER);    /* LOAD R5 WITH ADDR OF TODAFTER    00770000
*                                      FIELD IN CPU/REL SA           */ 00771000
         L     @10,LCCACPUS(,LCCAPTR)                              0206 00772000
         L     WORKREG5,WSACREST(,@10)                             0206 00773000
         LA    WORKREG5,TODAFTER(,WORKREG5)                        0206 00774000
*       GEN(STCK 0(WORKREG5));      /* STORE TOD CLOCK IN TODAFTER   */ 00775000
         STCK 0(WORKREG5)                                               00776000
*       BC(7,SETCOMP);              /* IF NOT CC0 BYPASS JOB STEP  0208 00777000
*                                      TIMING ADJUSTMENT AND GO SET     00778000
*                                      BIT ON IN THE COMPLETE MASK 0208 00779000
*                                      FOR THIS CPU                  */ 00780000
*                                                                  0208 00781000
         BC    7,SETCOMP                                           0208 00782000
*       /*************************************************************/ 00783000
*       /*                                                           */ 00784000
*       /* THE FOLLOWING SEGMENT OF CODE WILL COMPUTE THE AMOUNT OF  */ 00785000
*       /* TIME THE CPU('S) WERE IN THE STOPPED STATE AND ADJUST THE */ 00786000
*       /* LCCADTOD FIELD FOR THE DISPATCHER. THE CHECK WILL BE MADE */ 00787000
*       /* TO DETERMINE IF THE DISPATCHER WAS UPDATEING JOB STEP     */ 00788000
*       /* TIMING AT THE TIME THE CPU WAS STOPPED. IF SO, I WILL SET */ 00789000
*       /* THE DISPATCHER BACK TO THE BEGINNING OF THE JST           */ 00790000
*       /* COMPUTATION CODE.                                         */ 00791000
*       /*                                                           */ 00792000
*       /*************************************************************/ 00793000
*                                                                  0209 00794000
*       DTOD=DTOD+(HIAFTER-HIBEFOR);/* SUBTRACT TODBEFOR FROM      0209 00795000
*                                      TODAFTER AND ADD IT TO      0209 00796000
*                                      LCCADTOD                      */ 00797000
         L     @10,LCCACPUS(,LCCAPTR)                              0209 00798000
         L     @10,WSACREST(,@10)                                  0209 00799000
         L     @10,HIAFTER(,@10)                                   0209 00800000
         SL    @10,HIBEFOR(,GLOSAPTR)                              0209 00801000
         L     @09,DTOD(,LCCAPTR)                                  0209 00802000
         ALR   @09,@10                                             0209 00803000
         ST    @09,DTOD(,LCCAPTR)                                  0209 00804000
*/* IF ITOD HAS BEEN SET, ADD ELAPSED TIME TO IT ALSO.  THIS IS FOR     00805000
*    DEPENDENCY IN SMF IEAQWAIT SO THAT IF THEY SUBTRACT LCCADTOD  0210 00806000
*    FROM LCCAITOD, THEY WILL BE INSURED THE ANSWER IS NOT NEGATIVE. */ 00807000
*                                                                  0210 00808000
*       IF ITOD^=0                  /* IF ITOD IS SET        @YM07640*/ 00809000
*         THEN                      /* THEN UPDATE IT        @YM07640*/ 00810000
         L     @09,ITOD(,LCCAPTR)                                  0210 00811000
         LTR   @09,@09                                             0210 00812000
         BZ    @RF00210                                            0210 00813000
*         ITOD=ITOD+(HIAFTER-HIBEFOR);/*                     @YM07640*/ 00814000
*                                                                  0211 00815000
         ALR   @09,@10                                             0211 00816000
         ST    @09,ITOD(,LCCAPTR)                                  0211 00817000
*       /*************************************************************/ 00818000
*       /*                                                           */ 00819000
*       /* DETERMINE IF DISPATCHER IS IN JST WINDOW BY TESTING THE   */ 00820000
