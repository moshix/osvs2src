*   /*                                                               */ 00094000
*   /* CLEAR SWITCHES AND CRITICAL WORK AREAS                        */ 00095000
*   /*                                                               */ 00096000
*   /*****************************************************************/ 00097000
*                                                                  0067 00098000
*   SWITCHES='00000000'B;           /* SET SWITCHES TO ZERO          */ 00099000
         MVI   SWITCHES(GLOSAPTR),B'00000000'                      0067 00100000
*   CLEARWA=''B;                    /* CLEAR TEMPORARY WORK AREA   0068 00101000
*                                                            @ZD08005*/ 00102000
*                                                                  0068 00103000
         XC    CLEARWA(8,GLOSAPTR),CLEARWA(GLOSAPTR)               0068 00104000
*   /*****************************************************************/ 00105000
*   /*                                                               */ 00106000
*   /* BUILD DISABLED WAITSTATE PSW WITH CALLERS WAIT STATE CODE     */ 00107000
*   /*                                                               */ 00108000
*   /*****************************************************************/ 00109000
*                                                                  0069 00110000
*   FIRSTWRD='000A000000'X;         /* 1ST PART OF PSW = '000A0000'X    00111000
*                                      EXTENDED MODE AND WAIT BITS ON*/ 00112000
         MVC   FIRSTWRD(5,GLOSAPTR),@CB02272                       0069 00113000
*   WAITWORD=WAITCODE;              /* 2ND PART OF PSW = CALLERS WAIT   00114000
*                                      STATE CODE FROM CALLERS R0    */ 00115000
*                                                                  0070 00116000
         MVC   WAITWORD(3,GLOSAPTR),WAITCODE(GLOSAPTR)             0070 00117000
*   /*****************************************************************/ 00118000
*   /*                                                               */ 00119000
*   /* STORE TOD CLOCK VALUE TO BE USED IN THE CALCULATION OF THE    */ 00120000
*   /* PERIOD OF TIME CPUS ARE IN THE STOPPED STATE                  */ 00121000
*   /*                                                               */ 00122000
*   /*****************************************************************/ 00123000
*                                                                  0071 00124000
*   GEN(STCK TODBEFOR(11));         /* TOD CLOCK VALUE INTO GLOBAL SA*/ 00125000
         STCK TODBEFOR(11)                                              00126000
*   BC(8,CKFORMP);                  /* BRANCH ON CC 0 TO CHECK FOR MP*/ 00127000
         BC    8,CKFORMP                                           0072 00128000
*   BADCLOCK='1'B;                  /* IF NOT CC 0 SET SWITCH IN   0073 00129000
*                                      GLOBAL SA TO INDICATE TOD   0073 00130000
*                                      CLOCK VALUE IS NOT VALID      */ 00131000
         OI    BADCLOCK(GLOSAPTR),B'00010000'                      0073 00132000
*   RFY                                                            0074 00133000
*    (LCCAPTR,                                                     0074 00134000
*     PCCAPTR) RSTD;                /* RESTRICT R7 AND R8            */ 00135000
*                                                                  0074 00136000
*   /*****************************************************************/ 00137000
*   /*                                                               */ 00138000
*   /* DETERMINE IF THE CPU WE ARE RUNNING ON IS A UNIPROC OR MP     */ 00139000
*   /*                                                               */ 00140000
*   /*****************************************************************/ 00141000
*                                                                  0075 00142000
*CKFORMP:                                                          0075 00143000
*   CURRCPUA=0;                     /* SET CURRENT CPU TO ZERO       */ 00144000
CKFORMP  SLR   @10,@10                                             0075 00145000
         STH   @10,CURRCPUA(,GLOSAPTR)                             0075 00146000
*   IF CSDMP='1'B THEN                                             0076 00147000
         L     @10,CVTPTR                                          0076 00148000
         L     @10,CVTCSD(,@10)                                    0076 00149000
         TM    CSDMP(@10),B'10000000'                              0076 00150000
         BNO   @RF00076                                            0076 00151000
*     DO;                                                          0077 00152000
*       STAP(MSTRADDR);             /* SAVE ADDR OF CPU I'M RUNNING     00153000
*                                      ON. (MSTRADDR)                */ 00154000
         STAP  MSTRADDR(GLOSAPTR)                                  0078 00155000
*       MAXCPUS='10'X;              /* SET MAXCPUS TO NUMBER OF CPUS    00156000
*                                      THAT COULD POSSIBLY BE ONLINE */ 00157000
         MVI   MAXCPUS(GLOSAPTR),X'10'                             0079 00158000
*     END;                                                         0080 00159000
*   ELSE                                                           0081 00160000
*UNIPRCDO:                                                         0081 00161000
*     DO;                           /* DO THIS IF UNIPROCESSOR       */ 00162000
         B     @RC00076                                            0081 00163000
@RF00076 DS    0H                                                  0081 00164000
UNIPRCDO DS    0H                                                  0082 00165000
*       GOTO LASTCPU;               /* GO PROCESS THE LAST CPU       */ 00166000
         B     LASTCPU                                             0082 00167000
*     END UNIPRCDO;                 /* END UNIPROCESSOR DO GROUP     */ 00168000
*/***** START OF CPUSCAN ******************************************* */ 00169000
*/*                                                                  */ 00170000
*/* THIS SEGMENT OF CODE WILL SCAN ALL POSSIBLE CPUS FOR ONLINE.     */ 00171000
*/* IT WILL CALL THE STOPSTORE ROUTINE TO PROCESS EACH LIVE CPU      */ 00172000
*/* EXCEPT THE ONE I'M RUNNING ON (MSTRADDR).                        */ 00173000
*/*                                                                  */ 00174000
*/*****************************************************************  */ 00175000
*                                                                  0084 00176000
*CPULOOP:                                                          0084 00177000
*   DO WHILE(CPUID<MAXCPUS);        /* LOOP UNTIL ALL CPUS HAVE BEEN    00178000
*                                      TESTED FOR ONLINE             */ 00179000
*                                                                  0084 00180000
@RC00076 DS    0H                                                  0084 00181000
CPULOOP  B     @DE00084                                            0084 00182000
@DL00084 DS    0H                                                  0085 00183000
*     /***************************************************************/ 00184000
*     /*                                                             */ 00185000
*     /* DETERMINE IF THE CURR CPU IS THE MASTER. IF NOT THE MASTER  */ 00186000
*     /* THEN PROCESS IT. IF IT IS THE MASTER INCREMENT TO NEXT CPU  */ 00187000
*     /*                                                             */ 00188000
*     /***************************************************************/ 00189000
*                                                                  0085 00190000
*     IF MSTRADDR^=CURRCPUA THEN                                   0085 00191000
         CLC   MSTRADDR(2,GLOSAPTR),CURRCPUA(GLOSAPTR)             0085 00192000
         BE    @RF00085                                            0085 00193000
*CHECKVTE:                          /* CHECK PCCA VECTOR TABLE ENTRY */ 00194000
*       DO;                         /* FOR THIS CPU                  */ 00195000
CHECKVTE DS    0H                                                  0087 00196000
*         IF(CSDCPUAL&BITMASK(CPUID+1))^=0 THEN                    0087 00197000
         L     @10,CVTPTR                                          0087 00198000
         L     @10,CVTCSD(,@10)                                    0087 00199000
         SLR   @09,@09                                             0087 00200000
         IC    @09,CPUID(,GLOSAPTR)                                0087 00201000
         ALR   @09,@09                                             0087 00202000
         LA    @06,BITMASK(@09)                                    0087 00203000
         SLR   @09,@09                                             0087 00204000
         ICM   @09,3,0(@06)                                        0087 00205000
         SLR   @06,@06                                             0087 00206000
         ICM   @06,3,CSDCPUAL(@10)                                 0087 00207000
         NR    @09,@06                                             0087 00208000
         LTR   @09,@09                                             0087 00209000
         BZ    @RF00087                                            0087 00210000
*ADDSETUP:                                                         0088 00211000
*           DO;                     /* DO IF CPU IS ALIVE            */ 00212000
ADDSETUP DS    0H                                                  0089 00213000
*             PCCAPTR=PCCAT00P(CPUID+1);                           0089 00214000
         SLR   @10,@10                                             0089 00215000
         IC    @10,CPUID(,GLOSAPTR)                                0089 00216000
         SLA   @10,2                                               0089 00217000
         L     @09,CVTPTR                                          0089 00218000
         L     @01,CVTPCCAT(,@09)                                  0089 00219000
         L     PCCAPTR,PCCAT00P(@10,@01)                           0089 00220000
*             PSAPTR=PCCAPSAV;      /* ESTABLISH ADDRESSABILITY TO 0090 00221000
*                                      PSA FOR THIS CPU              */ 00222000
         L     PSAPTR,PCCAPSAV(,PCCAPTR)                           0090 00223000
*             LCCAPTR=LCCAT00P(CPUID+1);/* ESTABLISH ADDRESSABILITY     00224000
*                                      TO THIS CPUS LCCA             */ 00225000
         L     @06,CVTLCCAT(,@09)                                  0091 00226000
         L     LCCAPTR,LCCAT00P(@10,@06)                           0091 00227000
*             CALL STOPSTOR;        /* CALL INTERNAL PROC TO PROCESS    00228000
*                                      CPUS                          */ 00229000
*                                                                  0092 00230000
         BAL   @14,STOPSTOR                                        0092 00231000
*             /*******************************************************/ 00232000
*             /*                                                     */ 00233000
*             /* DETERMINE IF THE STOP AND STORE STATUS ROUTINE SET  */ 00234000
*             /* AN ERROR RETURN CODE IN THE CALLER'S R15            */ 00235000
*             /*                                                     */ 00236000
*             /*******************************************************/ 00237000
*                                                                  0093 00238000
*             IF RETCODE^=0 THEN                                   0093 00239000
         CLI   RETCODE(GLOSAPTR),0                                 0093 00240000
         BNE   @RT00093                                            0093 00241000
*               GOTO ERRCLNUP;      /* GO RESTART CPUS AND CLEANUP   */ 00242000
*             ELSE                                                 0095 00243000
*               ;                                                  0095 00244000
*           END ADDSETUP;           /* END ADDRESS SETUP DO GROUP    */ 00245000
*         ELSE                                                     0097 00246000
*           ;                                                      0097 00247000
@RF00087 DS    0H                                                  0098 00248000
*       END CHECKVTE;               /* END CHECK VECTOR TABLE ENTRY     00249000
*                                      DO GROUP                      */ 00250000
*     ELSE                                                         0099 00251000
*       ;                                                          0099 00252000
@RF00085 DS    0H                                                  0100 00253000
*     CURRCPUA=CURRCPUA+1;          /* INCREMENT CPU ADDRESS         */ 00254000
         LA    @10,1                                               0100 00255000
         AH    @10,CURRCPUA(,GLOSAPTR)                             0100 00256000
         STH   @10,CURRCPUA(,GLOSAPTR)                             0100 00257000
*   END CPULOOP;                                                   0101 00258000
@DE00084 CLC   CPUID(1,GLOSAPTR),MAXCPUS(GLOSAPTR)                 0101 00259000
         BL    @DL00084                                            0101 00260000
*   CURRCPUA=MSTRADDR;              /* SET UP TO PROCESS LAST CPU    */ 00261000
         MVC   CURRCPUA(2,GLOSAPTR),MSTRADDR(GLOSAPTR)             0102 00262000
*/**** START OF LASTCPU **********************************************/ 00263000
*/*                                                                  */ 00264000
*/*  THIS SEGMENT OF CODE WILL PROCESS THE LAST CPU(MSTRADDR). IN THE*/ 00265000
*/*  UNIPROCESSOR CASE IT WILL PROCESS THE ONLY CPU.                 */ 00266000
*/*                                                                  */ 00267000
