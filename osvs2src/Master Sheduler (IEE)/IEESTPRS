*                                      COUNTER TO THE CLEANUP SEG    */ 00298000
         LA    @10,NRMCLNUP                                        0112 00299000
         ST    @10,RESUMEIC(,PSAPTR)                               0112 00300000
*   SAVRESTN=FLCRNPSW;              /* SAVE ORIGINAL RESTART NEW PSW    00301000
*                                      IN CPU/RELATED SAVE AREA      */ 00302000
