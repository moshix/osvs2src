*   RSTRTIC=ADDR(RESTFLIH);         /*                       @ZD03005*/ 00326000
         LA    @09,RESTFLIH                                        0119 00327000
         ST    @09,RSTRTIC(,PSAPTR)                                0119 00328000
*   RESTMASK=RESTMASK|PCCACAFM;     /* SET BIT IN RESTART MASK     0120 00329000
*                                                            @ZD03005*/ 00330000
*                                                                  0120 00331000
         OC    RESTMASK(2,GLOSAPTR),PCCACAFM(PCCAPTR)              0120 00332000
*   /*****************************************************************/ 00333000
*   /*                                                               */ 00334000
*   /* DETERMINE IF WE NEED TO SAVE STATUS AWAY BY TESTING THE       */ 00335000
*   /* CALLER'S R1 WHICH IS STORED IN THE GLOBAL SA(STATSAVE).       */ 00336000
*   /*                                                               */ 00337000
*   /*****************************************************************/ 00338000
*                                                                  0121 00339000
*   IF STATSAVE^=0 THEN                                            0121 00340000
