*       FLCPNPSW=FLCRNPSW;          /* SET PROG CK NEW POINTING TO MY   00395000
*                                      FLIH                          */ 00396000
         MVC   FLCPNPSW(8,PSAPTR),FLCRNPSW(PSAPTR)                 0130 00397000
