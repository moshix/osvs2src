*                       RFY                                        0083 00290000
*                        (R15) UNRSTD;                             0083 00291000
*                     END;                                         0084 00292000
*             END;                  /* END OF IECVIOPM PROCESSING    */ 00293000
*                                                                  0085 00294000
*       /*************************************************************/ 00295000
*       /*                                                           */ 00296000
*       /* NO AVAILABLE PATHS TO THE DEVICE EXIST.. INDICATE TO      */ 00297000
*       /* CALLER BY SETTING RETURN CODE 4 ALSO SET HIERACRY REASON  */ 00298000
*       /* INDICATOR IN UCB                                          */ 00299000
*       /*                                                           */ 00300000
*       /*************************************************************/ 00301000
*                                                                  0086 00302000
*           ELSE                    /* THERE WERE NO PATHS           */ 00303000
*             DO;                   /* SO SET HIERARCHY REASON       */ 00304000
         B     @RC00059                                            0086 00305000
@RF00059 DS    0H                                                  0087 00306000
*               RCODE=4;            /* BIT IN UCB AND SET RETURN     */ 00307000
