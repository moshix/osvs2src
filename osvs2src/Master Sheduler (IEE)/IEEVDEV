*               FUNCTION=0;         /* FUNCTION CODE FOR PATH TEST   */ 00170000
*                                                                  0062 00171000
         MVI   FUNCTION,X'00'                                      0062 00172000
*               /*****************************************************/ 00173000
*               /*                                                   */ 00174000
*               /* REGISTER ONE MUST POINT DIRECTLY TO ONE WORD      */ 00175000
*               /* PARAMETER                                         */ 00176000
*               /*                                                   */ 00177000
*               /*****************************************************/ 00178000
*                                                                  0063 00179000
*               RFY                                                0063 00180000
*                (R1) RSTD;         /* RESTRICT REGISTER ONE         */ 00181000
*               R1=ADDR(FUNCPTR);   /* PASS PARMS TO IOS             */ 00182000
         LA    R1,FUNCPTR                                          0064 00183000
*               GEN(LINK EP=IECVIOPM);/* LINK TO IOS PATH TEST       */ 00184000
         LINK EP=IECVIOPM                                               00185000
*               RFY                                                0066 00186000
*                (R1) UNRSTD;       /* RELEASE REGISTER ONE          */ 00187000
*                                                                  0066 00188000
*               /*****************************************************/ 00189000
*               /*                                                   */ 00190000
*               /* UPON RETURN REGISTER 15 WILL HAVE RETURN CODE     */ 00191000
*               /* INDICATING STATUS OF PATHS. CONVERT RETURN CODE TO*/ 00192000
*               /* VALUE EXPECTED BY MY CALLER AND RETURN            */ 00193000
*               /*                                                   */ 00194000
*               /*****************************************************/ 00195000
*                                                                  0067 00196000
*               RFY                                                0067 00197000
*                (R15) RSTD;        /* RESTRICT REG 15 FOR TESTS     */ 00198000
*                                                                  0067 00199000
*               /*****************************************************/ 00200000
*               /*                                                   */ 00201000
*               /* CHECK FOR ANY OPERATIONAL PATH. RET CODE 0 MEANS  */ 00202000
*               /* AT LEAST ONE PATH EXISTS TO DEVICE. RET CODE      */ 00203000
*               /* TWENTY MEANS DEVICE IS RESERVED BUT PATH EXISTS.  */ 00204000
*               /*                                                   */ 00205000
*               /*****************************************************/ 00206000
*                                                                  0068 00207000
*               IF R15=0|R15=20 THEN/* CHECK FOR OPERATIONAL PATH    */ 00208000
         LTR   R15,R15                                             0068 00209000
         BZ    @RT00068                                            0068 00210000
         C     R15,@CF01784                                        0068 00211000
         BNE   @RF00068                                            0068 00212000
@RT00068 DS    0H                                                  0069 00213000
*                 DO;                                              0069 00214000
*                   UCBVHRSN='0'B;  /* RESET HIER REASON INDICATOR   */ 00215000
         NI    UCBVHRSN(UCBPTR),B'11111101'                        0070 00216000
*                   RCODE=0;        /* INDICATE OPERATIONAL PATHS    */ 00217000
         MVI   RCODE,X'00'                                         0071 00218000
*                 END;                                             0072 00219000
*                                                                  0072 00220000
*               /*****************************************************/ 00221000
*               /*                                                   */ 00222000
*               /* OPERATIONAL PATH DOESN'T EXIST CHECK FURTHER      */ 00223000
*               /*                                                   */ 00224000
*               /*****************************************************/ 00225000
*                                                                  0073 00226000
*               ELSE                                               0073 00227000
*                                                                  0073 00228000
*                 /***************************************************/ 00229000
*                 /*                                                 */ 00230000
*                 /* CHECK IF NO OPERATIONAL PATHS EXIST TO THE      */ 00231000
*                 /* DEVICE RET CODE 4 MEANS NO PATHS EXIST I/O WAS  */ 00232000
*                 /* ISSUED, 8 MEANS NO PATHS EXIST I/O WAS NOT      */ 00233000
*                 /* ISSUED                                          */ 00234000
*                 /*                                                 */ 00235000
*                 /***************************************************/ 00236000
*                                                                  0073 00237000
*                 IF R15=4|R15=8 THEN/* CHECK FOR NO OPERATIONAL PATH*/ 00238000
         B     @RC00068                                            0073 00239000
@RF00068 C     R15,@CF00086                                        0073 00240000
         BE    @RT00073                                            0073 00241000
         C     R15,@CF00039                                        0073 00242000
         BNE   @RF00073                                            0073 00243000
@RT00073 DS    0H                                                  0074 00244000
*                   DO;                                            0074 00245000
*                     UCBVORSN='1'B;/* SET OPERATOR REASON INDICATOR */ 00246000
         OI    UCBVORSN(UCBPTR),B'00000100'                        0075 00247000
*                     RCODE=12;     /* SET NO OPERATIONAL PATH RCODE */ 00248000
         MVI   RCODE,X'0C'                                         0076 00249000
*                   END;                                           0077 00250000
*                                                                  0077 00251000
*               /*****************************************************/ 00252000
*               /*                                                   */ 00253000
*               /* PATH IS NEITHER OPERATIONAL NOR INOPERATIONAL     */ 00254000
*               /* CHECK FOR ERROR OF SOME KIND                      */ 00255000
*               /*                                                   */ 00256000
*               /*****************************************************/ 00257000
*                                                                  0078 00258000
*                 ELSE                                             0078 00259000
*                                                                  0078 00260000
*                   /*************************************************/ 00261000
*                   /*                                               */ 00262000
*                   /* RETURN CODE 12 INDICATES NO STORAGE WAS       */ 00263000
*                   /* AVAILABLE FOR PROCESSING                      */ 00264000
*                   /*                                               */ 00265000
*                   /*************************************************/ 00266000
*                                                                  0078 00267000
*                   IF R15=12 THEN  /* CHECK FOR NO CORE             */ 00268000
         B     @RC00073                                            0078 00269000
@RF00073 C     R15,@CF00033                                        0078 00270000
         BNE   @RF00078                                            0078 00271000
*                     RCODE=8;      /* INDICATE NOCORE               */ 00272000
         MVI   RCODE,X'08'                                         0079 00273000
*                   ELSE            /* INVALID RETURN CODE           */ 00274000
*                                                                  0080 00275000
*                     /***********************************************/ 00276000
*                     /*                                             */ 00277000
*                     /* INVALID RETURN CODE RECEIVED FROM IOS SET   */ 00278000
*                     /* REASON CODE AND ABEND INDICATOR             */ 00279000
*                     /*                                             */ 00280000
*                     /***********************************************/ 00281000
*                                                                  0080 00282000
*                     DO;                                          0080 00283000
         B     @RC00078                                            0080 00284000
@RF00078 DS    0H                                                  0081 00285000
*                       RCODE=20;   /* SET REASON CODE               */ 00286000
