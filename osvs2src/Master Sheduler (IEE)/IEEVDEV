*             END;                  /* ARE AVAILABLE                 */ 00311000
*     END;                          /* END OF FUNCTION CODE 00     0090 00312000
*                                      PROCESSING                    */ 00313000
*                                                                  0090 00314000
*   /*****************************************************************/ 00315000
*   /*                                                               */ 00316000
*   /* FUNCTION CODE WAS NOT 00.. CHECK FOR FUNCTION CODE 01 AND IF  */ 00317000
*   /* SO CALL CPULEVEL ROUTINE                                      */ 00318000
*   /*                                                               */ 00319000
*   /*****************************************************************/ 00320000
*                                                                  0091 00321000
*   ELSE                            /* FUNCTION CODE OTHER THAN 00   */ 00322000
*     IF FCODE=1 THEN               /* IF FUNCTION CODE IS 01        */ 00323000
         B     @RC00053                                            0091 00324000
@RF00053 L     @10,@PC00001                                        0091 00325000
         CLI   FCODE(@10),1                                        0091 00326000
         BNE   @RF00091                                            0091 00327000
*       CALL CPULEVEL;              /* THEN GO TO CPU ROUTINE        */ 00328000
*                                                                  0092 00329000
         BAL   @14,CPULEVEL                                        0092 00330000
*   /*****************************************************************/ 00331000
*   /*                                                               */ 00332000
*   /* FUNCTION CODE WAS NOT 01.. CHECK FOR FUNCTION CODE 02 AND IF  */ 00333000
*   /* SO CALL CHANNEL LEVEL ROUTINE.. OTHERWISE SET ABEND           */ 00334000
*   /* INDICATOR... INVALID PARAMETERS                               */ 00335000
*   /*                                                               */ 00336000
*   /*****************************************************************/ 00337000
*                                                                  0093 00338000
*     ELSE                          /* FUNCTION CODE NOT 00 OR 01    */ 00339000
*       IF FCODE=2 THEN             /* IF FUNCTION CODE IS 02        */ 00340000
         B     @RC00091                                            0093 00341000
@RF00091 L     @10,@PC00001                                        0093 00342000
         CLI   FCODE(@10),2                                        0093 00343000
         BNE   @RF00093                                            0093 00344000
*         CALL CHNLEVEL;            /* THEN GO TO CHANNEL ROUTINE    */ 00345000
*                                                                  0094 00346000
         BAL   @14,CHNLEVEL                                        0094 00347000
*   /*****************************************************************/ 00348000
*   /*                                                               */ 00349000
*   /* WAS NOT FUNCTION CODE 0, 1, OR 2, CHECK FOR 3 OR 4            */ 00350000
*   /*                                                               */ 00351000
*   /*****************************************************************/ 00352000
*                                                                  0095 00353000
*       ELSE                        /* OTHERWISE IT IS AN            */ 00354000
*                                                                  0095 00355000
*         /***********************************************************/ 00356000
*         /*                                                         */ 00357000
*         /* CHECK FOR 3 OR 4                                        */ 00358000
*         /*                                                         */ 00359000
*         /***********************************************************/ 00360000
*                                                                  0095 00361000
*         IF FCODE=3 THEN           /* FUNCTION CODE 3?              */ 00362000
         B     @RC00093                                            0095 00363000
@RF00093 L     @10,@PC00001                                        0095 00364000
         CLI   FCODE(@10),3                                        0095 00365000
         BNE   @RF00095                                            0095 00366000
*           CALL CPUTEST;           /* CHECK CPU STATUS              */ 00367000
         BAL   @14,CPUTEST                                         0096 00368000
*         ELSE                                                     0097 00369000
*           IF FCODE=4 THEN         /* FUNCTION CODE 4?              */ 00370000
         B     @RC00095                                            0097 00371000
@RF00095 L     @10,@PC00001                                        0097 00372000
         CLI   FCODE(@10),4                                        0097 00373000
         BNE   @RF00097                                            0097 00374000
*             CALL CHNTEST;         /* CALL CHANNEL TEST             */ 00375000
*                                                                  0098 00376000
         BAL   @14,CHNTEST                                         0098 00377000
*   /*****************************************************************/ 00378000
*   /*                                                               */ 00379000
*   /* FUNCTION CODE WAS NOT 0, 1, 2, 3, OR 4. SET ERROR FLAG        */ 00380000
*   /*                                                               */ 00381000
*   /*****************************************************************/ 00382000
*                                                                  0099 00383000
*           ELSE                                                   0099 00384000
*             DO;                                                  0099 00385000
         B     @RC00097                                            0099 00386000
@RF00097 DS    0H                                                  0100 00387000
*               RCODE=12;           /* REASON CODE FOR ABEND         */ 00388000
         MVI   RCODE,X'0C'                                         0100 00389000
*               ABIND=1;            /* INVALID FUNCTION CODE         */ 00390000
         MVI   ABIND,X'01'                                         0101 00391000
*             END;                                                 0102 00392000
*                                                                  0102 00393000
*   /*****************************************************************/ 00394000
*   /*                                                               */ 00395000
*   /* CHECK IF INVALID PARAMETERS WERE DETECTED ANYWHERE AND IF SO  */ 00396000
*   /* ISSUE ABEND                                                   */ 00397000
*   /*                                                               */ 00398000
*   /*****************************************************************/ 00399000
*                                                                  0103 00400000
*   IF ABIND=1 THEN                 /* IF INDICATOR IS ON THEN       */ 00401000
@RC00097 DS    0H                                                  0103 00402000
@RC00095 DS    0H                                                  0103 00403000
@RC00093 DS    0H                                                  0103 00404000
@RC00091 DS    0H                                                  0103 00405000
@RC00053 CLI   ABIND,1                                             0103 00406000
         BNE   @RF00103                                            0103 00407000
*ABEND1:                                                           0104 00408000
*     DO;                                                          0104 00409000
ABEND1   DS    0H                                                  0105 00410000
*       REASON=RCODE;               /* RESTORE REASON CODE TO      0105 00411000
*                                      REGISTER 15                   */ 00412000
         SLR   REASON,REASON                                       0105 00413000
         IC    REASON,RCODE                                        0105 00414000
*       GEN(SPKA  0(@02));          /* RESTORE TO KEY OF CALLER    0106 00415000
*                                                            @YM07661*/ 00416000
         SPKA  0(@02)                                                   00417000
*       DO;                         /* ABEND(126) SYSTEM             */ 00418000
*         RESPECIFY                                                0108 00419000
*          (GPR01F) RESTRICTED;                                    0108 00420000
*         GPR01F=((126)&'00000FFF'X)*4096;/* COMP CODE IN BITS 8-19  */ 00421000
         L     GPR01F,@CF01817                                     0109 00422000
*         SVC(13);                  /* ISSUE ABEND SVC               */ 00423000
