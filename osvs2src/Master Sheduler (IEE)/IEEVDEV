         B     @RC00135                                            0137 00669000
@RF00135 MVI   RCODE,X'04'                                         0137 00670000
*   /*****************************************************************/ 00671000
*   /*                                                               */ 00672000
*   /* WAS NOT FIRST CHANNEL IN MAP.. MUST BE SECOND                 */ 00673000
*   /*                                                               */ 00674000
*   /*****************************************************************/ 00675000
*                                                                  0138 00676000
*           ELSE                    /* THIS IS CHN 2 CPU 1           */ 00677000
*                                                                  0138 00678000
*             /*******************************************************/ 00679000
*             /*                                                     */ 00680000
*             /* CHECK IF PATH EXISTS TO DEVICE ON ANY OF THE OTHER  */ 00681000
*             /* THREE POSSABILITIES IF SO SET RCODE = 0             */ 00682000
*             /*                                                     */ 00683000
*             /*******************************************************/ 00684000
*                                                                  0138 00685000
*             IF PTH00='01'X|PTH01='01'X/* CHECK IF PATH IS AVAILABLE*/ 00686000
*                 |PTH10='01'X THEN /* ON ANY OTHER CHANNEL          */ 00687000
         B     @RC00134                                            0138 00688000
@RF00134 CLI   PTH00,X'01'                                         0138 00689000
         BE    @RT00138                                            0138 00690000
         CLI   PTH01,X'01'                                         0138 00691000
         BE    @RT00138                                            0138 00692000
         CLI   PTH10,X'01'                                         0138 00693000
         BNE   @RF00138                                            0138 00694000
@RT00138 DS    0H                                                  0139 00695000
*               RCODE=0;            /* PATH DOES NOT NEED THIS CHN   */ 00696000
*                                                                  0139 00697000
         MVI   RCODE,X'00'                                         0139 00698000
*   /*****************************************************************/ 00699000
*   /*                                                               */ 00700000
*   /* NO OTHER PATHS EXIST.. SET RCODE = 4                          */ 00701000
*   /*                                                               */ 00702000
*   /*****************************************************************/ 00703000
*                                                                  0140 00704000
*             ELSE                  /* THIS CHANNEL IS LAST PATH     */ 00705000
*               RCODE=4;            /* RETURN CODE                   */ 00706000
*                                                                  0140 00707000
         B     @RC00138                                            0140 00708000
@RF00138 MVI   RCODE,X'04'                                         0140 00709000
*   /*****************************************************************/ 00710000
*   /*                                                               */ 00711000
*   /* CHANNEL BEING REMOVED DOES NOT MATCH EITHER CHANNEL ENTRY IN  */ 00712000
*   /* MAP.. DEVICE NOT DEPENDENT ON THIS CHANNEL SET RCODE = 0      */ 00713000
*   /*                                                               */ 00714000
*   /*****************************************************************/ 00715000
*                                                                  0141 00716000
*     ELSE                          /* CHANNEL ID DID NOT MATCH      */ 00717000
*       RCODE=0;                    /* PATH NOT ON THESE CHANNELS    */ 00718000
*                                                                  0141 00719000
         B     @RC00120                                            0141 00720000
@RF00120 MVI   RCODE,X'00'                                         0141 00721000
*   /*****************************************************************/ 00722000
*   /*                                                               */ 00723000
*   /* NO AVAILABLE PATHS EXIST TO THE DEVICE ANYWHERE.. DEVICE NOT  */ 00724000
*   /* DEPENDENT ON THIS CHANNEL SET RCODE = 0                       */ 00725000
*   /*                                                               */ 00726000
*   /*****************************************************************/ 00727000
*                                                                  0142 00728000
*   ELSE                            /* NO AVAILABLE PATHS            */ 00729000
*     RCODE=0;                      /* NOT DEPENDENT ON CHANNELS     */ 00730000
         B     @RC00119                                            0142 00731000
@RF00119 MVI   RCODE,X'00'                                         0142 00732000
*   END;                            /* END OF CHANNEL LEVEL CHECK    */ 00733000
@EL00002 DS    0H                                                  0143 00734000
@EF00002 DS    0H                                                  0143 00735000
@ER00002 LM    @14,@12,12(@13)                                     0143 00736000
         BR    @14                                                 0143 00737000
*CPULEVEL:                                                         0144 00738000
*   PROC;                                                          0144 00739000
*                                                                  0144 00740000
CPULEVEL STM   @14,@12,12(@13)                                     0144 00741000
*/* *************************************************************    */ 00742000
*/*  CPU LEVEL CHECK WILL CHECK IF A PATH EXISTS TO THE DEVICE ON    */ 00743000
*/*  A CPU OTHER THAN THE ONE THE CALLER HAS SPECIFIED IN THE        */ 00744000
*/*  INPUT PARAMETERS A RCODE OF 0 INDICATES THAT A PATH DOES        */ 00745000
*/*  EXIST ON ANOTHER CPU A RCODE OF 4 INDICATES THAT A PATH DOES NOT*/ 00746000
*/*  EXIST ON ANOTHER CPU                                            */ 00747000
*/* *************************************************************    */ 00748000
*                                                                  0145 00749000
*   /*****************************************************************/ 00750000
*   /*                                                               */ 00751000
*   /* CHECK FOR A AVAILABLE PATH ANYWHERE                           */ 00752000
*   /*                                                               */ 00753000
*   /*****************************************************************/ 00754000
*                                                                  0145 00755000
*   IF PTH00='01'X|PTH01='01'X|     /* CHECK FOR ANY AVAL PATHS      */ 00756000
*       PTH10='01'X|PTH11='01'X THEN/* ANYWHERE                      */ 00757000
*                                                                  0145 00758000
         CLI   PTH00,X'01'                                         0145 00759000
         BE    @RT00145                                            0145 00760000
         CLI   PTH01,X'01'                                         0145 00761000
         BE    @RT00145                                            0145 00762000
         CLI   PTH10,X'01'                                         0145 00763000
         BE    @RT00145                                            0145 00764000
         CLI   PTH11,X'01'                                         0145 00765000
         BNE   @RF00145                                            0145 00766000
@RT00145 DS    0H                                                  0146 00767000
*     /***************************************************************/ 00768000
*     /*                                                             */ 00769000
*     /* A AVAILABLE PATH DOES EXIST.. VERIFY INPUT PARAMETERS IF CPU*/ 00770000
*     /* ADDRESS IS GREATER THAN 1 SET ABEND INDICATOR               */ 00771000
*     /*                                                             */ 00772000
*     /***************************************************************/ 00773000
*                                                                  0146 00774000
*     IF CPUID=>2 THEN              /* CHECK FOR VALID CPUID         */ 00775000
         L     @10,@PC00001                                        0146 00776000
         CLI   CPUID(@10),2                                        0146 00777000
         BL    @RF00146                                            0146 00778000
*       DO;                                                        0147 00779000
*         RCODE=16;                 /* SET REASON CODE               */ 00780000
         MVI   RCODE,X'10'                                         0148 00781000
*         ABIND=1;                  /* INVALID.. SET ABEND IND.      */ 00782000
         MVI   ABIND,X'01'                                         0149 00783000
*       END;                                                       0150 00784000
*                                                                  0150 00785000
*   /*****************************************************************/ 00786000
*   /*                                                               */ 00787000
*   /* INPUT PARAMETERS ARE VALID.. CONTINUE                         */ 00788000
*   /*                                                               */ 00789000
*   /*****************************************************************/ 00790000
*                                                                  0151 00791000
*     ELSE                          /* VALID CONTINUE                */ 00792000
*                                                                  0151 00793000
*       /*************************************************************/ 00794000
*       /*                                                           */ 00795000
*       /* DETERMINE WHICH CPU IS BEING REMOVED..                    */ 00796000
*       /*                                                           */ 00797000
*       /*************************************************************/ 00798000
*                                                                  0151 00799000
*       IF CPUID=0 THEN             /* CPU 0 BEING REMOVED?          */ 00800000
*                                                                  0151 00801000
         B     @RC00146                                            0151 00802000
@RF00146 L     @10,@PC00001                                        0151 00803000
         CLI   CPUID(@10),0                                        0151 00804000
         BNE   @RF00151                                            0151 00805000
*         /***********************************************************/ 00806000
*         /*                                                         */ 00807000
*         /* CPU 0 BEING REMOVED.. DOES AVAILABLE PATH EXIST ON CPU 1*/ 00808000
*         /* ? IF SO SET RCODE 0                                     */ 00809000
*         /*                                                         */ 00810000
*         /***********************************************************/ 00811000
*                                                                  0152 00812000
*         IF PTH01='01'X|PTH11='01'X THEN/* IS PATH ON OTHER CPU     */ 00813000
         CLI   PTH01,X'01'                                         0152 00814000
         BE    @RT00152                                            0152 00815000
         CLI   PTH11,X'01'                                         0152 00816000
         BNE   @RF00152                                            0152 00817000
@RT00152 DS    0H                                                  0153 00818000
*           RCODE=0;                /* PATH DOES NOT NEED CPU        */ 00819000
*                                                                  0153 00820000
         MVI   RCODE,X'00'                                         0153 00821000
*   /*****************************************************************/ 00822000
*   /*                                                               */ 00823000
*   /* PATH DOES NOT EXIST SET RCODE 4                               */ 00824000
*   /*                                                               */ 00825000
*   /*****************************************************************/ 00826000
*                                                                  0154 00827000
*         ELSE                      /* PATH DOES NEED THIS CPU       */ 00828000
*           RCODE=4;                /* SET DEPENDENT RETURN CODE     */ 00829000
*                                                                  0154 00830000
         B     @RC00152                                            0154 00831000
@RF00152 MVI   RCODE,X'04'                                         0154 00832000
*   /*****************************************************************/ 00833000
*   /*                                                               */ 00834000
*   /* WAS NOT CPU0 BEING REMOVED.. MUST BE CPU1 THEN                */ 00835000
*   /*                                                               */ 00836000
*   /*****************************************************************/ 00837000
*                                                                  0155 00838000
*       ELSE                        /* CPU 1 BEING REMOVED THEN      */ 00839000
*                                                                  0155 00840000
*         /***********************************************************/ 00841000
*         /*                                                         */ 00842000
*         /* CHECK IF PATH EXISTS ON CPU 0 IF SO SET RCODE = 0       */ 00843000
*         /*                                                         */ 00844000
*         /***********************************************************/ 00845000
*                                                                  0155 00846000
*         IF PTH00='01'X|PTH10='01'X THEN/* PATH ON OTHER CPU?       */ 00847000
         B     @RC00151                                            0155 00848000
@RF00151 CLI   PTH00,X'01'                                         0155 00849000
         BE    @RT00155                                            0155 00850000
         CLI   PTH10,X'01'                                         0155 00851000
         BNE   @RF00155                                            0155 00852000
@RT00155 DS    0H                                                  0156 00853000
*           RCODE=0;                /* YES.. SET OK RETURN CODE      */ 00854000
*                                                                  0156 00855000
         MVI   RCODE,X'00'                                         0156 00856000
*   /*****************************************************************/ 00857000
*   /*                                                               */ 00858000
*   /* OTHER PATH DOES NOT EXIST.. SET RCODE = 4                     */ 00859000
*   /*                                                               */ 00860000
*   /*****************************************************************/ 00861000
*                                                                  0157 00862000
*         ELSE                      /* PATH DEPENDENT ON THIS        */ 00863000
*           RCODE=4;                /* DEPENDENT RETURN CODE         */ 00864000
*                                                                  0157 00865000
         B     @RC00155                                            0157 00866000
@RF00155 MVI   RCODE,X'04'                                         0157 00867000
*   /*****************************************************************/ 00868000
*   /*                                                               */ 00869000
*   /* NO AVAILABLE PATHS EXIST TO THE DEVICE.. SET RCODE 0 DEVICE   */ 00870000
*   /* NOT DEPENDENT UPON SPECIFIED CPU                              */ 00871000
*   /*                                                               */ 00872000
*   /*****************************************************************/ 00873000
*                                                                  0158 00874000
*   ELSE                            /* NO AVALIABLE PATHS            */ 00875000
*     RCODE=0;                      /* PATH NOT DEPENDENT ON CPU     */ 00876000
         B     @RC00145                                            0158 00877000
@RF00145 MVI   RCODE,X'00'                                         0158 00878000
*   END;                            /* END OF CPU LEVEL CHECK        */ 00879000
@EL00003 DS    0H                                                  0159 00880000
@EF00003 DS    0H                                                  0159 00881000
@ER00003 LM    @14,@12,12(@13)                                     0159 00882000
         BR    @14                                                 0159 00883000
*CHNTEST:                                                          0160 00884000
*   PROC;                                                          0160 00885000
*                                                                  0160 00886000
CHNTEST  STM   @14,@12,12(@13)                                     0160 00887000
*/****************************************************************** */ 00888000
*/* CHNTEST ROUTINE WILL DETERMINE IF A CPU/CHANNEL COMBINATION IS   */ 00889000
*/* INVOLVED IN A PATH TO A SPECIFIC DEVICE. RETURN CODE ZERO        */ 00890000
*/* INDICATES THAT THE CHANNEL IS INVOLVED IN THE PATH, RETURN CODE  */ 00891000
*/* FOUR INDICATES THAT THE CHANNEL IS NOT INVOLVED IN THE PATH.     */ 00892000
*/****************************************************************** */ 00893000
*                                                                  0161 00894000
*   /*****************************************************************/ 00895000
*   /*                                                               */ 00896000
*   /* CHECK FOR VALID CPU ADDRESS. ACCEPT ONLY 0 OR 1               */ 00897000
*   /*                                                               */ 00898000
*   /*****************************************************************/ 00899000
*                                                                  0161 00900000
*   IF CPUID>1 THEN                 /* VALID?                        */ 00901000
         L     @10,@PC00001                                        0161 00902000
         CLI   CPUID(@10),1                                        0161 00903000
         BNH   @RF00161                                            0161 00904000
*     DO;                                                          0162 00905000
*       RCODE=16;                   /* REASON CODE                   */ 00906000
         MVI   RCODE,X'10'                                         0163 00907000
*       ABIND=1;                    /* SET ABEND FLAG                */ 00908000
         MVI   ABIND,X'01'                                         0164 00909000
*     END;                                                         0165 00910000
*                                                                  0165 00911000
*   /*****************************************************************/ 00912000
*   /*                                                               */ 00913000
*   /* VALID CPU ADDRESS. CONTINUE                                   */ 00914000
*   /*                                                               */ 00915000
*   /*****************************************************************/ 00916000
*                                                                  0166 00917000
*   ELSE                                                           0166 00918000
*                                                                  0166 00919000
*     /***************************************************************/ 00920000
*     /*                                                             */ 00921000
*     /* CHECK WHICH CPU IS TO BE TESTED                             */ 00922000
*     /*                                                             */ 00923000
*     /***************************************************************/ 00924000
*                                                                  0166 00925000
*     IF CPUID=0 THEN               /* CPU ZERO?                     */ 00926000
*                                                                  0166 00927000
         B     @RC00161                                            0166 00928000
@RF00161 L     @10,@PC00001                                        0166 00929000
         CLI   CPUID(@10),0                                        0166 00930000
         BNE   @RF00166                                            0166 00931000
*       /*************************************************************/ 00932000
*       /*                                                           */ 00933000
*       /* DETERMINE IF A PATH EXISTS USING THE SPECIFIED CHANNEL    */ 00934000
*       /*                                                           */ 00935000
*       /*************************************************************/ 00936000
*                                                                  0167 00937000
*       IF CHNID=CHN00&PTH00='01'X  /* PATH AVAILABLE ON FIRST?      */ 00938000
*           |CHNID=CHN10&PTH10='01'X THEN/* PATH EXIST?              */ 00939000
         CLC   CHNID(1,@10),CHN00                                  0167 00940000
         BNE   @GL00023                                            0167 00941000
         CLI   PTH00,X'01'                                         0167 00942000
         BE    @RT00167                                            0167 00943000
@GL00023 L     @10,@PC00001                                        0167 00944000
         CLC   CHNID(1,@10),CHN10                                  0167 00945000
         BNE   @RF00167                                            0167 00946000
         CLI   PTH10,X'01'                                         0167 00947000
         BNE   @RF00167                                            0167 00948000
@RT00167 DS    0H                                                  0168 00949000
*         RCODE=0;                  /* YES, SET RETURN CODE 0        */ 00950000
*                                                                  0168 00951000
         MVI   RCODE,X'00'                                         0168 00952000
*   /*****************************************************************/ 00953000
*   /*                                                               */ 00954000
*   /* NO PATHS EXIST. SET RETURN CODE                               */ 00955000
*   /*                                                               */ 00956000
*   /*****************************************************************/ 00957000
*                                                                  0169 00958000
*       ELSE                                                       0169 00959000
*         RCODE=4;                  /* SET RETURN CODE 4             */ 00960000
*                                                                  0169 00961000
         B     @RC00167                                            0169 00962000
@RF00167 MVI   RCODE,X'04'                                         0169 00963000
*   /*****************************************************************/ 00964000
*   /*                                                               */ 00965000
*   /* CPUID WAS NOT ZERO, MUST THEN BE 1                            */ 00966000
*   /*                                                               */ 00967000
*   /*****************************************************************/ 00968000
*                                                                  0170 00969000
*     ELSE                                                         0170 00970000
*                                                                  0170 00971000
*       /*************************************************************/ 00972000
*       /*                                                           */ 00973000
*       /* CHECK IF A PATH EXISTS USING SAME CHANNEL, OTHER CPU      */ 00974000
*       /*                                                           */ 00975000
*       /*************************************************************/ 00976000
*                                                                  0170 00977000
*       IF CHNID=CHN00&PTH01='01'X|CHNID=CHN10&PTH11='01'X THEN/*  0170 00978000
*                                      PATH?                         */ 00979000
         B     @RC00166                                            0170 00980000
@RF00166 L     @10,@PC00001                                        0170 00981000
         CLC   CHNID(1,@10),CHN00                                  0170 00982000
         BNE   @GL00026                                            0170 00983000
         CLI   PTH01,X'01'                                         0170 00984000
         BE    @RT00170                                            0170 00985000
@GL00026 L     @10,@PC00001                                        0170 00986000
         CLC   CHNID(1,@10),CHN10                                  0170 00987000
         BNE   @RF00170                                            0170 00988000
         CLI   PTH11,X'01'                                         0170 00989000
         BNE   @RF00170                                            0170 00990000
@RT00170 DS    0H                                                  0171 00991000
*         RCODE=0;                  /* SET RETURN CODE ZERO          */ 00992000
         MVI   RCODE,X'00'                                         0171 00993000
*       ELSE                                                       0172 00994000
*         RCODE=4;                  /* SET RETURN CODE FOUR          */ 00995000
         B     @RC00170                                            0172 00996000
@RF00170 MVI   RCODE,X'04'                                         0172 00997000
*   END;                            /* END OF CHANNEL TEST ROUTINE   */ 00998000
@EL00004 DS    0H                                                  0173 00999000
@EF00004 DS    0H                                                  0173 01000000
@ER00004 LM    @14,@12,12(@13)                                     0173 01001000
         BR    @14                                                 0173 01002000
*CPUTEST:                                                          0174 01003000
*   PROC;                                                          0174 01004000
*                                                                  0174 01005000
CPUTEST  STM   @14,@12,12(@13)                                     0174 01006000
*/****************************************************************** */ 01007000
*/* CPU TEST WILL DETERMINE IF THE SPECIFIED CPU IS PART OF ANY PATH */ 01008000
*/* TO THE SPECIFIED DEVICE, AND INFORM THE CALLER OF SUCH.          */ 01009000
*/****************************************************************** */ 01010000
*                                                                  0175 01011000
*   /*****************************************************************/ 01012000
*   /*                                                               */ 01013000
*   /* CHECK FOR VALID CPU ADDRESS.                                  */ 01014000
*   /*                                                               */ 01015000
*   /*****************************************************************/ 01016000
*                                                                  0175 01017000
*   IF CPUID>1 THEN                 /* VALID?                        */ 01018000
         L     @10,@PC00001                                        0175 01019000
         CLI   CPUID(@10),1                                        0175 01020000
         BNH   @RF00175                                            0175 01021000
*     DO;                                                          0176 01022000
*       RCODE=16;                   /* REASON CODE                   */ 01023000
         MVI   RCODE,X'10'                                         0177 01024000
*       ABIND=1;                    /* SET ABEND FLAG                */ 01025000
         MVI   ABIND,X'01'                                         0178 01026000
*     END;                                                         0179 01027000
*                                                                  0179 01028000
*   /*****************************************************************/ 01029000
*   /*                                                               */ 01030000
*   /* VALID CPU ADDRESS, CONTINUE                                   */ 01031000
*   /*                                                               */ 01032000
*   /*****************************************************************/ 01033000
*                                                                  0180 01034000
*   ELSE                                                           0180 01035000
*                                                                  0180 01036000
*     /***************************************************************/ 01037000
*     /*                                                             */ 01038000
*     /* CHECK WHICH CPU TO TEST.                                    */ 01039000
*     /*                                                             */ 01040000
*     /***************************************************************/ 01041000
*                                                                  0180 01042000
*     IF CPUID=0 THEN               /* CPU ZERO?                     */ 01043000
*                                                                  0180 01044000
         B     @RC00175                                            0180 01045000
@RF00175 L     @10,@PC00001                                        0180 01046000
         CLI   CPUID(@10),0                                        0180 01047000
         BNE   @RF00180                                            0180 01048000
*       /*************************************************************/ 01049000
*       /*                                                           */ 01050000
*       /* CHECK FOR ANY PATH FROM CPU 0                             */ 01051000
*       /*                                                           */ 01052000
*       /*************************************************************/ 01053000
*                                                                  0181 01054000
*       IF PTH00='01'X|PTH10='01'X THEN/* ANY PATHS?                 */ 01055000
         CLI   PTH00,X'01'                                         0181 01056000
         BE    @RT00181                                            0181 01057000
         CLI   PTH10,X'01'                                         0181 01058000
         BNE   @RF00181                                            0181 01059000
@RT00181 DS    0H                                                  0182 01060000
*         RCODE=0;                  /* SET RETURN CODE ZERO          */ 01061000
*                                                                  0182 01062000
         MVI   RCODE,X'00'                                         0182 01063000
*   /*****************************************************************/ 01064000
*   /*                                                               */ 01065000
*   /* NO PATHS EXIST. SET RETURN CODE                               */ 01066000
*   /*                                                               */ 01067000
*   /*****************************************************************/ 01068000
*                                                                  0183 01069000
*       ELSE                                                       0183 01070000
*         RCODE=4;                  /* SET RETURN CODE FOUR          */ 01071000
*                                                                  0183 01072000
         B     @RC00181                                            0183 01073000
@RF00181 MVI   RCODE,X'04'                                         0183 01074000
*   /*****************************************************************/ 01075000
*   /*                                                               */ 01076000
*   /* CPUID WAS NOT ZERO, MUST THEN BE ONE                          */ 01077000
*   /*                                                               */ 01078000
*   /*****************************************************************/ 01079000
*                                                                  0184 01080000
*     ELSE                                                         0184 01081000
*                                                                  0184 01082000
*       /*************************************************************/ 01083000
*       /*                                                           */ 01084000
*       /* CHECK IF ANY AVAILABLE PATHS EXIST ON CPU 1               */ 01085000
*       /*                                                           */ 01086000
*       /*************************************************************/ 01087000
*                                                                  0184 01088000
*       IF PTH01='01'X|PTH11='01'X THEN/* ANY PATHS?                 */ 01089000
         B     @RC00180                                            0184 01090000
@RF00180 CLI   PTH01,X'01'                                         0184 01091000
         BE    @RT00184                                            0184 01092000
         CLI   PTH11,X'01'                                         0184 01093000
         BNE   @RF00184                                            0184 01094000
@RT00184 DS    0H                                                  0185 01095000
*         RCODE=0;                  /* SET RETURN CODE ZERO          */ 01096000
*                                                                  0185 01097000
         MVI   RCODE,X'00'                                         0185 01098000
*   /*****************************************************************/ 01099000
*   /*                                                               */ 01100000
*   /* NO PATHS EXIST. SET RETURN CODE                               */ 01101000
*   /*                                                               */ 01102000
*   /*****************************************************************/ 01103000
*                                                                  0186 01104000
*       ELSE                                                       0186 01105000
*         RCODE=4;                  /* SET RETURN CODE FOUR          */ 01106000
         B     @RC00184                                            0186 01107000
@RF00184 MVI   RCODE,X'04'                                         0186 01108000
*   END;                            /* END OF CPU TEST               */ 01109000
@EL00005 DS    0H                                                  0187 01110000
@EF00005 DS    0H                                                  0187 01111000
@ER00005 LM    @14,@12,12(@13)                                     0187 01112000
         BR    @14                                                 0187 01113000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 01114000
