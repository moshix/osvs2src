*     END;                                                         0114 00431000
*                                                                  0114 00432000
*   /*****************************************************************/ 00433000
*   /*                                                               */ 00434000
*   /* LOAD RETURN CODE AND PASS TO CALLER                           */ 00435000
*   /*                                                               */ 00436000
*   /*****************************************************************/ 00437000
*                                                                  0115 00438000
*   ELSE                            /* OTHERWISE PASS RETURN         */ 00439000
*     DO;                                                          0115 00440000
*                                                                  0115 00441000
         B     @RC00103                                            0115 00442000
@RF00103 DS    0H                                                  0116 00443000
*       /*************************************************************/ 00444000
*       /*                                                           */ 00445000
*       /* PICK UP ALL INFORMATION OUT OF WORK AREA, CHANGE BACK TO  */ 00446000
*       /* CALLERS KEY, AND ISSUE FREEMAIN                           */ 00447000
*       /*                                                           */ 00448000
*       /*************************************************************/ 00449000
*                                                                  0116 00450000
*       GENERATE;                                                  0116 00451000
     SLR    @10,@10                                                     00452000
     IC     @10,RCODE                                                   00453000
     L      @13,4(,@13)                                                 00454000
     L      @00,@SIZDATD                                                00455000
     LR     @01,@11                                                     00456000
     SPKA   0(@02)                                       YM08605        00457000
     FREEMAIN R,LV=(0),A=(1)                                            00458000
     LR     @15,@10                                                     00459000
     L      @14,12(,@13)                                                00460000
     LM     @00,@12,20(@13)                                             00461000
     BR     @14                                                         00462000
*     END;                                                         0117 00463000
*CHNLEVEL:                                                         0118 00464000
*   PROC OPTIONS(NOSAVEAREA);       /* CHANNEL LEVEL PROCEDURE       */ 00465000
@EL00001 L     @13,4(,@13)                                         0118 00466000
@EF00001 L     @00,@SIZDATD                                        0118 00467000
         LR    @01,@11                                             0118 00468000
         FREEMAIN R,LV=(0),A=(1)                                        00469000
@ER00001 LM    @14,@12,12(@13)                                     0118 00470000
         BR    @14                                                 0118 00471000
CHNLEVEL STM   @14,@12,12(@13)                                     0118 00472000
*/***************************************************************    */ 00473000
*/*  CHANNEL LEVEL ROUTINE WILL DETERMINE IF A PATH EXISTS           */ 00474000
*/*  TO THE REFERENCED DEVICE USING A CHANNEL OTHER THAN THE         */ 00475000
*/*  ONE THE CALLER HAS SPECIFIED IN THE INPUT PARAMETERS            */ 00476000
*/*                                                                  */ 00477000
*/*  A RETURN CODE OF 00 INDICATES THAT ANOTHER PATH DOES EXIST      */ 00478000
*/*  A RETURN CODE OF 04 INDICATES THAT ANOTHER PATH DOESNOT EXIST   */ 00479000
*/***************************************************************    */ 00480000
*                                                                  0119 00481000
*   /*****************************************************************/ 00482000
*   /*                                                               */ 00483000
*   /* CHECK IF A AVAILABLE PATH EXISTS ANYWHERE FOR THIS DEVICE BY  */ 00484000
*   /* INTEROGATING THE PATHMAP                                      */ 00485000
*   /*                                                               */ 00486000
*   /*****************************************************************/ 00487000
*                                                                  0119 00488000
*   IF PTH00='01'X|PTH01='01'X|     /* CHECK FOR AN AVAILABLE        */ 00489000
*       PTH10='01'X|PTH11='01'X THEN/* PATH ON ANY CHANNEL           */ 00490000
*                                                                  0119 00491000
         CLI   PTH00,X'01'                                         0119 00492000
         BE    @RT00119                                            0119 00493000
         CLI   PTH01,X'01'                                         0119 00494000
         BE    @RT00119                                            0119 00495000
         CLI   PTH10,X'01'                                         0119 00496000
         BE    @RT00119                                            0119 00497000
         CLI   PTH11,X'01'                                         0119 00498000
         BNE   @RF00119                                            0119 00499000
@RT00119 DS    0H                                                  0120 00500000
*     /***************************************************************/ 00501000
*     /*                                                             */ 00502000
*     /* CHECK IF THE CHANNEL IN QUESTION IS REPRESENTED IN THE PATH */ 00503000
*     /* MAP FOR THIS DEVICE                                         */ 00504000
*     /*                                                             */ 00505000
*     /***************************************************************/ 00506000
*                                                                  0120 00507000
*     IF CHNID=CHN00|CHNID=CHN10 THEN/* CHANNEL ID MATCH MAP ?       */ 00508000
*                                                                  0120 00509000
         L     @10,@PC00001                                        0120 00510000
         CLC   CHNID(1,@10),CHN00                                  0120 00511000
         BE    @RT00120                                            0120 00512000
         CLC   CHNID(1,@10),CHN10                                  0120 00513000
         BNE   @RF00120                                            0120 00514000
@RT00120 DS    0H                                                  0121 00515000
*       /*************************************************************/ 00516000
*       /*                                                           */ 00517000
*       /* A PATH DOES EXIST.. VERIFY THAT THE CPU ADDRESS PASSED IS */ 00518000
*       /* VALID AND IF NOT SET ABEND INDICATOR                      */ 00519000
*       /*                                                           */ 00520000
*       /*************************************************************/ 00521000
*                                                                  0121 00522000
*       IF CPUID=>2 THEN            /* CPU ID VALID?                 */ 00523000
         L     @10,@PC00001                                        0121 00524000
         CLI   CPUID(@10),2                                        0121 00525000
         BL    @RF00121                                            0121 00526000
*         DO;                                                      0122 00527000
*           RCODE=16;               /* SET REASON CODE               */ 00528000
         MVI   RCODE,X'10'                                         0123 00529000
*           ABIND=1;                /* SET ABEND INDICATOR           */ 00530000
         MVI   ABIND,X'01'                                         0124 00531000
*         END;                                                     0125 00532000
*                                                                  0125 00533000
*   /*****************************************************************/ 00534000
*   /*                                                               */ 00535000
*   /* CPU ADDRESS IS VALID                                          */ 00536000
*   /*                                                               */ 00537000
*   /*****************************************************************/ 00538000
*                                                                  0126 00539000
*       ELSE                        /* OTHERWISE                     */ 00540000
*                                                                  0126 00541000
*         /***********************************************************/ 00542000
*         /*                                                         */ 00543000
*         /* DETERMINE WHICH CPU CONTAINS THE CHANNEL IN QUESTION    */ 00544000
*         /*                                                         */ 00545000
*         /***********************************************************/ 00546000
*                                                                  0126 00547000
*         IF CPUID=0 THEN           /* IS THIS CPU ZERO              */ 00548000
*                                                                  0126 00549000
         B     @RC00121                                            0126 00550000
@RF00121 L     @10,@PC00001                                        0126 00551000
         CLI   CPUID(@10),0                                        0126 00552000
         BNE   @RF00126                                            0126 00553000
*           /*********************************************************/ 00554000
*           /*                                                       */ 00555000
*           /* CHANNEL IS ON FIRST CPU.. CHECK IF CHANNEL IS FIRST   */ 00556000
*           /* CHANNEL REPRESENTED IN PATHMAP                        */ 00557000
*           /*                                                       */ 00558000
*           /*********************************************************/ 00559000
*                                                                  0127 00560000
*           IF CHNID=CHN00 THEN     /* FIRST CHANNEL BEING REMOVED   */ 00561000
*                                                                  0127 00562000
         CLC   CHNID(1,@10),CHN00                                  0127 00563000
         BNE   @RF00127                                            0127 00564000
*             /*******************************************************/ 00565000
*             /*                                                     */ 00566000
*             /* FIRST CHANNEL ON FIRST CPU BEING REMOVED... CHECK IF*/ 00567000
*             /* PATH EXISTS ON ANY OTHER CHANNEL IF SO SET RCODE 0  */ 00568000
*             /*                                                     */ 00569000
*             /*******************************************************/ 00570000
*                                                                  0128 00571000
*             IF PTH01='01'X|PTH10='01'X/* IS A PATH AVAILABLE ON ANY*/ 00572000
*                 |PTH11='01'X THEN /* OTHER CHANNEL                 */ 00573000
         CLI   PTH01,X'01'                                         0128 00574000
         BE    @RT00128                                            0128 00575000
         CLI   PTH10,X'01'                                         0128 00576000
         BE    @RT00128                                            0128 00577000
         CLI   PTH11,X'01'                                         0128 00578000
         BNE   @RF00128                                            0128 00579000
@RT00128 DS    0H                                                  0129 00580000
*               RCODE=0;            /* SET ZERO RETURN CODE          */ 00581000
*                                                                  0129 00582000
         MVI   RCODE,X'00'                                         0129 00583000
*   /*****************************************************************/ 00584000
*   /*                                                               */ 00585000
*   /* NO OTHER PATH EXISTS.. SET RCODE 4                            */ 00586000
*   /*                                                               */ 00587000
*   /*****************************************************************/ 00588000
*                                                                  0130 00589000
