         TITLE 'IEEVDEV DEVICE SUBROUTINE MAINLINE                     *00001000
                        '                                               00002000
IEEVDEV  CSECT                                                          00003000
@MAINENT DS    0H                                                       00004000
         USING  *,15                                                    00005000
IEEVDEV  MODID  BR=YES                                                  00006000
@PROLOG  STM    @14,@12,12(@13)                                         00007000
         BALR   @12,0                                                   00008000
@PSTART  DS     0H                                                      00009000
         USING  @PSTART,@12                                             00010000
         L      @00,@SIZDATD                                            00011000
         GETMAIN R,LV=(0)                                               00012000
         LR     @11,@01                                                 00013000
         USING  @DATD,@11                                               00014000
         IPK                           SAVE USERS KEY          YM07661  00015000
         SPKA   0(@00)                 CHANGE TO KEY 0         YM07661  00016000
         ST     @13,@SA00001+4                                          00017000
         LM     @00,@01,20(@13)                                         00018000
         ST     @11,8(@13)                                              00019000
         LR     @13,@11                                                 00020000
         MVC    @PC00001(4),0(@01)                                      00021000
*   UCBPTR=UCBADD;                  /* INITILIZE UCB POINTER         */ 00022000
         L     @15,@PC00001                                        0037 00023000
         L     UCBPTR,UCBADD(,@15)                                 0037 00024000
*   ABIND=0;                        /* RESET ABEND INDICATOR         */ 00025000
*                                                                  0038 00026000
         MVI   ABIND,X'00'                                         0038 00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* VERIFY THAT UCB ADDRESS POINTS TO UCB COMMON SEGEMENT BY      */ 00030000
*   /* CHECKING FOR UCB IDENTIFIER                                   */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                                  0039 00034000
*   IF UCBID^='FF'X THEN            /* CHECK FOR UCB IDENTIFIER      */ 00035000
*                                                                  0039 00036000
         CLI   UCBID(UCBPTR),X'FF'                                 0039 00037000
         BE    @RF00039                                            0039 00038000
*     /***************************************************************/ 00039000
*     /*                                                             */ 00040000
*     /* UCB ADDRESS PASSED DID NOT POINT TO UCB COMMON SEGEMENT SET */ 00041000
*     /* REASON CODE OF 4 AND ISSUE ABEND                            */ 00042000
*     /*                                                             */ 00043000
*     /***************************************************************/ 00044000
*                                                                  0040 00045000
*     DO;                                                          0040 00046000
*       RCODE=4;                    /* SET REASON CODE TO 4          */ 00047000
         MVI   RCODE,X'04'                                         0041 00048000
*       ABIND=1;                    /* SET ABEND ERROR FLAG          */ 00049000
         MVI   ABIND,X'01'                                         0042 00050000
*       GOTO ABEND1;                /* GO ISSUE ABEND INVALID ADDR   */ 00051000
         B     ABEND1                                              0043 00052000
*     END;                                                         0044 00053000
*                                                                  0044 00054000
*   /*****************************************************************/ 00055000
*   /*                                                               */ 00056000
*   /* SET UP REGISTERS FOR IOSGEN MAP FUNCTION                      */ 00057000
*   /*                                                               */ 00058000
*   /*****************************************************************/ 00059000
*                                                                  0045 00060000
*   R4=UCBPTR;                      /* ADDRESS OF UCB FROM PARM      */ 00061000
@RF00039 LR    R4,UCBPTR                                           0045 00062000
*   REGSAVE=SAVEPTR;                /* SAVE ADDR OF OUR SAVE AREA    */ 00063000
         ST    SAVEPTR,REGSAVE                                     0046 00064000
*   SAVEPTR=ADDR(REGSAVEA);         /* PTR TO SAVE AREA FOR IOSGEN   */ 00065000
         LA    SAVEPTR,REGSAVEA                                    0047 00066000
*   R5=ADDR(PATHMAP);               /* ADDRESS TO BUILD PATHMAP      */ 00067000
         LA    R5,PATHMAP                                          0048 00068000
*   R14=ADDR(ENDMAP);               /* ADDRESS FOR END OF BUILD      */ 00069000
         LA    R14,ENDMAP                                          0049 00070000
*   GEN REFS(UCBCHA,UCBCHM,UCBFL5,UCBLCI,UCBUA,CVTILCH,CVTCSD,CVTPCCAT, 00071000
*       PCCACPUA,PCCACAFM,PCCACAT,CSDCPUAL,CATFLG,LCHTCH,CVTPTR,UCBBGN, 00072000
*       UCBBASE,UCBWGT,CVTCRCA)(IOSGEN MAP,VAR=1);                 0050 00073000
         IOSGEN MAP,VAR=1                                               00074000
*ENDMAP:                                                           0051 00075000
*   SAVEPTR=REGSAVE;                /* RELOAD SAVEAREA POINTER       */ 00076000
ENDMAP   L     SAVEPTR,REGSAVE                                     0051 00077000
*   RFY                                                            0052 00078000
*    (R4,                                                          0052 00079000
*     R5,                                                          0052 00080000
*     R14,                                                         0052 00081000
*     SAVEPTR) UNRSTD;              /* RELEASE REGISTERS FOR USE     */ 00082000
*                                                                  0052 00083000
*   /*****************************************************************/ 00084000
*   /*                                                               */ 00085000
*   /* CHECK FUCNTION CODE PASSED AS A PARAMETER FOR FUNCTION CODE 00*/ 00086000
*   /*                                                               */ 00087000
*   /*****************************************************************/ 00088000
*                                                                  0053 00089000
*   IF FCODE=0 THEN                 /* CHECK FUNCTION CODE ZERO      */ 00090000
         L     @10,@PC00001                                        0053 00091000
         CLI   FCODE(@10),0                                        0053 00092000
         BNE   @RF00053                                            0053 00093000
*     DO;                                                          0054 00094000
*                                                                  0054 00095000
*       /*************************************************************/ 00096000
*       /*                                                           */ 00097000
*       /* IS FUNCTION CODE 00... CHECK IF DEVICE IS IN USE IN A     */ 00098000
*       /* OFFLINE CONDITION                                         */ 00099000
*       /*                                                           */ 00100000
*       /*************************************************************/ 00101000
*                                                                  0055 00102000
*       IF UCBNALOC='1'B THEN       /* IF OLTEP HAS DEVICE           */ 00103000
         TM    UCBNALOC(UCBPTR),B'00000100'                        0055 00104000
         BNO   @RF00055                                            0055 00105000
*         RCODE=16;                 /* SET OLTEP RETURN CODE         */ 00106000
*                                                                  0056 00107000
         MVI   RCODE,X'10'                                         0056 00108000
*       /*************************************************************/ 00109000
*       /*                                                           */ 00110000
*       /* DEVICE NOT IN USE WHILE OFFLINE CHECK FURTHER             */ 00111000
*       /*                                                           */ 00112000
*       /*************************************************************/ 00113000
*                                                                  0057 00114000
*       ELSE                        /* OTHERWISE CONTINUE            */ 00115000
*                                                                  0057 00116000
*         /***********************************************************/ 00117000
*         /*                                                         */ 00118000
*         /* DETERMINE IF DEVICE IS OFFLINE.. IF NOT THEN SET RETURN */ 00119000
*         /* CODE TWENTY AND RETURN TO CALLER                        */ 00120000
*         /*                                                         */ 00121000
*         /***********************************************************/ 00122000
*                                                                  0057 00123000
*         IF UCBONLI='1'B THEN      /* IF UCB IS NOT OFFLINE         */ 00124000
         B     @RC00055                                            0057 00125000
@RF00055 TM    UCBONLI(UCBPTR),B'10000000'                         0057 00126000
         BNO   @RF00057                                            0057 00127000
*           RCODE=20;               /* SET RETURN CODE OF TWENTY     */ 00128000
