         CLI   PTH11,X'01'                                         0135 00654000
         BNE   @RF00135                                            0135 00655000
@RT00135 DS    0H                                                  0136 00656000
*               RCODE=0;            /* PATH DOES NOT NEED THIS CHN   */ 00657000
*                                                                  0136 00658000
         MVI   RCODE,X'00'                                         0136 00659000
*   /*****************************************************************/ 00660000
*   /*                                                               */ 00661000
*   /* NO OTHER PATHS EXIST SET RCODE = 4                            */ 00662000
*   /*                                                               */ 00663000
*   /*****************************************************************/ 00664000
*                                                                  0137 00665000
*             ELSE                  /* PATH DOES NEED THIS CHANNEL   */ 00666000
*               RCODE=4;            /* THIS CHANNEL RCODE 4          */ 00667000
