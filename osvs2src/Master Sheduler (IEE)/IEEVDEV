@RT00131 DS    0H                                                  0132 00612000
*               RCODE=0;            /* IS.. NOT DEPENDENT ON CHAN    */ 00613000
*                                                                  0132 00614000
         MVI   RCODE,X'00'                                         0132 00615000
*   /*****************************************************************/ 00616000
*   /*                                                               */ 00617000
*   /* NO OTHER PATH EXISTS.. SET RCODE 4                            */ 00618000
*   /*                                                               */ 00619000
*   /*****************************************************************/ 00620000
*                                                                  0133 00621000
*             ELSE                  /* ONLY PATH IS ON THIS CHAN     */ 00622000
*               RCODE=4;            /* AVAILABLE RETURN CODE         */ 00623000
*                                                                  0133 00624000
         B     @RC00131                                            0133 00625000
@RF00131 MVI   RCODE,X'04'                                         0133 00626000
*   /*****************************************************************/ 00627000
*   /*                                                               */ 00628000
*   /* CHANNEL BEING REMOVED WAS NOT ON CPU1.. MUST BE CPU1 THEN..   */ 00629000
*   /* CHECK IF CHANNEL IS FIRST CHANNEL IN MAP OR SECOND            */ 00630000
*   /*                                                               */ 00631000
*   /*****************************************************************/ 00632000
*                                                                  0134 00633000
*         ELSE                      /* CHANNEL MUST BE ON CPU 1      */ 00634000
*           IF CHNID=CHN00 THEN     /* FIRST CHANNEL ON THIS CPU     */ 00635000
*                                                                  0134 00636000
         B     @RC00126                                            0134 00637000
@RF00126 L     @10,@PC00001                                        0134 00638000
         CLC   CHNID(1,@10),CHN00                                  0134 00639000
         BNE   @RF00134                                            0134 00640000
*             /*******************************************************/ 00641000
*             /*                                                     */ 00642000
*             /* FIRST CHANNEL IN MAP.. CHECK IF PATH EXISTS ON ANY  */ 00643000
*             /* OF THE OTHER THREE POSSABILITIES IF SO SET RCODE = 0*/ 00644000
*             /*                                                     */ 00645000
*             /*******************************************************/ 00646000
*                                                                  0135 00647000
