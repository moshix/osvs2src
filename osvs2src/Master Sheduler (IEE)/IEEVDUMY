IEEVDUMY CSECT ,                                                   0001 00004000
@MAINENT BALR  @15,0                                               0001 00005000
         USING *,@15                                               0001 00006000
         B     @PROLOG                                             0001 00007000
         DC    AL2(@EP00001-@MAINENT-2)                                 00008000
IEAVTACR BALR  @15,0                                               0001 00009000
         USING *,@15                                               0001 00010000
         B     @PROLOG                                             0001 00011000
         DC    AL2(@EP00002-IEAVTACR-2)                                 00012000
         ENTRY IEAVTACR                                                 00013000
IEAVTCR1 BALR  @15,0                                               0001 00014000
         USING *,@15                                               0001 00015000
         B     @PROLOG                                             0001 00016000
         DC    AL2(@EP00003-IEAVTCR1-2)                                 00017000
         ENTRY IEAVTCR1                                                 00018000
IGFPXMFA BALR  @15,0                                               0001 00019000
         USING *,@15                                               0001 00020000
         B     @PROLOG                                             0001 00021000
         DC    AL2(@EP00004-IGFPXMFA-2)                                 00022000
         ENTRY IGFPXMFA                                                 00023000
IGFPTAIM BALR  @15,0                                               0001 00024000
         USING *,@15                                               0001 00025000
         B     @PROLOG                                             0001 00026000
         DC    AL2(@EP00005-IGFPTAIM-2)                                 00027000
         ENTRY IGFPTAIM                                                 00028000
         DROP  @15                                                      00029000
@PROLOG  BALR  @12,0                                               0001 00030000
@PSTART  DS    0H                                                  0001 00031000
         USING @PSTART,@12                                         0001 00032000
         AH    @15,4(,@15)                                         0001 00033000
         BR    @15                                                 0001 00034000
@EP00001 DS    0H                                                  0002 00035000
*/*A-000000-999999                                            @Y02703*/ 00036000
