*     END;                                                         0095 00267000
*                                                                  0095 00268000
*   /*****************************************************************/ 00269000
*   /*                                                               */ 00270000
*   /* TEST FOR THE DATA TYPE AND SET THE APPROPRIATE CLASS BITS ON  */ 00271000
*   /* IN THE UCB. ALSO SET THE RESERVED BIT ON AND RETURN NOTE- THIS*/ 00272000
*   /* ROUTINE ONLY TESTS THE DATA TYPE AND THEN CALLS AN INTERNAL   */ 00273000
*   /* ROUTINE WHICH SETS THE BITS USING COMPARE AND SWAP. WE PROVIDE*/ 00274000
*   /* THE NECESSARY INFO TO COMPARE AND SWAP BY STORING IN FLAG1 THE*/ 00275000
*   /* BITS TO BE TURNED OFF AND STORING IN FLAG2 THE BITS TO BE     */ 00276000
*   /* TURNED ON                                                     */ 00277000
*   /*                                                               */ 00278000
*   /*****************************************************************/ 00279000
*                                                                  0096 00280000
*   IF INDATA(1:7)=PRIVATE THEN     /* IS THE DATA TYPE = PRIVATE    */ 00281000
@RF00090 CLC   INDATA(7,DATAPTR),@CC00080                          0096 00282000
         BNE   @RF00096                                            0096 00283000
*     DO;                           /* IF IT IS THEN                 */ 00284000
*       IF UCB3DACC=ON THEN         /* IF D/A THEN TURN OFF PUB AND     00285000
*                                      STORAGE BITS                  */ 00286000
         TM    UCB3DACC(UCBPTR),B'00100000'                        0098 00287000
         BNO   @RF00098                                            0098 00288000
*         FLAG1=PUBSTOR;            /* SET FLAG1 TO TURN OFF THE   0099 00289000
*                                      PUBLIC AND STORAGE BITS IN THE   00290000
*                                      UCB                           */ 00291000
         L     FLAG1,@CF00059                                      0099 00292000
*       ELSE                        /* FOR TAPE JUST TURN OFF THE  0100 00293000
*                                      PUBLIC BIT                    */ 00294000
*         FLAG1=PUBOFF;             /* FLAG1 WILL TURN OFF THE PUBLIC   00295000
*                                      BIT                           */ 00296000
         B     @RC00098                                            0100 00297000
@RF00098 L     FLAG1,@CF00067                                      0100 00298000
*       FLAG2=PVT;                  /* SET FLAG2 TO TURN ON THE    0101 00299000
*                                      PRIVATE BIT IN THE UCB        */ 00300000
@RC00098 L     FLAG2,@CF00055                                      0101 00301000
*       CALL SETBITS;               /* GO TO INTERNAL PROC WHICH SET    00302000
*                                      THE BITS IN THE UCB           */ 00303000
         BAL   @14,SETBITS                                         0102 00304000
*       RETURN;                     /* RETURN TO IEFSD263            */ 00305000
         B     @EL00001                                            0103 00306000
*     END;                                                         0104 00307000
*   ELSE                                                           0105 00308000
*     IF INDATA(1:6)=PUBLIC THEN    /* IS THE DATA TYPE = PUBLIC     */ 00309000
@RF00096 CLC   INDATA(6,DATAPTR),@CC00077                          0105 00310000
         BNE   @RF00105                                            0105 00311000
*       DO;                         /* IF IT IS THEN                 */ 00312000
*         IF UCB3DACC=ON THEN       /* IF D/A THEN TURN OFF PVT AND     00313000
*                                      STORAGE BITS                  */ 00314000
         TM    UCB3DACC(UCBPTR),B'00100000'                        0107 00315000
         BNO   @RF00107                                            0107 00316000
*           FLAG1=PVTSTOR;          /* SET FLAG1 TO TURN OFF THE   0108 00317000
*                                      PRIVATE AND STORAGE BITS IN 0108 00318000
*                                      THE UCB                       */ 00319000
         L     FLAG1,@CF00061                                      0108 00320000
*         ELSE                      /* FOR TAPE JUST TURN OFF THE  0109 00321000
