*   XAV=MOUNT;                      /* SET THE VERB TO MOUNT         */ 00490000
         MVC   XAV(8,XSAPTR),@CC00087                              0156 00491000
*   XAU=CIBCONID;                   /* SET CONSOLE ID                */ 00492000
         MVC   XAU(1,XSAPTR),CIBCONID(CIBPTR)                      0157 00493000
*   GEN(LINK EP=IEE0503D);                                         0158 00494000
         LINK EP=IEE0503D                                               00495000
*   R1=XSAPTR;                      /* SET THE PTR TO THE XSA IN R1     00496000
*                                      FOR FREEING                   */ 00497000
         LR    R1,XSAPTR                                           0159 00498000
*   R0=LENGTH(XASVRB);              /* SET LEN OF XSA FOR FREEING    */ 00499000
         LA    R0,48                                               0160 00500000
*   GEN(FREEMAIN R,LV=(0),A=(1));   /* FREE THE XSA                  */ 00501000
         FREEMAIN R,LV=(0),A=(1)                                        00502000
*   END ERR0503D;                                                  0162 00503000
