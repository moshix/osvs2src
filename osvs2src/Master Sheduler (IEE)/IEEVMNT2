         TITLE 'IEEVMNT2 - STC MOUNT COMMAND PROCESSOR                 *00001000
                        '                                               00002000
IEEVMNT2 CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(24)                                             0001 00007000
         DC    C'IEEVMNT2 77259  UZ14415 '                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
*                                                                  0040 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* SET UP BASED VARIABLES                                        */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                                  0040 00028000
*   R1PTR=R1;                       /* SAVE CONTENTS OF R1           */ 00029000
         LR    R1PTR,R1                                            0040 00030000
*   TIOTPTR=TCBTIO;                 /* LOCATE THE TIOT FROM THE TCB  */ 00031000
*                                                                  0041 00032000
         L     @10,CVTPTR                                          0041 00033000
         L     @10,CVTTCBP(,@10)                                   0041 00034000
         L     @10,TCBPTR(,@10)                                    0041 00035000
         L     TIOTPTR,TCBTIO(,@10)                                0041 00036000
*   /*****************************************************************/ 00037000
*   /*                                                               */ 00038000
*   /* FIND THE CIB - (1) GET CORE FOR THE EXTRACT REMOTE LIST. (2)  */ 00039000
*   /* R1 POINTS TO THE REMOTE LIST, THE FIRST WORD POINTS TO THE    */ 00040000
*   /* ANSWER AREA. AFTER THE EXTRACT, THE ANSWER AREA POINTS TO A   */ 00041000
*   /* COMMUNICATIONS AREA WHICH CONTAINS A PTR TO THE ECB, AND A    */ 00042000
*   /* POINTER TO THE CIB.                                           */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0042 00046000
*   GEN(GETMAIN R,LV=16,SP=0);      /* THE EXTRACT REMOTE LIST IS 16    00047000
*                                      BYTES LONG                    */ 00048000
         GETMAIN R,LV=16,SP=0                                           00049000
*   GETMNADD=R1;                    /* POINTER TO GOTTEN AREA        */ 00050000
         LR    GETMNADD,R1                                         0043 00051000
*   ERL=''B;                        /* CLEAR REMOTE LIST             */ 00052000
         XC    ERL(16,GETMNADD),ERL(GETMNADD)                      0044 00053000
*   AAPTR=ADDR(ANSAREA);            /* THE FIRST WORD IN THE REMOTE     00054000
*                                      LIST POINTS TO THE 4TH WORD 0045 00055000
*                                      WHICH WILL POINT TO THE COMM     00056000
*                                      AREA                          */ 00057000
         LA    @10,ANSAREA(,GETMNADD)                              0045 00058000
         ST    @10,AAPTR(,GETMNADD)                                0045 00059000
*   R1=GETMNADD;                    /* INSURE THAT R1 POINTS TO THE     00060000
*                                      GOTTEN AREA                   */ 00061000
         LR    R1,GETMNADD                                         0046 00062000
*   GEN(EXTRACT  ,FIELDS=COMM,MF=(E,(1)));/* ISSUE EXTRACT           */ 00063000
         EXTRACT  ,FIELDS=COMM,MF=(E,(1))                               00064000
*   CIBPTR=PTRTCIB;                 /* GET THE POINTER TO THE CIB    */ 00065000
         L     @10,ANSAREA(,GETMNADD)                              0048 00066000
         L     CIBPTR,PTRTCIB(,@10)                                0048 00067000
*   R1=GETMNADD;                    /* SET R1 POINTING TO THE EXTRACT   00068000
*                                      LIST FOR FREEING              */ 00069000
         LR    R1,GETMNADD                                         0049 00070000
*   GEN(FREEMAIN R,LV=16,A=(1));    /* FREE THE EXTRACT LIST         */ 00071000
*                                                                  0050 00072000
         FREEMAIN R,LV=16,A=(1)                                         00073000
*   /*****************************************************************/ 00074000
*   /*                                                               */ 00075000
*   /* LOCATE THE UCB FOR THE ALLOCATED DEVICE                       */ 00076000
*   /*                                                               */ 00077000
*   /*****************************************************************/ 00078000
*                                                                  0051 00079000
*   STOP='0'B;                      /* STOP IS SET TO CONTROL LOOPING*/ 00080000
         NI    STOP,B'01111111'                                    0051 00081000
*   DO WHILE(TIOELNGH^=0)&(STOP='0'B);/* SCAN THE TIOT ENTRY FOR DDNM   00082000
*                                      = IEFRDER                     */ 00083000
         B     @DE00052                                            0052 00084000
@DL00052 DS    0H                                                  0053 00085000
*     IF TIOEDDNM=IEFRDER THEN      /* IF DDNM = IEFRDER THEN THE UCB   00086000
*                                      HAS BEEN LOCATED              */ 00087000
         CLC   TIOEDDNM(8,TIOTPTR),@CC00085                        0053 00088000
         BNE   @RF00053                                            0053 00089000
*       DO;                                                        0054 00090000
*         UCBPTR=TIOEDEVE(1);       /* SET THE PTR TO THE UCB        */ 00091000
         L     UCBPTR,TIOEDEVE(,TIOTPTR)                           0055 00092000
*         STOP=ON;                  /* SET TO GET OUT OF LOOP        */ 00093000
         OI    STOP,B'10000000'                                    0056 00094000
*       END;                                                       0057 00095000
*     ELSE                          /* IF DDNM ^ = IEFRDER GO TO THE    00096000
*                                      NEXT ENTRY AND CONT- INUE THE    00097000
*                                      SEARCH                        */ 00098000
*       TIOTPTR=TIOTPTR+TIOELNGH;   /* UPDATE THE PTR TO THE NEXT  0058 00099000
*                                      ENTRY                         */ 00100000
         B     @RC00053                                            0058 00101000
@RF00053 SLR   @10,@10                                             0058 00102000
         IC    @10,TIOELNGH(,TIOTPTR)                              0058 00103000
         ALR   TIOTPTR,@10                                         0058 00104000
*   END;                                                           0059 00105000
*                                                                  0059 00106000
@RC00053 DS    0H                                                  0059 00107000
@DE00052 CLI   TIOELNGH(TIOTPTR),0                                 0059 00108000
         BE    @DC00052                                            0059 00109000
         TM    STOP,B'10000000'                                    0059 00110000
         BZ    @DL00052                                            0059 00111000
@DC00052 DS    0H                                                  0060 00112000
*   /*****************************************************************/ 00113000
*   /*                                                               */ 00114000
*   /* TEST TIOELNGH - IF EQUAL TO ZERO THEN THE UCB WAS NOT LOCATED */ 00115000
*   /* - AN ERROR HAS OCCURRED.                                      */ 00116000
*   /*                                                               */ 00117000
*   /*****************************************************************/ 00118000
*                                                                  0060 00119000
*   IF TIOELNGH=0 THEN              /* IF = 0 THEN SET UP FOR AN   0060 00120000
*                                      ERROR                         */ 00121000
         CLI   TIOELNGH(TIOTPTR),0                                 0060 00122000
         BNE   @RF00060                                            0060 00123000
*     DO;                                                          0061 00124000
*       ERRCODE=7;                  /* SET THE ERROR CODE USED BY THE   00125000
*                                      ERROR MSG MODULE              */ 00126000
         LA    ERRCODE,7                                           0062 00127000
*       CALL ERRVSMSG;              /* GO TO THE INTERNAL PROC WHICH    00128000
*                                      WILL SET UP FOR THE MSG MODULE   00129000
*                                      IEEVSMSG                      */ 00130000
         BAL   @14,ERRVSMSG                                        0063 00131000
*       RETURN;                     /* RETURN TO IEFSD263            */ 00132000
@EL00001 L     @13,4(,@13)                                         0064 00133000
@EF00001 L     @00,@SIZDATD                                        0064 00134000
         LR    @01,@11                                             0064 00135000
         FREEMAIN R,LV=(0),A=(1)                                        00136000
@ER00001 LM    @14,@12,12(@13)                                     0064 00137000
         BR    @14                                                 0064 00138000
*     END;                                                         0065 00139000
*                                                                  0065 00140000
*   /*****************************************************************/ 00141000
*   /*                                                               */ 00142000
*   /* TEST FOR TAPE OR DIRECT ACCESS - THESE ARE THE ONLY VALID     */ 00143000
*   /* DEVICES - IF OTHERS ERROR                                     */ 00144000
*   /*                                                               */ 00145000
*   /*****************************************************************/ 00146000
*                                                                  0066 00147000
*   IF(UCB3TAPE='0'B)&(UCB3DACC='0'B) THEN/* IF BOTH ARE OFF ERROR   */ 00148000
@RF00060 TM    UCB3TAPE(UCBPTR),B'10100000'                        0066 00149000
         BNZ   @RF00066                                            0066 00150000
*     DO;                                                          0067 00151000
