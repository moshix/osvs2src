*                                                                  0129 00381000
*   /*****************************************************************/ 00382000
*   /*                                                               */ 00383000
*   /* INTERNAL ROUTINE USED TO TURN ON REQUIRED CLASS BITS AND      */ 00384000
*   /* RESERVED BITS IN THE UCB                                      */ 00385000
*   /*                                                               */ 00386000
*   /*****************************************************************/ 00387000
*                                                                  0129 00388000
*SETBITS:                                                          0129 00389000
*   PROC;                                                          0129 00390000
*                                                                  0129 00391000
SETBITS  STM   @14,@12,12(@13)                                     0129 00392000
*   /*****************************************************************/ 00393000
*   /*                                                               */ 00394000
*   /* TURN ON THE RESERVED BIT IN THE UCB                           */ 00395000
*   /*                                                               */ 00396000
*   /*****************************************************************/ 00397000
*                                                                  0130 00398000
*   TEMP1=STATBIT;                  /* SET UP OPERAND 1              */ 00399000
         L     TEMP1,STATBIT(,UCBPTR)                              0130 00400000
*LOOP2:                                                            0131 00401000
*   TEMP2=TEMP1;                    /* SET UP OPERAND 2              */ 00402000
LOOP2    LR    TEMP2,TEMP1                                         0131 00403000
*   TEMP2=TEMP2|RES;                /* RES WILL TURN ON THE RESERVED    00404000
*                                      BIT                           */ 00405000
         O     TEMP2,@CF00051                                      0132 00406000
*   CS(TEMP1,TEMP2,STATBIT);        /* COMPARE AND SWAP              */ 00407000
         CS    TEMP1,@05,STATBIT(UCBPTR)                           0133 00408000
*   BC(4,LOOP2);                    /* IF NO SWAP GO BACK AND TRY  0134 00409000
*                                      AGAIN                         */ 00410000
*                                                                  0134 00411000
         BC    4,LOOP2                                             0134 00412000
*   /*****************************************************************/ 00413000
*   /*                                                               */ 00414000
*   /* TURN ON REQUIRED CLASS BITS IN THE UCB                        */ 00415000
*   /*                                                               */ 00416000
*   /*****************************************************************/ 00417000
*                                                                  0135 00418000
*   TEMP1=CLASSBIT;                 /* SET UP OPERAND 1              */ 00419000
         L     TEMP1,CLASSBIT(,UCBPTR)                             0135 00420000
*LOOP1:                                                            0136 00421000
*   TEMP2=TEMP1;                    /* SET UP OPERAND 2              */ 00422000
LOOP1    LR    TEMP2,TEMP1                                         0136 00423000
*   TEMP2=TEMP2&FLAG1;              /* FLAG1 HAS THE BITS TO TURN OFF*/ 00424000
         NR    TEMP2,FLAG1                                         0137 00425000
*   TEMP2=TEMP2|FLAG2;              /* FLAG2 HAS THE BIT TO TURN ON  */ 00426000
         OR    TEMP2,FLAG2                                         0138 00427000
*   CS(TEMP1,TEMP2,CLASSBIT);       /* COMPARE AND SWAP              */ 00428000
         CS    TEMP1,@05,CLASSBIT(UCBPTR)                          0139 00429000
*   BC(4,LOOP1);                    /* IF NO SWAP GO BACK AND TRY  0140 00430000
*                                      AGAIN                         */ 00431000
         BC    4,LOOP1                                             0140 00432000
*   END SETBITS;                                                   0141 00433000
@EL00002 DS    0H                                                  0141 00434000
@EF00002 DS    0H                                                  0141 00435000
@ER00002 LM    @14,@12,12(@13)                                     0141 00436000
         BR    @14                                                 0141 00437000
*                                                                  0142 00438000
*   /*****************************************************************/ 00439000
*   /*                                                               */ 00440000
*   /* INTERNAL PROC THAT WILL INITIALIZE A 3 WORD PARM LIST FOR     */ 00441000
*   /* IEEVSMSG AND CALL IEEVSMSG                                    */ 00442000
*   /*                                                               */ 00443000
*   /*****************************************************************/ 00444000
*                                                                  0142 00445000
*ERRVSMSG:                                                         0142 00446000
*   PROC;                                                          0142 00447000
ERRVSMSG STM   @14,@12,@SA00003                                    0142 00448000
*   SMSGPARM=''B;                   /* CLEAR PARM LIST               */ 00449000
         XC    SMSGPARM(12),SMSGPARM                               0143 00450000
*   SMSGCODE=ERRCODE;               /* SET THE MESSAGE CODE          */ 00451000
         ST    ERRCODE,SMSGCODE                                    0144 00452000
*   SMSGCONS=ADDR(CIBCONID);        /* SET PTR TO CONSOLE ID         */ 00453000
         LA    @10,CIBCONID(,CIBPTR)                               0145 00454000
         ST    @10,SMSGCONS                                        0145 00455000
*   SMSGLAST=ON;                    /* SET END OF PARM LIST          */ 00456000
         OI    SMSGLAST,B'10000000'                                0146 00457000
*   R1=ADDR(SMSGPARM);              /* POINT TO PARM LIST            */ 00458000
         LA    R1,SMSGPARM                                         0147 00459000
*   CALL IEEVSMSG;                  /* GO TO ERROR MSG MODULE        */ 00460000
         L     @15,@CV00099                                        0148 00461000
         BALR  @14,@15                                             0148 00462000
*   END ERRVSMSG;                                                  0149 00463000
@EL00003 DS    0H                                                  0149 00464000
@EF00003 DS    0H                                                  0149 00465000
@ER00003 LM    @14,@12,@SA00003                                    0149 00466000
         BR    @14                                                 0149 00467000
*                                                                  0150 00468000
*   /*****************************************************************/ 00469000
*   /*                                                               */ 00470000
*   /* INTERNAL PROC THAT WILL GET AND INITIALIZE AN EXTENDED SAVE   */ 00471000
*   /* AREA AND LINK TO IEE0503D NOTE IEE0503D DESTROYS REG0,1,3,4,5 */ 00472000
*   /*                                                               */ 00473000
*   /*****************************************************************/ 00474000
*                                                                  0150 00475000
*ERR0503D:                                                         0150 00476000
*   PROC;                                                          0150 00477000
ERR0503D STM   @14,@12,@SA00004                                    0150 00478000
*   R0=LENGTH(XASVRB);              /* SET THE LEN OF THE XSA        */ 00479000
         LA    R0,48                                               0151 00480000
*   GEN(GETMAIN R,LV=(0));          /* GET CORE FOR THE XSA MAPPING  */ 00481000
         GETMAIN R,LV=(0)                                               00482000
*   XSAPTR=R1;                      /* BASE THE XSA ON THE PTR     0153 00483000
*                                      RETURNED IN R1                */ 00484000
         LR    XSAPTR,R1                                           0153 00485000
*   XASVRB=''B;                     /* CLEAR THE XSA                 */ 00486000
         XC    XASVRB(48,XSAPTR),XASVRB(XSAPTR)                    0154 00487000
*   XAE=ERRCODE;                    /* SET THE MESSAGE CODE          */ 00488000
