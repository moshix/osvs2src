*                                      PUBLIC BIT IN THE UCB         */ 00328000
@RC00107 LA    FLAG2,2048                                          0110 00329000
*         CALL SETBITS;             /* GO TO INTERNAL PROC WHICH SET    00330000
*                                      THE BITS IN THE UCB           */ 00331000
         BAL   @14,SETBITS                                         0111 00332000
*         RETURN;                   /* RETURN TO IEFSD263            */ 00333000
         B     @EL00001                                            0112 00334000
*       END;                                                       0113 00335000
*     ELSE                                                         0114 00336000
*       IF INDATA(1:7)=STORAGE THEN /* IS THE DATA TYPE = STORAGE    */ 00337000
@RF00105 CLC   INDATA(7,DATAPTR),@CC00082                          0114 00338000
         BNE   @RF00114                                            0114 00339000
*         DO;                       /* IF IT IS THEN                 */ 00340000
*           FLAG1=PUBPVT;           /* SET FLAG1 TO TURN OFF THE   0116 00341000
*                                      PRIVATE AND PUBLIC BITS IN THE   00342000
*                                      UCB                           */ 00343000
         L     FLAG1,@CF00063                                      0116 00344000
*           FLAG2=STORE;            /* SET FLAG2 TO TURN ON THE    0117 00345000
*                                      STORAGE BIT IN THE UCB        */ 00346000
         LA    FLAG2,1024                                          0117 00347000
