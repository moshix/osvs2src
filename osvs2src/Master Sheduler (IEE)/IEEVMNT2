*       RETURN;                     /* RETURN TO IEFSD263            */ 00183000
         B     @EL00001                                            0076 00184000
*     END;                                                         0077 00185000
*                                                                  0077 00186000
*   /*****************************************************************/ 00187000
*   /*                                                               */ 00188000
*   /* TEST FOR CIB DATA AND PARM DATA - IF = 0 SET THE PVT AND      */ 00189000
*   /* RESERVED BITS ON IN THE UCB AND RETURN                        */ 00190000
*   /*                                                               */ 00191000
*   /*****************************************************************/ 00192000
*                                                                  0078 00193000
*   IF(CIBDATLN=0)&(PARMLEN=0) THEN                                0078 00194000
@RF00072 SLR   @10,@10                                             0078 00195000
         CH    @10,CIBDATLN(,CIBPTR)                               0078 00196000
         BNE   @RF00078                                            0078 00197000
         L     @06,PROCPTR(,R1PTR)                                 0078 00198000
         CH    @10,PARMLEN(,@06)                                   0078 00199000
         BNE   @RF00078                                            0078 00200000
*     DO;                                                          0079 00201000
*       IF UCB3DACC=ON THEN         /* IF D/A THEN TURN OFF PUB AND     00202000
*                                      STORAGE BITS                  */ 00203000
         TM    UCB3DACC(UCBPTR),B'00100000'                        0080 00204000
         BNO   @RF00080                                            0080 00205000
*         FLAG1=PUBSTOR;            /* FLAG1 WILL TURN OFF THE PUBLIC   00206000
