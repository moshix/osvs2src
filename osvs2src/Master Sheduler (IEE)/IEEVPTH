*         DO;                       /* YES, NOT A COMMA: MUST BE ')' */ 00828000
*           IF BUFFER(SCANPTR)^=')' THEN                           0290 00829000
         L     @08,SCANPTR                                         0290 00830000
         ALR   @09,@08                                             0290 00831000
         CLI   BUFFER-1(@09),C')'                                  0290 00832000
         BE    @RF00290                                            0290 00833000
*             SYNTXVAL='0'B;        /* NOT A RIGHT PAREN, ERROR      */ 00834000
*                                                                  0291 00835000
         NI    SYNTXVAL,B'11111101'                                0291 00836000
*           /*********************************************************/ 00837000
*           /*                                                       */ 00838000
*           /* IF NO CPU SPECIFIED, AND THERE IS MORE THAN ONE CPU ON*/ 00839000
*           /* LINE, THEN THE COMMAND HAS A SYNTAX ERROR. IF THERE IS*/ 00840000
*           /* ONLY ONE CPU ON LINE, THEN THE CPU ID DEFAULTS TO THAT*/ 00841000
*           /* CPU.                                                  */ 00842000
*           /*                                                       */ 00843000
*           /*********************************************************/ 00844000
*                                                                  0292 00845000
*           ELSE                    /* RIGHT PAREN, DETERMINE IF CPU    00846000
*                                      IS VALIDLY OMITTED.           */ 00847000
*             IF CSDCPUOL>1 THEN                                   0292 00848000
         B     @RC00290                                            0292 00849000
@RF00290 L     @09,CVTPTR                                          0292 00850000
         L     @09,CVTCSD(,@09)                                    0292 00851000
         LH    @09,CSDCPUOL(,@09)                                  0292 00852000
         CH    @09,@CH00042                                        0292 00853000
         BNH   @RF00292                                            0292 00854000
*               SYNTXVAL='0'B;      /* MORE THAN 1 ONLINE CPU, THE 0293 00855000
*                                      COMMAND HAD BAD SYNTAX.       */ 00856000
*                                                                  0293 00857000
         NI    SYNTXVAL,B'11111101'                                0293 00858000
*           /*********************************************************/ 00859000
*           /*                                                       */ 00860000
*           /* ONLY 1 CPU ONLINE, GET ITS CPU ID.                    */ 00861000
*           /*                                                       */ 00862000
*           /*********************************************************/ 00863000
*                                                                  0294 00864000
*             ELSE                                                 0294 00865000
*               LOGCPUID=PSACPUPA;  /* ONLY 1 CPU ONLINE.            */ 00866000
         B     @RC00292                                            0294 00867000
@RF00292 MVC   LOGCPUID(1),PSACPUPA+1                              0294 00868000
*           ENDCHARS=SCANPTR;       /* GET INDEX TO END OF (DDD,X)   */ 00869000
@RC00292 DS    0H                                                  0295 00870000
@RC00290 MVC   ENDCHARS(2),SCANPTR+2                               0295 00871000
*         END;                                                     0296 00872000
*                                                                  0296 00873000
*       /*************************************************************/ 00874000
*       /*                                                           */ 00875000
*       /* PROCESSING FOR A COMMAND WHICH HAS A CPU ID .             */ 00876000
*       /*                                                           */ 00877000
*       /*************************************************************/ 00878000
*                                                                  0297 00879000
*       ELSE                        /* CHARACTER WAS COMMA.          */ 00880000
*         DO;                                                      0297 00881000
         B     @RC00288                                            0297 00882000
@RF00288 DS    0H                                                  0298 00883000
*           SCANPTR=SCANPTR+1;      /* POINT TO NEXT CHARACTER.      */ 00884000
         LA    @09,1                                               0298 00885000
         AL    @09,SCANPTR                                         0298 00886000
         ST    @09,SCANPTR                                         0298 00887000
*           LOGCPUID=BUFFER(SCANPTR);/* LOGCPUID HAS CPU ID IN EBCDIC*/ 00888000
*                                                                  0299 00889000
         L     @08,CSCBPTR                                         0299 00890000
         ALR   @08,@09                                             0299 00891000
         MVC   LOGCPUID(1),BUFFER-1(@08)                           0299 00892000
*           /*********************************************************/ 00893000
*           /*                                                       */ 00894000
*           /* CHECK TO ENSURE THAT CPU ID IS VALID.                 */ 00895000
*           /*                                                       */ 00896000
*           /*********************************************************/ 00897000
*                                                                  0300 00898000
*           IF^(((LOGCPUID>='A')&   /* CPU ID > EBCDIC A, AND        */ 00899000
*               (LOGCPUID<='F'))|   /* LESS THAN EBCDIC F, OR        */ 00900000
*               ((LOGCPUID>='0')&   /* GREATER THAN EBCDIC 0, AND    */ 00901000
*               (LOGCPUID<='9')))   /* GREATER THAN EBCDIC 9 ?       */ 00902000
*             THEN                                                 0300 00903000
         CLI   LOGCPUID,C'A'                                       0300 00904000
         BL    @GL00010                                            0300 00905000
         CLI   LOGCPUID,C'F'                                       0300 00906000
         BNH   @RF00300                                            0300 00907000
@GL00010 CLI   LOGCPUID,C'0'                                       0300 00908000
         BL    @RT00300                                            0300 00909000
         CLI   LOGCPUID,C'9'                                       0300 00910000
         BNH   @RF00300                                            0300 00911000
@RT00300 DS    0H                                                  0301 00912000
*             SYNTXVAL='0'B;        /* NO, THEN SYNTAX IS INVALID.   */ 00913000
*                                                                  0301 00914000
         NI    SYNTXVAL,B'11111101'                                0301 00915000
*           /*********************************************************/ 00916000
*           /*                                                       */ 00917000
*           /* CPU ID IS VALID CHARACTER, NOW CONVERT IT TO HEX.     */ 00918000
*           /*                                                       */ 00919000
*           /*********************************************************/ 00920000
*                                                                  0302 00921000
*           ELSE                    /* CHARACTER IS VALID.           */ 00922000
*             DO;                   /* PERFORM THE CONVERSION.       */ 00923000
         B     @RC00300                                            0302 00924000
@RF00300 DS    0H                                                  0303 00925000
*               IF BUFFER(SCANPTR)>='0' THEN/* CONVERT CHARACTERS  0303 00926000
*                                      0-9.                          */ 00927000
         L     @09,CSCBPTR                                         0303 00928000
         L     @08,SCANPTR                                         0303 00929000
         ALR   @08,@09                                             0303 00930000
         CLI   BUFFER-1(@08),C'0'                                  0303 00931000
         BL    @RF00303                                            0303 00932000
*                 LOGCPUID=BUFFER(SCANPTR)-240;                    0304 00933000
         L     @08,SCANPTR                                         0304 00934000
         SLR   @05,@05                                             0304 00935000
         IC    @05,BUFFER-1(@08,@09)                               0304 00936000
         SH    @05,@CH03105                                        0304 00937000
         STC   @05,LOGCPUID                                        0304 00938000
*               ELSE                /* CONVERT CHARACTERS A-F.       */ 00939000
*                 LOGCPUID=BUFFER(SCANPTR)+9-192;                  0305 00940000
         B     @RC00303                                            0305 00941000
@RF00303 L     @09,CSCBPTR                                         0305 00942000
         L     @08,SCANPTR                                         0305 00943000
         SLR   @05,@05                                             0305 00944000
         IC    @05,BUFFER-1(@08,@09)                               0305 00945000
         SH    @05,@CH03144                                        0305 00946000
         STC   @05,LOGCPUID                                        0305 00947000
*               SCANPTR=SCANPTR+1;  /* POINT TO NEXT CHARACTER.      */ 00948000
@RC00303 LA    @09,1                                               0306 00949000
         AL    @09,SCANPTR                                         0306 00950000
         ST    @09,SCANPTR                                         0306 00951000
*               ENDCHARS=SCANPTR;   /* GET INDEX TO END OF (DDD,X)   */ 00952000
         STH   @09,ENDCHARS                                        0307 00953000
*             END;                  /* FINISHED PROCESSING CPU ID    */ 00954000
*         END;                      /* INCLUDING DEFAULT CASE.       */ 00955000
@RC00300 DS    0H                                                  0310 00956000
*       IF SYNTXVAL='1'B THEN       /* CONTINUE ONLY IF SYNTAX O.K.  */ 00957000
@RC00288 TM    SYNTXVAL,B'00000010'                                0310 00958000
         BNO   @RF00310                                            0310 00959000
*         DO;                       /* EXAMINE REST OF COMMAND.      */ 00960000
*           IF BUFFER(SCANPTR:SCANPTR+8)='),ONLINE ' THEN/* ONLINE 0312 00961000
*                                      REQUEST?                      */ 00962000
         L     @09,CSCBPTR                                         0312 00963000
         AL    @09,SCANPTR                                         0312 00964000
         CLC   BUFFER-1(9,@09),@CC03108                            0312 00965000
         BNE   @RF00312                                            0312 00966000
*             DO;                   /* YES, SET INDICATORS.          */ 00967000
*               ONLINREQ='1'B;      /* SET ONLINE FLAG.              */ 00968000
         OI    ONLINREQ,B'00001000'                                0314 00969000
*             END;                  /* PROCESSING FOR COMMAND DONE.  */ 00970000
*                                                                  0315 00971000
*           /*********************************************************/ 00972000
*           /*                                                       */ 00973000
*           /* IF VALID, MUST BE AN OFFLINE REQUEST.                 */ 00974000
*           /*                                                       */ 00975000
*           /*********************************************************/ 00976000
*                                                                  0316 00977000
*           ELSE                    /* DETERMINE TYPE OF OFFLINE   0316 00978000
*                                      REQUEST.                      */ 00979000
*             IF BUFFER(SCANPTR:SCANPTR+9)=/* OFFLINE, BUT NOT       */ 00980000
*                 '),OFFLINE ' THEN /* UNCONDITIONAL.                */ 00981000
         B     @RC00312                                            0316 00982000
@RF00312 L     @09,CSCBPTR                                         0316 00983000
         AL    @09,SCANPTR                                         0316 00984000
         CLC   BUFFER-1(10,@09),@CC03110                           0316 00985000
         BNE   @RF00316                                            0316 00986000
*               ONLINREQ='0'B;      /* SET OFFLINE FUNC FLAG         */ 00987000
*                                                                  0317 00988000
         NI    ONLINREQ,B'11110111'                                0317 00989000
*           /*********************************************************/ 00990000
*           /*                                                       */ 00991000
*           /* COULD BE SPECIFIED 'OFFLINE,UNCOND' OR                */ 00992000
*           /* 'UNCOND,OFFLINE' .                                    */ 00993000
*           /*                                                       */ 00994000
*           /*********************************************************/ 00995000
*                                                                  0318 00996000
*             ELSE                  /* DETERMINE IF OFFLINE AND    0318 00997000
*                                      UNCONDITIONAL REQUEST.        */ 00998000
*               IF BUFFER(SCANPTR:SCANPTR+16)=/* OFFLINE AND         */ 00999000
*                   '),OFFLINE,UNCOND '|/* UNCONDITIONAL REQUEST.    */ 01000000
*                   BUFFER(SCANPTR:SCANPTR+16)=/* ALSO, OFFLINE AND  */ 01001000
*                   '),UNCOND,OFFLINE ' THEN/* UNCONDITIONAL.        */ 01002000
         B     @RC00316                                            0318 01003000
@RF00316 L     @09,CSCBPTR                                         0318 01004000
         L     @08,SCANPTR                                         0318 01005000
         ALR   @08,@09                                             0318 01006000
         CLC   BUFFER-1(17,@08),@CC03112                           0318 01007000
         BE    @RT00318                                            0318 01008000
         L     @08,SCANPTR                                         0318 01009000
         ALR   @09,@08                                             0318 01010000
         CLC   BUFFER-1(17,@09),@CC03114                           0318 01011000
         BNE   @RF00318                                            0318 01012000
@RT00318 DS    0H                                                  0319 01013000
*                 DO;               /* EITHER WAY ALLOWED, SET FLAGS */ 01014000
*                                                                  0319 01015000
*                   /*************************************************/ 01016000
*                   /*                                               */ 01017000
*                   /* SET UP FOR UNCONDITIONAL OFFLINE              */ 01018000
*                   /*                                               */ 01019000
*                   /*************************************************/ 01020000
*                                                                  0320 01021000
*                   UNCOND='1'B;    /* SET PERMANANT- UNCOND REQUEST    01022000
*                                                            @G55AP2L*/ 01023000
         OI    UNCOND,B'00000100'                                  0320 01024000
*                   UNCONREQ='1'B;  /* SET FLAGS FOR UNCONDITIONAL.  */ 01025000
*                   ONLINREQ='0'B;  /* OFFLINE REQUEST               */ 01026000
         OI    UNCONREQ,B'00000100'                                0322 01027000
         NI    ONLINREQ,B'11110111'                                0322 01028000
*                 END;              /* PROCESSING FOR OFFLINE,     0323 01029000
*                                      UNCONDITIONAL REQUEST IS    0323 01030000
*                                      COMPLETE.                     */ 01031000
*                                                                  0323 01032000
*           /*********************************************************/ 01033000
*           /*                                                       */ 01034000
*           /* LAST ALTERNATIVE IS THAT THERE IS A SYNTAX ERROR.     */ 01035000
*           /*                                                       */ 01036000
*           /*********************************************************/ 01037000
*                                                                  0324 01038000
*               ELSE                /* SYNTAX MUST BE INVALID.       */ 01039000
*                 SYNTXVAL='0'B;    /* NO OTHER CONSTRUCTIONS ALLOWED*/ 01040000
         B     @RC00318                                            0324 01041000
@RF00318 NI    SYNTXVAL,B'11111101'                                0324 01042000
*         END;                                                     0325 01043000
@RC00318 DS    0H                                                  0325 01044000
@RC00316 DS    0H                                                  0325 01045000
@RC00312 DS    0H                                                  0326 01046000
*SYNTXOUT:                                                         0326 01047000
*       ;                           /*                       @G55AP2L*/ 01048000
@RF00310 DS    0H                                                  0326 01049000
SYNTXOUT DS    0H                                                  0327 01050000
*     END SYNTXCHK;                 /* END ROUTINE           @G55AP2L*/ 01051000
@EL00002 DS    0H                                                  0328 01052000
@EF00002 DS    0H                                                  0328 01053000
@ER00002 LM    @14,@12,12(@13)                                     0328 01054000
         BR    @14                                                 0328 01055000
*/* UCBCKOUT- OBTAIN THE ADDRESS OF THE UNIT CONTROL BLOCK REPRESENTED  01056000
*                                                                    */ 01057000
*/*           BY THE DEVICE ADDRESS, DETERMINE IF THE REQUEST CAN BE */ 01058000
*/*           UNCONDITIONAL.                                         */ 01059000
*/*  INPUT-                                                          */ 01060000
*/*        HEXDEVAD- DEVICE ADDRESS IN HEXADECIAML.                  */ 01061000
*/*        ONLINREQ- ON, COMMAND IS AN ON-LINE REQUEST.              */ 01062000
*/*        UCBONLI- ON, DEVICE IS ON-LINE.                           */ 01063000
*/*        UCBALOC- ON, UCB REPRESENTS AN ALLOCATED DEVICE WHICH IS  */ 01064000
*/*                     IN USE.                                      */ 01065000
*/*        UCBSYSR- ON, UCB REPRESENTS A CONSOLE.                    */ 01066000
*/*        UCBNALOC- ON, UCB REPRESENTS AN UNALLOCATED DEVICE WHICH IS  01067000
*                                                                    */ 01068000
*/*                      IN USE.                                     */ 01069000
*/*  OUTPUT-                                                         */ 01070000
*/*        UCBPTR- CONTAINS ADDRESS OF THE DEVICE'S UCB.             */ 01071000
*/*        VALIDPTH- OFF, UCB FOR PATH DOES NOT EXIST, WTO WILL BE   */ 01072000
*/*                       ISSUED.                                    */ 01073000
*/*        UNCONREQ- ON, PATH SHOULD BE VARY'ED UNCONDITIONALLY.     */ 01074000
*/*                                                                  */ 01075000
*                                                                  0329 01076000
*UCBCKOUT:                                                         0329 01077000
*   PROC;                                                          0329 01078000
*                                                                  0329 01079000
UCBCKOUT STM   @14,@03,@SA00003                                    0329 01080000
         STM   @05,@12,@SA00003+24                                 0329 01081000
*   /*****************************************************************/ 01082000
*   /*                                                               */ 01083000
*   /* PERFORM SERIALIZATION ON OTHER VARY PATH COMMANDS .           */ 01084000
*   /*                                                               */ 01085000
*   /*****************************************************************/ 01086000
*                                                                  0330 01087000
*   IF VARYENQ^='1'B THEN                                          0330 01088000
         TM    VARYENQ,B'00000010'                                 0330 01089000
         BO    @RF00330                                            0330 01090000
*     DO;                           /* ENQ ONLY IF RESOURCE NOT HELD    01091000
*                                                            @G55AP2L*/ 01092000
*       DO;                         /* ENQ (SYSZVARY,CPU,E,3,SYSTEM) */ 01093000
*         RESPECIFY                                                0333 01094000
*          (GPR01P) RESTRICTED;                                    0333 01095000
*         I056(1:16)=I056(1:16)&&I056(1:16);/* ZERO OUT THE TABLE    */ 01096000
         XC    I056(16),I056                                       0334 01097000
*         I05604(1)=3;              /* LENGTH OF MINOR RESOURCE NAME */ 01098000
         MVI   I05604,X'03'                                        0335 01099000
*         I05606(1)=ADDR(SYSZVARY); /* ADDRESS OF MAJOR RESOURCE NAME*/ 01100000
         LA    @05,SYSZVARY                                        0336 01101000
         ST    @05,I05606                                          0336 01102000
*         I05607(1)=ADDR(CPU);      /* ADDRESS OF MINOR RESOURCE NAME*/ 01103000
         LA    @05,CPU                                             0337 01104000
         ST    @05,I05607                                          0337 01105000
*         I05603(1)='01000000'B;    /* SET BIT STRING 1 FOR RESOURCE    01106000
*                                      ELEMENT                       */ 01107000
         MVI   I05603,B'01000000'                                  0338 01108000
*         I05605(1)='01000000'B;    /* SET BIT STRING 2 FOR RESOURCE    01109000
*                                      ELEMENT                       */ 01110000
         MVI   I05605,B'01000000'                                  0339 01111000
*         I05603(1,1)='1'B;         /* SET END OF LIST BIT           */ 01112000
         OI    I05603,B'10000000'                                  0340 01113000
*         GPR01P=ADDR(I05602);      /* LOAD LIST ADDRESS             */ 01114000
         LA    GPR01P,I05602                                       0341 01115000
*         SVC(56);                  /* ISSUE ENQ SVC                 */ 01116000
         SVC   56                                                  0342 01117000
*         RESPECIFY                                                0343 01118000
*          (GPR01P) UNRESTRICTED;                                  0343 01119000
*       END;                        /* ENQ (SYSZVARY,CPU,E,3,SYSTEM) */ 01120000
*       VARYENQ='1'B;               /* SET FLAG INDICATING ENQ.      */ 01121000
         OI    VARYENQ,B'00000010'                                 0345 01122000
*     END;                          /* END ENQ HELD TEST     @G55AP2L*/ 01123000
*                                                                  0346 01124000
*   /*****************************************************************/ 01125000
*   /*                                                               */ 01126000
*   /* WE WILL OBTAIN ACCESS TO THE UCBS VIA ENQ-SHARED ONLY FOR     */ 01127000
*   /* PLACING A PATH ONLINE (WHERE WE MIGHT SET UCBONLI TO ON). WE  */ 01128000
*   /* WILL OBTAIN ACCESS TO THE UCB'S VIA ENQ EXCLUSIVE FOR AN      */ 01129000
*   /* OFFLINE REQUEST, BUT WE WILL NOT LEAVE AN EXCLUSIVE (BUT      */ 01130000
*   /* UNFULFILLED) REQUEST IN ENQ'S QUEUES FOR MORE THAN FIVE       */ 01131000
*   /* SECONDS AT A TIME. AFTER 5 SECONDS WE WILL WITHDRAW THE ENQ   */ 01132000
*   /* REQUESTS (VIA DEQ) THEREBY ALLOWING ANY ENQ-SHARED REQUESTORS */ 01133000
*   /* (BELOW US IN THE QUEUE) TO GAIN ACCESS.                       */ 01134000
*   /*                                                               */ 01135000
*   /*****************************************************************/ 01136000
*                                                                  0347 01137000
*   IF UCBENQ^='1'B THEN                                           0347 01138000
@RF00330 TM    UCBENQ,B'00000100'                                  0347 01139000
         BO    @RF00347                                            0347 01140000
*     DO;                           /* ENQ ONLY IF RESOURCE NOT HELD    01141000
*                                                            @G55AP2L*/ 01142000
*       IF ONLINREQ='1'B THEN       /* ONLINE REQUEST?               */ 01143000
         TM    ONLINREQ,B'00001000'                                0349 01144000
         BNO   @RF00349                                            0349 01145000
*         DO;                       /* YES,                          */ 01146000
*           DO;                     /* ENQ (SYSIEFSD,Q4,S,2,SYSTEM)  */ 01147000
*             RESPECIFY                                            0352 01148000
*              (GPR01P) RESTRICTED;                                0352 01149000
*             I056(1:16)=I056(1:16)&&I056(1:16);/* ZERO OUT THE TABLE*/ 01150000
         XC    I056(16),I056                                       0353 01151000
*             I05604(1)=2;          /* LENGTH OF MINOR RESOURCE NAME */ 01152000
         MVI   I05604,X'02'                                        0354 01153000
*             I05606(1)=ADDR(SYSIEFSD);/* ADDRESS OF MAJOR RESOURCE     01154000
*                                      NAME                          */ 01155000
         LA    @05,SYSIEFSD                                        0355 01156000
         ST    @05,I05606                                          0355 01157000
*             I05607(1)=ADDR(Q4);   /* ADDRESS OF MINOR RESOURCE NAME*/ 01158000
         LA    @05,Q4                                              0356 01159000
         ST    @05,I05607                                          0356 01160000
*             I05603(1)='01000000'B;/* SET BIT STRING 1 FOR RESOURCE    01161000
*                                      ELEMENT                       */ 01162000
         MVI   I05603,B'01000000'                                  0357 01163000
*             I05605(1)='11000000'B;/* SET BIT STRING 2 FOR RESOURCE    01164000
*                                      ELEMENT                       */ 01165000
         MVI   I05605,B'11000000'                                  0358 01166000
*             I05603(1,1)='1'B;     /* SET END OF LIST BIT           */ 01167000
         OI    I05603,B'10000000'                                  0359 01168000
*             GPR01P=ADDR(I05602);  /* LOAD LIST ADDRESS             */ 01169000
         LA    GPR01P,I05602                                       0360 01170000
*             SVC(56);              /* ISSUE ENQ SVC                 */ 01171000
         SVC   56                                                  0361 01172000
*             RESPECIFY                                            0362 01173000
*              (GPR01P) UNRESTRICTED;                              0362 01174000
*           END;                    /* ENQ (SYSIEFSD,Q4,S,2,SYSTEM)  */ 01175000
*           UCBENQ='1'B;            /* SET ENQ HELD.         @G55AP2L*/ 01176000
         OI    UCBENQ,B'00000100'                                  0364 01177000
*         END;                                                     0365 01178000
*       ELSE                        /* NO, ATTEMPT ENQ EXCLUSIVE.    */ 01179000
*         DO;                                                      0366 01180000
         B     @RC00349                                            0366 01181000
@RF00349 DS    0H                                                  0367 01182000
*ENQAGAIN:                                                         0367 01183000
*           ;                                                      0367 01184000
ENQAGAIN DS    0H                                                  0368 01185000
*           EUCBECB=''B;            /* ZERO ECB FOR ENQ              */ 01186000
         XC    EUCBECB(4),EUCBECB                                  0368 01187000
*           DO;                     /* ENQ (SYSIEFSD,Q4,E,2,SYSTEM)     01188000
*                                      ECB(EUCBECB)                  */ 01189000
*             RESPECIFY                                            0370 01190000
*              (GPR01P) RESTRICTED;                                0370 01191000
*             I056(1:16)=I056(1:16)&&I056(1:16);/* ZERO OUT THE TABLE*/ 01192000
         XC    I056(16),I056                                       0371 01193000
*             I05604(1)=2;          /* LENGTH OF MINOR RESOURCE NAME */ 01194000
         MVI   I05604,X'02'                                        0372 01195000
*             I05606(1)=ADDR(SYSIEFSD);/* ADDRESS OF MAJOR RESOURCE     01196000
*                                      NAME                          */ 01197000
         LA    @05,SYSIEFSD                                        0373 01198000
         ST    @05,I05606                                          0373 01199000
*             I05607(1)=ADDR(Q4);   /* ADDRESS OF MINOR RESOURCE NAME*/ 01200000
         LA    @05,Q4                                              0374 01201000
         ST    @05,I05607                                          0374 01202000
*             I05603(1)='01000000'B;/* SET BIT STRING 1 FOR RESOURCE    01203000
*                                      ELEMENT                       */ 01204000
         MVI   I05603,B'01000000'                                  0375 01205000
*             I05605(1)='01000100'B;/* SET BIT STRING 2 FOR RESOURCE    01206000
*                                      ELEMENT                       */ 01207000
         MVI   I05605,B'01000100'                                  0376 01208000
*             I05601=ADDR(EUCBECB); /* ECB ADDRESS                   */ 01209000
         LA    @05,EUCBECB                                         0377 01210000
         ST    @05,I05601                                          0377 01211000
*             I05603(1,1)='1'B;     /* SET END OF LIST BIT           */ 01212000
         OI    I05603,B'10000000'                                  0378 01213000
*             GPR01P=ADDR(I05602);  /* LOAD LIST ADDRESS             */ 01214000
         LA    GPR01P,I05602                                       0379 01215000
*             SVC(56);              /* ISSUE ENQ SVC                 */ 01216000
         SVC   56                                                  0380 01217000
*             RESPECIFY                                            0381 01218000
*              (GPR01P) UNRESTRICTED;                              0381 01219000
*           END;                    /* ENQ (SYSIEFSD,Q4,E,2,SYSTEM)     01220000
*                                      ECB(EUCBECB)                  */ 01221000
*/*   MUST INDICATE THAT WE HAVE A REQUEST PENDING. WE DO THIS BY    */ 01222000
*/*   SETTING UCBENQ TO ON. THIS USUALLY MEANS THAT WE HAVE THE      */ 01223000
*/*   RESOURCE, BUT IN ALL CASES, UCBENQ SET TO ON INDICATES THAT    */ 01224000
*/*   THE RESOURCE SHOULD BE DEQ'ED (RELEASED) WHEN IEEVPTH FINISHES */ 01225000
*/*   NORMAL OR ABNORMAL PROCESSING. A DEQ WHEN WE DON'T HAVE THE RE-*/ 01226000
*/*   SOURCE WILL REMOVE THE REQUEST FROM THE REQUEST QUEUE.         */ 01227000
*                                                                  0383 01228000
*           UCBENQ='1'B;            /* HAVE REQUEST (PENDING) FOR  0383 01229000
*                                      UCB'S.                        */ 01230000
*                                                                  0383 01231000
         OI    UCBENQ,B'00000100'                                  0383 01232000
*           /*********************************************************/ 01233000
*           /*                                                       */ 01234000
*           /* REGISTER 15 CONTAINING ZERO MEANS THAT WE HAVE THE    */ 01235000
*           /* RESOURCE.                                             */ 01236000
*           /*                                                       */ 01237000
*           /*********************************************************/ 01238000
*                                                                  0384 01239000
*           IF REG15^=0 THEN        /* DID WE NOT GET RESOURCE?      */ 01240000
         LTR   REG15,REG15                                         0384 01241000
         BZ    @RF00384                                            0384 01242000
*             DO;                   /* YES,                          */ 01243000
*               GEN( STIMER WAIT,BINTVL=ASLEEP);/* SLEEP FOR 5     0386 01244000
*                                      SECONDS.                      */ 01245000
*                                                                  0386 01246000
          STIMER WAIT,BINTVL=ASLEEP                                     01247000
*               /*****************************************************/ 01248000
*               /*                                                   */ 01249000
*               /* WE HAVE BEEN AWAKENED.                            */ 01250000
*               /*                                                   */ 01251000
*               /*****************************************************/ 01252000
*                                                                  0387 01253000
*               IF POSTBIT='0'B THEN/* STILL DON'T HAVE RESOURCE?    */ 01254000
         TM    POSTBIT,B'01000000'                                 0387 01255000
         BNZ   @RF00387                                            0387 01256000
*                 DO;               /* YES,                          */ 01257000
*                   CALL DEQUCBS;   /* RELEASE UCB'S SO THAT THE   0389 01258000
*                                      ENQ-SHARES BELOW US CAN GET 0389 01259000
*                                      THEM.                         */ 01260000
         BAL   @14,DEQUCBS                                         0389 01261000
*                   GOTO ENQAGAIN;  /* GET BACK ON ENQ'S QUEUES, NOW    01262000
*                                      WE'LL BE BEHIND THE ENQ-SHARES   01263000
*                                      WHICH WE WERE FORMERLY AHEAD     01264000
*                                      OF.                           */ 01265000
         B     ENQAGAIN                                            0390 01266000
*                 END;                                             0391 01267000
*               ELSE                                               0392 01268000
*                 ;                 /* HAVE CONTROL OF RESOURCE.     */ 01269000
@RF00387 DS    0H                                                  0393 01270000
*             END;                                                 0393 01271000
*           ELSE                                                   0394 01272000
*             ;                     /* HAVE CONTROL OF RESOURCE.     */ 01273000
@RF00384 DS    0H                                                  0395 01274000
*         END;                                                     0395 01275000
*       RFY                                                        0396 01276000
*        (REG6,                                                    0396 01277000
*         REG7) RSTD;                                              0396 01278000
@RC00349 DS    0H                                                  0397 01279000
*     END;                          /* END ENQ HELD TEST     @G55AP2L*/ 01280000
*   REG6=HEXDEVAD;                  /* UCBLOOK FUNCTION REQUIRES HEX.   01281000
*                                      DEVICE @ IN REG. 6            */ 01282000
@RF00347 L     @05,VRYTPTR                                         0398 01283000
         LH    REG6,HEXDEVAD(,@05)                                 0398 01284000
*   SAVE13=REG13;                   /* SAVE OUR SAVE AREA PTR.       */ 01285000
         ST    REG13,SAVE13                                        0399 01286000
*   REG13=ADDR(INTSAVE);            /* GIVE IOSGEN ANOTHER SAVE AREA    01287000
*                                      TO USE.                       */ 01288000
*                                                                  0400 01289000
         LA    REG13,INTSAVE                                       0400 01290000
*   /*****************************************************************/ 01291000
*   /*                                                               */ 01292000
*   /* GET UCB ADDRESS                                               */ 01293000
*   /*                                                               */ 01294000
*   /*****************************************************************/ 01295000
*                                                                  0401 01296000
*   GEN REFS(CVTILK1,CVTILK2,CVTIXAVL,CVTIXAVL,CVTMAP,IOCOMEX,CVTPTR,   01297000
*       UCBOB,UCBCHAN,UCBFL5,UCBLCI,IOXHICH,/*               @YM06032*/ 01298000
*       CVTILCH,LCH,LCHTCH,IOCOM,IOX,IOCUCBBM,IOXCHMSK)(IOSGEN UCBLOOK) 01299000
*       ;                           /*                       @YM02128*/ 01300000
         IOSGEN UCBLOOK                                                 01301000
*   REG13=SAVE13;                   /* RESTORE POINTER TO OUR SAVE 0402 01302000
*                                      AREA                          */ 01303000
         L     REG13,SAVE13                                        0402 01304000
*   IF REG15^=0 THEN                /* REG. 15 WILL BE ZERO WHEN THE    01305000
*                                      PATH IS VALID.                */ 01306000
         LTR   REG15,REG15                                         0403 01307000
         BZ    @RF00403                                            0403 01308000
*     VALIDPTH='0'B;                /* INDICATE PATH NOT VALID       */ 01309000
         NI    VALIDPTH,B'11111110'                                0404 01310000
*   IF VALIDPTH='1'B THEN           /* CONTINUE IF THE PATH WAS    0405 01311000
*                                      VALID.                        */ 01312000
@RF00403 TM    VALIDPTH,B'00000001'                                0405 01313000
         BNO   @RF00405                                            0405 01314000
*     DO;                                                          0406 01315000
*       UCBADDR=REG7;               /* GET ADDRESS OF UCB'S BASE.    */ 01316000
*                                                                  0407 01317000
         ST    REG7,UCBADDR                                        0407 01318000
*       /*************************************************************/ 01319000
*       /*                                                           */ 01320000
*       /* UCBPTR WILL POINT TO THE UCB BASE (COMMON SECTION). ALL   */ 01321000
*       /* DISPLACEMENTS ARE FROM THE START OF THE UCB PREFIX BUT THE*/ 01322000
*       /* ADDRESS OF THE PREFIX IS BASED ON UCBPTR-512 (512 IS THE  */ 01323000
*       /* LENGTH OF THE PREFIX).                                    */ 01324000
*       /*                                                           */ 01325000
*       /*************************************************************/ 01326000
*                                                                  0408 01327000
*       UCBPTR=REG7;                /* IOSGEN RETURNS THE BASE UCB @    01328000
*                                      IN REG. 7. NOW HAVE ADDRESS OF   01329000
*                                      BASE.                         */ 01330000
         LR    UCBPTR,REG7                                         0408 01331000
*       RFY                                                        0409 01332000
*        (REG6,                                                    0409 01333000
*         REG7) UNRSTD;                                            0409 01334000
*                                                                  0409 01335000
*       /*************************************************************/ 01336000
*       /*                                                           */ 01337000
*       /* IF WE ARE LOOKING AT A 2305'S UCB IT MUST BE THE BASE'S,  */ 01338000
*       /* OR ELSE THE PATH ENTERED IS NOT VALID. THE UCBLOOK        */ 01339000
*       /* FUNCTION WILL HAVE POINTED US TO AN EXPOSURE'S UCB IN THAT*/ 01340000
*       /* CASE.                                                     */ 01341000
*       /*                                                           */ 01342000
*       /*************************************************************/ 01343000
*                                                                  0410 01344000
*       IF(UCBMTPXP='1'B)&          /* ARE WE ON A 2305'S UCB, AND   */ 01345000
*           (UCBBASE^=(UCBPTR-512)) THEN/* IS IT NOT THE BASE?       */ 01346000
         TM    UCBMTPXP(UCBPTR),B'00001000'                        0410 01347000
         BNO   @RF00410                                            0410 01348000
         LR    @05,UCBPTR                                          0410 01349000
         SH    @05,@CH01220                                        0410 01350000
         C     @05,UCBBASE(,UCBPTR)                                0410 01351000
         BE    @RF00410                                            0410 01352000
*         DO;                       /* YES, PATH NOT VALID.          */ 01353000
*           VALIDPTH='0'B;          /* SET INVALID PATH BIT.         */ 01354000
         NI    VALIDPTH,B'11111110'                                0412 01355000
*           GOTO FIN;               /* NOTHING ELSE TO DETERMINE.    */ 01356000
         B     FIN                                                 0413 01357000
*         END;                                                     0414 01358000
*       EBCDEVAD=UCBNAME;           /* PUT BASE EBCDIC NAME IN THE 0415 01359000
*                                      TABLE FOR IOSGEN VARY       0415 01360000
*                                      FUNCTION.                     */ 01361000
@RF00410 L     @05,VRYTPTR                                         0415 01362000
         MVC   EBCDEVAD(3,@05),UCBNAME(UCBPTR)                     0415 01363000
*       DEVCLASS=UCBTBYT3;          /* GET DEVICE CLASS.             */ 01364000
         MVC   DEVCLASS(1),UCBTBYT3(UCBPTR)                        0416 01365000
*       DEVTYPE=UCBTBYT4;           /* GET UNIT TYPE.                */ 01366000
*                                                                  0417 01367000
         MVC   DEVTYPE(1),UCBTBYT4(UCBPTR)                         0417 01368000
*       /*************************************************************/ 01369000
*       /*                                                           */ 01370000
*       /* IF THE REQUEST IS AN OFFLINE REQUEST, DETERMINE IF THE    */ 01371000
*       /* LAST PATH CAN BE VARY'ED OFFLINE. WE WON'T ALLOW IT TO BE */ 01372000
*       /* VARY'ED IF THE DEVICE IS A CONSOLE OR IT'S BEING USED.    */ 01373000
*       /*                                                           */ 01374000
*       /*************************************************************/ 01375000
*                                                                  0418 01376000
*       IF ONLINREQ='0'B THEN       /* IS IT AN OFFLINE REQUEST?     */ 01377000
         TM    ONLINREQ,B'00001000'                                0418 01378000
         BNZ   @RF00418                                            0418 01379000
*         DO;                       /* YES, OFFLINE REQUEST.         */ 01380000
*           IF((UCBONLI='0'B)&(UCBNALOC='1'B))|/* DEVICE OFFLINE & IN   01381000
*                                      USE, OR                       */ 01382000
*               ((UCBONLI='1'B)&(UCBSYSR='1'B))|/* ONLINE AND A    0420 01383000
*                                      CONSOLE, OR                   */ 01384000
*               ((UCBONLI='1'B)&(UCBALOC='1'B))/* ONLINE AND IN USE? */ 01385000
*             THEN                  /* YES,                          */ 01386000
         TM    UCBONLI(UCBPTR),B'10000000'                         0420 01387000
         BNZ   @GL00016                                            0420 01388000
         TM    UCBNALOC(UCBPTR),B'00000100'                        0420 01389000
         BO    @RT00420                                            0420 01390000
@GL00016 TM    UCBONLI(UCBPTR),B'10000010'                         0420 01391000
         BO    @RT00420                                            0420 01392000
         TM    UCBONLI(UCBPTR),B'10001000'                         0420 01393000
         BNO   @RF00420                                            0420 01394000
@RT00420 DS    0H                                                  0421 01395000
*             UNCONREQ='0'B;        /* WON'T VARY LAST PATH.         */ 01396000
*                                                                  0421 01397000
         NI    UNCONREQ,B'11111011'                                0421 01398000
*           /*********************************************************/ 01399000
*           /*                                                       */ 01400000
*           /* WE WILL VARY THE LAST PATH TO AN OFFLINE DEVICE WHICH */ 01401000
*           /* ISN'T IN USE.                                         */ 01402000
*           /*                                                       */ 01403000
*           /*********************************************************/ 01404000
*                                                                  0422 01405000
*           ELSE                    /* NONE OF THE ABOVE...          */ 01406000
*             IF((UCBONLI='0'B)&(UCBNALOC='0'B)) THEN/* DEVICE     0422 01407000
*                                      OFFLINE, NOT IN USE?          */ 01408000
         B     @RC00420                                            0422 01409000
@RF00420 TM    UCBONLI(UCBPTR),B'10000000'                         0422 01410000
         BNZ   @RF00422                                            0422 01411000
         TM    UCBNALOC(UCBPTR),B'00000100'                        0422 01412000
         BNZ   @RF00422                                            0422 01413000
*               UNCONREQ='1'B;      /* YES, SET INDICATOR.           */ 01414000
         OI    UNCONREQ,B'00000100'                                0423 01415000
*         END;                      /* FINISHED DETERMINATIONS FOR 0424 01416000
*                                      UNCONDITIONAL FLAG FOR OFFLINE   01417000
*                                      REQUESTS.                     */ 01418000
*     END;                          /* FINISHED PROCESSING FOR VALID    01419000
*                                      PATHS.                        */ 01420000
*FIN:                                                              0426 01421000
*   END UCBCKOUT;                                                  0426 01422000
@EL00003 DS    0H                                                  0426 01423000
@EF00003 DS    0H                                                  0426 01424000
@ER00003 LM    @14,@03,@SA00003                                    0426 01425000
         LM    @05,@12,@SA00003+24                                 0426 01426000
         BR    @14                                                 0426 01427000
*                                                                  0427 01428000
*/* VARYOFF - CALL IOSGEN TO CONDITIONALLY VARY PATH OFFLINE.        */ 01429000
*/*           CHECK THE RETURN CODES FROM THE IOSGEN VARY FUNCTION.  */ 01430000
*/*           DETERMINE WHAT MESSAGES SHOULD BE ISSUED, SET BITS     */ 01431000
*/*           IF NECESSARY IN THE UCB, AND WRITE AN SMF              */ 01432000
*/*           RECORD IF NECESSARY. IF BAD ERROR EXIT THRU ESTAE RTN. */ 01433000
*/*                                                                  */ 01434000
*/*    INPUT-                                                        */ 01435000
*/*          UCBONLI- ON, UCB (DEVICE) IS ON-LINE.                   */ 01436000
*/*          UCBVHRSN- ON, HIERARCHY NOT AVAILABLE; NO PATH EXISTS TO*/ 01437000
*/*                      THE DEVICE.                                 */ 01438000
*/*                                                                  */ 01439000
*/*    OUTPUT-                                                       */ 01440000
*/*           PTHVARED- ON, ISSUE 'PATH VARYED' MESSAGE.             */ 01441000
*/*                          OR                                      */ 01442000
*/*           LASTPATH- ON, ISSUE 'LAST PATH- CAN'T VARY' MESSAGE.   */ 01443000
*/*                          OR                                      */ 01444000
*/*           VALIDPTH- OFF, ISSUE 'PATH DOES NOT EXIST ' MESSAGE.   */ 01445000
*/*                          OR                                      */ 01446000
*/*           RESERPTH- ON, ISSUE 'PATH RESERVED-CAN'T VARY MESSAGE. */ 01447000
*/*                                                                  */ 01448000
*                                                                  0427 01449000
*VARYOFF:                                                          0427 01450000
*   PROC;                                                          0427 01451000
*                                                                  0427 01452000
VARYOFF  STM   @14,@12,@SA00004                                    0427 01453000
*   /*****************************************************************/ 01454000
*   /*                                                               */ 01455000
*   /* SET UP CALL TO IOSGEN VARY. CALL IOSGEN TO CONDITIONALLY VARY */ 01456000
*   /* PATH TO DEVICE OFFLINE                                        */ 01457000
*   /*                                                               */ 01458000
*   /*****************************************************************/ 01459000
*                                                                  0428 01460000
*   ONOFFIND='00'X;                 /* SET FUNCTION CODE             */ 01461000
         L     @05,VRYTPTR                                         0428 01462000
         MVI   ONOFFIND(@05),X'00'                                 0428 01463000
*   CALL IOSGENV;                   /* CALL VARY SUBROUTINE          */ 01464000
*                                                                  0429 01465000
         BAL   @14,IOSGENV                                         0429 01466000
*   /*****************************************************************/ 01467000
*   /*                                                               */ 01468000
*   /* RETURN CODE OF ZERO INDICATES THAT THE PATH WAS VARIED, AND IT*/ 01469000
*   /* WASN'T A LAST PATH. THE ONLY PROCESSING WHICH WILL BE         */ 01470000
*   /* PERFORMED AFTER HERE WILL BE TO ISSUE THE PATH VARIED MESSAGE.*/ 01471000
*   /*                                                               */ 01472000
*   /*****************************************************************/ 01473000
*                                                                  0430 01474000
*   IF RETCODE=0 THEN               /* RETURN CODE ZERO?             */ 01475000
         LTR   RETCODE,RETCODE                                     0430 01476000
         BNZ   @RF00430                                            0430 01477000
*     DO;                           /* YES,                          */ 01478000
*       PTHVARED='1'B;              /* INDICATE PATH VARY'ED.        */ 01479000
         OI    PTHVARED,B'10000000'                                0432 01480000
*     END;                                                         0433 01481000
*                                                                  0433 01482000
*   /*****************************************************************/ 01483000
*   /*                                                               */ 01484000
*   /* RETURN CODE OF NEITHER ZERO NOR EIGHT INDICATES THAT THE PATH */ 01485000
*   /* WAS NOT VARY'ED DUE TO AN ERROR CONDITION DETECTED BY THE     */ 01486000
*   /* IOSGEN VARY FUNCTION.. WE WILL NOT. SET ANY BITS IN THE UCB OR*/ 01487000
*   /* WRITE ANY SMF RECORDS.                                        */ 01488000
*   /*                                                               */ 01489000
*   /*****************************************************************/ 01490000
*                                                                  0434 01491000
*   IF(RETCODE^=0)&(RETCODE^=8) THEN/* IS RETURN CODE NOT ZERO, AND     01492000
*                                      NOT EIGHT?                    */ 01493000
@RF00430 LTR   RETCODE,RETCODE                                     0434 01494000
         BZ    @RF00434                                            0434 01495000
         CH    RETCODE,@CH00082                                    0434 01496000
         BE    @RF00434                                            0434 01497000
*     DO;                           /* YES, CONTINUE.                */ 01498000
*                                                                  0435 01499000
*       /*************************************************************/ 01500000
*       /*                                                           */ 01501000
*       /* A RETURN CODE OF TWELVE INDICATES THAT THE IOSGEN VARY    */ 01502000
*       /* FUNCTION WAS PASSED A PATH WHICH DOES NOT EXIST. ( CAN    */ 01503000
*       /* ONLY COME FROM AN INVALID CPU ADDRESS SINCE WE PASS HIM A */ 01504000
*       /* VALID DEVICE ADDRESS- WE USE UCBLOOK ( AN IOS FUNCTION) TO*/ 01505000
*       /* ENSURE THAT THE DEVICE ADDRESS IS VALID.                  */ 01506000
*       /*                                                           */ 01507000
*       /*************************************************************/ 01508000
*                                                                  0436 01509000
*       IF RETCODE=12 THEN          /* WAS RETURN CODE 12?           */ 01510000
         CH    RETCODE,@CH00034                                    0436 01511000
         BNE   @RF00436                                            0436 01512000
*         VALIDPTH='0'B;            /* SET INDICATOR.                */ 01513000
*                                                                  0437 01514000
         NI    VALIDPTH,B'11111110'                                0437 01515000
*       /*************************************************************/ 01516000
*       /*                                                           */ 01517000
*       /* A RETURN CODE OF TWENTY INDICATES THAT THE PATH REPRESENTS*/ 01518000
*       /* A PATH TO A SHARED DASD DEVICE, AND IS THEREFORE RESERVED.*/ 01519000
*       /* IT WAS NOT VARY'ED OFFLINE.                               */ 01520000
*       /*                                                           */ 01521000
*       /*************************************************************/ 01522000
*                                                                  0438 01523000
*       ELSE                        /* RETURN CODE NOT 12.           */ 01524000
*         IF RETCODE=20 THEN        /* IS RETURN CODE 20?            */ 01525000
         B     @RC00436                                            0438 01526000
@RF00436 CH    RETCODE,@CH02923                                    0438 01527000
         BNE   @RF00438                                            0438 01528000
*           RESERPTH='1'B;          /* YES, SET INDICATOR.           */ 01529000
*                                                                  0439 01530000
         OI    RESERPTH,B'00100000'                                0439 01531000
*       /*************************************************************/ 01532000
*       /*                                                           */ 01533000
*       /* IF NOT 12 OR 20 RETURN CODE THEN ABEND                    */ 01534000
*       /*                                                           */ 01535000
*       /*************************************************************/ 01536000
*                                                                  0440 01537000
*         ELSE                                                     0440 01538000
*           DO;                     /* ABEND(183) SYSTEM             */ 01539000
         B     @RC00438                                            0440 01540000
@RF00438 DS    0H                                                  0441 01541000
*             RESPECIFY                                            0441 01542000
*              (GPR01F) RESTRICTED;                                0441 01543000
*             GPR01F=((183)&'00000FFF'X)*4096;/* COMP CODE IN BITS 0442 01544000
*                                      8-19                          */ 01545000
         L     GPR01F,@CF03248                                     0442 01546000
*             SVC(13);              /* ISSUE ABEND SVC               */ 01547000
         SVC   13                                                  0443 01548000
*             RESPECIFY                                            0444 01549000
*              (GPR01F) UNRESTRICTED;                              0444 01550000
*           END;                    /* ABEND(183) SYSTEM EXIT THRU 0445 01551000
*                                      ESTAE RTN,CODE= 0B7   @YM03534*/ 01552000
*     END;                          /* HANDLED RETURN CODES: 0,4,12,    01553000
*                                      16 AND 20.                    */ 01554000
*                                                                  0446 01555000
@RC00438 DS    0H                                                  0446 01556000
@RC00436 DS    0H                                                  0447 01557000
*   /*****************************************************************/ 01558000
*   /*                                                               */ 01559000
*   /* RETURN CODE 8 MEANS THAT THE PATH TO BE VARIED IS A LAST PATH */ 01560000
*   /* TO THE DEVICE. WE WILL INVOKE IOSGEN TO VARY IT ONLY IF WE'VE */ 01561000
*   /* PREVIOUSLY DETERMINED IT SHOULD BE UNCONDITIONAL, AND THEN    */ 01562000
*   /* AFTER WE DO ANY NECESSARY DEVICE-OFFLINE PROCESSING REWIND    */ 01563000
*   /* TAPE, ETC. ).                                                 */ 01564000
*   /*                                                               */ 01565000
*   /*****************************************************************/ 01566000
*                                                                  0447 01567000
*   IF RETCODE=8 THEN               /* WAS IT AN ATTEMPT TO VARY A 0447 01568000
*                                      LAST PATH?                    */ 01569000
@RF00434 CH    RETCODE,@CH00082                                    0447 01570000
         BNE   @RF00447                                            0447 01571000
*     DO;                           /* YES,                          */ 01572000
*       IF UNCONREQ='0'B THEN       /* REQUEST NOT TO BE           0449 01573000
*                                      UNCONDITIONAL ?               */ 01574000
         TM    UNCONREQ,B'00000100'                                0449 01575000
         BNZ   @RF00449                                            0449 01576000
*         DO;                       /* YES, CONDITIONAL .            */ 01577000
*           LASTPATH='1'B;          /* INDICATE TO ISSUE 'INVALID  0451 01578000
*                                      ATTEMPT' MESSAGE.             */ 01579000
         OI    LASTPATH,B'01000000'                                0451 01580000
*         END;                                                     0452 01581000
*       ELSE                        /* REQUEST IS UNCONDITIONAL.     */ 01582000
*         DO;                                                      0453 01583000
         B     @RC00449                                            0453 01584000
@RF00449 DS    0H                                                  0454 01585000
*           IF UCBONLI='1'B THEN    /* DEVICE ONLINE?                */ 01586000
         TM    UCBONLI(UCBPTR),B'10000000'                         0454 01587000
         BNO   @RF00454                                            0454 01588000
*             DO;                   /* YES,                          */ 01589000
*                                                                  0455 01590000
*               /*****************************************************/ 01591000
*               /*                                                   */ 01592000
*               /* CHECK IF DEVICE NEEDS TO BE UNLOADED              */ 01593000
*               /*                                                   */ 01594000
*               /*****************************************************/ 01595000
*                                                                  0456 01596000
*               IF(UCB3DACC='1'B)|  /* DEVICE DIRECT-ACCESS, OR      */ 01597000
*                   (UCB3TAPE='1'B) THEN/* TAPE?                     */ 01598000
         TM    UCB3DACC(UCBPTR),B'10100000'                        0456 01599000
         BZ    @RF00456                                            0456 01600000
*                 DO;               /* YES,                          */ 01601000
*                   VMVREQBP=ADDR(ALOCBLOK);/* GET ADDRESSABILITY TO    01602000
*                                      CONTROL BLOCK.                */ 01603000
         LA    @05,ALOCBLOK                                        0458 01604000
         ST    @05,VMVREQBP                                        0458 01605000
*                   ALOCBLOK=ALOCBLOK&&ALOCBLOK;/* INITIALIZE CONTROL   01606000
*                                      BLOCK.                        */ 01607000
         XC    ALOCBLOK(52),ALOCBLOK                               0459 01608000
*                   VMVUNLOD='1'B;  /* INDICATE TO UNLOAD DEVICE.    */ 01609000
         OI    VMVUNLOD(@05),B'10000000'                           0460 01610000
*                   VMVUCBP=UCBADDR;/* GET @ OF DEVICE'S UCB.        */ 01611000
         MVC   VMVUCBP(3,@05),UCBADDR+1                            0461 01612000
*                   VMVUCB='1'B;    /* FIND DEVICE WITH UCB ADDRESS. */ 01613000
*                                                                  0462 01614000
         OI    VMVUCB(@05),B'01000000'                             0462 01615000
*                   /*************************************************/ 01616000
*                   /*                                               */ 01617000
*                   /* CALL ALLOCATION UNLOAD ROUTINE                */ 01618000
*                   /*                                               */ 01619000
*                   /*************************************************/ 01620000
*                                                                  0463 01621000
*                   DO;             /* LINK EP('IEFAB49C')         0463 01622000
*                                      PARAM(VMVREQBP,0,0)           */ 01623000
*                     I00601='00'X; /* EP/DE FLAG                    */ 01624000
         MVI   I00601,X'00'                                        0464 01625000
*                     I00602=ADDR('IEFAB49C');/* ADDR OF EP/DE       */ 01626000
         LA    @05,@CC03149                                        0465 01627000
         STCM  @05,7,I00602                                        0465 01628000
*                     I00604=0;     /* DCB PTR                       */ 01629000
         SLR   @05,@05                                             0466 01630000
         STCM  @05,7,I00604                                        0466 01631000
*                     I00603='00'X; /* ERRET FLAG                    */ 01632000
         MVI   I00603,X'00'                                        0467 01633000
*                     IOPTLIST(1)=ADDR(VMVREQBP);/* ADDR OF PROB PROG   01634000
*                                      PARM                          */ 01635000
         LA    @05,VMVREQBP                                        0468 01636000
         ST    @05,IOPTLIST                                        0468 01637000
*                     IOPTLIST(2)=ADDR(0);/* ADDR OF PROB PROG PARM  */ 01638000
         LA    @05,@CF00101                                        0469 01639000
         ST    @05,IOPTLIST+4                                      0469 01640000
*                     IOPTLIST(3)=ADDR(0);/* ADDR OF PROB PROG PARM  */ 01641000
         ST    @05,IOPTLIST+8                                      0470 01642000
*                     IOPTLIST(3)=IOPTLIST(3)|'80000000'X;/* SET VL     01643000
*                                      BIT                           */ 01644000
         O     @05,@CF03153                                        0471 01645000
         ST    @05,IOPTLIST+8                                      0471 01646000
*                     RESPECIFY                                    0472 01647000
*                      (GPR01P) RESTRICTED;                        0472 01648000
*                     GPR01P=ADDR(IOPTLIST);/* ADDR OF OPTLIST       */ 01649000
         LA    GPR01P,IOPTLIST                                     0473 01650000
*                     RESPECIFY                                    0474 01651000
*                      (GPR15P) RESTRICTED;                        0474 01652000
*                     GPR15P=ADDR(I006);/* ADDR OF LINK LIST         */ 01653000
         LA    GPR15P,I006                                         0475 01654000
*                     SVC(6);       /* ISSUE LINK SVC                */ 01655000
         SVC   6                                                   0476 01656000
*                     RESPECIFY                                    0477 01657000
*                      (GPR01P) UNRESTRICTED;                      0477 01658000
*                     RESPECIFY                                    0478 01659000
*                      (GPR15P) UNRESTRICTED;                      0478 01660000
*                   END;            /* LINK EP('IEFAB49C')         0479 01661000
*                                      PARAM(VMVREQBP,0,0)           */ 01662000
*                 END;              /* END PROC TAPE OR DISK         */ 01663000
*                                                                  0480 01664000
*               /*****************************************************/ 01665000
*               /*                                                   */ 01666000
*               /* CHECK IF DEVICE HAS ERROR BUFFER WHICH MUST BE    */ 01667000
*               /* RECORDED                                          */ 01668000
*               /*                                                   */ 01669000
*               /*****************************************************/ 01670000
*                                                                  0481 01671000
*               ELSE                                               0481 01672000
*                 IF UCBERLOG='1'B THEN/* ERROR BUFFER?      @Z40MPXA*/ 01673000
         B     @RC00456                                            0481 01674000
@RF00456 L     @05,UCBEXTPT(,UCBPTR)                               0481 01675000
         TM    UCBERLOG(@05),B'00000100'                           0481 01676000
         BNO   @RF00481                                            0481 01677000
*                   DO;             /*                       @Z40MPXA*/ 01678000
*                     RFY                                          0483 01679000
*                      (REG0,                                      0483 01680000
*                       REG1) RSTD; /* RESTRICT REG 0        @Z40MPXA*/ 01681000
*                     REG0=-UCBADDR;/* UCB ADDR IN COMPLEMENT FORM 0484 01682000
*                                                            @Z40MPXA*/ 01683000
         L     REG0,UCBADDR                                        0484 01684000
         LCR   REG0,REG0                                           0484 01685000
*                     REG1=0;       /* ZERO REG1             @Y30MPXG*/ 01686000
         SLR   REG1,REG1                                           0485 01687000
*                     SVC(91);      /* RECORD BUFFER         @Z40MPXA*/ 01688000
         SVC   91                                                  0486 01689000
*                     RFY                                          0487 01690000
*                      (REG0,                                      0487 01691000
*                       REG1) UNRSTD;/* RELEASE REG 0        @Z40MPXA*/ 01692000
*                   END;            /*                       @Z40MPXA*/ 01693000
*               UCBONLI='0'B;       /* MARK UCB OFFLINE              */ 01694000
@RF00481 DS    0H                                                  0489 01695000
@RC00456 NI    UCBONLI(UCBPTR),B'01111111'                         0489 01696000
*               DVCEOFF='1'B;       /* SET FLAG FOR DEVICE MSG       */ 01697000
         OI    DVCEOFF,B'01000000'                                 0490 01698000
*               CALL SMF(11);       /* WRITE DEVICE OFFLINE SMF RCRD */ 01699000
         LA    @01,@AL00491                                        0491 01700000
         BAL   @14,SMF                                             0491 01701000
*               UCBVHRSN='1'B;      /* HIERARCHY UNAVAILABLE         */ 01702000
         OI    UCBVHRSN(UCBPTR),B'00000010'                        0492 01703000
*               UCBRESV='0'B;       /* CAN NO LONGER HAVE RESERVE.   */ 01704000
*               UCBCHGS='0'B;       /* UCB NO LONGER CHANGING STATUS.*/ 01705000
         NI    UCBRESV(UCBPTR),B'10011111'                         0494 01706000
*             END;                  /* END ONLINE DEVICE PROC        */ 01707000
*                                                                  0495 01708000
*           /*********************************************************/ 01709000
*           /*                                                       */ 01710000
*           /* CALL IOSGEN VARY FUNCTION TO UNCONDITIONALLY VARY LAST*/ 01711000
*           /* PATH OFFLINE.                                         */ 01712000
*           /*                                                       */ 01713000
*           /*********************************************************/ 01714000
*                                                                  0496 01715000
*           ONOFFIND='80'X;         /* SET FUNCTION CODE             */ 01716000
@RF00454 L     @05,VRYTPTR                                         0496 01717000
         MVI   ONOFFIND(@05),X'80'                                 0496 01718000
*           CALL IOSGENV;                                          0497 01719000
         BAL   @14,IOSGENV                                         0497 01720000
*           IF RETCODE=4 THEN       /* WAS THE LAST PATH VARY'ED?    */ 01721000
         CH    RETCODE,@CH00124                                    0498 01722000
         BNE   @RF00498                                            0498 01723000
*             DO;                   /* YES,                          */ 01724000
*               PTHVARED='1'B;      /* INDICATE THAT PATH WAS VARIED.*/ 01725000
         OI    PTHVARED,B'10000000'                                0500 01726000
*             END;                                                 0501 01727000
*           ELSE                    /* NO, ABEND CALLER              */ 01728000
*             DO;                   /* ABEND(183) SYSTEM             */ 01729000
         B     @RC00498                                            0502 01730000
@RF00498 DS    0H                                                  0503 01731000
*               RESPECIFY                                          0503 01732000
*                (GPR01F) RESTRICTED;                              0503 01733000
*               GPR01F=((183)&'00000FFF'X)*4096;/* COMP CODE IN BITS    01734000
*                                      8-19                          */ 01735000
         L     GPR01F,@CF03248                                     0504 01736000
*               SVC(13);            /* ISSUE ABEND SVC               */ 01737000
         SVC   13                                                  0505 01738000
*               RESPECIFY                                          0506 01739000
*                (GPR01F) UNRESTRICTED;                            0506 01740000
*             END;                  /* ABEND(183) SYSTEM EXIT THRU 0507 01741000
*                                      ESTAE RTN ABEND CODE = 0B7  0507 01742000
*                                                            @YM03534*/ 01743000
*         END;                                                     0508 01744000
*     END;                                                         0509 01745000
*   END VARYOFF;                                                   0510 01746000
@EL00004 DS    0H                                                  0510 01747000
@EF00004 DS    0H                                                  0510 01748000
@ER00004 LM    @14,@12,@SA00004                                    0510 01749000
         BR    @14                                                 0510 01750000
*                                                                  0511 01751000
*/*  POSCONCK- DETERMINE IF DEVICE WAS SYSGEN'ED AS A CONSOLE.       */ 01752000
*/*    INPUT-                                                        */ 01753000
*/*           UCMVEA- ADDRESS OF FIRST UCME ENTRY.                   */ 01754000
*/*           UCMVEZ- LENGTH OF EACH UCME.                           */ 01755000
*/*           UCMVEL- ADDRESS OF LAST UCME.                          */ 01756000
*/*    OUTPUT-                                                       */ 01757000
*/*            SGCONSLE- ON, DEVICE IS A SYSGEN'ED CONSOLE.          */ 01758000
*/*                                                                  */ 01759000
*/* SEARCH THROUGH THE UCME'S SERIALLY, LOOKING FOR ONE WHOSE UCMUCB */ 01760000
*/* FIELD POINTS TO THE SAME UCB AS OUR DEVICE. IF WE FIND SUCH A    */ 01761000
*/* UCME, THE DEVICE IS A SYGEN'ED CONSOLE; IF WE EXHAUST THE        */ 01762000
*/* UCME'S WITHOUT MATCHING THE UCB ADDRESSES, THEN THE DEVICE       */ 01763000
*/* WASN'T SYSGEN'ED AS A CONSOLE.                                   */ 01764000
*/*                                                                  */ 01765000
*                                                                  0511 01766000
*POSCONCK:                                                         0511 01767000
*   PROC OPTIONS(NOSAVEAREA);                                      0511 01768000
POSCONCK STM   @14,@12,12(@13)                                     0511 01769000
*   UCMDVEPT=UCMVEA;                /* POINT TO FIRST UCME.          */ 01770000
         L     UCMDVEPT,UCMVEA(,UCMBASPT)                          0512 01771000
*   DO WHILE((UCMDVEPT<=UCMVEL)&(SGCONSLE='0'B));/* SEARCH THROUGH 0513 01772000
*                                      THE UCME'S UP TO AND INCLUDING   01773000
*                                      THE LAST STOP THE SEARCH WHEN    01774000
*                                      WE'VE FOUND THAT THE DEVICE IS   01775000
*                                      A SYSGEN'ED CONSOLE OR WHEN 0513 01776000
*                                      WE'VE REACHED THE END OF THE     01777000
*                                      UCME'S.                       */ 01778000
         B     @DE00513                                            0513 01779000
@DL00513 DS    0H                                                  0514 01780000
*     IF UCMUCB=UCBADDR THEN        /* DO THE UCB ADDRESSES MATCH?   */ 01781000
         CLC   UCMUCB(4,UCMDVEPT),UCBADDR                          0514 01782000
         BNE   @RF00514                                            0514 01783000
*       SGCONSLE='1'B;              /* YES, SET INDICATOR.           */ 01784000
         OI    SGCONSLE,B'01000000'                                0515 01785000
*     UCMDVEPT=UCMDVEPT+UCMVEZ;     /* POINT TO NEXT UCME.           */ 01786000
@RF00514 AL    UCMDVEPT,UCMVEZ(,UCMBASPT)                          0516 01787000
*   END;                            /* FINISHED SEARCH.              */ 01788000
@DE00513 C     UCMDVEPT,UCMVEL(,UCMBASPT)                          0517 01789000
         BH    @DC00513                                            0517 01790000
         TM    SGCONSLE,B'01000000'                                0517 01791000
         BZ    @DL00513                                            0517 01792000
@DC00513 DS    0H                                                  0518 01793000
*   END POSCONCK;                                                  0518 01794000
@EL00005 DS    0H                                                  0518 01795000
@EF00005 DS    0H                                                  0518 01796000
@ER00005 LM    @14,@12,12(@13)                                     0518 01797000
         BR    @14                                                 0518 01798000
*                                                                  0519 01799000
*/*                                                                  */ 01800000
*/*  VARYON-CALL IOSGEN VARY TO VARY PATH ONLINE.                    */ 01801000
*/*         CHECK THE RETURN CODES FROM THE IOSGEN VARY              */ 01802000
*/*         FUNCTION TO DETERMINE WHICH OF THE FOLLOWING ACTIONS     */ 01803000
*/*         SHOULD BE TAKEN:                                         */ 01804000
*/*                                                                  */ 01805000
*/*          RETURN CODE ZERO- ATTEMPT TO BRING THE DEVICE ONLINE,   */ 01806000
*/*          AFTER FIRST ENSURING THAT IT IS OPERATIONAL ( IF IT IS  */ 01807000
*/*          NOT OPERATIONAL, WE SET A BIT TO INDICATE THAT AN       */ 01808000
*/*          APPROPRIATE MESSAGE SHOULD BE ISSUED.). WHEN THE DEVICE */ 01809000
*/*          IS BROUGHT ONLINE, THE UCB HIERARCHY BIT IS TURNED OFF, */ 01810000
*/*          THE ONLINE BIT IS TURNED ON, AND AN SMF RECORD IS       */ 01811000
*/*          WRITTEN TO RECORD THESE FACTS.                          */ 01812000
*/*          RETURN CODE 12- PATH DOES NOT EXIST: SET BIT TO INDI-   */ 01813000
*/*          CATE THAT MESSAGE SHOULD BE WRITTEN.                    */ 01814000
*/*          ALL OTHER RETURN CODES- IMPLY AN ERROR IN THE           */ 01815000
*/*          PROCESSING OF AN ONLINE REQUEST. ABEND,                 */ 01816000
*/*          ROUTINE WILL ISSUE AN ERROR MESSAGE.                    */ 01817000
*/*   INPUT-                                                         */ 01818000
*/*         UCBONLI- OFF, DEVICE IS OFFLINE.                         */ 01819000
*/*         UCBNALOC-ON, DEVICE IS IN USE.                           */ 01820000
*/*         UCBVORSN- ON, DEVICE IS OFFLINE BECAUSE OF A SPECIFIC    */ 01821000
*/*                   OPERATOR REQUEST.                              */ 01822000
*/*                                                                  */ 01823000
*/*   OUTPUT-                                                        */ 01824000
*/*          PTHVARED- ON,INDICATE TO ISSUE PATH VARY'ED MESSAGE.    */ 01825000
*/*          VALIDPTH- OFF, GIVEN PATH TO DEVICE DOESN'T EXIST,      */ 01826000
*/*                    HAVE CLEAN-UP ROUTINE ISSUE MESSAGE.          */ 01827000
*/*          UCBVHRSN- OFF, (OPERATIONAL) PATH TO DEVICE EXISTS      */ 01828000
*/*                    (HIERARCHY AVAILABLE.                         */ 01829000
*/*          UCBONLI- ON, DEVICE ONLINE.                             */ 01830000
*/*          UCBMRKED- ON, UCBONLI AND UCBVHRSN BITS SET.            */ 01831000
*/*          PTHNOTOP- ON, PATH BROUGHT ONLINE BUT IS NOT            */ 01832000
*/*                    OPERATIONAL; CLEAN-UP ROUTINE WILL ISSUE MSG. */ 01833000
*/*                                                                  */ 01834000
*                                                                  0519 01835000
*VARYON:                                                           0519 01836000
*   PROC;                                                          0519 01837000
*                                                                  0519 01838000
VARYON   STM   @14,@12,@SA00006                                    0519 01839000
*   /*****************************************************************/ 01840000
*   /*                                                               */ 01841000
*   /* DETERMINE IF REQUESTED PATH CAN BE VERIFIED                   */ 01842000
*   /*                                                               */ 01843000
*   /*****************************************************************/ 01844000
*                                                                  0520 01845000
*   IF UCB3COMM='1'B&UCBACTV='1'B THEN/* ACTIVE TP DEVICE?   @ZA00826*/ 01846000
         TM    UCB3COMM(UCBPTR),B'01000000'                        0520 01847000
         BNO   @RF00520                                            0520 01848000
         TM    UCBACTV(UCBPTR),B'00000010'                         0520 01849000
         BNO   @RF00520                                            0520 01850000
*     DO;                                                          0521 01851000
*                                                                  0521 01852000
*       /*************************************************************/ 01853000
*       /*                                                           */ 01854000
*       /* CHECK IF PATH IS ALREADY ONLINE, IE. IF I/O IS ON PATH TO */ 01855000
*       /* BE VARIED, THEN PUT OUT ONLINE MESSAGE, ELSE BRING PATH   */ 01856000
*       /* ONLINE, AND INFORM OPERATOR THAT PATH WAS NOT VERIFIED    */ 01857000
*       /*                                                           */ 01858000
*       /*************************************************************/ 01859000
*                                                                  0522 01860000
*       IF UCBCPU=LOGCPUID&UCBCHAN=HEXDEVAD THEN/* I/O FROM PATH     */ 01861000
         CLC   UCBCPU(1,UCBPTR),LOGCPUID                           0522 01862000
         BNE   @RF00522                                            0522 01863000
         SLR   @05,@05                                             0522 01864000
         ICM   @05,3,UCBCHAN(UCBPTR)                               0522 01865000
         L     @01,VRYTPTR                                         0522 01866000
         CH    @05,HEXDEVAD(,@01)                                  0522 01867000
         BNE   @RF00522                                            0522 01868000
*         DO;                                                      0523 01869000
*           PTHVARED='1'B;          /* INDICATE PATH ONLINE  @ZA00826*/ 01870000
         OI    PTHVARED,B'10000000'                                0524 01871000
*           GOTO ONEXIT;            /* EXIT TO CLEANUP       @ZA00826*/ 01872000
         B     ONEXIT                                              0525 01873000
*         END;                                                     0526 01874000
*       ELSE                                                       0527 01875000
*         DO;                                                      0527 01876000
@RF00522 DS    0H                                                  0528 01877000
*           NOTVER='1'B;            /* INDICATE PATH NOT VERIFIED  0528 01878000
*                                                            @ZA00826*/ 01879000
         OI    NOTVER,B'00001000'                                  0528 01880000
*           GOTO PTHVARY;           /*                       @ZA00826*/ 01881000
         B     PTHVARY                                             0529 01882000
*         END;                                                     0530 01883000
*     END;                                                         0531 01884000
*                                                                  0531 01885000
*   /*****************************************************************/ 01886000
*   /*                                                               */ 01887000
*   /* CHECK IF REQUESTED PATH IS OPERATIONAL                        */ 01888000
*   /*                                                               */ 01889000
*   /*****************************************************************/ 01890000
*                                                                  0532 01891000
*   IOPMFC=2;                       /* VARY PATH FUNCTION CODE     0532 01892000
*                                                            @ZA00826*/ 01893000
@RF00520 MVI   IOPMFC,X'02'                                        0532 01894000
*   IOPMUCB=UCBPTR;                 /* UCB ADDRESS           @ZA00826*/ 01895000
         STCM  UCBPTR,7,IOPMUCB                                    0533 01896000
*   IOPMCPU=CSDMASK(LOGCPUID+1);    /* CPU ADDRESS           @ZA00826*/ 01897000
         SLR   @05,@05                                             0534 01898000
         IC    @05,LOGCPUID                                        0534 01899000
         ALR   @05,@05                                             0534 01900000
         L     @01,CVTPTR                                          0534 01901000
         L     @01,CVTCSD(,@01)                                    0534 01902000
         ALR   @01,@05                                             0534 01903000
         MVC   IOPMCPU(2),CSDMASK(@01)                             0534 01904000
*   IOPMCHN=HEXCHNAD;               /* CHANNEL ADDRESS       @ZA00826*/ 01905000
*                                                                  0535 01906000
         L     @05,VRYTPTR                                         0535 01907000
         SLR   @09,@09                                             0535 01908000
         IC    @09,HEXCHNAD(,@05)                                  0535 01909000
         STH   @09,IOPMCHN                                         0535 01910000
*   /*****************************************************************/ 01911000
*   /*                                                               */ 01912000
*   /* CHECK IF PATHS HAVE BEEN VALIDATED BEFORE                     */ 01913000
*   /*                                                               */ 01914000
*   /*****************************************************************/ 01915000
*                                                                  0536 01916000
*   IF UCBVALPH='1'B THEN           /* PATHS BEEN VALIDATED? @ZA00826*/ 01917000
         TM    UCBVALPH(UCBPTR),B'00000010'                        0536 01918000
         BNO   @RF00536                                            0536 01919000
*     DO;                                                          0537 01920000
*       REG1=ADDR(IOPMPARM);        /* POINT TO PARMS        @ZA00826*/ 01921000
         LA    REG1,IOPMPARM                                       0538 01922000
*       GEN(LINK EP=IECVIOPM);      /* LINK TO IOPM          @ZA00826*/ 01923000
         LINK EP=IECVIOPM                                               01924000
*     END;                                                         0540 01925000
*                                                                  0540 01926000
*   /*****************************************************************/ 01927000
*   /*                                                               */ 01928000
*   /* MAKE LINK TO VERIFY THAT REQUESTED PATH IS OPERATIONAL        */ 01929000
*   /*                                                               */ 01930000
*   /*****************************************************************/ 01931000
*                                                                  0541 01932000
*   REG15=20;                       /* INIT FOR RESERVED PATH RETRY     01933000
*                                                            @ZA20196*/ 01934000
@RF00536 LA    REG15,20                                            0541 01935000
*   RETRYCTR=RETRY;                 /* INIT COUNTER FROM STATIC RETRY   01936000
*                                      COUNT                 @ZA20196*/ 01937000
         LA    RETRYCTR,5                                          0542 01938000
*   DO WHILE RETRYCTR^=0&REG15=20;  /* RETRY 5 TIMES IF PATH RESERVED   01939000
*                                      CONDITION             @ZA20196*/ 01940000
         B     @DE00543                                            0543 01941000
@DL00543 DS    0H                                                  0544 01942000
*     REG1=ADDR(IOPMPARM);          /* ADDRESS OF PARMS      @ZA00826*/ 01943000
         LA    REG1,IOPMPARM                                       0544 01944000
*     REG15=IOPMADDR;               /* GET IECVIOPM ENTRY ADDRESS  0545 01945000
*                                                            @ZA20196*/ 01946000
         L     REG15,IOPMADDR                                      0545 01947000
*     GEN( BALR REG14,REG15);       /* BRANCH TO IECVIOPM    @ZA20196*/ 01948000
          BALR REG14,REG15                                              01949000
*     RETRYCTR=RETRYCTR-1;          /* DECREMENT RETRY COUNT BY ONE     01950000
*                                                            @ZA20196*/ 01951000
         BCTR  RETRYCTR,0                                          0547 01952000
*   END;                            /* END BRANCH TO TEST PATH     0548 01953000
*                                                            @ZA20196*/ 01954000
*                                                                  0548 01955000
@DE00543 LTR   RETRYCTR,RETRYCTR                                   0548 01956000
         BZ    @DC00543                                            0548 01957000
         CH    REG15,@CH02923                                      0548 01958000
         BE    @DL00543                                            0548 01959000
@DC00543 DS    0H                                                  0549 01960000
*   /*****************************************************************/ 01961000
*   /*                                                               */ 01962000
*   /* CHECK IF PATH IS OPERATIONAL                                  */ 01963000
*   /*                                                               */ 01964000
*   /*****************************************************************/ 01965000
*                                                                  0549 01966000
*   IF REG15^=0 THEN                /* IS PATH OPERATIONAL?  @ZA00826*/ 01967000
         LTR   REG15,REG15                                         0549 01968000
         BZ    @RF00549                                            0549 01969000
*     IF REG15=20 THEN                                             0550 01970000
         CH    REG15,@CH02923                                      0550 01971000
         BNE   @RF00550                                            0550 01972000
*       RESERPTH='1'B;              /* SET PATH RESERVED     @ZA20196*/ 01973000
         OI    RESERPTH,B'00100000'                                0551 01974000
*     ELSE                                                         0552 01975000
*       PTHNOTOP='1'B;              /* SET NOT OPERATIONAL FLAG    0552 01976000
*                                                            @ZA20196*/ 01977000
         B     @RC00550                                            0552 01978000
@RF00550 OI    PTHNOTOP,B'00010000'                                0552 01979000
*   ELSE                                                           0553 01980000
*     DO;                                                          0553 01981000
*                                                                  0553 01982000
         B     @RC00549                                            0553 01983000
@RF00549 DS    0H                                                  0554 01984000
*       /*************************************************************/ 01985000
*       /*                                                           */ 01986000
*       /* SET UP AND CALL IOSGEN VARY TO VARY PATH ONLINE           */ 01987000
*       /*                                                           */ 01988000
*       /*************************************************************/ 01989000
*                                                                  0554 01990000
*PTHVARY:                                                          0554 01991000
*       ONOFFIND='04'X;             /* SET FUNCTION CODE             */ 01992000
PTHVARY  L     @05,VRYTPTR                                         0554 01993000
         MVI   ONOFFIND(@05),X'04'                                 0554 01994000
*       CALL IOSGENV;               /* CALL VARY SUBROUTINE          */ 01995000
         BAL   @14,IOSGENV                                         0555 01996000
*       IF RETCODE^=0 THEN          /* WAS THE PATH VARY'ED?         */ 01997000
         LTR   RETCODE,RETCODE                                     0556 01998000
         BZ    @RF00556                                            0556 01999000
*         DO;                                                      0557 02000000
*           IF RETCODE=12 THEN      /* NO PATH ERROR?                */ 02001000
         CH    RETCODE,@CH00034                                    0558 02002000
         BNE   @RF00558                                            0558 02003000
*             DO;                                                  0559 02004000
*               VALIDPTH='0'B;      /* SET MSG INDICATOR FOR NON-  0560 02005000
*                                      EXSISTANT PATH MESSAGE        */ 02006000
         NI    VALIDPTH,B'11111110'                                0560 02007000
*             END;                  /* DEQ UCBS AND EXIT SUBRTN      */ 02008000
*           ELSE                    /* IF NOT 12 THEN UNEXPECTED RC  */ 02009000
*             DO;                   /* ABEND(183) SYSTEM             */ 02010000
         B     @RC00558                                            0562 02011000
@RF00558 DS    0H                                                  0563 02012000
*               RESPECIFY                                          0563 02013000
*                (GPR01F) RESTRICTED;                              0563 02014000
*               GPR01F=((183)&'00000FFF'X)*4096;/* COMP CODE IN BITS    02015000
*                                      8-19                          */ 02016000
         L     GPR01F,@CF03248                                     0564 02017000
*               SVC(13);            /* ISSUE ABEND SVC               */ 02018000
         SVC   13                                                  0565 02019000
*               RESPECIFY                                          0566 02020000
*                (GPR01F) UNRESTRICTED;                            0566 02021000
*             END;                  /* ABEND(183) SYSTEM ABEND, EXIT    02022000
*                                      THRU ESTAE RTN. ABEND CODE =     02023000
*                                      0B7                   @YM03534*/ 02024000
*         END;                                                     0568 02025000
*                                                                  0568 02026000
*       /*************************************************************/ 02027000
*       /*                                                           */ 02028000
*       /* REACHING THIS POINT, THE PATH WAS VARY'ED. ATTEMPT TO     */ 02029000
*       /* BRING THE DEVICE ON-LINE.                                 */ 02030000
*       /*                                                           */ 02031000
*       /*************************************************************/ 02032000
*                                                                  0569 02033000
*       ELSE                                                       0569 02034000
*         DO;                                                      0569 02035000
         B     @RC00556                                            0569 02036000
@RF00556 DS    0H                                                  0570 02037000
*           PTHVARED='1'B;          /* SET INDICATOR TO ISSUE MSG.   */ 02038000
*                                                                  0570 02039000
         OI    PTHVARED,B'10000000'                                0570 02040000
*           /*********************************************************/ 02041000
*           /*                                                       */ 02042000
*           /* PATH HAS BEEN VARIED. CHECK TO SEE IF THE CHANNEL IS  */ 02043000
*           /* ONLINE                                                */ 02044000
*           /*                                                       */ 02045000
*           /*********************************************************/ 02046000
*                                                                  0571 02047000
*           CATPTR=(ADDR((PCCAT00P(LOGCPUID+1))->PCCACAT)+(8*HEXCHNAD)) 02048000
*               ;                                                  0571 02049000
         SLR   @05,@05                                             0571 02050000
         IC    @05,LOGCPUID                                        0571 02051000
         SLA   @05,2                                               0571 02052000
         L     @01,CVTPTR                                          0571 02053000
         L     @01,CVTPCCAT(,@01)                                  0571 02054000
         L     CATPTR,PCCAT00P(@05,@01)                            0571 02055000
         LA    CATPTR,PCCACAT(,CATPTR)                             0571 02056000
         L     @05,VRYTPTR                                         0571 02057000
         SLR   @03,@03                                             0571 02058000
         IC    @03,HEXCHNAD(,@05)                                  0571 02059000
         SLA   @03,3                                               0571 02060000
         ALR   CATPTR,@03                                          0571 02061000
*           IF CATNOP='1'B THEN     /* CHANNEL ONLINE?       @ZA00826*/ 02062000
         TM    CATNOP(CATPTR),B'01000000'                          0572 02063000
         BNO   @RF00572                                            0572 02064000
*             NOTCMPLT='1'B;        /* SET ERROR FLAG        @ZA00826*/ 02065000
         OI    NOTCMPLT,B'00010000'                                0573 02066000
*           ELSE                                                   0574 02067000
*                                                                  0574 02068000
*             /*******************************************************/ 02069000
*             /*                                                     */ 02070000
*             /* ATTEMPT TO PUT DEVICE ONLINE ONLY IF IT IS OFFLINE, */ 02071000
*             /* NOT BEING USED, AND IT'S OFFLINE FOR REASONS OTHER  */ 02072000
*             /* THAN THE OPERATOR REQUESTED IT TO GO OFFLINE.       */ 02073000
*             /*                                                     */ 02074000
*             /*******************************************************/ 02075000
*                                                                  0574 02076000
*             IF(UCBONLI='0'B)&     /* IS THE DEVICE OFFLINE &       */ 02077000
*                 (UCBNALOC='0'B)&  /* NOT IN USE BY OLTEP &         */ 02078000
*                 (UCBVORSN='0'B)   /* NOT OFFLINE BECAUSE OF A SPEC.   02079000
*                                      OPERATOR REQUEST?             */ 02080000
*               THEN                /* YES, WE CAN ATTEMPT TO PLACE     02081000
*                                      DEVICE ONLINE                 */ 02082000
         B     @RC00572                                            0574 02083000
@RF00572 TM    UCBONLI(UCBPTR),B'10000000'                         0574 02084000
         BNZ   @RF00574                                            0574 02085000
         TM    UCBNALOC(UCBPTR),B'00000100'                        0574 02086000
         BNZ   @RF00574                                            0574 02087000
         TM    UCBVORSN(UCBPTR),B'00000100'                        0574 02088000
         BNZ   @RF00574                                            0574 02089000
*               DO;                                                0575 02090000
*                                                                  0575 02091000
*                 /***************************************************/ 02092000
*                 /*                                                 */ 02093000
*                 /* PATH OPERATIONAL, DEVICE CAN BE PUT ONLINE. SET */ 02094000
*                 /* BITS IN THE UCB, RELEASE SERIALIZATION OF THE   */ 02095000
*                 /* UCB'S, AND WRITE AN SMF RECORD.                 */ 02096000
*                 /*                                                 */ 02097000
*                 /***************************************************/ 02098000
*                                                                  0576 02099000
*                 UCBONLI='1'B;     /* DEVICE IS ON-LINE.            */ 02100000
         OI    UCBONLI(UCBPTR),B'10000000'                         0576 02101000
*                 DVCEON='1'B;      /* SET FLAG FOR DEVICE ONLIN MSG */ 02102000
         OI    DVCEON,B'00100000'                                  0577 02103000
*                 UCBVHRSN='0'B;    /* (OPERATIONAL) PATH TO DEVICE     02104000
*                                      EXISTS.                       */ 02105000
*                                                                  0578 02106000
         NI    UCBVHRSN(UCBPTR),B'11111101'                        0578 02107000
*                 /***************************************************/ 02108000
*                 /*                                                 */ 02109000
*                 /* UPON MAKING THE DEVICE ONLINE, AN SMF RECORD    */ 02110000
*                 /* MUST BE WRITTEN.                                */ 02111000
*                 /*                                                 */ 02112000
*                 /***************************************************/ 02113000
*                                                                  0579 02114000
*                 CALL SMF(9);      /* WRITE TYPE-9 SMF RECORD.      */ 02115000
         LA    @01,@AL00579                                        0579 02116000
         BAL   @14,SMF                                             0579 02117000
*               END;                /* FINISHED TAKING THE DEVICE  0580 02118000
*                                      ONLINE.                       */ 02119000
*         END;                      /* FINISHED DETERMINING IF DEVICE   02120000
*                                      SHOULD BE TAKEN ONLINE.       */ 02121000
*     END;                          /* END PROCESS RC OF 0 FROM    0582 02122000
*                                      IOSGEN                        */ 02123000
*ONEXIT:                                                           0583 02124000
*   ;                               /* CALL TO DEQUCBS REMOVED.    0583 02125000
*                                                            @G55AP2L*/ 02126000
*                                                                  0583 02127000
*   /*****************************************************************/ 02128000
*   /*                                                               */ 02129000
*   /* RETURN TO MAINLINE.                                           */ 02130000
*   /*                                                               */ 02131000
*   /*****************************************************************/ 02132000
*                                                                  0584 02133000
*   RETURN;                                                        0584 02134000
@EL00006 DS    0H                                                  0584 02135000
@EF00006 DS    0H                                                  0584 02136000
@ER00006 LM    @14,@12,@SA00006                                    0584 02137000
         BR    @14                                                 0584 02138000
*   END VARYON;                                                    0585 02139000
         B     @EL00006                                            0585 02140000
*                                                                  0586 02141000
*/* IOSGENV - INTERNAL ROUTINE FOR IOSGEN VARY FUNCTION TO           */ 02142000
*/*           VARY DEVICE ONLINE OR OFFLINE .                        */ 02143000
*/*                                                                  */ 02144000
*/*  INPUT - VARYTABL SET UP WITH IOSGEN VARY PARAMETERS             */ 02145000
*/*  OUTPUT - RETCODE SET TO IOSGEN RETURN CODE                      */ 02146000
*/*                                                                  */ 02147000
*                                                                  0586 02148000
*IOSGENV:                                                          0586 02149000
*   PROC OPTIONS(NOSAVEAREA);                                      0586 02150000
IOSGENV  STM   @14,@01,12(@13)                                     0586 02151000
         STM   @03,@12,32(@13)                                     0586 02152000
*   CVARTABL=VARYTABL;              /* SAVE A COPY OF PARAMETERS.    */ 02153000
         L     @03,VRYTPTR                                         0587 02154000
         MVC   CVARTABL(8),VARYTABL(@03)                           0587 02155000
*   ADDR(CVARTABL)->RANSTRT='0'B;   /* TURN OFF INTERNAL FLAG.     0588 02156000
*                                                            @G55AP2L*/ 02157000
         LA    @03,CVARTABL                                        0588 02158000
         NI    RANSTRT(@03),B'01111111'                            0588 02159000
*   SAVE13=REG13;                   /* SAVE OUR SAVE AREA PTR.       */ 02160000
         ST    REG13,SAVE13                                        0589 02161000
*   REG13=ADDR(INTSAVE);            /* GIVE IOSGEN ANOTHER SAVE AREA    02162000
*                                      TO USE.                       */ 02163000
*                                                                  0590 02164000
         LA    REG13,INTSAVE                                       0590 02165000
*   /*****************************************************************/ 02166000
*   /*                                                               */ 02167000
*   /* CALL IOSGEN VARY FUNCTION. GENERATE IN-LINE CODE              */ 02168000
*   /*                                                               */ 02169000
*   /*****************************************************************/ 02170000
*                                                                  0591 02171000
*   GEN REFS(UCBOB,UCBNAME,CVTPTR,CVTMAP,CVTILK2,CVTIXAVL,CVTPCCAT,     02172000
*       IOCOM,IOCGENA,UCBCHAN,UCBFL5)( IOSGEN VARY,TABLE=CVARTABL);/*   02173000
*                                                            @G55AP2L*/ 02174000
          IOSGEN VARY,TABLE=CVARTABL                                    02175000
*   REG13=SAVE13;                   /* RESTORE POINTER TO OUR SAVE 0592 02176000
*                                      AREA.                         */ 02177000
         L     REG13,SAVE13                                        0592 02178000
*   RETCODE=REG15;                  /* SAVE IOSGEN'S RETURN CODE.    */ 02179000
         LR    RETCODE,REG15                                       0593 02180000
*   RETURN;                         /* RETURN TO CALLER              */ 02181000
@EL00007 DS    0H                                                  0594 02182000
@EF00007 DS    0H                                                  0594 02183000
@ER00007 LM    @14,@01,12(@13)                                     0594 02184000
         LM    @03,@12,32(@13)                                     0594 02185000
         BR    @14                                                 0594 02186000
*   END IOSGENV;                                                   0595 02187000
*                                                                  0596 02188000
*/* DEQUCBS- END SERIALIZATION ON THE UCB'S.                         */ 02189000
*/*                                                                  */ 02190000
*/*   INPUT- NONE.                                                   */ 02191000
*/*   OUTPUT- UCBENQ- OFF, HAVE DEQ'ED THE UCB'S.                    */ 02192000
*                                                                  0596 02193000
*DEQUCBS:                                                          0596 02194000
*   PROC OPTIONS(NOSAVEAREA);                                      0596 02195000
DEQUCBS  STM   @14,@12,12(@13)                                     0596 02196000
*   DO;                             /* DEQ (SYSIEFSD,Q4,2,SYSTEM)  0597 02197000
*                                      RET(HAVE)                     */ 02198000
*     I048(1:20)=I048(1:20)&&I048(1:20);                           0598 02199000
         XC    I048(20),I048                                       0598 02200000
*     RFY                                                          0599 02201000
*       GPR01F RESTRICTED;                                         0599 02202000
*     I04804(1)='11000000'B;        /* LISTEND BYTE                  */ 02203000
         MVI   I04804,B'11000000'                                  0600 02204000
*     I04805(1)=2;                  /* SET LENGTH                    */ 02205000
         MVI   I04805,X'02'                                        0601 02206000
*     I04806(1)='01000001'B;        /* OPTIONS                       */ 02207000
         MVI   I04806,B'01000001'                                  0602 02208000
*     I04807(1)=ADDR(SYSIEFSD);     /* QNAME ADDRESS                 */ 02209000
         LA    @00,SYSIEFSD                                        0603 02210000
