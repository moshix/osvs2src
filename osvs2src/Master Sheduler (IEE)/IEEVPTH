*                                                                  0883 03311000
*/*    SMF- WRITE AN SMF RECORDFOR DEVICES WHICH HAVE BECOME ONLINE OR  03312000
*                                                                    */ 03313000
*/*         OFFLINE ( RECORD TYPES 9 AND 11 RESPECTIVELY).           */ 03314000
*/*                                                                  */ 03315000
*/*     INPUT- RECTYPE- TYPE OF SMF RECORD.                          */ 03316000
*/*     OUTPUT- ENDSMFRC- ON, AN SMF RECORD WAS WRITTEN.             */ 03317000
*/*                                                                  */ 03318000
*                                                                  0883 03319000
*SMF:                                                              0883 03320000
*   PROC(RECTYPE) OPTIONS(NOSAVEAREA);                             0883 03321000
SMF      STM   @14,@12,12(@13)                                     0883 03322000
         MVC   @PC00012(4),0(@01)                                  0883 03323000
*   DCL                                                            0884 03324000
*     RECTYPE FIXED;                /* =9, ONLINE RECORD TYPE. =11,     03325000
*                                      OFFLINE RECORD TYPE.          */ 03326000
*   RECSMF=RECSMF&&RECSMF;          /* INITIALIZE SMF RECORD.        */ 03327000
*                                                                  0885 03328000
         XC    RECSMF(24),RECSMF                                   0885 03329000
*   /*****************************************************************/ 03330000
*   /*                                                               */ 03331000
*   /* NOTE: SINCE RECORD TYPES 9 AND 11 BOTH HAVE THE SAME FORMAT,  */ 03332000
*   /* MAPPING OF TYPE-11 RECORD IS DONE FOR BOTH.                   */ 03333000
*   /*                                                               */ 03334000
*   /*****************************************************************/ 03335000
*                                                                  0886 03336000
*   SMF11PTR=ADDR(RECSMF);          /* ESTABLISH ADDRESSABILITY TO 0886 03337000
*                                      SMF RECORD.                   */ 03338000
         LA    @05,RECSMF                                          0886 03339000
         LR    SMF11PTR,@05                                        0886 03340000
*   SMF11RTY=RECTYPE;               /* GET RECORD TYPE.              */ 03341000
         L     @03,@PC00012                                        0887 03342000
         MVC   SMF11RTY(1,SMF11PTR),RECTYPE+3(@03)                 0887 03343000
*   SMF11LEN=24;                    /* TOTAL RECORD LENGTH.          */ 03344000
         MVC   SMF11LEN(2,SMF11PTR),@CH00054                       0888 03345000
*   SMF11SEG=0;                     /* NO RECORD SPANNING.           */ 03346000
         SLR   @03,@03                                             0889 03347000
         STH   @03,SMF11SEG(,SMF11PTR)                             0889 03348000
*   SMF11LN=6;                      /* THIS FIELD + DEVICE SECTION= 6   03349000
*                                      BYTES OF STORAGE.             */ 03350000
*                                                                  0890 03351000
         MVC   SMF11LN(2,SMF11PTR),@CH00253                        0890 03352000
*   /*****************************************************************/ 03353000
*   /*                                                               */ 03354000
*   /* NOW, FILL IN DEVICE CLASS AND TYPE, AND CHANNEL AND UNIT      */ 03355000
*   /* ADDRESS FIELDS IN THE DEVICE SECTION OF THE SMF RECORD.       */ 03356000
*   /*                                                               */ 03357000
*   /*****************************************************************/ 03358000
*                                                                  0891 03359000
*   SMF11PTR=ADDR(RECSMF)+LENGTH(SMFRCD11);/* ESTABLISH            0891 03360000
*                                      ADDRESSABILITY TO DEVICE    0891 03361000
*                                      SECTION.                      */ 03362000
         AH    @05,@CH02923                                        0891 03363000
         LR    SMF11PTR,@05                                        0891 03364000
*   SMF11PTR->SMF11DUT(1:1)=DEVCLASS;/* FILL IN DEVICE CLASS.        */ 03365000
         MVC   SMF11DUT(1,SMF11PTR),DEVCLASS                       0892 03366000
*   SMF11PTR->SMF11DUT(2:2)=DEVTYPE;/* FILL IN DEVICE TYPE.          */ 03367000
*                                                                  0893 03368000
         MVC   SMF11DUT+1(1,SMF11PTR),DEVTYPE                      0893 03369000
*   /*****************************************************************/ 03370000
*   /*                                                               */ 03371000
*   /* GET HEXADECIMAL CHANNEL AND UNIT ADDRESSES, EACH INTO SEPARATE*/ 03372000
*   /* BYTES IN THE SMF11CUA FIELD.                                  */ 03373000
*   /*                                                               */ 03374000
*   /*****************************************************************/ 03375000
*                                                                  0894 03376000
*   SMF11PTR->SMF11CUA=UCBCHAN;     /* FILL IN CHANNEL, UNIT     @'S.*/ 03377000
         MVC   SMF11CUA(2,SMF11PTR),UCBCHAN(UCBPTR)                0894 03378000
*   GEN( SMFWTM RECSMF );           /* WRITE THE SMF RECORD.         */ 03379000
