*   /*                                                               */ 02670000
*   /*****************************************************************/ 02671000
*                                                                  0709 02672000
*   LENBAS=LENGTH(WPLLGH)+LENGTH(WPLMCSF);/* GET LENGTH OF BASE OF 0709 02673000
*                                      WTO PARAMETER LIST.           */ 02674000
         LA    LENBAS,4                                            0709 02675000
*   WTOPLIST=WTOPLIST&&WTOPLIST;    /* ZERO OUT ENTIRE FIELD.        */ 02676000
