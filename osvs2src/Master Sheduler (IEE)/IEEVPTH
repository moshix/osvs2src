         BAL   @14,DYNMSG                                          0752 02842000
*       CALL WTO;                   /* ISSUE WTO.                    */ 02843000
         BAL   @14,WTO                                             0753 02844000
*       GOTO NEXTDEV;               /* CHECK FOR ANOTHER DEVICE.   0754 02845000
*                                                            @G55AP2L*/ 02846000
         B     NEXTDEV                                             0754 02847000
*     END;                                                         0755 02848000
*                                                                  0755 02849000
*   /*****************************************************************/ 02850000
*   /*                                                               */ 02851000
*   /* CHECK FOR PATH NOT OPERATIONAL FLAG                           */ 02852000
*   /*                                                               */ 02853000
*   /*****************************************************************/ 02854000
*                                                                  0756 02855000
*   IF PTHNOTOP='1'B THEN           /* PATH NOT OPERATIONAL? @ZA00826*/ 02856000
@RF00748 TM    PTHNOTOP,B'00010000'                                0756 02857000
         BNO   @RF00756                                            0756 02858000
*     DO;                                                          0757 02859000
*       WPLLGH=LENGTH(MSGNOTOP)+LENBAS;/* ASSIGN WPL LENGTH  @ZA00826*/ 02860000
         L     @03,WPLPTR                                          0758 02861000
         LA    @09,36                                              0758 02862000
         ALR   @09,LENBAS                                          0758 02863000
         STH   @09,WPLLGH(,@03)                                    0758 02864000
*       WPLTXT=MSGNOTOP;            /* MOVE MESSAGE TO LINE  @ZA00826*/ 02865000
         MVI   WPLTXT+36(@03),C' '                                 0759 02866000
         MVC   WPLTXT+37(89,@03),WPLTXT+36(@03)                    0759 02867000
         MVC   WPLTXT(36,@03),MSGNOTOP                             0759 02868000
*       CALL DYNMSG(LENGTH(NOTOPSTA));/* FILL IN PATH FIELD  @ZA00826*/ 02869000
         LA    @01,@AL00760                                        0760 02870000
         BAL   @14,DYNMSG                                          0760 02871000
*       CALL WTO;                   /* WRITE OUT MESSAGE     @ZA00826*/ 02872000
         BAL   @14,WTO                                             0761 02873000
*       GOTO NEXTDEV;               /* CHECK FOR ANOTHER DEVICE.   0762 02874000
*                                                            @G55AP2L*/ 02875000
         B     NEXTDEV                                             0762 02876000
*     END;                                                         0763 02877000
*                                                                  0763 02878000
*   /*****************************************************************/ 02879000
*   /*                                                               */ 02880000
*   /* IF THE PATH WASN'T VARY'ED AND IT WAS BECAUSE OF AN ERROR     */ 02881000
*   /* SITUATION OTHER THAN THE ONES ALREADY ACCOUNTED FOR, WE MUST  */ 02882000
*   /* DETERMINE WHAT THAT SITUATION WAS, AND ISSUE THE APPROPRIATE  */ 02883000
*   /* MESSAGE.                                                      */ 02884000
*   /*                                                               */ 02885000
*   /*****************************************************************/ 02886000
*                                                                  0764 02887000
*   IF PTHVARED='0'B THEN           /* WAS THE PATH NOT VARY'ED?     */ 02888000
@RF00756 TM    PTHVARED,B'10000000'                                0764 02889000
         BNZ   @RF00764                                            0764 02890000
*     DO;                           /* YES, DETERMINE THE CONDITION  */ 02891000
*                                                                  0765 02892000
*       /*************************************************************/ 02893000
*       /*                                                           */ 02894000
*       /* IF ERROR WAS AN INVALID ATTEMPT TO VARY THE LAST PATH TO  */ 02895000
*       /* THE DEVICE, ISSUE THE CORRESPONDING ERROR MESSAGE.        */ 02896000
*       /*                                                           */ 02897000
*       /*************************************************************/ 02898000
*                                                                  0766 02899000
*       IF LASTPATH='1'B THEN       /* INVALID REQUEST- LAST PATH?   */ 02900000
         TM    LASTPATH,B'01000000'                                0766 02901000
         BNO   @RF00766                                            0766 02902000
*         DO;                       /* YES,                          */ 02903000
*           WPLLGH=LENGTH(MSGLAST)+LENBAS;/* GET WPL LENGTH.         */ 02904000
         L     @03,WPLPTR                                          0768 02905000
         LA    @09,54                                              0768 02906000
         ALR   @09,LENBAS                                          0768 02907000
         STH   @09,WPLLGH(,@03)                                    0768 02908000
*           WPLTXT=MSGLAST;         /* GET MESSAGE TEXT INTO WPL.    */ 02909000
         MVI   WPLTXT+54(@03),C' '                                 0769 02910000
         MVC   WPLTXT+55(71,@03),WPLTXT+54(@03)                    0769 02911000
         MVC   WPLTXT(54,@03),MSGLAST                              0769 02912000
*           CALL DYNMSG(LENGTH(LASTSTA));/* FILL IN PATH FIELD.      */ 02913000
         LA    @01,@AL00770                                        0770 02914000
         BAL   @14,DYNMSG                                          0770 02915000
*           CALL WTO;               /* ISSUE MSG.                    */ 02916000
         BAL   @14,WTO                                             0771 02917000
*           GOTO NEXTDEV;           /* CHECK FOR ANOTHER DEVICE.   0772 02918000
*                                                            @G55AP2L*/ 02919000
         B     NEXTDEV                                             0772 02920000
*         END;                      /* END 'LAST PATH' PROCESSING.   */ 02921000
*       IF RESERPTH='1'B THEN       /* OR WAS ERROR DUE TO AN ATTEMPT   02922000
*                                      TO VARY A PATH TO A SHARED  0774 02923000
*                                      DASD DEVICE?                  */ 02924000
@RF00766 TM    RESERPTH,B'00100000'                                0774 02925000
         BNO   @RF00774                                            0774 02926000
*         DO;                       /* YES,                          */ 02927000
*           WPLLGH=LENGTH(MSGRESV)+LENBAS;/* ASSIGN WPL LENGTH.      */ 02928000
         L     @03,WPLPTR                                          0776 02929000
         LA    @09,44                                              0776 02930000
         ALR   @09,LENBAS                                          0776 02931000
         STH   @09,WPLLGH(,@03)                                    0776 02932000
*           WPLTXT=MSGRESV;         /* MOVE MESSAGE TEXT INTO WPL.   */ 02933000
         MVI   WPLTXT+44(@03),C' '                                 0777 02934000
         MVC   WPLTXT+45(81,@03),WPLTXT+44(@03)                    0777 02935000
         MVC   WPLTXT(44,@03),MSGRESV                              0777 02936000
*           CALL DYNMSG(LENGTH(RESVSTA));/* FILL IN PATH FIELD.      */ 02937000
         LA    @01,@AL00778                                        0778 02938000
         BAL   @14,DYNMSG                                          0778 02939000
*           CALL WTO;               /* ISSUE MSG.                    */ 02940000
         BAL   @14,WTO                                             0779 02941000
*           GOTO NEXTDEV;           /* CHECK FOR ANOTHER DEVICE    0780 02942000
*                                                            @G55AP2L*/ 02943000
*                                                                  0780 02944000
         B     NEXTDEV                                             0780 02945000
*           /*********************************************************/ 02946000
*           /*                                                       */ 02947000
*           /* PATH NOT VARY'ED. NO AUTHORIZATION, SYNTAX, PATH      */ 02948000
*           /* VALIDITY, LAST PATH, OR RESERVED PATH ERRORS. THE     */ 02949000
*           /* COMMAND PROCESSOR MUST HAVE FAILED, WE WILL ISSUE A   */ 02950000
*           /* MESSAGE STATING THIS.                                 */ 02951000
*           /*                                                       */ 02952000
*           /*********************************************************/ 02953000
*                                                                  0781 02954000
*         END;                      /* END PATH RESERVED MSG. PRO- 0781 02955000
*                                      CESSING.                      */ 02956000
*                                                                  0781 02957000
*       /*************************************************************/ 02958000
*       /*                                                           */ 02959000
*       /* ELSE COMMAND PROCESSOR FAILED DON'T ISSUE COMMAND         */ 02960000
*       /* PROCESSOR FAILED MESSAGE IF IT WAS ALREADY ISSUED BY THE  */ 02961000
*       /* ESTAE ROUTINE. IF THE ESTAE ROUTINE IS ENTERED, IT ALWAYS */ 02962000
*       /* ISSUES THE COMMAND PROCESSOR FAILED MESSAGE.              */ 02963000
*       /*                                                           */ 02964000
*       /*************************************************************/ 02965000
*                                                                  0782 02966000
*       IF ERRENTRY='0'B THEN       /* WAS ESTAE NOT ENTERED?        */ 02967000
@RF00774 TM    ERRENTRY,B'00001000'                                0782 02968000
         BNZ   @RF00782                                            0782 02969000
*         DO;                       /* YES, ESTAE NOT ENTERED.       */ 02970000
*FAIL:                                                             0784 02971000
*           WPLLGH=LENGTH(MSGFAIL)+LENBAS;/* SET BUFFER LENGTH.      */ 02972000
FAIL     L     @03,WPLPTR                                          0784 02973000
         LA    @09,43                                              0784 02974000
         ALR   @09,LENBAS                                          0784 02975000
         STH   @09,WPLLGH(,@03)                                    0784 02976000
*           WPLTXT=MSGFAIL;         /* MOVE MESSAGE TEXT TO WPL.     */ 02977000
         MVI   WPLTXT+43(@03),C' '                                 0785 02978000
         MVC   WPLTXT+44(82,@03),WPLTXT+43(@03)                    0785 02979000
         MVC   WPLTXT(43,@03),MSGFAIL                              0785 02980000
*           CALL WTO;               /* ISSUE MESSAGE.                */ 02981000
         BAL   @14,WTO                                             0786 02982000
*         END;                      /* OF COMMAND-PROCESSOR FAILURE     02983000
*                                      MESSAGE - ISSUING CASE.       */ 02984000
*       GO TO FREE;                 /* FREE RESOURCES AND EXIT SUB 0788 02985000
*                                      ROUTINE                       */ 02986000
         B     FREE                                                0788 02987000
*     END;                          /* OF HANDLING ERRORS WHERE A  0789 02988000
*                                      PATH WASN'T VARY'ED.          */ 02989000
*                                                                  0789 02990000
*   /*****************************************************************/ 02991000
*   /*                                                               */ 02992000
*   /* NOW, ISSUE THE PATH VARY'ED (ONLINE OR OFFLINE) MESSAGE.      */ 02993000
*   /*                                                               */ 02994000
*   /*****************************************************************/ 02995000
*                                                                  0790 02996000
*   IF ONLINREQ='1'B THEN           /* PATH MADE ONLINE?             */ 02997000
@RF00764 TM    ONLINREQ,B'00001000'                                0790 02998000
         BNO   @RF00790                                            0790 02999000
*     DO;                           /* YES,                          */ 03000000
*       WPLLGH=LENGTH(MSGON)+LENBAS;/* ASSIGN WPL LENGTH.            */ 03001000
         L     @03,WPLPTR                                          0792 03002000
         LA    @09,27                                              0792 03003000
         ALR   @09,LENBAS                                          0792 03004000
         STH   @09,WPLLGH(,@03)                                    0792 03005000
*       WPLTXT=MSGON;               /* MOVE TEXT INTO WPL.           */ 03006000
         MVI   WPLTXT+27(@03),C' '                                 0793 03007000
         MVC   WPLTXT+28(98,@03),WPLTXT+27(@03)                    0793 03008000
         MVC   WPLTXT(27,@03),MSGON                                0793 03009000
*       CALL DYNMSG(LENGTH(ONSTA)); /* FILL IN PATH FIELD.           */ 03010000
*                                                                  0794 03011000
         LA    @01,@AL00794                                        0794 03012000
         BAL   @14,DYNMSG                                          0794 03013000
*       /*************************************************************/ 03014000
*       /*                                                           */ 03015000
*       /* CHECK IF NOT VERIFIED FLAG HAS BEEN SET                   */ 03016000
*       /*                                                           */ 03017000
*       /*************************************************************/ 03018000
*                                                                  0795 03019000
*       IF NOTVER='1'B THEN         /* PATH VERIFIED?        @ZA00826*/ 03020000
         TM    NOTVER,B'00001000'                                  0795 03021000
         BNO   @RF00795                                            0795 03022000
*         DO;                                                      0796 03023000
*           WPLLGH=WPLLGH+LENGTH(NTVER);/* ADD LENGTH OF NEW TEXT  0797 03024000
*                                                            @ZA00826*/ 03025000
         L     @03,WPLPTR                                          0797 03026000
         LA    @09,13                                              0797 03027000
         AH    @09,WPLLGH(,@03)                                    0797 03028000
         STH   @09,WPLLGH(,@03)                                    0797 03029000
*           WPLTXT1=NTVER;          /* APPEND NOT VERIFIED MESSAGE 0798 03030000
*                                                            @ZA00826*/ 03031000
         MVC   WPLTXT1(13,@03),NTVER                               0798 03032000
*         END;                                                     0799 03033000
*       CALL WTO;                   /* ISSUE MSG.                    */ 03034000
@RF00795 BAL   @14,WTO                                             0800 03035000
*     END;                                                         0801 03036000
*   ELSE                            /* PATH MADE OFFLINE, ISSUE MSG. */ 03037000
*     DO;                                                          0802 03038000
         B     @RC00790                                            0802 03039000
@RF00790 DS    0H                                                  0803 03040000
*       WPLLGH=LENGTH(MSGOFF)+LENBAS;/* ASSIGN WPL LENGTH.           */ 03041000
         L     @03,WPLPTR                                          0803 03042000
         LA    @09,28                                              0803 03043000
         ALR   @09,LENBAS                                          0803 03044000
         STH   @09,WPLLGH(,@03)                                    0803 03045000
*       WPLTXT=MSGOFF;              /* MOVE TEXT INTO WPL.           */ 03046000
         MVI   WPLTXT+28(@03),C' '                                 0804 03047000
         MVC   WPLTXT+29(97,@03),WPLTXT+28(@03)                    0804 03048000
         MVC   WPLTXT(28,@03),MSGOFF                               0804 03049000
*       CALL DYNMSG(LENGTH(OFFSTA));/* FILL IN PATH FIELD.           */ 03050000
         LA    @01,@AL00805                                        0805 03051000
         BAL   @14,DYNMSG                                          0805 03052000
*       CALL WTO;                   /* ISSUE MESSAGE.                */ 03053000
         BAL   @14,WTO                                             0806 03054000
*     END;                                                         0807 03055000
*                                                                  0807 03056000
*   /*****************************************************************/ 03057000
*   /*                                                               */ 03058000
*   /* IF ATTEMPT TO VARY PATH ONLINE AND PATH WAS NOT OPERATIONAL,  */ 03059000
*   /* THEN WE MUST ISSUE THAT MESSAGE.                              */ 03060000
*   /*                                                               */ 03061000
*   /*****************************************************************/ 03062000
*                                                                  0808 03063000
*   IF NOTCMPLT='1'B THEN           /* WAS THE PATH FOUND TO BE NOT     03064000
*                                      OPERATIONAL?          @ZA00826*/ 03065000
@RC00790 TM    NOTCMPLT,B'00010000'                                0808 03066000
         BNO   @RF00808                                            0808 03067000
*     DO;                           /* YES,                          */ 03068000
*       WPLLGH=LENGTH(MSGNTCMP)+LENBAS;/* ASSIGN BUFFER LENGTH.      */ 03069000
         L     @03,WPLPTR                                          0810 03070000
         LA    @09,30                                              0810 03071000
         ALR   @09,LENBAS                                          0810 03072000
         STH   @09,WPLLGH(,@03)                                    0810 03073000
*       WPLTXT=MSGNTCMP;            /* MOVE MSG INTO WPL.    @ZA00826*/ 03074000
         MVI   WPLTXT+30(@03),C' '                                 0811 03075000
         MVC   WPLTXT+31(95,@03),WPLTXT+30(@03)                    0811 03076000
         MVC   WPLTXT(30,@03),MSGNTCMP                             0811 03077000
*       CALL DYNMSG(LENGTH(NOTCMPM));/* FILL IN PATH FIELD.  @ZA00826*/ 03078000
         LA    @01,@AL00812                                        0812 03079000
         BAL   @14,DYNMSG                                          0812 03080000
*       CALL WTO;                   /* WRITE OUT MESSAGE.            */ 03081000
         BAL   @14,WTO                                             0813 03082000
*     END;                                                         0814 03083000
*                                                                  0814 03084000
*   /*****************************************************************/ 03085000
*   /*                                                               */ 03086000
*   /* IF DEVICE WAS TAKEN OFFLINE, ISSUE MESSAGE.                   */ 03087000
*   /*                                                               */ 03088000
*   /*****************************************************************/ 03089000
*                                                                  0815 03090000
*   IF DVCEOFF='1'B THEN            /* WAS DEVICE PLACED OFFLINE?    */ 03091000
@RF00808 TM    DVCEOFF,B'01000000'                                 0815 03092000
         BNO   @RF00815                                            0815 03093000
*     DO;                           /* YES,                          */ 03094000
*       WPLLGH=LENGTH(MSGDOFF)+LENBAS;/* ASSIGN WPL LENGTH.          */ 03095000
         L     @03,WPLPTR                                          0817 03096000
         LA    @09,19                                              0817 03097000
         ALR   @09,LENBAS                                          0817 03098000
         STH   @09,WPLLGH(,@03)                                    0817 03099000
*       WPLTXT=MSGDOFF;             /* MOVE TEXT INTO WPL.           */ 03100000
         MVI   WPLTXT+19(@03),C' '                                 0818 03101000
         MVC   WPLTXT+20(106,@03),WPLTXT+19(@03)                   0818 03102000
         MVC   WPLTXT(19,@03),MSGDOFF                              0818 03103000
*       WPLTXT(9:11)=UCBNAME;       /* GET BASE DEVICE ADDRESS IN  0819 03104000
*                                      EBCDIC.                       */ 03105000
         MVC   WPLTXT+8(3,@03),UCBNAME(UCBPTR)                     0819 03106000
*       CALL WTO;                   /* ISSUE MESSAGE.                */ 03107000
         BAL   @14,WTO                                             0820 03108000
*     END;                                                         0821 03109000
*                                                                  0821 03110000
*   /*****************************************************************/ 03111000
*   /*                                                               */ 03112000
*   /* IF DEVICE WAS PLACED ONLINE, ISSUE MESSAGE.                   */ 03113000
*   /*                                                               */ 03114000
*   /*****************************************************************/ 03115000
*                                                                  0822 03116000
*   IF DVCEON='1'B THEN             /* WAS DEVICE PLACED OFFLINE?    */ 03117000
@RF00815 TM    DVCEON,B'00100000'                                  0822 03118000
         BNO   @RF00822                                            0822 03119000
*     DO;                           /* YES,                          */ 03120000
*       WPLLGH=LENGTH(MSGDON)+LENBAS;/* ASSIGN WPL LENGTH.           */ 03121000
         L     @03,WPLPTR                                          0824 03122000
         LA    @09,18                                              0824 03123000
         ALR   @09,LENBAS                                          0824 03124000
         STH   @09,WPLLGH(,@03)                                    0824 03125000
*       WPLTXT=MSGDON;              /* MOVE TEXT INTO WPL.           */ 03126000
         MVI   WPLTXT+18(@03),C' '                                 0825 03127000
         MVC   WPLTXT+19(107,@03),WPLTXT+18(@03)                   0825 03128000
         MVC   WPLTXT(18,@03),MSGDON                               0825 03129000
*       WPLTXT(9:11)=UCBNAME;       /* GET BASE DEVICE ADDRESS IN  0826 03130000
*                                      EBCDIC.                       */ 03131000
         MVC   WPLTXT+8(3,@03),UCBNAME(UCBPTR)                     0826 03132000
*       CALL WTO;                   /* ISSUE MESSAGE.                */ 03133000
         BAL   @14,WTO                                             0827 03134000
*     END;                                                         0828 03135000
*                                                                  0828 03136000
*   /*****************************************************************/ 03137000
*   /*                                                               */ 03138000
*   /* THIS CODE DETEMINES WHETHER ANOTHER DEVICE MUST BE PROCESSED. */ 03139000
*   /* IF ANOTHER DEVICE EXISTS A BRANCH WILL BE MADE BACK TO THE    */ 03140000
*   /* MAINLINE CODE TO PROCESS IT AFTER RESETTING ANY PREVIOUS ERROR*/ 03141000
*   /* INDICATORS OR SWITCHES.                               @G55AP2L*/ 03142000
*   /*                                                               */ 03143000
*   /*****************************************************************/ 03144000
*                                                                  0829 03145000
*NEXTDEV:                                                          0829 03146000
*   ;                               /*                       @G55AP2L*/ 03147000
@RF00822 DS    0H                                                  0829 03148000
NEXTDEV  DS    0H                                                  0830 03149000
*   IF RANSTRT='1'B&                /* DEVICE RANGE START?   @G55AP2L*/ 03150000
*       (HEXDEVAD+1<(VRYTPTR+LENGTH(VARYTABL))->/*           @G55AP2L*/ 03151000
*       HEXDEVAD) THEN                                             0830 03152000
         L     @09,VRYTPTR                                         0830 03153000
         TM    RANSTRT(@09),B'10000000'                            0830 03154000
         BNO   @RF00830                                            0830 03155000
         LA    @08,1                                               0830 03156000
         AH    @08,HEXDEVAD(,@09)                                  0830 03157000
         CH    @08,HEXDEVAD+8(,@09)                                0830 03158000
         BNL   @RF00830                                            0830 03159000
*     HEXDEVAD=HEXDEVAD+1;          /*                       @G55AP2L*/ 03160000
         STH   @08,HEXDEVAD(,@09)                                  0831 03161000
*   ELSE                                                           0832 03162000
*     VRYTPTR=VRYTPTR+LENGTH(VARYTABL);/*                    @G55AP2L*/ 03163000
         B     @RC00830                                            0832 03164000
@RF00830 LA    @09,8                                               0832 03165000
         AL    @09,VRYTPTR                                         0832 03166000
         ST    @09,VRYTPTR                                         0832 03167000
*   IF VRYTPTR<VARYPTR+VRYTBLNG THEN                               0833 03168000
@RC00830 L     @09,VARYPTR                                         0833 03169000
         AL    @09,VRYTBLNG                                        0833 03170000
         C     @09,VRYTPTR                                         0833 03171000
         BNH   @RF00833                                            0833 03172000
*     DO;                           /*                       @G55AP2L*/ 03173000
*       NOTAUTH='0'B;               /* ISSUER IS AUTHORIZED. @G55AP2L*/ 03174000
         NI    NOTAUTH,B'01111111'                                 0835 03175000
*       MAINSWCH='0'B;              /* DENOTE NO ERRORS      @G55AP2L*/ 03176000
         NI    MAINSWCH,B'01111111'                                0836 03177000
*       VALIDPTH='1'B;              /* ASSUME PATH SYSGENED  @G55AP2L*/ 03178000
         OI    VALIDPTH,B'00000001'                                0837 03179000
*       PTHNOTOP='0'B;              /* ASSUME PATH OPERATIONAL     0838 03180000
*                                                            @G55AP2L*/ 03181000
*       PTHVARED='0'B;              /* IOSGEN VARY FUNCTION NOT    0839 03182000
*                                      INVOKED               @G55AP2L*/ 03183000
*       LASTPATH='0'B;              /* RESET LAST PATH INDICATOR   0840 03184000
*                                                            @G55AP2L*/ 03185000
*       RESERPTH='0'B;              /* PATH NOT PATH TO SHARED DASD     03186000
*                                                            @G55AP2L*/ 03187000
         NI    PTHNOTOP,B'00001111'                                0841 03188000
*       NOTVER='0'B;                /* RESET NOT VERIFIED INDICATOR     03189000
*                                                            @G55AP2L*/ 03190000
*       NOTCMPLT='0'B;              /* PATH IS VALID         @G55AP2L*/ 03191000
*       DVCEOFF='0'B;               /* DEVICE NOT TAKEN OFFLINE    0844 03192000
*                                                            @G55AP2L*/ 03193000
*       DVCEON='0'B;                /* DEVICE NOT BROUGHT ONLINE   0845 03194000
*                                                            @G55AP2L*/ 03195000
         NI    NOTVER,B'10000111'                                  0845 03196000
*       IF UNCOND='1'B THEN                                        0846 03197000
         TM    UNCOND,B'00000100'                                  0846 03198000
         BNO   @RF00846                                            0846 03199000
*         UNCONREQ='1'B;            /* UNCONDITIONAL REQUEST       0847 03200000
*                                      ORIGINALLY            @G55AP2L*/ 03201000
         OI    UNCONREQ,B'00000100'                                0847 03202000
*       GOTO DEVLOOP;               /* GO AND PROCESS THIS DEVICE. 0848 03203000
*                                                            @G55AP2L*/ 03204000
         B     DEVLOOP                                             0848 03205000
*     END;                          /* END ANOTHER DEVICE FOUND.   0849 03206000
*                                                            @G55AP2L*/ 03207000
*                                                                  0849 03208000
*   /*****************************************************************/ 03209000
*   /*                                                               */ 03210000
*   /* FREE THE RESOURCES WE OWN.                                    */ 03211000
*   /*                                                               */ 03212000
*   /*****************************************************************/ 03213000
*                                                                  0850 03214000
*FREE:                                                             0850 03215000
*   ;                                                              0850 03216000
@RF00833 DS    0H                                                  0850 03217000
FREE     DS    0H                                                  0851 03218000
*   IF IOPMLOAD='1'B THEN                                          0851 03219000
         TM    IOPMLOAD,B'10000000'                                0851 03220000
         BNO   @RF00851                                            0851 03221000
*     DO;                           /* WAS IECVIOPM LOADED?  @ZA20196*/ 03222000
*       GEN( DELETE EP=IECVIOPM);   /* YES, DELETE IECVIOPM  @ZA20196*/ 03223000
          DELETE EP=IECVIOPM                                            03224000
*     END;                          /* END DELETE IECVIOPM   @ZA20196*/ 03225000
*   IF UCBENQ='1'B THEN             /* DO WE OWN THE UCB'S?          */ 03226000
@RF00851 TM    UCBENQ,B'00000100'                                  0855 03227000
         BNO   @RF00855                                            0855 03228000
*     CALL DEQUCBS;                 /* YES, RELEASE THEM.            */ 03229000
         BAL   @14,DEQUCBS                                         0856 03230000
*   IF VARYENQ='1'B THEN            /* ARE WE STILL SERIALIZED ON    */ 03231000
@RF00855 TM    VARYENQ,B'00000010'                                 0857 03232000
         BNO   @RF00857                                            0857 03233000
*     DO;                           /* OTHER VARY COMMANDS?          */ 03234000
*                                                                  0858 03235000
*       /*************************************************************/ 03236000
*       /*                                                           */ 03237000
