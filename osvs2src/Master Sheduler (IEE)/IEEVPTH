         TITLE ' IEEVPTH: VARY PATH COMMAND PROCESSOR'                  00001000
IEEVPTH  CSECT ,                                                   0001 00002000
@MAINENT DS    0H                                                  0001 00003000
         USING *,@15                                               0001 00004000
         B     @PROLOG                                             0001 00005000
         DC    AL1(16)                                             0001 00006000
         DC    C'IEEVPTH   78.209'                                 0001 00007000
         DROP  @15                                                      00008000
@PROLOG  BALR  @11,0                                               0001 00009000
@PSTART  LA    @12,4095(,@11)                                      0001 00010000
         USING @PSTART,@11                                         0001 00011000
         USING @PSTART+4095,@12                                    0001 00012000
         LR    @09,@01                                             0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @10,@01                                             0001 00016000
         USING @DATD,@10                                           0001 00017000
         LR    @01,@09                                             0001 00018000
         ST    @13,@SA00001+4                                      0001 00019000
         LR    @13,@10                                             0001 00020000
         MVC   @PC00001(4),0(@01)                                  0001 00021000
*                                      DCL                   @ZA20333*/ 00022000
*   CSCBPTR=REG1;                   /* GET PTR TO CSCB.              */ 00023000
         ST    REG1,CSCBPTR                                        0129 00024000
*   SAVE14=REG14;                   /* SAVE RETURN POINT.            */ 00025000
*                                                                  0130 00026000
         ST    REG14,SAVE14                                        0130 00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* GET DATA FOR DUMPS.                                           */ 00030000
*   /*                                                               */ 00031000
*   /*****************************************************************/ 00032000
*                                                                  0131 00033000
*   ENTRYPT=ADDR(IEEVPTH);          /* GET START OF MODULE ADDRESS.  */ 00034000
         L     @09,@CA00028                                        0131 00035000
         ST    @09,ENTRYPT                                         0131 00036000
*   STARTMWA=REG10;                 /* REG10 IS DATAREG- HAS ADDRESS    00037000
*                                      OF START OF MAIN WORK AREA.   */ 00038000
         ST    REG10,STARTMWA                                      0132 00039000
*   ENDMWA=REG10+@SIZDATD;          /* ADDRESS OF END= ADDRESS OF  0133 00040000
*                                      START + LENGTH.               */ 00041000
*                                                                  0133 00042000
         LR    @09,REG10                                           0133 00043000
         AL    @09,@SIZDATD                                        0133 00044000
         ST    @09,ENDMWA                                          0133 00045000
*   /*****************************************************************/ 00046000
*   /*                                                               */ 00047000
*   /* INITIALIZE LOOP-CONTROL SWITCHES.                             */ 00048000
*   /*                                                               */ 00049000
*   /*****************************************************************/ 00050000
*                                                                  0134 00051000
*   IOPMLOAD='0'B;                  /* IECVIOPM NOT LOADED YET     0134 00052000
*                                                            @ZA20196*/ 00053000
         NI    IOPMLOAD,B'01111111'                                0134 00054000
*   MAINSWCH='0'B;                  /* TURNED ON ONLY IN CASE OF   0135 00055000
*                                      ERROR.                        */ 00056000
*                                                                  0135 00057000
         NI    MAINSWCH,B'01111111'                                0135 00058000
*   /*****************************************************************/ 00059000
*   /*                                                               */ 00060000
*   /* ASSUMPTIONS INDICATED BY FLAG SETTINGS ARE:                   */ 00061000
*   /*                                                               */ 00062000
*   /*****************************************************************/ 00063000
*                                                                  0136 00064000
*   NOTAUTH='0'B;                   /* COMMAND'S ISSUER IS         0136 00065000
*                                      AUTHROIZED.                   */ 00066000
         NI    NOTAUTH,B'01111111'                                 0136 00067000
*   NOAUTH='0'B;                    /* ISSUER IS AUTHROIZED. @G55AP2L*/ 00068000
         NI    NOAUTH,B'11111110'                                  0137 00069000
*   SGCONSLE='0'B;                  /* PATH DOESN'T REPRESENT A PATH    00070000
*                                      TO A SYSGEN'ED CONSOLE.       */ 00071000
*   IOAUTH2='0'B;                   /* ISSUER NOT AUTHORIZED TO VARY    00072000
*                                      PATHS OF NON-SYSGEN'ED CONSOLE   00073000
*                                      I/O DEVICES.                  */ 00074000
*   CONAUTH3='0'B;                  /* ISSUER NOT AUTHORIZED TO VARY    00075000
*                                      PATHS OF SYSGEN'ED CONSOLE  0140 00076000
*                                      DEVICES.                      */ 00077000
*   ONLINREQ='0'B;                  /* ASSUME AN OFF-LINE REQUEST.   */ 00078000
         NI    SGCONSLE,B'10000111'                                0141 00079000
*   NOTCMPLT='0'B;                  /* PATH IS VALID         @ZA00826*/ 00080000
*   NOTVER='0'B;                    /* PATH NOT VERIFIED     @ZA00826*/ 00081000
         NI    NOTCMPLT,B'11100111'                                0143 00082000
*   UNCONREQ='0'B;                  /* THE LAST PATH TO THE DEVICE 0144 00083000
*                                      MAY NOT BE VARY'ED.           */ 00084000
         NI    UNCONREQ,B'11111011'                                0144 00085000
*   UNCOND='0'B;                    /* NOT UNCONDITIONAL REQUEST   0145 00086000
*                                                            @G55AP2L*/ 00087000
*   GETMFAIL='0'B;                  /* SET GETMAIN NOT FAILED YET. 0146 00088000
*                                                            @G55AP2L*/ 00089000
         NI    UNCOND,B'11111001'                                  0146 00090000
*   SYNTXVAL='1'B;                  /* COMMAND IS SYNTACTICALLY    0147 00091000
*                                      CORRECT.                      */ 00092000
*   VALIDPTH='1'B;                  /* PATH WAS SYSGEN'ED.           */ 00093000
         OI    SYNTXVAL,B'00000011'                                0148 00094000
*   PTHVARED='0'B;                  /* IOSGEN VARY FUNCTION HASN'T 0149 00095000
*                                      BEEN (SUCCESSFULLY) INVOKED.  */ 00096000
*   LASTPATH='0'B;                  /* THERE HAS NOT BEEN AN INVALID    00097000
*                                      ATTEMPT TO REMOVE A LAST PATH    00098000
*                                      TO A DEVICE.                  */ 00099000
*   RESERPTH='0'B;                  /* PATH NOT A PATH TO SHARED   0151 00100000
*                                      DASD.                         */ 00101000
*   PTHNOTOP='0'B;                  /* PATH TO A DEVICE BROUGHT ON-     00102000
*                                      LINE IS OPERATIONAL.          */ 00103000
*   ERRENTRY='0'B;                  /* ESTAE HASN'T BEEN INVOKED.    */ 00104000
*   VARYENQ='0'B;                   /* HAVEN'T ENQ'ED ON THE       0154 00105000
*                                      OPERATION OF OTHER VARY     0154 00106000
*                                      FUNCTIONS.                    */ 00107000
*   UCBENQ='0'B;                    /* HAVE NOT ISSUED ENQ ON UCB'S. */ 00108000
*   HIERFLG='0'B;                   /* HIERARCHY BIT SHOULDN'T BE SET*/ 00109000
         NI    PTHVARED,B'00000000'                                0156 00110000
*   OFFLINFG='0'B;                  /* DON'T TURN UCBONLI OFF.       */ 00111000
*   DVCEOFF='0'B;                   /* HAVEN'T TAKEN A DEVICE OFFLINE*/ 00112000
*   DVCEON='0'B;                    /* HAVEN'T PLACED DEVICE ONLINE  */ 00113000
*                                                                  0159 00114000
         NI    OFFLINFG,B'00011111'                                0159 00115000
*   /*****************************************************************/ 00116000
*   /*                                                               */ 00117000
*   /* SET UP THE RECOVERY ENVIRONMENT WITH AN ESTAE EXIT. GET VALUES*/ 00118000
*   /* FOR CODE AND DATA REGISTERS TO BE RESTORED BY ESTAE ROUTINE.  */ 00119000
*   /*                                                               */ 00120000
*   /*****************************************************************/ 00121000
*                                                                  0160 00122000
*   DREG=REG10;                     /* GET DYNAMIC DATA REGISTER.    */ 00123000
         ST    REG10,DREG                                          0160 00124000
*   CREG1=REG11;                    /* SAVE BASE REGISTER CONTENTS.  */ 00125000
         ST    REG11,CREG1                                         0161 00126000
*   CREG2=REG12;                    /* GET SECOND BASE REG. CONTENTS.*/ 00127000
         ST    REG12,CREG2                                         0162 00128000
*   RFY                                                            0163 00129000
*    (REG2) RSTD;                                                  0163 00130000
*   REG2=ADDR(PARMESTA);            /* GET ADDRESS OF PARAMETER LIST    00131000
*                                      FOR ESTAE EXIT ROUTINE.       */ 00132000
         LA    REG2,PARMESTA                                       0164 00133000
*   REG1=ADDR(ESTLIST);             /* GET ADDRESS OF ESTAE SVC    0165 00134000
*                                      PARAM- ETER LIST.             */ 00135000
         LA    REG1,ESTLIST                                        0165 00136000
*   ESTLIST=ESTLIST&&ESTLIST;       /* INITIALIZE PARAMETER LIST.    */ 00137000
         XC    ESTLIST(16),ESTLIST                                 0166 00138000
*   GEN( ESTAE VPTHESTA,PARAM=(2),MF=(E,(1)));/* MAKE ESTAE EXIT   0167 00139000
*                                      KNOWN TO SYSTEM.              */ 00140000
          ESTAE VPTHESTA,PARAM=(2),MF=(E,(1))                           00141000
*   RFY                                                            0168 00142000
*    (REG2) UNRSTD;                                                0168 00143000
*                                                                  0168 00144000
*/*  WILL SKIP FURTHER PROCESSING OF COMMAND IF ESTAE NOT SET UP.    */ 00145000
*                                                                  0169 00146000
*   IF REG15^=0 THEN                /* IF FAILURE TO SET UP ESTAE  0169 00147000
*                                      ROUTINE                       */ 00148000
         LTR   REG15,REG15                                         0169 00149000
         BZ    @RF00169                                            0169 00150000
*     MAINSWCH='1'B;                /* THEN INDICATE TO BYPASS OTHER    00151000
*                                      PROCESSING AND THEN TO ENTER     00152000
*                                      THE CLEAN-UP ROUTINE.         */ 00153000
         OI    MAINSWCH,B'10000000'                                0170 00154000
*/*  CONTINUE ONLY IF NO ERRORS SO FAR.                              */ 00155000
*                                                                  0171 00156000
*   IF MAINSWCH='0'B THEN           /* NO ERRORS THUS FAR?           */ 00157000
@RF00169 TM    MAINSWCH,B'10000000'                                0171 00158000
         BNZ   @RF00171                                            0171 00159000
*     DO;                           /* YES,                          */ 00160000
*                                                                  0172 00161000
*       /*************************************************************/ 00162000
*       /*                                                           */ 00163000
*       /* ESTABLISH ADDRESSABILITY TO UCM BASE AND UCM PREFIX.      */ 00164000
*       /*                                                           */ 00165000
*       /*************************************************************/ 00166000
*                                                                  0173 00167000
*       UCMBASPT=CVTCUCB;           /* UCMBASPT PTS TO THE UCM.      */ 00168000
         L     @09,CVTPTR                                          0173 00169000
         L     UCMBASPT,CVTCUCB(,@09)                              0173 00170000
*       UCMMCSPT=UCMPRFXP;          /* GET ADDRESS OF UCM PREFIX.    */ 00171000
*                                                                  0174 00172000
         LR    @09,UCMBASPT                                        0174 00173000
         AL    @09,@CF03287                                        0174 00174000
         MVC   UCMMCSPT(4),UCMPRFXP(@09)                           0174 00175000
*       /*************************************************************/ 00176000
*       /*                                                           */ 00177000
*       /* DETERMINE IF THE ISSUER OF THE COMMAND IS AUTHORIZED. IF  */ 00178000
*       /* IS AUTHORIZED, SAVE THE CODES INDICATING THE TYPES OF     */ 00179000
*       /* DEVICES FOR WHICH HE IS AUTHORIZED: SYSGEN'ED CONSOLE     */ 00180000
*       /* DEVICES ONLY, NON-SYSGEN'ED CONSOLE DEVICES, OR BOTH      */ 00181000
*       /* (IOAUTH2 AND CONAUTH3 BOTH ON ).                          */ 00182000
*       /*                                                           */ 00183000
*       /*************************************************************/ 00184000
*                                                                  0175 00185000
*       CALL CMDAUTH;               /* ROUTINE DETERMINES COMMAND  0175 00186000
*                                      ISSUER'S AUTHORIZATION.       */ 00187000
*                                                                  0175 00188000
         BAL   @14,CMDAUTH                                         0175 00189000
*       /*************************************************************/ 00190000
*       /*                                                           */ 00191000
*       /* STOP FURTHER PROCESSING IF COMMAND ISSUER HAS NO          */ 00192000
*       /* AUTHORIZATIONS AT ALL.                                    */ 00193000
*       /*                                                           */ 00194000
*       /*************************************************************/ 00195000
*                                                                  0176 00196000
*       IF NOAUTH='1'B THEN         /* WAS THE ISSUER NOT AUTHORIZED?   00197000
*                                                            @G55AP2L*/ 00198000
         TM    NOAUTH,B'00000001'                                  0176 00199000
         BNO   @RF00176                                            0176 00200000
*         MAINSWCH='1'B;            /* YES, THEN SET THE SWITCH      */ 00201000
         OI    MAINSWCH,B'10000000'                                0177 00202000
*     END;                                                         0178 00203000
*                                                                  0178 00204000
@RF00176 DS    0H                                                  0179 00205000
*   /*****************************************************************/ 00206000
*   /*                                                               */ 00207000
*   /* CONTINUE ONLY IF NO ERRORS FOUND SO FAR.                      */ 00208000
*   /*                                                               */ 00209000
*   /*****************************************************************/ 00210000
*                                                                  0179 00211000
*   IF MAINSWCH='0'B THEN           /* HAVE THERE BEEN NO ERRORS SO     00212000
*                                      FAR?                          */ 00213000
@RF00171 TM    MAINSWCH,B'10000000'                                0179 00214000
         BNZ   @RF00179                                            0179 00215000
*     CALL SYNTXCHK;                /* CHECK THE COMMAND FOR       0180 00216000
*                                      SYNTACTIC CORRECTNESS.        */ 00217000
*                                                                  0180 00218000
         BAL   @14,SYNTXCHK                                        0180 00219000
*   /*****************************************************************/ 00220000
*   /*                                                               */ 00221000
*   /* CONTINUE PROCESSING ONLY IF NO SYNTAX ERRORS FOUND.           */ 00222000
*   /*                                                               */ 00223000
*   /*****************************************************************/ 00224000
*                                                                  0181 00225000
*   IF SYNTXVAL='0'B|GETMFAIL='1'B THEN/* IF ERROR IN SYNTAX ROUTINE    00226000
*                                      THEN                  @G55AP2L*/ 00227000
@RF00179 TM    SYNTXVAL,B'00000010'                                0181 00228000
         BZ    @RT00181                                            0181 00229000
         TM    GETMFAIL,B'00000010'                                0181 00230000
         BNO   @RF00181                                            0181 00231000
@RT00181 DS    0H                                                  0182 00232000
*     MAINSWCH='1'B;                /* INDICATE SKIP OTHER MAINLINE  */ 00233000
*                                                                  0182 00234000
         OI    MAINSWCH,B'10000000'                                0182 00235000
*   /*****************************************************************/ 00236000
*   /*                                                               */ 00237000
*   /* MAKE FURTHER SUBROUTINE CALLS ONLY IF NO ERRORS ENCOUNTERED UP*/ 00238000
*   /* TO THIS POINT.                                                */ 00239000
*   /*                                                               */ 00240000
*   /*****************************************************************/ 00241000
*                                                                  0183 00242000
*   GEN( LOAD EP=IECVIOPM);         /* LOAD IECVIOPM         @ZA20196*/ 00243000
@RF00181 DS    0H                                                  0183 00244000
          LOAD EP=IECVIOPM                                              00245000
*   IOPMADDR=REG0;                  /* SAVE IECVIOPM ENTRY ADDR    0184 00246000
*                                                            @ZA20196*/ 00247000
         ST    REG0,IOPMADDR                                       0184 00248000
*   IOPMLOAD='1'B;                  /* INDICATE TO DELETE    @ZA20196*/ 00249000
         OI    IOPMLOAD,B'10000000'                                0185 00250000
*   VRYTPTR=VARYPTR;                /* POINT TO FIRST DEVICE TO    0186 00251000
*                                      PROCESS               @G55AP2L*/ 00252000
*                                                                  0186 00253000
         MVC   VRYTPTR(4),VARYPTR                                  0186 00254000
*   /*****************************************************************/ 00255000
*   /*                                                               */ 00256000
*   /* THIS IS THE START OF THE LOOP TO PROCESS ALL THE DEVICES      */ 00257000
*   /* ENTERED AS INPUT. IT IS BRANCHED DIRECTLY TO FROM THE CLEANUP */ 00258000
*   /* ROUTINE WHEN THE CLEANUP ROUTINE IS NOT ENTERED FOR ESTAE     */ 00259000
*   /* PROCESSING OR A SYNTAX ERROR OR AN AUTHORITY ERROR AND ANOTHER*/ 00260000
*   /* DEVICE HAS TO BE PROCESSED.                           @G55AP2L*/ 00261000
*   /*                                                               */ 00262000
*   /*****************************************************************/ 00263000
*                                                                  0187 00264000
*DEVLOOP:                                                          0187 00265000
*   ;                               /* START OF DEVICE(S) PROCESSING    00266000
*                                      LOOP                  @G55AP2L*/ 00267000
DEVLOOP  DS    0H                                                  0188 00268000
*   IF MAINSWCH='0'B THEN           /* NO ERRORS THUS FAR?           */ 00269000
         TM    MAINSWCH,B'10000000'                                0188 00270000
         BNZ   @RF00188                                            0188 00271000
*     DO;                           /* YES,                          */ 00272000
*       CALL UCBCKOUT;              /* OBTAIN UCB ADDRESS FOR THE  0190 00273000
*                                      DEVICE, DETERMINE IF THE LAST    00274000
*                                      PATH TO THE DEVICE CAN BE   0190 00275000
*                                      VARY'ED (I.E., SHOULD THE VARY   00276000
*                                      BE UNCONDITIONAL?).           */ 00277000
*                                                                  0190 00278000
         BAL   @14,UCBCKOUT                                        0190 00279000
*       /*************************************************************/ 00280000
*       /*                                                           */ 00281000
*       /* IF PATH NOT VALID, SIGNAL TO BYPASS FURTHER SUBROUTINE    */ 00282000
*       /* CALLS                                                     */ 00283000
*       /*                                                           */ 00284000
*       /*************************************************************/ 00285000
*                                                                  0191 00286000
*       IF VALIDPTH='0'B THEN       /* IF PATH ISN'T VALID, THEN WE  */ 00287000
         TM    VALIDPTH,B'00000001'                                0191 00288000
         BNZ   @RF00191                                            0191 00289000
*         MAINSWCH='1'B;            /* WANT TO BYPASS OTHER        0192 00290000
*                                      PROCESSING.                   */ 00291000
         OI    MAINSWCH,B'10000000'                                0192 00292000
*     END;                                                         0193 00293000
*                                                                  0193 00294000
@RF00191 DS    0H                                                  0194 00295000
*   /*****************************************************************/ 00296000
*   /*                                                               */ 00297000
*   /* PERFORM FURTHER SUBROUTINE CALLS ONLY IF NO ERRORS ENCOUNTERED*/ 00298000
*   /* UP TO THIS POINT.                                             */ 00299000
*   /*                                                               */ 00300000
*   /*****************************************************************/ 00301000
*                                                                  0194 00302000
*   IF MAINSWCH='0'B THEN           /* NO ERRORS YET?                */ 00303000
@RF00188 TM    MAINSWCH,B'10000000'                                0194 00304000
         BNZ   @RF00194                                            0194 00305000
*     DO;                                                          0195 00306000
*       CALL POSCONCK;              /* DETERMINE IF THE DEVICE IS  0196 00307000
*                                      (SYSGEN'ED AS A ) CONSOLE. IF    00308000
*                                      SO, SPECIAL AUTHORIZATION IS     00309000
*                                      NEEDED TO 'VARY' ITS PATHS.   */ 00310000
*                                                                  0196 00311000
         BAL   @14,POSCONCK                                        0196 00312000
*       /*************************************************************/ 00313000
*       /*                                                           */ 00314000
*       /* BYPASS OTHER PROCESSING IF COMMAND ISSUER DOESN'T HAVE THE*/ 00315000
*       /* REQUIRED AUTHORIZATION.                                   */ 00316000
*       /*                                                           */ 00317000
*       /*************************************************************/ 00318000
*                                                                  0197 00319000
*       IF((SGCONSLE='1'B)&(CONAUTH3='0'B))|/* DO WE HAVE A SYSGEN'ED   00320000
*                                      CONSOLE, COMMAND ISSUER     0197 00321000
*                                      DOESN'T HAVE CONSOLE        0197 00322000
*                                      AUTHORIZATION, OR             */ 00323000
*           ((SGCONSLE='0'B)&(IOAUTH2='0'B))/* DEVICE IS NOT A     0197 00324000
*                                      CONSOLE, BUT THE COMMAND    0197 00325000
*                                      ISSUER ISN'T AUTH- ORIZED FOR    00326000
*                                      VARYING THE OTHER I/O DEVICE     00327000
*                                      PATHS?                        */ 00328000
*         THEN                      /* YES, COMMAND ISSUER NOT     0197 00329000
*                                      AUTHORIZED.                   */ 00330000
         TM    SGCONSLE,B'01000000'                                0197 00331000
         BNO   @GL00002                                            0197 00332000
         TM    CONAUTH3,B'00010000'                                0197 00333000
         BZ    @RT00197                                            0197 00334000
@GL00002 TM    SGCONSLE,B'01100000'                                0197 00335000
         BNZ   @RF00197                                            0197 00336000
@RT00197 DS    0H                                                  0198 00337000
*         DO;                                                      0198 00338000
*           NOTAUTH='1'B;           /* SET THE NOT-AUTHORIZED FLAG: A   00339000
*                                      WT0 WILL BE ISSUED INDICATING    00340000
*                                      THAT THE COMMAND ISSUER WASN'T   00341000
*                                      AUTHORIZED.                   */ 00342000
         OI    NOTAUTH,B'10000000'                                 0199 00343000
*           MAINSWCH='1'B;          /* BYPASS OTHER MAINLINE       0200 00344000
*                                      PROCESSING.                   */ 00345000
         OI    MAINSWCH,B'10000000'                                0200 00346000
*         END;                                                     0201 00347000
*     END;                                                         0202 00348000
*                                                                  0202 00349000
@RF00197 DS    0H                                                  0203 00350000
*   /*****************************************************************/ 00351000
*   /*                                                               */ 00352000
*   /* PROCESS VARY PATH REQUEST IF NO ERRORS                        */ 00353000
*   /*                                                               */ 00354000
*   /*****************************************************************/ 00355000
*                                                                  0203 00356000
*   IF MAINSWCH='0'B THEN           /* UNLESS ERRORS WERE FOUND,     */ 00357000
@RF00194 TM    MAINSWCH,B'10000000'                                0203 00358000
         BNZ   @RF00203                                            0203 00359000
*     DO;                           /* VARY THE PATH.                */ 00360000
*       CPUIDLOG=LOGCPUID;          /* PUT CPU ID INTO VARYTABL.   0205 00361000
*                                                            @G55AP2L*/ 00362000
*                                                                  0205 00363000
         L     @09,VRYTPTR                                         0205 00364000
         MVC   CPUIDLOG(1,@09),LOGCPUID                            0205 00365000
*       /*************************************************************/ 00366000
*       /*                                                           */ 00367000
*       /* ROUTE PROCESSING TO OFFLINE OR ONLINE CONTROLLER, AS      */ 00368000
*       /* REQUIRED.                                                 */ 00369000
*       /*                                                           */ 00370000
*       /*************************************************************/ 00371000
*                                                                  0206 00372000
*       IF ONLINREQ='0'B THEN                                      0206 00373000
         TM    ONLINREQ,B'00001000'                                0206 00374000
         BNZ   @RF00206                                            0206 00375000
*         CALL VARYOFF;             /* OFFLINE REQUEST               */ 00376000
*                                                                  0207 00377000
         BAL   @14,VARYOFF                                         0207 00378000
*       /*************************************************************/ 00379000
*       /*                                                           */ 00380000
*       /* VARYON HANDLES REQUESTS FOR ONLINE REQUESTS.              */ 00381000
*       /*                                                           */ 00382000
*       /*************************************************************/ 00383000
*                                                                  0208 00384000
*       ELSE                        /* ONLINE REQUESTS.              */ 00385000
*         CALL VARYON;              /* PROCESS ONLINE REQUEST      0208 00386000
*                                                            @ZA20196*/ 00387000
         B     @RC00206                                            0208 00388000
@RF00206 BAL   @14,VARYON                                          0208 00389000
*     END;                                                         0209 00390000
@RC00206 DS    0H                                                  0210 00391000
*   CALL CLEANUP;                   /* FREE RESOURCES WHICH WE     0210 00392000
*                                      OBTAINED AND ISSUE THE PROPER    00393000
*                                      MESSAGE(S) IN RESPONSE TO THE    00394000
*                                      INPUT COMMAND.                */ 00395000
@RF00203 BAL   @14,CLEANUP                                         0210 00396000
*   GEN( ESTAE 0);                  /* REMOVE ESTAE FROM STACK.      */ 00397000
          ESTAE 0                                                       00398000
*   REG14=SAVE14;                   /* RESTORE RETURN POINT.         */ 00399000
         L     REG14,SAVE14                                        0212 00400000
*   RETURN;                         /* RETURN TO SYSTEM.             */ 00401000
@EL00001 L     @13,4(,@13)                                         0213 00402000
@EF00001 L     @00,@SIZDATD                                        0213 00403000
         LR    @01,@10                                             0213 00404000
         FREEMAIN R,LV=(0),A=(1)                                        00405000
@ER00001 BR    @14                                                 0213 00406000
*                                                                  0214 00407000
*/* STARTOF SYNTAX CHECKING ROUTINE.                                 */ 00408000
*/*SYNTXCHK - EXAMINE THE COMMAND TEXT LOOKING FOR THE               */ 00409000
*/* FOLLOWING INFORMATION: DEVICE ADDRESS, CPU ADDRESS,              */ 00410000
*/* ONLINE OR OFFLINE REQUEST, WHETHER THE REQUEST IS                */ 00411000
*/* A REQUEST FOR A PATH TO BE VARY'ED OFFLINE UNCOND-              */ 00412000
*/* DITIONALLY.                                                      */ 00413000
*/*INPUT - CHBUF (IN THE CSCB) HAS THE COMMAND.                      */ 00414000
*/*OUTPUT -                                                          */ 00415000
*/* . HEXDEVAD-DEVICE ADDRESS IN HEXADECIAML                         */ 00416000
*/* . OFFONIND-X'00'IF OFFLINE REQUEST,                              */ 00417000
*/* .         -X'04' IF ONLINE REQUEST                               */ 00418000
*/* . LOGCPUID- LOGICAL CPU IDENTIFIER.                              */ 00419000
*/* . EBCDEVAD-DEVICE ADDRESS IN EBCDIC.                             */ 00420000
*/* . SYNTXVAL-SET OFF IF THERE IS A SYNTAX ERROR.                   */ 00421000
*/* . ONLINREQ-SET ON IF THERE IS AN ONLINE REQUEST.                 */ 00422000
*                                                                  0214 00423000
*SYNTXCHK:                                                         0214 00424000
*   PROC OPTIONS(NOSAVEAREA);                                      0214 00425000
SYNTXCHK STM   @14,@12,12(@13)                                     0214 00426000
*   SCANPTR=5;                      /* INIT THE INDEX TO POINT TO THE   00427000
*                                      FIRST CHAR AFTER 'PATH'       */ 00428000
