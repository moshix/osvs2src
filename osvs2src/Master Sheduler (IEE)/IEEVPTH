*   GEN( MVN ZONES+1(1),TEMP+3);    /* 3RD HEX = 2ND CHAR.   @G55AP2L*/ 03523000
          MVN ZONES+1(1),TEMP+3                                         03524000
*   DO I=1 TO 3;                    /* CORRECT ZONE C-TYPE         0924 03525000
*                                      CHARACTERS.           @G55AP2L*/ 03526000
