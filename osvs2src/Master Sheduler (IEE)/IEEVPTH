*   WPLDESCE='1'B;                  /* MSG IS RESPONSE TO A COMMAND. */ 03441000
         OI    WPLDESCE(@08),B'00001000'                           0906 03442000
*   REG0=CONSLEID;                  /* SEND MSG TO CORRECT CONSOLE.  */ 03443000
         LH    REG0,CONSLEID                                       0907 03444000
*   WPLPTR=ADDR(WTOPLIST);          /* POINT TO BASE OF PARM. LIST   */ 03445000
         LA    @09,WTOPLIST                                        0908 03446000
