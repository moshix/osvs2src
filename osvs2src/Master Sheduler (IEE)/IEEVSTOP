         TITLE 'IEEVSTOP VARY CPU SHUTDOWN                             *00001000
                        '                                               00002000
IEEVSTOP CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(19)                                             0001 00007000
         DC    C'IEEVSTOPR0300075016'                              0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @12,0                                               0001 00010000
@PSTART  DS    0H                                                  0001 00011000
         USING @PSTART,@12                                         0001 00012000
         STNSM  PSAVSTAP,X'FB'                                          00013000
         STAP PSAVSTAP                                                  00014000
*   IF(CSDMASK(PSAVSTAP+1)&CSDCPUAL)^=0 THEN/* HAS CPU BEEN MARKED 0017 00015000
*                                      ALIVE                 @ZA01893*/ 00016000
         LH    @04,PSAVSTAP                                        0017 00017000
         ALR   @04,@04                                             0017 00018000
         L     @01,CVTPTR                                          0017 00019000
         L     @15,CVTCSDRL(,@01)                                  0017 00020000
         SLR   @14,@14                                             0017 00021000
         ICM   @14,3,CSDCPUAL(@15)                                 0017 00022000
         ST    @04,@TF00001                                        0017 00023000
         ALR   @04,@15                                             0017 00024000
         SLR   @11,@11                                             0017 00025000
         ICM   @11,3,CSDMASK(@04)                                  0017 00026000
         NR    @14,@11                                             0017 00027000
         LTR   @14,@14                                             0017 00028000
         BZ    @RF00017                                            0017 00029000
*     DO;                           /* CPU HAS BEEN MARKED ALIVE   0018 00030000
*                                                            @ZA01893*/ 00031000
*       CSDCPUAL=(CSDCPUAL&(CSDMASK(PSAVSTAP+1)&&'FFFF'X));/*      0019 00032000
*                                                            @ZA01893*/ 00033000
         LR    @04,@15                                             0019 00034000
         AL    @04,@TF00001                                        0019 00035000
         MVC   @TS00001(2),CSDMASK(@04)                            0019 00036000
         XC    @TS00001(2),@CB01041                                0019 00037000
         NC    CSDCPUAL(2,@15),@TS00001                            0019 00038000
*       CSDCPUOL=CSDCPUOL-1;        /* DECREMENT ONLINE CPU COUNT  0020 00039000
*                                                            @ZA01893*/ 00040000
         LH    @04,CSDCPUOL(,@15)                                  0020 00041000
         BCTR  @04,0                                               0020 00042000
         STH   @04,CSDCPUOL(,@15)                                  0020 00043000
*     END;                          /* END CPU ALIVE PROCESSING    0021 00044000
*                                                            @ZA01893*/ 00045000
*   R5=PSAVSTAP;                    /* GET CPU ADDRESS       @ZA01893*/ 00046000
@RF00017 LH    R5,PSAVSTAP                                         0022 00047000
*   SPX(RESETPR);                   /* RESET PREFIX REGISTER         */ 00048000
*                                                                  0023 00049000
         SPX   RESETPR                                             0023 00050000
*   /*****************************************************************/ 00051000
*   /*                                                               */ 00052000
*   /* ISSUE SIGP STOP TO CPU THAT IS EXECUTING CODE. IF STOP FAILS  */ 00053000
*   /* THEN NEXT INSTRUCTION WILL LOOP BACK AND RETRY STOP.          */ 00054000
*   /*                                                               */ 00055000
*   /*****************************************************************/ 00056000
*                                                                  0024 00057000
*STOP:                                                             0024 00058000
*   GEN(SIGP 4,5,5(0));             /* SIGP STOP THIS CPU            */ 00059000
STOP     SIGP 4,5,5(0)                                                  00060000
*   GOTO STOP;                      /* LOOP TILL CPU STOPS           */ 00061000
         B     STOP                                                0025 00062000
*   END                                                            0026 00063000
*                                                                  0026 00064000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00065000
*/*%INCLUDE SYSLIB  (IHACSD  )                                       */ 00066000
*/*%INCLUDE SYSLIB  (IHAPSA  )                                       */ 00067000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 00068000
