         L     @07,TPCPTR(,@07)                                    0074 00272000
         L     R15,TPCVPTR(,@07)                                   0074 00273000
*   GEN(BALR  14,15);               /* LINK TO TIMMER ROUTINE        */ 00274000
*                                                                  0075 00275000
         BALR  14,15                                                    00276000
*   /*****************************************************************/ 00277000
*   /*                                                               */ 00278000
*   /* ENABLE FOR MACHINE CHECKS BY LOADING NEW PSW POINTING TO NEXT */ 00279000
*   /* SEQUENTIAL INSTRUCTION                                        */ 00280000
*   /*                                                               */ 00281000
*   /*****************************************************************/ 00282000
*                                                                  0076 00283000
*ENABLE:                                                           0076 00284000
*   LPSW(NEWPSW);                   /* LOAD NEW ENABLED PSW          */ 00285000
*                                                                  0076 00286000
ENABLE   LPSW  NEWPSW                                              0076 00287000
*   /*****************************************************************/ 00288000
*   /*                                                               */ 00289000
*   /* CAT TABLE FOR CPU WILL BE INITIALIZED. ALL CHANNELS WILL BE   */ 00290000
*   /* MARKED ONLINE THAT RESPOND TO A TEST CHANNEL INST. ANY        */ 00291000
*   /* CHANNELS ABOVE THE VALUE IN CSDCHAD WILL BE MARKED AS NOT     */ 00292000
*   /* SYSGENED LOAD ADDRESS OF CAT TABLE TO INITIALIZE              */ 00293000
*   /*                                                               */ 00294000
*   /*****************************************************************/ 00295000
*                                                                  0077 00296000
*CATINIT:                           /* CAT TABLE INITILIZATION     0077 00297000
*                                      ROUTINE                       */ 00298000
*   PCCAPTR=PSAPCCAV;               /* SET PCCA POINTER              */ 00299000
CATINIT  L     PCCAPTR,PSAPCCAV                                    0077 00300000
*   PCCAIO=NOIO;                    /* THIS CPU HAS NO IO    @G47AN2O   00301000
*                                      ABILITY, AS YET.      @G47AN2O*/ 00302000
         NI    PCCAIO(PCCAPTR),B'10111111'                         0078 00303000
*   CATPTR=ADDR(PCCACAT);           /* CAT TABLE ADDRESS             */ 00304000
*                                                                  0079 00305000
         LA    CATPTR,PCCACAT(,PCCAPTR)                            0079 00306000
*   /*****************************************************************/ 00307000
*   /*                                                               */ 00308000
*   /* START WITH CHANNEL ZERO                                       */ 00309000
*   /*                                                               */ 00310000
*   /*****************************************************************/ 00311000
*                                                                  0080 00312000
