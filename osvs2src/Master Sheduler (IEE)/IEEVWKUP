*   /*****************************************************************/ 00898000
*   /*                                                               */ 00899000
*   /* VERIFY THAT CODE IS EXECUTING ON PROPER CPU AND IF NOT RETURN */ 00900000
*   /* TO DISPATCHER TO CONTINUE PROCESSING                          */ 00901000
*   /*                                                               */ 00902000
*   /*****************************************************************/ 00903000
*                                                                  0171 00904000
*   IF CPUADDR^=PSACPUPA THEN       /* CHECK IF PROPER CPU           */ 00905000
         CLC   CPUADDR(2,PARMPTR),PSACPUPA                         0171 00906000
         BE    @RF00171                                            0171 00907000
*     DO;                                                          0172 00908000
*                                                                  0172 00909000
*       /*************************************************************/ 00910000
*       /*                                                           */ 00911000
*       /* WE ARE NOT RUNNING ON THE PROPER CPU RESET DISPATCHER HOOK*/ 00912000
*       /* AND RETURN TO DISPATCHER                                  */ 00913000
*       /*                                                           */ 00914000
*       /*************************************************************/ 00915000
*                                                                  0173 00916000
*       LCCAVCPU='0'B;              /* RESET DISPATCHER HOOK         */ 00917000
         L     @07,LCCAPTR                                         0173 00918000
         NI    LCCAVCPU(@07),B'10111111'                           0173 00919000
*       R15=CVT0DS;                 /* ADDRESS OF DISPATCHER ENTRY   */ 00920000
         L     @07,CVTPTR                                          0174 00921000
         L     R15,CVT0DS(,@07)                                    0174 00922000
*       GEN(BR  R15);               /* BRANCH TO DISPATCHER          */ 00923000
         BR  R15                                                        00924000
*     END;                                                         0176 00925000
*                                                                  0176 00926000
*   /*****************************************************************/ 00927000
*   /*                                                               */ 00928000
*   /* SET UP FOR AND CALL CONTROL REGISTER RESET ROUTINE ROUTINE    */ 00929000
*   /* WILL RESET CONTROL REGISTERS TO RESET VALUE                   */ 00930000
*   /*                                                               */ 00931000
*   /*****************************************************************/ 00932000
*                                                                  0177 00933000
*   R0=12;                          /* SET FUNCTION CODE IN REG      */ 00934000
@RF00171 LA    R0,12                                               0177 00935000
*   R1=CPUADDR;                     /* PICK UP CPU ADDRESS           */ 00936000
*                                                                  0178 00937000
         LH    R1,CPUADDR(,PARMPTR)                                0178 00938000
*   /*****************************************************************/ 00939000
*   /*                                                               */ 00940000
*   /* GET ADDRESS OF RMS ROUTINE AND LINK TO IT                     */ 00941000
*   /*                                                               */ 00942000
*   /*****************************************************************/ 00943000
*                                                                  0179 00944000
*   R15=RVTPBUCR;                   /* ADDRESS OF RESET ROUTINE      */ 00945000
         L     @07,CVTPTR                                          0179 00946000
         L     @07,RVTPTR(,@07)                                    0179 00947000
         L     R15,RVTPBUCR(,@07)                                  0179 00948000
*   GEN(BALR 14,15);                /* LINK TO ROUTINE               */ 00949000
*                                                                  0180 00950000
         BALR 14,15                                                     00951000
*   /*****************************************************************/ 00952000
*   /*                                                               */ 00953000
*   /* SET COMPLETE FLAG FOR MAINLINE CODE                           */ 00954000
*   /*                                                               */ 00955000
*   /*****************************************************************/ 00956000
*                                                                  0181 00957000
*   COMPLET='1'B;                   /* SET COMPLETE FLAG             */ 00958000
*                                                                  0181 00959000
         OI    COMPLET(PARMPTR),B'10000000'                        0181 00960000
*   /*****************************************************************/ 00961000
*   /*                                                               */ 00962000
*   /* BRANCH TO CPU SHUTDOWN CODE TO STOP CPU ROUTINE IS POINTED TO */ 00963000
*   /* BY THE CVT AND WILL RESET CPU PREFIX REGISTER AND SIGP STOP   */ 00964000
*   /* THE CPU.                                                      */ 00965000
*   /*                                                               */ 00966000
*   /*****************************************************************/ 00967000
*                                                                  0182 00968000
*   R15=CVTVSTOP;                   /* LOAD STOP ROUTINE ADDRESS     */ 00969000
         L     @07,CVTPTR                                          0182 00970000
         L     R15,CVTVSTOP(,@07)                                  0182 00971000
*   GEN(BR  15);                    /* BRANCH TO STOP ROUTINE TO STOP*/ 00972000
         BR  15                                                         00973000
*   END                             /* END OF WKUP PROC              */ 00974000
*                                                                  0184 00975000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00976000
*/*%INCLUDE SYSLIB  (CVT     )                                       */ 00977000
