*FLGTEST:                                                          0116 00554000
*                                                                  0116 00555000
*     /***************************************************************/ 00556000
*     /*                                                             */ 00557000
*     /* INCREMENT CAT TABLE POINTER TO NEXT CAT TABLE ENTRY AND     */ 00558000
*     /* INCREMENT CHANNEL ADDRESS                                   */ 00559000
*     /*                                                             */ 00560000
*     /***************************************************************/ 00561000
*                                                                  0116 00562000
*     CATPTR=CATPTR+8;              /* UPDATE TO NEXT CAT ENTRY      */ 00563000
FLGTEST  AL    CATPTR,@CF00046                                     0116 00564000
*     CHNADDR=CHNADDR+'100'X;       /* UPDATE TO NEXT CHANNEL        */ 00565000
         AL    CHNADDR,@CF01767                                    0117 00566000
*   END;                            /* END OF DO LOOP                */ 00567000
*                                                                  0118 00568000
         AL    LOOPCNT,@CF00049                                    0118 00569000
         C     LOOPCNT,@CF00101                                    0118 00570000
         BNH   @DL00082                                            0118 00571000
*   /*****************************************************************/ 00572000
*   /*                                                               */ 00573000
*   /* THE CAT TABLE HAS BEEN INITIALIZED, RESTORE I/O NEW PSW SAVED */ 00574000
*   /* AT BEGINNING.                                                 */ 00575000
*   /*                                                               */ 00576000
*   /*****************************************************************/ 00577000
*                                                                  0119 00578000
*   FLCINPSW=IONSAV;                /* RESTORE I/O NEW PSW           */ 00579000
