*         /***********************************************************/ 00469000
*         /*                                                         */ 00470000
*         /* A CONDITION CODE 1 OR 2 WAS RECEIVED FROM A STIDC       */ 00471000
*         /* INSTRUCTION WE MUST ENABLE FOR I/O INTERUPTIONS TO FLUSH*/ 00472000
*         /* OUT ANY PENDING INTERUPTS IN THE CHANNEL TO PERMIT THE  */ 00473000
*         /* STIDC TO BE SUCCESSFUL. IF THE BAD CONDITION CODE WAS   */ 00474000
*         /* RECEIVED DURRING ACR THEN DO NOT ENABLE FOR I/O, MARK ID*/ 00475000
*         /* INVALID.                                        @YM03538*/ 00476000
*         /*                                                         */ 00477000
*         /***********************************************************/ 00478000
*                                                                  0102 00479000
*         IF FLGTST=12 THEN         /* ACR ENTRY?            @YM03538*/ 00480000
ENABIO   C     FLGTST,@CF00859                                     0102 00481000
         BNE   @RF00102                                            0102 00482000
*           DO;                                                    0103 00483000
*             CATNID='1'B;          /* MARK INVALID          @YM03538*/ 00484000
         OI    CATNID(CATPTR),B'00001000'                          0104 00485000
*             GOTO FLGTEST;         /* CONTINUE TO NEXT CHANNEL    0105 00486000
*                                                            @YM03538*/ 00487000
         B     FLGTEST                                             0105 00488000
*           END;                                                   0106 00489000
*         IOFLUSH=ADDR(STID);       /* POINT I/O NEW TO STIDC      0107 00490000
*                                                            @YM03538*/ 00491000
@RF00102 LA    @07,STID                                            0107 00492000
         ST    @07,IOFLUSH                                         0107 00493000
*         R15=ADDR(FLCINPSW);       /* POINT TO I/O NEW MASK @YM03538*/ 00494000
*                                                                  0108 00495000
         LA    R15,FLCINPSW                                        0108 00496000
*         /***********************************************************/ 00497000
*         /*                                                         */ 00498000
*         /* PLACE CURRENT MASK IN I/O NEW PSW, AND ENABLE CURRENT   */ 00499000
*         /* PSW FOR I/O INTERUPTS                                   */ 00500000
*         /*                                                         */ 00501000
*         /***********************************************************/ 00502000
*                                                                  0109 00503000
*         GEN(STOSM 0(15),X'02');   /* ENABLE FOR I/O INTER  @YM03538*/ 00504000
*                                                                  0109 00505000
         STOSM 0(15),X'02'                                              00506000
*         /***********************************************************/ 00507000
*         /*                                                         */ 00508000
*         /* IF ANY I/O INTERRUPTS ARE PENDING THEN THAY WILL BE     */ 00509000
*         /* TAKEN HERE AND THE FOLLOWING INSTRUCTION WILL NOT BE    */ 00510000
*         /* EXECUTED. IF THE STIDC CONDITION CODE 1 OR 2 WAS NOT    */ 00511000
*         /* CAUSED BY A PENDING INTERRUPT THEN THE NEXT INSTRUCTION */ 00512000
*         /* WILL DISABLE AGAIN AND THE CHANNEL ID MARKED INVALID IN */ 00513000
*         /* THE CAT TABLE. IF A INTERRUPT IS TAKEN, THE I/O NEW PSW */ 00514000
*         /* WILL CAUSE THE STIDC TO BE RETRIED. AND PROCESSING      */ 00515000
*         /* CONTINUED.                                              */ 00516000
*         /*                                                         */ 00517000
*         /***********************************************************/ 00518000
*                                                                  0110 00519000
*         GEN(STNSM 0(15),X'FD');   /* DISABLE FOR I/O AND CONT.   0110 00520000
*                                                            @YM03538*/ 00521000
         STNSM 0(15),X'FD'                                              00522000
*         GOTO CHNINV;              /* MARK CHANNEL ID INVALID.    0111 00523000
*                                                            @YM03538*/ 00524000
*                                                                  0111 00525000
         B     CHNINV                                              0111 00526000
*         /***********************************************************/ 00527000
*         /*                                                         */ 00528000
*         /* IN CASE OF ERROR SET INVALID FLAGS IN CAT ENTRY         */ 00529000
*         /*                                                         */ 00530000
*         /***********************************************************/ 00531000
*                                                                  0112 00532000
*CHNNOP:                                                           0112 00533000
*         CATNOP='1'B;              /* SET NOT OPERATIONAL FLAG      */ 00534000
*                                                                  0112 00535000
CHNNOP   OI    CATNOP(CATPTR),B'01000000'                          0112 00536000
*         /***********************************************************/ 00537000
*         /*                                                         */ 00538000
*         /* SET ID INVALID BIT ONLY IF THIS IS A VARY CPU COMMAND   */ 00539000
*         /* AND THERE WAS NO OLD CAT TABLE IN THE PCCA.             */ 00540000
*         /*                                                         */ 00541000
*         /***********************************************************/ 00542000
*                                                                  0113 00543000
*CHNINV:                                                           0113 00544000
*         IF(FLGTST^=12&CATCHK^='1'B) THEN/* VCPU NO CAT.    @YM03538*/ 00545000
