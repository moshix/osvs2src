         TITLE 'IEEVWKUP VARY CPU WAKEUP AND QUIET CODE                *00001000
                        '                                               00002000
IEEVWKUP CSECT  ,                                                       00003000
         BALR   15,0                                                    00004000
@MAINENT DS     0H                                                      00005000
         USING  *,15                                                    00006000
         B      @PROLOG                                                 00007000
         DC     AL2(@EP00002-@MAINENT)                                  00008000
IEEVWKUP MODID  BR=YES                                                  00009000
IEEVQUIT DS     0H                                                      00010000
         USING  *,15                                                    00011000
         B      @PROLOG                                                 00012000
         DC     AL2(QUITEP-IEEVQUIT)                                    00013000
         ENTRY  IEEVQUIT                                                00014000
IEEVTCH  DS     0H                                                      00015000
         USING  *,15                                                    00016000
         B      @PROLOG                                                 00017000
         DC     AL2(IEETCHEP-IEEVTCH)                                   00018000
         ENTRY  IEEVTCH                                                 00019000
IEEVACR  DS     0H                                                      00020000
         USING  *,15                                                    00021000
         B      @PROLOG                                                 00022000
         DC     AL2(IEEACREP-IEEVACR)                                   00023000
         ENTRY  IEEVACR                                                 00024000
         DROP   15                                                      00025000
@PROLOG  BALR   @12,0                                                   00026000
@PSTART  DS     0H                                                      00027000
         USING  @PSTART,12                                              00028000
         AH     15,4(15)                                                00029000
         BR     15                                                      00030000
VIRTBASE DC     A(@PSTART)                                              00031000
@EP00002 DS     0H                                                      00032000
*                                                                  0039 00033000
*   /*****************************************************************/ 00034000
*   /*                                                               */ 00035000
*   /* PARAMETERS ARE PASSED TO WAKEUP ROUTINE FROM VARY CPU MAINLINE*/ 00036000
*   /* BY PLACING A REAL ADDRESS POINTER IN REAL 0-4K PSA. ROUTINE IS*/ 00037000
*   /* ENTERED BY ISSUING A SIGP RESTART, DAT IS DISABLED UNTIL      */ 00038000
*   /* CONTROL REGS ARE INITIALIZED                                  */ 00039000
*   /*                                                               */ 00040000
*   /*****************************************************************/ 00041000
*                                                                  0039 00042000
*   PARMPTR=PSAWKRAP;               /* LOAD ADDRESS OF PARMS         */ 00043000
*                                                                  0039 00044000
         L     PARMPTR,PSAWKRAP                                    0039 00045000
*   /*****************************************************************/ 00046000
*   /*                                                               */ 00047000
*   /* ZERO FLGTEST REGISTER TO INDICATE NORMAL ENTRY                */ 00048000
*   /*                                                               */ 00049000
*   /*****************************************************************/ 00050000
*                                                                  0040 00051000
*   FLGTST=0;                       /* ZERO FLAG REGISTER            */ 00052000
         SLR   FLGTST,FLGTST                                       0040 00053000
*   STAP(PSAVSTAP);                 /* DO STAP TO CHECK FOR PROPER 0041 00054000
*                                      CPU                           */ 00055000
*                                                                  0041 00056000
         STAP  PSAVSTAP                                            0041 00057000
*   /*****************************************************************/ 00058000
*   /*                                                               */ 00059000
*   /* VERIFY THAT WE ARE ON PROPER CPU BY COMPARING CPU ADDRESS WITH*/ 00060000
*   /* PARM PASSED IN WORK AREA                                      */ 00061000
*   /*                                                               */ 00062000
*   /*****************************************************************/ 00063000
*                                                                  0042 00064000
*   IF CPUADD^=PSAVSTAP THEN        /* VERIFY PROPER CPU             */ 00065000
*                                                                  0042 00066000
         LH    @07,PSAVSTAP                                        0042 00067000
         LH    @05,CPUADD(,PARMPTR)                                0042 00068000
         N     @05,@CF01790                                        0042 00069000
         CR    @07,@05                                             0042 00070000
         BE    @RF00042                                            0042 00071000
*     /***************************************************************/ 00072000
*     /*                                                             */ 00073000
*     /* IF WRONG CPU ENTERS ROUTINE THEN PLACE HIM IN DISABLED WAIT */ 00074000
*     /* STATE                                                       */ 00075000
*     /*                                                             */ 00076000
*     /***************************************************************/ 00077000
*                                                                  0043 00078000
*     DO;                           /* NOT PROPER CPU.. STOP IT      */ 00079000
*       R6=PSAVSTAP;                /* LOAD CPU ADDRESS FOR STOP     */ 00080000
         LR    R6,@07                                              0044 00081000
*       GEN(SIGP 0,R6,5(0));        /* STOP THIS CPU.. SHOULD NOT BE */ 00082000
         SIGP 0,R6,5(0)                                                 00083000
*       LPSW(FLCPNPSW);             /* LOAD PROGRAM NEW PSW          */ 00084000
         LPSW  FLCPNPSW                                            0046 00085000
*     END;                                                         0047 00086000
*                                                                  0047 00087000
*   /*****************************************************************/ 00088000
*   /*                                                               */ 00089000
*   /* WE ARE OPERATING ON PROPER CPU. LOAD CONTROL REGISTERS TO     */ 00090000
*   /* PREPARE FOR DAT. SET FLAG FOR MAINLINE INDICATING WAKEUP IS IN*/ 00091000
*   /* CONTROL                                                       */ 00092000
*   /*                                                               */ 00093000
*   /*****************************************************************/ 00094000
*                                                                  0048 00095000
*   HARDWAIT='000A'X;               /* SET DISABLED WAIT             */ 00096000
@RF00042 MVC   HARDWAIT(2),@CB01750                                0048 00097000
*   INPROCS='1'B;                   /* SET FLAG FOR MAINLINE         */ 00098000
         OI    INPROCS(PARMPTR),B'10000000'                        0049 00099000
*   R6=ADDR(XREG0);                 /* LOAD ADDRESS OF CONTROL REG   */ 00100000
*                                                                  0050 00101000
         LA    R6,XREG0(,PARMPTR)                                  0050 00102000
*   /*****************************************************************/ 00103000
*   /*                                                               */ 00104000
*   /* LOAD CONTROL REGISTERS PASSED BY MSINLINE. CONTROL REG IS     */ 00105000
*   /* POINTING TO MASTER REGIONS SEGEMENT TABLES                    */ 00106000
*   /*                                                               */ 00107000
*   /*****************************************************************/ 00108000
*                                                                  0051 00109000
*   GEN(LCTL 0,1,0(R6));            /* LOAD CONTROL REGISTERS        */ 00110000
*                                                                  0051 00111000
         LCTL 0,1,0(R6)                                                 00112000
*   /*****************************************************************/ 00113000
*   /*                                                               */ 00114000
*   /* LOAD NEW PSA ADDRESS INTO PREFIX REGISTER ON THIS CPU         */ 00115000
*   /*                                                               */ 00116000
*   /*****************************************************************/ 00117000
*                                                                  0052 00118000
*   SPX(PSAADDR);                   /* LOAD PREFIX REGISTER          */ 00119000
*                                                                  0052 00120000
         SPX   PSAADDR(PARMPTR)                                    0052 00121000
*   /*****************************************************************/ 00122000
*   /*                                                               */ 00123000
*   /* SET UP FRR TO STOP CPU IF ANY ERROR OCCURES DURING CPU        */ 00124000
*   /* INITILIZATION. ROUTINE WILL RESET PREFIX REGISTER AND SHUT CPU*/ 00125000
*   /* DOWN                                                          */ 00126000
*   /*                                                               */ 00127000
*   /*****************************************************************/ 00128000
*                                                                  0053 00129000
*   R6=CVTVSTOP;                    /* ADDRESS OF SHUTDOWN ROUTINE   */ 00130000
         L     @05,CVTPTR                                          0053 00131000
         L     R6,CVTVSTOP(,@05)                                   0053 00132000
*   GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,FRRSELEN,FRRSFRRA,FRRSEMP,  0054 00133000
*       PSALSFCC)(SETFRR A,FRRAD=(R6),WRKREGS=(14,15));/* SET FRR ON    00134000
*                                      STACK                         */ 00135000
*                                                                  0054 00136000
         SETFRR A,FRRAD=(R6),WRKREGS=(14,15)                            00137000
*   /*****************************************************************/ 00138000
*   /*                                                               */ 00139000
*   /* SET DISPATCHER SUPER BIT TO INSURE FRR WILL GAIN CONTROL      */ 00140000
*   /*                                                               */ 00141000
*   /*****************************************************************/ 00142000
*                                                                  0055 00143000
*   PSADISP='1'B;                   /* SET DISPATCHER LOCK           */ 00144000
*                                                                  0055 00145000
         OI    PSADISP,B'00000100'                                 0055 00146000
*   /*****************************************************************/ 00147000
*   /*                                                               */ 00148000
*   /* LOAD VIRTUAL ADDRESS OF PARMS TO USE AFTER DAT HAS BEEN       */ 00149000
*   /* ENABLED                                                       */ 00150000
*   /*                                                               */ 00151000
*   /*****************************************************************/ 00152000
*                                                                  0056 00153000
*   R14=VIRTADDR;                   /* LOAD VIRT ADDRESS OF PARMS    */ 00154000
*                                                                  0056 00155000
         L     R14,VIRTADDR(,PARMPTR)                              0056 00156000
*   /*****************************************************************/ 00157000
*   /*                                                               */ 00158000
*   /* LOAD VIRTUAL ADDRESS OF MODULE USING POINT AND SAVE FOR USE   */ 00159000
*   /* AFTER DAT IS ENABELED                                         */ 00160000
*   /*                                                               */ 00161000
*   /*****************************************************************/ 00162000
*                                                                  0057 00163000
*   R6=VIRTBASE;                    /* LOAD VIRTUAL BASE             */ 00164000
*                                                                  0057 00165000
         L     R6,VIRTBASE                                         0057 00166000
*   /*****************************************************************/ 00167000
*   /*                                                               */ 00168000
*   /* LOAD NEW PSW POINTING TO NEXT INSTRUCTION. DAT IS ENABLED IN  */ 00169000
*   /* NEW PSW AND VIRTUAL ADDRESS OF NEXT INSTRUCTION IS USED. BASE */ 00170000
*   /* REGISTER MUST BE RELOADED WITH VIRTUAL ADDRESS OF USING POINT */ 00171000
*   /*                                                               */ 00172000
*   /*****************************************************************/ 00173000
*                                                                  0058 00174000
*DATON:                                                            0058 00175000
*   LPSW(PSWDATON);                 /* LOAD NEW PSW WITH DAT         */ 00176000
*                                                                  0058 00177000
DATON    LPSW  PSWDATON                                            0058 00178000
*   /*****************************************************************/ 00179000
*   /*                                                               */ 00180000
*   /* LOAD NEW BASE REG                                             */ 00181000
*   /*                                                               */ 00182000
*   /*****************************************************************/ 00183000
*                                                                  0059 00184000
*   BASEREG=R6;                     /* COPY VIRTUAL BASE REG INTO  0059 00185000
*                                      BASEREG                       */ 00186000
*                                                                  0059 00187000
         LR    BASEREG,R6                                          0059 00188000
*   /*****************************************************************/ 00189000
*   /*                                                               */ 00190000
*   /* DAT HAS BEEN ENABLED. VIRTUAL ADDRESS OF PARMS MUST NOW BE    */ 00191000
*   /* USED                                                          */ 00192000
*   /*                                                               */ 00193000
*   /*****************************************************************/ 00194000
*                                                                  0060 00195000
*   PARMPTR=R14;                    /* USE VIRT ADDRESS FOR WORK AREA*/ 00196000
*                                                                  0060 00197000
         LR    PARMPTR,R14                                         0060 00198000
*   /*****************************************************************/ 00199000
*   /*                                                               */ 00200000
*   /* SET UP REGISTERS FOR CONTROL REGISTER INITIALIZE ROUTINE LINK */ 00201000
*   /* TO CONTROL REGISTER INITIALIZE ROUTINE                        */ 00202000
*   /*                                                               */ 00203000
*   /*****************************************************************/ 00204000
*                                                                  0061 00205000
*   RFY                                                            0061 00206000
*    (R0,                                                          0061 00207000
*     R1,                                                          0061 00208000
*     RETCODE) RSTD;                /* RESTRICT REGISTERS            */ 00209000
*   R0=8;                           /* SET FUNCTION CODE INTO REG    */ 00210000
         LA    R0,8                                                0062 00211000
*   R1=CPUADD;                      /* LOAD CPU ADDRESS FROM PARMS   */ 00212000
*                                                                  0063 00213000
         LH    R1,CPUADD(,PARMPTR)                                 0063 00214000
         N     R1,@CF01790                                         0063 00215000
*   /*****************************************************************/ 00216000
*   /*                                                               */ 00217000
*   /* LOAD ADDRESS OF RMS ROUTINE AND BRANCH TO IT                  */ 00218000
*   /*                                                               */ 00219000
*   /*****************************************************************/ 00220000
*                                                                  0064 00221000
*   R15=RVTPBUCR;                   /* ADDRESS OF RMS ROUTINE        */ 00222000
         L     @07,CVTPTR                                          0064 00223000
         L     @07,RVTPTR(,@07)                                    0064 00224000
         L     R15,RVTPBUCR(,@07)                                  0064 00225000
*   GEN(BALR 14,15);                /* LINK TO ROUTINE               */ 00226000
*                                                                  0065 00227000
         BALR 14,15                                                     00228000
*   /*****************************************************************/ 00229000
*   /*                                                               */ 00230000
*   /* IF CONTROL REGISTERS COULD NOT BE INITIALIZED THEN BRANCH TO  */ 00231000
*   /* IEEVSTOP THAT WILL SHUT CPU DOWN                              */ 00232000
*   /*                                                               */ 00233000
*   /*****************************************************************/ 00234000
*                                                                  0066 00235000
*   IF RETCODE^=0 THEN              /* IF ERROR STOP MACHINE         */ 00236000
         LTR   RETCODE,RETCODE                                     0066 00237000
         BZ    @RF00066                                            0066 00238000
*     DO;                                                          0067 00239000
*       R15=CVTVSTOP;               /* LOAD STOP ADDRESS             */ 00240000
         L     @07,CVTPTR                                          0068 00241000
         L     R15,CVTVSTOP(,@07)                                  0068 00242000
*       GEN(BR  15);                /* GO SHUTDOWN CPU               */ 00243000
         BR  15                                                         00244000
*     END;                                                         0070 00245000
*   RFY                                                            0071 00246000
*    (R0,                                                          0071 00247000
*     R1,                                                          0071 00248000
*     RETCODE) UNRSTD;              /* RELEASE REGISTERS             */ 00249000
*                                                                  0071 00250000
@RF00066 DS    0H                                                  0072 00251000
*   /*****************************************************************/ 00252000
*   /*                                                               */ 00253000
*   /* INITIALIZE CONTROL REGISTER 8 FOR GTF                         */ 00254000
*   /*                                                               */ 00255000
*   /*****************************************************************/ 00256000
*                                                                  0072 00257000
*   R15=CVTGTFR8;                   /* ADDRESS OF GTF ROUTINE        */ 00258000
         L     @07,CVTPTR                                          0072 00259000
         L     R15,CVTGTFR8(,@07)                                  0072 00260000
*   GEN(BALR  14,15);               /* LINK TO GTF ROUTINE           */ 00261000
*                                                                  0073 00262000
         BALR  14,15                                                    00263000
*   /*****************************************************************/ 00264000
*   /*                                                               */ 00265000
*   /* LINK TO TIMER ROUTINE TO INITIALIZE TIMERS AND CONTROL REGS   */ 00266000
*   /*                                                               */ 00267000
*   /*****************************************************************/ 00268000
*                                                                  0074 00269000
*   R15=TPCVPTR;                    /* LOAD ADDRESS OF TIMER ROUTINE */ 00270000
