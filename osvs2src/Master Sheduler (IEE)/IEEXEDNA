         TITLE 'IEEXEDNA - DISPLAY CONSOLES COMMAND PROCESSOR          *00001000
                        '                                               00002000
IEEXEDNA CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(24)                                             0001 00007000
         DC    C'IEEXEDNA 78061  UZ17073 '                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  BALR  @09,0                                               0001 00010000
@PSTART  LA    @03,4095(,@09)                                      0001 00011000
         USING @PSTART,@09                                         0001 00012000
         USING @PSTART+4095,@03                                    0001 00013000
         LR    @12,@01                                             0001 00014000
         L     @00,@SIZDATD                                        0001 00015000
         GETMAIN  R,LV=(0)                                              00016000
         LR    @10,@01                                             0001 00017000
         USING @DATD,@10                                           0001 00018000
         LR    @01,@12                                             0001 00019000
         ST    @13,@SA00001+4                                      0001 00020000
         LR    @13,@10                                             0001 00021000
         MVC   @PC00001(4),0(@01)                                  0001 00022000
TOPSEG   DS    0H                                                  0148 00023000
*   CSCBPTR=R1;                     /* SAVE POINTER TO CSCB          */ 00024000
         LR    CSCBPTR,R1                                          0148 00025000
*   RETADDR=R14;                    /* SAVE RETURN ADDRESS           */ 00026000
         ST    R14,RETADDR                                         0149 00027000
*   UCMBASPT=CVTCUCB;               /* SET UCM BASE PTR      @ZA11392*/ 00028000
         L     @12,CVTPTR                                          0150 00029000
         L     UCMBASPT,CVTCUCB(,@12)                              0150 00030000
*   UCMMCSPT=UCMPRFXP;              /* SET UCM PREFIX REG            */ 00031000
         LR    @12,UCMBASPT                                        0151 00032000
         AL    @12,@CF02811                                        0151 00033000
         L     @12,UCMPRFXP(,@12)                                  0151 00034000
         ST    @12,UCMMCSPT                                        0151 00035000
*   MAXUCMES=(UCMVEL-UCMVEA)/UCMVEZ+1;/* UCMES IN SYS                */ 00036000
         L     @12,UCMVEL(,UCMBASPT)                               0152 00037000
         SL    @12,UCMVEA(,UCMBASPT)                               0152 00038000
         LR    @06,@12                                             0152 00039000
         SRDA  @06,32                                              0152 00040000
         D     @06,UCMVEZ(,UCMBASPT)                               0152 00041000
         AL    @07,@CF00068                                        0152 00042000
         ST    @07,MAXUCMES                                        0152 00043000
*   GEN(LOAD EP=IEECB860);          /* LOAD STAE CREATION ROUTINE    */ 00044000
         LOAD EP=IEECB860                                               00045000
*   CBADDR=R0;                      /* SAVE ADDRESS OF CB860         */ 00046000
         LR    CBADDR,R0                                           0154 00047000
*   WORD1=CSCBPTR;                  /* CSCB PTR INTO PARMLIST+0      */ 00048000
         ST    CSCBPTR,WORD1                                       0155 00049000
*   WORD2=ADDR(COMMNAME);           /* PTR TO D CONSOLES COMMAND     */ 00050000
         LA    @12,COMMNAME                                        0156 00051000
         ST    @12,WORD2                                           0156 00052000
*   WORD3=RETADDR;                  /* RETURN PT ADDR INTO PARMLST   */ 00053000
         L     @12,RETADDR                                         0157 00054000
         ST    @12,WORD3                                           0157 00055000
*   R15=CBADDR;                     /* GET CB860 EP ADDR             */ 00056000
         LR    R15,CBADDR                                          0158 00057000
*   R1=ADDR(WORD1);                 /* SET POINTER TO PARMLIST       */ 00058000
         LA    R1,WORD1                                            0159 00059000
*   GENERATE;                                                      0160 00060000
*                                      /* CODE TO GO TO CB860 YM02650*/ 00061000
         STM   14,12,12(13)        SAVE REGS BEFORE CB860   YM02650     00062000
         BALR 14,15                  GO ESTAB. STAE ENVIRON.  YM02650   00063000
         LM    0,12,20(13)         RESTORE REGS             YM02650     00064000
*   RCODE=R15;                      /* SAVE RETURN CODE              */ 00065000
         LR    RCODE,R15                                           0161 00066000
*   TARGID=CHCNID;                  /* SAVE TARGET. CONSOLE ID     0162 00067000
*                                                            @ZA15315*/ 00068000
         SLR   @12,@12                                             0162 00069000
         IC    @12,CHCNID(,CSCBPTR)                                0162 00070000
         STH   @12,TARGID                                          0162 00071000
*   R0SAVE=TARGID;                  /* SET R0SAVE FOR 1ST WTO      0163 00072000
*                                                            @ZA15315*/ 00073000
         ST    @12,R0SAVE                                          0163 00074000
*   TARGAREA=CHARID;                /* SAVE DISP. SCREEN AREA ID   0164 00075000
*                                                            @ZA15315*/ 00076000
         MVC   TARGAREA(1),CHARID(CSCBPTR)                         0164 00077000
*   CHFC='1'B;                      /* SIGNAL FREE THIS CSCB         */ 00078000
         OI    CHFC(CSCBPTR),B'00000010'                           0165 00079000
*   R15=CSCBPTR;                    /* GET CSCB ADDR. ISSUE THE      */ 00080000
         LR    R15,CSCBPTR                                         0166 00081000
*   GEN(MGCR (15),CHAIN);           /* MGCR MACRO TO FREE THE CORE   */ 00082000
         MGCR (15),CHAIN                                                00083000
*   RFY                                                            0168 00084000
*     R0 UNRSTD;                    /*                       @ZA11392*/ 00085000
*                                                                  0168 00086000
*   /*****************************************************************/ 00087000
*   /*                                                               */ 00088000
*   /* HAVE NOW FINISHED INITIAL SETUP. NOW CHECK FOR GOOD RET. CODE */ 00089000
*   /* FROM CB860. IF ZERO, CONTINUE. IF NOT, DELETE CB860, FREE OUR */ 00090000
*   /* CORE, AND RETURN, TO CALLER.                                  */ 00091000
*   /*                                                               */ 00092000
*   /*****************************************************************/ 00093000
*                                                                  0169 00094000
*   BITBYTE=0;                      /* INIT ALL BITS TO ZERO @ZA15750*/ 00095000
         MVI   BITBYTE,X'00'                                       0169 00096000
*   IF RCODE^=0                     /* IF BAD RET. CODE FROM CB860   */ 00097000
*     THEN                          /* THEN GOTO CLEANUP RTN. THE    */ 00098000
         LTR   RCODE,RCODE                                         0170 00099000
         BZ    @RF00170                                            0170 00100000
*     CALL CLEANUP;                 /* CLEANUP RTN. WILL NOT RETURN  */ 00101000
         BAL   @14,CLEANUP                                         0171 00102000
*   RFY                                                            0172 00103000
*    (R4,                                                          0172 00104000
*     R5) RSTD;                     /*                       @ZA11392*/ 00105000
@RF00170 DS    0H                                                  0173 00106000
*   R5=MAXUCMES*(LENGTH(CONSENT))+  /* # UCMES * DATA SAVE AREA LEN     00107000
*                                      FOR EACH CONS         @ZA11392*/ 00108000
*       LENGTH(WPLFLGS)+            /* PLUS LEN OF ROUT + DESC CODES    00109000
*                                      FOR CNTL LINE         @ZA11392*/ 00110000
*       LENGTH(WPLLS01)+            /* PLUS LEN OF LINE TYPE,AREA ID    00111000
*                                      AND # LINES FOR CNTL LINE   0173 00112000
*                                                            @ZA11392*/ 00113000
*       10*LENGTH(DLINE);           /* PLUS LEN OF TEN DATA LINES  0173 00114000
*                                                            @ZA11392*/ 00115000
         L     R5,MAXUCMES                                         0173 00116000
         MH    R5,@CH00077                                         0173 00117000
         AL    R5,@CF02802                                         0173 00118000
*   WKLEN=R5;                       /* SAVE GOTTEN CORE LENGTH       */ 00119000
         STH   R5,WKLEN                                            0174 00120000
*   GEN(GETMAIN R,LV=(5),SP=229);   /* GET AREA FROM SUBPOOL 229     */ 00121000
         GETMAIN R,LV=(5),SP=229                                        00122000
*   CONSPTR=R1;                     /* SET BASE TO DATA-SAVE-ENT     */ 00123000
         LR    CONSPTR,R1                                          0176 00124000
*   R4=R1;                          /* SET POINTER TO THE CORE       */ 00125000
         LR    R4,R1                                               0177 00126000
*   R14=R1;                         /* SET POINTER TO THE CORE       */ 00127000
         LR    R14,R1                                              0178 00128000
*   R15=0;                          /* SIGNAL ZERO OPERATION         */ 00129000
         SLR   R15,R15                                             0179 00130000
*   GEN(MVCL R4,R14);               /* ZERO THE CORE GOTTEN          */ 00131000
         MVCL R4,R14                                                    00132000
*   RFY                                                            0181 00133000
*    (R1,                                                          0181 00134000
*     R4,                                                          0181 00135000
*     R5,                                                          0181 00136000
*     R14,                                                         0181 00137000
*     R15) UNRSTD;                  /*                       @ZA11392*/ 00138000
*                                                                  0181 00139000
*   /*****************************************************************/ 00140000
*   /*                                                               */ 00141000
*   /* NOW SET WPLBASE TO POINT INTO THE AREA GOTTEN, TO THE SECTION */ 00142000
*   /* TO BE USED FOR MULTI-LINE WTO PARAMETER LIST                  */ 00143000
*   /*                                                               */ 00144000
*   /*****************************************************************/ 00145000
*                                                                  0182 00146000
*   GCORE='1'B;                     /* SIGNAL CORE GOTTEN            */ 00147000
         OI    GCORE,B'00000100'                                   0182 00148000
*   WPLBASE=CONSPTR+MAXUCMES*LENGTH(CONSENT);/* SET BASE     @ZA15315*/ 00149000
         L     @12,MAXUCMES                                        0183 00150000
         MH    @12,@CH00077                                        0183 00151000
         ALR   @12,CONSPTR                                         0183 00152000
         ST    @12,WPLBASE                                         0183 00153000
*   WPLPTR=WPLBASE;                 /* SET PTR TO 1ST WPL    @ZA15315*/ 00154000
*                                                                  0184 00155000
         LR    WPLPTR,@12                                          0184 00156000
*   /*****************************************************************/ 00157000
*   /*                                                               */ 00158000
*   /* NOW CALL TITLEBLD AND LABELBLD TO BUILD THE FIRST THREE LINES */ 00159000
*   /* OF THE MULTI-LINE WTO IN THE PARAMETER LIST                   */ 00160000
*   /*                                                               */ 00161000
*   /*****************************************************************/ 00162000
*                                                                  0185 00163000
*   CALL TITLEBLD;                  /* BUILD CONTROL LINE FOR WTO    */ 00164000
         BAL   @14,TITLEBLD                                        0185 00165000
*   CALL SETLOCK;                   /* OBTAIN LOCAL/CMS LOCKS      0186 00166000
*                                                            @ZA11392*/ 00167000
         BAL   @14,SETLOCK                                         0186 00168000
*   CALL LABELBLD;                  /* BUILD LABEL LINE FOR WTO      */ 00169000
*                                                                  0187 00170000
         BAL   @14,LABELBLD                                        0187 00171000
*   /*****************************************************************/ 00172000
*   /*                                                               */ 00173000
*   /* CALL GETDATA TO GATHER DATA FROM THE UCMES,UCBS       @ZA11392*/ 00174000
*   /* AND DCMS.                                             @ZA11392*/ 00175000
*   /* THEN CALL FREELOCK TO RELEASE THE LOCKS HELD.                 */ 00176000
*   /*                                                               */ 00177000
*   /*****************************************************************/ 00178000
*                                                                  0188 00179000
*   CALL GETDATA;                   /* GATHER DATA FOR MESSAGES      */ 00180000
         BAL   @14,GETDATA                                         0188 00181000
*   CALL FREELOCK;                  /* FREE LOCAL/CMS LOCKS          */ 00182000
         BAL   @14,FREELOCK                                        0189 00183000
*   INDEX=0;                        /* CLEAR COUNT OF # OF CONSOLES     00184000
*                                      PROCESSED. NO ENTRY INTO ARRAY   00185000
*                                      EXISTS. THIS CODE WAS ADDED 0190 00186000
*                                      FOR ZA07158.          @ZA15315*/ 00187000
         SLR   INDEX,INDEX                                         0190 00188000
*   CALL MSGSET;                    /* INIT NEXT WPL         @ZA15315*/ 00189000
*                                                                  0191 00190000
         BAL   @14,MSGSET                                          0191 00191000
*   /*****************************************************************/ 00192000
*   /*                                                               */ 00193000
*   /* CALL HARDCOPY TO SEE IF A HARDCOPY MESSAGE SHOULD BE WRITTEN. */ 00194000
*   /* IF SO, THEN FINISH THE FIELDS IN THE HARDCOPY MESSAGE. IF NOT */ 00195000
*   /* THEN CONTINUE TO NORMAL PROCESSING OF EACH SEPARATE UCM ENTRY,*/ 00196000
*   /* TO BUILD ONE DATA LINE PER UCME.                              */ 00197000
*   /*                                                               */ 00198000
*   /*****************************************************************/ 00199000
*                                                                  0192 00200000
*   CALL HARDCOPY;                  /* PREPARE HC. STATUS MSG.       */ 00201000
*                                                                  0192 00202000
         BAL   @14,HARDCOPY                                        0192 00203000
*   /*****************************************************************/ 00204000
*   /*                                                               */ 00205000
*   /* CHECK TO SEE IF HARDCOPY MESSAGE IS BEING BUILT               */ 00206000
*   /*                                                               */ 00207000
*   /*****************************************************************/ 00208000
*                                                                  0193 00209000
*   IF HDCOPY='1'B                  /* IF THIS IS THE HARDCOPY MSG   */ 00210000
*     THEN                          /* BEING WRITTEN, THEN IF HC.    */ 00211000
         TM    HDCOPY,B'01000000'                                  0193 00212000
         BNO   @RF00193                                            0193 00213000
*     DO;                           /*                       @ZA12702*/ 00214000
*                                                                  0194 00215000
*       /*************************************************************/ 00216000
*       /*                                                           */ 00217000
*       /* CHECK FOR HARDCOPY TO SYSLOG IF TO SYSLOG, THEN NO CONSOLE*/ 00218000
*       /* ID WILL BE PUT IN THE MESSAGE. ALSO NO AREA ID WILL BE PUT*/ 00219000
*       /* INTO THE MESSAGE.                                         */ 00220000
*       /*                                                           */ 00221000
*       /*************************************************************/ 00222000
*                                                                  0195 00223000
*       IF HCUCM=0                  /* IS TO SYSLOG, GO FINISH MSG   */ 00224000
*         THEN                      /* IF SYSLOG,BYPASS CONSOLE ID   */ 00225000
         LTR   HCUCM,HCUCM                                         0195 00226000
         BNZ   @RF00195                                            0195 00227000
*         CALL MSGPROC2;            /* AND AREA ID PROCESSING. IF    */ 00228000
         BAL   @14,MSGPROC2                                        0196 00229000
*       ELSE                        /* TO A CONSOLE, INCLUDE ID &    */ 00230000
*         CALL MSGPROC;             /* AREA ID SECTIONS OF MSG       */ 00231000
         B     @RC00195                                            0197 00232000
@RF00195 BAL   @14,MSGPROC                                         0197 00233000
*       HDCOPY='0'B;                /* HC LINE HAS BEEN BUILT      0198 00234000
*                                                            @ZA12702*/ 00235000
@RC00195 NI    HDCOPY,B'10111111'                                  0198 00236000
*     END;                          /*                       @ZA12702*/ 00237000
*                                                                  0199 00238000
*   /*****************************************************************/ 00239000
*   /*                                                               */ 00240000
*   /* THE FOLLOWING LOOP IS EXECUTED ONCE FOR EACH UCM ENTRY IN THE */ 00241000
*   /* SYSTEM. THE UCME IS SKIPPED IF IT IS PART OF A COMPOSITE      */ 00242000
*   /* CONSOLE AND HAS ONLY OUTPUT CAPABILITY. IN THAT CASE ITS      */ 00243000
*   /* STATUS WILL BE REFLECTED IN THE MESSAGE BUILT TO REPRESENT THE*/ 00244000
*   /* STATUS OF THE INPUT HALF OF THE COMPOSITE CONSOLE. IF THE     */ 00245000
*   /* OUTPUT HALF'S UCME IS                                 @ZA15315*/ 00246000
*   /* THE LAST ONE THEN THE PREVIOUS LINE WILL HAVE         @ZA15315*/ 00247000
*   /* ALREADY BEEN MARKED AS THE END LINE.                  @ZA15315*/ 00248000
*   /*                                                               */ 00249000
*   /*****************************************************************/ 00250000
*                                                                  0200 00251000
*   DO INDEX=1 TO MAXUCMES;         /* LOOP OVER ALL UCME DATA-      */ 00252000
*                                                                  0200 00253000
@RF00193 LA    INDEX,1                                             0200 00254000
         B     @DE00200                                            0200 00255000
@DL00200 DS    0H                                                  0201 00256000
*     /***************************************************************/ 00257000
*     /*                                                             */ 00258000
*     /* CHECK FOR PART OF COMPOSITE CONSOLE AND OUTPUT HALF         */ 00259000
*     /*                                                             */ 00260000
*     /***************************************************************/ 00261000
*                                                                  0201 00262000
*     IF COMPC(INDEX)=0             /* SAVE-AREAS. IF PART OF COMP   */ 00263000
*         |                         /* OSITE CONSOLE AND OUTPUT      */ 00264000
*         MIF(INDEX)='1'B           /* ONLY DEVICE, IGNORE IT, AND   */ 00265000
*       THEN                        /* GO TO NEXT ENTRY. OTHERWISE   */ 00266000
         LR    @12,INDEX                                           0201 00267000
         MH    @12,@CH00077                                        0201 00268000
         LR    @05,CONSPTR                                         0201 00269000
         AL    @05,@CF02815                                        0201 00270000
         LH    @05,COMPC-14(@12,@05)                               0201 00271000
         LTR   @05,@05                                             0201 00272000
         BZ    @RT00201                                            0201 00273000
         ALR   @12,CONSPTR                                         0201 00274000
         AL    @12,@CF02816                                        0201 00275000
         TM    MIF-5(@12),B'01000000'                              0201 00276000
         BNO   @RF00201                                            0201 00277000
@RT00201 DS    0H                                                  0202 00278000
*       DO;                         /* FILL IN UCB NAMES IN MSG      */ 00279000
*         CALL UNITNAME;            /* FILL IN CONSOLE CONDI-        */ 00280000
         BAL   @14,UNITNAME                                        0203 00281000
*         CALL MSGCOND;             /* TION AND AUTHORITY            */ 00282000
         BAL   @14,MSGCOND                                         0204 00283000
*         CALL AUTHCHK;             /* COLUMNS IN MESSAGE            */ 00284000
         BAL   @14,AUTHCHK                                         0205 00285000
*         INDEXSVE=INDEX;           /* SAVE INDEX OF CONS    @ZA11392*/ 00286000
         LR    INDEXSVE,INDEX                                      0206 00287000
*         IF COMPC(INDEX)^=0 THEN   /* IS THIS A COMPOSITE CONSOLE?     00288000
*                                                            @ZA11392*/ 00289000
         LR    @12,INDEX                                           0207 00290000
         MH    @12,@CH00077                                        0207 00291000
         AL    @12,@CF02815                                        0207 00292000
         LH    @12,COMPC-14(@12,CONSPTR)                           0207 00293000
         LTR   @12,@12                                             0207 00294000
         BZ    @RF00207                                            0207 00295000
*           INDEX=COMPC(INDEX);     /* YES, POINT TO OUTPUT HALF   0208 00296000
*                                                            @ZA11392*/ 00297000
         LR    INDEX,@12                                           0208 00298000
*         CALL TOEBCDIC(WQECNT(INDEX),NWQES);/* CONVERT NUMBER OF  0209 00299000
*                                      WQES TO EBCDIC AND PUT IN MSG    00300000
*                                                            @ZA11392*/ 00301000
@RF00207 LR    @12,INDEX                                           0209 00302000
         MH    @12,@CH00077                                        0209 00303000
         AL    @12,@CF02817                                        0209 00304000
         LA    @12,WQECNT-2(@12,CONSPTR)                           0209 00305000
         ST    @12,@AL00001                                        0209 00306000
         LA    @12,NWQES(,WPLPTR)                                  0209 00307000
         ST    @12,@AL00001+4                                      0209 00308000
         LA    @01,@AL00001                                        0209 00309000
         BAL   @14,TOEBCDIC                                        0209 00310000
*         INDEX=INDEXSVE;           /* RESTORE INDEX TO INPUT HALF 0210 00311000
*                                                            @ZA11392*/ 00312000
         LR    INDEX,INDEXSVE                                      0210 00313000
*         CALL MSGPROC;             /* GO FINISH MESSAGE INFO        */ 00314000
         BAL   @14,MSGPROC                                         0211 00315000
*       END;                        /* END GOOD UCME PROCESSING      */ 00316000
*   END;                            /* END UCME LOOP                 */ 00317000
@RF00201 AL    INDEX,@CF00068                                      0213 00318000
@DE00200 C     INDEX,MAXUCMES                                      0213 00319000
         BNH   @DL00200                                            0213 00320000
         EJECT                                                          00321000
*                                                                  0214 00322000
*/********************************************************************/ 00323000
*/*                                                                  */ 00324000
*/* ENTRY - CLEANUP                                          @ZA15750*/ 00325000
*/*                                                                  */ 00326000
*/* INPUTS - WRKLEN, LENGTH OF GETMAIN AREA TO FREE          @ZA15750*/ 00327000
*/* OUTPUTS - NONE.  RETURN IS TO THE SYSTEM VIA BR 14.              */ 00328000
*/*                                                                  */ 00329000
*/********************************************************************/ 00330000
*                                                                  0214 00331000
*CLEANUP:                                                          0214 00332000
*   ;                               /* CLEANUP AND EXIT RTN  @ZA15750*/ 00333000
CLEANUP  DS    0H                                                  0215 00334000
*   RFY                                                            0215 00335000
*    (R11,                                                         0215 00336000
*     R12) RSTD;                    /*                       @ZA15750*/ 00337000
*   GEN(DELETE EP=IEECB860);        /* DELETE STAE ROUTINE           */ 00338000
         DELETE EP=IEECB860                                             00339000
*   R11=WKLEN;                      /* GET LENGTH TO FREE            */ 00340000
         LH    R11,WKLEN                                           0217 00341000
*   R12=CONSPTR;                    /* GET START ADDR TO FREE      0218 00342000
*                                                            @ZA15750*/ 00343000
         LR    R12,CONSPTR                                         0218 00344000
*   IF GCORE='1'B                   /* IF CORE WAS GOTTEN            */ 00345000
*     THEN                          /* THEN WE MUST FREE IT          */ 00346000
         TM    GCORE,B'00000100'                                   0219 00347000
         BNO   @RF00219                                            0219 00348000
*     GEN(FREEMAIN R,LV=(11),A=(12),SP=229);/* FREE WORKAREA         */ 00349000
         FREEMAIN R,LV=(11),A=(12),SP=229                               00350000
*   RETURN TO RETADDR;              /* RETURN TO SYSTEM      @ZA15750*/ 00351000
@RF00219 L     @02,RETADDR                                         0221 00352000
         L     @13,4(,@13)                                         0221 00353000
         L     @00,@SIZDATD                                        0221 00354000
         LR    @01,@10                                             0221 00355000
         FREEMAIN R,LV=(0),A=(1)                                        00356000
         LR    @14,@02                                             0221 00357000
         BR    @14                                                 0221 00358000
*   RFY                                                            0222 00359000
*    (R11,                                                         0222 00360000
*     R12) UNRSTD;                  /*                       @ZA15750*/ 00361000
         EJECT                                                          00362000
*                                                                  0223 00363000
*/********************************************************************/ 00364000
*/*                                                                  */ 00365000
*/* PROCEDURE - MSGPROC                                              */ 00366000
*/*                                                                  */ 00367000
*/* INPUTS - CONSOLE DATA-SAVE-ENTRIES.                              */ 00368000
*/*                                                                  */ 00369000
*/* OUTPUTS - BUILDS CONSOLE ID AND GRAPHICS AREA ID COLUMNS OF LINE */ 00370000
*/*                                                                  */ 00371000
*/********************************************************************/ 00372000
*                                                                  0223 00373000
*MSGPROC:                                                          0223 00374000
*   PROC;                           /* CONSOLE ID/GRAPHICS PROCESS   */ 00375000
@EL00001 L     @13,4(,@13)                                         0223 00376000
@EF00001 L     @00,@SIZDATD                                        0223 00377000
         LR    @01,@10                                             0223 00378000
         FREEMAIN R,LV=(0),A=(1)                                        00379000
@ER00001 BR    @14                                                 0223 00380000
MSGPROC  STM   @14,@03,@SA00002                                    0223 00381000
         STM   @05,@10,@SA00002+24                                 0223 00382000
         ST    @12,@SA00002+48                                     0223 00383000
*   CALL CONSID;                    /* FILL CONSOLE ID INTO MSG      */ 00384000
*                                                                  0224 00385000
         BAL   @14,CONSID                                          0224 00386000
*   /*****************************************************************/ 00387000
*   /*                                                               */ 00388000
*   /* CHECK TO SEE IF GRAPHICS IS IN THE SYSTEM                     */ 00389000
*   /*                                                               */ 00390000
*   /*****************************************************************/ 00391000
*                                                                  0225 00392000
*   IF UCMSYSF='1'B                 /* IF THIS SYSTEM HAS GRAPHICS   */ 00393000
*     THEN                          /* THEN FILL IN AREA ID          */ 00394000
         L     @12,UCMMCSPT                                        0225 00395000
         TM    UCMSYSF(@12),B'00000100'                            0225 00396000
         BNO   @RF00225                                            0225 00397000
*     CALL GRAPHICS;                /* SET AREA IDS                  */ 00398000
         BAL   @14,GRAPHICS                                        0226 00399000
*   CALL MSGPROC2;                  /* GO FINISH MSG                 */ 00400000
@RF00225 BAL   @14,MSGPROC2                                        0227 00401000
*   RETURN;                         /* RETURN TO CALLER              */ 00402000
@EL00002 DS    0H                                                  0228 00403000
@EF00002 DS    0H                                                  0228 00404000
@ER00002 LM    @14,@03,@SA00002                                    0228 00405000
         LM    @05,@10,@SA00002+24                                 0228 00406000
         L     @12,@SA00002+48                                     0228 00407000
         BR    @14                                                 0228 00408000
*   END MSGPROC;                    /* END CONSID/GRAPHICS PROC.     */ 00409000
         EJECT                                                          00410000
*                                                                  0230 00411000
*/********************************************************************/ 00412000
*/*                                                                  */ 00413000
*/* PROCEDURE - MSGPROC2                                             */ 00414000
*/*                                                                  */ 00415000
*/* INPUTS - CONSOLE DATA-SAVE-ENTRIES.                              */ 00416000
*/*                                                                  */ 00417000
*/* OUTPUTS - BUILDS ROUTE CODES COLUMN OF LINE & SETS WPL FOR THE   */ 00418000
*/*           NEXT LINE.                                             */ 00419000
*/*                                                                  */ 00420000
*/********************************************************************/ 00421000
*                                                                  0230 00422000
*MSGPROC2:                                                         0230 00423000
*   PROC;                           /* RCODES PROCESSING             */ 00424000
MSGPROC2 STM   @14,@03,@SA00003                                    0230 00425000
         STM   @05,@10,@SA00003+24                                 0230 00426000
         ST    @12,@SA00003+48                                     0230 00427000
*   CALL RCODES;                    /* PROCESS ROUTE CODES           */ 00428000
         BAL   @14,RCODES                                          0231 00429000
*   CALL MSGSET;                    /* SET WPL FOR NEXT LINE         */ 00430000
         BAL   @14,MSGSET                                          0232 00431000
*   RETURN;                         /* RETURN TO CALLER              */ 00432000
@EL00003 DS    0H                                                  0233 00433000
@EF00003 DS    0H                                                  0233 00434000
