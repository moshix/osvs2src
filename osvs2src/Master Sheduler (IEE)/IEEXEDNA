         ST    @12,@AL00001+4                                      0272 00588000
         LA    @01,@AL00001                                        0272 00589000
         BAL   @14,TOEBCDIC                                        0272 00590000
*   IF UCMWQLM1^=0 THEN             /* IF COUNT IS NOT ZERO THEN   0273 00591000
*                                      CONVERT TO EBCDIC     @ZA11392*/ 00592000
@RF00271 LH    @12,UCMWQLM1(,UCMBASPT)                             0273 00593000
         LTR   @12,@12                                             0273 00594000
         BZ    @RF00273                                            0273 00595000
*     CALL TOEBCDIC(UCMWQLM1,WPLMLTXT(41:44));/* CONVERT WQE LIMIT TO   00596000
*                                      EBCDIC                @ZA11392*/ 00597000
         LA    @12,UCMWQLM1(,UCMBASPT)                             0274 00598000
         ST    @12,@AL00001                                        0274 00599000
         LA    @12,WPLMLTXT+40(,WPLPTR)                            0274 00600000
         ST    @12,@AL00001+4                                      0274 00601000
         LA    @01,@AL00001                                        0274 00602000
         BAL   @14,TOEBCDIC                                        0274 00603000
*   WPLMLLTB='1'B;                  /* SET LINE TYPE = LABEL LINE  0275 00604000
*                                                            @ZA11392*/ 00605000
@RF00273 OI    WPLMLLTB(WPLPTR),B'01000000'                        0275 00606000
*   WPLMLLEN=LENGTH(LAB1TEXT)+4;    /* LENGTH IN WPL = TEXT LEN +  0276 00607000
*                                      HEADER LENGTH         @ZA11392*/ 00608000
         LA    @12,48                                              0276 00609000
         STC   @12,WPLMLLEN(,WPLPTR)                               0276 00610000
*   WPLPTR=WPLPTR+WPLMLLEN;         /* NEXT WPL SECTION      @ZA11392*/ 00611000
*                                                                  0277 00612000
         ALR   WPLPTR,@12                                          0277 00613000
*   /*****************************************************************/ 00614000
*   /*                                                               */ 00615000
*   /* BUILD SECOND LABEL LINE                               @ZA11392*/ 00616000
*   /* CHECK TO SEE IF GRAPHICS IS IN THE SYSTEM                     */ 00617000
*   /*                                                               */ 00618000
*   /*****************************************************************/ 00619000
*                                                                  0278 00620000
*   IF UCMSYSF='1'B                 /* IF GRAPHICS IN SYSTEM,        */ 00621000
*     THEN                          /* MOVE IN TEXT WITH AREA ID     */ 00622000
         L     @12,UCMMCSPT                                        0278 00623000
         TM    UCMSYSF(@12),B'00000100'                            0278 00624000
         BNO   @RF00278                                            0278 00625000
*     DO;                           /* COLUMN HEADER.                */ 00626000
*       WPLMLTXT(1:LENGTH(LABLTEXT))=LABLTEXT;/* MOVE TEXT           */ 00627000
         MVC   WPLMLTXT(51,WPLPTR),LABLTEXT                        0280 00628000
*       WPLMLLEN=LENGTH(LABLTEXT)+4;/* SET LENGTH IN                 */ 00629000
         MVI   WPLMLLEN(WPLPTR),X'37'                              0281 00630000
*     END;                          /* WPL. TEXT LEN + 4 (HEADER)    */ 00631000
*   ELSE                            /* IF NO GRAPHICS IN SYSTEM      */ 00632000
*     DO;                           /* THEN MUST USE TEXT WITHOUT  0283 00633000
*                                      AREA ID COLUMN HEADER. SET    */ 00634000
         B     @RC00278                                            0283 00635000
@RF00278 DS    0H                                                  0284 00636000
*       WPLMLTXT(1:LENGTH(LABLNOGP))=LABLNOGP;/*                     */ 00637000
         MVC   WPLMLTXT(51,WPLPTR),LABLNOGP                        0284 00638000
