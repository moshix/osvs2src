*       RFY                                                        0584 02313000
*        (R0,                                                      0584 02314000
*         R1,                                                      0584 02315000
*         R15) RSTD;                /*                       @ZA15315*/ 02316000
*       R0=R0SAVE;                  /* PICK UP CONS ID       @ZA15315*/ 02317000
         L     R0,R0SAVE                                           0585 02318000
*       R1=WPLBASE;                 /* POINT TO WPL          @ZA15315*/ 02319000
         L     R1,WPLBASE                                          0586 02320000
*       GEN REFS(R0) SETS(R1,R15)(WTO   MF=(E,(1)));/* ISSUE WTO   0587 02321000
*                                                            @ZA15315*/ 02322000
         WTO   MF=(E,(1))                                               02323000
*       IF R15^=0 THEN              /* GOOD RETURN CODE?     @ZA15315*/ 02324000
         LTR   R15,R15                                             0588 02325000
         BZ    @RF00588                                            0588 02326000
*         CALL CLEANUP;             /* NO. CLEANUP RETURNS CONTROL TO   02327000
*                                      THE SYSTEM            @ZA15315*/ 02328000
         BAL   @14,CLEANUP                                         0589 02329000
*       SLL(R1,8);                  /* SHIFT MSG ID RETURNED BY WTO     02330000
*                                                            @ZA15315*/ 02331000
@RF00588 SLL   R1,8                                                0590 02332000
*       R0SAVE=R1|TARGID;           /* SAVE MSG ID AND CONS ID     0591 02333000
*                                                            @ZA15315*/ 02334000
         LH    @14,TARGID                                          0591 02335000
         OR    @14,R1                                              0591 02336000
         ST    @14,R0SAVE                                          0591 02337000
*       RFY                                                        0592 02338000
*        (R0,                                                      0592 02339000
*         R1,                                                      0592 02340000
*         R15) UNRSTD;              /*                       @ZA15315*/ 02341000
*       IF DONE='0'B THEN           /* LAST LINE ISSUED?     @ZA15315*/ 02342000
         TM    DONE,B'00100000'                                    0593 02343000
         BNZ   @RF00593                                            0593 02344000
*         DO;                       /* NO. INIT NEXT LINE    @ZA15315*/ 02345000
*           ONELINE='1'B;           /* MARK THAT NEXT LINE IS THE 1ST   02346000
*                                      LINE OF A CONNECTING MLWTO  0595 02347000
*                                                            @ZA15315*/ 02348000
         OI    ONELINE,B'00001000'                                 0595 02349000
*           LINECT=0;               /* CLEAR NUMBER OF LINES @ZA15315*/ 02350000
         SLR   @12,@12                                             0596 02351000
