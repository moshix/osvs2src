*/*  PROCEDURE NAME: IEEXEDNA.SCANCQES                       @ZA11392*/ 02521000
*/*                                                          @ZA11392*/ 02522000
*/*  FUNCTION: SCANS A CONSOLES CQE CHAIN AND COUNTS THE     @ZA11392*/ 02523000
*/*            NUMBER OF WQE'S QUEUE TO THE CONSOLE          @ZA11392*/ 02524000
*/*                                                          @ZA11392*/ 02525000
*/*  INPUTS:   UCMOUTQ - ADDRESS OF CQE QUEUE                @ZA11392*/ 02526000
*/*            I - INDEX INTO CONSOLE-DATA-SAVE-ENTRY ARRAY  @ZA11392*/ 02527000
*/*                                                          @ZA11392*/ 02528000
*/*  OUTPUTS:  WQECNT(I) - NUMBER OF WQE'S QUEUED TO THIS    @ZA11392*/ 02529000
*/*                        CONSOLE.                          @ZA11392*/ 02530000
*/*                                                          @ZA11392*/ 02531000
*/*  CALLS:    SCANMINR                                      @ZA11392*/ 02532000
*/*                                                          @ZA11392*/ 02533000
*/***********************************************************@ZA11392*/ 02534000
*                                                                  0634 02535000
*SCANCQES:                                                         0634 02536000
*   PROC;                           /*                       @ZA11392*/ 02537000
SCANCQES STM   @14,@12,@SA00018                                    0634 02538000
*   WQECNT(I)=0;                    /* INIT NUMBER OF WQE'S  @ZA11392*/ 02539000
         LR    @12,I                                               0635 02540000
         MH    @12,@CH00077                                        0635 02541000
         SLR   @11,@11                                             0635 02542000
         AL    @12,@CF02817                                        0635 02543000
         STH   @11,WQECNT-2(@12,CONSPTR)                           0635 02544000
*   CQEPTR=UCMOUTQ;                 /* GET ADDR OF 1ST CQE   @ZA11392*/ 02545000
         L     CQEPTR,UCMOUTQ(,UCMDVEPT)                           0636 02546000
*   DO WHILE CQEPTR^=0;             /* DO UNTIL END OF CQE CHAIN   0637 02547000
*                                                            @ZA11392*/ 02548000
         B     @DE00637                                            0637 02549000
@DL00637 DS    0H                                                  0638 02550000
*     IF CQEEOB='11'B THEN          /* IS THIS END OF CQE BLOCK?   0638 02551000
*                                                            @ZA11392*/ 02552000
         TM    CQEEOB(CQEPTR),B'11000000'                          0638 02553000
         BNO   @RF00638                                            0638 02554000
*       CQEPTR=CQEWQEA;             /* YES, POINT TO NEXT BLOCK    0639 02555000
*                                                            @ZA11392*/ 02556000
         L     @12,CQEWQEA-1(,CQEPTR)                              0639 02557000
         LA    @12,0(,@12)                                         0639 02558000
         LR    CQEPTR,@12                                          0639 02559000
*     IF CQEENTR='1'B THEN          /* DOES CQE CONTAIN VALID WQE  0640 02560000
*                                                            @ZA11392*/ 02561000
@RF00638 TM    CQEENTR(CQEPTR),B'00000001'                         0640 02562000
         BNO   @RF00640                                            0640 02563000
*       DO;                         /* YES                   @ZA11392*/ 02564000
*         WQECNT(I)=WQECNT(I)+1;    /* INCREASE COUNT OF WQE'S     0642 02565000
*                                                            @ZA11392*/ 02566000
         LR    @12,I                                               0642 02567000
         MH    @12,@CH00077                                        0642 02568000
         LA    @11,1                                               0642 02569000
         LR    @07,CONSPTR                                         0642 02570000
         AL    @07,@CF02817                                        0642 02571000
         AH    @11,WQECNT-2(@12,@07)                               0642 02572000
         LR    @07,CONSPTR                                         0642 02573000
         AL    @07,@CF02817                                        0642 02574000
         STH   @11,WQECNT-2(@12,@07)                               0642 02575000
*         WQEPTR=CQEWQEA;           /* ADDR OF WQE           @ZA11392*/ 02576000
         L     @11,CQEWQEA-1(,CQEPTR)                              0643 02577000
         LA    @11,0(,@11)                                         0643 02578000
         ST    @11,WQEPTR                                          0643 02579000
*         IF CQEMAJOR='1'B&         /* WQE ALSO A MAJOR FOR MLWTO? 0644 02580000
*                                                            @ZA11392*/ 02581000
*             WMJMMIN^=0            /* AND AT LEAST ONE MINOR EXISTS    02582000
*                                                            @ZA11392*/ 02583000
*           THEN                    /*                       @ZA11392*/ 02584000
         TM    CQEMAJOR(CQEPTR),B'00000100'                        0644 02585000
         BNO   @RF00644                                            0644 02586000
         L     @11,WMJPTR                                          0644 02587000
         L     @11,WMJMMIN(,@11)                                   0644 02588000
         LTR   @11,@11                                             0644 02589000
         BZ    @RF00644                                            0644 02590000
*           CALL SCANMINR(WQECNT(I));/* YES COUNT NUMBER OF WQE'S  0645 02591000
*                                      THAT THE MLWTO IS USING     0645 02592000
*                                                            @ZA11392*/ 02593000
         AL    @12,@CF02817                                        0645 02594000
         LA    @12,WQECNT-2(@12,CONSPTR)                           0645 02595000
         ST    @12,@AL00001                                        0645 02596000
         LA    @01,@AL00001                                        0645 02597000
         BAL   @14,SCANMINR                                        0645 02598000
*       END;                        /*                       @ZA11392*/ 02599000
@RF00644 DS    0H                                                  0647 02600000
*     IF CQEEOB='10'B THEN          /* WAS THIS CQE THE LAST @ZA11392*/ 02601000
@RF00640 TM    CQEEOB(CQEPTR),B'10000000'                          0647 02602000
         BNO   @RF00647                                            0647 02603000
         TM    CQEEOB(CQEPTR),B'01000000'                          0647 02604000
         BNZ   @RF00647                                            0647 02605000
*       CQEPTR=0;                   /* YES, GET OUT OF LOOP  @ZA11392*/ 02606000
         SLR   CQEPTR,CQEPTR                                       0648 02607000
*     ELSE                          /* NO, MORE CQES         @ZA11392*/ 02608000
*       CQEPTR=CQEPTR+4;            /* POINT TO NEXT CQE     @ZA11392*/ 02609000
         B     @RC00647                                            0649 02610000
@RF00647 AL    CQEPTR,@CF00060                                     0649 02611000
*   END;                            /* OF DO WHILE           @ZA11392*/ 02612000
@RC00647 DS    0H                                                  0650 02613000
@DE00637 LTR   CQEPTR,CQEPTR                                       0650 02614000
         BNZ   @DL00637                                            0650 02615000
*   END SCANCQES;                   /*                       @ZA11392*/ 02616000
@EL00018 DS    0H                                                  0651 02617000
@EF00018 DS    0H                                                  0651 02618000
@ER00018 LM    @14,@12,@SA00018                                    0651 02619000
         BR    @14                                                 0651 02620000
         EJECT                                                          02621000
*                                                                  0652 02622000
*/***********************************************************@ZA11392*/ 02623000
*/*                                                          @ZA11392*/ 02624000
*/*  PROCEDURE NAME: IEEXEDNA.SYSLGWQE                       @ZA11392*/ 02625000
*/*                                                          @ZA11392*/ 02626000
*/*  FUNCTION: COUNTS THE NUMBER OF WQE'S QUEUED TO HARDCOPY @ZA11392*/ 02627000
*/*            WHEN SYSLOG IS HARDCOPY.                      @ZA11392*/ 02628000
*/*                                                          @ZA11392*/ 02629000
*/*  INPUTS:   UCMWTOQ - ADDRESS OF WQE QUEUE                @ZA11392*/ 02630000
*/*                                                          @ZA11392*/ 02631000
*/*  OUTPUTS:  HCWQECNT - NUMBER OF WQE'S QUEUED TO HARDCOPY @ZA11392*/ 02632000
*/*                                                          @ZA11392*/ 02633000
*/*  CALLS:    SCANMINR                                      @ZA11392*/ 02634000
*/*                                                          @ZA11392*/ 02635000
*/***********************************************************@ZA11392*/ 02636000
*                                                                  0652 02637000
*SYSLGWQE:                                                         0652 02638000
*   PROC;                           /*                       @ZA11392*/ 02639000
SYSLGWQE STM   @14,@12,@SA00019                                    0652 02640000
*   HCWQECNT=0;                     /* INIT NUMBER WQE'S TO HARDCOPY    02641000
*                                                            @ZA11392*/ 02642000
         SLR   @12,@12                                             0653 02643000
         STH   @12,HCWQECNT                                        0653 02644000
*   WQEPTR=UCMWTOQ;                 /* POINT TO START OF QUEUE     0654 02645000
*                                                            @ZA11392*/ 02646000
         L     @12,UCMWTOQ(,UCMBASPT)                              0654 02647000
         ST    @12,WQEPTR                                          0654 02648000
*   DO WHILE WQEPTR^=0;             /* DO UNTIL END OF QUEUE @ZA11392*/ 02649000
         B     @DE00655                                            0655 02650000
@DL00655 DS    0H                                                  0656 02651000
*     IF(WQEQFHC='1'B&              /* WQE QUEUE TO HARDCOPY ON    0656 02652000
*                                                            @ZA11392*/ 02653000
*         WQEBUFC='1'B&             /* WQE READY TO GO TO HARDCOPY 0656 02654000
*                                                            @ZA11392*/ 02655000
*         WQEQDFHC='0'B)            /* WQE NOT QUEUED FOR HARDCOPY 0656 02656000
*                                                            @ZA11392*/ 02657000
*       THEN                        /* SHOULD WQE BE COUNTED TO GO TO   02658000
*                                      HARDCPY               @ZA11392*/ 02659000
         L     @12,WQEPTR                                          0656 02660000
         TM    WQEQFHC(@12),B'01000000'                            0656 02661000
         BNO   @RF00656                                            0656 02662000
         TM    WQEBUFC(@12),B'00100000'                            0656 02663000
         BNO   @RF00656                                            0656 02664000
         TM    WQEQDFHC(@12),B'00010000'                           0656 02665000
         BNZ   @RF00656                                            0656 02666000
*       HCWQECNT=HCWQECNT+1;        /* YES, COUNT THE WQE    @ZA11392*/ 02667000
         LH    @12,HCWQECNT                                        0657 02668000
         LA    @12,1(,@12)                                         0657 02669000
         STH   @12,HCWQECNT                                        0657 02670000
*     IF(WMJMMLWB='1'B&             /* IS THIS A MAJOR WQE?  @ZA11392*/ 02671000
*         WMJMMIN^=0) THEN          /* AND IS THERE A MINOR  @ZA11392*/ 02672000
@RF00656 L     @12,WMJPTR                                          0658 02673000
         TM    WMJMMLWB(@12),B'01000000'                           0658 02674000
         BNO   @RF00658                                            0658 02675000
         L     @12,WMJMMIN(,@12)                                   0658 02676000
         LTR   @12,@12                                             0658 02677000
         BZ    @RF00658                                            0658 02678000
*       CALL SCANMINR(HCWQECNT);    /* YES TO BOTH, COUNT NO. OF   0659 02679000
*                                      WQE'S WQE'S USED BY MLWTO   0659 02680000
*                                                            @ZA11392*/ 02681000
         LA    @12,HCWQECNT                                        0659 02682000
         ST    @12,@AL00001                                        0659 02683000
         LA    @01,@AL00001                                        0659 02684000
         BAL   @14,SCANMINR                                        0659 02685000
*     WQEPTR=WQELKPA;               /* POINT TO NEXT WQE     @ZA11392*/ 02686000
@RF00658 L     @12,WQEPTR                                          0660 02687000
         L     @12,WQELKPA-1(,@12)                                 0660 02688000
         LA    @12,0(,@12)                                         0660 02689000
         ST    @12,WQEPTR                                          0660 02690000
*   END;                            /* OF DO WHILE           @ZA11392*/ 02691000
@DE00655 L     @12,WQEPTR                                          0661 02692000
         LTR   @12,@12                                             0661 02693000
         BNZ   @DL00655                                            0661 02694000
*   END SYSLGWQE;                   /*                       @ZA11392*/ 02695000
@EL00019 DS    0H                                                  0662 02696000
@EF00019 DS    0H                                                  0662 02697000
@ER00019 LM    @14,@12,@SA00019                                    0662 02698000
         BR    @14                                                 0662 02699000
         EJECT                                                          02700000
*                                                                  0663 02701000
*/***********************************************************@ZA11392*/ 02702000
*/*                                                          @ZA11392*/ 02703000
*/*  PROCEDURE NAME: IEEXEDNA.SCANMINR                       @ZA11392*/ 02704000
*/*                                                          @ZA11392*/ 02705000
*/*  FUNCTION: COUNTS THE NUMBER OF WQE'S ON THE MINOR CHAIN @ZA11392*/ 02706000
*/*            FOR A MLWTO                                   @ZA11392*/ 02707000
*/*                                                          @ZA11392*/ 02708000
*/*  INPUTS:   WQEPTR - POINT TO THE MAJOR WQE OF IMLWTO     @ZA11392*/ 02709000
*/*                     A MINOR WILL ALWAYS BE PRESENT       @ZA11392*/ 02710000
*/*            COUNT  - THE COUNT WHICH SHOULD BE INCREASED  @ZA11392*/ 02711000
*/*                     FOR EACH WQE FOUND                   @ZA11392*/ 02712000
*/*                                                          @ZA11392*/ 02713000
*/*  OUTPUT:   COUNT  - NUMBER OF WQE'S FOUND SO FAR         @ZA11392*/ 02714000
*/*                                                          @ZA11392*/ 02715000
*/*  CALLS:    NONE                                          @ZA11392*/ 02716000
*/*                                                          @ZA11392*/ 02717000
*/***********************************************************@ZA11392*/ 02718000
*                                                                  0663 02719000
*SCANMINR:                                                         0663 02720000
*   PROC(COUNT);                    /*                       @ZA11392*/ 02721000
SCANMINR STM   @14,@12,12(@13)                                     0663 02722000
         MVC   @PC00020(4),0(@01)                                  0663 02723000
*   DCL                                                            0664 02724000
*     COUNT FIXED(15);              /* TO BE INCREASED FOR EACH WQE     02725000
*                                      IN MLWTO MINOR CHAIN  @ZA11392*/ 02726000
*   WMNPTR=WMJMMIN;                 /* POINT TO MINOR        @ZA11392*/ 02727000
         L     @12,WMJPTR                                          0665 02728000
         L     WMNPTR,WMJMMIN(,@12)                                0665 02729000
*   COUNT=COUNT+1;                  /* ONE MINOR EXISTS SINCE CONTROL   02730000
*                                      REACHED HERE          @ZA11392*/ 02731000
         L     @12,@PC00020                                        0666 02732000
         LA    @11,1                                               0666 02733000
         AH    @11,COUNT(,@12)                                     0666 02734000
         STH   @11,COUNT(,@12)                                     0666 02735000
*   DO WHILE WMNMNX1^=0&            /* DO WHILE A MINOR EXISTS IN 2ND   02736000
*                                      HALF OF WQE AND       @ZA11392*/ 02737000
*         WMNMNX2^=0;               /* 2ND MINOR IN WQE IS NOT LAST     02738000
*                                      MINOR                 @ZA11392*/ 02739000
         B     @DE00667                                            0667 02740000
@DL00667 DS    0H                                                  0668 02741000
*     WMNPTR=WMNMNX2;               /* POINT TO NEXT MINOR PAIR    0668 02742000
*                                                            @ZA11392*/ 02743000
         L     @12,WMNMNX2-1(,WMNPTR)                              0668 02744000
         LA    @12,0(,@12)                                         0668 02745000
         LR    WMNPTR,@12                                          0668 02746000
*     COUNT=COUNT+1;                /* ONE MORE WQE FOUND    @ZA11392*/ 02747000
         L     @12,@PC00020                                        0669 02748000
         LA    @11,1                                               0669 02749000
         AH    @11,COUNT(,@12)                                     0669 02750000
         STH   @11,COUNT(,@12)                                     0669 02751000
*   END;                            /* OF DO WHILE           @ZA11392*/ 02752000
@DE00667 L     @12,WMNMNX1-1(,WMNPTR)                              0670 02753000
         LA    @12,0(,@12)                                         0670 02754000
         LTR   @12,@12                                             0670 02755000
         BZ    @DC00667                                            0670 02756000
         L     @12,WMNMNX2-1(,WMNPTR)                              0670 02757000
         LA    @12,0(,@12)                                         0670 02758000
         LTR   @12,@12                                             0670 02759000
         BNZ   @DL00667                                            0670 02760000
@DC00667 DS    0H                                                  0671 02761000
*   END SCANMINR;                   /*                       @ZA11392*/ 02762000
@EL00020 DS    0H                                                  0671 02763000
@EF00020 DS    0H                                                  0671 02764000
@ER00020 LM    @14,@12,12(@13)                                     0671 02765000
         BR    @14                                                 0671 02766000
*   END IEEXEDNA                                                   0672 02767000
*                                                                  0672 02768000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 02769000
