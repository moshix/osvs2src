*   LINECT=LINECT+1;                /* INCRE. LINE COUNT BY 1        */ 02190000
*                                                                  0558 02191000
         LH    @12,LINECT                                          0558 02192000
         LA    @12,1(,@12)                                         0558 02193000
         STH   @12,LINECT                                          0558 02194000
*   /*****************************************************************/ 02195000
*   /*                                                               */ 02196000
*   /* CHECK TO SEE IF PROCESSING IS FOR THE LAST LINE       @ZA15315*/ 02197000
*   /*                                                               */ 02198000
*   /*****************************************************************/ 02199000
*                                                                  0559 02200000
*   IF(HDCOPY='0'B)&                /* BUILDING HC LINE. IF SO THEN     02201000
*                                      NOT DONE YET.         @ZA15315*/ 02202000
*       (INDEX^=0)&                 /* IF ZERO THEN NOT PROCESSING 0559 02203000
*                                      ANY UCMES YET.        @ZA15315*/ 02204000
*       ((INDEX=MAXUCMES)|          /* PROCESSING LAST UCME OR     0559 02205000
*                                                            @ZA15315*/ 02206000
*       ((COMPC(INDEX)^=0)&         /* A COMPOSITE CONSOLE AND     0559 02207000
*                                                            @ZA15315*/ 02208000
*       (INDEX+1=MAXUCMES))) THEN   /* THE OUTPUT HALF IS THE LAST 0559 02209000
*                                      UCME                  @ZA15315*/ 02210000
         TM    HDCOPY,B'01000000'                                  0559 02211000
         BNZ   @RF00559                                            0559 02212000
