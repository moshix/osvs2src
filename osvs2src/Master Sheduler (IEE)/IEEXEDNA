*       END;                        /* THE DCM.                      */ 00804000
*     UCMDVEPT=UCMDVEPT+UCMVEZ;     /* UPDATE TO NEXT UCME           */ 00805000
@RF00308 AL    UCMDVEPT,UCMVEZ(,UCMBASPT)                          0313 00806000
*   END;                            /* END UCME SCAN LOOP            */ 00807000
*                                                                  0314 00808000
         AL    I,@CF00068                                          0314 00809000
@DE00292 C     I,MAXUCMES                                          0314 00810000
         BNH   @DL00292                                            0314 00811000
*   /*****************************************************************/ 00812000
*   /*                                                               */ 00813000
*   /* IF HARDCOPY IS IN THE SYSTEM, THEN FIGURE INDEX TO THE CONSOLE*/ 00814000
*   /* (IF ANY) TO WHICH HARDCOPY IS TO GO                           */ 00815000
*   /*                                                               */ 00816000
*   /*****************************************************************/ 00817000
*                                                                  0315 00818000
*   IF UCMHCUCM^=0                  /* IF HARDCOPY TO A DEVICE       */ 00819000
*     THEN                          /* FIGURE INDEX TO HARDCOPY      */ 00820000
         L     @12,UCMMCSPT                                        0315 00821000
         L     @12,UCMHCUCM(,@12)                                  0315 00822000
         LTR   @12,@12                                             0315 00823000
         BZ    @RF00315                                            0315 00824000
*     HCUCM=(UCMHCUCM-UCMVEA)/UCMVEZ+1;/* UCME                       */ 00825000
         SL    @12,UCMVEA(,UCMBASPT)                               0316 00826000
         LR    @00,@12                                             0316 00827000
         SRDA  @00,32                                              0316 00828000
         D     @00,UCMVEZ(,UCMBASPT)                               0316 00829000
         AL    @01,@CF00068                                        0316 00830000
         LR    HCUCM,@01                                           0316 00831000
*   ELSE                            /* OTHERWISE, ZERO               */ 00832000
*     DO;                           /*                       @ZA11392*/ 00833000
         B     @RC00315                                            0317 00834000
@RF00315 DS    0H                                                  0318 00835000
*       HCUCM=0;                    /* THE FIELD                     */ 00836000
         SLR   HCUCM,HCUCM                                         0318 00837000
*       IF UCMSYSG='1'B THEN        /* IS SYSLOG HARDCPY?    @ZA11392*/ 00838000
         L     @12,UCMMCSPT                                        0319 00839000
         TM    UCMSYSG(@12),B'00000010'                            0319 00840000
         BNO   @RF00319                                            0319 00841000
*         CALL SYSLGWQE;            /* YES, COUNT THE NUMBER OF WQE'S   00842000
*                                      THAT ARE QUEUED TO HARDCOPY.     00843000
*                                                            @ZA11392*/ 00844000
         BAL   @14,SYSLGWQE                                        0320 00845000
*     END;                          /*                       @ZA11392*/ 00846000
*   END GETDATA;                    /* END DATA GATHERING RTN        */ 00847000
@EL00006 DS    0H                                                  0322 00848000
@EF00006 DS    0H                                                  0322 00849000
@ER00006 LM    @14,@04,@SA00006                                    0322 00850000
         LM    @06,@12,@SA00006+28                                 0322 00851000
         BR    @14                                                 0322 00852000
         EJECT                                                          00853000
*                                                                  0323 00854000
*/********************************************************************/ 00855000
*/*                                                                  */ 00856000
*/* PROCEDURE NAME: IEEXEDNA.UNITNAME                                */ 00857000
*/*                                                                  */ 00858000
*/* FUNCTION: FILLS IN EBCDIC UNIT NAMES OF CONSOLE NOW BEING        */ 00859000
*/*           PROCESSED (BOTH UNIT NAMES IF A COMPOSITE), AND ALSO   */ 00860000
*/*           FILLS IN UNIT NAME OF ALTERNATE CONSOLE (BOTH NAMES IF */ 00861000
*/*           A COMPOSITE).    THESE NAMES ARE PLACED INTO THE MSG   */ 00862000
*/*           CURRENTLY BEING BUILT.                                 */ 00863000
*/*                                                                  */ 00864000
*/* INPUTS: 1.WPLPTR  - BASEPTR FOR THE MESSAGE BEING BUILT. MESSAGE */ 00865000
*/*           DSECT IS BASED ON THIS POINTER.                        */ 00866000
*/*                                                                  */ 00867000
*/*         2.CONSPTR - BASEPTR FOR THE DATA ENTRY            FOR    */ 00868000
*/*           THE CONSOLE BEING PROCESSED                            */ 00869000
*/*                                                                  */ 00870000
*/* OUTPUTS:  PORTION OF MESSAGE TEXT IN THE FOLLOWING FORMAT:       */ 00871000
*/*                                                                  */ 00872000
*/*            XXX,YYY/AAA,BBB                                       */ 00873000
*/*                                                                  */ 00874000
*/*           WHERE: XXX IS NAMEOF INPUT-HALF IF COMPOSITE CONSOLE   */ 00875000
*/*                  YYY IS NAMEOF OUTPUT-HALF IF COMPOSITE. IF NOT  */ 00876000
*/*                      COMPOSITE,YYY WILLBE CONSOLE UNIT NAME.     */ 00877000
*/*                  AAA IS NAME OF INPUT HALF OF ALTERNATE IF A     */ 00878000
*/*                      COMPOSITE. IF NOT COMPOSITE THEN AAA WILL BE*/ 00879000
*/*                      UNIT NAME OF THE ALTERNATE CONSOLE.         */ 00880000
*/*                  BBB IS THE NAME OF THE OUTPUT HALF OF THE ALTER-*/ 00881000
*/*                      NATE CONSOLE, IF IT IS A COMPOSITE.         */ 00882000
*/*                                                                  */ 00883000
*/*                                                                  */ 00884000
*/****************************************************************** */ 00885000
*                                                                  0323 00886000
*UNITNAME:                                                         0323 00887000
*   PROC OPTIONS(NOSAVEAREA);       /* SEARCH FOR AND FILL IN UNIT   */ 00888000
UNITNAME STM   @14,@03,12(@13)                                     0323 00889000
         STM   @05,@12,40(@13)                                     0323 00890000
*   INDEXSVE=INDEX;                 /* SAVE CURRENT ENTRY INDEX      */ 00891000
*                                                                  0324 00892000
         LR    INDEXSVE,INDEX                                      0324 00893000
*   /*****************************************************************/ 00894000
*   /*                                                               */ 00895000
*   /* CHECK FOR COMPOSITE CONSOLE                                   */ 00896000
*   /*                                                               */ 00897000
*   /*****************************************************************/ 00898000
*                                                                  0325 00899000
*   IF COMPC(INDEX)^=0              /* IF THIS IS A COMPOSITE        */ 00900000
*     THEN                          /* THEN WE ASSUME IT IS THE      */ 00901000
         LR    @12,INDEX                                           0325 00902000
         MH    @12,@CH00077                                        0325 00903000
         LR    @07,CONSPTR                                         0325 00904000
         AL    @07,@CF02815                                        0325 00905000
         LH    @07,COMPC-14(@12,@07)                               0325 00906000
         LTR   @07,@07                                             0325 00907000
         BZ    @RF00325                                            0325 00908000
*     DO;                           /* INPUT HALF.                   */ 00909000
*       ICONS=NAME(INDEX);          /* FILL UNIT NAME INTO MSG       */ 00910000
         ALR   @12,CONSPTR                                         0327 00911000
         AL    @12,@CF02829                                        0327 00912000
         MVC   ICONS(3,WPLPTR),NAME-16(@12)                        0327 00913000
*       COMMA1=',';                 /* PUT COMMA AFTER UNIT NAME     */ 00914000
         MVI   COMMA1(WPLPTR),C','                                 0328 00915000
*       INDEX=COMPC(INDEX);         /* SET INDEX TO 0-HALF           */ 00916000
         LR    INDEX,@07                                           0329 00917000
*     END;                          /* CONTINUE                      */ 00918000
*   OCONS=NAME(INDEX);              /* SET UNIT NAME IN MESSAGE      */ 00919000
@RF00325 LR    @12,INDEX                                           0331 00920000
         MH    @12,@CH00077                                        0331 00921000
         ST    @12,@TF00001                                        0331 00922000
         ALR   @12,CONSPTR                                         0331 00923000
         AL    @12,@CF02829                                        0331 00924000
         MVC   OCONS(3,WPLPTR),NAME-16(@12)                        0331 00925000
*   IF DISPK(INDEX)='1'B THEN       /* SUBSYSTEM CONSOLE     @Y02668P*/ 00926000
         LR    @12,CONSPTR                                         0332 00927000
         AL    @12,@TF00001                                        0332 00928000
         AL    @12,@CF02830                                        0332 00929000
         TM    DISPK-9(@12),B'00100000'                            0332 00930000
         BNO   @RF00332                                            0332 00931000
*     DO;                           /*                       @Y02668P*/ 00932000
*       IALTRNT='JES';              /* ALTERNATE NAME-JES    @Y02668P*/ 00933000
         MVC   IALTRNT(3,WPLPTR),@CC02711                          0334 00934000
*       ALTEN(INDEX)=0;             /* JES ALTERNATE         @Y02668P*/ 00935000
         SLR   @12,@12                                             0335 00936000
         L     @07,@TF00001                                        0335 00937000
         AL    @07,@CF02827                                        0335 00938000
         STH   @12,ALTEN-12(@07,CONSPTR)                           0335 00939000
*     END;                          /*                       @Y02668P*/ 00940000
*   INDEX=INDEXSVE;                 /* RESTORE INDEX                 */ 00941000
@RF00332 LR    INDEX,INDEXSVE                                      0337 00942000
*   SLASH='/';                      /* SET SLASH IN MSG.             */ 00943000
*                                                                  0338 00944000
         MVI   SLASH(WPLPTR),C'/'                                  0338 00945000
*   /*****************************************************************/ 00946000
*   /*                                                               */ 00947000
*   /* CHECK TO SEE IF AN ALTERNATE EXISTS IF THE ALTERNATE IS A     */ 00948000
*   /* SUBSYSTEM CONSOLE, UCMDISPK                           @Y02668P*/ 00949000
*   /* IS ON, THEN THE ALTERNATE CONSOLE NAME WILL BE        @Y02668P*/ 00950000
*   /* 'JES'                                                 @Y02668P*/ 00951000
*   /*                                                               */ 00952000
*   /*****************************************************************/ 00953000
*                                                                  0339 00954000
*   IF ALTEN(INDEX)^=0              /* IF THERE IS AN ALTERNATE      */ 00955000
*     THEN                          /* CONSOLE, GO GET ITS UNIT      */ 00956000
         SLR   @12,@12                                             0339 00957000
         LA    @07,24                                              0339 00958000
         LR    @01,INDEX                                           0339 00959000
         MR    @00,@07                                             0339 00960000
         AL    @01,@CF02827                                        0339 00961000
         LH    @05,ALTEN-12(@01,CONSPTR)                           0339 00962000
         CR    @05,@12                                             0339 00963000
         BE    @RF00339                                            0339 00964000
*     DO;                           /* NAME.                         */ 00965000
*       INDEX=ALTEN(INDEX);         /* SET INDEX TO ALTEN ENTRY      */ 00966000
         LR    INDEX,@05                                           0341 00967000
*       IALTRNT=NAME(INDEX);        /* PUT UNIT NAME IN MESSAGE      */ 00968000
*                                                                  0342 00969000
         LR    @01,INDEX                                           0342 00970000
         MR    @00,@07                                             0342 00971000
         ST    @01,@TF00001                                        0342 00972000
         ALR   @01,CONSPTR                                         0342 00973000
         AL    @01,@CF02829                                        0342 00974000
         MVC   IALTRNT(3,WPLPTR),NAME-16(@01)                      0342 00975000
*       /*************************************************************/ 00976000
*       /*                                                           */ 00977000
*       /* SEE IF ALTERNATE HAS A COMPOSITE                          */ 00978000
*       /*                                                           */ 00979000
*       /*************************************************************/ 00980000
*                                                                  0343 00981000
*       IF COMPC(INDEX)^=0          /* IF ALTEN HAS A COMPOSITE      */ 00982000
*         THEN                      /* THEN WE MUST GET UNIT NAME    */ 00983000
         L     @05,@TF00001                                        0343 00984000
         AL    @05,@CF02815                                        0343 00985000
         LH    @05,COMPC-14(@05,CONSPTR)                           0343 00986000
         CR    @05,@12                                             0343 00987000
         BE    @RF00343                                            0343 00988000
*         DO;                       /* OF THE OUTPUT HALF.           */ 00989000
*           INDEX=COMPC(INDEX);     /* SET INDEX TO COMPC ENT.       */ 00990000
         LR    INDEX,@05                                           0345 00991000
*           COMMA2=',';             /* SET COMMA AFTER INPUT NAME    */ 00992000
         MVI   COMMA2(WPLPTR),C','                                 0346 00993000
*           OALTRNT=NAME(INDEX);    /* GET THE NAME                  */ 00994000
         LR    @01,@07                                             0347 00995000
         MR    @00,INDEX                                           0347 00996000
         ALR   @01,CONSPTR                                         0347 00997000
         AL    @01,@CF02829                                        0347 00998000
         MVC   OALTRNT(3,WPLPTR),NAME-16(@01)                      0347 00999000
*         END;                      /* END COMPC PROCESSING.         */ 01000000
*       INDEX=INDEXSVE;             /* RESTORE ORIGINAL INDEX.       */ 01001000
@RF00343 LR    INDEX,INDEXSVE                                      0349 01002000
*     END;                          /* END ALTERNATE PROCESSING      */ 01003000
*   END UNITNAME;                   /* END UNIT NAME PROCESSING      */ 01004000
@EL00007 DS    0H                                                  0351 01005000
@EF00007 DS    0H                                                  0351 01006000
@ER00007 LM    @14,@03,12(@13)                                     0351 01007000
         LM    @05,@12,40(@13)                                     0351 01008000
         BR    @14                                                 0351 01009000
         EJECT                                                          01010000
*                                                                  0352 01011000
*/********************************************************************/ 01012000
*/*                                                                  */ 01013000
*/* PROCEDURE NAME: IEEXEDNA.RCODES                                  */ 01014000
*/*                                                                  */ 01015000
*/* FUNCTION: FILL IN CONSOLE ROUTE CODES COLUMN IN DATA LINES.      */ 01016000
*/*                                                                  */ 01017000
*/* INPUTS:   CURRENT CONSOLE ENTRY INDEX. RTCD FIELD IS ONLY FIELD  */ 01018000
*/*           USED FOR OUTPUT PRODUCTION.                            */ 01019000
*/*                                                                  */ 01020000
*/* OUTPUTS:  ROUTE CODES IN MESSAGE TEXT IN THE FOLLOWING GENERAL   */ 01021000
*/*           FORMAT:                                                */ 01022000
*/*                                                                  */ 01023000
*/*           RC,RC,RC-RC     WHERE  RC IS A SPECIFIC ROUTE CODE,    */ 01024000
*/*                             AND RC-RC IS A RANGE OF SEQUENTIAL   */ 01025000
*/*                             ROUTE CODES.                         */ 01026000
*/*                                                                  */ 01027000
*/* CALLS:    NONE.                                                  */ 01028000
*/*                                                                  */ 01029000
*/********************************************************************/ 01030000
*                                                                  0352 01031000
*RCODES:                                                           0352 01032000
*   PROC OPTIONS(NOSAVEAREA);       /* ROUTING CODES PROCESSOR       */ 01033000
RCODES   STM   @14,@03,12(@13)                                     0352 01034000
         STM   @05,@12,40(@13)                                     0352 01035000
*   RSAVE=0;                        /* INITIALIZE RSAVE TO ZERO      */ 01036000
         SLR   RSAVE,RSAVE                                         0353 01037000
*   SKIP='0'B;                      /* INITIALIZE SKIP TO ZERO       */ 01038000
         NI    SKIP,B'11101111'                                    0354 01039000
*   L=1;                            /* INIT COLUMN INDEX TO 1        */ 01040000
*                                                                  0355 01041000
         LA    L,1                                                 0355 01042000
*   /*****************************************************************/ 01043000
*   /*                                                               */ 01044000
*   /* CHECK TO SEE IF HARDCOPY MESSAGE BEING BUILT                  */ 01045000
*   /*                                                               */ 01046000
*   /*****************************************************************/ 01047000
*                                                                  0356 01048000
*   IF HDCOPY='1'B                  /* IF ON THE HARDCOPY MSG        */ 01049000
*     THEN                          /* THEN GET ROUTE CODES          */ 01050000
         TM    HDCOPY,B'01000000'                                  0356 01051000
         BNO   @RF00356                                            0356 01052000
*     RTCODES=UCMHRDRT;             /* FROM THE UCM BASE             */ 01053000
         L     @12,UCMMCSPT                                        0357 01054000
         LH    @12,UCMHRDRT(,@12)                                  0357 01055000
         N     @12,@CF02819                                        0357 01056000
         STH   @12,RTCODES                                         0357 01057000
*   ELSE                            /* IF NOT HARDCOPY MSG, THEN     */ 01058000
*                                                                  0358 01059000
*     /***************************************************************/ 01060000
*     /*                                                             */ 01061000
*     /* CHECK TO SEE IF CONSOLE IS A COMPOSITE                      */ 01062000
*     /*                                                             */ 01063000
*     /***************************************************************/ 01064000
*                                                                  0358 01065000
*     IF COMPC(INDEX)=0             /* IF NOT A COMPOSITE            */ 01066000
*       THEN                        /* THEN GET RCODES FROM UCME     */ 01067000
         B     @RC00356                                            0358 01068000
@RF00356 LR    @12,INDEX                                           0358 01069000
         MH    @12,@CH00077                                        0358 01070000
         LR    @06,CONSPTR                                         0358 01071000
         AL    @06,@CF02815                                        0358 01072000
         LH    @06,COMPC-14(@12,@06)                               0358 01073000
         LTR   @06,@06                                             0358 01074000
         BNZ   @RF00358                                            0358 01075000
*       RTCODES=RTCD(INDEX);        /* GET THEM FROM THE UCME        */ 01076000
         AL    @12,@CF02826                                        0359 01077000
         LH    @12,RTCD-10(@12,CONSPTR)                            0359 01078000
         STH   @12,RTCODES                                         0359 01079000
*     ELSE                          /* IF IT IS COMPOSITE            */ 01080000
*       DO;                         /* THEN WE MUST GET THE          */ 01081000
         B     @RC00358                                            0360 01082000
@RF00358 DS    0H                                                  0361 01083000
*         INDEXSVE=INDEX;           /* SAVE CURRENT INDEX            */ 01084000
         LR    INDEXSVE,INDEX                                      0361 01085000
*         INDEX=COMPC(INDEX);       /* SET INDEX TO OUTPUT           */ 01086000
         LA    @12,24                                              0362 01087000
         LR    @01,INDEX                                           0362 01088000
         MR    @00,@12                                             0362 01089000
         AL    @01,@CF02815                                        0362 01090000
         LH    @02,COMPC-14(@01,CONSPTR)                           0362 01091000
         LR    INDEX,@02                                           0362 01092000
*         RTCODES=RTCD(INDEX);      /* HALF OF COMPOSITE             */ 01093000
         LR    @01,@12                                             0363 01094000
         MR    @00,INDEX                                           0363 01095000
         AL    @01,@CF02826                                        0363 01096000
         LH    @12,RTCD-10(@01,CONSPTR)                            0363 01097000
         STH   @12,RTCODES                                         0363 01098000
*         INDEX=INDEXSVE;           /* RESTORE INDEX                 */ 01099000
         LR    INDEX,INDEXSVE                                      0364 01100000
*       END;                        /* END GET RCODES SECTION        */ 01101000
*                                                                  0365 01102000
*   /*****************************************************************/ 01103000
*   /*                                                               */ 01104000
*   /* CHECK FOR NO ROUTE CODES                                      */ 01105000
*   /*                                                               */ 01106000
*   /*****************************************************************/ 01107000
*                                                                  0366 01108000
*   IF RTCODES=0                    /* IF NO ROUTE CODES             */ 01109000
*     THEN                                                         0366 01110000
@RC00358 DS    0H                                                  0366 01111000
@RC00356 LH    @12,RTCODES                                         0366 01112000
         N     @12,@CF02819                                        0366 01113000
         LTR   @12,@12                                             0366 01114000
         BNZ   @RF00366                                            0366 01115000
*     ROUTCDES(1:4)='NONE';         /* IN THE MESSAGE                */ 01116000
         MVC   ROUTCDES(4,WPLPTR),@CC02716                         0367 01117000
*   ELSE                            /* IF NOT NONE, IS IT ALL ?      */ 01118000
*                                                                  0368 01119000
*     /***************************************************************/ 01120000
*     /*                                                             */ 01121000
*     /* CHECK FOR ROUTE CODES = ALL                                 */ 01122000
*     /*                                                             */ 01123000
*     /***************************************************************/ 01124000
*                                                                  0368 01125000
*     IF RTCODES='FFFE'X            /* IF ALL SPECIFIED              */ 01126000
*         |                         /* AS 1-15 OR 1-16               */ 01127000
*         RTCODES='FFFF'X           /* IN EITHER WAY                 */ 01128000
*       THEN                        /* THEN PLACE ALL                */ 01129000
         B     @RC00366                                            0368 01130000
@RF00366 CLC   RTCODES(2),@CH02717                                 0368 01131000
         BE    @RT00368                                            0368 01132000
         CLC   RTCODES(2),@CH02718                                 0368 01133000
         BNE   @RF00368                                            0368 01134000
@RT00368 DS    0H                                                  0369 01135000
*       ROUTCDES(1:3)='ALL';        /* IN THE MESSAGE                */ 01136000
         MVC   ROUTCDES(3,WPLPTR),@CC02719                         0369 01137000
*     ELSE                          /* OTHERWISE CHECK EACH BIT      */ 01138000
*       DO I=1 TO 16;               /* LOOP OVER ALL 16 R/C BITS     */ 01139000
*                                                                  0370 01140000
         B     @RC00368                                            0370 01141000
@RF00368 LA    I,1                                                 0370 01142000
@DL00370 DS    0H                                                  0371 01143000
*         /***********************************************************/ 01144000
*         /*                                                         */ 01145000
*         /* CHECK TO SEE IF CURRENT BIT IS ON                       */ 01146000
*         /*                                                         */ 01147000
*         /***********************************************************/ 01148000
*                                                                  0371 01149000
*         IF(BMASK(I)&RTCODES)^=0   /* IF CURRENT BIT ON             */ 01150000
*           THEN                    /* THEN FIRST DECIDE IF WORK-    */ 01151000
         SLR   @12,@12                                             0371 01152000
         LR    @02,I                                               0371 01153000
         ALR   @02,@02                                             0371 01154000
         LH    @15,RTCODES                                         0371 01155000
         N     @15,@CF02819                                        0371 01156000
         LH    @14,BMASK-2(@02)                                    0371 01157000
         N     @14,@CF02819                                        0371 01158000
         NR    @15,@14                                             0371 01159000
         CR    @15,@12                                             0371 01160000
         BE    @RF00371                                            0371 01161000
*           DO;                     /* ON A SERIES. IF RSAVE=0       */ 01162000
*                                                                  0372 01163000
*             /*******************************************************/ 01164000
*             /*                                                     */ 01165000
*             /* CHECK FOR PART OF A SERIES OF ROUTCODES             */ 01166000
*             /*                                                     */ 01167000
*             /*******************************************************/ 01168000
*                                                                  0373 01169000
*             IF RSAVE=0            /* THEN THIS IS NOT PART OF A    */ 01170000
*               THEN                /* SERIES,SO IT IS TO GO INTO    */ 01171000
         CR    RSAVE,@12                                           0373 01172000
         BNE   @RF00373                                            0373 01173000
*               DO;                 /* THE MESSAGE. INITIALIZE THE   */ 01174000
*                 SKIP='0'B;        /* SKIP FLAG TO SHOW NONE SKIP   */ 01175000
         NI    SKIP,B'11101111'                                    0375 01176000
*                 ROUTCDES(L:L+1)=RCODS(I);/* PED,MOVE R/C           */ 01177000
*                                                                  0376 01178000
         LR    @12,WPLPTR                                          0376 01179000
         ALR   @12,L                                               0376 01180000
         LA    @07,RCODS-2(@02)                                    0376 01181000
         MVC   ROUTCDES-1(2,@12),0(@07)                            0376 01182000
*                 /***************************************************/ 01183000
*                 /*                                                 */ 01184000
*                 /* CHECK FOR LAST R/C LESS THAN TEN                */ 01185000
*                 /*                                                 */ 01186000
*                 /***************************************************/ 01187000
*                                                                  0377 01188000
*                 IF I<10           /* INTO MSG. IF A ONE DIGIT NO   */ 01189000
*                   THEN            /* THEN ONLY INDEX UP BY ONE     */ 01190000
         C     I,@CF00035                                          0377 01191000
         BNL   @RF00377                                            0377 01192000
*                   L=L+1;          /* TO SET THE COMMA. IF TWO      */ 01193000
         AL    L,@CF00068                                          0378 01194000
*                 ELSE              /* DIGIT NO. THEN INDEX UP BY    */ 01195000
*                   L=L+2;          /* TWO TO GET CORRECT COMMA      */ 01196000
         B     @RC00377                                            0379 01197000
@RF00377 AL    L,@CF00080                                          0379 01198000
*                 ROUTCDES(L)=',';  /* POSITION. COMMA IN MSG        */ 01199000
@RC00377 LR    @12,WPLPTR                                          0380 01200000
         ALR   @12,L                                               0380 01201000
         MVI   ROUTCDES-1(@12),C','                                0380 01202000
*                 L=L+1;            /* INDEX PAST THE COMMA          */ 01203000
         AL    L,@CF00068                                          0381 01204000
*                 RSAVE=I;          /* SET RSAVE IN CASE NEXT R/C    */ 01205000
         LR    RSAVE,I                                             0382 01206000
*               END;                /* IS SEQUENTIAL.                */ 01207000
*             ELSE                  /* IF THIS IS IN A SERIES,THEN   */ 01208000
*               SKIP='1'B;          /* SIGNAL R/C SKIPPED & CONTIN   */ 01209000
         B     @RC00373                                            0384 01210000
@RF00373 OI    SKIP,B'00010000'                                    0384 01211000
*           END;                    /* UE TO NEXT BIT POSITION       */ 01212000
*         ELSE                      /* IF CURRENT BIT NOT ON,THEN    */ 01213000
*           DO;                     /* CHECK TO SEE IF WE WERE IN    */ 01214000
*                                                                  0386 01215000
         B     @RC00371                                            0386 01216000
@RF00371 DS    0H                                                  0387 01217000
*             /*******************************************************/ 01218000
*             /*                                                     */ 01219000
*             /* CHECK TO SEE IF WE WERE IN A SERIES AFTER LAST R/C  */ 01220000
*             /*                                                     */ 01221000
*             /*******************************************************/ 01222000
*                                                                  0387 01223000
*             IF RSAVE^=0           /* A SERIES. IF SO, AND IF THE   */ 01224000
*               THEN                /* SKIP FLAG IS ON, THEN WE      */ 01225000
         C     RSAVE,@CF00044                                      0387 01226000
         BE    @RF00387                                            0387 01227000
*               DO;                 /* MUST FILL IN THE LAST R/C     */ 01228000
*                                                                  0388 01229000
*                 /***************************************************/ 01230000
*                 /*                                                 */ 01231000
*                 /* SEE IF ANY ROUTE CODES SKIPPED IN SERIES        */ 01232000
*                 /*                                                 */ 01233000
*                 /***************************************************/ 01234000
*                                                                  0389 01235000
*                 IF SKIP='1'B      /* OF THE SERIES. IT IS IN       */ 01236000
*                   THEN            /* RCODS(I-1) SINCE THIS IS      */ 01237000
         TM    SKIP,B'00010000'                                    0389 01238000
         BNO   @RF00389                                            0389 01239000
*                   DO;             /* THE FIRST NON-MATCH FOUND     */ 01240000
*                     L=L-1;        /* BACK UP OVER COMMA            */ 01241000
         BCTR  L,0                                                 0391 01242000
*                     ROUTCDES(L)='-';/* AFTER SERIES. PUT           */ 01243000
         LR    @12,WPLPTR                                          0392 01244000
         ALR   @12,L                                               0392 01245000
         MVI   ROUTCDES-1(@12),C'-'                                0392 01246000
*                     ROUTCDES(L+1:L+2)=RCODS(I-1);/* DASH           */ 01247000
*                                                                  0393 01248000
         LR    @12,WPLPTR                                          0393 01249000
         ALR   @12,L                                               0393 01250000
         LR    @07,I                                               0393 01251000
         ALR   @07,@07                                             0393 01252000
         LA    @02,RCODS-4(@07)                                    0393 01253000
         MVC   ROUTCDES(2,@12),0(@02)                              0393 01254000
*                     /***********************************************/ 01255000
*                     /*                                             */ 01256000
*                     /* SEE IF PREVIOUS R/C WAS LESS THAN 10        */ 01257000
*                     /*                                             */ 01258000
*                     /***********************************************/ 01259000
*                                                                  0394 01260000
*                     IF I<11       /* PUT THE PREVIOUS R/C IN MSG   */ 01261000
*                       THEN        /* IF 1 DIGIT, INDEX UP BY 2     */ 01262000
         C     I,@CF02721                                          0394 01263000
         BNL   @RF00394                                            0394 01264000
*                       L=L+2;      /* TO SKIP DASH & DIGIT. IF      */ 01265000
         AL    L,@CF00080                                          0395 01266000
*                     ELSE          /* 2 DIGIT R/C, INDEX BY 3 TO    */ 01267000
*                       L=L+3;      /* SKIP DASH & 2 DIGITS          */ 01268000
         B     @RC00394                                            0396 01269000
@RF00394 AL    L,@CF00034                                          0396 01270000
*                     ROUTCDES(L)=',';/* SET COMM AFTER R/C          */ 01271000
@RC00394 LR    @12,WPLPTR                                          0397 01272000
         ALR   @12,L                                               0397 01273000
         MVI   ROUTCDES-1(@12),C','                                0397 01274000
*                     L=L+1;        /* INDEX UP BY 1 PAST THE        */ 01275000
         AL    L,@CF00068                                          0398 01276000
*                   END;            /* COMMA JUST SET                */ 01277000
*                 RSAVE=0;          /* SIGNAL END OF SERIES          */ 01278000
@RF00389 SLR   RSAVE,RSAVE                                         0400 01279000
*               END;                /* END END-OF-SERIES PROCESS.    */ 01280000
*           END;                    /* END CURRENT BIT NOT ON PROC   */ 01281000
@RF00387 DS    0H                                                  0403 01282000
*       END;                        /* END SEARCH LOOP AFTER 16      */ 01283000
@RC00371 AL    I,@CF00068                                          0403 01284000
         C     I,@CF00041                                          0403 01285000
         BNH   @DL00370                                            0403 01286000
*   ROUTCDES(L-1)=' ';              /* BLANK OUT EXTRA LAST COMMA    */ 01287000
@RC00368 DS    0H                                                  0404 01288000
@RC00366 LR    @12,WPLPTR                                          0404 01289000
         ALR   @12,L                                               0404 01290000
         MVI   ROUTCDES-2(@12),C' '                                0404 01291000
*   RETURN;                         /* RETURN TO CALLER              */ 01292000
@EL00008 DS    0H                                                  0405 01293000
@EF00008 DS    0H                                                  0405 01294000
@ER00008 LM    @14,@03,12(@13)                                     0405 01295000
         LM    @05,@12,40(@13)                                     0405 01296000
         BR    @14                                                 0405 01297000
*   END RCODES;                     /* END ROUTE CODE PROCESS. RTN   */ 01298000
         EJECT                                                          01299000
*                                                                  0407 01300000
*/********************************************************************/ 01301000
*/*                                                                  */ 01302000
*/*  PROCEDURE - HARDCOPY                                            */ 01303000
*/*                                                                  */ 01304000
*/*  IF HCUCM IS NOT ZERO THEN IT WILL BE THE INDEX OF THE DATA-SAVE-*/ 01305000
*/*  ENTRY FOR THE HARDCOPY CONSOLE(OUTPUT HALF IF ITS A COMPOSITE). */ 01306000
*/*                                                                  */ 01307000
*/*  IF HCUCM IS ZERO, THEN IF UCMSYSG IS ON, HARDCOPY IS TO SYSLOG. */ 01308000
*/*  IF UCMSYSG IS NOT ON, THEN THERE IS NO HARDCOPY IN THE SYSTEM.  */ 01309000
*/*                                                                  */ 01310000
*/*  INPUTS - HCUCM, AND THE ARRAY OF CONSOLE DATA-SAVE-AREAS.       */ 01311000
*/*                                                                  */ 01312000
*/*  OUTPUTS - IF HARDCOPY IS NOT IN THE SYSTEM, THEN THIS ROUTINE   */ 01313000
*/*            HAS NO OUTPUTS. HC. INDICATOR WILL BE OFF.            */ 01314000
*/*                                                                  */ 01315000
*/*            IF HARDCOPY IS TO SYSLOG, THE UNIT NAME IN THE MESSAGE*/ 01316000
*/*            WILL BE 'SYSLOG'. THE AUTHORITY PORTION OF THE MESSAGE*/ 01317000
*/*            WILL BE ONE OF THE FOLLOWING: INCMDS,STCMDS,CMDS, OR  */ 01318000
*/*            NOCMDS. THE CONDITION PART OF THE MESSAGE WILL BE 'H'.*/ 01319000
*/*                                                                  */ 01320000
*/*            IF HARDCOPY IS TO A CONSOLE, THE UNIT NAME(S) OF THE  */ 01321000
*/*            CONSOLE,ITS COMPOSITE (IF ANY), ITS ALTERNATE (IF ANY)*/ 01322000
*/*            AND THE ALTERNATE'S COMPOSITE (IF ANY). THESE ARE     */ 01323000
*/*            FILLED IN BY CALLING THE UNITNAME SUBROUTINE. OTHER   */ 01324000
*/*            PARTS OF THE MESSAGE WILL BE FILLED IN WITH THE SAME  */ 01325000
*/*            VALUES STATED ABOVE FOR HARDCOPY TO SYSLOG.           */ 01326000
*/*                                     --                           */ 01327000
*/*  CALLS - UNITNAME -TO PROCESS UNIT ADDRESSE(S) OF THE CONSOLE.   */ 01328000
*/*          TOEBCIDC - TO CONVERT A 4 DIGIT VALUE TO EBCDIC @ZA11392*/ 01329000
*/*                     AND PUT RESULT IN A MESSAGE.         @ZA11392*/ 01330000
*/*                                                                  */ 01331000
*/********************************************************************/ 01332000
*                                                                  0407 01333000
*HARDCOPY:                                                         0407 01334000
*   PROC;                                                          0407 01335000
*                                                                  0407 01336000
HARDCOPY STM   @14,@03,@SA00009                                    0407 01337000
         STM   @05,@12,@SA00009+24                                 0407 01338000
*   /*****************************************************************/ 01339000
*   /*                                                               */ 01340000
*   /* CHECK TO SEE IF HARDCOPY IS TO A CONSOLE                      */ 01341000
*   /*                                                               */ 01342000
*   /*****************************************************************/ 01343000
*                                                                  0408 01344000
*   IF HCUCM^=0                     /* IF HARDCOPY TO A CONSOLE      */ 01345000
*     THEN                          /* THEN SET HC. MSG INDICATOR.   */ 01346000
         LTR   HCUCM,HCUCM                                         0408 01347000
         BZ    @RF00408                                            0408 01348000
*     DO;                           /* NEXT CHECK FOR COMPOSITE,     */ 01349000
*       HDCOPY='1'B;                /* AFTER SETTING INDEX INTO      */ 01350000
         OI    HDCOPY,B'01000000'                                  0410 01351000
*       INDEX=HCUCM;                /* ARRAY OF DATA-SAVE AREAS.     */ 01352000
         LR    INDEX,HCUCM                                         0411 01353000
*       CALL TOEBCDIC(WQECNT(INDEX),NWQES);/* CONVERT NUMBER OF WQE'S   01354000
*                                      TO EBCDIC AND PUT IN MSG    0412 01355000
*                                                            @ZA11392*/ 01356000
*                                                                  0412 01357000
         LR    @12,INDEX                                           0412 01358000
         MH    @12,@CH00077                                        0412 01359000
         AL    @12,@CF02817                                        0412 01360000
         LA    @12,WQECNT-2(@12,CONSPTR)                           0412 01361000
         ST    @12,@AL00001                                        0412 01362000
         LA    @12,NWQES(,WPLPTR)                                  0412 01363000
         ST    @12,@AL00001+4                                      0412 01364000
         LA    @01,@AL00001                                        0412 01365000
         BAL   @14,TOEBCDIC                                        0412 01366000
*       /*************************************************************/ 01367000
*       /*                                                           */ 01368000
*       /* CHECK FOR COMPOSITE CONSOLE                               */ 01369000
*       /*                                                           */ 01370000
*       /*************************************************************/ 01371000
*                                                                  0413 01372000
*       IF COMPC(INDEX)^=0          /* IF COMPOSITE, THEN THIS IS    */ 01373000
*         THEN                      /* THE OUTPUT HALF. GET THE      */ 01374000
         LR    @12,INDEX                                           0413 01375000
         MH    @12,@CH00077                                        0413 01376000
         AL    @12,@CF02815                                        0413 01377000
         LH    @12,COMPC-14(@12,CONSPTR)                           0413 01378000
         LTR   @12,@12                                             0413 01379000
         BZ    @RF00413                                            0413 01380000
*         INDEX=COMPC(INDEX);       /* INDEX TO INPUT HALF.          */ 01381000
         LR    INDEX,@12                                           0414 01382000
*       CALL UNITNAME;              /* PROCESS DEVICE NAMES.         */ 01383000
@RF00413 BAL   @14,UNITNAME                                        0415 01384000
*       CONDITN='H    ';            /* FILL IN HCPY CONDITION.       */ 01385000
         MVC   CONDITN(5,WPLPTR),@CC02726                          0416 01386000
*     END;                          /* END HC. TO CONSOLE PROCESS.   */ 01387000
*   ELSE                            /* IF HC. NOT TO A CONSOLE       */ 01388000
*                                                                  0418 01389000
*     /***************************************************************/ 01390000
*     /*                                                             */ 01391000
*     /* SEE IF HARDCOPY IS TO SYSLOG                                */ 01392000
*     /*                                                             */ 01393000
*     /***************************************************************/ 01394000
*                                                                  0418 01395000
*     IF UCMSYSG='1'B               /* THEN IF ITS TO SYSLOG         */ 01396000
*       THEN                        /* THEN FIRST FILL IN THE        */ 01397000
         B     @RC00408                                            0418 01398000
@RF00408 L     @12,UCMMCSPT                                        0418 01399000
         TM    UCMSYSG(@12),B'00000010'                            0418 01400000
         BNO   @RF00418                                            0418 01401000
*       DO;                         /* UNIT NAME FIELD WITH THE      */ 01402000
*         CONSOLE='SYSLOG';         /* WORD 'SYSLOG'. SIGNAL @ZA11392*/ 01403000
         MVI   CONSOLE+6(WPLPTR),C' '                              0420 01404000
         MVC   CONSOLE(6,WPLPTR),@CC02727                          0420 01405000
*         HDCOPY='1'B;              /* HC. MSG. BEING BUILT.         */ 01406000
         OI    HDCOPY,B'01000000'                                  0421 01407000
*         CONDITN='H    ';          /* FILL IN HC. CONDITION.        */ 01408000
         MVC   CONDITN(5,WPLPTR),@CC02726                          0422 01409000
*         CALL TOEBCDIC(HCWQECNT,NWQES);/* CONVERT NUMBER OF WQE'S TO   01410000
*                                      EBCDIC AND PUT IN MSG @ZA11392*/ 01411000
         LA    @12,HCWQECNT                                        0423 01412000
         ST    @12,@AL00001                                        0423 01413000
         LA    @12,NWQES(,WPLPTR)                                  0423 01414000
         ST    @12,@AL00001+4                                      0423 01415000
         LA    @01,@AL00001                                        0423 01416000
         BAL   @14,TOEBCDIC                                        0423 01417000
*       END;                        /* END HC. TO SYSLOG PROCESS.    */ 01418000
*                                                                  0424 01419000
*   /*****************************************************************/ 01420000
*   /*                                                               */ 01421000
*   /* CHECK TO SEE IF THIS IS THE HARDCOPY MESSAGE BEING BUILT      */ 01422000
*   /*                                                               */ 01423000
*   /*****************************************************************/ 01424000
*                                                                  0425 01425000
*   IF HDCOPY='1'B                  /* IF HC. WAS IN SYSTEM          */ 01426000
*     THEN                          /* THEN CHECK AUTHORITY PARMS.   */ 01427000
*                                                                  0425 01428000
@RF00418 DS    0H                                                  0425 01429000
@RC00408 TM    HDCOPY,B'01000000'                                  0425 01430000
         BNO   @RF00425                                            0425 01431000
*     /***************************************************************/ 01432000
*     /*                                                             */ 01433000
*     /* CHECK FOR INCMDS TO HARDCOPY                                */ 01434000
*     /*                                                             */ 01435000
*     /***************************************************************/ 01436000
*                                                                  0426 01437000
*     IF UCMSDS1B='1'B              /* IF INCMDS TO HARDCOPY         */ 01438000
*       THEN                        /* THEN MOVE 'INCMDS' TO         */ 01439000
         L     @12,UCMMCSPT                                        0426 01440000
         TM    UCMSDS1B(@12),B'01000000'                           0426 01441000
         BNO   @RF00426                                            0426 01442000
*       AUTH='INCMDS';              /* THE AUTH PART OF MESSAGE      */ 01443000
         MVI   AUTH+6(WPLPTR),C' '                                 0427 01444000
         MVI   AUTH+7(WPLPTR),C' '                                 0427 01445000
         MVC   AUTH(6,WPLPTR),@CC02728                             0427 01446000
*     ELSE                          /* ELSE IF STCMDS TO             */ 01447000
*                                                                  0428 01448000
*       /*************************************************************/ 01449000
*       /*                                                           */ 01450000
*       /* CHECK FOR STCMDS TO HARDCOPY                              */ 01451000
*       /*                                                           */ 01452000
*       /*************************************************************/ 01453000
*                                                                  0428 01454000
*       IF UCMSDS1A='1'B            /* HARDCOPY THEN MOVE            */ 01455000
*         THEN                      /* 'STCMDS' TO AUTH PART OF      */ 01456000
         B     @RC00426                                            0428 01457000
@RF00426 L     @12,UCMMCSPT                                        0428 01458000
         TM    UCMSDS1A(@12),B'10000000'                           0428 01459000
         BNO   @RF00428                                            0428 01460000
*         AUTH='STCMDS';            /* MESSAGE.                      */ 01461000
         MVI   AUTH+6(WPLPTR),C' '                                 0429 01462000
         MVI   AUTH+7(WPLPTR),C' '                                 0429 01463000
         MVC   AUTH(6,WPLPTR),@CC02729                             0429 01464000
*       ELSE                        /* ELSE IF CMDS TO HARDCOPY      */ 01465000
*                                                                  0430 01466000
*         /***********************************************************/ 01467000
*         /*                                                         */ 01468000
*         /* CHECK FOR CMDS TO HARDCOPY                              */ 01469000
*         /*                                                         */ 01470000
*         /***********************************************************/ 01471000
*                                                                  0430 01472000
*         IF UCMSYSC='1'B           /* THEN MOVE 'CMDS'              */ 01473000
*           THEN                    /* TO THE AUTH PART OF THE       */ 01474000
         B     @RC00428                                            0430 01475000
@RF00428 L     @12,UCMMCSPT                                        0430 01476000
         TM    UCMSYSC(@12),B'00100000'                            0430 01477000
         BNO   @RF00430                                            0430 01478000
*           AUTH=' CMDS ';          /* MESSAGE.                      */ 01479000
         MVI   AUTH+6(WPLPTR),C' '                                 0431 01480000
         MVI   AUTH+7(WPLPTR),C' '                                 0431 01481000
         MVC   AUTH(6,WPLPTR),@CC02730                             0431 01482000
*         ELSE                      /* ELSE IF NONE OF THE ABOVE,    */ 01483000
*           AUTH='NOCMDS';          /* MOVE 'NOCMDS' TO MESSAGE      */ 01484000
         B     @RC00430                                            0432 01485000
@RF00430 MVI   AUTH+6(WPLPTR),C' '                                 0432 01486000
         MVI   AUTH+7(WPLPTR),C' '                                 0432 01487000
         MVC   AUTH(6,WPLPTR),@CC02731                             0432 01488000
*   END HARDCOPY;                   /* END HC. MSG. ROUTINE.         */ 01489000
@EL00009 DS    0H                                                  0433 01490000
@EF00009 DS    0H                                                  0433 01491000
@ER00009 LM    @14,@03,@SA00009                                    0433 01492000
         LM    @05,@12,@SA00009+24                                 0433 01493000
         BR    @14                                                 0433 01494000
         EJECT                                                          01495000
*                                                                  0434 01496000
*/********************************************************************/ 01497000
*/*                                                                  */ 01498000
*/*  PROCEDURE - SETLOCK                                             */ 01499000
*/*                                                                  */ 01500000
*/*  FUNCTION - OBTAIN LOCAL AND CMS LOCKS TO SERIALIZE USE OF UCM,  */ 01501000
*/*             UCB'S,DCM'S.                                         */ 01502000
*/*                                                                  */ 01503000
*/*  NOTES - SETLOCK MACRO DESTROYS R11-R14. R13 WILL BE SAVED AND   */ 01504000
*/*          RESTORED.                                               */ 01505000
*/*                                                                  */ 01506000
*/*  INPUTS - NONE.                                                  */ 01507000
*/*                                                                  */ 01508000
*/*  OUTPUTS - NONE.                                                 */ 01509000
*/*                                                                  */ 01510000
*/********************************************************************/ 01511000
*                                                                  0434 01512000
*SETLOCK:                                                          0434 01513000
*   PROC OPTIONS(NOSAVEAREA);                                      0434 01514000
SETLOCK  STM   @14,@12,12(@13)                                     0434 01515000
*   RFY                                                            0435 01516000
*    (R0,                                                          0435 01517000
*     R13) RSTD;                    /* USE R0 TO SAVE R13    @ZA11392*/ 01518000
*   R0=R13;                         /* SAVE IT.                Y02751*/ 01519000
         LR    R0,R13                                              0436 01520000
*   GEN REFS(PSALITA,FLC) SETS(R11,R12,R13,R14);                   0437 01521000
*                                                     /*       Y02751*/ 01522000
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                       **01523000
               RELATED=(UCM,IEEXEDNA(FREELOCK))                         01524000
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                          *01525000
               RELATED=(UCM,IEEXEDNA(FREELOCK))                         01526000
*   R13=R0;                         /* RESTORE R13             Y02751*/ 01527000
         LR    R13,R0                                              0438 01528000
*   RFY                                                            0439 01529000
*    (R0,                                                          0439 01530000
*     R13) UNRSTD;                  /* FREE R0 FOR USE       @ZA11392*/ 01531000
*   RETURN;                         /* RETURN TO CALLER        Y02751*/ 01532000
@EL00010 DS    0H                                                  0440 01533000
@EF00010 DS    0H                                                  0440 01534000
@ER00010 LM    @14,@12,12(@13)                                     0440 01535000
         BR    @14                                                 0440 01536000
*   END SETLOCK;                    /* END OBTAIN LOCKS RTN    Y02751*/ 01537000
         EJECT                                                          01538000
*                                                                  0442 01539000
*/********************************************************************/ 01540000
*/*                                                                  */ 01541000
*/*  PROCEDURE - FREELOCK                                            */ 01542000
*/*                                                                  */ 01543000
*/*  FUNCTION - RELEASE LOCAL AND CMS LOCKS TO SERIALIZE USE OF UCM, */ 01544000
*/*             UCB'S,DCM'S.                                         */ 01545000
*/*                                                                  */ 01546000
*/*  NOTES - SETLOCK MACRO DESTROYS R11-R14. R13 WILL BE SAVED AND   */ 01547000
*/*          RESTORED.                                               */ 01548000
*/*                                                                  */ 01549000
*/*  INPUTS - NONE.                                                  */ 01550000
*/*                                                                  */ 01551000
*/*  OUTPUTS - NONE.                                                 */ 01552000
*/*                                                                  */ 01553000
*/********************************************************************/ 01554000
*                                                                  0442 01555000
*FREELOCK:                                                         0442 01556000
*   PROC OPTIONS(NOSAVEAREA);                                      0442 01557000
FREELOCK STM   @14,@12,12(@13)                                     0442 01558000
*   RFY                                                            0443 01559000
*    (R0,                                                          0443 01560000
*     R13) RSTD;                    /* USE R0 TO SAVE R13    @ZA11392*/ 01561000
*   R0=R13;                         /* SAVE IT.                Y02751*/ 01562000
         LR    R0,R13                                              0444 01563000
*   GEN REFS(PSALITA,FLC) SETS(R11,R12,R13,R14);                   0445 01564000
*                                                     /*       Y02751*/ 01565000
         SETLOCK RELEASE,TYPE=CMS,                                     *01566000
               RELATED=(UCM,IEEXEDNA(SETLOCK))                          01567000
         SETLOCK RELEASE,TYPE=LOCAL,                                   *01568000
               RELATED=(UCM,IEEXEDNA(SETLOCK))                          01569000
*   R13=R0;                         /* RESTORE R13             Y02751*/ 01570000
         LR    R13,R0                                              0446 01571000
*   RFY                                                            0447 01572000
*    (R0,                                                          0447 01573000
*     R13) UNRSTD;                  /* FREE R0 FOR USE       @ZA11392*/ 01574000
*   RETURN;                         /* RETURN TO CALLER        Y02751*/ 01575000
@EL00011 DS    0H                                                  0448 01576000
@EF00011 DS    0H                                                  0448 01577000
@ER00011 LM    @14,@12,12(@13)                                     0448 01578000
         BR    @14                                                 0448 01579000
*   END FREELOCK;                   /* END OBTAIN LOCKS RTN    Y02751*/ 01580000
         EJECT                                                          01581000
*                                                                  0450 01582000
*/********************************************************************/ 01583000
*/*                                                                  */ 01584000
*/*  PROCEDURE - CONSID                                              */ 01585000
*/*                                                                  */ 01586000
*/*  FUNCTION - CHANGE CONSOLE ID TO EBCDIC & FILL IT INTO MESSAGE.  */ 01587000
*/*                                                                  */ 01588000
*/*  INPUTS - ARRAY OF CONSOLE DATA-SAVE-ENTRIED, AND INDEX TO THE   */ 01589000
*/*             ENTRY CURRENTLY BEING PROCESSED.                     */ 01590000
*/*                                                                  */ 01591000
*/*  OUTPUTS - CONSOLE ID IN THE ID COLUMN OF THE MESSAGE.           */ 01592000
*/*                                                                  */ 01593000
*/*  CALLS - NONE.                                                   */ 01594000
*/*                                                                  */ 01595000
*/********************************************************************/ 01596000
*                                                                  0450 01597000
*CONSID:                                                           0450 01598000
*   PROC OPTIONS(NOSAVEAREA);       /* CONSOLE ID ROUTINE            */ 01599000
CONSID   STM   @14,@03,12(@13)                                     0450 01600000
         STM   @05,@12,40(@13)                                     0450 01601000
*   INDEXSVE=INDEX;                 /* SAVE CURRENT INDEX            */ 01602000
*                                                                  0451 01603000
         LR    INDEXSVE,INDEX                                      0451 01604000
*   /*****************************************************************/ 01605000
*   /*                                                               */ 01606000
*   /* CHECK TO SEE IF THIS IS A COMPOSITE CONSOLE                   */ 01607000
*   /*                                                               */ 01608000
*   /*****************************************************************/ 01609000
*                                                                  0452 01610000
*   IF COMPC(INDEX)^=0              /* IF COMPOSITE, WE ASSUME IT    */ 01611000
*     THEN                          /* IS INPUT HALF SINCE TOPSEG  0452 01612000
*                                      SKIPS OUTPUT ONLY COMPC CON   */ 01613000
         LR    @12,INDEX                                           0452 01614000
         MH    @12,@CH00077                                        0452 01615000
         AL    @12,@CF02815                                        0452 01616000
         LH    @12,COMPC-14(@12,CONSPTR)                           0452 01617000
         LTR   @12,@12                                             0452 01618000
         BZ    @RF00452                                            0452 01619000
*     INDEX=COMPC(INDEX);           /* SET INDEX TO OUTPUT HALF.     */ 01620000
         LR    INDEX,@12                                           0453 01621000
*   RFY                                                            0454 01622000
*     R0 RSTD;                      /*                       @ZA11392*/ 01623000
@RF00452 DS    0H                                                  0455 01624000
*   R0=IDCON(INDEX);                /* PUT ID IN REG 0 FOR CVD     0455 01625000
*                                                            @ZA11392*/ 01626000
         LR    @12,INDEX                                           0455 01627000
         MH    @12,@CH00077                                        0455 01628000
         AL    @12,@CF02821                                        0455 01629000
         LH    R0,IDCON(@12,CONSPTR)                               0455 01630000
*   GEN(CVD  0,UNPACK);             /* CONVERT TO PACKED DECIMAL     */ 01631000
         CVD  0,UNPACK                                                  01632000
*   RFY                                                            0457 01633000
*     R0 UNRSTD;                    /*                       @ZA11392*/ 01634000
*   GEN(UNPK UNPACK(3),UNPACK+6(2));/* CONV. TO EBCDIC               */ 01635000
         UNPK UNPACK(3),UNPACK+6(2)                                     01636000
*   GEN(MVZ UNPACK+2(1),UNPACK+1);  /* FIX ZONE                      */ 01637000
         MVZ UNPACK+2(1),UNPACK+1                                       01638000
*   IDMSG=UNPACK(2:3);              /* MOVE ID TO MESSAGE            */ 01639000
         MVC   IDMSG(2,WPLPTR),UNPACK+1                            0460 01640000
*   INDEX=INDEXSVE;                 /* RESTORE ORIGINAL INDEX        */ 01641000
         LR    INDEX,INDEXSVE                                      0461 01642000
*   RETURN;                         /* RETURN TO CALLER              */ 01643000
@EL00012 DS    0H                                                  0462 01644000
@EF00012 DS    0H                                                  0462 01645000
@ER00012 LM    @14,@03,12(@13)                                     0462 01646000
         LM    @05,@12,40(@13)                                     0462 01647000
         BR    @14                                                 0462 01648000
*   END CONSID;                     /* END CONSOLE ID ROUTINE.       */ 01649000
         EJECT                                                          01650000
*                                                                  0464 01651000
*/********************************************************************/ 01652000
*/*                                                                  */ 01653000
*/*  PROCEDURE - MSGCOND.                                            */ 01654000
*/*                                                                  */ 01655000
*/*  FUNCTION - FILL IN CONSOLE CONDITION COL. OF MESSAGE.           */ 01656000
*/*                                                                  */ 01657000
*/*  INPUTS - CONSOLE DATA-SAVE-ENTRIES,AND INDEX OF CURRENT ENTRY.  */ 01658000
*/*                                                                  */ 01659000
*/*  OUTPUTS - CONDITION COL. OF MESSAGE. CAN BE:                    */ 01660000
*/*                                                                  */ 01661000
*/*     (1) M - MASTER CONSOLE.                                      */ 01662000
*/*     (2) A - ACTIVE, NOT MASTER.                                  */ 01663000
*/*     (3) N - INACTIVE, NOT MASTER.                                */ 01664000
*/*     (4) S - SUBSYSTEM CONSOLE                            @Y02668P*/ 01665000
*/*                                                                  */ 01666000
*/*     (5) ,P - STATUS PENDING. IE. A,P = PENDING TO BECOME ACTIVE  */ 01667000
*/*     (6) ,T - REQUESTING JOBNAMES & TIME STAMPS BEFORE MESSAGES.  */ 01668000
*/*     (7) ,J - REQUESTING ONLY JOBNAMES BEFORE MESSAGES.           */ 01669000
*/*                                                                  */ 01670000
*/*  CALLS - NONE.                                                   */ 01671000
*/*                                                                  */ 01672000
*/********************************************************************/ 01673000
*                                                                  0464 01674000
*MSGCOND:                                                          0464 01675000
*   PROC OPTIONS(NOSAVEAREA);       /* CONSOLE CONDITION ROUTINE     */ 01676000
MSGCOND  STM   @14,@12,12(@13)                                     0464 01677000
*   L=1;                            /* SET COL. INDEX TO BEGIN       */ 01678000
*                                                                  0465 01679000
         LA    @12,1                                               0465 01680000
         LR    L,@12                                               0465 01681000
*   /*****************************************************************/ 01682000
*   /*                                                               */ 01683000
*   /* CHECK TO SEE IF THIS IS NOT THE MASTER CONSOLE                */ 01684000
*   /*                                                               */ 01685000
*   /*****************************************************************/ 01686000
*                                                                  0466 01687000
*   IF DISPA(INDEX)^='1'B           /* IF NOT MASTER CONSOLE, THEN   */ 01688000
*     THEN                          /* ASSUME ACTIVE CONSOLE. PUT    */ 01689000
         LR    @02,INDEX                                           0466 01690000
         MH    @02,@CH00077                                        0466 01691000
         ST    @02,@TF00001                                        0466 01692000
         ALR   @02,CONSPTR                                         0466 01693000
         AL    @02,@CF02825                                        0466 01694000
         TM    DISPA-8(@02),B'10000000'                            0466 01695000
         BO    @RF00466                                            0466 01696000
*     DO;                           /* 'A' IN FIRST COLUMN.          */ 01697000
*                                                                  0467 01698000
*       /*************************************************************/ 01699000
*       /*                                                           */ 01700000
*       /* CHECK TO SEE IF IT IS A SUBSYSTEM CONSOLE                 */ 01701000
*       /*                                                           */ 01702000
*       /*************************************************************/ 01703000
*                                                                  0468 01704000
*       CONDITN(1)='A';             /* SET ACTIVE                    */ 01705000
         MVI   CONDITN(WPLPTR),C'A'                                0468 01706000
*       L=L+1;                      /* UPDATE COLUMN INDEX.          */ 01707000
*                                                                  0469 01708000
         ALR   L,@12                                               0469 01709000
*       /*************************************************************/ 01710000
*       /*                                                           */ 01711000
*       /* SEE IF THE CONSOLE IS NOT CURRENTLY ACTIVE                */ 01712000
*       /*                                                           */ 01713000
*       /*************************************************************/ 01714000
*                                                                  0470 01715000
*       IF UF(INDEX)^='1'B          /* IF IT IS NOT ACTIVE           */ 01716000
*         THEN                      /* THEN CHECK TO SEE IF IT IS    */ 01717000
*                                                                  0470 01718000
         LR    @12,CONSPTR                                         0470 01719000
         AL    @12,@TF00001                                        0470 01720000
         AL    @12,@CF02816                                        0470 01721000
         TM    UF-5(@12),B'00010000'                               0470 01722000
         BO    @RF00470                                            0470 01723000
*         /***********************************************************/ 01724000
*         /*                                                         */ 01725000
*         /* CHECK TO SEE IF STATUS TO CHANGE                        */ 01726000
*         /*                                                         */ 01727000
*         /***********************************************************/ 01728000
*                                                                  0471 01729000
*         IF DADI(INDEX)='1'B       /* PENDING TO BECOME ACTIVE.     */ 01730000
*           THEN                    /* IF SO, THEN WE                */ 01731000
         LR    @12,CONSPTR                                         0471 01732000
         AL    @12,@TF00001                                        0471 01733000
         AL    @12,@CF02828                                        0471 01734000
         TM    DADI-19(@12),B'00000001'                            0471 01735000
         BNO   @RF00471                                            0471 01736000
*           DO;                     /* MUST PUT IN A ',P' TO         */ 01737000
*             CONDITN(L:L+1)=',P';  /* SHOW PENDING.                 */ 01738000
         LR    @12,WPLPTR                                          0473 01739000
         ALR   @12,L                                               0473 01740000
         MVC   CONDITN-1(2,@12),@CC02745                           0473 01741000
*             L=L+2;                /* UPDATE COLUMN INDEX           */ 01742000
         AL    L,@CF00080                                          0474 01743000
*           END;                    /* END A,P PROCESSING.           */ 01744000
*         ELSE                      /* IF NOT PENDING ACTIVE THEN    */ 01745000
*           IF DISPK(INDEX)='1'B THEN/* IF SUBSYSTEM CONSOLE @Y02668P*/ 01746000
         B     @RC00471                                            0476 01747000
@RF00471 LR    @12,INDEX                                           0476 01748000
         MH    @12,@CH00077                                        0476 01749000
         ALR   @12,CONSPTR                                         0476 01750000
         AL    @12,@CF02830                                        0476 01751000
         TM    DISPK-9(@12),B'00100000'                            0476 01752000
         BNO   @RF00476                                            0476 01753000
*             DO;                   /* THEN THE CONDITION    @Y02668P*/ 01754000
*               CONDITN(1)='S';     /* WILL BE 'S'           @Y02668P*/ 01755000
         MVI   CONDITN(WPLPTR),C'S'                                0478 01756000
*               L=L+1;              /* UPDATE COLUMN INDEX   @Y02668P*/ 01757000
         AL    L,@CF00068                                          0479 01758000
*             END;                  /*                       @Y02668P*/ 01759000
*           ELSE                    /* SYSTEM CONSOLE        @Y02668P*/ 01760000
*             DO;                   /* WE PUT IN AN 'N' TO MEAN      */ 01761000
         B     @RC00476                                            0481 01762000
@RF00476 DS    0H                                                  0482 01763000
*               CONDITN(1)='N';     /* INACTIVE CONSOLE              */ 01764000
         MVI   CONDITN(WPLPTR),C'N'                                0482 01765000
*               L=2;                /* SET COLUMN INDEX TO TWO       */ 01766000
         LA    L,2                                                 0483 01767000
*             END;                  /* END INACTIVE CONS PROC        */ 01768000
*       ELSE                        /* IF IT IS ACTIVE, THEN WE      */ 01769000
*                                                                  0485 01770000
*         /***********************************************************/ 01771000
*         /*                                                         */ 01772000
*         /* CHECK TO SEE IF STATUS IS CHANGING                      */ 01773000
*         /*                                                         */ 01774000
*         /***********************************************************/ 01775000
*                                                                  0485 01776000
*         IF DADI(INDEX)='1'B       /* ALREADY HAVE THE 'A' IN       */ 01777000
*           THEN                    /* THE TEXT. BUT IF PENDING      */ 01778000
         B     @RC00470                                            0485 01779000
@RF00470 LR    @12,INDEX                                           0485 01780000
         MH    @12,@CH00077                                        0485 01781000
         ALR   @12,CONSPTR                                         0485 01782000
         AL    @12,@CF02828                                        0485 01783000
         TM    DADI-19(@12),B'00000001'                            0485 01784000
         BNO   @RF00485                                            0485 01785000
*           DO;                     /* CHANGE BIT IS ON, IT IS       */ 01786000
*             CONDITN(1:3)='N,P';   /* PEND. INACT.                  */ 01787000
         MVC   CONDITN(3,WPLPTR),@CC02748                          0487 01788000
*             L=4;                  /* SET COL. INDEX AFTER 'N,P'    */ 01789000
         LA    L,4                                                 0488 01790000
*           END;                    /* END 'N,P' CONS. PROCESSING    */ 01791000
*     END;                          /* END ACTIVE CONS. PROCESSING   */ 01792000
*   ELSE                            /* IF IT IS THE MASTER CONSOLE   */ 01793000
*     DO;                           /* THEN WE MOVE AN 'M' INTO      */ 01794000
         B     @RC00466                                            0491 01795000
@RF00466 DS    0H                                                  0492 01796000
*       CONDITN(1)='M';             /* THE MSG.                      */ 01797000
         MVI   CONDITN(WPLPTR),C'M'                                0492 01798000
*       L=2;                        /* SET COL. INDEX AFTER 'M'.     */ 01799000
         LA    L,2                                                 0493 01800000
*     END;                          /* END MASTER CONS. PROCESSING   */ 01801000
*                                                                  0494 01802000
*   /*****************************************************************/ 01803000
*   /*                                                               */ 01804000
*   /* CHECK FOR REQUESTING TIMESTAMPS & JOBNAMES                    */ 01805000
*   /*                                                               */ 01806000
*   /*****************************************************************/ 01807000
*                                                                  0495 01808000
*   IF DISPI(INDEX)='1'B            /* IF REQ. JBNMS & TIME STMP     */ 01809000
*     THEN                          /* THEN ADD ',T' TO MSG.         */ 01810000
@RC00466 LR    @12,INDEX                                           0495 01811000
         MH    @12,@CH00077                                        0495 01812000
         ALR   @12,CONSPTR                                         0495 01813000
         AL    @12,@CF02830                                        0495 01814000
         TM    DISPI-9(@12),B'10000000'                            0495 01815000
         BNO   @RF00495                                            0495 01816000
*     CONDITN(L:L+1)=',T';          /* TO SHOW IT.                   */ 01817000
         LR    @12,WPLPTR                                          0496 01818000
         ALR   @12,L                                               0496 01819000
         MVC   CONDITN-1(2,@12),@CC02750                           0496 01820000
*   ELSE                            /* ELSE IF                       */ 01821000
*                                                                  0497 01822000
*     /***************************************************************/ 01823000
*     /*                                                             */ 01824000
*     /* CHECK FOR REQUESTING ONLY JOBNAMES                          */ 01825000
*     /*                                                             */ 01826000
*     /***************************************************************/ 01827000
*                                                                  0497 01828000
*     IF DISPJ(INDEX)='1'B          /* REQ. ONLY JOBNAMES            */ 01829000
*       THEN                        /* THEN ADD ',J' TO MSG.         */ 01830000
         B     @RC00495                                            0497 01831000
@RF00495 LR    @12,INDEX                                           0497 01832000
         MH    @12,@CH00077                                        0497 01833000
         ALR   @12,CONSPTR                                         0497 01834000
         AL    @12,@CF02830                                        0497 01835000
         TM    DISPJ-9(@12),B'01000000'                            0497 01836000
         BNO   @RF00497                                            0497 01837000
*       CONDITN(L:L+1)=',J';        /* TO SHOW IT.                   */ 01838000
         LR    @12,WPLPTR                                          0498 01839000
         ALR   @12,L                                               0498 01840000
         MVC   CONDITN-1(2,@12),@CC02751                           0498 01841000
*     ELSE                                                         0499 01842000
*       ;                           /* ELSE LEAVE MSG ALONE          */ 01843000
@RF00497 DS    0H                                                  0500 01844000
*   END MSGCOND;                    /* END CONSOLE COND. ROUTINE     */ 01845000
@EL00013 DS    0H                                                  0500 01846000
@EF00013 DS    0H                                                  0500 01847000
@ER00013 LM    @14,@12,12(@13)                                     0500 01848000
         BR    @14                                                 0500 01849000
         EJECT                                                          01850000
*                                                                  0501 01851000
*/********************************************************************/ 01852000
*/*                                                                  */ 01853000
*/*  PROCEDURE - AUTHCHK                                             */ 01854000
*/*                                                                  */ 01855000
*/*  FUNCTION - CHECK CONSOLE COMMAND AUTHORITY AND PUT IT IN MSG.   */ 01856000
*/*                                                                  */ 01857000
*/*  INPUTS - CONSOLE-DATA-SAVE-ENTRIES AND INDEX TO CURRENT ONE.    */ 01858000
*/*                                                                  */ 01859000
*/*  OUTPUTS - CONSOLE AUTHORITY COLUMN OF MESSAGE AS FOLLOWS:       */ 01860000
*/*                                                                  */ 01861000
*/*            POSSIBLE VALUES:                                      */ 01862000
*/*               ONE OF: ALL,NONE,SYS,IO,CONS.                      */ 01863000
*/*               TWO OF: SYS,IO,CONS.                               */ 01864000
*/*                                                                  */ 01865000
*/*  CALLS - NONE.                                                   */ 01866000
*/*                                                                  */ 01867000
*/********************************************************************/ 01868000
*                                                                  0501 01869000
*AUTHCHK:                                                          0501 01870000
*   PROC OPTIONS(NOSAVEAREA);       /* AUTHORITY CHECK ROUTINE       */ 01871000
AUTHCHK  STM   @14,@12,12(@13)                                     0501 01872000
*   L=1;                            /* SET COLUMN INDEX TO ONE       */ 01873000
*                                                                  0502 01874000
         LA    L,1                                                 0502 01875000
*   /*****************************************************************/ 01876000
*   /*                                                               */ 01877000
*   /* SEE IF THE CONSOLE HAS INPUT CAPABILITY                       */ 01878000
*   /*                                                               */ 01879000
*   /*****************************************************************/ 01880000
*                                                                  0503 01881000
*   IF MIF(INDEX)='1'B              /* IF CONS. HAS INPUT ABILITY    */ 01882000
*     THEN                          /* THEN CHECK FOR AUTH           */ 01883000
*                                                                  0503 01884000
         LR    @12,INDEX                                           0503 01885000
         MH    @12,@CH00077                                        0503 01886000
         ST    @12,@TF00001                                        0503 01887000
         ALR   @12,CONSPTR                                         0503 01888000
         AL    @12,@CF02816                                        0503 01889000
         TM    MIF-5(@12),B'01000000'                              0503 01890000
         BNO   @RF00503                                            0503 01891000
*     /***************************************************************/ 01892000
*     /*                                                             */ 01893000
*     /* CHECK TO SEE IF AUTH = ALL                                  */ 01894000
*     /*                                                             */ 01895000
*     /***************************************************************/ 01896000
*                                                                  0504 01897000
*     IF AUTHA(INDEX)>='E0'X        /* = ALL. IF ALL                 */ 01898000
*       THEN                        /* THEN                          */ 01899000
         LR    @12,CONSPTR                                         0504 01900000
         AL    @12,@TF00001                                        0504 01901000
         AL    @12,@CF02823                                        0504 01902000
         CLI   AUTHA-6(@12),X'E0'                                  0504 01903000
         BL    @RF00504                                            0504 01904000
*       AUTH='  ALL';               /* PUT 'ALL' INTO MESSAGE.       */ 01905000
         MVI   AUTH+5(WPLPTR),C' '                                 0505 01906000
         MVC   AUTH+6(2,WPLPTR),AUTH+5(WPLPTR)                     0505 01907000
         MVC   AUTH(5,WPLPTR),@CC02756                             0505 01908000
*     ELSE                          /* IF NOT ALL                    */ 01909000
*                                                                  0506 01910000
*       /*************************************************************/ 01911000
*       /*                                                           */ 01912000
*       /* SEE IF THERE IS NO AUTH SPECIFIED                         */ 01913000
*       /*                                                           */ 01914000
*       /*************************************************************/ 01915000
*                                                                  0506 01916000
*       IF AUTHA(INDEX)=0           /* IF NO AUTH                    */ 01917000
*         THEN                      /* THEN SIGNAL                   */ 01918000
         B     @RC00504                                            0506 01919000
@RF00504 LR    @12,INDEX                                           0506 01920000
         MH    @12,@CH00077                                        0506 01921000
         AL    @12,@CF02823                                        0506 01922000
         SLR   @02,@02                                             0506 01923000
         IC    @02,AUTHA-6(@12,CONSPTR)                            0506 01924000
         C     @02,@CF00044                                        0506 01925000
         BNE   @RF00506                                            0506 01926000
*         AUTH='  INFO';            /* INFO ONLY                     */ 01927000
         MVI   AUTH+6(WPLPTR),C' '                                 0507 01928000
         MVI   AUTH+7(WPLPTR),C' '                                 0507 01929000
         MVC   AUTH(6,WPLPTR),@CC02757                             0507 01930000
*       ELSE                        /* OTHERWISE SCAN AUTH CODES     */ 01931000
*         DO;                       /* THEN CHECK EACH CODE    SEPAR-*/ 01932000
         B     @RC00506                                            0508 01933000
@RF00506 DS    0H                                                  0509 01934000
*           FRSTPARM='0'B;          /* SET INDICATOR OFF.            */ 01935000
*                                                                  0509 01936000
         NI    FRSTPARM,B'01111111'                                0509 01937000
*           /*********************************************************/ 01938000
*           /*                                                       */ 01939000
*           /* SEE IF 'SYS' WAS SPECIFIED                            */ 01940000
*           /*                                                       */ 01941000
*           /*********************************************************/ 01942000
*                                                                  0510 01943000
*           IF AUTH1(INDEX)='1'B    /* IF FIRST ONE ON               */ 01944000
*             THEN                  /* THEN                          */ 01945000
         LR    @12,INDEX                                           0510 01946000
         MH    @12,@CH00077                                        0510 01947000
         ALR   @12,CONSPTR                                         0510 01948000
         AL    @12,@CF02823                                        0510 01949000
         TM    AUTH1-6(@12),B'10000000'                            0510 01950000
         BNO   @RF00510                                            0510 01951000
*             DO;                   /* MOVE 'SYS' INTO THE NEXT      */ 01952000
*               AUTH(L:L+2)='SYS';  /* AVAILABLE COLS.               */ 01953000
         LR    @12,WPLPTR                                          0512 01954000
         ALR   @12,L                                               0512 01955000
         MVC   AUTH-1(3,@12),@CC02758                              0512 01956000
*               L=4;                /* SET COL. INDEX PAST 'SYS'     */ 01957000
         LA    L,4                                                 0513 01958000
*               FRSTPARM='1'B;      /* SIGNAL 1ST PARM WAS           */ 01959000
         OI    FRSTPARM,B'10000000'                                0514 01960000
*             END;                  /* FOUND. END SYS PROCESSING.    */ 01961000
*                                                                  0515 01962000
*           /*********************************************************/ 01963000
*           /*                                                       */ 01964000
*           /* CHECK TO SEE IF 'IO' WAS SPECIFIED                    */ 01965000
*           /*                                                       */ 01966000
*           /*********************************************************/ 01967000
*                                                                  0516 01968000
*           IF AUTH2(INDEX)='1'B    /* IF 2ND BIT ON                 */ 01969000
*             THEN                  /* THEN                          */ 01970000
@RF00510 LR    @12,INDEX                                           0516 01971000
         MH    @12,@CH00077                                        0516 01972000
         ALR   @12,CONSPTR                                         0516 01973000
         AL    @12,@CF02823                                        0516 01974000
         TM    AUTH2-6(@12),B'01000000'                            0516 01975000
         BNO   @RF00516                                            0516 01976000
*             DO;                   /* SEE IF THIS WILL BE 1ST       */ 01977000
*                                                                  0517 01978000
*               /*****************************************************/ 01979000
*               /*                                                   */ 01980000
*               /* SEE IF IO IS FIRST PARAMETER                      */ 01981000
*               /*                                                   */ 01982000
*               /*****************************************************/ 01983000
*                                                                  0518 01984000
*               IF FRSTPARM='0'B    /* PARM. IF SO                   */ 01985000
*                 THEN              /* THEN SIGNAL                   */ 01986000
         TM    FRSTPARM,B'10000000'                                0518 01987000
         BNZ   @RF00518                                            0518 01988000
*                 FRSTPARM='1'B;    /* FIRST PARM END.               */ 01989000
         OI    FRSTPARM,B'10000000'                                0519 01990000
*               ELSE                /* IF NOT FIRST PARM             */ 01991000
*                 DO;               /* THEN SEPARATE THIS PARM       */ 01992000
         B     @RC00518                                            0520 01993000
@RF00518 DS    0H                                                  0521 01994000
*                   AUTH(L)=',';    /* WITH COMMA                    */ 01995000
         LR    @12,WPLPTR                                          0521 01996000
         ALR   @12,L                                               0521 01997000
         MVI   AUTH-1(@12),C','                                    0521 01998000
*                   L=L+1;          /* UP INDEX BY COMMA             */ 01999000
         AL    L,@CF00068                                          0522 02000000
*                 END;              /* END NOT FIRST PARM PROC.      */ 02001000
*               AUTH(L:L+1)='IO';   /* MOVE IO IN NEXT               */ 02002000
@RC00518 LR    @12,WPLPTR                                          0524 02003000
         ALR   @12,L                                               0524 02004000
         MVC   AUTH-1(2,@12),@CC02759                              0524 02005000
*               L=L+2;              /* SLOT. UP INDEX PAST IO        */ 02006000
         AL    L,@CF00080                                          0525 02007000
*             END;                  /* END IO PROCESSING.            */ 02008000
*                                                                  0526 02009000
*           /*********************************************************/ 02010000
*           /*                                                       */ 02011000
*           /* SEE IF CONS WAS SPECIFIED                             */ 02012000
*           /*                                                       */ 02013000
*           /*********************************************************/ 02014000
*                                                                  0527 02015000
*           IF AUTH3(INDEX)='1'B    /* IF 3RD BIT ON                 */ 02016000
*             THEN                  /* THEN                          */ 02017000
@RF00516 LR    @12,INDEX                                           0527 02018000
         MH    @12,@CH00077                                        0527 02019000
         ALR   @12,CONSPTR                                         0527 02020000
         AL    @12,@CF02823                                        0527 02021000
         TM    AUTH3-6(@12),B'00100000'                            0527 02022000
         BNO   @RF00527                                            0527 02023000
*             DO;                   /* CHECK FOR FIRST PARM. IF      */ 02024000
*                                                                  0528 02025000
*               /*****************************************************/ 02026000
*               /*                                                   */ 02027000
*               /* SEE IF CONS IS FIRST PARAMETER                    */ 02028000
*               /*                                                   */ 02029000
*               /*****************************************************/ 02030000
*                                                                  0529 02031000
*               IF FRSTPARM='1'B    /* NOT THEN                      */ 02032000
*                 THEN              /* SEPARATE THE TWO PARMS        */ 02033000
         TM    FRSTPARM,B'10000000'                                0529 02034000
         BNO   @RF00529                                            0529 02035000
*                 DO;               /* BY A COMMA IN THE NEXT        */ 02036000
*                   AUTH(L)=',';    /* AVAIL. COL.                   */ 02037000
         LR    @12,WPLPTR                                          0531 02038000
         ALR   @12,L                                               0531 02039000
         MVI   AUTH-1(@12),C','                                    0531 02040000
*                   L=L+1;          /* UP INDEX BY COMM              */ 02041000
         AL    L,@CF00068                                          0532 02042000
*                 END;              /* END NOT 1ST PARM PROC-        */ 02043000
*               AUTH(L:L+3)='CONS'; /* MOVE 'CONS'                   */ 02044000
@RF00529 LR    @12,WPLPTR                                          0534 02045000
         ALR   @12,L                                               0534 02046000
         MVC   AUTH-1(4,@12),@CC02760                              0534 02047000
*             END;                  /* IN NEXT SLOT. END CONS PROC   */ 02048000
*         END;                      /* END AUTH ^= ALL PROCESSING    */ 02049000
*   ELSE                            /* IF NO INPUT CAPABILITY        */ 02050000
*     AUTH(1:6)='  NONE';           /* THEN SET NO COMMAND AUTH.     */ 02051000
         B     @RC00503                                            0537 02052000
@RF00503 MVC   AUTH(6,WPLPTR),@CC02761                             0537 02053000
*   END AUTHCHK;                    /* END AUTHORITY CHK ROUTINE.    */ 02054000
@EL00014 DS    0H                                                  0538 02055000
