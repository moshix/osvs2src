*/********************************************************************/ 00442000
*/*                                                                  */ 00443000
*/*  PROCEDURE NAME - TITLEBLD                                       */ 00444000
*/*                                                                  */ 00445000
*/*  FUNCTION - BUILD CONTROL LINE FOR THE CONSOLE DISPLAY           */ 00446000
*/*                                                                  */ 00447000
*/*  INPUTS - WPLPTR,  POINTING TO BEGINNING OF THE WTO PARAMETER    */ 00448000
*/*           LIST.                                                  */ 00449000
*/*                                                                  */ 00450000
*/*  OUTPUTS - CONTROL LINE BUILT IN WPL WITH TIME STAMP.            */ 00451000
*/*                                                                  */ 00452000
*/*  MACROS USED - TIME                                              */ 00453000
*/*                                                                  */ 00454000
*/********************************************************************/ 00455000
*                                                                  0235 00456000
*TITLEBLD:                                                         0235 00457000
*   PROC OPTIONS(NOSAVEAREA);       /* BUILD CONTROL LINE RTN.       */ 00458000
TITLEBLD STM   @14,@10,12(@13)                                     0235 00459000
         ST    @12,68(,@13)                                        0235 00460000
*   WPLMCSFA='1'B;                  /* ROUT/DESC CODES EXIST         */ 00461000
*   WPLMCSFB='1'B;                  /* REG0 -Q TO CONS IF ACTIVE     */ 00462000
*   WPLMCSFC='1'B;                  /* COMMAND RESPONSE INDIC        */ 00463000
         OI    WPLMCSFA(WPLPTR),B'11100000'                        0238 00464000
*   WPLMCSFJ='1'B;                  /* TURN ON MLWTO INDICATOR       */ 00465000
         OI    WPLMCSFJ(WPLPTR),B'01000000'                        0239 00466000
*   MCSFSAVE=WPLMCSF;               /* SAVE MCS FLAGS                */ 00467000
*                                                                  0240 00468000
         LH    @12,WPLMCSF(,WPLPTR)                                0240 00469000
         N     @12,@CF02819                                        0240 00470000
         STH   @12,MCSFSAVE                                        0240 00471000
*   /*****************************************************************/ 00472000
*   /*                                                               */ 00473000
*   /* GEN A STORE CLOCK INSTRUCTION TO SEE IF THE CLOCK IS WORKING. */ 00474000
*   /* IF A ZERO RETURN CODE, THEN ITS OKAY TO GO AHEAD AND ISSUE THE*/ 00475000
*   /* TIME MACRO. IF NOT, MOVE IN TEXT WITHOUT THE TIME STAMP.      */ 00476000
*   /*                                                               */ 00477000
*   /*****************************************************************/ 00478000
*                                                                  0241 00479000
*   GEN(STCK CONVERT);              /* IS THE CLOCK WORKING ?        */ 00480000
         STCK CONVERT                                                   00481000
*   BC(8,TIMEON);                   /* YES, GO GET TIME              */ 00482000
         BC    8,TIMEON                                            0242 00483000
*   WPLTXT(1:LENGTH(CTEXTNOT))=CTEXTNOT;/* FILL IN SHORT TEXT        */ 00484000
         MVC   WPLTXT(23,WPLPTR),CTEXTNOT                          0243 00485000
*   WPLLGH=LENGTH(CTEXTNOT)+4;      /* SET CNTL LINE LENGTH          */ 00486000
         MVC   WPLLGH(2,WPLPTR),@CH02803                           0244 00487000
*TITLEND:                                                          0245 00488000
*   WPLPTR=WPLPTR+WPLLGH;           /* UPDATE PAST TEXT IN WPL       */ 00489000
TITLEND  AH    WPLPTR,WPLLGH(,WPLPTR)                              0245 00490000
*   WPLDESCE='1'B;                  /* DESC. CODE 5                  */ 00491000
*   WPLDESCH='1'B;                  /* DESC. CODE 8                  */ 00492000
         OI    WPLDESCE(WPLPTR),B'00001001'                        0247 00493000
*   WPLDESCI='1'B;                  /* DESC. CODE 9                  */ 00494000
         OI    WPLDESCI(WPLPTR),B'10000000'                        0248 00495000
*   DESCSAVE=WPLDESC;               /* SAVE THE DESCRIPTOR FLAGS     */ 00496000
         MVC   DESCSAVE(2),WPLDESC(WPLPTR)                         0249 00497000
*   WPLROUT=0;                      /* ZERO ROUTE CODES              */ 00498000
         SLR   @12,@12                                             0250 00499000
         STCM  @12,3,WPLROUT(WPLPTR)                               0250 00500000
*   WPLPTR=WPLPTR+4;                /* UPDATE TO NEXT WPL SECTION    */ 00501000
         LA    @12,4                                               0251 00502000
         ALR   WPLPTR,@12                                          0251 00503000
*   WPLLTFA='1'B;                   /* LINE TYPE = CONTROL LINE      */ 00504000
         OI    WPLLTFA(WPLPTR),B'10000000'                         0252 00505000
*   WPLAREA=TARGAREA;               /* SET AREA ID-ISSUING CONSOLE   */ 00506000
         MVC   WPLAREA(1,WPLPTR),TARGAREA                          0253 00507000
*   LINEBASE=ADDR(WPLLINES);        /* SAVE PTR TO # OF LINES FLD    */ 00508000
         LA    @08,WPLLINES(,WPLPTR)                               0254 00509000
         ST    @08,LINEBASE                                        0254 00510000
*   WPLPTR=WPLPTR+4;                /* UPDATE TO NEXT WPL SECTION    */ 00511000
         ALR   WPLPTR,@12                                          0255 00512000
*   RETURN;                         /* RETURN TO CALLER              */ 00513000
@EL00004 DS    0H                                                  0256 00514000
@EF00004 DS    0H                                                  0256 00515000
@ER00004 LM    @14,@10,12(@13)                                     0256 00516000
         L     @12,68(,@13)                                        0256 00517000
         BR    @14                                                 0256 00518000
*   RFY                                                            0257 00519000
*     R0 RSTD;                      /*                       @ZA11392*/ 00520000
*TIMEON:                                                           0258 00521000
*   GEN(TIME DEC);                  /* GET THE TIME                  */ 00522000
TIMEON   TIME DEC                                                       00523000
*   CONVERT1=R0;                    /* SAVE TIME FROM REG0           */ 00524000
         LA    @12,CONVERT                                         0259 00525000
         ST    R0,CONVERT1(,@12)                                   0259 00526000
*   RFY                                                            0260 00527000
*     R0 UNRSTD;                    /*                       @ZA11392*/ 00528000
*   GEN(UNPK UNPACK(7),CONVERT(4)); /* CONV. TO EBCDIC               */ 00529000
         UNPK UNPACK(7),CONVERT(4)                                      00530000
*   WPLTXT(1:LENGTH(CNTLTEXT))=CNTLTEXT;/* CONTROL LINE TEXT         */ 00531000
         MVC   WPLTXT(32,WPLPTR),CNTLTEXT                          0262 00532000
*   WPLTXT(9:10)=UNPACK(1:2);       /* SET HOURS IN TIME STAMP       */ 00533000
         MVC   WPLTXT+8(2,WPLPTR),UNPACK                           0263 00534000
*   WPLTXT(12:13)=UNPACK(3:4);      /* SET MINUTES IN STAMP          */ 00535000
         MVC   WPLTXT+11(2,WPLPTR),UNPACK+2                        0264 00536000
*   WPLTXT(15:16)=UNPACK(5:6);      /* SET SECONDS IN TIME STAMP     */ 00537000
         MVC   WPLTXT+14(2,WPLPTR),UNPACK+4                        0265 00538000
*   WPLLGH=LENGTH(CNTLTEXT)+4;      /* SET CNTL LINE LENGTH          */ 00539000
         MVC   WPLLGH(2,WPLPTR),@CH02804                           0266 00540000
*   GOTO TITLEND;                   /* GO FINISH BUILDING LINE       */ 00541000
         B     TITLEND                                             0267 00542000
*   END TITLEBLD;                   /* END CONTROL LINE BUILD RTN    */ 00543000
         EJECT                                                          00544000
*                                                                  0269 00545000
*/********************************************************************/ 00546000
*/*                                                                  */ 00547000
*/*  PROCEDURE NAME - LABELBLD                                       */ 00548000
*/*                                                                  */ 00549000
*/*  FUNCTION - BUILDS LABEL LINES FOR THE CONSOLE DISPLAY   @ZA11392*/ 00550000
*/*                                                                  */ 00551000
