*                                                                  0611 02402000
*/********************************************************************/ 02403000
*/*                                                          @ZA11392*/ 02404000
*/* PROCDEURE NAME: IEEXEDNA.TOEBCDIC                        @ZA11392*/ 02405000
*/*                                                          @ZA11392*/ 02406000
*/* FUNCTION: CONVERTS A NUMBER TO EBCDIC AND CHANGES        @ZA11392*/ 02407000
*/*           LEADING ZEROS TO BLANKS. THE RESULT IS THEN    @ZA11392*/ 02408000
*/*           PLACED IN THE GIVEN AREA.                      @ZA11392*/ 02409000
*/*                                                          @ZA11392*/ 02410000
*/* INPUTS:   1. VALUE - NUMBER TO CONVERT TO EBCDIC         @ZA11392*/ 02411000
*/*           2. AREA  - PLACE TO PUT EBCDIC NUMBER(4 DIGITS)@ZA11392*/ 02412000
*/*                                                          @ZA11392*/ 02413000
*/* OUTPUTS:  AREA WILL CONTAIN THE 4 DIGIT NUMBER OR '****' @ZA11392*/ 02414000
*/*           ASTERISKS WILL APPEAR IF NUMBER TO BE          @ZA11392*/ 02415000
*/*           CONVERTED CONTAINS MORE THAN 4 DECMIAL DIGITS. @ZA11392*/ 02416000
*/*                                                          @ZA11392*/ 02417000
*/* CALLS:    NONE                                           @ZA11392*/ 02418000
*/*                                                          @ZA11392*/ 02419000
*/********************************************************************/ 02420000
*                                                                  0611 02421000
*TOEBCDIC:                                                         0611 02422000
*   PROC(VALUE,AREA) OPTIONS(NOSAVEAREA,NOSAVE);/*           @ZA11392*/ 02423000
*                                                                  0611 02424000
TOEBCDIC MVC   @PC00017(8),0(@01)                                  0611 02425000
*   /*****************************************************************/ 02426000
*   /*                                                               */ 02427000
*   /* DECLARES FOR THIS PROC                                @ZA11392*/ 02428000
*   /*                                                               */ 02429000
*   /*****************************************************************/ 02430000
*                                                                  0612 02431000
*   DCL                                                            0612 02432000
*     VALUE FIXED(15);              /* NUMBER TO CONVERT INTO EBCDIC    02433000
*                                                            @ZA11392*/ 02434000
*   DCL                                                            0613 02435000
*     AREA CHAR(4);                 /* PLACE TO PUT EBCDIC NUMBER  0613 02436000
*                                                            @ZA11392*/ 02437000
*   DCL                                                            0614 02438000
*     TEMPAREA CHAR(4);             /* TEMPORARY PLACE TO HOLD VALUE    02439000
*                                                            @ZA11392*/ 02440000
*   DCL                                                            0615 02441000
*     J FIXED(15);                  /* LOOP CONTROL VARIABLE @ZA11392*/ 02442000
*   DCL                                                            0616 02443000
*     ASTRIC4 CHAR(4) CONSTANT('****');/* USED IF VALUE HAS MORE THAN   02444000
*                                      4 DECMIAL DIGITS      @ZA11392*/ 02445000
*   DCL                                                            0617 02446000
*     NINE999 FIXED(31) CONSTANT(9999);/* LARGEST 4 DIGIT NUMBER   0617 02447000
*                                                            @ZA11392*/ 02448000
*   DCL                                                            0618 02449000
*     FOXZERO CHAR(1) CONSTANT('0');/* CHAR ZERO             @ZA11392*/ 02450000
*                                                                  0618 02451000
*   /*****************************************************************/ 02452000
*   /*                                                               */ 02453000
*   /* IS VALUE GREATER THAN 4 DIGIT DECMIAL NUMBER          @ZA11392*/ 02454000
*   /*                                                               */ 02455000
*   /*****************************************************************/ 02456000
*                                                                  0619 02457000
*   IF VALUE>NINE999 THEN           /*                       @ZA11392*/ 02458000
         L     @12,@PC00017                                        0619 02459000
         LH    @12,VALUE(,@12)                                     0619 02460000
         C     @12,@CF02783                                        0619 02461000
         BNH   @RF00619                                            0619 02462000
*     AREA=ASTRIC4;                 /* YES, FILL WITH ****   @ZA11392*/ 02463000
         L     @12,@PC00017+4                                      0620 02464000
         MVC   AREA(4,@12),@CC02781                                0620 02465000
*   ELSE                            /* NO                    @ZA11392*/ 02466000
*     DO;                           /*                       @ZA11392*/ 02467000
         B     @RC00619                                            0621 02468000
@RF00619 DS    0H                                                  0622 02469000
*       RFY                                                        0622 02470000
*         R0 RSTD;                  /*                       @ZA11392*/ 02471000
*       R0=VALUE;                   /* PUT NUMBER TO CONVERT IN R0 0623 02472000
*                                                            @ZA11392*/ 02473000
         L     @12,@PC00017                                        0623 02474000
         LH    R0,VALUE(,@12)                                      0623 02475000
*       CVD(R0,UNPACK);             /* CONVERT NUMBER TO DECMIAL   0624 02476000
*                                                            @ZA11392*/ 02477000
         CVD   R0,UNPACK                                           0624 02478000
*       UNPK(TEMPAREA,UNPACK(5:8)); /* CONVERT NUMBER TO EBCDIC    0625 02479000
*                                                            @ZA11392*/ 02480000
         UNPK  TEMPAREA(4),UNPACK+4(4)                             0625 02481000
*       GEN SETS(TEMPAREA)(OI    TEMPAREA+3,X'F0');/* MAKE LAST DIGIT   02482000
*                                      PRINTABLE             @ZA11392*/ 02483000
*                                                                  0626 02484000
         OI    TEMPAREA+3,X'F0'                                         02485000
*       /*************************************************************/ 02486000
*       /*                                                           */ 02487000
*       /* MAKE LEADING ZERO'S BLANK                         @ZA11392*/ 02488000
*       /*                                                           */ 02489000
*       /*************************************************************/ 02490000
*                                                                  0627 02491000
*       DO J=1 TO 4 BY 1 WHILE TEMPAREA(J)=FOXZERO;/* IF LEADING ZERO   02492000
*                                      THEN REPLACE WITH A BLANK   0627 02493000
*                                                            @ZA11392*/ 02494000
         LA    J,1                                                 0627 02495000
@DL00627 LA    @12,TEMPAREA-1(J)                                   0627 02496000
         CLI   0(@12),C'0'                                         0627 02497000
         BNE   @DC00627                                            0627 02498000
*         TEMPAREA(J)=' ';          /*                       @ZA11392*/ 02499000
         LA    @12,TEMPAREA-1(J)                                   0628 02500000
         MVI   0(@12),C' '                                         0628 02501000
*       END;                        /*                       @ZA11392*/ 02502000
         AL    J,@CF00068                                          0629 02503000
         C     J,@CF00060                                          0629 02504000
         BNH   @DL00627                                            0629 02505000
@DC00627 DS    0H                                                  0630 02506000
*       AREA=TEMPAREA;              /* PUT VALUE IN MESSAGE  @ZA11392*/ 02507000
         L     @12,@PC00017+4                                      0630 02508000
         MVC   AREA(4,@12),TEMPAREA                                0630 02509000
*       RFY                                                        0631 02510000
*         R0 UNRSTD;                /*                       @ZA11392*/ 02511000
*     END;                          /*                       @ZA11392*/ 02512000
*   END TOEBCDIC;                   /*                       @ZA11392*/ 02513000
