         BZ    @RT00116                                            0116 00075000
*     GOTO SETIMER;                 /* AT SETIMER.                   */ 00076000
*VALRQCK:                           /*                               */ 00077000
*   IF DERQ='1'B|                   /* WAITDE OR WAITANY             */ 00078000
*       ANYRQ='1'B|                 /* OR                            */ 00079000
*       WAITRQ='0'B&                /* NONE OF VALID                 */ 00080000
