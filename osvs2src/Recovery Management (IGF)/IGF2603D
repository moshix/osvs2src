*   RESPECIFY                                                      0606 02800000
*     I RSTD;                       /* RESTRICT LOOP CONTROL REGISTER*/ 02801000
*                                                                  0606 02802000
*   /*****************************************************************/ 02803000
*   /*                                                               */ 02804000
*   /* INITIALLY SET RETURN CODE TO 4 AND SET THE DECIMAL TO BINARY  */ 02805000
*   /* CONVERSION WORKAREA TO ZERO                                   */ 02806000
*   /*                                                               */ 02807000
*   /*****************************************************************/ 02808000
*                                                                  0607 02809000
*   RETCODE=4;                                                     0607 02810000
         MVC   RETCODE(2),@CH00038                                 0607 02811000
*   WORKAREA=WORKAREA&&WORKAREA;                                   0608 02812000
*                                                                  0608 02813000
         XC    WORKAREA(8),WORKAREA                                0608 02814000
*   /*****************************************************************/ 02815000
*   /*                                                               */ 02816000
*   /* SET MAX LENGTH OF PARM                                @G55CP2P*/ 02817000
*   /*                                                               */ 02818000
*   /*****************************************************************/ 02819000
*                                                                  0609 02820000
*   IF PARMTYPE=INTVPARM            /*                       @G55CP2P*/ 02821000
*     THEN                                                         0609 02822000
