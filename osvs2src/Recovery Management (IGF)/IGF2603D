*                                                                  0294 00938000
         B     @DE00294                                            0294 00939000
@DL00294 DS    0H                                                  0295 00940000
*         /***********************************************************/ 00941000
*         /*                                                         */ 00942000
*         /* SET LOOP CONTROL SWITCH TO FORCE LOOP TO TERMINATE IF AN*/ 00943000
*         /* ERROR IS ENCOUNTERED                                    */ 00944000
*         /*                                                         */ 00945000
*         /***********************************************************/ 00946000
*                                                                  0295 00947000
*         ENDBIT=BITON;                                            0295 00948000
*                                                                  0295 00949000
         OI    ENDBIT,B'10000000'                                  0295 00950000
*         /***********************************************************/ 00951000
*         /*                                                         */ 00952000
*         /* CALL FINDEL SUBROUTINE TO CHECK FIRST (NEXT) VERBS      */ 00953000
*         /*                                                         */ 00954000
*         /***********************************************************/ 00955000
*                                                                  0296 00956000
*         CALL FINDEL;                                             0296 00957000
*                                                                  0296 00958000
         BAL   @14,FINDEL                                          0296 00959000
*         /***********************************************************/ 00960000
*         /*                                                         */ 00961000
*         /* IF RETURN CODE FROM FINDEL SUBROUTINE = 4 THEN INVOKE   */ 00962000
*         /* WTO958I SUBROUTINE TO INFORM THE OPERATOR THAT AN       */ 00963000
*         /* INVALID VERB WAS ISSUED, SET THE RETURN CODE = 20 AND   */ 00964000
*         /* RETURN TO CALLER                                        */ 00965000
*         /*                                                         */ 00966000
*         /***********************************************************/ 00967000
*                                                                  0297 00968000
*         IF RETCODE=4 THEN                                        0297 00969000
         CLC   RETCODE(2),@CH00038                                 0297 00970000
         BNE   @RF00297                                            0297 00971000
