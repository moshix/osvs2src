*           PACK(WORKAREA,WORKAREA);/* CHANGE WORKAREA TO PACKED   0619 02910000
*                                      DECIMAL                       */ 02911000
         PACK  WORKAREA(8),WORKAREA(8)                             0619 02912000
*           CVB(WKREG1,WORKAREA);   /* CONVERT DECIMAL TO BINARY     */ 02913000
*                                                                  0620 02914000
         CVB   WKREG1,WORKAREA                                     0620 02915000
*           /*********************************************************/ 02916000
*           /*                                                       */ 02917000
*           /* IF NUMBER JUST CONVERTED IS GREATER THAN ZERO PLACE   */ 02918000
*           /* AMOUNT IN IGF955I MESSAGE BUFFER, INCREMENT THE VERB  */ 02919000
*           /* POINTER BY THE VALUE CONTAINED IN I, STORE THE VALUE  */ 02920000
*           /* IN BINARY IN THE PROPER ELEMENT OF VALUES, AND SET THE*/ 02921000
*           /* RETURN CODE = TO ZERO.                                */ 02922000
