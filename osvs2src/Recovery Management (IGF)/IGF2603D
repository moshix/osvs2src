         LTR   @10,@10                                             0277 00886000
         BNZ   @RF00277                                            0277 00887000
*     DO;                                                          0278 00888000
*                                                                  0278 00889000
*       /*************************************************************/ 00890000
*       /*                                                           */ 00891000
*       /* DEFAULT IS STATUS. CALL MODESTAT SUBROUTINE, SET RETURN   */ 00892000
*       /* CODE TO 4 AND RETURN TO CALLER                            */ 00893000
*       /*                                                           */ 00894000
*       /*************************************************************/ 00895000
*                                                                  0279 00896000
*       CALL MODESTAT;                                             0279 00897000
         BAL   @14,MODESTAT                                        0279 00898000
*       RETCODE=4;                                                 0280 00899000
         MVC   RETCODE(2),@CH00038                                 0280 00900000
*     END;                                                         0281 00901000
*                                                                  0281 00902000
*   /*****************************************************************/ 00903000
*   /*                                                               */ 00904000
*   /* IF NOT A NULL COMMAND THEN INITIALIZE SWITCHES AND SYNTAX     */ 00905000
*   /* CHECK ALL VERBS SUPPLIED                                      */ 00906000
*   /*                                                               */ 00907000
*   /*****************************************************************/ 00908000
*                                                                  0282 00909000
*   ELSE                                                           0282 00910000
*     DO;                                                          0282 00911000
