         STH   @10,RETCODE                                         0635 02975000
*                   END;            /*                       @G55CP2P*/ 02976000
*                 ELSE              /*                       @G556P2P*/ 02977000
*                   IF PARMTYPE=REPTPARM THEN                      0637 02978000
         B     @RC00631                                            0637 02979000
@RF00631 CLC   PARMTYPE(2),@CH00362                                0637 02980000
         BNE   @RF00637                                            0637 02981000
*                     DO;           /*                       @G556P2P*/ 02982000
*                       VMSGPRM2='REPORT=';/*                @G556P2P*/ 02983000
         MVI   VMSGPRM2+7,C' '                                     0639 02984000
         MVC   VMSGPRM2+8(2),VMSGPRM2+7                            0639 02985000
         MVC   VMSGPRM2(7),@CC01738                                0639 02986000
*                       PRM2VAL=INPUTEXT(1:VARYCHAR);/*      @G556P2P*/ 02987000
         MVI   PRM2VAL+1,C' '                                      0640 02988000
         MVI   PRM2VAL+2,C' '                                      0640 02989000
