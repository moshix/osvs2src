         L     @01,TEXTPTR-1                                       0647 03021000
         LA    @01,0(,@01)                                         0647 03022000
         EX    @07,@SM02064                                        0647 03023000
*                         TEXTPTR=TEXTPTR+VARYCHAR;/*        @G556P2P*/ 03024000
         ALR   @01,@08                                             0648 03025000
         STCM  @01,7,TEXTPTR                                       0648 03026000
*                         RETCODE=0;/*                       @G556P2P*/ 03027000
         SLR   @10,@10                                             0649 03028000
         STH   @10,RETCODE                                         0649 03029000
*                       END;        /*                       @G556P2P*/ 03030000
*             END;                  /*                       @G55CP2P*/ 03031000
*           ELSE                                                   0652 03032000
*             ;                                                    0652 03033000
@RF00621 DS    0H                                                  0653 03034000
*         END;                                                     0653 03035000
*       ELSE                                                       0654 03036000
*         ;                                                        0654 03037000
@RF00616 DS    0H                                                  0655 03038000
*     END;                                                         0655 03039000
*   ELSE                                                           0656 03040000
*     ;                                                            0656 03041000
@RF00612 DS    0H                                                  0657 03042000
*   RESPECIFY                                                      0657 03043000
*    (WKREG1,                                                      0657 03044000
*     I) UNRSTD;                                                   0657 03045000
*   END VALDEC;                                                    0658 03046000
@EL00010 DS    0H                                                  0658 03047000
@EF00010 DS    0H                                                  0658 03048000
@ER00010 LM    @14,@12,12(@13)                                     0658 03049000
         BR    @14                                                 0658 03050000
@PB00010 DS    0H                                                  0659 03051000
*   GEN(EJECT);                                                    0659 03052000
         EJECT                                                          03053000
*                                                                  0660 03054000
*/*   VALCPU - SUBROUTINE OF MODE COMMAND PROCESSOR (IGF2603D)     0660 03055000
*                                                                  0660 03056000
*      THIS SUBROUTINE IS CALLED TO VERIFY THAT THE VARIABLE FIELD IN   03057000
*      THE CPU=X VERB SUPPLIED BY THE OPERATOR IN THE MODE COMMAND IS   03058000
*      VALID.  IF VALID THE VARIABLE FIELD IS CONVERTED TO BINARY.  IN  03059000
*      EITHER CASE A RETURN CODE IS SET AND CONTROL IS RETURNED TO THE  03060000
*      CALLER.  RETURN CODES ARE SET AS FOLLOWS:                   0660 03061000
*           0 - VALUE CONTAINED IN THE CPU=X VERB IS VALID         0660 03062000
*                AND HAS BEEN CONVERTED TO BINARY AND SAVED IN     0660 03063000
*                THE VARIABLE 'CPUID'                              0660 03064000
*            4 - THE CPU=X VERB CONTAINS MORE THAN FOUR OR LESS THAN    03065000
*                ONE CHARACTER OR ONE OR MORE OF THE CHARACTERS    0660 03066000
*                SPECIFIED IS NOT AN EBCDIC CHARACTER WITHIN THE   0660 03067000
*                OF 0 TO F.                                        0660 03068000
*            8 - THE CPU=X VERB CONTAINS THE ADDRESS OF A CPU NOT  0660 03069000
*                CURRENTLY ON LINE.                                0660 03070000
*                                                                  0660 03071000
*      THE EXTERNAL PROCEDURE CALLING THIS SUBROUTINE MUST:        0660 03072000
*         1. HAVE PROVIDED AN 18 WORD REGISTER SAVE AREA WHOSE ADDRESS  03073000
*            IS IN REGISTER 13 AT ENTRY TO THIS SUBROUTINE.        0660 03074000
*         2. HAVE SET THE VARIABLE 'TEXTPTR' TO POINT TO THE VARIABLE   03075000
*            TO BE CHECKED.                                        0660 03076000
*         3. HAVE PROVIDED AN 8 BYTE AREA KNOWN AS 'WORKAREA'.     0660 03077000
*         4. HAVE DECLARED THE VARIABLES KNOWN AS 'CPUID' AND      0660 03078000
*            'RETCODE'.                                            0660 03079000
*         5. HAVE A DSECT FOR CVT AND CSD.                         0660 03080000
*                                                                  0660 03081000
*                                                                  0660 03082000
*                                                                  0660 03083000
*                                                                    */ 03084000
*                                                                  0660 03085000
*VALCPU:                                                           0660 03086000
*   PROC;                                                          0660 03087000
*                                                                  0660 03088000
         B     @PB00011                                            0660 03089000
VALCPU   STM   @14,@12,@SA00011                                    0660 03090000
*   /*****************************************************************/ 03091000
*   /*                                                               */ 03092000
*   /* VALCPU INTERNAL DECLARES                                      */ 03093000
*   /*                                                               */ 03094000
*   /*****************************************************************/ 03095000
*                                                                  0661 03096000
*   DCL                                                            0661 03097000
*     CPUCHAR CHAR(4) BASED(TEXTPTR);/* CHARACTER(S) TO BE CHECKED BY   03098000
*                                      THIS SUBROUTINE               */ 03099000
*   DCL                                                            0662 03100000
*     WRKINDEX FIXED(31);           /* INDEX FOR WORKAREA            */ 03101000
*                                                                  0662 03102000
*   /*****************************************************************/ 03103000
*   /*                                                               */ 03104000
*   /* VALCPU INTERNAL REGISTER DECLARES                             */ 03105000
*   /*                                                               */ 03106000
*   /*****************************************************************/ 03107000
*                                                                  0663 03108000
*   DCL                                                            0663 03109000
*     WKREG2 FIXED(31) REG(6) RESTRICTED;/* WORKREG FOR GENERATED SLL*/ 03110000
*   RESPECIFY                                                      0664 03111000
*    (CPUALREG,                                                    0664 03112000
*     I) RSTD;                                                     0664 03113000
*                                                                  0664 03114000
*   /*****************************************************************/ 03115000
*   /*                                                               */ 03116000
*   /* INITIALLY SET THE RETURN CODE TO FOUR                         */ 03117000
*   /*                                                               */ 03118000
*   /*****************************************************************/ 03119000
*                                                                  0665 03120000
*   RETCODE=4;                                                     0665 03121000
*                                                                  0665 03122000
         MVC   RETCODE(2),@CH00038                                 0665 03123000
*   /*****************************************************************/ 03124000
*   /*                                                               */ 03125000
*   /* TEST TO DETERMINE IF NUMBER OF CHARACTERS PASSED IN THE CPU = */ 03126000
*   /* VERB IS BETWEEN 1 TO 4. IF SO CONTINUE TO PROCESS, IF NOT     */ 03127000
*   /* RETURN.                                                       */ 03128000
*   /*                                                               */ 03129000
*   /*****************************************************************/ 03130000
*                                                                  0666 03131000
*   IF CPIDLNGT>0&CPIDLNGT<5 THEN                                  0666 03132000
         CLI   CPIDLNGT,0                                          0666 03133000
         BNH   @RF00666                                            0666 03134000
         CLI   CPIDLNGT,5                                          0666 03135000
         BNL   @RF00666                                            0666 03136000
*     DO;                                                          0667 03137000
*                                                                  0667 03138000
*       /*************************************************************/ 03139000
*       /*                                                           */ 03140000
*       /* SET THE WORKAREA POINTER TO THE LAST BYTE IN THE WORKAREA */ 03141000
*       /* AND SET THE WORKAREA TO ZERO                              */ 03142000
*       /*                                                           */ 03143000
*       /*************************************************************/ 03144000
*                                                                  0668 03145000
*       WRKINDEX=LENGTH(WORKAREA);                                 0668 03146000
         LA    WRKINDEX,8                                          0668 03147000
*       WORKAREA=WORKAREA&&WORKAREA;                               0669 03148000
*                                                                  0669 03149000
         XC    WORKAREA(8),WORKAREA                                0669 03150000
*       /*************************************************************/ 03151000
*       /*                                                           */ 03152000
*       /* INPUT CHARACTERS WILL NOW BE MOVED TO THE WORKAREA (LAST  */ 03153000
*       /* CHARACTER IN INPUT STRING TO LAST CHARACTER IN THE WORK   */ 03154000
*       /* AREA) AND EBCDIC CHARACTERS FROM A - F WILL BE CONVERTED  */ 03155000
*       /* TO HEX 'FA' - 'FF'.                                       */ 03156000
*       /*                                                           */ 03157000
*       /*************************************************************/ 03158000
*                                                                  0670 03159000
*       DO I=CPIDLNGT TO 1 BY-1;                                   0670 03160000
         SLR   I,I                                                 0670 03161000
         IC    I,CPIDLNGT                                          0670 03162000
         B     @DE00670                                            0670 03163000
@DL00670 DS    0H                                                  0671 03164000
*         WORKAREA(WRKINDEX)=CPUCHAR(I);/* MOVE ONE CHARACTER TO   0671 03165000
*                                      WORKAREA                      */ 03166000
         LA    @07,WORKAREA-1(WRKINDEX)                            0671 03167000
         L     @01,TEXTPTR-1                                       0671 03168000
         ALR   @01,I                                               0671 03169000
         BCTR  @01,0                                               0671 03170000
         MVC   0(1,@07),CPUCHAR(@01)                               0671 03171000
*         IF WORKAREA(WRKINDEX)>='A'&WORKAREA(WRKINDEX)<='F' THEN  0672 03172000
         LA    @07,WORKAREA-1(WRKINDEX)                            0672 03173000
         CLI   0(@07),C'A'                                         0672 03174000
         BL    @RF00672                                            0672 03175000
         LA    @07,WORKAREA-1(WRKINDEX)                            0672 03176000
         CLI   0(@07),C'F'                                         0672 03177000
         BH    @RF00672                                            0672 03178000
*           WORKAREA(WRKINDEX)=WORKAREA(WRKINDEX)+HEXADDER;/* CONVERT   03179000
*                                      EBCDIC A - F TO 'FA'X - 'FF'X.   03180000
*                                      EBCDIC A('C1'X) +           0673 03181000
*                                      HEXADDER('39'X) EQUALS A HEX     03182000
*                                      'A'('FA'X)                    */ 03183000
         LA    @07,57                                              0673 03184000
         SLR   @01,@01                                             0673 03185000
         IC    @01,WORKAREA-1(WRKINDEX)                            0673 03186000
         ALR   @07,@01                                             0673 03187000
         STC   @07,WORKAREA-1(WRKINDEX)                            0673 03188000
*         ELSE                                                     0674 03189000
*           ;                                                      0674 03190000
*                                                                  0674 03191000
@RF00672 DS    0H                                                  0675 03192000
*         /***********************************************************/ 03193000
*         /*                                                         */ 03194000
*         /* IF VALUE JUST PLACED IN WORKAREA IS NOT EQUAL TO 'F0'X  */ 03195000
*         /* TO 'FF'X THE SET I EQUAL TO ZERO TO FORCE LOOP TO       */ 03196000
*         /* TERMINATE.                                              */ 03197000
*         /*                                                         */ 03198000
*         /***********************************************************/ 03199000
*                                                                  0675 03200000
*         IF(WORKAREA(WRKINDEX)&CHARMASK)^=CHARMASK THEN           0675 03201000
         LA    @07,WORKAREA-1(WRKINDEX)                            0675 03202000
         MVC   @TS00001(1),0(@07)                                  0675 03203000
         NI    @TS00001,X'F0'                                      0675 03204000
         CLI   @TS00001,X'F0'                                      0675 03205000
         BE    @RF00675                                            0675 03206000
*           I=0;                                                   0676 03207000
         SLR   I,I                                                 0676 03208000
*         ELSE                                                     0677 03209000
*           ;                                                      0677 03210000
*                                                                  0677 03211000
@RF00675 DS    0H                                                  0678 03212000
*         /***********************************************************/ 03213000
*         /*                                                         */ 03214000
*         /* DECREMENT THE INDEX TO THE WORKREG BY ONE.              */ 03215000
*         /*                                                         */ 03216000
*         /***********************************************************/ 03217000
*                                                                  0678 03218000
*         WRKINDEX=WRKINDEX-1;                                     0678 03219000
         BCTR  WRKINDEX,0                                          0678 03220000
*       END;                        /* END OF CONVERSION LOOP        */ 03221000
*                                                                  0679 03222000
         BCTR  I,0                                                 0679 03223000
@DE00670 LTR   I,I                                                 0679 03224000
         BP    @DL00670                                            0679 03225000
*       /*************************************************************/ 03226000
*       /*                                                           */ 03227000
*       /* IF LOOP WAS NOT FORCED TO TERMINATE (I NOT EQUAL TO -1)   */ 03228000
*       /* THEN PACK THE WORKAREA AND REMOVE THE SIGN DIGIT          */ 03229000
*       /*                                                           */ 03230000
*       /*************************************************************/ 03231000
*                                                                  0680 03232000
*       IF I=0 THEN                                                0680 03233000
         SLR   @08,@08                                             0680 03234000
         CR    I,@08                                               0680 03235000
         BNE   @RF00680                                            0680 03236000
*         DO;                                                      0681 03237000
*           PACK(WORKAREA,WORKAREA);                               0682 03238000
         PACK  WORKAREA(8),WORKAREA(8)                             0682 03239000
*           WKREG2=WORKAREA(5:8)/16;                               0683 03240000
*                                                                  0683 03241000
         ICM   WKREG2,15,WORKAREA+4                                0683 03242000
         SRL   WKREG2,4                                            0683 03243000
*           /*********************************************************/ 03244000
*           /*                                                       */ 03245000
*           /* DETERMINE IF VALUE JUST CONVERTED IS A VALID CPU      */ 03246000
*           /* ADDRESS                                               */ 03247000
*           /*                                                       */ 03248000
*           /*********************************************************/ 03249000
*                                                                  0684 03250000
*           IF WKREG2>=0&WKREG2<16 THEN                            0684 03251000
         CR    WKREG2,@08                                          0684 03252000
         BL    @RF00684                                            0684 03253000
         C     WKREG2,@CF00040                                     0684 03254000
         BNL   @RF00684                                            0684 03255000
*             DO;                                                  0685 03256000
*                                                                  0685 03257000
*               /*****************************************************/ 03258000
*               /*                                                   */ 03259000
*               /* IF CPU ADDRESS WAS VALID DETERMINE IF CPU IS ON   */ 03260000
*               /* LINE. IF NOT ON LINE SET RETURN CODE EQUAL TO     */ 03261000
*               /* EIGHT, AND RETURN. IF ON LINE PLACE THE CPU       */ 03262000
*               /* ADDRESS IN THE IGF955I,IGF956I MESSAGE            */ 03263000
*               /* BUFFERS,SAVE THE CPU ADDRESS, INCREMENT THE VERB  */ 03264000
*               /* POINTER AND SET THE RETURN CODE TO ZERO AND       */ 03265000
*               /* RETURN.                                           */ 03266000
*               /*                                                   */ 03267000
*               /*****************************************************/ 03268000
*                                                                  0686 03269000
*               CPUALREG=CSDCPUAL;  /* OBTAIN CPU ALIVE BIT MASK     */ 03270000
         L     @08,CVTPTR                                          0686 03271000
         L     @01,CVTCSD(,@08)                                    0686 03272000
         LH    CPUALREG,CSDCPUAL(,@01)                             0686 03273000
         N     CPUALREG,@CF02044                                   0686 03274000
*               GEN REFS(WKREG2) SETS(CPUALREG)                    0687 03275000
*                   (SLL CPUALREG,16(WKREG2));                     0687 03276000
         SLL CPUALREG,16(WKREG2)                                        03277000
*               IF CPUALREG^<0 THEN                                0688 03278000
         LTR   CPUALREG,CPUALREG                                   0688 03279000
         BM    @RF00688                                            0688 03280000
*                 RETCODE=8;                                       0689 03281000
         MVC   RETCODE(2),@CH00048                                 0689 03282000
*               ELSE                                               0690 03283000
*                 DO;                                              0690 03284000
         B     @RC00688                                            0690 03285000
@RF00688 DS    0H                                                  0691 03286000
*                   CPU955=INPUTEXT(1:CPIDLNGT);                   0691 03287000
         MVI   CPU955+1,C' '                                       0691 03288000
         MVC   CPU955+2(2),CPU955+1                                0691 03289000
         SLR   @08,@08                                             0691 03290000
         IC    @08,CPIDLNGT                                        0691 03291000
         LR    @07,@08                                             0691 03292000
         BCTR  @07,0                                               0691 03293000
         L     @01,TEXTPTR-1                                       0691 03294000
         LA    @01,0(,@01)                                         0691 03295000
         EX    @07,@SM02066                                        0691 03296000
*                   CPU956=INPUTEXT(1:CPIDLNGT);/*           @G556P2P*/ 03297000
         MVI   CPU956+1,C' '                                       0692 03298000
         MVC   CPU956+2(2),CPU956+1                                0692 03299000
         EX    @07,@SM02068                                        0692 03300000
*                   CPUADDR=WKREG2;                                0693 03301000
         STH   WKREG2,CPUADDR                                      0693 03302000
*                   TEXTPTR=TEXTPTR+CPIDLNGT;                      0694 03303000
         ALR   @01,@08                                             0694 03304000
         STCM  @01,7,TEXTPTR                                       0694 03305000
*                   RETCODE=0;                                     0695 03306000
         SLR   @08,@08                                             0695 03307000
         STH   @08,RETCODE                                         0695 03308000
*                 END;                                             0696 03309000
*             END;                                                 0697 03310000
*           ELSE                                                   0698 03311000
*             ;                                                    0698 03312000
@RF00684 DS    0H                                                  0699 03313000
*         END;                                                     0699 03314000
*       ELSE                                                       0700 03315000
*         ;                                                        0700 03316000
@RF00680 DS    0H                                                  0701 03317000
*     END;                                                         0701 03318000
*   ELSE                                                           0702 03319000
*     ;                                                            0702 03320000
@RF00666 DS    0H                                                  0703 03321000
*   RESPECIFY                                                      0703 03322000
*    (WKREG2,                                                      0703 03323000
*     CPUALREG,                                                    0703 03324000
*     I) UNRSTD;                                                   0703 03325000
*   END VALCPU;                                                    0704 03326000
@EL00011 DS    0H                                                  0704 03327000
@EF00011 DS    0H                                                  0704 03328000
@ER00011 LM    @14,@12,@SA00011                                    0704 03329000
         BR    @14                                                 0704 03330000
@PB00011 DS    0H                                                  0705 03331000
*   GEN(EJECT);                                                    0705 03332000
         EJECT                                                          03333000
*                                                                  0706 03334000
*/*    WTO959I - SUBROUTINE OF MODE COMMAND PROCESSOR (IGF2603D)   0706 03335000
*                                                                  0706 03336000
*      THIS SUBROUTINE IS INVOKED WHEN THE CPU=X VERB SPECIFIES A CPU   03337000
*      THAT IS OFFLINE OR INVALID, OR WHEN THE ENABLE KEYWORD IS   0706 03338000
*      SPECIFIED AND THE CPU SPECIFIED(OR DEFAULTED) DOES NOT SUPPORT   03339000
*      BUFFER/TLB  ENABLEMENT.  IT WILL OUTPUT A MESSAGE TO THE    0706 03340000
*      OPERATOR INDICATING THE ERROR.                              0706 03341000
*                                                                  0706 03342000
*      THE EXTERNAL PROCEDURE CALLING THIS SUBROUTINE MUST:        0706 03343000
*         1. HAVE PROVIDED AN 18 WORD REGISTER SAVE AREA WHOSE ADDRESS  03344000
*            IS IN REGISTER 13 AT ENTRY TO THIS ROUTINE.           0706 03345000
*         2. HAVE PROVIDED A SKELETON MESSAGE DEFINED IN A STRUCTURE    03346000
*            KNOWN AS 'MSG959'.                                    0706 03347000
*         3. HAVE PROVIDED A DESECT FOR XSA.                       0706 03348000
*         4. HAVE SET VARIABLE 'RETCODE' TO INDICATE ACTION REQUIRED    03349000
*            (4 = INVALID AND 8 = OFFLINE)                         0706 03350000
*                                                                  0706 03351000
*                                                                  0706 03352000
*      AT EXIT ALL REGISTERS ARE RETURNED TO THE CALLER UNCHANGED. 0706 03353000
*                                                                  0706 03354000
*                                                                    */ 03355000
*                                                                  0706 03356000
*WTO959I:                                                          0706 03357000
*   PROC;                                                          0706 03358000
*                                                                  0706 03359000
         B     @PB00012                                            0706 03360000
WTO959I  STM   @14,@12,12(@13)                                     0706 03361000
*   /*****************************************************************/ 03362000
*   /*                                                               */ 03363000
*   /* DETERMINE IF INPUT RETCODE IS 4 OR 8. IF 4 OR 8 THEN CONTINUE,*/ 03364000
*   /* IF NOT THEN SET XAE = X'3A' AND RETURN                        */ 03365000
*   /*                                                               */ 03366000
*   /*****************************************************************/ 03367000
*                                                                  0707 03368000
*   IF RETCODE^=4&RETCODE^=8 THEN                                  0707 03369000
         LH    @10,RETCODE                                         0707 03370000
         C     @10,@CF00038                                        0707 03371000
         BE    @RF00707                                            0707 03372000
         C     @10,@CF00048                                        0707 03373000
         BE    @RF00707                                            0707 03374000
*     XAE=ABORT;                    /* INDICATE MODE COMMAND ABORTED */ 03375000
         MVI   XAE(XSAPTR),X'3A'                                   0708 03376000
*   ELSE                                                           0709 03377000
*     DO;                                                          0709 03378000
*                                                                  0709 03379000
         B     @RC00707                                            0709 03380000
@RF00707 DS    0H                                                  0710 03381000
*       /*************************************************************/ 03382000
*       /*                                                           */ 03383000
*       /* MOVE MESSAGE TEXT INTO MESSAGE BUFFER                     */ 03384000
*       /*                                                           */ 03385000
*       /*************************************************************/ 03386000
*                                                                  0710 03387000
*       BUF959=MSG959;                                             0710 03388000
*                                                                  0710 03389000
         MVC   BUF959(63),MSG959                                   0710 03390000
*       /*************************************************************/ 03391000
*       /*                                                           */ 03392000
*       /* IF RETURN CODE WAS 8 THEN MOVE OFFLINE INTO MESSAGE AS    */ 03393000
*       /* MESSAGE WAS INITIALIZED WITH 'INVALID'                    */ 03394000
*       /*                                                           */ 03395000
*       /*************************************************************/ 03396000
*                                                                  0711 03397000
*       IF RETCODE=8 THEN                                          0711 03398000
         CLC   RETCODE(2),@CH00048                                 0711 03399000
         BNE   @RF00711                                            0711 03400000
*         TEXT959='OFFLINE';                                       0712 03401000
*                                                                  0712 03402000
         MVC   TEXT959(7),@CC02025                                 0712 03403000
*       /*************************************************************/ 03404000
*       /*                                                           */ 03405000
*       /* SET REGISTER 0 TO CONSOLE ID FOR WTO                      */ 03406000
*       /*                                                           */ 03407000
*       /*************************************************************/ 03408000
*                                                                  0713 03409000
*       REGZERO=XAU;                                               0713 03410000
*                                                                  0713 03411000
@RF00711 SLR   REGZERO,REGZERO                                     0713 03412000
         IC    REGZERO,XAU(,XSAPTR)                                0713 03413000
*       /*************************************************************/ 03414000
*       /*                                                           */ 03415000
*       /* OUTPUT MESSAGE TO OPERATOR                                */ 03416000
*       /*                                                           */ 03417000
*       /*************************************************************/ 03418000
*                                                                  0714 03419000
*       DO;                         /* WTOP MF(E,BUF959)             */ 03420000
*         RFY                                                      0715 03421000
*           GPR01P RSTD;            /* RESTRICT REG 1                */ 03422000
*         GPR01P=ADDR(BUF959);      /* ADDRESS CONTROL PROGRAM LIST  */ 03423000
         LA    GPR01P,BUF959                                       0716 03424000
*         SVC(35);                  /* ISSUE WTO SVC                 */ 03425000
