         LR    @08,I                                               0348 01361000
         BCTR  @08,0                                               0348 01362000
         STC   @08,VARYCHAR(,SWADDR)                               0348 01363000
*   /*****************************************************************/ 01364000
*   /*                                                               */ 01365000
*   /* DETERMINE IF CPU=X VERB WAS SPECIFIED. IF SPECIFIED DETERMINE */ 01366000
*   /* IF THE VALCPU SUBROUTINE HAS PREVIOUSLY CALLED. IF NOT SET THE*/ 01367000
*   /* RECURSION SWITCH TO INDICATE VALCPU CALLED AND CALL THE VALCPU*/ 01368000
*   /* SUBROUTINE TO CHECK THE CHARACTERS SPECIFIED.                 */ 01369000
*   /*                                                               */ 01370000
*   /*****************************************************************/ 01371000
*                                                                  0349 01372000
*   IF CPUID=CPUX&RECURSW=BITOFF THEN                              0349 01373000
         CLI   CPUID,2                                             0349 01374000
         BNE   @RF00349                                            0349 01375000
         TM    RECURSW,B'00100000'                                 0349 01376000
         BNZ   @RF00349                                            0349 01377000
*     DO;                                                          0350 01378000
