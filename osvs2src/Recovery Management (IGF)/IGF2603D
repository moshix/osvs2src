         IC    @08,VARYCHAR(,SWADDR)                               0640 02991000
         LR    @07,@08                                             0640 02992000
         BCTR  @07,0                                               0640 02993000
         L     @01,TEXTPTR-1                                       0640 02994000
         LA    @01,0(,@01)                                         0640 02995000
         EX    @07,@SM02062                                        0640 02996000
*                       TEXTPTR=TEXTPTR+VARYCHAR;/*          @G556P2P*/ 02997000
         ALR   @01,@08                                             0641 02998000
         STCM  @01,7,TEXTPTR                                       0641 02999000
*                       RETCODE=0;  /*                       @G556P2P*/ 03000000
         SLR   @10,@10                                             0642 03001000
         STH   @10,RETCODE                                         0642 03002000
*                     END;          /*                       @G556P2P*/ 03003000
*                   ELSE            /*                       @G55CP2P*/ 03004000
*                     IF PARMTYPE=ENABPARM THEN                    0644 03005000
         B     @RC00637                                            0644 03006000
@RF00637 CLC   PARMTYPE(2),@CH00038                                0644 03007000
         BNE   @RF00644                                            0644 03008000
*                       DO;         /*                       @G556P2P*/ 03009000
*                         VMSGPRM1='ENABLE=';/*              @G556P2P*/ 03010000
         MVI   VMSGPRM1+7,C' '                                     0646 03011000
         MVC   VMSGPRM1+8(2),VMSGPRM1+7                            0646 03012000
         MVC   VMSGPRM1(7),@CC01744                                0646 03013000
*                         PRM1VAL=INPUTEXT(1:VARYCHAR);/*    @G556P2P*/ 03014000
         MVI   PRM1VAL+1,C' '                                      0647 03015000
         MVI   PRM1VAL+2,C' '                                      0647 03016000
         SLR   @08,@08                                             0647 03017000
