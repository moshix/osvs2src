*       RECADDR=WKREG1;             /* SAVE GETMAINED ADDR   @G51BPLC*/ 02895000
         LR    RECADDR,WKREG1                                      0696 02896000
*       WKREG6=WKREG15;             /* SAVE RETURN CODE      @G51BPLC*/ 02897000
         LR    WKREG6,WKREG15                                      0697 02898000
*       GEN REFS(PSALITA);                                         0698 02899000
*                           /*RELEASE SALLOC LOCK           @G51BPLC*/  02900000
         SETLOCK RELEASE,TYPE=SALLOC,DISABLED,REGS=USE,               XX02901000
               RELATED=(IGFCCHRD,'FREE LOCK AFTER GETMAIN')             02902000
*       PCCASLCK='0'B;              /* TURN OFF LOCK HELD INDICATOR     02903000
*                                                            @G51BPLC*/ 02904000
         NI    PCCASLCK(PCCAPTR),B'10111111'                       0699 02905000
*       IF WKREG6=ZERO THEN                                        0700 02906000
         LTR   WKREG6,WKREG6                                       0700 02907000
         BNZ   @RF00700                                            0700 02908000
*         DO;                       /* IF GETMAIN GOOD RETRY RECORD     02909000
*                                                            @G51BPLC*/ 02910000
*           WKREG1=RECADDR;         /*                       @G51BPLC*/ 02911000
         LR    WKREG1,RECADDR                                      0702 02912000
*           WKREG6=RECLNGTH;        /* SAVE LENGTH SINCE IT WILL BE     02913000
*                                      SET TO 0 BY MVCL      @G51BPLC*/ 02914000
         LR    WKREG6,RECLNGTH                                     0703 02915000
*           WKREG14=PCCAELBA;       /* SET UP EVEN-ODD REG PAIR FOR     02916000
*                                      MVCL                  @G51BPLC*/ 02917000
         L     WKREG14,PCCAELBA(,PCCAPTR)                          0704 02918000
*           WKREG15=RECLNGTH;       /* R4=GETMAIN ADDR, R15=LENGTH 0705 02919000
*                                                            @G51BPLC*/ 02920000
         LR    WKREG15,RECLNGTH                                    0705 02921000
*           MVCL(RECADDR,WKREG14);  /* MOVE LRB TO GETMAIN AREA    0706 02922000
*                                                            @G51BPLC*/ 02923000
         MVCL  RECADDR,WKREG14                                     0706 02924000
*           GEN REFS(WKREG6,WKREG1,CVTMAP,CVTRECRQ,FLCCVT);        0707 02925000
*                                                        /* @G51BPLC*/  02926000
         RECORD TYPE=LOGREC,RCVRY=SETFRR,DATAADR=(WKREG1),            XX02927000
               LENGTH=(WKREG6),BUFFER=NO                                02928000
*         END;                                                     0708 02929000
*       RECADDR=PCCAELBA;           /* RESTORE ORIG LRB ADDR @G51BPLC*/ 02930000
@RF00700 L     RECADDR,PCCAELBA(,PCCAPTR)                          0709 02931000
*     END;                          /*                       @G51BPLC*/ 02932000
*   RFY                                                            0711 02933000
*    (WKREG0,                                                      0711 02934000
*     WKREG1,                                                      0711 02935000
*     WKREG6,                                                      0711 02936000
*     WKREG14,                                                     0711 02937000
*     WKREG15) UNRSTD;              /*                       @G51BPLC*/ 02938000
@RF00689 DS    0H                                                  0712 02939000
*   CCHMSGBF=CDACCHM1;              /* INIT MSG BUFFER       @G51BPLC*/ 02940000
*                                                                  0712 02941000
         L     @15,PCCACCHM(,PCCAPTR)                              0712 02942000
         L     @12,CVTPTR                                          0712 02943000
         L     @12,RVTPTR(,@12)                                    0712 02944000
         L     @12,CDAPTR(,@12)                                    0712 02945000
         MVC   CCHMSGBF(61,@15),CDACCHM1(@12)                      0712 02946000
*   /*****************************************************************/ 02947000
*   /*                                                               */ 02948000
*   /* THE DEVICE ADDRESS WILL NOW BE CONVERTED FROM HEX TO PRINTABLE*/ 02949000
*   /* CHARACTERS IN A WORK AREA IN THE PCCA (PCCACHID) AND THEN     */ 02950000
*   /* MOVED INTO THE CURRENT MESSAGE BUFFER                         */ 02951000
*   /*                                                               */ 02952000
*   /*****************************************************************/ 02953000
*                                                                  0713 02954000
*   UNPK(DEVADDR,LOGDEV);           /* PLACE A HEX F IN FRONT OF EACH   02955000
*                                      HEX CHARACTER                 */ 02956000
         UNPK  DEVADDR(4,PCCAPTR),LOGDEV(3)                        0713 02957000
*   TR(DEVADDR(1:3),TABLE);         /* CONVERT TO PRINTABLE        0714 02958000
*                                      CHARACTERS VIA TRANSLATE      */ 02959000
         LA    @12,TAB                                             0714 02960000
         AL    @12,@CF03525                                        0714 02961000
         TR    DEVADDR(3,PCCAPTR),TABLE(@12)                       0714 02962000
*   CCHMSGUA=DEVADDR;               /* MOVE CONVERTED DATA TO MESSAGE   02963000
*                                      BUFFER                        */ 02964000
*                                                                  0715 02965000
         MVC   CCHMSGUA(3,@15),DEVADDR(PCCAPTR)                    0715 02966000
*   /*****************************************************************/ 02967000
*   /*                                                               */ 02968000
*   /* A TEST WILL NOW BE MADE TO DETERMINE IF THE DEVICE ADDRESS    */ 02969000
*   /* JUST CONVERTED IS VALID BY TESTING A BIT IN THE PCCA          */ 02970000
*   /* (PCCACDAV). IF THE DEVICE ADDRESS IS NOT VALID THEN THE DEVICE*/ 02971000
*   /* ADDRESS FIELD IN THE MESSAGE BUFFER WILL BE MADE 'XX'.        */ 02972000
*   /*                                                               */ 02973000
*   /*****************************************************************/ 02974000
*                                                                  0716 02975000
*   IF PCCACDAV=BINARY0 THEN                                       0716 02976000
         TM    PCCACDAV(PCCAPTR),B'00000001'                       0716 02977000
