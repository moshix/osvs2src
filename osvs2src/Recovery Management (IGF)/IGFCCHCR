*IGFCCHUC:                                                         0561 01936000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0561 01937000
*                                                                  0561 01938000
IGFCCHUC DS    0H                                                  0562 01939000
*   /*****************************************************************/ 01940000
*   /*                                                               */ 01941000
*   /* INTERNAL DECLARES                                             */ 01942000
*   /*                                                               */ 01943000
*   /*****************************************************************/ 01944000
*                                                                  0562 01945000
*   DCL                                                            0562 01946000
*     CHANNEL CHAR(2) DEFINED(UCBCHA);/* OVERLAY FOR UCBCHA          */ 01947000
*   DCL                                                            0563 01948000
*     CPUAD CHAR(1) DEF(PCCACPUA) POS(2);                          0563 01949000
*   DCL                                                            0564 01950000
*     ADLSTPTR PTR(31) REG(3) RESTRICTED;/* UCB ADDRESS LIST POINTER */ 01951000
*   DCL                                                            0565 01952000
*     UCBADTAB PTR(16) BASED(ADLSTPTR);/* UCB ADDRESS                */ 01953000
*   DCL                                                            0566 01954000
*     UCBADDR PTR(24) REG(6) RESTRICTED;/* ALTERNATE UCB POINTER     */ 01955000
*   DCL                                                            0567 01956000
*     COUNTER PTR(31) REG(5) RESTRICTED;/* LRBCAIO POINTER           */ 01957000
*   DCL                                                            0568 01958000
*     LIMIT PTR(31) REG(9) RESTRICTED;/* LIMIT FOR LOOP CONTROL      */ 01959000
*   DCL                                                            0569 01960000
*     CHANAD FIXED(31) REG(4) RESTRICTED;/* ISOLATED CHANNEL ADDR    */ 01961000
*   DCL                                                            0570 01962000
*     UCLRBPTR PTR(31) REG(1) RESTRICTED;/* UC LRB POINTER           */ 01963000
*   DCL                                                            0571 01964000
*     FAILCHAN CHAR(2) DEFINED(PCCACHW1) POS(3);/* ADDRESS OF FAILING   01965000
*                                      CHANNEL                       */ 01966000
*   RESPECIFY                                                      0572 01967000
*     LRB BASED(UCLRBPTR);                                         0572 01968000
*   RESPECIFY                                                      0573 01969000
*     UCBOB BASED(UCBADDR);                                        0573 01970000
*                                                                  0573 01971000
*   /*****************************************************************/ 01972000
*   /*                                                               */ 01973000
*   /* SET UCB SEARCH FOOTPRINT IN PCCA                              */ 01974000
*   /*                                                               */ 01975000
*   /*****************************************************************/ 01976000
*                                                                  0574 01977000
*   PCCACF25=BINARY1;                                              0574 01978000
*                                                                  0574 01979000
         OI    PCCACF25(PCCAPTR),B'00001000'                       0574 01980000
*   /*****************************************************************/ 01981000
*   /*                                                               */ 01982000
*   /* SAVE THE CONTENTS OF THE UNIT ADDRESS REGISTER (REG-6) IN THE */ 01983000
*   /* PCCA CHANNEL SAVE AREA, INITIALIZE THE COUNTER, THE LRB       */ 01984000
*   /* POINTER FOR THIS SUBROUTINE (UCLRBPTR) AND OBTAIN UCB ADDRESS */ 01985000
*   /* LIST ADDRESS FROM THE CVT                                     */ 01986000
*   /*                                                               */ 01987000
*   /*****************************************************************/ 01988000
*                                                                  0575 01989000
*   PCCACHW1=UAREG;                 /* SAVE REG 6 FOR IGFCCHRD     0575 01990000
*                                      SUBROUTINE                    */ 01991000
         ST    UAREG,PCCACHW1(,PCCAPTR)                            0575 01992000
*   COUNTER=ONE;                                                   0576 01993000
         LA    COUNTER,1                                           0576 01994000
*   UCLRBPTR=PCCAELBA;              /*                       @G51BPLC*/ 01995000
         L     UCLRBPTR,PCCAELBA(,PCCAPTR)                         0577 01996000
*   ADLSTPTR=CVTILK2;                                              0578 01997000
*                                                                  0578 01998000
         L     @15,CVTPTR                                          0578 01999000
         L     ADLSTPTR,CVTILK2(,@15)                              0578 02000000
*   /*****************************************************************/ 02001000
*   /*                                                               */ 02002000
*   /* OBTAIN THE ADDRESS OF THE FIRST UCB                           */ 02003000
*   /*                                                               */ 02004000
*   /*****************************************************************/ 02005000
*                                                                  0579 02006000
*   UCBADDR=UCBADTAB;                                              0579 02007000
*                                                                  0579 02008000
         LH    UCBADDR,UCBADTAB(,ADLSTPTR)                         0579 02009000
         N     UCBADDR,@CF03517                                    0579 02010000
*   /*****************************************************************/ 02011000
*   /*                                                               */ 02012000
*   /* IF EIGHT CHAN/DEVICE ADDRESSES HAVE BEEN PLACED IN THE LRBCAIO*/ 02013000
*   /* FIELD OR THE END OF THE UCB ADDRESS LIST HAS BEEN ENCOUNTERED */ 02014000
*   /* RETURN TO CALLER                                              */ 02015000
*   /*                                                               */ 02016000
*   /*****************************************************************/ 02017000
*                                                                  0580 02018000
*   DO WHILE COUNTER<NINE&UCBADDR^=ALLONES;                        0580 02019000
*                                                                  0580 02020000
         B     @DE00580                                            0580 02021000
@DL00580 DS    0H                                                  0581 02022000
*     /***************************************************************/ 02023000
*     /*                                                             */ 02024000
*     /* A TEST IS NOW MADE TO DETERMINE IF THE UCB ADDRESS IS VALID,*/ 02025000
*     /* THE CHANNEL ADDRESS CONTAINED IN THE CURRENT UCB IS THE     */ 02026000
*     /* CHANNEL ON WHICH AN ERROR HAS BEEN DETECTED AND ALSO        */ 02027000
*     /* DETERMINE IF THIS DEVICE IS CURRENTLY IN USE                */ 02028000
*     /*                                                             */ 02029000
*     /***************************************************************/ 02030000
*                                                                  0581 02031000
*     IF UCBADDR^=ZERO THEN                                        0581 02032000
         LTR   UCBADDR,UCBADDR                                     0581 02033000
         BZ    @RF00581                                            0581 02034000
*       DO;                                                        0582 02035000
*         CHANAD=UCBCHA;            /* OBTAIN CHANNEL ADDRESS        */ 02036000
         SLR   CHANAD,CHANAD                                       0583 02037000
         IC    CHANAD,UCBCHA(,UCBADDR)                             0583 02038000
*         CHANAD=(CHANAD&MASKA);    /* ISOLATE CHAN ADDR             */ 02039000
         N     CHANAD,@CF03138                                     0584 02040000
*         IF CHANAD=FAILCHAN(1)&UCBBUSYD=BINARY1 THEN              0585 02041000
         SLR   @15,@15                                             0585 02042000
         IC    @15,FAILCHAN(,PCCAPTR)                              0585 02043000
         CR    CHANAD,@15                                          0585 02044000
         BNE   @RF00585                                            0585 02045000
         TM    UCBBUSYD(UCBADDR),B'10000000'                       0585 02046000
         BNO   @RF00585                                            0585 02047000
*           DO;                                                    0586 02048000
*                                                                  0586 02049000
*             /*******************************************************/ 02050000
*             /*                                                     */ 02051000
*             /* IF THE RESULTS OF THE ABOVE TEST ARE TRUE A TEST    */ 02052000
*             /* WILL BE MADE TO DETERMINE IF THIS DEVICE ADDRESS HAS*/ 02053000
*             /* ALREADY BEEN PLACED INTO THE LRB                    */ 02054000
*             /*                                                     */ 02055000
*             /*******************************************************/ 02056000
*                                                                  0587 02057000
*             DO LIMIT=ONE TO(COUNTER-ONE) WHILE LRBCAIO(LIMIT,2)^=     02058000
*                   UCBUA;                                         0587 02059000
         LA    LIMIT,1                                             0587 02060000
         B     @DE00587                                            0587 02061000
@DL00587 LR    @15,LIMIT                                           0587 02062000
         ALR   @15,@15                                             0587 02063000
         SLR   @12,@12                                             0587 02064000
         IC    @12,LRBCAIO-1(@15,UCLRBPTR)                         0587 02065000
         SLR   @15,@15                                             0587 02066000
         IC    @15,UCBUA(,UCBADDR)                                 0587 02067000
         CR    @12,@15                                             0587 02068000
         BE    @DC00587                                            0587 02069000
*             END;                                                 0588 02070000
*                                                                  0588 02071000
         AL    LIMIT,@CF00063                                      0588 02072000
@DE00587 LR    @15,COUNTER                                         0588 02073000
         BCTR  @15,0                                               0588 02074000
         CR    LIMIT,@15                                           0588 02075000
         BNH   @DL00587                                            0588 02076000
@DC00587 DS    0H                                                  0589 02077000
*             /*******************************************************/ 02078000
*             /*                                                     */ 02079000
*             /* IF THIS DEVICE ADDRESS HAS NOT ALREADY BEEN PLACED  */ 02080000
*             /* INTO THE LRB (INDICATED BY THE LIMIT CONTAINING A   */ 02081000
*             /* VALUE EQUAL TO THE COUNTER) THEN PLACE IT THERE AND */ 02082000
*             /* INCREMENT THE COUNTER BY ONE                        */ 02083000
*             /*                                                     */ 02084000
*             /*******************************************************/ 02085000
*                                                                  0589 02086000
*             IF LIMIT=COUNTER THEN                                0589 02087000
         CR    LIMIT,COUNTER                                       0589 02088000
         BNE   @RF00589                                            0589 02089000
*               DO;                                                0590 02090000
*                 IF(PCCACCRA='1'B&UCBIORST='1'B)/*          @Y30CJJE*/ 02091000
*                     |(PCCACCRA^='1'B&UCBIORST^='1'B/*      @Y30CJJE*/ 02092000
*                     &UCBCPU=CPUAD) THEN                          0591 02093000
         TM    PCCACCRA(PCCAPTR),B'00000100'                       0591 02094000
         BNO   @GL00011                                            0591 02095000
         TM    UCBIORST(UCBADDR),B'10000000'                       0591 02096000
         BO    @RT00591                                            0591 02097000
@GL00011 TM    PCCACCRA(PCCAPTR),B'00000100'                       0591 02098000
         BO    @RF00591                                            0591 02099000
         TM    UCBIORST(UCBADDR),B'10000000'                       0591 02100000
         BO    @RF00591                                            0591 02101000
         CLC   UCBCPU(1,UCBADDR),CPUAD(PCCAPTR)                    0591 02102000
         BNE   @RF00591                                            0591 02103000
@RT00591 DS    0H                                                  0592 02104000
*                   DO;             /*                       @Y30CJJE*/ 02105000
*                     LRBCAIO(COUNTER)=(CHANNEL&MASK);             0593 02106000
         LR    @15,COUNTER                                         0593 02107000
         ALR   @15,@15                                             0593 02108000
         ALR   @15,UCLRBPTR                                        0593 02109000
         MVC   LRBCAIO-2(2,@15),CHANNEL(UCBADDR)                   0593 02110000
         NC    LRBCAIO-2(2,@15),@CB03132                           0593 02111000
*                     COUNTER=COUNTER+ONE;                         0594 02112000
         AL    COUNTER,@CF00063                                    0594 02113000
*                   END;                                           0595 02114000
*               END;                                               0596 02115000
@RF00591 DS    0H                                                  0597 02116000
*           END;                                                   0597 02117000
@RF00589 DS    0H                                                  0598 02118000
*       END;                                                       0598 02119000
*                                                                  0598 02120000
@RF00585 DS    0H                                                  0599 02121000
*     /***************************************************************/ 02122000
*     /*                                                             */ 02123000
*     /* UPDATE POINTER TO LOCATE NEXT UCB ADDRESS AND DO ABOVE AGAIN*/ 02124000
*     /* FOR THE NEXT UCB                                            */ 02125000
*     /*                                                             */ 02126000
*     /***************************************************************/ 02127000
*                                                                  0599 02128000
*     ADLSTPTR=ADLSTPTR+TWO;                                       0599 02129000
*                                                                  0599 02130000
@RF00581 AL    ADLSTPTR,@CF00223                                   0599 02131000
*     /***************************************************************/ 02132000
*     /*                                                             */ 02133000
*     /* OBTAIN THE ADDRESS OF THE NEXT UCB                          */ 02134000
*     /*                                                             */ 02135000
*     /***************************************************************/ 02136000
*                                                                  0600 02137000
*     UCBADDR=UCBADTAB;                                            0600 02138000
         LH    UCBADDR,UCBADTAB(,ADLSTPTR)                         0600 02139000
         N     UCBADDR,@CF03517                                    0600 02140000
*   END;                                                           0601 02141000
*                                                                  0601 02142000
@DE00580 C     COUNTER,@CF03119                                    0601 02143000
         BNL   @DC00580                                            0601 02144000
         C     UCBADDR,@CF03134                                    0601 02145000
         BNE   @DL00580                                            0601 02146000
@DC00580 DS    0H                                                  0602 02147000
*   /*****************************************************************/ 02148000
*   /*                                                               */ 02149000
*   /* RESTORE REGISTER 6 FOR IGFCCHRD SUBROUTINE                    */ 02150000
*   /*                                                               */ 02151000
*   /*****************************************************************/ 02152000
*                                                                  0602 02153000
*   UAREG=PCCACHW1;                                                0602 02154000
         L     UAREG,PCCACHW1(,PCCAPTR)                            0602 02155000
*   RESPECIFY                                                      0603 02156000
*    (ADLSTPTR,                                                    0603 02157000
*     UCBADDR,                                                     0603 02158000
*     COUNTER,                                                     0603 02159000
*     LIMIT,                                                       0603 02160000
*     CHANAD,                                                      0603 02161000
*     UCLRBPTR) UNRSTD;                                            0603 02162000
*   RESPECIFY                                                      0604 02163000
*     LRB BASED(LRBPTR);                                           0604 02164000
*   RESPECIFY                                                      0605 02165000
*     UCBOB BASED(UCBPTR);                                         0605 02166000
*   END IGFCCHUC;                                                  0606 02167000
@EL00006 DS    0H                                                  0606 02168000
@EF00006 DS    0H                                                  0606 02169000
@ER00006 BR    @14                                                 0606 02170000
*                                                                  0607 02171000
*/*           MULTI-PROCESSOR INFORMATION SUBROUTINE                 */ 02172000
*/*  START OF SPECIFICATIONS ****                                  0607 02173000
*                                                                  0607 02174000
*01*  MODULE-NAME = IGFCCHMP                                       0607 02175000
*                                                                  0607 02176000
*01*  DESCRIPTIVE-NAME = MULTI-PROCESSING INFORMATION SUBROUTINE   0607 02177000
*                                                                  0607 02178000
*01*  COPYRIGHT = NONE                                             0607 02179000
*                                                                  0607 02180000
*01*  STATUS = VERSION 1 FOR VS/2 RELEASE 2                        0607 02181000
*                                                                  0607 02182000
*01*  FUNCTION = THE IGFCCHMP SUBROUTINE IS CALLED BY IGFCCHRD TO FILL  02183000
*     IN THE MULTI-PROCESSING PORTION OF THE CCH RECORD.  THE PROCESSOR 02184000
*     ADDRESS AND CHANNEL STATUS (ONLINE-OFFLINE) FOR EACH OF THE  0607 02185000
*     PROCESSORS IS RECORDED.  CONTROL IS RETURNED TO IGFCCHRD.    0607 02186000
*                                                                  0607 02187000
*01*  NOTES = NONE                                                 0607 02188000
*                                                                  0607 02189000
*02*    CHARACTER-CODE-DEPENDENCIES = EBCDIC DEPENDANT             0607 02190000
*                                                                  0607 02191000
