         BE    @RT00689                                            0689 02874000
         C     WKREG15,@CF01703                                    0689 02875000
         BNE   @RF00689                                            0689 02876000
@RT00689 DS    0H                                                  0690 02877000
*     DO;                           /* RECORD DID NOT WORK DUE TO  0690 02878000
*                                      LACK OF BUFFER        @G51BPLC*/ 02879000
*       GEN REFS(RECLNGTH,PSALITA);                                0691 02880000
*                                    /* GET NEW BUFFER      @G51BPLC*/  02881000
        SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,DISABLED,REGS=USE,     XX02882000
               RELATED=(IGFCCHRD,'SALLOC LOCK NEEDED FOR GETMAIN')      02883000
*       PCCASLCK='1'B;              /* INDICATE SALLOC HELD FOR FRR     02884000
*                                                            @G51BPLC*/ 02885000
         OI    PCCASLCK(PCCAPTR),B'01000000'                       0692 02886000
*       RFY                                                        0693 02887000
*        (WKREG3) RSTD;             /*                       @G51BPLC*/ 02888000
*       GEN REFS(CVTSPSA,WSAGGMFM,WSAG,CVTGLMN,CVTMAP,CVTPTR) SETS(     02889000
