*   RESPECIFY                                                      0865 03923000
*    (WKREG3,                                                      0865 03924000
*     WKREG4,                                                      0865 03925000
*     WKREG5,                                                      0865 03926000
*     SDWAPTR) RSTD;                                               0865 03927000
*                                                                  0865 03928000
*   /*****************************************************************/ 03929000
*   /*                                                               */ 03930000
*   /* OBTAIN THE PCCAPTR FROM THE FRR WORK AREA                     */ 03931000
*   /*                                                               */ 03932000
*   /*****************************************************************/ 03933000
*                                                                  0866 03934000
*   RESPECIFY                                                      0866 03935000
*     FRRSAREA BASED(SDWAPARM);     /*                       @YM02589*/ 03936000
*   PCCAPTR=FRRPCCA;                /* LOAD PCCAPTR FROM SDWA      0867 03937000
*                                      PARAMETER LIST        @YM02589*/ 03938000
         L     @15,SDWAPARM(,SDWAPTR)                              0867 03939000
         L     PCCAPTR,FRRPCCA(,@15)                               0867 03940000
*   BASEREG=FRRBASE;                /* LOAD BASE REG FROM SDWA     0868 03941000
*                                      PARAMETER LIST        @YM02589*/ 03942000
         L     BASEREG,FRRBASE(,@15)                               0868 03943000
*   SAVREG=FRRSAVE;                 /* LOAD SAVE REG FROM SDWA     0869 03944000
*                                      PARAMETER LIST        @YM02589*/ 03945000
         L     SAVREG,FRRSAVE(,@15)                                0869 03946000
*   RESPECIFY                                                      0870 03947000
