         L     @15,FLCIOA                                          0829 03698000
         O     @15,@CF03460                                        0829 03699000
         ST    @15,SRBPARM(,SRBPTR)                                0829 03700000
*       RFY                                                        0830 03701000
*        (WKREG1,                                                  0830 03702000
*         WKREG0,                                                  0830 03703000
*         WKREG15) RSTD;            /*                       @G51BPLC*/ 03704000
