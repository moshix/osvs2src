         L     @15,PCCACCHM(,PCCAPTR)                              0724 03023000
         MVC   CCHMSGBL(4,@15),@CC03425                            0724 03024000
*     ELSE                                                         0725 03025000
*       IF PCCACSCU=BINARY1         /* CHAN NOT AT FAULT, CHECK SCU  */ 03026000
*         THEN                                                     0725 03027000
         B     @RC00723                                            0725 03028000
@RF00723 TM    PCCACSCU(PCCAPTR),B'00100000'                       0725 03029000
         BNO   @RF00725                                            0725 03030000
*         CCHMSGBL='SCU ';          /* INDICATE SCU AT FAULT         */ 03031000
         L     @15,PCCACCHM(,PCCAPTR)                              0726 03032000
         MVC   CCHMSGBL(4,@15),@CC03426                            0726 03033000
*       ELSE                                                       0727 03034000
*         IF PCCACSTG=BINARY1       /* SCU NOT AT FAULT, CHECK     0727 03035000
*                                      STORAGE                       */ 03036000
*           THEN                                                   0727 03037000
         B     @RC00725                                            0727 03038000
@RF00725 TM    PCCACSTG(PCCAPTR),B'00010000'                       0727 03039000
         BNO   @RF00727                                            0727 03040000
*           CCHMSGBL='STOR';        /* INDICATE STORAGE AT FAULT     */ 03041000
         L     @15,PCCACCHM(,PCCAPTR)                              0728 03042000
         MVC   CCHMSGBL(4,@15),@CC03427                            0728 03043000
*         ELSE                                                     0729 03044000
*           IF PCCACCUE=BINARY1     /* STORAGE NOT AT FAULT, CHECK 0729 03045000
