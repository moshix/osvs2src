*   /*                                                               */ 03128000
*   /*****************************************************************/ 03129000
*                                                                  0741 03130000
*   GEN;                                                           0741 03131000
*                                                                  0741 03132000
 RECORD  TYPE=WTO,RCVRY=SETFRR,DATAADR=(RECADDR),LENGTH=(RECLNGTH),   **03133000
               BUFFER=YES                                               03134000
*/********************************************************** @G51BPLC*/ 03135000
*/* IF RECORD SUFFERS A BUFFER SHORTAGE A GETMAIN WILL BE    @G51BPLC*/ 03136000
*/* DONE FROM SQA AND THE RECORD WILL BE COPIED INTO THE     @G51BPLC*/ 03137000
*/* GETMAINED AREA. THE RECORD WILL BE REISSUED THIS TIME    @G51BPLC*/ 03138000
*/* WITH BUFFER=NO WHICH MEANS THAT RECORD WILL NOT ATTEMPT  @G51BPLC*/ 03139000
*/* TO COPY THE RECORD BUT WILL USE THE GETMAINED AREA WE    @G51BPLC*/ 03140000
*/* HAVE OBTAINED AS A BUFFER. IT CANNOT BE FREMAINED.       @G51BPLC*/ 03141000
*/********************************************************** @G51BPLC*/ 03142000
*                                                                  0742 03143000
*   IF WKREG15=EIGHT|WKREG15=TWELVE THEN                           0742 03144000
         C     WKREG15,@CF00056                                    0742 03145000
         BE    @RT00742                                            0742 03146000
         C     WKREG15,@CF01703                                    0742 03147000
         BNE   @RF00742                                            0742 03148000
@RT00742 DS    0H                                                  0743 03149000
*     DO;                           /* RECORD DID NOT WORK DUE TO  0743 03150000
*                                      LACK OF BUFFER        @G51BPLC*/ 03151000
*       GEN REFS(PSALITA);                                         0744 03152000
*                                    /* GET NEW BUFFER      @G51BPLC*/  03153000
         SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,DISABLED,REGS=USE,    XX03154000
               RELATED=(IGFCCHRD,'SALLOC LOCK NEEDED FOR GETMAIN')      03155000
*       PCCASLCK='1'B;              /* INDICATE SALLOC HELD  @G51BPLC*/ 03156000
         OI    PCCASLCK(PCCAPTR),B'01000000'                       0745 03157000
*       RFY                                                        0746 03158000
*        (WKREG3) RSTD;             /*                       @G51BPLC*/ 03159000
*       GEN REFS(CVTSPSA,WSAGGMFM,WSAG,CVTPTR,CVTGLMN,CVTMAP) SETS(     03160000
*           WKREG3,RECADDR)                                        0747 03161000
*           (GETMAIN RC,LV=(5),SP=245,BRANCH=(YES,GLOBAL));        0747 03162000
         GETMAIN RC,LV=(5),SP=245,BRANCH=(YES,GLOBAL)                   03163000
*       RFY                                                        0748 03164000
*        (WKREG3) UNRSTD;           /*                       @G51BPLC*/ 03165000
*       RECADDR=WKREG1;             /* SAVE GETMAIN ADDR     @G51BPLC*/ 03166000
         LR    RECADDR,WKREG1                                      0749 03167000
*       WKREG6=WKREG15;             /* SAVE R15              @G51BPLC*/ 03168000
         LR    WKREG6,WKREG15                                      0750 03169000
*       GEN REFS(PSALITA);                                         0751 03170000
*                           /*RELEASE SALLOC LOCK           @G51BPLC*/  03171000
         SETLOCK RELEASE,TYPE=SALLOC,DISABLED,REGS=USE,               XX03172000
               RELATED=(IGFCCHRD,'FREE LOCK AFTER GETMAIN')             03173000
*       PCCASLCK='0'B;              /* RESET SALLOC LOCK HELD      0752 03174000
*                                      INDICATOR             @G51BPLC*/ 03175000
         NI    PCCASLCK(PCCAPTR),B'10111111'                       0752 03176000
*       IF WKREG6=ZERO THEN                                        0753 03177000
         LTR   WKREG6,WKREG6                                       0753 03178000
         BNZ   @RF00753                                            0753 03179000
*         DO;                       /* IF GETMAIN GOOD RETRY RECORD     03180000
*                                                            @G51BPLC*/ 03181000
*           WKREG1=RECADDR;         /* SAVE GETMAIN ADDR FOR RECORD     03182000
*                                                            @G51BPLC*/ 03183000
         LR    WKREG1,RECADDR                                      0755 03184000
*           WKREG6=RECLNGTH;        /* SAVE LENGTH FOR RECORD      0756 03185000
*                                                            @G51BPLC*/ 03186000
         LR    WKREG6,RECLNGTH                                     0756 03187000
*           WKREG14=PCCACCHM;       /* SET UP EVEN-ODD REG PAIR FOR     03188000
*                                      MVCL                  @G51BPLC*/ 03189000
         L     WKREG14,PCCACCHM(,PCCAPTR)                          0757 03190000
*           WKREG15=RECLNGTH;       /* R4=GETMAIN ADDR, R15=LENGTH 0758 03191000
*                                                            @G51BPLC*/ 03192000
         LR    WKREG15,RECLNGTH                                    0758 03193000
*           MVCL(RECADDR,WKREG14);  /* MOVE MSG BUF TO GETMAIN AREA     03194000
*                                                            @G51BPLC*/ 03195000
         MVCL  RECADDR,WKREG14                                     0759 03196000
*           GEN REFS(WKREG6,WKREG1,CVTMAP,CVTRECRQ,FLCCVT);        0760 03197000
*                                                        /* @G51BPLC*/  03198000
         RECORD TYPE=WTO,RCVRY=SETFRR,DATAADR=(WKREG1),               XX03199000
               LENGTH=(WKREG6),BUFFER=NO                                03200000
*         END;                                                     0761 03201000
*     END;                          /*                       @G51BPLC*/ 03202000
@RF00753 DS    0H                                                  0763 03203000
*   RFY                                                            0763 03204000
*    (WKREG0,                                                      0763 03205000
*     WKREG1,                                                      0763 03206000
*     WKREG6,                                                      0763 03207000
*     WKREG14,                                                     0763 03208000
*     WKREG15) UNRSTD;              /*                       @G51BPLC*/ 03209000
*                                                                  0763 03210000
@RF00742 DS    0H                                                  0764 03211000
*   /*****************************************************************/ 03212000
*   /*                                                               */ 03213000
*   /* GPR 13 WILL BE RESTORED AND THE BUFFER USED TO CONTAIN THE LRB*/ 03214000
*   /* WILL NOW BE RESET TO ZERO'S TO PREPARE FOR THE NEXT ERROR, IF */ 03215000
*   /* ANY                                                           */ 03216000
*   /*                                                               */ 03217000
*   /*****************************************************************/ 03218000
*                                                                  0764 03219000
*   SAVREG=PCCACHSV(3);             /*                       @YM00965*/ 03220000
*                                                                  0764 03221000
         L     SAVREG,PCCACHSV+8(,PCCAPTR)                         0764 03222000
*   /*****************************************************************/ 03223000
*   /*                                                               */ 03224000
*   /* REGISTER 14 WILL NOW BE RESTORED. IT WAS DESTROYED BY THE     */ 03225000
*   /* RECORD MACRO                                                  */ 03226000
*   /*                                                               */ 03227000
*   /*****************************************************************/ 03228000
*                                                                  0765 03229000
*   RTNREG=PCCACHSV(2);                                            0765 03230000
