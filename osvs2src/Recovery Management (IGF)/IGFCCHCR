*                                                                  0607 02249000
*IGFCCHMP:                                                         0607 02250000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0607 02251000
*                                                                  0607 02252000
IGFCCHMP DS    0H                                                  0608 02253000
*   /*****************************************************************/ 02254000
*   /*                                                               */ 02255000
*   /* INTERNAL DECLARES                                             */ 02256000
*   /*                                                               */ 02257000
*   /*****************************************************************/ 02258000
*                                                                  0608 02259000
*   DCL                                                            0608 02260000
*     N PTR(31) REG(3) RESTRICTED;  /* CHANNEL INDEX FOR CAT AND LOOP   02261000
*                                      CONTROL                       */ 02262000
*   DCL                                                            0609 02263000
*     M PTR(31) REG(4) RESTRICTED;  /* PCCAT INDEX AND LOOP CONTROL  */ 02264000
*   DCL                                                            0610 02265000
*     INDEX PTR(31) REG(2) RESTRICTED;/* INDEX FOR LRB MP FIELDS     */ 02266000
*   DCL                                                            0611 02267000
*     MPCCAPTR PTR(31) REG(5) RESTRICTED;/* ALTERNATE PCCA POINTER   */ 02268000
*   DCL                                                            0612 02269000
*     MPCSREG PTR(31) REG(7) RESTRICTED;/* WORK REG USED TO CONTAIN     02270000
*                                      MP CHANNEL STATUS             */ 02271000
*   DCL                                                            0613 02272000
*     CPUALREG FIXED(31) REG(7);    /* WORK REG USED TO CONTAIN MP 0613 02273000
*                                      CPU ACTIVE STATUS             */ 02274000
*   DCL                                                            0614 02275000
*     MPCATPTR PTR(31) REG(6) RESTRICTED;/* ALTERNATE CAT POINTER    */ 02276000
*   DCL                                                            0615 02277000
*     MLRBCVPT PTR(31) REG(12) RESTRICTED;/* SECOND LRB POINTER      */ 02278000
*   RESPECIFY                                                      0616 02279000
*     CAT BASED(MPCATPTR);                                         0616 02280000
*   RESPECIFY                                                      0617 02281000
*     LRBCVAR BASED(MLRBCVPT);                                     0617 02282000
*                                                                  0617 02283000
*   /*****************************************************************/ 02284000
*   /*                                                               */ 02285000
*   /* SET MULTI-PROCESSING INFORMATION FOOTPRINT IN PCCA            */ 02286000
*   /*                                                               */ 02287000
*   /*****************************************************************/ 02288000
*                                                                  0618 02289000
*   PCCACF24=BINARY1;                                              0618 02290000
*                                                                  0618 02291000
         OI    PCCACF24(PCCAPTR),B'00010000'                       0618 02292000
*   /*****************************************************************/ 02293000
*   /*                                                               */ 02294000
*   /* SET UP REQUIRED SECOND LRB POINTER TO ADDRESS MP INFORMATION  */ 02295000
*   /* AREA IN LRB                                                   */ 02296000
*   /*                                                               */ 02297000
*   /*****************************************************************/ 02298000
*                                                                  0619 02299000
*   MLRBCVPT=ADDR(LRBCCLOG)+LRBCLOGL;                              0619 02300000
*                                                                  0619 02301000
         L     @15,LRBPTR(,PCCAPTR)                                0619 02302000
         LA    MLRBCVPT,LRBCCLOG(,@15)                             0619 02303000
         LH    @15,LRBCLOGL(,@15)                                  0619 02304000
         N     @15,@CF03517                                        0619 02305000
         ALR   MLRBCVPT,@15                                        0619 02306000
*   /*****************************************************************/ 02307000
*   /*                                                               */ 02308000
*   /* INITIALIZE THE INDEX AND THE LOOP CONTROL REGISTER, M         */ 02309000
*   /*                                                               */ 02310000
*   /*****************************************************************/ 02311000
*                                                                  0620 02312000
*   INDEX=ONE;                                                     0620 02313000
         LA    INDEX,1                                             0620 02314000
*   M=ZERO;                                                        0621 02315000
*                                                                  0621 02316000
         SLR   M,M                                                 0621 02317000
*   /*****************************************************************/ 02318000
*   /*                                                               */ 02319000
*   /* SET LRBCMPNO FIELD EQUAL TO THE NUMBER OF CURRENTLY ACTIVE    */ 02320000
*   /* PROCESSORS                                                    */ 02321000
*   /*                                                               */ 02322000
*   /*****************************************************************/ 02323000
*                                                                  0622 02324000
*   LRBCMPNO=CSDCPUOL;                                             0622 02325000
*                                                                  0622 02326000
         L     @15,CVTPTR                                          0622 02327000
         L     @10,CVTCSD(,@15)                                    0622 02328000
         MVC   LRBCMPNO(2,MLRBCVPT),CSDCPUOL(@10)                  0622 02329000
*   /*****************************************************************/ 02330000
*   /*                                                               */ 02331000
*   /* SET THE ALTERNATE PCCA POINTER EQUAL TO THE VALUE OF THE      */ 02332000
*   /* POINTER PASSED TO THIS SUBROUTINE IN REGISTER 8               */ 02333000
*   /*                                                               */ 02334000
*   /*****************************************************************/ 02335000
*                                                                  0623 02336000
*   IF PCCACCRA='1'B THEN           /*                       @Y30CJJE*/ 02337000
         TM    PCCACCRA(PCCAPTR),B'00000100'                       0623 02338000
         BNO   @RF00623                                            0623 02339000
*     MPCCAPTR=CRCAPCCA;            /*                       @Y30CJJE*/ 02340000
         L     @15,CRCAPTR(,@15)                                   0624 02341000
         L     MPCCAPTR,CRCAPCCA(,@15)                             0624 02342000
*   ELSE                                                           0625 02343000
*     MPCCAPTR=PCCAPTR;             /*                       @Y30CJJE*/ 02344000
*                                                                  0625 02345000
         B     @RC00623                                            0625 02346000
@RF00623 LR    MPCCAPTR,PCCAPTR                                    0625 02347000
*   /*****************************************************************/ 02348000
*   /*                                                               */ 02349000
*   /* THE REMAINING CODE IN THIS SUBROUTINE WILL BE EXECUTED ONCE   */ 02350000
*   /* FOR THE CPU WITH A CHANNEL ERROR AND THEN AGAIN FOR EVERY CPU */ 02351000
*   /* (UP TO 16) ON LINE AT THE TIME OF THE ERROR.                  */ 02352000
*   /*                                                               */ 02353000
*   /*****************************************************************/ 02354000
*                                                                  0626 02355000
*   DO WHILE M<17;                                                 0626 02356000
*                                                                  0626 02357000
@RC00623 B     @DE00626                                            0626 02358000
@DL00626 DS    0H                                                  0627 02359000
*     /***************************************************************/ 02360000
*     /*                                                             */ 02361000
*     /* SET LRBCMPPA BY THE INDEX EQUAL TO THE PROCESSOR ID (00 -   */ 02362000
*     /* 3F)                                                         */ 02363000
*     /*                                                             */ 02364000
*     /***************************************************************/ 02365000
*                                                                  0627 02366000
*     LRBCMPPA(INDEX)=MPCCAPTR->PCCACPUA;                          0627 02367000
*                                                                  0627 02368000
         LR    @15,INDEX                                           0627 02369000
         SLA   @15,2                                               0627 02370000
         LH    @10,PCCACPUA(,MPCCAPTR)                             0627 02371000
         ALR   @15,MLRBCVPT                                        0627 02372000
         STCM  @10,3,LRBCMPPA-4(@15)                               0627 02373000
*     /***************************************************************/ 02374000
*     /*                                                             */ 02375000
*     /* SET THE MPCATPTR TO POINT TO THE CURRENT CHANNEL            */ 02376000
*     /* AVAILABILITY TABLE                                          */ 02377000
*     /*                                                             */ 02378000
*     /***************************************************************/ 02379000
*                                                                  0628 02380000
*     MPCATPTR=ADDR(MPCCAPTR->PCCACAT);                            0628 02381000
*                                                                  0628 02382000
         LA    MPCATPTR,PCCACAT(,MPCCAPTR)                         0628 02383000
*     /***************************************************************/ 02384000
*     /*                                                             */ 02385000
*     /* INITIALIZE THE CHANNEL AVAILABLE WORK REGISTER (MPCSREG) AND*/ 02386000
*     /* EXECUTE THE FOLLOWING CODE FOR ALL OF THE 16 POSSIBLE       */ 02387000
*     /* CHANNELS                                                    */ 02388000
*     /*                                                             */ 02389000
*     /***************************************************************/ 02390000
*                                                                  0629 02391000
*     MPCSREG=ZERO;                                                0629 02392000
         SLR   MPCSREG,MPCSREG                                     0629 02393000
*     DO N=ONE TO SIXTEEN;                                         0630 02394000
*                                                                  0630 02395000
         LA    N,1                                                 0630 02396000
@DL00630 DS    0H                                                  0631 02397000
*       /*************************************************************/ 02398000
*       /*                                                           */ 02399000
*       /* SET THE LOW ORDER BIT OF THE WORKREG ON IF CHANNEL IS OFF */ 02400000
*       /* LINE BY ADDING 1 TO THE WORKREG (MPCSREG)                 */ 02401000
*       /*                                                           */ 02402000
*       /*************************************************************/ 02403000
*                                                                  0631 02404000
*       IF CATNOP=BINARY1 THEN                                     0631 02405000
         TM    CATNOP(MPCATPTR),B'01000000'                        0631 02406000
         BNO   @RF00631                                            0631 02407000
*         MPCSREG=MPCSREG+ONE;                                     0632 02408000
*                                                                  0632 02409000
         AL    MPCSREG,@CF00063                                    0632 02410000
*       /*************************************************************/ 02411000
*       /*                                                           */ 02412000
*       /* SHIFT WORKREG LEFT BY ONE POSITION TO PREPARE FOR NEXT    */ 02413000
*       /* CHANNEL ENTRY                                             */ 02414000
*       /*                                                           */ 02415000
*       /*************************************************************/ 02416000
*                                                                  0633 02417000
*       MPCSREG=MPCSREG*TWO;                                       0633 02418000
*                                                                  0633 02419000
@RF00631 ALR   MPCSREG,MPCSREG                                     0633 02420000
*       /*************************************************************/ 02421000
*       /*                                                           */ 02422000
*       /* UPDATE MPCATPTR TO LOCATE NEXT CHANNEL ENTRY              */ 02423000
*       /*                                                           */ 02424000
*       /*************************************************************/ 02425000
*                                                                  0634 02426000
*       MPCATPTR=MPCATPTR+LENGTH(CAT);                             0634 02427000
         AL    MPCATPTR,@CF00056                                   0634 02428000
*     END;                                                         0635 02429000
*                                                                  0635 02430000
         AL    N,@CF00063                                          0635 02431000
         C     N,@CF00051                                          0635 02432000
         BNH   @DL00630                                            0635 02433000
*     /***************************************************************/ 02434000
*     /*                                                             */ 02435000
*     /* SHIFT WORKREG RIGHT BY ONE POSITION TO CORRECTLY POSITION   */ 02436000
*     /* THE CHANNEL STATUS BITS AND STORE THE CHANNEL STATUS IN THE */ 02437000
*     /* LRBCMPCS FIELD INDEXED BY INDEX                             */ 02438000
*     /*                                                             */ 02439000
*     /***************************************************************/ 02440000
*                                                                  0636 02441000
*     LRBCMPCS(INDEX)=MPCSREG/TWO;                                 0636 02442000
*                                                                  0636 02443000
         LR    @15,INDEX                                           0636 02444000
         SLA   @15,2                                               0636 02445000
         LR    @10,MPCSREG                                         0636 02446000
         SRL   @10,1                                               0636 02447000
         ALR   @15,MLRBCVPT                                        0636 02448000
         STCM  @10,3,LRBCMPCS-4(@15)                               0636 02449000
*     /***************************************************************/ 02450000
*     /*                                                             */ 02451000
*     /* DETERMINE IF THE CPU WITH THE CHANNEL ERROR IS THE ONLY CPU */ 02452000
*     /* ON LINE. IF IT IS RETURN TO IGFCCHRD, IF NOT PROCESS ALL    */ 02453000
*     /* OTHER CPU'S ON LINE                                         */ 02454000
*     /*                                                             */ 02455000
*     /***************************************************************/ 02456000
*                                                                  0637 02457000
*     IF(INDEX=ONE&PCCACCRA='1'B) THEN                             0637 02458000
         LA    @15,1                                               0637 02459000
         CR    INDEX,@15                                           0637 02460000
         BNE   @RF00637                                            0637 02461000
         TM    PCCACCRA(PCCAPTR),B'00000100'                       0637 02462000
         BNO   @RF00637                                            0637 02463000
*       DO;                         /*                       @Y30CJJE*/ 02464000
*         MPCCAPTR=PCCAPTR;                                        0639 02465000
         LR    MPCCAPTR,PCCAPTR                                    0639 02466000
*         INDEX=INDEX+ONE;                                         0640 02467000
         ALR   INDEX,@15                                           0640 02468000
*       END;                                                       0641 02469000
*     ELSE                                                         0642 02470000
*       ;                                                          0642 02471000
@RF00637 DS    0H                                                  0643 02472000
*     IF LRBCMPNO<TWO THEN                                         0643 02473000
         CLC   LRBCMPNO(2,MLRBCVPT),@CH00223                       0643 02474000
         BNL   @RF00643                                            0643 02475000
*       M=SEVNTEEN;                 /* IF ONLY 1 CPU IS ON LINE FORCE   02476000
*                                      THE LOOP TO TERMINATE BY    0644 02477000
*                                      SETTING M = 17 (UPPER LIMIT OF   02478000
*                                      LOOP IS 16)                   */ 02479000
*                                                                  0644 02480000
         LA    M,17                                                0644 02481000
*     /***************************************************************/ 02482000
*     /*                                                             */ 02483000
*     /* THIS LOOP WILL BE ENTERED ONLY WHEN MORE THAN 1 CPU IS ON   */ 02484000
*     /* LINE. WHENEVER THIS LOOP IS ENTERED N SHOULD EQUAL 17       */ 02485000
*     /* BECAUSE OF PREVIOUS LOOP USING N FOR CHANNEL STATUS INDEX   */ 02486000
*     /* (VALUE OF N IS NOT IMPORTANT, ONLY THE FACT THAT IT IS      */ 02487000
*     /* GREATER THAN 1)                                             */ 02488000
*     /*                                                             */ 02489000
*     /***************************************************************/ 02490000
*                                                                  0645 02491000
*     ELSE                                                         0645 02492000
*       DO WHILE N>ONE&M<SEVNTEEN;                                 0645 02493000
*                                                                  0645 02494000
         B     @RC00643                                            0645 02495000
@RF00643 B     @DE00645                                            0645 02496000
@DL00645 DS    0H                                                  0646 02497000
*         /***********************************************************/ 02498000
*         /*                                                         */ 02499000
*         /* UPDATE CPU ACTIVE POINTER (M) TO POINT TO THE CPU TO BE */ 02500000
*         /* TESTED (M WAS INITIALLY SET TO 0). THEN ISOLATE THE CPU */ 02501000
*         /* ACTIVE BIT FOR THIS CPU BY SHIFTING THE CPU ACTIVE BITS */ 02502000
*         /* IN THE WORKREG (CPUALREG) TO THE LEFT 15 + M POSITIONS  */ 02503000
*         /* TO MAKE BIT TO BE TESTED THE REGISTER SIGN BIT.         */ 02504000
*         /*                                                         */ 02505000
*         /***********************************************************/ 02506000
*                                                                  0646 02507000
*         M=M+ONE;                                                 0646 02508000
         AL    M,@CF00063                                          0646 02509000
*         CPUALREG=CSDCPUAL;        /* LOAD ACTIVE BITS FOR ALL 16 0647 02510000
*                                      CPU'S                         */ 02511000
         L     @15,CVTPTR                                          0647 02512000
         L     @15,CVTCSD(,@15)                                    0647 02513000
         LH    CPUALREG,CSDCPUAL(,@15)                             0647 02514000
         N     CPUALREG,@CF03517                                   0647 02515000
*         GEN REFS(CPUALREG,M)(SLL CPUALREG,15(M));                0648 02516000
*                                                                  0648 02517000
         SLL CPUALREG,15(M)                                             02518000
*         /***********************************************************/ 02519000
*         /*                                                         */ 02520000
*         /* IF THE CPU ACTIVE WORKREG (CPUALREG) CONTAINS A NEGATIVE*/ 02521000
*         /* NUMBER IT INDACATES THAT THE CPU BEING TESTED IS ON     */ 02522000
*         /* LINE. IF THIS CPU IS NOT ON LINE THEN THE NEXT ONE WILL */ 02523000
*         /* BE TESTED. IF IT IS ON LINE THEN A TEST WILL BE MADE TO */ 02524000
*         /* DETERMINE IF THIS IS THE CPU WITH THE ORIGINAL CHANNEL  */ 02525000
*         /* ERROR. IF THIS IS THE CPU WITH THE ORIGINAL CHANNEL     */ 02526000
*         /* ERROR THEN SKIP THIS ONE AS IT IS ALREADY RECORDED.     */ 02527000
*         /*                                                         */ 02528000
*         /***********************************************************/ 02529000
*                                                                  0649 02530000
*         IF CPUALREG<ZERO THEN                                    0649 02531000
         LTR   CPUALREG,CPUALREG                                   0649 02532000
         BNM   @RF00649                                            0649 02533000
*           DO;                     /* IS CPU ON LINE?               */ 02534000
*             MPCCAPTR=PCCAT00P(M); /* YES, LOCATE IT'S PCCA         */ 02535000
*                                                                  0651 02536000
         LR    @15,M                                               0651 02537000
         SLA   @15,2                                               0651 02538000
         L     @10,CVTPTR                                          0651 02539000
         L     @09,CVTPCCAT(,@10)                                  0651 02540000
         AL    @09,@CF03524                                        0651 02541000
         L     MPCCAPTR,PCCAT00P(@15,@09)                          0651 02542000
*             /*******************************************************/ 02543000
*             /*                                                     */ 02544000
*             /* IF THIS IS NOT THE CPU WITH THE ORIGINAL CHANNEL    */ 02545000
*             /* ERROR THEN LOCATE THE PSA ADDRESS OF THIS CPU,      */ 02546000
*             /* INCREMENT THE INDEX BY ONE AND FORCE LOOP TO        */ 02547000
*             /* TERMINATE BY SETTING N EQUAL TO ONE.                */ 02548000
*             /*                                                     */ 02549000
*             /*******************************************************/ 02550000
*                                                                  0652 02551000
*             IF PCCAPTR^=MPCCAPTR THEN                            0652 02552000
         CR    PCCAPTR,MPCCAPTR                                    0652 02553000
         BE    @RF00652                                            0652 02554000
*               DO;                 /* IS THIS THE PCCA OF THE     0653 02555000
*                                      ORIGINAL CPU WITH A CHANNEL 0653 02556000
*                                      ERROR?                        */ 02557000
*                 IF(PCCACCRA^='1'B|CRCAPCCA^=MPCCAPTR) THEN       0654 02558000
         TM    PCCACCRA(PCCAPTR),B'00000100'                       0654 02559000
         BNO   @RT00654                                            0654 02560000
         L     @15,CRCAPTR(,@10)                                   0654 02561000
         C     MPCCAPTR,CRCAPCCA(,@15)                             0654 02562000
         BE    @RF00654                                            0654 02563000
@RT00654 DS    0H                                                  0655 02564000
*                   DO;             /*                       @Y30CJJE*/ 02565000
*                     INDEX=INDEX+ONE;                             0656 02566000
         LA    @15,1                                               0656 02567000
         ALR   INDEX,@15                                           0656 02568000
*                     N=ONE;        /* FORCE LOOP TO TERMINATE BY  0657 02569000
*                                      SETTING N = 1         @Y30CJJE*/ 02570000
         LR    N,@15                                               0657 02571000
*                   END;                                           0658 02572000
*                 ELSE                                             0659 02573000
*                   ;                                              0659 02574000
@RF00654 DS    0H                                                  0660 02575000
*               END;                                               0660 02576000
*           END;                                                   0661 02577000
@RF00652 DS    0H                                                  0662 02578000
*       END;                                                       0662 02579000
@RF00649 DS    0H                                                  0662 02580000
@DE00645 C     N,@CF00063                                          0662 02581000
         BNH   @DC00645                                            0662 02582000
         C     M,@CF03122                                          0662 02583000
         BL    @DL00645                                            0662 02584000
@DC00645 DS    0H                                                  0663 02585000
*   END;                                                           0663 02586000
*                                                                  0663 02587000
@RC00643 DS    0H                                                  0663 02588000
@DE00626 C     M,@CF03122                                          0663 02589000
         BL    @DL00626                                            0663 02590000
*   /*****************************************************************/ 02591000
*   /*                                                               */ 02592000
*   /* RESTORE CCH REGISTERS DESTROYED BY THIS SUBROUTINE            */ 02593000
*   /*                                                               */ 02594000
*   /*****************************************************************/ 02595000
*                                                                  0664 02596000
*   UCBPTR=SAVUCB;                                                 0664 02597000
         L     UCBPTR,SAVUCB(,SAVREG)                              0664 02598000
*   ERPIBREG=SAVERPIB;                                             0665 02599000
         L     ERPIBREG,SAVERPIB(,SAVREG)                          0665 02600000
*   RESPECIFY                                                      0666 02601000
*    (N,                                                           0666 02602000
*     M,                                                           0666 02603000
*     INDEX,                                                       0666 02604000
*     MPCCAPTR,                                                    0666 02605000
*     MPCSREG,                                                     0666 02606000
*     MPCATPTR,                                                    0666 02607000
*     MLRBCVPT) UNRSTD;             /*                       @Y30CJJE*/ 02608000
*   RESPECIFY                                                      0667 02609000
*     CAT BASED(CATPTR);                                           0667 02610000
*   END IGFCCHMP;                                                  0668 02611000
@EL00007 DS    0H                                                  0668 02612000
@EF00007 DS    0H                                                  0668 02613000
@ER00007 BR    @14                                                 0668 02614000
*                                                                  0669 02615000
*/*           ASYNCHRONOUS RECORDER SUBROUTINE                       */ 02616000
*/*  START OF SPECIFICATIONS ****                                  0669 02617000
*                                                                  0669 02618000
*01*  MODULE-NAME = IGFCCHAS                                       0669 02619000
*                                                                  0669 02620000
*01*  DESCRIPTIVE-NAME = ASYNCH. RECORDER INTERFACE SUBROUTINE     0669 02621000
*                                                                  0669 02622000
*01*  COPYRIGHT = NONE                                             0669 02623000
*                                                                  0669 02624000
*01*  STATUS = VERSION 1 FOR VS/2 RELEASE 2                        0669 02625000
*                                                                  0669 02626000
*01*  FUNCTION = THE ASYNCHRONOUS RECORDER INTERFACE SUBROUTINE,   0669 02627000
*     IGFCCHAS, RECEIVES CONTROL FROM THE CENTRAL MODULE TO PERFORM THE 02628000
*     QUEUEING OF THE CCH RECORD FOR RECORDING ONTO THE SYS1.LOGREC     02629000
*     DATA SET AND THE FORMATTING AND ISSUING OF THE CCH OPERATOR  0669 02630000
*     AWARENESS MESSAGE.  BOTH THE RECORD AND THE MESSAGE ARE OUTPUTED  02631000
*     ASYNCHRONOUSLY VIA THE RECORD MACRO OF RTM.                  0669 02632000
*                                                                  0669 02633000
*01*  NOTES = NONE                                                 0669 02634000
*                                                                  0669 02635000
*02*    CHARACTER-CODE-DEPENDENCIES = EBCDIC DEPENDANT             0669 02636000
*                                                                  0669 02637000
*02*    DEPENDENCIES = NONE                                        0669 02638000
*                                                                  0669 02639000
*02*    RESTRICTIONS = NONE                                        0669 02640000
*                                                                  0669 02641000
*02*    REGISTER-CONVENTIONS = NONE                                0669 02642000
*                                                                  0669 02643000
*02*    PATCH-LABEL = NO PATCH AREA IS INCLUDED IN THIS MODULE     0669 02644000
*                                                                  0669 02645000
*01*  MODULE-TYPE = MODULE                                         0669 02646000
*                                                                  0669 02647000
*02*    PROCESSOR = PLS                                            0669 02648000
*                                                                  0669 02649000
*02*    MODULE-SIZE = 100 BYTES                                    0669 02650000
*                                                                  0669 02651000
*02*    ATTRIBUTES = REUSABLE, NUCLEUS RESIDENT, DISABLED          0669 02652000
*                                                                  0669 02653000
*01*  ENTRY-POINT = IGFCCHAS                                       0669 02654000
*                                                                  0669 02655000
*02*    PURPOSE = TO PROVIDE AN INTERFACE TO THE RTM RECORD MACRO SO    02656000
*       THAT THE LRB MAY BE MOVED TO SYS1.LOGREC AND THE CCH OPERATOR   02657000
*       AWARENESS MESSAGE MAY BE ISSUED.                           0669 02658000
*                                                                  0669 02659000
*02*    LINKAGE = CALL                                             0669 02660000
*                                                                  0669 02661000
*02*    INPUT = ADDRESS OF PCCA IN REGISTER 8                      0669 02662000
*               ADDRESS OF THE IOS SAVE AREA IN REGISTER 13        0669 02663000
*               RETURN ADDRESS IN REGISTER 14                      0669 02664000
*                                                                  0669 02665000
*02*    OUTPUT = THE CCH RECORD IS MOVED FROM THE CCH BUFFER TO THE     02666000
*       RTM'S BUFFER AND QUEUED FOR RECORDING ONTO THE SYS1.LOGREC 0669 02667000
*       DATASET AND THE CCH MESSAGE IS MOVED FROM THE CCH MESSAGE  0669 02668000
*       BUFFER TO AN RTM BUFFER AND IS QUEUED TO BE ISSUED         0669 02669000
*       ASYNCHRONOUSLY.  IF RTM IS OUT OF BUFFERS A GETMAIN    @G51BPLC 02670000
*       WILL BE ISSUED AND THE DATA COPIED TO THE GETMAINED    @G51BPLC 02671000
*       AREA WHICH WILL BE GIVEN TO RECORD.                    @G51BPLC 02672000
*                                                                  0669 02673000
*02*    EXIT-NORMAL = RETURN TO IGFCCHCR                           0669 02674000
*                                                                  0669 02675000
*02*    EXIT-ERROR = NONE                                          0669 02676000
*                                                                  0669 02677000
*01*  EXTERNAL-REFERENCES                                          0669 02678000
*                                                                  0669 02679000
*02*    ROUTINES = NONE                                            0669 02680000
*                                                                  0669 02681000
*02*    DATA-SETS = NONE                                           0669 02682000
*                                                                  0669 02683000
*02*    DATA-AREA = PCCACHSV(2),PCCACHID                           0669 02684000
*                                                                  0669 02685000
*02*    CONTROL-BLOCKS = PCCA,LRB,CDACCHR(N),CDACCHM(N)            0669 02686000
