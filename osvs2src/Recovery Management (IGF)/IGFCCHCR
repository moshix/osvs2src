*         GPR15P=ADDR(SAVEMASK);    /* GET PTR TO REG UPDATE BLOCK   */ 04128000
         LA    GPR15P,SAVEMASK(,SAVREG)                            0906 04129000
*         GPR00P=GPR15P->I015F;     /* ACCESS REGISTER BIT PATTERN   */ 04130000
