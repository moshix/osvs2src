*   RFY                                                            0687 02845000
*    (WKREG0,                                                      0687 02846000
*     WKREG1,                                                      0687 02847000
*     WKREG6,                                                      0687 02848000
*     WKREG14,                                                     0687 02849000
*     WKREG15) RSTD;                /*                       @G51BPLC*/ 02850000
*                                                                  0687 02851000
*   /*****************************************************************/ 02852000
*   /*                                                               */ 02853000
*   /* INVOKE THE RECORD MACRO PASSING TO IT THE LENGTH AND STARTING */ 02854000
*   /* ADDRESS OF THE RECORD TO BE PLACED ON SYS1.LOGREC             */ 02855000
*   /*                                                               */ 02856000
*   /*****************************************************************/ 02857000
*                                                                  0688 02858000
