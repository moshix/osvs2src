*       BC(INUSE,NOSCHED);          /* SRB IN USE - NO SCHEDULE    0821 03674000
*                                      REQUIRED              @G51BPLC*/ 03675000
         BC    4,NOSCHED                                           0821 03676000
*       SRBPTR=ADDR(PCCASRB);       /* GET SRB PTR           @G51BPLC*/ 03677000
         LA    SRBPTR,PCCASRB(,PCCAPTR)                            0822 03678000
*       SRB=SRB&&SRB;               /* CLEAR SRB             @G51BPLC*/ 03679000
