         MVC   WKAREA1(4,ADDRESS),@CB03136                         0810 03634000
*/********************************************************** @G51BPLC*/ 03635000
*/* CHECK TO SEE IF HUNG INTERFACE PROCESSING IS REQUIRED.   @G51BPLC*/ 03636000
*/* IF IT IS THE CAT ENTRY FOR THE CHANNEL WILL BE MARKED    @G51BPLC*/ 03637000
*/* BY TURNING ON CATIORST AND CATCCRST. AN SRB WILL THEN BE @G51BPLC*/ 03638000
*/* SCHEDULED TO RUN IECVIRST TO PROCESS THE CHANNEL.        @G51BPLC*/ 03639000
*/* THE SRB IS THE PCCA AND IF IT IS ALREADY IN USE THEN     @G51BPLC*/ 03640000
*/* IECVIRST HAS ALREADY BEEN SCHEDULED AND WILL PROCESS     @G51BPLC*/ 03641000
*/* THIS ERROR WITHOUT ANOTHER SCHEDULE.                     @G51BPLC*/ 03642000
*/********************************************************** @G51BPLC*/ 03643000
*                                                                  0811 03644000
*   IF PCCAISRB='1'B THEN                                          0811 03645000
         TM    PCCAISRB(PCCAPTR),B'10000000'                       0811 03646000
         BNO   @RF00811                                            0811 03647000
*     DO;                           /*                       @G51BPLC*/ 03648000
*       RFY                                                        0813 03649000
*        (CATPTR,                                                  0813 03650000
*         SRBPTR) RSTD;             /*                       @G51BPLC*/ 03651000
*       WKREG4=FLCIOA&CHANONLY;     /* GET CHAN PORTION OF CUA     0814 03652000
*                                                            @G51BPLC*/ 03653000
         L     WKREG4,FLCIOA                                       0814 03654000
         N     WKREG4,@CF03462                                     0814 03655000
*       WKREG4=WKREG4/256;          /* DIVIDE BY 256 TO GET CHANNEL     03656000
*                                      NUMBER                @G51BPLC*/ 03657000
         SRL   WKREG4,8                                            0815 03658000
*       CATPTR=ADDR(PCCACAT)+WKREG4*LENGTH(CAT);/* GET CAT ENTRY   0816 03659000
*                                                            @G51BPLC*/ 03660000
         LA    CATPTR,PCCACAT(,PCCAPTR)                            0816 03661000
         LR    @15,WKREG4                                          0816 03662000
         SLA   @15,3                                               0816 03663000
         ALR   CATPTR,@15                                          0816 03664000
*       CATIORST='1'B;              /* INDICATE CHANNEL REQUIRES   0817 03665000
*                                      PROCESSING            @G51BPLC*/ 03666000
*       CATCCRST='1'B;              /* DUE TO CHANNEL ERROR  @G51BPLC*/ 03667000
         OI    CATIORST(CATPTR),B'01100000'                        0818 03668000
*       CATNOP='1'B;                /* PREVENT USE OF CHANNEL      0819 03669000
*                                                            @G51BPLC*/ 03670000
         OI    CATNOP(CATPTR),B'01000000'                          0819 03671000
*       TS(PCCASRBL);               /* RESERVE SRB IF AVAIL  @G51BPLC*/ 03672000
