@RC00104 L     @14,PCCAPTR                                         0124 00291000
         L     SRBPTR,PCCAPWAV(,@14)                               0124 00292000
*       GEN(LA    SRBPTR,PWACCSRB(SRBPTR));/* VIRTUAL ADDRESS OF SRB */ 00293000
         LA    SRBPTR,PWACCSRB(SRBPTR)                                  00294000
*       GEN(STOSM  0(R13),X'04');   /* DAT ON FOR SCHEDULE BECAUSE 0126 00295000
*                                      SRB IN VIRTUAL STORAGE - SQA  */ 00296000
         STOSM  0(R13),X'04'                                            00297000
*       GEN(SCHEDULE SRB=(SRBPTR),SCOPE=GLOBAL);/* SCHEDULE I/O    0127 00298000
*                                      RESTART                       */ 00299000
         SCHEDULE SRB=(SRBPTR),SCOPE=GLOBAL                             00300000
*       GEN(STNSM  3(SRBPTR),X'FB');/* TURN DAT OFF, TARGET OF ST  0128 00301000
*                                      MUST BE VIRTUAL ADDR          */ 00302000
         STNSM  3(SRBPTR),X'FB'                                         00303000
*       RFY                                                        0129 00304000
*         SRBPTR UNRSTD;                                           0129 00305000
*DNOTSCH:                                                          0130 00306000
*       ;                           /* IF SRB IN USE, SCH IS BYPASSED*/ 00307000
DNOTSCH  DS    0H                                                  0131 00308000
*     END;                                                         0131 00309000
*   ELSE                                                           0132 00310000
*     ;                                                            0132 00311000
@RF00093 DS    0H                                                  0133 00312000
*   RFY                                                            0133 00313000
