         TITLE 'IGFCCHED - CCH EXTERNAL DAMAGE MACHINE CHECK PROCESSOR *00001000
                        '                                               00002000
IGFCCHED CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGFCCHED  77.139'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*   LRBPTR=R1;                      /* ADDR OF LRB                   */ 00014000
         LR    LRBPTR,R1                                           0058 00015000
*   RFY                                                            0059 00016000
*     R1 UNRSTD;                                                   0059 00017000
*   RFY                                                            0060 00018000
*     R15 RSTD;                     /* RET IND FOR LOST CHANNELS     */ 00019000
*   R15=0;                          /* INIT - NO LOST CHANNELS       */ 00020000
*                                                                  0061 00021000
         SLR   R15,R15                                             0061 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* THIS ROUTINE ANALYZES THE CHANNELS IN THIS CPU'S CHANNEL      */ 00025000
*   /* AVAILABILITY TABLE (CAT). IF A CHANNEL IS INDICATED AS        */ 00026000
*   /* AVAILABLE, A TCH INSTRUCTION IS ISSUED FOR THE CHANNEL. A     */ 00027000
*   /* CONDITION CODE OF 3 REFLECTS A LOST CHANNEL. ANY OTHER        */ 00028000
*   /* CONDITION CODE IS IGNORED. IF A CHANNEL IS LOST (CC=3 FOR ANY */ 00029000
*   /* TCH INSTR), R15 IS SET TO ^0. IF ALL INDICATED AVAILABLE      */ 00030000
*   /* CHANNELS ARE REALLY AVAILABLE (NO CC=3), R15 IS SET TO 0.     */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                                  0062 00034000
*   RFY                                                            0062 00035000
*    (CATPTR,                                                      0062 00036000
*     I) RSTD;                      /* CATPTR=R3, I=R4               */ 00037000
*   CATPTR=ADDR(PCCACAT);           /* POINT TO FIRST ENTRY IN CAT   */ 00038000
         L     CATPTR,PCCAPTR                                      0063 00039000
         LA    CATPTR,PCCACAT(,CATPTR)                             0063 00040000
*   DO I=1 TO LENGTH(PCCACAT)/LENGTH(CAT);/* CHECK EACH ENTRY IN CAT */ 00041000
         LA    I,1                                                 0064 00042000
@DL00064 DS    0H                                                  0065 00043000
*     IF(CATNOP='0'B&               /* CHANNEL OPERATIONAL           */ 00044000
*         CATNGEN='0'B&             /* CHANNEL HAS BEEN SYSGENED     */ 00045000
*         CATNCPU='0'B&             /* CHANNEL ON THIS CPU           */ 00046000
*         CATNID='0'B)|             /* CHANNEL ID (CATCHID) VALID    */ 00047000
*         (CHAN(I)='0600'X&         /* CHANNEL IS CRH CONNECTION     */ 00048000
*         CVTCRCA^=0&               /* CRH IS ACTIVE                 */ 00049000
*         CRCADIAG='1'B) THEN       /* A CRH CONNECT IS OUTSTANDING  */ 00050000
         TM    CATNOP(CATPTR),B'01111000'                          0065 00051000
         BZ    @RT00065                                            0065 00052000
         LR    @14,I                                               0065 00053000
         ALR   @14,@14                                             0065 00054000
         LA    @05,CHAN-2(@14)                                     0065 00055000
         CLC   0(2,@05),@CB01635                                   0065 00056000
         BNE   @RF00065                                            0065 00057000
         L     @14,CVTPTR                                          0065 00058000
         L     @05,CVTCRCA(,@14)                                   0065 00059000
         LTR   @05,@05                                             0065 00060000
         BZ    @RF00065                                            0065 00061000
         L     @14,CRCAPTR(,@14)                                   0065 00062000
         TM    CRCADIAG(@14),B'10000000'                           0065 00063000
         BNO   @RF00065                                            0065 00064000
@RT00065 DS    0H                                                  0066 00065000
*       DO;                         /* CHECK IF LOST THIS CHANNEL    */ 00066000
*         TCH(CHAN(I));             /* DETERMINE IF CHAN STILL AVAIL */ 00067000
         LR    @14,I                                               0067 00068000
         ALR   @14,@14                                             0067 00069000
         LA    @05,CHAN-2(@14)                                     0067 00070000
         SLR   @14,@14                                             0067 00071000
         ICM   @14,3,0(@05)                                        0067 00072000
         TCH   0(@14)                                              0067 00073000
*         BC(NOTCC3,CHANOK);        /* B NOT CC=3, CHAN STILL AVAIL  */ 00074000
         BC    14,CHANOK                                           0068 00075000
*         IF CHAN(I)='0600'X&       /* IF UNAVAILABLE CHAN IS THE ONE   00076000
*                                      CRH USES TO CONNECT OFFLINE 0069 00077000
*                                      CPU'S CHANNELS AND            */ 00078000
*             CVTCRCA^=0&           /* CRH IS ACTIVE AND             */ 00079000
*             CRCADIAG='1'B THEN    /* A CRH CONNECT IS OUTSTANDING  */ 00080000
         LR    @14,I                                               0069 00081000
         ALR   @14,@14                                             0069 00082000
         LA    @05,CHAN-2(@14)                                     0069 00083000
         CLC   0(2,@05),@CB01635                                   0069 00084000
         BNE   @RF00069                                            0069 00085000
         L     @14,CVTPTR                                          0069 00086000
         L     @05,CVTCRCA(,@14)                                   0069 00087000
         LTR   @05,@05                                             0069 00088000
         BZ    @RF00069                                            0069 00089000
         L     @14,CRCAPTR(,@14)                                   0069 00090000
         TM    CRCADIAG(@14),B'10000000'                           0069 00091000
         BNO   @RF00069                                            0069 00092000
*           DO;                     /* LOST CHANNEL IS NOT OUR     0070 00093000
*                                      CHANNEL BUT THE CHANNEL ON THE   00094000
*                                      OFFLINE CPU THAT CRH IS     0070 00095000
*                                      CONNECTED TO                  */ 00096000
*             R15=CRCACAT;          /* CIR ADDR OF OFFLINE CPU CAT   */ 00097000
         L     R15,CRCACAT(,@14)                                   0071 00098000
*             GEN(LRA R15,0(R15));  /* REAL ADDR OF CAT              */ 00099000
         LRA R15,0(R15)                                                 00100000
*             EVAL(R15+((CRCAMCWF&'0F'X)*LENGTH(CAT)))->CATNOP='1'B;    00101000
         L     @14,CVTPTR                                          0073 00102000
         L     @14,CRCAPTR(,@14)                                   0073 00103000
         LA    @05,15                                              0073 00104000
         SLR   @11,@11                                             0073 00105000
         IC    @11,CRCAMCWF(,@14)                                  0073 00106000
         NR    @05,@11                                             0073 00107000
         SLA   @05,3                                               0073 00108000
         LR    @14,R15                                             0073 00109000
         ALR   @14,@05                                             0073 00110000
         OI    CATNOP(@14),B'01000000'                             0073 00111000
*                                   /* IND THAT CHAN NOT OPERATIONAL */ 00112000
*             EVAL(R15+((CRCAMCWF&'0F'X)*LENGTH(CAT)))->CATIORST='1'B;  00113000
         ALR   @05,R15                                             0074 00114000
         OI    CATIORST(@05),B'01000000'                           0074 00115000
*                                   /* IND THAT CHAN NEEDS I/O     0075 00116000
*                                      RESTART                       */ 00117000
*             R15=8;                /* CRH LOST A CHANNEL            */ 00118000
         LA    R15,8                                               0075 00119000
*           END;                                                   0076 00120000
*         ELSE                                                     0077 00121000
*           DO;                                                    0077 00122000
         B     @RC00069                                            0077 00123000
@RF00069 DS    0H                                                  0078 00124000
*             CATNOP='1'B;          /* THIS CPU LOST CHANNEL         */ 00125000
         OI    CATNOP(CATPTR),B'01000000'                          0078 00126000
*             CATIORST='1'B;        /* CHANNEL NEEDS I/O RESTART     */ 00127000
         OI    CATIORST(CATPTR),B'01000000'                        0079 00128000
*             IF LRBMVED='0'B|      /* IF EXTERNAL DAMAGE REASON CODE   00129000
*                                      IS NOT VALID EITHER THE CLRCH    00130000
*                                      IS NOT SUPPORTED OR A DOUBLE     00131000
*                                      ERROR HAS OCCURRED, SCHEDULE     00132000
*                                      IECVRSTI. IT ASSUMES ANY    0080 00133000
*                                      RESERVES HAVE BEEN RELEASED 0080 00134000
*                                      AND WILL LOAD A WAIT STATE IF    00135000
*                                      ANY DEVICES ON THE LOST     0080 00136000
*                                      CHANNEL(S) WERE RESERVED.   0080 00137000
*                                                            @G51BPLM*/ 00138000
*                 (LRBMVED='1'B&    /* EXT DAM REASON CODE IS VALID     00139000
*                                      WITH                  @G51BPLM*/ 00140000
*                 LRBMCCF='1'B) THEN/* CHANNEL CONTROL FAILURE,    0080 00141000
*                                      RESERVES MAY HAVE BEEN      0080 00142000
*                                      RELEASED DUE TO A POWER     0080 00143000
*                                      FAILURE               @G51BPLM*/ 00144000
         TM    LRBMVED(LRBPTR),B'00100000'                         0080 00145000
         BZ    @RT00080                                            0080 00146000
         TM    LRBMVED(LRBPTR),B'00100000'                         0080 00147000
         BNO   @RF00080                                            0080 00148000
         TM    LRBMCCF(LRBPTR),B'00001000'                         0080 00149000
         BNO   @RF00080                                            0080 00150000
@RT00080 DS    0H                                                  0081 00151000
*               R15=4;              /* SCHED IECVRSTI        @G51BPLM*/ 00152000
         LA    R15,4                                               0081 00153000
*             ELSE                  /*                       @G51BPLM*/ 00154000
*               DO;                 /* NEW CHANNELS          @G51BPLM*/ 00155000
         B     @RC00080                                            0082 00156000
@RF00080 DS    0H                                                  0083 00157000
*                 CATMCRST='1'B;    /* IND DETECTED ON MC    @G51BPLM*/ 00158000
         OI    CATMCRST(CATPTR),B'00010000'                        0083 00159000
*                 R15=12;           /* SCHEDULE IECVIRST     @G51BPLM*/ 00160000
         LA    R15,12                                              0084 00161000
*               END;                /*                       @G51BPLM*/ 00162000
*           END;                                                   0086 00163000
@RC00080 DS    0H                                                  0087 00164000
*CHANOK:                                                           0087 00165000
*         ;                         /* TARGET OF BC(NOTCC3,CHANOK)   */ 00166000
@RC00069 DS    0H                                                  0087 00167000
CHANOK   DS    0H                                                  0088 00168000
*       END;                                                       0088 00169000
*     ELSE                                                         0089 00170000
*       ;                                                          0089 00171000
@RF00065 DS    0H                                                  0090 00172000
*     CATPTR=CATPTR+LENGTH(CAT);    /* POINT TO NEXT ENTRY IN CAT    */ 00173000
         AL    CATPTR,@CF00068                                     0090 00174000
*   END;                                                           0091 00175000
         AL    I,@CF00070                                          0091 00176000
         C     I,@CF00798                                          0091 00177000
         BNH   @DL00064                                            0091 00178000
*   RFY                                                            0092 00179000
*    (CATPTR,                                                      0092 00180000
*     I) UNRSTD;                                                   0092 00181000
*   IF R15^=0 THEN                  /* IF CHANNELS LOST              */ 00182000
         LTR   R15,R15                                             0093 00183000
         BZ    @RF00093                                            0093 00184000
*     DO;                                                          0094 00185000
*                                                                  0094 00186000
*       /*************************************************************/ 00187000
*       /*                                                           */ 00188000
*       /* THIS ROUTINE WILL INVOKE IECVRSTI/IECVIRST TO DO I/O      */ 00189000
*       /* RESTART PROCESSING FOR A LOST CHANNEL. THERE IS ONE SRB   */ 00190000
*       /* FOR EACH PROCESSOR LOCATED IN THE PWA. A TS INSTR IS USED */ 00191000
*       /* TO CHECK THE AVAILABILITY OF THE SRB AND RESERVE IT IF    */ 00192000
*       /* AVAILABLE. IF NOT AVAILABLE, IT IS ON THE SCHEDULE QUEUE  */ 00193000
*       /* FOR A PREVIOUS LOST CHANNEL FROM THIS PROCESSOR. WHEN I/O */ 00194000
*       /* RESTART RECEIVES CONTROL, IT WILL PROCESS ALL LOST        */ 00195000
*       /* CHANNELS. THE SRB IS SCHEDULED WITH AFFINITY TO THIS      */ 00196000
*       /* PROCESSOR. I/O RESTART WILL BE ABLE TO REFERENCE THIS     */ 00197000
*       /* PROCESSORS CONTROL BLOCKS. IF THIS PROCESSOR FAILS (ACR), */ 00198000
*       /* IECVRSTI WILL NOT PROCESS THE LOST CHANNEL. IT WOULD DO NO*/ 00199000
*       /* GOOD TO HAVE IECVRSTI RUN ON ANOTHER PROCESSOR FOR A      */ 00200000
*       /* CHANNEL LOST FROM THIS PROCESSOR AS IECVRSTI WOULD HAVE   */ 00201000
*       /* ALREADY DONE I/O RESTART PROCESSING FOR THIS PROCESSOR,   */ 00202000
*       /* BEING CALLED BY ACR. R15=4 OR 8 SCHEDULE IECVRSTI, R15=12 */ 00203000
*       /* SCHEDULE IECVIRST                                         */ 00204000
*       /*                                                           */ 00205000
*       /*************************************************************/ 00206000
*                                                                  0095 00207000
*       TS(PWAEDSRB);               /* IF SRB AVAIL, RESERVE IT ELSE    00208000
*                                      SET CONDITION CODE            */ 00209000
         L     @14,PCCAPTR                                         0095 00210000
         L     @14,PWAPTR(,@14)                                    0095 00211000
         TS    PWAEDSRB(@14)                                       0095 00212000
*       BC(INUSE,DNOTSCH);          /* IF ALREADY SET, BYPASS      0096 00213000
*                                      SCHEDULE                      */ 00214000
         BC    4,DNOTSCH                                           0096 00215000
*       RFY                                                        0097 00216000
*         SRBPTR RSTD;              /* SRBPTR = R3                   */ 00217000
*       SRBPTR=ADDR(PWACCSRB);      /* SRB IS IN PROCESSOR WORK AREA */ 00218000
         L     @14,PCCAPTR                                         0098 00219000
         L     SRBPTR,PWAPTR(,@14)                                 0098 00220000
         LA    SRBPTR,PWACCSRB(,SRBPTR)                            0098 00221000
*       SRB=SRB&&SRB;               /* CLEAR SRB                     */ 00222000
         XC    SRB(44,SRBPTR),SRB(SRBPTR)                          0099 00223000
*       SRBID='SRB ';               /* IDENTIFY SRB                  */ 00224000
         MVC   SRBID(4,SRBPTR),@CC01684                            0100 00225000
*       SRBASCB=ADDR(IEAMASCB);     /* SCHEDULE INTO MASTER ADDR   0101 00226000
*                                      SPACE                         */ 00227000
         L     @11,@CV01656                                        0101 00228000
         ST    @11,SRBASCB(,SRBPTR)                                0101 00229000
*       SRBCPAFF=PCCACAFM;          /* SCH SRB ON THIS PROCESSOR - 0102 00230000
*                                      ONE THAT LOST THE CHANNEL   0102 00231000
*                                                            @G51BPLM*/ 00232000
         MVC   SRBCPAFF(2,SRBPTR),PCCACAFM(@14)                    0102 00233000
*       SRBRMTR=ADDR(CVTBRET);      /* RESOURCE MGR-BR14 INST      0103 00234000
*                                                            @G51BPLM*/ 00235000
         L     @01,CVTPTR                                          0103 00236000
         LA    @11,CVTBRET(,@01)                                   0103 00237000
         ST    @11,SRBRMTR(,SRBPTR)                                0103 00238000
*       IF R15=12 THEN              /* IF IECVIRST TO BE SCHD      0104 00239000
*                                                            @G51BPLM*/ 00240000
         C     R15,@CF00764                                        0104 00241000
         BNE   @RF00104                                            0104 00242000
*         DO;                       /*                       @G51BPLM*/ 00243000
*           IF PCCAIRST='1'B THEN   /* IF IECVIRST PROCESSING      0106 00244000
*                                                            @G51BPLM*/ 00245000
         TM    PCCAIRST(@14),B'10000000'                           0106 00246000
         BNO   @RF00106                                            0106 00247000
*             DO;                   /* INDICATE 'MORE' CHAN  @G51BPLM*/ 00248000
*               PCCAEXDM='1'B;      /* LOST SO IECVIRST WILL @G51BPLM   00249000
*                                      LOAD A NON-RESTRT WAIT      0108 00250000
*                                                            @G51BPLM*/ 00251000
         OI    PCCAEXDM(@14),B'01000000'                           0108 00252000
*               GO TO DNOTSCH;      /* DO NOT SCHEDULE SRB   @G51BPLM*/ 00253000
         B     DNOTSCH                                             0109 00254000
*             END;                  /*                       @G51BPLM*/ 00255000
*           ELSE                    /* IECVIRST NOT IN PROGRS      0111 00256000
*                                                            @G51BPLM*/ 00257000
*             DO;                   /*                       @G51BPLM*/ 00258000
@RF00106 DS    0H                                                  0112 00259000
*               SRBEP=ADDR(IECVIRST);/* EP ADR-NEW CHANNEL SUP     0112 00260000
*                                                            @G51BPLM*/ 00261000
         L     @14,@CV01655                                        0112 00262000
         ST    @14,SRBEP(,SRBPTR)                                  0112 00263000
*               IOREDID='01'X;      /* PARM FOR IECVIRST     @G51BPLM*/ 00264000
         MVI   IOREDID(SRBPTR),X'01'                               0113 00265000
*             END;                  /*                       @G51BPLM*/ 00266000
*         END;                      /*                       @G51BPLM*/ 00267000
*       ELSE                        /* IECVRSTI IS TO BE     @G51BPLM*/ 00268000
*         DO;                       /* SCHEDULED             @G51BPLM*/ 00269000
         B     @RC00104                                            0116 00270000
@RF00104 DS    0H                                                  0117 00271000
*           SRBEP=ADDR(IECVRSTI);   /* EP ADDR IECVRSTI      @G51BPLM*/ 00272000
         L     @14,@CV01654                                        0117 00273000
         ST    @14,SRBEP(,SRBPTR)                                  0117 00274000
*           IOREDID='03'X;          /* PARM FOR IECVRSTI     @G51BPLM*/ 00275000
         MVI   IOREDID(SRBPTR),X'03'                               0118 00276000
*           IORCPUID=PSACPUPA;      /* PHY ID FOR IECVRSTI   @G51BPLM*/ 00277000
         LH    @14,PSACPUPA                                        0119 00278000
         STH   @14,IORCPUID(,SRBPTR)                               0119 00279000
*           IF R15=8 THEN           /* IF CRH CHAN LOST      @G51BPLM*/ 00280000
         C     R15,@CF00068                                        0120 00281000
         BNE   @RF00120                                            0120 00282000
*             IORCRHLS='1'B;        /* INDICATE CRH CHAN LOST      0121 00283000
*                                                            @G51BPLM*/ 00284000
         OI    IORCRHLS(SRBPTR),B'10000000'                        0121 00285000
