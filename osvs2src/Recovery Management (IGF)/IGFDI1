*         DERRETRY=ADDR(IGFDI1R);   /* POINT TO RETRY ROUTINE        */ 00094000
         LA    @14,IGFDI1R                                         0267 00095000
         ST    @14,DERRETRY(,DERPTR)                               0267 00096000
*         GEN SETS(DERDATD) REFS(DERPTR)                           0268 00097000
*             (MVC DERDATD(4,DERPTR),@SIZDATD)   ;/* SET UP SPN, SIZE*/ 00098000
         MVC DERDATD(4,DERPTR),@SIZDATD                                 00099000
*         DERPFX=DERRMASK;          /* SET UP RUB MASK               */ 00100000
         MVC   DERPFX(2,DERPTR),@CB01166                           0269 00101000
*         DERRCODE=CODEBASE;        /* SAVE CODEREG                  */ 00102000
         ST    CODEBASE,DERRCODE(,DERPTR)                          0270 00103000
*         DERRDATA=DATABASE;        /* SAVE DATAREG                  */ 00104000
         ST    DATABASE,DERRDATA(,DERPTR)                          0271 00105000
*         DERRSAVE=SAVEPTR;         /* SAVE REGISTER 13              */ 00106000
         ST    SAVEPTR,DERRSAVE(,DERPTR)                           0272 00107000
*         CALL IGFDE0(DERPLIST);    /* ISSUE ESTAE                   */ 00108000
         ST    DERPTR,@AL00001                                     0273 00109000
         L     @15,@CV00100                                        0273 00110000
         LA    @01,@AL00001                                        0273 00111000
         BALR  @14,@15                                             0273 00112000
*       END;                        /* ?IGFDERR INIT NODDRCOM        */ 00113000
*                                                                  0274 00114000
*       /*************************************************************/ 00115000
*       /*                                                           */ 00116000
*       /* INFORM JES3 THAT PROCESSING FOR CURRENT SWAP REQUEST IS   */ 00117000
*       /* COMPLETED                                         @Y02BKCI*/ 00118000
*       /*                                                           */ 00119000
*       /*************************************************************/ 00120000
*                                                                  0275 00121000
*       SSOBFUNC=SSOBDDR4;          /* SET FUNCTION CODE FOR SWAP  0275 00122000
*                                      PROCESSING COMPLETED  @Y02BKCI*/ 00123000
         L     @14,DDRSSOB(,DDRPTR)                                0275 00124000
         MVC   SSOBFUNC(2,@14),@CH00044                            0275 00125000
*       IF DDRTER1=0 THEN           /* Q. SWAP SUCCESSFUL    @Y02BKCI*/ 00126000
