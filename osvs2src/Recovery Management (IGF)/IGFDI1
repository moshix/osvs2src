*             /*                                                     */ 00209000
*             /* IGFDMSG - INVOKE THE DDR/MIH MESSAGE WRITER         */ 00210000
*             /*                                                     */ 00211000
*             /*******************************************************/ 00212000
*                                                                  0288 00213000
*             DDRMSGCD=DDRM512I;    /* SET UP MSG CODE               */ 00214000
