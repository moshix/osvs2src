         TITLE 'IGFDI1, DDR TERMINATOR                                 *00001000
                        '                                               00002000
IGFDI1   CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGFDI1    77.223'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @08,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@08                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @09,@01                                             0001 00016000
         USING @DATD,@09                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @09,8(,@13)                                         0001 00020000
         LR    @13,@09                                             0001 00021000
*                                                                  0251 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* INITIALIZE INTERNAL WORK AREAS                                */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                                  0251 00028000
*   ABTERMSW='0'B;                  /* NOT ABNORMAL TERMINATION      */ 00029000
*   DROPQSW='0'B;                   /* DO NOT TERMINATE ENTIRE QUEUE */ 00030000
*   TAPESW='0'B;                    /* NOT TAPE SWAP COMPLETE YET  0253 00031000
*                                                            @YM07712*/ 00032000
         NI    ABTERMSW,B'00011111'                                0253 00033000
*   ORIGUCB=0;                      /* NO UCB DETERMINED YET         */ 00034000
         SLR   ORIGUCB,ORIGUCB                                     0254 00035000
*   ASCBPTR=R1;                     /* SAVE ADDRESS OF CURRENT ASCB  */ 00036000
         LR    ASCBPTR,R1                                          0255 00037000
*   FROMUCB=0;                      /* FROM UCB NOT FOUND YET        */ 00038000
         SLR   FROMUCB,FROMUCB                                     0256 00039000
*   TOUCB=0;                        /* TO UCB NOT FOUND YET          */ 00040000
*                                                                  0257 00041000
         SLR   TOUCB,TOUCB                                         0257 00042000
*   /*****************************************************************/ 00043000
*   /*                                                               */ 00044000
*   /* DETERMINE ENTRY CONDITION (NORMAL OR ESOTERIC EXIT)           */ 00045000
*   /*                                                               */ 00046000
*   /*****************************************************************/ 00047000
*                                                                  0258 00048000
*   IF DDRPTR=0                     /* NO DDRCOM ADDRESS - KILL QUEUE*/ 00049000
*       |(DERPTR^=0&DERCRASH='1'B)  /* ESOTERIC EXIT - DDR CANNOT  0258 00050000
*                                      RECOVER - TERMINATE QUEUE     */ 00051000
*       |(SDWAPTR>0&SDWACLUP='1'B)  /* ESOTERIC EXIT - DDR CANNOT  0258 00052000
*                                      RECOVER - TERMINATE QUEUE     */ 00053000
*     THEN                          /* MUST TERMINATE DDR WAIT QUEUE */ 00054000
         SLR   @14,@14                                             0258 00055000
         CR    DDRPTR,@14                                          0258 00056000
         BE    @RT00258                                            0258 00057000
         CR    DERPTR,@14                                          0258 00058000
         BE    @GL00001                                            0258 00059000
         TM    DERCRASH(DERPTR),B'10000000'                        0258 00060000
         BO    @RT00258                                            0258 00061000
@GL00001 LTR   SDWAPTR,SDWAPTR                                     0258 00062000
         BNP   @RF00258                                            0258 00063000
         LR    @14,SDWAPTR                                         0258 00064000
         TM    SDWACLUP(@14),B'10000000'                           0258 00065000
         BNO   @RF00258                                            0258 00066000
@RT00258 DS    0H                                                  0259 00067000
*     DROPQSW='1'B;                 /* INDICATE DROP QUEUE REQUIRED  */ 00068000
         OI    DROPQSW,B'01000000'                                 0259 00069000
*   ELSE                            /* NOT FATAL CONDITION           */ 00070000
*     DO;                           /* SINGLE REQUEST TO PROCESS     */ 00071000
         B     @RC00258                                            0260 00072000
@RF00258 DS    0H                                                  0261 00073000
*       DO;                         /* ?IGFDERR INIT NODDRCOM        */ 00074000
*                                                                  0261 00075000
*         /***********************************************************/ 00076000
*         /*                                                         */ 00077000
*         /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT               */ 00078000
*         /*                                                         */ 00079000
*         /***********************************************************/ 00080000
*                                                                  0262 00081000
*         DERPTR=ADDR(DERAREA);     /* POINT TO DERPLIST AREA        */ 00082000
         LA    DERPTR,DERAREA                                      0262 00083000
*         DERPLIST=DERPLIST&&DERPLIST;/* CLEAR PARAMETER LIST        */ 00084000
