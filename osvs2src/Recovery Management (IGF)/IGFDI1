         STC   @14,DDRRETRY(,DDRPTR)                               0293 00227000
@DL00293 CH    RTNCODE,@CH00128                                    0293 00228000
         BNE   @DC00293                                            0293 00229000
*               CALL IGFDM0;        /* OUTPUT MESSAGE                */ 00230000
         L     @15,@CV00101                                        0294 00231000
