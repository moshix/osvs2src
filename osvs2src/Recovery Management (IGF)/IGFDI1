*             DDRMSGPN=2;           /* SET UP NO. OF SUBST. CODES    */ 00216000
         MVI   DDRMSGPN(DDRPTR),X'02'                              0289 00217000
*             DDRMSGPC(1)=DDRPFM;   /* SET SUBSTITUTION CODE 1       */ 00218000
         MVI   DDRMSGPC(DDRPTR),X'02'                              0290 00219000
*             DDRMSGPC(2)=DDRPTM;   /* SET SUBSTITUTION CODE 2       */ 00220000
         MVI   DDRMSGPC+1(DDRPTR),X'07'                            0291 00221000
*             RTNCODE=12;           /* INDICATE MESSAGE NOT DONE YET */ 00222000
