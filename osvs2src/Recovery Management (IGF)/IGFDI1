*         DO;                       /* ABNORMAL TERMINATION          */ 00195000
*           ABTERMSW='1'B;          /* INDICATE ABNORMAL TERMINATION */ 00196000
*                                                                  0286 00197000
         OI    ABTERMSW,B'10000000'                                0286 00198000
*           /*********************************************************/ 00199000
*           /*                                                       */ 00200000
*           /* IGF512I SWAP FROM XXX TERMINATED - REASON             */ 00201000
*           /*                                                       */ 00202000
*           /*********************************************************/ 00203000
*                                                                  0287 00204000
*           DO;                     /* ?IGFDMSG MSG(IGF512I)RETRY(2)C   00205000
