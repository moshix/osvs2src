         L     @14,SSOBINDV(,@14)                                  0278 00141000
         NI    SSDR4SWP(@14),B'11110111'                           0278 00142000
*       /*************************************************************/ 00143000
*       /*                                                           */ 00144000
*       /* ? IEFSSREQ (SSOB);                                        */ 00145000
*       /*                                                           */ 00146000
*       /*************************************************************/ 00147000
*                                                                  0279 00148000
*/*                                                                  */ 00149000
*/*  PASS CONTROL TO JOB ENTRY SUBSYSTEM TO                          */ 00150000
*/*  PROCESS REQUEST -                                               */ 00151000
*/*                                                                  */ 00152000
*/*    INPUT - REGISTER 1 POINTS TO A ONE-                           */ 00153000
*/*            WORD PARAMETER LIST WHICH                             */ 00154000
*/*            POINTS TO THE SSOB                                    */ 00155000
*/*                                                                  */ 00156000
*                                                                  0279 00157000
*       CALL SSREQ(SSOB);           /* ATTEMPT TO COMMUNICATE WITH 0279 00158000
*                                      JES3                  @Y02BKCI*/ 00159000
@RC00276 L     @14,DDRSSOB(,DDRPTR)                                0279 00160000
         ST    @14,@AL00001                                        0279 00161000
         MVI   @AL00001,X'80'                                      0279 00162000
         L     @12,CVTPTR                                          0279 00163000
         L     @12,CVTJESCT(,@12)                                  0279 00164000
         L     @15,JESSSREQ(,@12)                                  0279 00165000
         LA    @01,@AL00001                                        0279 00166000
         BALR  @14,@15                                             0279 00167000
*       IF^(RTNCODE=SSRTOK|RTNCODE=SSRTNSUP|RTNCODE=SSRTNTUP) THEN/*    00168000
*                                      Q. RETURN CODE ACCEPTABLE   0280 00169000
*                                                            @Y02BKCI*/ 00170000
