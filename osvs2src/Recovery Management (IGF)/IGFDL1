*             DO;                   /* THEN, RETURN CODE FROM SSREQ     00153000
*                                      OR JES3 IS INVALID OR       0161 00154000
*                                      INDICATES LOGIC ERROR @Y02BKCI*/ 00155000
*               GEN(ABEND X'C0D',DUMP,,SYSTEM);/* FORCE ENTRY TO   0162 00156000
*                                      ESTAE HANDLER (IGFDE1)      0162 00157000
*                                                            @Y02BKCI*/ 00158000
         ABEND X'C0D',DUMP,,SYSTEM                                      00159000
*             END;                                                 0163 00160000
*           ELSE                                                   0164 00161000
*             DO;                                                  0164 00162000
