         ST    DATABASE,DERRDATA                                   0144 00048000
*     DERRDDR=DDRPTR;               /* SAVE DDRCOM POINTER           */ 00049000
         ST    DDRPTR,DERRDDR                                      0145 00050000
*     DERRSAVE=SAVEPTR;             /* SAVE REGISTER 13              */ 00051000
         ST    SAVEPTR,DERRSAVE                                    0146 00052000
*     CALL IGFDE0(DERPLIST);        /* ISSUE ESTAE                   */ 00053000
         LA    @14,DERPLIST                                        0147 00054000
         ST    @14,@AL00001                                        0147 00055000
         L     @15,@CV01555                                        0147 00056000
         LA    @01,@AL00001                                        0147 00057000
         BALR  @14,@15                                             0147 00058000
*   END;                            /* ?IGFDERR INIT ESTAE         0148 00059000
*                                      ESTABLISHED                   */ 00060000
*   CHOICE=0;                       /* 'TO' DEVICE TENTATIVE CHOICE  */ 00061000
         SLR   CHOICE,CHOICE                                       0149 00062000
*   DERLRC=4;                       /* SET LOCAL RETURN CODE         */ 00063000
         MVI   DERLRC,X'04'                                        0150 00064000
*   LOOPVAR=4;                      /* INIT LOOP CONTROL VARIABLE  0151 00065000
*                                                            @Y02BKCI*/ 00066000
