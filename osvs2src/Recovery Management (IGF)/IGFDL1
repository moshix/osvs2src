         L     @15,JESSSREQ(,@12)                                  0156 00115000
         LA    @01,@AL00001                                        0156 00116000
         BALR  @14,@15                                             0156 00117000
*     IF^(RTNCODE=SSRTNSUP|RTNCODE=SSRTNTUP) THEN/* IF JES3 NOT    0157 00118000
*                                      INACTIVE              @Y02BKCI*/ 00119000
         C     RTNCODE,@CF00055                                    0157 00120000
         BE    @RF00157                                            0157 00121000
         C     RTNCODE,@CF00061                                    0157 00122000
         BE    @RF00157                                            0157 00123000
*       IF^(RTNCODE=SSRTOK&SSOBRETN=SSDR1EDL) THEN/* OR JES3 DID NOT    00124000
*                                      PROVIDE AN ELIGIBLE DEVICE  0158 00125000
*                                      LIST (INCLUSIVE LIST) @Y02BKCI*/ 00126000
