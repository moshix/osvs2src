*   DDRJES3L='1'B;                  /* INIT LOOP CONTROL VARIABLE  0152 00068000
*                                                            @Y02BKCI*/ 00069000
*                                                                  0152 00070000
         OI    DDRJES3L(DDRPTR),B'00000010'                        0152 00071000
*   /*****************************************************************/ 00072000
*   /*                                                               */ 00073000
*   /* INVOKE JES3 TO OBTAIN A LIST OF ELIGIBLE OR INELIGIBLE 'TO'   */ 00074000
*   /* CANDIDATES FOR THE SWAP                               @Y02BKCI*/ 00075000
*   /*                                                               */ 00076000
*   /*****************************************************************/ 00077000
*                                                                  0153 00078000
*   SSOBFUNC=SSOBDDR1;              /* SET FUNCTION CODE FOR       0153 00079000
*                                      CANDIDATE SELECTION   @Y02BKCI*/ 00080000
         L     @14,DDRSSOB(,DDRPTR)                                0153 00081000
         MVC   SSOBFUNC(2,@14),@CH01535                            0153 00082000
*   SSDRSFRU=DDRFMUCB;              /* SET PTR TO 'FROM' UCB @Y02BKCI*/ 00083000
