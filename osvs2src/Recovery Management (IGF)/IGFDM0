*   DO;                             /* ?IGFDERR INIT                 */ 00033000
*                                                                  0123 00034000
*     /***************************************************************/ 00035000
*     /*                                                             */ 00036000
*     /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT                   */ 00037000
*     /*                                                             */ 00038000
*     /***************************************************************/ 00039000
*                                                                  0124 00040000
*     DERPLIST=DERPLIST&&DERPLIST;  /* CLEAR PARAMETER LIST          */ 00041000
         XC    DERPLIST(48),DERPLIST                               0124 00042000
*     DERFUNK=DERINIT;              /* INDICATE INIT FUNCTION        */ 00043000
         MVI   DERFUNK,X'01'                                       0125 00044000
*     DERECALL='1'B;                /* INDICATE RECALLABLE MODULE    */ 00045000
         OI    DERECALL,B'00100000'                                0126 00046000
*     DERREC=ADDR(DERRECPM);        /* POINT TO REC PARMS            */ 00047000
         LA    @12,DERRECPM                                        0127 00048000
         ST    @12,DERREC                                          0127 00049000
*     DERRETRY=ADDR(IGFDM0R);       /* POINT TO RETRY ROUTINE        */ 00050000
         LA    @12,IGFDM0R                                         0128 00051000
         ST    @12,DERRETRY                                        0128 00052000
*     GEN SETS(DERDATD)(MVC DERDATD,@SIZDATD);/* SET UP SPN, SIZE    */ 00053000
         MVC DERDATD,@SIZDATD                                           00054000
*     DERPFX=DERRMASK;              /* SET UP RUB MASK               */ 00055000
         MVC   DERPFX(2),@CB00341                                  0130 00056000
*     DERRCODE=CODEBASE;            /* SAVE CODEREG                  */ 00057000
         ST    CODEBASE,DERRCODE                                   0131 00058000
*     DERRDATA=DATABASE;            /* SAVE DATAREG                  */ 00059000
         ST    DATABASE,DERRDATA                                   0132 00060000
*     DERRDDR=DDRPTR;               /* SAVE DDRCOM POINTER           */ 00061000
         ST    DDRPTR,DERRDDR                                      0133 00062000
*     DERRSAVE=SAVEPTR;             /* SAVE REGISTER 13              */ 00063000
         ST    SAVEPTR,DERRSAVE                                    0134 00064000
*     CALL IGFDE0(DERPLIST);        /* ISSUE ESTAE                   */ 00065000
         LA    @12,DERPLIST                                        0135 00066000
         ST    @12,@AL00001                                        0135 00067000
         L     @15,@CV00073                                        0135 00068000
         LA    @01,@AL00001                                        0135 00069000
         BALR  @14,@15                                             0135 00070000
*   END;                            /* ?IGFDERR INIT                 */ 00071000
*   DM0IRC=VALID;                   /* INDICATE NO ERRORS YET        */ 00072000
         SR    @12,@12                                             0137 00073000
         LR    DM0IRC,@12                                          0137 00074000
*   DERLRC=VALID;                   /* INDICATE INPUT OK SO FAR      */ 00075000
*                                                                  0138 00076000
         STC   @12,DERLRC                                          0138 00077000
*   /*****************************************************************/ 00078000
*   /*                                                               */ 00079000
*   /* INITIALIZE THE WTO/WTOR PARAMETER LIST                        */ 00080000
*   /*                                                               */ 00081000
*   /*****************************************************************/ 00082000
*                                                                  0139 00083000
*   WTORRPL=LENGTH(RPLYAREA);       /* FILL IN REPLY BUFFER LENGTH   */ 00084000
         MVI   WTORRPL,X'03'                                       0139 00085000
*   WTORRPA=ADDR(RPLYAREA);         /* FILL IN REPLY BUFFER ADDRESS  */ 00086000
         LA    @12,RPLYAREA                                        0140 00087000
         STCM  @12,7,WTORRPA                                       0140 00088000
*   WTORECB=ADDR(ECB);              /* FILL IN REPLY ECB ADDRESS     */ 00089000
         LA    @12,ECB                                             0141 00090000
         ST    @12,WTORECB                                         0141 00091000
*   WTOMK1=MK1INIT;                 /* INITIALIZE WTO FLAG BYTE      */ 00092000
         MVI   WTOMK1,X'00'                                        0142 00093000
*   WTOMCSF=MCSFLAGS;               /* FILL IN MCS FLAGS             */ 00094000
         MVC   WTOMCSF(2),@CB00046                                 0143 00095000
*   RPLYAREA='';                    /* CLEAR REPLY BUFFER            */ 00096000
         MVI   RPLYAREA,C' '                                       0144 00097000
         MVC   RPLYAREA+1(2),RPLYAREA                              0144 00098000
*   ECB=0;                          /* CLEAR REPLY ECB               */ 00099000
         SR    @12,@12                                             0145 00100000
         ST    @12,ECB                                             0145 00101000
*   WTOPTR=ADDR(WTOMSG1);           /* INITIALIZE WTO TEXT POINTER   */ 00102000
*                                                                  0146 00103000
         LA    WTOPTR,WTOMSG1                                      0146 00104000
*   /*****************************************************************/ 00105000
*   /*                                                               */ 00106000
*   /* TEST MESSAGE CODE (DDRMSGCD) AND NUMBER OF SUBSTITUTION CODES */ 00107000
*   /* (DDRMSGPN) FOR VALIDITY                                       */ 00108000
*   /*                                                               */ 00109000
*   /*****************************************************************/ 00110000
*                                                                  0147 00111000
*   IF DDRMSGCD<1|DDRMSGCD>IGFDMIND(1)|DDRMSGPN>DIM(DDRMSGPC)/* THE     00112000
*                                      MESSAGE CODE MUST BE BETWEEN 1   00113000
*                                      AND THE NUMBER OF MESSAGES IN    00114000
*                                      THE IGFDMIND ARRAY AND THE  0147 00115000
*                                      NUMBER OF SUBSTITUTION CODES     00116000
*                                      MUST NOT EXCEED THE SIZE OF 0147 00117000
*                                      THE SUBSTITUTION CODE ARRAY   */ 00118000
*     THEN                          /* MESSAGE CODE OR NUMBER OF   0147 00119000
*                                      SUBSTITUTION CODES IS INVALID */ 00120000
         SR    @12,@12                                             0147 00121000
         IC    @12,DDRMSGCD(,DDRPTR)                               0147 00122000
         LTR   @12,@12                                             0147 00123000
         BNP   @RT00147                                            0147 00124000
         L     @11,@CA00107                                        0147 00125000
         C     @12,IGFDMIND-IGFDMIND(,@11)                         0147 00126000
         BH    @RT00147                                            0147 00127000
         CLI   DDRMSGPN(DDRPTR),7                                  0147 00128000
         BNH   @RF00147                                            0147 00129000
@RT00147 DS    0H                                                  0148 00130000
*     DERLRC=INVALID;               /* INDICATE INVALID INPUT DATA   */ 00131000
         MVI   DERLRC,X'04'                                        0148 00132000
*   ELSE                            /* CODES ARE VALID - CONTINUE    */ 00133000
*     DO;                                                          0149 00134000
         B     @RC00147                                            0149 00135000
@RF00147 DS    0H                                                  0150 00136000
*       SKLPTR=IGFDMIND(DDRMSGCD+1);/* PICK UP ADDRESS OF THE FIRST     00137000
*                                      TEXT SEGMENT FOR THE SPECIFIED   00138000
*                                      MESSAGE                       */ 00139000
*                                                                  0150 00140000
         SR    @12,@12                                             0150 00141000
         IC    @12,DDRMSGCD(,DDRPTR)                               0150 00142000
         SLA   @12,2                                               0150 00143000
         L     @11,@CA00107                                        0150 00144000
         L     SKLPTR,IGFDMIND-IGFDMIND(@12,@11)                   0150 00145000
*       /*************************************************************/ 00146000
*       /*                                                           */ 00147000
*       /* PROCESS ALL SPECIFIED SUBSTITUTION CODES BY MOVING FIXED  */ 00148000
*       /* TEXT SEGMENTS AND SUBSTITUTION STRINGS TO THE WTO MESSAGE */ 00149000
*       /* BUFFER ALTERNATELY                                        */ 00150000
*       /*                                                           */ 00151000
*       /*************************************************************/ 00152000
*                                                                  0151 00153000
*       DO SCOUNT=1 TO DDRMSGPN WHILE(DERLRC=VALID);/* PROCESS ALL 0151 00154000
*                                      SUBSTITUTION CODES UNLESS AN     00155000
*                                      ERROR IS DETECTED             */ 00156000
         LA    SCOUNT,1                                            0151 00157000
         B     @DE00151                                            0151 00158000
@DL00151 CLI   DERLRC,0                                            0151 00159000
         BNE   @DC00151                                            0151 00160000
*         CALL DM0MOVE;             /* MOVE CURRENT TEXT SEGMENT TO     00161000
*                                      WTO BUFFER                    */ 00162000
         BAL   @14,DM0MOVE                                         0152 00163000
*         SCODE=DDRMSGPC(SCOUNT);   /* PICK UP NEXT SUBSTITUTION CODE*/ 00164000
*                                                                  0153 00165000
         SR    SCODE,SCODE                                         0153 00166000
         IC    SCODE,DDRMSGPC-1(SCOUNT,DDRPTR)                     0153 00167000
*         /***********************************************************/ 00168000
*         /*                                                         */ 00169000
*         /* TEST SUBSTITUTION CODE FOR VALIDITY                     */ 00170000
*         /*                                                         */ 00171000
*         /***********************************************************/ 00172000
*                                                                  0154 00173000
*         IF SCODE=0|SCODE>DIM(DM0SUBS)THEN/* SUBSTITUTION CODE IS 0154 00174000
*                                      INVALID                       */ 00175000
         CH    SCODE,@CH00039                                      0154 00176000
         BE    @RT00154                                            0154 00177000
         CH    SCODE,@CH00112                                      0154 00178000
         BNH   @RF00154                                            0154 00179000
@RT00154 DS    0H                                                  0155 00180000
*           DERLRC=INVALID;         /* INDICATE INPUT DATA INVALID   */ 00181000
         MVI   DERLRC,X'04'                                        0155 00182000
*         ELSE                      /* CODE IS VALID - PROCESS IT    */ 00183000
*           DO;                                                    0156 00184000
         B     @RC00154                                            0156 00185000
@RF00154 DS    0H                                                  0157 00186000
*             SUBPTR=DM0SUBS(SCODE);/* PICK UP ADDRESS OF PROPER   0157 00187000
*                                      PROCESSING SUBROUTINE         */ 00188000
         LR    @12,SCODE                                           0157 00189000
         SLA   @12,2                                               0157 00190000
         L     SUBPTR,DM0SUBS-4(@12)                               0157 00191000
*             CALL DM0SUB;          /* PROCESS THE SUBSTITUTION CODE */ 00192000
         LR    @15,SUBPTR                                          0158 00193000
         BALR  @14,@15                                             0158 00194000
*             IF DM0IRC=INVALID     /* CHECK FOR AN ERROR CONDITION  */ 00195000
*               THEN                /* ERROR - DATA NO GOOD          */ 00196000
         LA    @12,4                                               0159 00197000
         CR    DM0IRC,@12                                          0159 00198000
         BNE   @RF00159                                            0159 00199000
*               DERLRC=INVALID;     /* INDICATE INPUT DATA INVALID   */ 00200000
         STC   @12,DERLRC                                          0160 00201000
*           END;                                                   0161 00202000
@RF00159 DS    0H                                                  0162 00203000
*       END;                        /* END OF SUBSTITUTION CODE LOOP */ 00204000
*                                                                  0162 00205000
@RC00154 AH    SCOUNT,@CH00089                                     0162 00206000
@DE00151 SR    @12,@12                                             0162 00207000
         IC    @12,DDRMSGPN(,DDRPTR)                               0162 00208000
         CR    SCOUNT,@12                                          0162 00209000
         BNH   @DL00151                                            0162 00210000
@DC00151 DS    0H                                                  0163 00211000
*       /*************************************************************/ 00212000
*       /*                                                           */ 00213000
*       /* WERE ALL SUBSTITUTION CODES PROCESSED SUCCESSFULLY        */ 00214000
*       /*                                                           */ 00215000
*       /*************************************************************/ 00216000
*                                                                  0163 00217000
*       IF DERLRC=VALID THEN                                       0163 00218000
         CLI   DERLRC,0                                            0163 00219000
         BNE   @RF00163                                            0163 00220000
*         DO;                       /* NO ERRORS - CONTINUE          */ 00221000
*           CALL DM0MOVE;           /* MOVE LAST MESSAGE TEXT SECTION   00222000
*                                      TO WTO MESSAGE BUFFER         */ 00223000
         BAL   @14,DM0MOVE                                         0165 00224000
*           WTOLNG1=WTOPTR-ADDR(WTOMSG1)+LENGTH(WTOMK1)+LENGTH(WTOLNG1) 00225000
*               +LENGTH(WTOMCSF);   /* CALCULATE LENGTH OF WTO     0166 00226000
*                                      MESSAGE AND PUT INTO WTO    0166 00227000
*                                      PARAMETER LIST                */ 00228000
         LA    @12,WTOMSG1                                         0166 00229000
         LCR   @12,@12                                             0166 00230000
         AR    @12,WTOPTR                                          0166 00231000
         AH    @12,@CH00041                                        0166 00232000
         STC   @12,WTOLNG1                                         0166 00233000
*           CALL DM0SETCD;          /* ADD DESCRIPTOR AND ROUTING  0167 00234000
*                                      CODES TO WTO PARAMETER LIST   */ 00235000
         BAL   @14,DM0SETCD                                        0167 00236000
*           IF DDRMSGOP^=DDRWTOR THEN/* TEST FOR WTOR REQUEST        */ 00237000
*                                                                  0168 00238000
         CLI   DDRMSGOP(DDRPTR),X'01'                              0168 00239000
         BE    @RF00168                                            0168 00240000
*             /*******************************************************/ 00241000
*             /*                                                     */ 00242000
*             /* MESSAGE TO BE WRITTEN - NO REPLY EXPECTED           */ 00243000
*             /*                                                     */ 00244000
*             /*******************************************************/ 00245000
*                                                                  0169 00246000
*             DO;                   /* WTOP MF(E,PLSTWTO)            */ 00247000
*               RFY                                                0170 00248000
*                 GPR01P RSTD;      /* RESTRICT REG 1                */ 00249000
*               GPR01P=ADDR(PLSTWTO);/* ADDRESS CONTROL PROGRAM LIST */ 00250000
         LA    GPR01P,PLSTWTO                                      0171 00251000
*               SVC(35);            /* ISSUE WTO SVC                 */ 00252000
         SVC   35                                                  0172 00253000
*               RFY                                                0173 00254000
*                 GPR01P UNRSTD;    /* RELEASE REG 1                 */ 00255000
*             END;                  /* WTOP MF(E,PLSTWTO)            */ 00256000
*           ELSE                    /* WTOR REQUEST                  */ 00257000
*             DO;                                                  0175 00258000
*                                                                  0175 00259000
         B     @RC00168                                            0175 00260000
@RF00168 DS    0H                                                  0176 00261000
*               /*****************************************************/ 00262000
*               /*                                                   */ 00263000
*               /* MESSAGE TO BE WRITTEN AND REPLY CHECKED           */ 00264000
*               /*                                                   */ 00265000
*               /*****************************************************/ 00266000
*                                                                  0176 00267000
*               DO;                 /* WTORP MF(E,PLSTWTOR)          */ 00268000
*                 RFY                                              0177 00269000
*                   GPR01P RSTD;    /* RESTRICT REG 1                */ 00270000
*                 GPR01P=ADDR(PLSTWTOR);/* ADDRESS CONTROL PROGRAM 0178 00271000
*                                      LIST                          */ 00272000
         LA    GPR01P,PLSTWTOR                                     0178 00273000
*                 SVC(35);          /* ISSUE WTOR SVC                */ 00274000
         SVC   35                                                  0179 00275000
*                 RFY                                              0180 00276000
*                   GPR01P UNRSTD;  /* RELEASE REG 1                 */ 00277000
*               END;                /* WTORP MF(E,PLSTWTOR)          */ 00278000
*               DO;                 /* WAIT ECB(ECB)                 */ 00279000
*                 RESPECIFY                                        0183 00280000
*                  (GPR00P,                                        0183 00281000
*                   GPR01P)RESTRICTED;                             0183 00282000
*                 GPR00P=1;         /* LOAD WAIT COUNT               */ 00283000
         LA    GPR00P,1                                            0184 00284000
*                 GPR01P=ADDR(ECB); /* ADDRESS OF ECB                */ 00285000
         LA    GPR01P,ECB                                          0185 00286000
*                 SVC(1);           /* WAIT SVC                      */ 00287000
         SVC   1                                                   0186 00288000
*                 RESPECIFY                                        0187 00289000
*                  (GPR00P,                                        0187 00290000
*                   GPR01P)UNRESTRICTED;                           0187 00291000
*               END;                /* WAIT ECB(ECB)                 */ 00292000
*               RPLYAREA=RPLYAREA|FOLDMASK;/* FOLD LOWER CASE      0189 00293000
*                                      CHARACTERS TO UPPER CASE      */ 00294000
*                                                                  0189 00295000
         OC    RPLYAREA(3),@CB00049                                0189 00296000
*               /*****************************************************/ 00297000
*               /*                                                   */ 00298000
*               /* INDICATE REPLY RECEIVED IN DDRCOM FIELDS          */ 00299000
*               /*                                                   */ 00300000
*               /*****************************************************/ 00301000
*                                                                  0190 00302000
*               IF RPLYAREA='YES'&DDRTOUCB^=0/* TEST FOR REPLY OF  0190 00303000
*                                      'YES' IF APPLICABLE           */ 00304000
*                 THEN              /* 'YES' WAS SPECIFIED           */ 00305000
         CLC   RPLYAREA(3),@CC00641                                0190 00306000
         BNE   @RF00190                                            0190 00307000
         ICM   @12,15,DDRTOUCB(DDRPTR)                             0190 00308000
         BZ    @RF00190                                            0190 00309000
*                 DDRMSGOP=DDRYES;  /* INDICATE REPLY WAS 'YES'      */ 00310000
         MVI   DDRMSGOP(DDRPTR),X'02'                              0191 00311000
*               ELSE                /* TRY AGAIN                     */ 00312000
*                 IF RPLYAREA(1:2)='NO'/* WAS REPLY 'NO'             */ 00313000
*                   THEN            /* YES, 'NO' WAS SPECIFIED       */ 00314000
         B     @RC00190                                            0192 00315000
@RF00190 CLC   RPLYAREA(2),@CC00642                                0192 00316000
         BNE   @RF00192                                            0192 00317000
*                   DDRMSGOP=DDRNO; /* INDICATE REPLY WAS 'NO'       */ 00318000
         MVI   DDRMSGOP(DDRPTR),X'03'                              0193 00319000
*                 ELSE              /* NO - REPLY WAS CUA (OR 'YES'     00320000
*                                      WHEN 'YES' WAS NOT A GIVEN  0194 00321000
*                                      CHOICE - PRETEND THAT IT IS A    00322000
*                                      CUA - WILL BE REJECTED LATER  */ 00323000
*                   DO;                                            0194 00324000
         B     @RC00192                                            0194 00325000
@RF00192 DS    0H                                                  0195 00326000
*                     DDRMSGOP=DDRCUA;/* INDICATE REPLY WAS CUA      */ 00327000
         MVI   DDRMSGOP(DDRPTR),X'04'                              0195 00328000
*                     DDRTOCUA=RPLYAREA;/* PUT CUA INTO DDRCOM       */ 00329000
         MVC   DDRTOCUA(3,DDRPTR),RPLYAREA                         0196 00330000
*                   END;                                           0197 00331000
*             END;                                                 0198 00332000
@RC00192 DS    0H                                                  0198 00333000
@RC00190 DS    0H                                                  0199 00334000
*         END;                                                     0199 00335000
@RC00168 DS    0H                                                  0200 00336000
*     END;                                                         0200 00337000
@RF00163 DS    0H                                                  0201 00338000
*IGFDM0R:                           /* ESTAE RETRY ENTRY POINT       */ 00339000
*   DO;                             /* ?IGFDERR CANCEL               */ 00340000
*                                                                  0201 00341000
@RC00147 DS    0H                                                  0201 00342000
IGFDM0R  DS    0H                                                  0202 00343000
*     /***************************************************************/ 00344000
*     /*                                                             */ 00345000
*     /* IGFDERR - CANCEL RECOVERY ENVIRONMENT                       */ 00346000
*     /*                                                             */ 00347000
*     /***************************************************************/ 00348000
*                                                                  0202 00349000
*     DERFUNK=DERCANC;              /* INDICATE CANCEL               */ 00350000
         MVI   DERFUNK,X'02'                                       0202 00351000
*     CALL IGFDE0(DERPLIST);        /* CANCEL ESTAE                  */ 00352000
         LA    @12,DERPLIST                                        0203 00353000
         ST    @12,@AL00001                                        0203 00354000
         L     @15,@CV00073                                        0203 00355000
         LA    @01,@AL00001                                        0203 00356000
         BALR  @14,@15                                             0203 00357000
*   END;                            /* ?IGFDERR CANCEL               */ 00358000
*   RETURN CODE(DERLRC);            /* RETURN TO CALLER              */ 00359000
         SR    @12,@12                                             0205 00360000
         IC    @12,DERLRC                                          0205 00361000
         L     @13,4(,@13)                                         0205 00362000
         L     @00,@SIZDATD                                        0205 00363000
         LR    @01,@09                                             0205 00364000
         FREEMAIN R,LV=(0),A=(1)                                        00365000
         LR    @15,@12                                             0205 00366000
         L     @14,12(,@13)                                        0205 00367000
         LM    @00,@12,20(@13)                                     0205 00368000
         BR    @14                                                 0205 00369000
*   GEN NOREFS NOSETS(EJECT);                                      0206 00370000
*                                                                  0206 00371000
         EJECT                                                          00372000
*   /*****************************************************************/ 00373000
*   /*                                                               */ 00374000
*   /* SUBROUTINE DM0MOVE - COPIES A TEXT SEGMENT TO WTO BUFFER      */ 00375000
*   /*                                                               */ 00376000
*   /*****************************************************************/ 00377000
*                                                                  0207 00378000
*DM0MOVE:                           /* SUBROUTINE ENTRY POINT        */ 00379000
*   PROCEDURE;                      /* START OF INTERNAL PROCEDURE   */ 00380000
*                                                                  0207 00381000
         B     @PB00002                                            0207 00382000
DM0MOVE  STM   @14,@01,12(@13)                                     0207 00383000
         STM   @04,@12,36(@13)                                     0207 00384000
*   /*****************************************************************/ 00385000
*   /*                                                               */ 00386000
*   /* MOVE THE NEXT TEXT SEGMENT TO THE WTO MESSAGE BUFFER ONLY IF  */ 00387000
*   /* THE TEXT SEGMENT HAS A LENGTH OF AT LEAST ONE BYTE            */ 00388000
*   /*                                                               */ 00389000
*   /*****************************************************************/ 00390000
*                                                                  0208 00391000
*   IF SKLCOUNT>0 THEN              /* ANY DATA TO MOVE              */ 00392000
         CLI   SKLCOUNT(SKLPTR),0                                  0208 00393000
         BNH   @RF00208                                            0208 00394000
*     WTOTXT(1:SKLCOUNT)=SKLTEXT(1:SKLCOUNT);/* MOVE THE TEXT SEGMENT   00395000
*                                      TO THE WTO BUFFER             */ 00396000
*                                                                  0209 00397000
         SR    @12,@12                                             0209 00398000
         IC    @12,SKLCOUNT(,SKLPTR)                               0209 00399000
         BCTR  @12,0                                               0209 00400000
         EX    @12,@SM00699                                        0209 00401000
*   /*****************************************************************/ 00402000
*   /*                                                               */ 00403000
*   /* UPDATE THE SKELETON POINTER AND THE WTO BUFFER POINTER        */ 00404000
*   /*                                                               */ 00405000
*   /*****************************************************************/ 00406000
*                                                                  0210 00407000
*   WTOPTR=WTOPTR+SKLCOUNT;         /* POINT TO NEXT AVAILABLE BYTE  */ 00408000
@RF00208 SR    @12,@12                                             0210 00409000
         IC    @12,SKLCOUNT(,SKLPTR)                               0210 00410000
         AR    WTOPTR,@12                                          0210 00411000
*   SKLPTR=SKLPTR+SKLCOUNT+LENGTH(SKLCOUNT);/* POINT TO START OF NEXT   00412000
*                                      TEXT SEGMENT                  */ 00413000
         AR    @12,SKLPTR                                          0211 00414000
         AH    @12,@CH00089                                        0211 00415000
         LR    SKLPTR,@12                                          0211 00416000
*   RETURN;                         /* RETURN TO CALLER              */ 00417000
@EL00002 DS    0H                                                  0212 00418000
@EF00002 DS    0H                                                  0212 00419000
@ER00002 LM    @14,@01,12(@13)                                     0212 00420000
         LM    @04,@12,36(@13)                                     0212 00421000
         BR    @14                                                 0212 00422000
*   END DM0MOVE;                    /* END OF INTERNAL PROCEDURE     */ 00423000
         B     @EL00002                                            0213 00424000
@PB00002 DS    0H                                                  0214 00425000
*   GEN NOREFS NOSETS(EJECT);                                      0214 00426000
*                                                                  0214 00427000
         EJECT                                                          00428000
*   /*****************************************************************/ 00429000
