         TITLE 'IGFDM1, DDR I/O MODULE                                 *00001000
                        '                                               00002000
IGFDM1   CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGFDM1    77.018'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @08,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@08                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @09,@01                                             0001 00016000
         USING @DATD,@09                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @09,8(,@13)                                         0001 00020000
         LR    @13,@09                                             0001 00021000
*                                      VERSION                       */ 00022000
*   DO;                             /* ?IGFDERR INIT                 */ 00023000
*                                                                  0192 00024000
*     /***************************************************************/ 00025000
*     /*                                                             */ 00026000
*     /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT                   */ 00027000
*     /*                                                             */ 00028000
*     /***************************************************************/ 00029000
*                                                                  0193 00030000
*     DERPLIST=DERPLIST&&DERPLIST;  /* CLEAR PARAMETER LIST          */ 00031000
         XC    DERPLIST(48),DERPLIST                               0193 00032000
*     DERFUNK=DERINIT;              /* INDICATE INIT FUNCTION        */ 00033000
         MVI   DERFUNK,X'01'                                       0194 00034000
*     DERREC=ADDR(DERRECPM);        /* POINT TO REC PARMS            */ 00035000
         LA    @12,DERRECPM                                        0195 00036000
         ST    @12,DERREC                                          0195 00037000
*     DERRETRY=ADDR(IGFDM1R);       /* POINT TO RETRY ROUTINE        */ 00038000
         LA    @12,IGFDM1R                                         0196 00039000
         ST    @12,DERRETRY                                        0196 00040000
*     GEN SETS(DERDATD)(MVC DERDATD,@SIZDATD);/* SET UP SPN, SIZE    */ 00041000
         MVC DERDATD,@SIZDATD                                           00042000
*     DERPFX=DERRMASK;              /* SET UP RUB MASK               */ 00043000
         MVC   DERPFX(2),@CB02029                                  0198 00044000
*     DERRCODE=CODEBASE;            /* SAVE CODEREG                  */ 00045000
         ST    CODEBASE,DERRCODE                                   0199 00046000
*     DERRDATA=DATABASE;            /* SAVE DATAREG                  */ 00047000
         ST    DATABASE,DERRDATA                                   0200 00048000
*     DERRDDR=DDRPTR;               /* SAVE DDRCOM POINTER           */ 00049000
         ST    DDRPTR,DERRDDR                                      0201 00050000
*     DERRSAVE=SAVEPTR;             /* SAVE REGISTER 13              */ 00051000
         ST    SAVEPTR,DERRSAVE                                    0202 00052000
*     CALL IGFDE0(DERPLIST);        /* ISSUE ESTAE                   */ 00053000
         LA    @12,DERPLIST                                        0203 00054000
         ST    @12,@AL00001                                        0203 00055000
         L     @15,@CV01939                                        0203 00056000
         LA    @01,@AL00001                                        0203 00057000
         BALR  @14,@15                                             0203 00058000
*   END;                            /* ?IGFDERR INIT ESTAE         0204 00059000
*                                      ESTABLISHED                   */ 00060000
*   DERLRC=0;                       /* CLEAR LOCAL RETURN CODE       */ 00061000
         MVI   DERLRC,X'00'                                        0205 00062000
*   SAVER13=SAVEPTR;                /* SAVE REG 13                   */ 00063000
         LR    SAVER13,SAVEPTR                                     0206 00064000
*   IOSBPTR=ADDR(CBAREA);           /* INITIALIZE CONTROL BLOCKS BASE*/ 00065000
         LA    IOSBPTR,CBAREA                                      0207 00066000
*   IF DDRWHICH='1'B THEN           /* IS UCB INDICATOR SET ?        */ 00067000
         TM    DDRWHICH(DDRPTR),B'10000000'                        0208 00068000
         BNO   @RF00208                                            0208 00069000
*     UCBPTR=DDRFMUCB;              /* YES - USE FROM UCB            */ 00070000
         L     @12,DDRFMUCB(,DDRPTR)                               0209 00071000
         ST    @12,UCBPTR                                          0209 00072000
*   ELSE                            /* NO - INDICATOR NOT SET        */ 00073000
*     UCBPTR=DDRTOUCB;              /* USE TO UCB                    */ 00074000
         B     @RC00208                                            0210 00075000
@RF00208 L     @12,DDRTOUCB(,DDRPTR)                               0210 00076000
         ST    @12,UCBPTR                                          0210 00077000
*   COMPFLAG='0'B;                  /* CLEAR I/O COMPLETE FLAG       */ 00078000
@RC00208 NI    COMPFLAG,B'01111111'                                0211 00079000
*   CALL DM1BITS;                   /* SET NECESSARY UCB BITS        */ 00080000
         BAL   @14,DM1BITS                                         0212 00081000
*   IF^(UCBNRY='1'B&DDRIMNT='0'B) THEN/* IS PHYSICAL I/O NEEDED?     */ 00082000
         L     @12,UCBPTR                                          0213 00083000
         TM    UCBNRY(@12),B'01000000'                             0213 00084000
         BNO   @RT00213                                            0213 00085000
         TM    DDRIMNT(DDRPTR),B'00010000'                         0213 00086000
         BZ    @RF00213                                            0213 00087000
@RT00213 DS    0H                                                  0214 00088000
*     DO;                           /* YES - I/O IS NEEDED           */ 00089000
*       CALL DM1CB;                 /* CREATE CONTROL BLOCKS         */ 00090000
         BAL   @14,DM1CB                                           0215 00091000
*       PARMREG1=ADDR(CBSRB);       /* REG 1 = SRB ADDRESS   @YM02554*/ 00092000
*                                                                  0216 00093000
         LA    PARMREG1,CBSRB(,IOSBPTR)                            0216 00094000
*       /*************************************************************/ 00095000
*       /*                                                           */ 00096000
*       /* ALL DDR I/O IS SCHEDULED VIA STARTIO MACRO        @YM02554*/ 00097000
*       /*                                                           */ 00098000
*       /*************************************************************/ 00099000
*                                                                  0217 00100000
*       GEN REFS(SAVEPTR,PARMREG1,R14,R12,IOCSTIO,IOCOM,CVTPTR,CVTIXAVL 00101000
*           ,CVTMAP);                                              0217 00102000
*                                                                  0217 00103000
*                                                         /*@YM02554*/  00104000
              STM R14,R12,12(SAVEPTR)        SAVE REGS EXCEPT R13     **00105000
                                                              @YM02554  00106000
     STARTIO   SRB=(PARMREG1),TCB=SRB                         @YM02554  00107000
     LM  R14,R12,12(SAVEPTR)        RESTORE REGS              @YM02554  00108000
*/*  WAIT ON I/O ECB COMPLETION                                      */ 00109000
*                                                                  0218 00110000
*       DO;                         /* WAIT ECB(CBECB)               */ 00111000
*         RESPECIFY                                                0219 00112000
*          (GPR00P,                                                0219 00113000
*           GPR01P) RESTRICTED;                                    0219 00114000
*         GPR00P=1;                 /* LOAD WAIT COUNT               */ 00115000
         LA    GPR00P,1                                            0220 00116000
*         GPR01P=ADDR(CBECB);       /* ADDRESS OF ECB                */ 00117000
         LA    GPR01P,CBECB(,IOSBPTR)                              0221 00118000
*         SVC(1);                   /* WAIT SVC                      */ 00119000
         SVC   1                                                   0222 00120000
*         RESPECIFY                                                0223 00121000
*          (GPR00P,                                                0223 00122000
*           GPR01P) UNRESTRICTED;                                  0223 00123000
*       END;                        /* WAIT ECB(CBECB) WAIT FOR I/O     00124000
*                                      COMPLETION                    */ 00125000
*       COMPFLAG='1'B;              /* I/O IS COMPLETE               */ 00126000
         OI    COMPFLAG,B'10000000'                                0225 00127000
*       CALL DM1BITS;               /* UCB ENDING STATUS             */ 00128000
         BAL   @14,DM1BITS                                         0226 00129000
*       IF IOSCOD^=SUCCODE THEN     /* IS IOS CODE 7F ?              */ 00130000
         CLI   IOSCOD(IOSBPTR),127                                 0227 00131000
         BE    @RF00227                                            0227 00132000
*         DERLRC=4;                 /* NO - SET LOCAL RETURN CODE    */ 00133000
         MVI   DERLRC,X'04'                                        0228 00134000
*       ELSE                                                       0229 00135000
*         ;                         /* YES - SUCCESSFUL I/O          */ 00136000
@RF00227 DS    0H                                                  0230 00137000
*     END;                                                         0230 00138000
*   ELSE                                                           0231 00139000
*     ;                             /* I/O NOT NECESSARY(NOT READY IS   00140000
*                                      SET AND MOUNT IS OFF)         */ 00141000
@RF00213 DS    0H                                                  0232 00142000
*/* THIS IS THE COMMON EXIT ROUTINE FOR THIS MODULE                  */ 00143000
*                                                                  0232 00144000
*IGFDM1R:                                                          0232 00145000
*   ;                                                              0232 00146000
IGFDM1R  DS    0H                                                  0233 00147000
*   DO;                             /* ?IGFDERR CANCEL               */ 00148000
*                                                                  0233 00149000
*     /***************************************************************/ 00150000
*     /*                                                             */ 00151000
*     /* IGFDERR - CANCEL RECOVERY ENVIRONMENT                       */ 00152000
*     /*                                                             */ 00153000
*     /***************************************************************/ 00154000
*                                                                  0234 00155000
*     DERFUNK=DERCANC;              /* INDICATE CANCEL               */ 00156000
         MVI   DERFUNK,X'02'                                       0234 00157000
*     CALL IGFDE0(DERPLIST);        /* CANCEL ESTAE                  */ 00158000
         LA    @12,DERPLIST                                        0235 00159000
         ST    @12,@AL00001                                        0235 00160000
         L     @15,@CV01939                                        0235 00161000
         LA    @01,@AL00001                                        0235 00162000
         BALR  @14,@15                                             0235 00163000
*   END;                            /* ?IGFDERR CANCEL ESTAE CANCEL  */ 00164000
*   RETURN CODE(DERLRC);            /* RETURN TO CALLER              */ 00165000
         SLR   @12,@12                                             0237 00166000
         IC    @12,DERLRC                                          0237 00167000
         L     @13,4(,@13)                                         0237 00168000
         L     @00,@SIZDATD                                        0237 00169000
         LR    @01,@09                                             0237 00170000
         FREEMAIN R,LV=(0),A=(1)                                        00171000
         LR    @15,@12                                             0237 00172000
         L     @14,12(,@13)                                        0237 00173000
         LM    @00,@12,20(@13)                                     0237 00174000
         BR    @14                                                 0237 00175000
*                                                                  0238 00176000
*   /*****************************************************************/ 00177000
*   /*                                                               */ 00178000
*   /* THIS SUBROUTINE ADJUSTS THE UCB TO THE DESIRED STATUS BEFORE  */ 00179000
*   /* AND AFTER ISSUING THE I/O REQUEST.                            */ 00180000
*   /*                                                               */ 00181000
*   /*****************************************************************/ 00182000
*                                                                  0238 00183000
*DM1BITS:                                                          0238 00184000
*   PROC;                                                          0238 00185000
DM1BITS  STM   @14,@12,@SA00002                                    0238 00186000
*   SAVEPTR=ADDR(IOSGWA);           /* IOSGEN WORK AREA ADDRESS IN 0239 00187000
*                                      R13                           */ 00188000
         LA    SAVEPTR,IOSGWA                                      0239 00189000
*   PREPTR=UCBPTR;                  /* SETUP UCB FOR IOSGEN  @YM02567*/ 00190000
         L     PREPTR,UCBPTR                                       0240 00191000
*   IF COMPFLAG='1'B THEN           /* IS I/O COMPLETE ?             */ 00192000
         TM    COMPFLAG,B'10000000'                                0241 00193000
         BNO   @RF00241                                            0241 00194000
*     DO;                           /* YES - UCB POST PROCESSING IS     00195000
*                                      REQUIRED                      */ 00196000
*       IF DDRIOF1=DDRRUN THEN      /* IS I/O AN UNLOAD AND REWIND 0243 00197000
*                                      OP?                           */ 00198000
         CLI   DDRIOF1(DDRPTR),X'0F'                               0243 00199000
         BNE   @RF00243                                            0243 00200000
*         DO;                       /* YES - SET NOT READY FLAG      */ 00201000
*           GEN REFS(UCBFLA,UCBFLB,UCBFLC,CVTPTR,PREPTR,UCBNRY,CVTIXAVL 00202000
*               ,IOCGENA,UCBBGN,UCBCCWOF,UCB3TAPE);                0245 00203000
   IOSGEN UCBFLG,UCB=(PREPTR),VAR=ON,TABLE=UCBNRY,REG=UNCOND            00204000
*         END;                                                     0246 00205000
*       ELSE                        /* NOT AN UNLOAD AND REWIND    0247 00206000
*                                      OPERATION                     */ 00207000
*         IF DDRIMNT='1'B THEN      /* IS I/O A MOUNT ?              */ 00208000
         B     @RC00243                                            0247 00209000
@RF00243 TM    DDRIMNT(DDRPTR),B'00010000'                         0247 00210000
         BNO   @RF00247                                            0247 00211000
*           UCBMOUNT='0'B;          /* YES - RESET MOUNT PENDING IN     00212000
*                                      UCB                           */ 00213000
         L     @12,UCBPTR                                          0248 00214000
         NI    UCBMOUNT(@12),B'01111111'                           0248 00215000
*         ELSE                                                     0249 00216000
*           ;                       /* NOT A MOUNT OPERATION         */ 00217000
@RF00247 DS    0H                                                  0250 00218000
*     END;                                                         0250 00219000
*   ELSE                            /* COMPLETE FLAG NOT SET         */ 00220000
*     DO;                                                          0251 00221000
         B     @RC00241                                            0251 00222000
@RF00241 DS    0H                                                  0252 00223000
*       IF DDRIMNT='1'B THEN        /* IS I/O A MOUNT ?              */ 00224000
         TM    DDRIMNT(DDRPTR),B'00010000'                         0252 00225000
         BNO   @RF00252                                            0252 00226000
*         DO;                       /* YES - SET UCB NOT READY       */ 00227000
*           GEN REFS(UCBFLA,UCBFLB,UCBFLC,CVTPTR,PREPTR,UCBNRY,CVTIXAVL 00228000
*               ,IOCGENA,UCBBGN,UCBCCWOF,UCB3TAPE);                0254 00229000
    IOSGEN UCBFLG,UCB=(PREPTR),VAR=ON,TABLE=UCBNRY,REG=UNCOND           00230000
*           UCBATI=0;               /* CLEAR ATTENTION TABLE INDEX   */ 00231000
         L     @12,UCBPTR                                          0255 00232000
         L     @03,UCBEXTPT(,@12)                                  0255 00233000
         MVI   UCBATI(@03),X'00'                                   0255 00234000
*           UCBMOUNT='1'B;          /* SET UCB MOUNT PENDING         */ 00235000
         OI    UCBMOUNT(@12),B'10000000'                           0256 00236000
*         END;                                                     0257 00237000
*       ELSE                                                       0258 00238000
*         ;                         /* NOT A MOUNT OPERATION         */ 00239000
@RF00252 DS    0H                                                  0259 00240000
*       IF DDRITAKE='1'B&UCBQISCE='0'B THEN/* IS CONTROL REQUIRED ?  */ 00241000
         TM    DDRITAKE(DDRPTR),B'01000000'                        0259 00242000
         BNO   @RF00259                                            0259 00243000
         L     @12,UCBPTR                                          0259 00244000
         TM    UCBQISCE(@12),B'00000001'                           0259 00245000
         BNZ   @RF00259                                            0259 00246000
*         DO;                       /* YES - UCB CONTROL IS REQUIRED */ 00247000
*           GEN REFS(UCBFLA,UCBFLB,UCBFLC,CVTPTR,PREPTR,UCBQISCE,  0261 00248000
*               CVTIXAVL,IOCGENA,UCBBGN,UCBCCWOF,UCB3TAPE,UCBDDRSW);    00249000
   IOSGEN UCBFLG,UCB=(PREPTR),VAR=ON,TABLE=UCBQISCE                     00250000
   IOSGEN UCBFLG,UCB=(PREPTR),VAR=ON,TABLE=UCBDDRSW                     00251000
*         END;                                                     0262 00252000
*       ELSE                        /* NO CONTROL IS REQUIRED        */ 00253000
*         IF DDRITAKE='0'B&UCBQISCE='1'B THEN/* IS RELEASED REQUIRED    00254000
*                                      ?                             */ 00255000
         B     @RC00259                                            0263 00256000
@RF00259 TM    DDRITAKE(DDRPTR),B'01000000'                        0263 00257000
         BNZ   @RF00263                                            0263 00258000
         L     @12,UCBPTR                                          0263 00259000
         TM    UCBQISCE(@12),B'00000001'                           0263 00260000
         BNO   @RF00263                                            0263 00261000
*           DO;                     /* YES - UCB RELEASED IS REQUIRED*/ 00262000
*             GEN REFS(UCBFLA,UCBFLB,UCBFLC,CVTPTR,PREPTR,UCBQISCE,     00263000
*                 CVTIXAVL,IOCGENA,UCBBGN,UCBCCWOF,UCB3TAPE,UCBDDRSW);  00264000
   IOSGEN UCBFLG,UCB=(PREPTR),VAR=OFF,TABLE=UCBQISCE                    00265000
   IOSGEN UCBFLG,UCB=(PREPTR),VAR=OFF,TABLE=UCBDDRSW                    00266000
*           END;                                                   0266 00267000
*         ELSE                                                     0267 00268000
*           ;                       /* NO UCB RELEASE IS REQUIRED    */ 00269000
@RF00263 DS    0H                                                  0268 00270000
*     END;                                                         0268 00271000
@RC00259 DS    0H                                                  0269 00272000
*   SAVEPTR=SAVER13;                /* RESTORE REG 13        @YM02554*/ 00273000
@RC00241 LR    SAVEPTR,SAVER13                                     0269 00274000
*   RETURN;                                                        0270 00275000
@EL00002 DS    0H                                                  0270 00276000
@EF00002 DS    0H                                                  0270 00277000
@ER00002 LM    @14,@12,@SA00002                                    0270 00278000
         BR    @14                                                 0270 00279000
*   END DM1BITS;                                                   0271 00280000
*                                                                  0272 00281000
*   /*****************************************************************/ 00282000
*   /*                                                               */ 00283000
*   /* THIS SUBROUTINE INITIALIZES THE SRB,IOSB,AND BUILDS THE CCW'S */ 00284000
*   /* NECESSARY TO ISSUE THE I/O.                                   */ 00285000
*   /*                                                               */ 00286000
*   /*****************************************************************/ 00287000
*                                                                  0272 00288000
*DM1CB:                                                            0272 00289000
*   PROC;                                                          0272 00290000
DM1CB    STM   @14,@12,@SA00003                                    0272 00291000
*   CONTROLB=CONTROLB&&CONTROLB;    /* CLEAR CONTROL BLOCKS          */ 00292000
         XC    CONTROLB(172,IOSBPTR),CONTROLB(IOSBPTR)             0273 00293000
*   IF DDRICNT='1'B THEN            /* IS REPEAT COUNT ON ?          */ 00294000
         TM    DDRICNT(DDRPTR),B'00001000'                         0274 00295000
         BNO   @RF00274                                            0274 00296000
*     CBCOUNT=MAX(ABS(DDRCOUNT),1); /* YES - SET COUNT       @OZ16563*/ 00297000
         L     @12,DDRCOUNT(,DDRPTR)                               0275 00298000
         LPR   @12,@12                                             0275 00299000
         LA    @00,1                                               0275 00300000
         CR    @12,@00                                             0275 00301000
         BNL   *+6                                                      00302000
         LR    @12,@00                                             0275 00303000
         ST    @12,CBCOUNT(,IOSBPTR)                               0275 00304000
*   ELSE                            /* NO - REPEAT COUNT IS ZERO     */ 00305000
*     CBCOUNT=1;                    /* SET COUNT OF ONE              */ 00306000
         B     @RC00274                                            0276 00307000
@RF00274 MVC   CBCOUNT(4,IOSBPTR),@CF00069                         0276 00308000
*   CBDDRCOM=DDRPTR;                /* SAVE DDRCOM BASE REG          */ 00309000
@RC00274 ST    DDRPTR,CBDDRCOM(,IOSBPTR)                           0277 00310000
*   IOSBPTR=ADDR(CBIOSB);           /* INITIALIZE IOSB BASE REG      */ 00311000
*   IOSDVRID=IOSMISID;              /* MISCELLANEOUS DRIVER          */ 00312000
         MVI   IOSDVRID(IOSBPTR),X'01'                             0279 00313000
*   IOSASID=DDRASID;                /* ASID                          */ 00314000
         LH    @12,DDRASID(,DDRPTR)                                0280 00315000
         STH   @12,IOSASID(,IOSBPTR)                               0280 00316000
*   IOSPGAD=ADDR(DM1SRB);           /* ADDRESS OF SRB ROUTINE        */ 00317000
         LA    @12,DM1SRB                                          0281 00318000
         ST    @12,IOSPGAD(,IOSBPTR)                               0281 00319000
*   IOSLCL='1'B;                    /* LOCAL LOCK FOR APPENDAGES     */ 00320000
         OI    IOSLCL(IOSBPTR),B'00001000'                         0282 00321000
*   IOSUCB=UCBPTR;                  /* UCB PTR VARIABLE              */ 00322000
         L     @12,UCBPTR                                          0283 00323000
         ST    @12,IOSUCB(,IOSBPTR)                                0283 00324000
*   IOSSRB=ADDR(CBSRB);             /* WORKAREA SRB                  */ 00325000
         LA    @12,CBSRB(,IOSBPTR)                                 0284 00326000
         ST    @12,IOSSRB(,IOSBPTR)                                0284 00327000
*   IOSNRM=ADDR(DM1NRM);            /* NORMAL APPENDAGE              */ 00328000
         LA    @12,DM1NRM                                          0285 00329000
         ST    @12,IOSNRM(,IOSBPTR)                                0285 00330000
*   IOSABN=ADDR(DM1ABN);            /* ABNORMAL APPENDAGE            */ 00331000
         LA    @12,DM1ABN                                          0286 00332000
         ST    @12,IOSABN(,IOSBPTR)                                0286 00333000
*   IOSVST=ADDR(CBCCW);             /* CCW ADDRESS                   */ 00334000
         LA    @12,CBCCW(,IOSBPTR)                                 0287 00335000
         ST    @12,IOSVST(,IOSBPTR)                                0287 00336000
*   IOSQISCE='1'B;                  /* I/O TO A LOCKED DEVICE        */ 00337000
         OI    IOSQISCE(IOSBPTR),B'00100000'                       0288 00338000
*/* LOAD REAL CCW ADDRESS INTO IOSB                                  */ 00339000
*                                                                  0289 00340000
*   GEN REFS(CBCCW,IOSBPTR) SETS(LRAREG);                          0289 00341000
     LRA   LRAREG,CBCCW(,IOSBPTR)  LOAD REAL ADDRESS IN REG 1           00342000
*   IOSRST=LRAREG;                                                 0290 00343000
         ST    LRAREG,IOSRST(,IOSBPTR)                             0290 00344000
*   IF DDRMT='1'B THEN              /* IS DEVICE A TAPE ?            */ 00345000
         TM    DDRMT(DDRPTR),B'00100000'                           0291 00346000
         BNO   @RF00291                                            0291 00347000
*     DO;                           /* YES -DEVICE IS A TAPE         */ 00348000
*       MODESETA=DDRUMODE;          /* OBTAIN USER'S MODESET @YM04069*/ 00349000
         MVC   MODESETA(1),DDRUMODE(DDRPTR)                        0293 00350000
*       IF UCBPTR->UCB2OPT0='1'B&DDRLSD='1'B&DDRIMNT='1'B THEN/* IS     00351000
*                                      THIS A 7-TRACK STANDARD LABEL    00352000
*                                      TAPE WITH MOUNT SET ?         */ 00353000
*                                                                  0294 00354000
         L     @12,UCBPTR                                          0294 00355000
         TM    UCB2OPT0(@12),B'10000000'                           0294 00356000
         BNO   @RF00294                                            0294 00357000
         TM    DDRLSD(DDRPTR),B'00001000'                          0294 00358000
         BNO   @RF00294                                            0294 00359000
         TM    DDRIMNT(DDRPTR),B'00010000'                         0294 00360000
         BNO   @RF00294                                            0294 00361000
*         /***********************************************************/ 00362000
*         /*                                                         */ 00363000
*         /* YES - A 7-TRACK SD LABEL                                */ 00364000
*         /*                                                         */ 00365000
*         /***********************************************************/ 00366000
*                                                                  0295 00367000
*         MODEMASK=MODESETM;        /* INITIALIZE MODESET AREA MASK  */ 00368000
         NI    MODEMASK,B'11101011'                                0295 00369000
         OI    MODEMASK,B'00101011'                                0295 00370000
*       ELSE                                                       0296 00371000
*         ;                         /* NOT A 7-TRACK STANDARD LABEL     00372000
*                                      PROCESSING                    */ 00373000
@RF00294 DS    0H                                                  0297 00374000
*       IOSFMSK=MODESETA;           /* USER OR ADJUSTED MODESET      */ 00375000
         MVC   IOSFMSK(1,IOSBPTR),MODESETA                         0297 00376000
*     END;                                                         0298 00377000
*   ELSE                                                           0299 00378000
*     ;                             /* NOT A TAPE                    */ 00379000
@RF00291 DS    0H                                                  0300 00380000
*/*  SRB INITIALIZATION                                              */ 00381000
*                                                                  0300 00382000
*   SRBPTR=ADDR(CBSRB);             /* INITIALIZE PTR                */ 00383000
         LA    SRBPTR,CBSRB(,IOSBPTR)                              0300 00384000
*   SRBID='SRB ';                   /* LABEL                         */ 00385000
         MVC   SRBID(4,SRBPTR),@CC02052                            0301 00386000
*   SRBASCB=ASVTENTY(DDRASID);      /* ASCB ADDRESS                  */ 00387000
         LH    @03,DDRASID(,DDRPTR)                                0302 00388000
         LR    @02,@03                                             0302 00389000
         SLA   @02,2                                               0302 00390000
         L     @00,CVTPTR                                          0302 00391000
         LR    @03,@00                                             0302 00392000
         L     @03,ASVTPTR(,@03)                                   0302 00393000
         L     @03,ASVTENTY-4(@02,@03)                             0302 00394000
         ST    @03,SRBASCB(,SRBPTR)                                0302 00395000
*   SRBPARM=ADDR(CBIOSB);                                          0303 00396000
         ST    IOSBPTR,SRBPARM(,SRBPTR)                            0303 00397000
*   SRBEP=ADDR(DM1SRB);             /* SRB ROUTINE ADDRESS           */ 00398000
         LA    @03,DM1SRB                                          0304 00399000
         ST    @03,SRBEP(,SRBPTR)                                  0304 00400000
*   SRBPTCB=OLDTCB;                 /* OLD TCB ADDRESS               */ 00401000
         LR    @03,@00                                             0305 00402000
         L     @03,CVTTCBP(,@03)                                   0305 00403000
         L     @00,OLDTCB(,@03)                                    0305 00404000
         ST    @00,SRBPTCB(,SRBPTR)                                0305 00405000
*   SRBPASID=DDRASID;                                              0306 00406000
*                                                                  0306 00407000
         LH    @03,DDRASID(,DDRPTR)                                0306 00408000
         STH   @03,SRBPASID(,SRBPTR)                               0306 00409000
*/*  CCW INITIALIZATION                                              */ 00410000
*                                                                  0307 00411000
*   LRAREG=DDRIBUF;                 /* GET BUFFER ADDRESS            */ 00412000
         L     LRAREG,DDRIBUF(,DDRPTR)                             0307 00413000
*   GEN NOREFS SETS(LRAREG);                                       0308 00414000
       LRA LRAREG,0(,LRAREG)    LOAD REAL ADDRESS                       00415000
*   IF DDRIOF1='0C'X THEN           /* IS OPERATION A READ BACKWARDS    00416000
*                                                            @OZ16562*/ 00417000
         CLI   DDRIOF1(DDRPTR),X'0C'                               0309 00418000
         BNE   @RF00309                                            0309 00419000
*     LRAREG=LRAREG+'B'X;           /* MOVE PTR TO BUFFEREND @OZ16562*/ 00420000
         AL    LRAREG,@CF02053                                     0310 00421000
*   CBCCW1=LRAREG;                  /* LOAD REAL ADDRESS OF CCW      */ 00422000
@RF00309 STCM  LRAREG,7,CBCCW1(IOSBPTR)                            0311 00423000
*   CBCCW0=DDRIOF1;                 /* COMMAND CODE                  */ 00424000
         MVC   CBCCW0(1,IOSBPTR),DDRIOF1(DDRPTR)                   0312 00425000
*   CBCCW4='20'X;                   /* SET SLI BIT                   */ 00426000
         MVI   CBCCW4(IOSBPTR),X'20'                               0313 00427000
*   CBCCW6=MAX(DDRIBUFL,1);         /* SET BYTE COUNT                */ 00428000
         LH    @12,DDRIBUFL(,DDRPTR)                               0314 00429000
         LA    @00,1                                               0314 00430000
         CR    @12,@00                                             0314 00431000
         BNL   *+6                                                      00432000
         LR    @12,@00                                             0314 00433000
         STH   @12,CBCCW6(,IOSBPTR)                                0314 00434000
*   RETURN;                                                        0315 00435000
@EL00003 DS    0H                                                  0315 00436000
@EF00003 DS    0H                                                  0315 00437000
@ER00003 LM    @14,@12,@SA00003                                    0315 00438000
         BR    @14                                                 0315 00439000
*   END DM1CB;                                                     0316 00440000
*                                                                  0317 00441000
*   /*****************************************************************/ 00442000
*   /*                                                               */ 00443000
*   /* N O R M A L E N D A P P E N D A G E THIS SUBROUTINE IS ENTERED*/ 00444000
*   /* WHENEVER THE I/O OPERATION IS COMPLETED SUCCESSFULLY OR IF THE*/ 00445000
*   /* ONLY ERROR FOUND WAS A UNIT EXCEPTION (TAPE MARK).            */ 00446000
*   /*                                                               */ 00447000
*   /*****************************************************************/ 00448000
*                                                                  0317 00449000
*DM1NRM:                                                           0317 00450000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE(R14));/* IOS PROVIDES THE       0317 00451000
*                                      LOCALMEMORY LOCK WHICH PASSES    00452000
*                                      A SAVE AREA ADDRESS IN REG 13 */ 00453000
DM1NRM   STM   @15,@12,16(@13)                                     0317 00454000
*   GEN NOREFS SETS(CODEBASE);                                     0318 00455000
    BALR  CODEBASE,0        WITH INPUT REG 8                            00456000
    USING *,CODEBASE       ESTABLISH BASE                               00457000
*   IOSBPTR=PARMREG1;               /* WORKAREA IOSB ADDRESS         */ 00458000
         LR    IOSBPTR,PARMREG1                                    0319 00459000
*   CBCOUNT=CBCOUNT-1;              /* DECREMENT REPEAT COUNT        */ 00460000
         L     @12,CBCOUNT(,IOSBPTR)                               0320 00461000
         BCTR  @12,0                                               0320 00462000
         ST    @12,CBCOUNT(,IOSBPTR)                               0320 00463000
*   IOSEX='0'B;                     /* ZERO EXCEPTION CONDITION      */ 00464000
         NI    IOSEX(IOSBPTR),B'11111011'                          0321 00465000
*   IF CBCOUNT>0 THEN               /* IS COUNT GREATER THAN ZERO?   */ 00466000
         LTR   @12,@12                                             0322 00467000
         BNP   @RF00322                                            0322 00468000
*     R14=R14+8;                    /* CHANNEL PROGRAM WILL BE     0323 00469000
*                                      RE-EXECUTED(EXCP)             */ 00470000
         AL    R14,@CF00038                                        0323 00471000
*   ELSE                                                           0324 00472000
*     ;                             /* COUNT IS ZERO OR LESS         */ 00473000
@RF00322 DS    0H                                                  0325 00474000
*   RETURN;                                                        0325 00475000
@EL00004 DS    0H                                                  0325 00476000
@EF00004 DS    0H                                                  0325 00477000
@ER00004 LM    @15,@12,16(@13)                                     0325 00478000
         BR    @14                                                 0325 00479000
*   END DM1NRM;                                                    0326 00480000
*                                                                  0327 00481000
*   /*****************************************************************/ 00482000
*   /*                                                               */ 00483000
*   /* A B N O R M A L E N D A P P E N D A G E THIS SUBROUTINE       */ 00484000
*   /* RECEIVES CONTROL ON UNIT OR CHANNEL CHECKS AND DETERMINES IF  */ 00485000
*   /* THE ERROR IS IGNORABLE,PERMAMENT, OR RETRYABLE.               */ 00486000
*   /*                                                               */ 00487000
*   /*****************************************************************/ 00488000
*                                                                  0327 00489000
*DM1ABN:                                                           0327 00490000
*   PROC OPTIONS(NOSAVEAREA,NOSAVE(R14));/* REG 13 CONTAINS SAVE AREA   00491000
*                                      ADDRESS                       */ 00492000
DM1ABN   STM   @15,@12,16(@13)                                     0327 00493000
*   GEN NOREFS SETS(CODEBASE);                                     0328 00494000
    BALR  CODEBASE,0        WITH INPUT REG 8                            00495000
    USING *,CODEBASE       ESTABLISH BASE                               00496000
*   IOSBPTR=PARMREG1;               /* WORKAREA IOSB ADDRESS         */ 00497000
         LR    IOSBPTR,PARMREG1                                    0329 00498000
*   DDRPTR=CBDDRCOM;                /* ESTABLISH DDRCOM PTR          */ 00499000
         L     DDRPTR,CBDDRCOM(,IOSBPTR)                           0330 00500000
*   IF IOSCOD^=IOSERRC&IOSCOD^=IOSMIHCA&IOSCOD^=IOSINTC&IOSCOD^=   0331 00501000
*       IOSTAPEC THEN               /* SECOND ENTRY INTO THIS ROUTINE   00502000
*                                      ?                             */ 00503000
         CLI   IOSCOD(IOSBPTR),X'41'                               0331 00504000
         BE    @RF00331                                            0331 00505000
         CLI   IOSCOD(IOSBPTR),X'51'                               0331 00506000
         BE    @RF00331                                            0331 00507000
         CLI   IOSCOD(IOSBPTR),X'44'                               0331 00508000
         BE    @RF00331                                            0331 00509000
         CLI   IOSCOD(IOSBPTR),X'4B'                               0331 00510000
         BE    @RF00331                                            0331 00511000
*/*  NOT A SECOND ENTRY                                              */ 00512000
*                                                                  0332 00513000
*     IF DDRILAB='0'B&DDRIMNT='0'B THEN/* ERROR DURING LABEL       0332 00514000
*                                      PROCESSING? OR ERROR DURING 0332 00515000
*                                      MOUNT?                        */ 00516000
         TM    DDRILAB(DDRPTR),B'00110000'                         0332 00517000
         BNZ   @RF00332                                            0332 00518000
*       DO;                         /* NOT DURING LABEL PROCESSING   */ 00519000
*         IF IOSCC='30'X|(VIOSBUCK='1'B&DDRMT='1'B&VIOSSN1B='0'B&  0334 00520000
*             VIOSSN2B='0'B) THEN   /* IS DEVICE OPERATIONAL? CHECK     00521000
*                                      FOR SIO CONDITION CODE OR THE    00522000
*                                      ERROR IS A UNIT CHECK ON TAPE    00523000
*                                      AND THE TAPE IS NOT ACTIVE .  */ 00524000
         CLI   IOSCC(IOSBPTR),X'30'                                0334 00525000
         BE    @RT00334                                            0334 00526000
         TM    VIOSBUCK(IOSBPTR),B'00000010'                       0334 00527000
         BNO   @RF00334                                            0334 00528000
         TM    DDRMT(DDRPTR),B'00100000'                           0334 00529000
         BNO   @RF00334                                            0334 00530000
         TM    VIOSSN1B(IOSBPTR),B'01100000'                       0334 00531000
         BNZ   @RF00334                                            0334 00532000
@RT00334 DS    0H                                                  0335 00533000
*           IOSNERP='1'B;           /* DEVICE IS NOT OPERATIONAL AND    00534000
*                                      SET NO ERP PROCESSING         */ 00535000
         OI    IOSNERP(IOSBPTR),B'00001000'                        0335 00536000
*         ELSE                      /* DEVICE IS OPERATIONAL         */ 00537000
*           IF DDRIOF1=DDRRUN|(DDRIOF1=DDRNOP&DDRDA='1'B) THEN/* IS     00538000
*                                      I/O COMMAND A REWIND AND    0336 00539000
*                                      UNLOAD OR A DISK NOP ?      0336 00540000
*                                                            @YM05699*/ 00541000
         B     @RC00334                                            0336 00542000
@RF00334 CLI   DDRIOF1(DDRPTR),X'0F'                               0336 00543000
         BE    @RT00336                                            0336 00544000
         CLI   DDRIOF1(DDRPTR),X'03'                               0336 00545000
         BNE   @RF00336                                            0336 00546000
         TM    DDRDA(DDRPTR),B'01000000'                           0336 00547000
         BNO   @RF00336                                            0336 00548000
@RT00336 DS    0H                                                  0337 00549000
*             DO;                   /* YES- A REWIND AND UNLOAD OR A    00550000
*                                      DISK NOP                      */ 00551000
*               IF VIOSBUCK='1'B&VIOSINTR='0'B THEN/* TEST FOR UNIT     00552000
*                                      CHECK AND INTERVENTION      0338 00553000
*                                      REQUIRED              @YM07704*/ 00554000
         TM    VIOSBUCK(IOSBPTR),B'00000010'                       0338 00555000
         BNO   @RF00338                                            0338 00556000
         TM    VIOSINTR(IOSBPTR),B'01000000'                       0338 00557000
         BNZ   @RF00338                                            0338 00558000
*                 IOSEX='0'B;       /* NO INTERVENTION REQUIRED -  0339 00559000
*                                      IGNORE ERROR          @YM05699*/ 00560000
         NI    IOSEX(IOSBPTR),B'11111011'                          0339 00561000
*               ELSE                /* YES- AN INTERVENTION REQUIRED    00562000
*                                      SITUATION                     */ 00563000
*                 IF DDRWHICH='1'B THEN/* A FROM DEVICE?             */ 00564000
         B     @RC00338                                            0340 00565000
@RF00338 TM    DDRWHICH(DDRPTR),B'10000000'                        0340 00566000
         BNO   @RF00340                                            0340 00567000
*                   IOSEX='0'B;     /* YES- IGNORE ERROR     @YM05699*/ 00568000
         NI    IOSEX(IOSBPTR),B'11111011'                          0341 00569000
*                 ELSE                                             0342 00570000
*                   ;               /* CAUSE INTR MSG ON TO DEVICE OR   00571000
*                                      WAIT UNTIL INTERVENTION     0342 00572000
*                                      REQUIRED IS SATISFIED @YM05699*/ 00573000
@RF00340 DS    0H                                                  0343 00574000
*             END;                  /*                       @YM05699*/ 00575000
*           ELSE                    /* I/O COMMAND IS NOT A        0344 00576000
*                                      REWIND&UNLOAD OR A DISK NOP 0344 00577000
*                                                            @YM05699*/ 00578000
*             IF((DDRIOF1=DDRREAD|DDRIOF1=DDRRDBK)&(VIOSBUCK='1'B&(     00579000
*                 VIOSBDCK='1'B|VIOSBOVR='1'B)&DDRMT='1'B)) THEN/* 0344 00580000
*                                      ERROR CAN BE IGNORED IF I/O 0344 00581000
*                                      COMMAND WAS A NOP OR REWIND 0344 00582000
*                                      AND UNLOAD- OR IF I/O WAS A 0344 00583000
*                                      READ OR READ BACKWARDS AND THE   00584000
*                                      ERROR IS A DATA CHECK OR    0344 00585000
*                                      OVERRUN ON A TAPE.            */ 00586000
         B     @RC00336                                            0344 00587000
@RF00336 CLI   DDRIOF1(DDRPTR),X'02'                               0344 00588000
         BE    @GL00015                                            0344 00589000
         CLI   DDRIOF1(DDRPTR),X'0C'                               0344 00590000
         BNE   @RF00344                                            0344 00591000
@GL00015 TM    VIOSBUCK(IOSBPTR),B'00000010'                       0344 00592000
         BNO   @RF00344                                            0344 00593000
         TM    VIOSBDCK(IOSBPTR),B'00001100'                       0344 00594000
         BZ    @RF00344                                            0344 00595000
         TM    DDRMT(DDRPTR),B'00100000'                           0344 00596000
         BNO   @RF00344                                            0344 00597000
*               DO;                 /* IGNORABLE ERROR               */ 00598000
*                 IOSEX='0'B;       /* CLEAR EXCEPTION CONDITION     */ 00599000
         NI    IOSEX(IOSBPTR),B'11111011'                          0346 00600000
*                 IF DDRMT='1'B&((VIOSBDCK='1'B&IOSCSWRC=0)|VIOSBOVR=   00601000
*                     '1'B) THEN    /* TAPE MOVEMENT ? IF THE TAPE 0347 00602000
*                                      ERROR IS A DATA CHECK AND CSW    00603000
*                                      RESIDUAL COUNT IS ZERO - OR IF   00604000
*                                      THE ERROR IS A TAPE OVERRUN ,    00605000
*                                      THE REPEAT COUNT WILL BE    0347 00606000
*                                      DECREMENTED.                  */ 00607000
         TM    DDRMT(DDRPTR),B'00100000'                           0347 00608000
         BNO   @RF00347                                            0347 00609000
         TM    VIOSBDCK(IOSBPTR),B'00001000'                       0347 00610000
         BNO   @GL00020                                            0347 00611000
         LH    @12,IOSCSWRC(,IOSBPTR)                              0347 00612000
         N     @12,@CF02080                                        0347 00613000
         LTR   @12,@12                                             0347 00614000
         BZ    @RT00347                                            0347 00615000
@GL00020 TM    VIOSBOVR(IOSBPTR),B'00000100'                       0347 00616000
         BNO   @RF00347                                            0347 00617000
@RT00347 DS    0H                                                  0348 00618000
*                   CBCOUNT=CBCOUNT-1;/* DECREMENT COUNT             */ 00619000
         L     @12,CBCOUNT(,IOSBPTR)                               0348 00620000
         BCTR  @12,0                                               0348 00621000
         ST    @12,CBCOUNT(,IOSBPTR)                               0348 00622000
*                 ELSE                                             0349 00623000
*                   ;               /* NOT TAPE MOVEMENT             */ 00624000
@RF00347 DS    0H                                                  0350 00625000
*                 IF CBCOUNT^=0 THEN/* IS COUNT NON-ZERO ?           */ 00626000
         L     @12,CBCOUNT(,IOSBPTR)                               0350 00627000
         LTR   @12,@12                                             0350 00628000
         BZ    @RF00350                                            0350 00629000
*                   R14=R14+8;      /* YES- I/O TO BE RE-EXECUTED    */ 00630000
         AL    R14,@CF00038                                        0351 00631000
*                 ELSE                                             0352 00632000
*                   ;               /* COUNT IS ZERO                 */ 00633000
@RF00350 DS    0H                                                  0353 00634000
*               END;                                               0353 00635000
*             ELSE                                                 0354 00636000
*               ;                   /* ERROR CNA NOT BE IGNORED      */ 00637000
@RF00344 DS    0H                                                  0355 00638000
*       END;                                                       0355 00639000
*     ELSE                                                         0356 00640000
*       ;                           /* ERROR DURING LABEL PROCESSING */ 00641000
@RF00332 DS    0H                                                  0357 00642000
*   ELSE                                                           0357 00643000
*     ;                             /* SECOND ENTRY PERMAMENT ERROR  */ 00644000
@RF00331 DS    0H                                                  0358 00645000
*   RETURN;                                                        0358 00646000
@EL00005 DS    0H                                                  0358 00647000
@EF00005 DS    0H                                                  0358 00648000
@ER00005 LM    @15,@12,16(@13)                                     0358 00649000
         BR    @14                                                 0358 00650000
*   END DM1ABN;                                                    0359 00651000
         B     @EL00005                                            0359 00652000
*                                                                  0360 00653000
*   /*****************************************************************/ 00654000
*   /*                                                               */ 00655000
*   /* THIS SUBROUTINE IS SCHEDULED BY IOS UNDER A SRB WHEN THE I/O  */ 00656000
*   /* IS COMPLETE. IT'S FUNCTION IS TO POST THE DDR TASK COMPLETE   */ 00657000
*   /* FOR THE I/O.                                                  */ 00658000
*   /*                                                               */ 00659000
*   /*****************************************************************/ 00660000
*                                                                  0360 00661000
*DM1SRB:                                                           0360 00662000
*   PROC OPTIONS(NOSAVE);                                          0360 00663000
*                                                                  0360 00664000
DM1SRB   DS    0H                                                  0361 00665000
*/*   NO BASE REG NEEDED                                             */ 00666000
*                                                                  0361 00667000
*   IOSBPTR=PARMREG1;               /* WORKAREA IOSB ADDRESS         */ 00668000
         LR    IOSBPTR,PARMREG1                                    0361 00669000
*   SAVERET=R14;                    /* SAVE RETURN ADDRESS           */ 00670000
         LR    SAVERET,R14                                         0362 00671000
*/* OBTAIN LOCAL LOCK                                                */ 00672000
*                                                                  0363 00673000
*   GEN REFS(PSALITA,FLC) SETS(R11,R12,R14);                       0363 00674000
*                                                                  0363 00675000
GETLOC SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                         XX00676000
               RELATED=(LOCAL,IGFDM1(FREELOC))                          00677000
*/*  BRANCH ENTRY TO POST (IEA0PT01)                                 */ 00678000
*                                                                  0364 00679000
*   DDRPTR=0;                       /* CLEAR REG 10 FOR COMPLETE CODE*/ 00680000
         SLR   DDRPTR,DDRPTR                                       0364 00681000
*   R11=ADDR(CBECB);                /* ECB ADDRESS                   */ 00682000
         LA    R11,CBECB(,IOSBPTR)                                 0365 00683000
*   CALL IEA0PT01;                  /* POST BRANCH ENTRY             */ 00684000
         L     @12,CVTPTR                                          0366 00685000
         L     @15,CVT0PT01(,@12)                                  0366 00686000
         BALR  @14,@15                                             0366 00687000
*/* RELEASE LOCAL LOCK                                               */ 00688000
*                                                                  0367 00689000
*   GEN REFS(PSALITA,FLC) SETS(R11,R12,R14);                       0367 00690000
FREELOC SETLOCK RELEASE,TYPE=LOCAL,                                   XX00691000
               RELATED=(LOCAL,IGFDM1(FREELOC))                          00692000
*   R14=SAVERET;                    /* RESTORE RETURN ADDRESS        */ 00693000
         LR    R14,SAVERET                                         0368 00694000
*   RETURN;                         /* RETURN TO CALLER              */ 00695000
@EL00006 DS    0H                                                  0369 00696000
@EF00006 DS    0H                                                  0369 00697000
@ER00006 BR    @14                                                 0369 00698000
*   END DM1SRB;                                                    0370 00699000
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */ 00700000
*     GPR00P PTR(31) REG(0),                                       0371 00701000
*     GPR01P PTR(31) REG(1);                                       0371 00702000
*   DECLARE                         /* COMMON VARIABLES              */ 00703000
*     I256C CHAR(256) BASED,                                       0372 00704000
*     I031F FIXED(31) BASED,                                       0372 00705000
*     I031P PTR(31) BASED,                                         0372 00706000
