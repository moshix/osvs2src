*   /*                                                               */ 00068000
*   /*****************************************************************/ 00069000
*                                                                  0188 00070000
*   IF DDRREC1=DDRRIPL THEN         /* IS IT IPL RECORD REQUEST      */ 00071000
         CLI   DDRREC1(DDRPTR),X'04'                               0188 00072000
         BNE   @RF00188                                            0188 00073000
*     DO;                           /* YES, PROCESS IPL REQUEST      */ 00074000
*       SVC76R0=SVC76IPL;           /* INDICATE IPL RECORD REQUEST   */ 00075000
         LA    SVC76R0,8                                           0190 00076000
*       SVC(76);                    /* RECORD IPL RECORD             */ 00077000
         SVC   76                                                  0191 00078000
*     END;                                                         0192 00079000
*   ELSE                            /* NO, TRY NEXT TYPE             */ 00080000
