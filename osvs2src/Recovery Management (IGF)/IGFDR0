         B     @PB00002                                            0225 00209000
DR0MIH   STM   @14,@12,@SA00002                                    0225 00210000
*   CALL DR0HEAD;                   /* INITIALIZE THE RECORD HEADER  */ 00211000
*                                                                  0226 00212000
         BAL   @14,DR0HEAD                                         0226 00213000
*   /*****************************************************************/ 00214000
*   /*                                                               */ 00215000
*   /* INITIALIZE THE MIH PORTION OF THE RECORD HEADER               */ 00216000
*   /*                                                               */ 00217000
*   /*****************************************************************/ 00218000
*                                                                  0227 00219000
*   LRBHTYPE=LRBHMIH;               /* INDICATE MIH RECORD           */ 00220000
         MVI   LRBHTYPE(LRBPTR),X'70'                              0227 00221000
*   IF DDRMIH=DDRMIHC|DDRMIH=DDRMIHCD THEN/* TEST FOR CHANNEL END  0228 00222000
