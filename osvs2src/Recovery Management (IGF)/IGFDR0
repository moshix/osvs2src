*           ELSE                    /* NO, TRY NEXT TYPE     @YM02580*/ 00142000
*             IF DDRREC1=DDRRRDE THEN/* IS RDE REQUEST?      @YM02580*/ 00143000
         B     @RC00208                                            0210 00144000
@RF00208 CLI   DDRREC1(DDRPTR),X'06'                               0210 00145000
         BNE   @RF00210                                            0210 00146000
*               DO;                 /* YES-RECORD RDE RECORD @YM02580*/ 00147000
*                 SVC76R0=SVC76RDE; /* INDICATE RDE RECORD REQUEST   */ 00148000
         LA    SVC76R0,12                                          0212 00149000
*                 SVC(76);          /* RECORD RDE RECORD     @YM02580*/ 00150000
         SVC   76                                                  0213 00151000
*               END;                /*                       @YM02580*/ 00152000
*             ELSE                  /* NOT A VALID REQUEST           */ 00153000
*               DO;                                                0215 00154000
