*     LRBNDEM='1'B;                 /* IF SO, INDICATE IT            */ 00238000
*                                                                  0231 00239000
         OI    LRBNDEM(LRBPTR),B'01000000'                         0231 00240000
*   /*****************************************************************/ 00241000
*   /*                                                               */ 00242000
*   /* FILL IN THE CPU SERIAL NUMBER                                 */ 00243000
*   /*                                                               */ 00244000
*   /*****************************************************************/ 00245000
*                                                                  0232 00246000
*   UCBPTR=DDRFMUCB;                /* POINT TO UCB                  */ 00247000
@RF00230 L     UCBPTR,DDRFMUCB(,DDRPTR)                            0232 00248000
