*     GPR00P=ADDR(IGFDS1);          /* EP ADDRESS                    */ 00260000
         L     GPR00P,@CV00079                                     0252 00261000
         LR    @15,GPR00P                                          0252 00262000
         LR    GPR00P,@15                                          0252 00263000
*     GPR01F=LENGTH(IQESECT)/8;     /* WORK AREA SIZE                */ 00264000
         LA    GPR01F,3                                            0253 00265000
*     GPR01F=GPR01F|'410A0000'X;    /* SET FLAG BYTES                */ 00266000
         O     GPR01F,@CF02207                                     0254 00267000
*     CALL CVTS1EE;                 /* BRANCH ENTRY POINT            */ 00268000
         L     @12,CVTPTR                                          0255 00269000
         L     @15,CVTS1EE(,@12)                                   0255 00270000
         BALR  @14,@15                                             0255 00271000
*     RFY                                                          0256 00272000
*      (GPR00P,                                                    0256 00273000
*       GPR01F) UNRSTD;             /* RELEASE REGISTERS             */ 00274000
*   END;                            /* CIRB EP(IGFDS1) TYPE(IRB)   0257 00275000
*                                      KEY(SUPR) MODE(SUPR)        0257 00276000
*                                      SVAREA(YES)                 0257 00277000
*                                      WKAREA(LENGTH(IQESECT)/8)   0257 00278000
*                                      STAB(DYN) RETIQE(NO) BRANCH   */ 00279000
*                                                                  0257 00280000
*   /*****************************************************************/ 00281000
*   /*                                                               */ 00282000
*   /* BUILD THE IQE WITHIN THE IRB                                  */ 00283000
*   /*                                                               */ 00284000
*   /*****************************************************************/ 00285000
*                                                                  0258 00286000
*   IQEPTR=ADDR(RBIQEWRK);          /* POINT TO THE IQE WORK AREA    */ 00287000
         LA    IQEPTR,RBIQEWRK(,RBSECPTR)                          0258 00288000
*   IQEFLAGS=IQEFLAGS&&IQEFLAGS;    /* CLEAR IQEFLAGS FIELD  @OZ08764*/ 00289000
         XC    IQEFLAGS(1,IQEPTR),IQEFLAGS(IQEPTR)                 0259 00290000
*   IQEIRBA=RBSECPTR;               /* SET UP THE RB POINTER         */ 00291000
         STCM  RBSECPTR,7,IQEIRBA(IQEPTR)                          0260 00292000
*   IQETCBA=ASXBFTCB;               /* SET UP THE TCB POINTER        */ 00293000
         L     @15,ASCBPTR(,PARMPTR)                               0261 00294000
