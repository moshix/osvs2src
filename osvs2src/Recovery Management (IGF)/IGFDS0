         TITLE 'IGFDS0, DDR SRB ROUTINE                                *00001000
                        '                                               00002000
IGFDS0   CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IGFDS0    76.161'                                 0001 00009000
DS0FRR   DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00284-DS0FRR)                                     00013000
         ENTRY DS0FRR                                                   00014000
         DROP  @15                                                      00015000
@PROLOG  BALR  @08,0                                               0001 00016000
@PSTART  DS    0H                                                  0001 00017000
         USING @PSTART,@08                                         0001 00018000
         AH    @15,4(,@15)                                         0001 00019000
         BR    @15                                                 0001 00020000
@EP00001 DS    0H                                                  0002 00021000
RBPRFXLN EQU 64                                                         00022000
RBPRFLNA EQU 32                                                         00023000
PRBLEN   EQU 136                                                        00024000
SIRBLEN  EQU 200                                                        00025000
TIRBLEN  EQU 136                                                        00026000
IRBLEN   EQU 128                                                        00027000
SVRBLEN  EQU 200                                                        00028000
*   EXITADDR=RTNADDR;               /* SAVE RETURN ADDRESS (IN NON-     00029000
*                                      VOLATILE REGISTER)            */ 00030000
         LR    EXITADDR,RTNADDR                                    0200 00031000
*   SRBPTR=R0;                      /* SAVE ADDRESS OF SRB           */ 00032000
         LR    SRBPTR,R0                                           0201 00033000
*   DDRPTR=R1;                      /* SAVE ADDRESS OF DDRCOM        */ 00034000
         LR    DDRPTR,R1                                           0202 00035000
*   RFY                                                            0203 00036000
*    (R0,                                                          0203 00037000
*     R1,                                                          0203 00038000
*     RTNADDR) UNRSTD;                                             0203 00039000
*                                                                  0203 00040000
*   /*****************************************************************/ 00041000
*   /*                                                               */ 00042000
*   /* OBTAIN THE LOCK(S) REQUIRED TO SERIALIZE THE DDR WAIT QUEUE   */ 00043000
*   /*                                                               */ 00044000
*   /*****************************************************************/ 00045000
*                                                                  0204 00046000
