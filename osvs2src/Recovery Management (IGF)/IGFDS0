         LR    EXITADDR,RTNADDR                                    0248 00239000
*   RTCB=ASXBFTCB;                  /* POINT TO THE TCB FOR THE    0249 00240000
*                                      REGION CONTROL TASK           */ 00241000
*                                                                  0249 00242000
         L     @15,ASCBPTR(,PARMPTR)                               0249 00243000
         L     @15,ASXBPTR(,@15)                                   0249 00244000
         L     RTCB,ASXBFTCB(,@15)                                 0249 00245000
*   /*****************************************************************/ 00246000
*   /*                                                               */ 00247000
*   /* CREATE THE IRB FOR THE DDR TASK TO RUN UNDER                  */ 00248000
*   /*                                                               */ 00249000
*   /*****************************************************************/ 00250000
*                                                                  0250 00251000
*   DO;                             /* CIRB EP(IGFDS1) TYPE(IRB)   0250 00252000
*                                      KEY(SUPR) MODE(SUPR)        0250 00253000
*                                      SVAREA(YES)                 0250 00254000
*                                      WKAREA(LENGTH(IQESECT)/8)   0250 00255000
*                                      STAB(DYN) RETIQE(NO) BRANCH   */ 00256000
*     RFY                                                          0251 00257000
*      (GPR00P,                                                    0251 00258000
