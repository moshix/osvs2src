*   /* SUBROUTINE DS0ADDEL - ADD A DDRCOM TO THE DDR WAIT QUEUE      */ 00333000
*   /*                                                               */ 00334000
*   /*****************************************************************/ 00335000
*                                                                  0270 00336000
*DS0ADDEL:                          /* SUBROUTINE ENTRY POINT        */ 00337000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0270 00338000
         B     @PB00003                                            0270 00339000
DS0ADDEL DS    0H                                                  0271 00340000
*   RFY                                                            0271 00341000
*    (RTNADDR,                                                     0271 00342000
*     DDRPTR) RSTD;                                                0271 00343000
*   DDRPTR=0;                       /* SET UP DUMMY DDRCOM POINTER   */ 00344000
         SLR   DDRPTR,DDRPTR                                       0272 00345000
*   DDRPTR=ADDR(ASXBDDR)-(ADDR(DDRNXT)-ADDR(DDRSHORT));/* POINT TO 0273 00346000
*                                      VIRTUAL DDRCOM OVERLAYING THE    00347000
*                                      ASXB SO THAT ASXBDDR        0273 00348000
*                                      CORRESPONDS TO DDRNXT         */ 00349000
*                                                                  0273 00350000
         L     @15,ASCBPTR(,PARMPTR)                               0273 00351000
         L     @15,ASXBPTR(,@15)                                   0273 00352000
         LA    @15,ASXBDDR(,@15)                                   0273 00353000
         SL    @15,@CF00109                                        0273 00354000
         LR    DDRPTR,@15                                          0273 00355000
*   /*****************************************************************/ 00356000
*   /*                                                               */ 00357000
*   /* FIND THE PROPER PLACE ON THE WAIT QUEUE FOR THIS DDRCOM       */ 00358000
*   /*                                                               */ 00359000
*   /*****************************************************************/ 00360000
*                                                                  0274 00361000
*   DO WHILE(DDRNXT^=0&(DDRNXT->DDRSYS='1'B|DS0PDDR->DDROPER='1'B));    00362000
         B     @DE00274                                            0274 00363000
@DL00274 DS    0H                                                  0275 00364000
*     DDRPTR=DDRNXT;                /* POINT TO NEXT DDRCOM ON QUEUE */ 00365000
         L     DDRPTR,DDRNXT(,DDRPTR)                              0275 00366000
*   END;                            /* LOOP UNTIL PROPER PLACE IS  0276 00367000
*                                      FOUND (FOR SYSTEM SWAPS, END     00368000
*                                      OF SYSTEM DDRCOMS, FOR      0276 00369000
*                                      OPERATOR SWAPS, END OF QUEUE) */ 00370000
*                                                                  0276 00371000
@DE00274 L     @15,DDRNXT(,DDRPTR)                                 0276 00372000
         LTR   @15,@15                                             0276 00373000
         BZ    @DC00274                                            0276 00374000
         TM    DDRSYS(@15),B'01000000'                             0276 00375000
         BO    @DL00274                                            0276 00376000
         L     @15,DS0PDDR(,PARMPTR)                               0276 00377000
         TM    DDROPER(@15),B'10000000'                            0276 00378000
         BO    @DL00274                                            0276 00379000
@DC00274 DS    0H                                                  0277 00380000
*   /*****************************************************************/ 00381000
*   /*                                                               */ 00382000
*   /* ADD THE DDRCOM TO THE DDR WAIT QUEUE                          */ 00383000
*   /*                                                               */ 00384000
*   /*****************************************************************/ 00385000
*                                                                  0277 00386000
*   DS0PDDR->DDRNXT=DDRNXT;         /* SET UP NEXT POINTER IN NEW  0277 00387000
*                                      DDRCOM                        */ 00388000
         L     @15,DS0PDDR(,PARMPTR)                               0277 00389000
         L     @12,DDRNXT(,DDRPTR)                                 0277 00390000
         ST    @12,DDRNXT(,@15)                                    0277 00391000
*   DDRNXT=DS0PDDR;                 /* SET UP NEXT POINTER IN      0278 00392000
*                                      PREVIOUS DDRCOM ON QUEUE      */ 00393000
         ST    @15,DDRNXT(,DDRPTR)                                 0278 00394000
*   DS0PSQ='1'B;                    /* INDICATE DDRCOM ON QUEUE      */ 00395000
