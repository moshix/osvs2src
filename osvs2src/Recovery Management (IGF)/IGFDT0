         TITLE 'IGFDT0, DDR - TAPE SELECT ROUTINE                      *00001000
                        '                                               00002000
IGFDT0   CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGFDT0    76.362'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @08,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@08                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @09,@01                                             0001 00016000
         USING @DATD,@09                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @09,8(,@13)                                         0001 00020000
         LR    @13,@09                                             0001 00021000
*                                      SAVED BY RESTART FLIH MDC078  */ 00022000
*   DO;                             /* ?IGFDERR INIT                 */ 00023000
*                                                                  0109 00024000
*     /***************************************************************/ 00025000
*     /*                                                             */ 00026000
*     /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT                   */ 00027000
*     /*                                                             */ 00028000
*     /***************************************************************/ 00029000
*                                                                  0110 00030000
*     DERPLIST=DERPLIST&&DERPLIST;  /* CLEAR PARAMETER LIST          */ 00031000
         XC    DERPLIST(48),DERPLIST                               0110 00032000
*     DERFUNK=DERINIT;              /* INDICATE INIT FUNCTION        */ 00033000
         MVI   DERFUNK,X'01'                                       0111 00034000
*     DERREC=ADDR(DERRECPM);        /* POINT TO REC PARMS            */ 00035000
         LA    @14,DERRECPM                                        0112 00036000
         ST    @14,DERREC                                          0112 00037000
*     DERRETRY=ADDR(IGFDT0R);       /* POINT TO RETRY ROUTINE        */ 00038000
         LA    @14,IGFDT0R                                         0113 00039000
         ST    @14,DERRETRY                                        0113 00040000
*     GEN SETS(DERDATD)(MVC DERDATD,@SIZDATD);/* SET UP SPN, SIZE    */ 00041000
         MVC DERDATD,@SIZDATD                                           00042000
*     DERTAPE='1'B;                 /* INDICATE RESOURCE HELD        */ 00043000
         OI    DERTAPE,B'10000000'                                 0115 00044000
*     DERPFX=DERRMASK;              /* SET UP RUB MASK               */ 00045000
         MVC   DERPFX(2),@CB01009                                  0116 00046000
*     DERRCODE=CODEBASE;            /* SAVE CODEREG                  */ 00047000
         ST    CODEBASE,DERRCODE                                   0117 00048000
*     DERRDATA=DATABASE;            /* SAVE DATAREG                  */ 00049000
         ST    DATABASE,DERRDATA                                   0118 00050000
*     DERRDDR=DDRPTR;               /* SAVE DDRCOM POINTER           */ 00051000
         ST    DDRPTR,DERRDDR                                      0119 00052000
*     DERRSAVE=SAVEPTR;             /* SAVE REGISTER 13              */ 00053000
         ST    SAVEPTR,DERRSAVE                                    0120 00054000
*     CALL IGFDE0(DERPLIST);        /* ISSUE ESTAE                   */ 00055000
         LA    @14,DERPLIST                                        0121 00056000
         ST    @14,@AL00001                                        0121 00057000
         L     @15,@CV00058                                        0121 00058000
         LA    @01,@AL00001                                        0121 00059000
         BALR  @14,@15                                             0121 00060000
*   END;                            /* ?IGFDERR INIT                 */ 00061000
*   DDRINV2=DDRVFM;                 /* SET UP TO VALIDATE 'FROM'     */ 00062000
         MVI   DDRINV2(DDRPTR),X'01'                               0123 00063000
*   CALL IGFDV1;                    /* VALIDATE 'FROM' - LOOK UP   0124 00064000
*                                      'FROM' UCB                    */ 00065000
         L     @15,@CV00063                                        0124 00066000
         BALR  @14,@15                                             0124 00067000
*   DERLRC=RTNCODE;                 /* SAVE RETURN CODE              */ 00068000
         STC   RTNCODE,DERLRC                                      0125 00069000
*   IF DERLRC^=CATERR THEN          /* WAS 'FROM' DEVICE FOUND       */ 00070000
         CLI   DERLRC,12                                           0126 00071000
         BE    @RF00126                                            0126 00072000
*     DO;                           /* YES, CONTINUE PROCESSING      */ 00073000
*       DDRINV2=DDRVUS;             /* SET UP TO VALIDATE USER       */ 00074000
         MVI   DDRINV2(DDRPTR),X'03'                               0128 00075000
*       CALL IGFDT1;                /* VALIDATE USER - LOOK UP USERS    00076000
*                                      DEB, DCB, ETC.                */ 00077000
         L     @15,@CV00060                                        0129 00078000
         BALR  @14,@15                                             0129 00079000
*       DERLRC=RTNCODE;             /* SAVE RETURN CODE              */ 00080000
         STC   RTNCODE,DERLRC                                      0130 00081000
*       IF DERLRC^=VALID THEN       /* WAS USER FOUND                */ 00082000
         CLI   DERLRC,0                                            0131 00083000
         BE    @RF00131                                            0131 00084000
*         DDRTER1=DDRTNF;           /* NO, INDICATE USER NOT FOUND   */ 00085000
         MVI   DDRTER1(DDRPTR),X'01'                               0132 00086000
*       ELSE                        /* YES, USER WAS FOUND           */ 00087000
*         DO;                       /* CONTINUE PROCESSING           */ 00088000
*                                                                  0133 00089000
         B     @RC00131                                            0133 00090000
@RF00131 DS    0H                                                  0134 00091000
*           /*********************************************************/ 00092000
*           /*                                                       */ 00093000
*           /* SWAP THE 'FROM' TAPE TO A SUITABLE COMPATIBLE DEVICE -*/ 00094000
*           /* ONCE THE SWAP IS INITIATED, IT WILL BE ATTEMPTED      */ 00095000
*           /* REPETITIVELY UNTIL THE TAPE IS SUCESSFULLY SWAPPED,   */ 00096000
*           /* THE OPERATOR TELLS DDR TO CANCEL THE SWAP, OR DDR IS  */ 00097000
*           /* TERMINATED DUE TO A CATASTROPHIC ERROR DURING SWAP    */ 00098000
*           /* PROCESSING                                            */ 00099000
*           /*                                                       */ 00100000
*           /*********************************************************/ 00101000
*                                                                  0134 00102000
*           DERLRC=NOTVALRC;        /* FORCE LOOP TO BE EXECUTED AT     00103000
*                                      LEAST ONCE                    */ 00104000
         MVI   DERLRC,X'10'                                        0134 00105000
*           DO;                     /* ENQ (QNAME,RNAME,E,,SYSTEM)   */ 00106000
*             RESPECIFY                                            0136 00107000
*              (GPR01P) RESTRICTED;                                0136 00108000
*             I056(1:16)=I056(1:16)&&I056(1:16);/* ZERO OUT THE TABLE*/ 00109000
         XC    I056(16),I056                                       0137 00110000
*             I05604(1)=LENGTH(RNAME);/* LENGTH OF MINOR RESOURCE  0138 00111000
*                                      NAME                          */ 00112000
         MVI   I05604,X'07'                                        0138 00113000
*             I05606(1)=ADDR(QNAME);/* ADDRESS OF MAJOR RESOURCE NAME*/ 00114000
         LA    @14,QNAME                                           0139 00115000
         ST    @14,I05606                                          0139 00116000
*             I05607(1)=ADDR(RNAME);/* ADDRESS OF MINOR RESOURCE NAME*/ 00117000
         LA    @14,RNAME                                           0140 00118000
         ST    @14,I05607                                          0140 00119000
*             I05603(1)='01000000'B;/* SET BIT STRING 1 FOR RESOURCE    00120000
*                                      ELEMENT                       */ 00121000
         MVI   I05603,B'01000000'                                  0141 00122000
*             I05605(1)='01000000'B;/* SET BIT STRING 2 FOR RESOURCE    00123000
*                                      ELEMENT                       */ 00124000
         MVI   I05605,B'01000000'                                  0142 00125000
*             I05603(1,1)='1'B;     /* SET END OF LIST BIT           */ 00126000
         OI    I05603,B'10000000'                                  0143 00127000
*             GPR01P=ADDR(I05602);  /* LOAD LIST ADDRESS             */ 00128000
         LA    GPR01P,I05602                                       0144 00129000
*             SVC(56);              /* ISSUE ENQ SVC                 */ 00130000
         SVC   56                                                  0145 00131000
*             RESPECIFY                                            0146 00132000
*              (GPR01P) UNRESTRICTED;                              0146 00133000
*           END;                    /* ENQ (QNAME,RNAME,E,,SYSTEM)   */ 00134000
*                                                                  0147 00135000
*           /*********************************************************/ 00136000
*           /*                                                       */ 00137000
*           /* INCREMENT THE TAPE ALLOCATION COUNT BY 1 - COMPARE AND*/ 00138000
*           /* SWAP MUST BE USED TO PREVENT INTERFERENCE FROM ANY    */ 00139000
*           /* OTHER CPU                                             */ 00140000
*           /*                                                       */ 00141000
*           /*********************************************************/ 00142000
*                                                                  0148 00143000
*           CSOLDVAL=CSDTCNT;       /* PICK UP CURRENT COUNT         */ 00144000
         L     @14,CVTPTR                                          0148 00145000
         L     @14,CVTCSD(,@14)                                    0148 00146000
         L     CSOLDVAL,CSDTCNT(,@14)                              0148 00147000
*CSLOOP1:                                                          0149 00148000
*           CSNEWVAL=CSOLDVAL+1;    /* ADD 1 TO COUNT                */ 00149000
CSLOOP1  LA    CSNEWVAL,1                                          0149 00150000
         ALR   CSNEWVAL,CSOLDVAL                                   0149 00151000
*           CS(CSOLDVAL,CSNEWVAL,CSDTCNT);/* ATTEMPT TO UPDATE COUNT    00152000
*                                      IN CSDA                       */ 00153000
         L     @03,CVTPTR                                          0150 00154000
         L     @03,CVTCSD(,@03)                                    0150 00155000
         CS    CSOLDVAL,@02,CSDTCNT(@03)                           0150 00156000
*           BC(CSNOGO,CSLOOP1);     /* LOOP UNTIL COUNT IS UPDATED   */ 00157000
         BC    7,CSLOOP1                                           0151 00158000
*           DDRRTENQ='1'B;          /* INDICATE TAPE ALLOCATION    0152 00159000
*                                      RESOURCE NOW HELD BY DDR      */ 00160000
         OI    DDRRTENQ(DDRPTR),B'10000000'                        0152 00161000
*           DO WHILE(DERLRC^=VALID&DERLRC^=CATERR);/* LOOP UNTIL SWAP   00162000
*                                      DONE OR CANNOT TRY AGAIN      */ 00163000
*                                                                  0153 00164000
         B     @DE00153                                            0153 00165000
@DL00153 DS    0H                                                  0154 00166000
*             /*******************************************************/ 00167000
*             /*                                                     */ 00168000
*             /* ATTEMPT TO PERFORM THE SWAP                         */ 00169000
*             /*                                                     */ 00170000
*             /*******************************************************/ 00171000
*                                                                  0154 00172000
*             CALL IGFDV0;          /* PERFORM THE SWAP              */ 00173000
         L     @15,@CV00062                                        0154 00174000
         BALR  @14,@15                                             0154 00175000
*             IF RTNCODE^=VALID THEN/* WAS SWAP SUCCESSFUL           */ 00176000
         LTR   RTNCODE,RTNCODE                                     0155 00177000
         BZ    @RF00155                                            0155 00178000
*               DERLRC=CATERR;      /* NO, INDICATE SWAP IS TO BE  0156 00179000
*                                      TERMINATED                    */ 00180000
         MVI   DERLRC,X'0C'                                        0156 00181000
*             ELSE                  /* YES, CONTINUE SWAP            */ 00182000
*                                                                  0157 00183000
*               /*****************************************************/ 00184000
*               /*                                                   */ 00185000
*               /* IGF502E PROCEED WITH SWAP XXX TO YYY              */ 00186000
*               /*                                                   */ 00187000
*               /*****************************************************/ 00188000
*                                                                  0157 00189000
*               DO;                 /* ?IGFDMSG MSG(IGF502E)CODE(FROM   00190000
*                                      -CUA,TO-CUA)RETRY(6)          */ 00191000
*                                                                  0157 00192000
         B     @RC00155                                            0157 00193000
@RF00155 DS    0H                                                  0158 00194000
*                 /***************************************************/ 00195000
*                 /*                                                 */ 00196000
*                 /* IGFDMSG - INVOKE THE DDR/MIH MESSAGE WRITER     */ 00197000
*                 /*                                                 */ 00198000
*                 /***************************************************/ 00199000
*                                                                  0158 00200000
*                 DDRMSGCD=DDRM502E;/* SET UP MSG CODE               */ 00201000
         MVI   DDRMSGCD(DDRPTR),X'03'                              0158 00202000
*                 DDRMSGPN=2;       /* SET UP NO. OF SUBST. CODES    */ 00203000
         MVI   DDRMSGPN(DDRPTR),X'02'                              0159 00204000
*                 DDRMSGPC(1)=DDRPFM;/* SET SUBSTITUTION CODE 1      */ 00205000
         MVI   DDRMSGPC(DDRPTR),X'02'                              0160 00206000
*                 DDRMSGPC(2)=DDRPTO;/* SET SUBSTITUTION CODE 2      */ 00207000
         MVI   DDRMSGPC+1(DDRPTR),X'03'                            0161 00208000
*                 RTNCODE=12;       /* INDICATE MESSAGE NOT DONE YET */ 00209000
         LA    RTNCODE,12                                          0162 00210000
*                 DO DDRRETRY=1 TO 6 WHILE(RTNCODE=12);/* LOOP UNTIL    00211000
*                                      MSG DONE                      */ 00212000
         LA    @14,1                                               0163 00213000
         STC   @14,DDRRETRY(,DDRPTR)                               0163 00214000
@DL00163 C     RTNCODE,@CF00041                                    0163 00215000
         BNE   @DC00163                                            0163 00216000
*                   CALL IGFDM0;    /* OUTPUT MESSAGE                */ 00217000
         L     @15,@CV00059                                        0164 00218000
         BALR  @14,@15                                             0164 00219000
*                 END;              /* END OF RETRY LOOP             */ 00220000
         LA    @14,1                                               0165 00221000
         SLR   @12,@12                                             0165 00222000
         IC    @12,DDRRETRY(,DDRPTR)                               0165 00223000
         ALR   @14,@12                                             0165 00224000
         STC   @14,DDRRETRY(,DDRPTR)                               0165 00225000
         C     @14,@CF01054                                        0165 00226000
         BNH   @DL00163                                            0165 00227000
@DC00163 DS    0H                                                  0166 00228000
*               END;                /* ?IGFDMSG MSG(IGF502E)CODE(FROM   00229000
*                                      -CUA,TO-CUA)RETRY(6)          */ 00230000
*             IF RTNCODE^=VALID THEN/* WAS MESSAGE OK                */ 00231000
@RC00155 LTR   RTNCODE,RTNCODE                                     0167 00232000
         BZ    @RF00167                                            0167 00233000
*               DERLRC=CATERR;      /* NO, TERMINATE SWAP            */ 00234000
         MVI   DERLRC,X'0C'                                        0168 00235000
*             ELSE                  /* YES, CONTINUE SWAP            */ 00236000
*               DO;                                                0169 00237000
*                                                                  0169 00238000
         B     @RC00167                                            0169 00239000
@RF00167 DS    0H                                                  0170 00240000
*                 /***************************************************/ 00241000
*                 /*                                                 */ 00242000
*                 /* REPOSITION THE TAPE ON THE 'TO' DEVICE          */ 00243000
*                 /*                                                 */ 00244000
*                 /***************************************************/ 00245000
*                                                                  0170 00246000
*                 CALL IGFDT2;      /* REPOSITION THE TAPE           */ 00247000
         L     @15,@CV00061                                        0170 00248000
         BALR  @14,@15                                             0170 00249000
*                 DERLRC=RTNCODE;   /* COPY RETURN CODE              */ 00250000
         STC   RTNCODE,DERLRC                                      0171 00251000
*                 IF RTNCODE=DEVCERR|RTNCODE=WRNGVOL THEN/* WAS THERE   00252000
*                                      AN ERROR WHILE REPOSITIONING     00253000
*                                      THE TAPE                      */ 00254000
         C     RTNCODE,@CF00045                                    0172 00255000
         BE    @RT00172                                            0172 00256000
         C     RTNCODE,@CF00038                                    0172 00257000
         BNE   @RF00172                                            0172 00258000
@RT00172 DS    0H                                                  0173 00259000
*                   DO;             /* YES, RETRYABLE ERROR          */ 00260000
*                                                                  0173 00261000
*                     /***********************************************/ 00262000
*                     /*                                             */ 00263000
*                     /* SET UP TO REDO THE SWAP ON ANOTHER DEVICE   */ 00264000
*                     /*                                             */ 00265000
*                     /***********************************************/ 00266000
*                                                                  0174 00267000
*                     DDRFMCUA=DDRTOCUA;/* MAKE THE OLD 'TO' DEVICE     00268000
*                                      THE 'FROM' DEVICE FOR THE NEW    00269000
*                                      SWAP                          */ 00270000
         MVC   DDRFMCUA(3,DDRPTR),DDRTOCUA(DDRPTR)                 0174 00271000
*                     DDRFMUCB=DDRTOUCB;/* SWAP UCB ADDRESSES        */ 00272000
         L     @14,DDRTOUCB(,DDRPTR)                               0175 00273000
         ST    @14,DDRFMUCB(,DDRPTR)                               0175 00274000
*                     DDRTOCUA=0;   /* INDICATE NO 'TO' CUA GIVEN    */ 00275000
         SLR   @14,@14                                             0176 00276000
         STCM  @14,7,DDRTOCUA(DDRPTR)                              0176 00277000
*                     DDRTOUCB=0;   /* INDICATE 'TO' UCB NOT FOUND   */ 00278000
         ST    @14,DDRTOUCB(,DDRPTR)                               0177 00279000
*                     IF DERLRC=DEVCERR THEN/* WAS IT AN I/O ERROR ON   00280000
*                                      THE DEVICE                    */ 00281000
         CLI   DERLRC,4                                            0178 00282000
         BNE   @RF00178                                            0178 00283000
*                       DO;         /* YES, DEVICE ERROR             */ 00284000
*                                                                  0179 00285000
*                         /*******************************************/ 00286000
*                         /*                                         */ 00287000
*                         /* IGF503I ERROR ON XXX, SELECT NEW DEVICE */ 00288000
*                         /*                                         */ 00289000
*                         /*******************************************/ 00290000
*                                                                  0180 00291000
*                         DO;       /* ?IGFDMSG MSG(IGF503I)CODE(FROM   00292000
*                                      -CUA)RETRY(2)                 */ 00293000
*                                                                  0180 00294000
*                           /*****************************************/ 00295000
*                           /*                                       */ 00296000
*                           /* IGFDMSG - INVOKE THE DDR/MIH MESSAGE  */ 00297000
*                           /* WRITER                                */ 00298000
*                           /*                                       */ 00299000
*                           /*****************************************/ 00300000
*                                                                  0181 00301000
*                           DDRMSGCD=DDRM503I;/* SET UP MSG CODE     */ 00302000
         MVI   DDRMSGCD(DDRPTR),X'04'                              0181 00303000
*                           DDRMSGPN=1;/* SET UP NO. OF SUBST. CODES */ 00304000
         MVI   DDRMSGPN(DDRPTR),X'01'                              0182 00305000
*                           DDRMSGPC(1)=DDRPFM;/* SET SUBSTITUTION 0183 00306000
*                                      CODE 1                        */ 00307000
         MVI   DDRMSGPC(DDRPTR),X'02'                              0183 00308000
*                           RTNCODE=12;/* INDICATE MESSAGE NOT DONE     00309000
*                                      YET                           */ 00310000
         LA    RTNCODE,12                                          0184 00311000
*                           DO DDRRETRY=1 TO 2 WHILE(RTNCODE=12);/*     00312000
*                                      LOOP UNTIL MSG DONE           */ 00313000
         LA    @14,1                                               0185 00314000
         STC   @14,DDRRETRY(,DDRPTR)                               0185 00315000
@DL00185 C     RTNCODE,@CF00041                                    0185 00316000
         BNE   @DC00185                                            0185 00317000
*                             CALL IGFDM0;/* OUTPUT MESSAGE          */ 00318000
         L     @15,@CV00059                                        0186 00319000
         BALR  @14,@15                                             0186 00320000
*                           END;    /* END OF RETRY LOOP             */ 00321000
         LA    @14,1                                               0187 00322000
         SLR   @12,@12                                             0187 00323000
         IC    @12,DDRRETRY(,DDRPTR)                               0187 00324000
         ALR   @14,@12                                             0187 00325000
         STC   @14,DDRRETRY(,DDRPTR)                               0187 00326000
         C     @14,@CF00141                                        0187 00327000
         BNH   @DL00185                                            0187 00328000
@DC00185 DS    0H                                                  0188 00329000
*                         END;      /* ?IGFDMSG MSG(IGF503I)CODE(FROM   00330000
*                                      -CUA)RETRY(2)                 */ 00331000
*                         DDRTER1=0;/* IGNORE MESSAGE ERRORS         */ 00332000
         MVI   DDRTER1(DDRPTR),X'00'                               0189 00333000
*                       END;                                       0190 00334000
*                   END;                                           0191 00335000
@RF00178 DS    0H                                                  0192 00336000
*               END;                                               0192 00337000
@RF00172 DS    0H                                                  0193 00338000
*           END;                                                   0193 00339000
@RC00167 DS    0H                                                  0193 00340000
@DE00153 CLI   DERLRC,0                                            0193 00341000
         BE    @DC00153                                            0193 00342000
         CLI   DERLRC,12                                           0193 00343000
         BNE   @DL00153                                            0193 00344000
@DC00153 DS    0H                                                  0194 00345000
*         END;                      /* RECYCLE SWAP UNTIL FINISHED OR   00346000
*                                      TERMINATED                    */ 00347000
*     END;                                                         0195 00348000
*                                                                  0195 00349000
@RC00131 DS    0H                                                  0196 00350000
*   /*****************************************************************/ 00351000
*   /*                                                               */ 00352000
*   /* END OF TAPE SWAP PROCESSING - CLEAN UP AND EXIT               */ 00353000
*   /*                                                               */ 00354000
*   /*****************************************************************/ 00355000
*                                                                  0196 00356000
*   IF DDRRTENQ='1'B THEN           /* IS TAPE ALLOCATION RESOURCE 0196 00357000
*                                      STILL HELD                    */ 00358000
@RF00126 TM    DDRRTENQ(DDRPTR),B'10000000'                        0196 00359000
         BNO   @RF00196                                            0196 00360000
*     DO;                           /* YES, FREE IT                  */ 00361000
*       DO;                         /* DEQ (QNAME,RNAME,,SYSTEM)     */ 00362000
*         I048(1:20)=I048(1:20)&&I048(1:20);                       0199 00363000
         XC    I048(20),I048                                       0199 00364000
*         RFY                                                      0200 00365000
*           GPR01F RESTRICTED;                                     0200 00366000
*         I04804(1)='11000000'B;    /* LISTEND BYTE                  */ 00367000
         MVI   I04804,B'11000000'                                  0201 00368000
*         I04805(1)=LENGTH(RNAME);  /* SET LENGTH                    */ 00369000
         MVI   I04805,X'07'                                        0202 00370000
*         I04806(1)='01000000'B;    /* OPTIONS                       */ 00371000
         MVI   I04806,B'01000000'                                  0203 00372000
*         I04807(1)=ADDR(QNAME);    /* QNAME ADDRESS                 */ 00373000
         LA    @14,QNAME                                           0204 00374000
         ST    @14,I04807                                          0204 00375000
*         I04808(1)=ADDR(RNAME);    /* RNAME ADDRESS                 */ 00376000
         LA    @14,RNAME                                           0205 00377000
         ST    @14,I04808                                          0205 00378000
*         GPR01F=ADDR(I04802);                                     0206 00379000
         LA    GPR01F,I04802                                       0206 00380000
