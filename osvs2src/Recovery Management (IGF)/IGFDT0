*           GPR01F UNRESTRICTED;                                   0208 00384000
*       END;                        /* DEQ (QNAME,RNAME,,SYSTEM)     */ 00385000
*                                                                  0209 00386000
*       /*************************************************************/ 00387000
*       /*                                                           */ 00388000
*       /* DECREMENT THE TAPE ALLOCATION COUNT                       */ 00389000
*       /*                                                           */ 00390000
*       /*************************************************************/ 00391000
*                                                                  0210 00392000
*       CSOLDVAL=CSDTCNT;           /* PICK UP CURRENT COUNT         */ 00393000
         L     @14,CVTPTR                                          0210 00394000
         L     @14,CVTCSD(,@14)                                    0210 00395000
         L     CSOLDVAL,CSDTCNT(,@14)                              0210 00396000
*CSLOOP3:                                                          0211 00397000
*       CSNEWVAL=CSOLDVAL-1;        /* DECREMENT COUNT               */ 00398000
CSLOOP3  LR    CSNEWVAL,CSOLDVAL                                   0211 00399000
         BCTR  CSNEWVAL,0                                          0211 00400000
*       CS(CSOLDVAL,CSNEWVAL,CSDTCNT);/* ATTEMPT TO UPDATE COUNT IN     00401000
*                                      CSDA                          */ 00402000
         L     @03,CVTPTR                                          0212 00403000
         L     @03,CVTCSD(,@03)                                    0212 00404000
         CS    CSOLDVAL,@02,CSDTCNT(@03)                           0212 00405000
*       BC(CSNOGO,CSLOOP3);         /* LOOP UNTIL COUNT IS UPDATED   */ 00406000
         BC    7,CSLOOP3                                           0213 00407000
*       DDRRTENQ='0'B;              /* INDICATE TAPE ALLOCATION    0214 00408000
*                                      RESOURCE NO LONGER HELD BY DDR*/ 00409000
         NI    DDRRTENQ(DDRPTR),B'01111111'                        0214 00410000
*     END;                                                         0215 00411000
*IGFDT0R:                           /* ESTAE RETRY ROUTINE ENTRY PT  */ 00412000
*   DO;                             /* ?IGFDERR CANCEL               */ 00413000
*                                                                  0216 00414000
@RF00196 DS    0H                                                  0216 00415000
