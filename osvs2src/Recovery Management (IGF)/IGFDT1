         TITLE 'IGFDT1, DDR - TAPE VALIDATE ROUTINE                    *00001000
                        '                                               00002000
IGFDT1   CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                             0001 00008000
         DC    C'IGFDT1    76.258'                                 0001 00009000
SRBRECOV DS    0H                                                  0001 00010000
         USING *,@15                                               0001 00011000
         B     @PROLOG                                             0001 00012000
         DC    AL2(@EP00411-SRBRECOV)                                   00013000
         ENTRY SRBRECOV                                                 00014000
         DROP  @15                                                      00015000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00016000
         BALR  @08,0                                               0001 00017000
@PSTART  DS    0H                                                  0001 00018000
         USING @PSTART,@08                                         0001 00019000
         L     @00,@SIZDATD                                        0001 00020000
         GETMAIN  R,LV=(0)                                              00021000
         LR    @09,@01                                             0001 00022000
         USING @DATD,@09                                           0001 00023000
         ST    @13,@SA00001+4                                      0001 00024000
         LM    @15,@01,16(@13)                                     0001 00025000
         ST    @09,8(,@13)                                         0001 00026000
         LR    @13,@09                                             0001 00027000
         AH    @15,4(,@15)                                         0001 00028000
         BR    @15                                                 0001 00029000
@EP00001 DS    0H                                                  0002 00030000
*   DO;                             /* ?IGFDERR INIT                 */ 00031000
*                                                                  0194 00032000
*     /***************************************************************/ 00033000
*     /*                                                             */ 00034000
*     /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT                   */ 00035000
*     /*                                                             */ 00036000
*     /***************************************************************/ 00037000
*                                                                  0195 00038000
*     DERPLIST=DERPLIST&&DERPLIST;  /* CLEAR PARAMETER LIST          */ 00039000
         XC    DERPLIST(48),DERPLIST                               0195 00040000
*     DERFUNK=DERINIT;              /* INDICATE INIT FUNCTION        */ 00041000
         MVI   DERFUNK,X'01'                                       0196 00042000
*     DERREC=ADDR(DERRECPM);        /* POINT TO REC PARMS            */ 00043000
         LA    @14,DERRECPM                                        0197 00044000
         ST    @14,DERREC                                          0197 00045000
*     DERRETRY=ADDR(IGFDT1R);       /* POINT TO RETRY ROUTINE        */ 00046000
         LA    @14,IGFDT1R                                         0198 00047000
         ST    @14,DERRETRY                                        0198 00048000
*     GEN SETS(DERDATD)(MVC DERDATD,@SIZDATD);/* SET UP SPN, SIZE    */ 00049000
         MVC DERDATD,@SIZDATD                                           00050000
*     DERPFX=DERRMASK;              /* SET UP RUB MASK               */ 00051000
         MVC   DERPFX(2),@CB01667                                  0200 00052000
*     DERRCODE=CODEBASE;            /* SAVE CODEREG                  */ 00053000
         ST    CODEBASE,DERRCODE                                   0201 00054000
*     DERRDATA=DATABASE;            /* SAVE DATAREG                  */ 00055000
         ST    DATABASE,DERRDATA                                   0202 00056000
*     DERRDDR=DDRPTR;               /* SAVE DDRCOM POINTER           */ 00057000
         ST    DDRPTR,DERRDDR                                      0203 00058000
*     DERRSAVE=SAVEPTR;             /* SAVE REGISTER 13              */ 00059000
         ST    SAVEPTR,DERRSAVE                                    0204 00060000
*     CALL IGFDE0(DERPLIST);        /* ISSUE ESTAE                   */ 00061000
         LA    @14,DERPLIST                                        0205 00062000
         ST    @14,@AL00001                                        0205 00063000
         L     @15,@CV00095                                        0205 00064000
         LA    @01,@AL00001                                        0205 00065000
         BALR  @14,@15                                             0205 00066000
*   END;                            /* ?IGFDERR INIT                 */ 00067000
*   DERLRC=INVALID;                 /* INDICATE VALIDATION FAILED -     00068000
*                                      IF VALIDATION IS SUCCESSFUL,     00069000
*                                      THE RETURN CODE WILL BE RESET */ 00070000
*                                                                  0207 00071000
         MVI   DERLRC,X'04'                                        0207 00072000
*   /*****************************************************************/ 00073000
*   /*                                                               */ 00074000
*   /* PERFORM DEVICE INDEPENDENT VALIDATION                         */ 00075000
*   /*                                                               */ 00076000
*   /*****************************************************************/ 00077000
*                                                                  0208 00078000
*   CALL IGFDV1;                    /* NOTE - IGFDV1 RETURNS THE   0208 00079000
*                                      ADDRESS OF THE UCB FOR 'FROM'    00080000
*                                      OR 'TO' VALIDATION IN DDRFMUCB   00081000
*                                      OR DDRTOUCB                   */ 00082000
         L     @15,@CV00096                                        0208 00083000
         BALR  @14,@15                                             0208 00084000
*   IF RTNCODE=CATERR THEN          /* DID IGFDV1 FAIL               */ 00085000
         CH    RTNCODE,@CH00045                                    0209 00086000
         BNE   @RF00209                                            0209 00087000
*     DERLRC=CATERR;                /* YES, INDICATE SWAP FAILURE    */ 00088000
         MVI   DERLRC,X'0C'                                        0210 00089000
*   ELSE                            /* NO, CONTINUE SWAP             */ 00090000
*     IF RTNCODE=VALID              /* WAS DEVICE INDEPENDENT      0211 00091000
*                                      VALIDATION SUCCESSFUL         */ 00092000
*       THEN                        /* YES, CONTINUE TESTING         */ 00093000
         B     @RC00209                                            0211 00094000
@RF00209 LTR   RTNCODE,RTNCODE                                     0211 00095000
         BNZ   @RF00211                                            0211 00096000
*       IF DDRSYS='1'B&DDRINV2=DDRVUS THEN/* IF THIS         @YM04069   00097000
*                                      IS A USER SYSTEM SWAP, IGFDV1    00098000
*                                      HAS PERFORMED ALL NECESSARY 0212 00099000
*                                      VALIDATION            @YM04069*/ 00100000
         TM    DDRSYS(DDRPTR),B'01000000'                          0212 00101000
         BNO   @RF00212                                            0212 00102000
         CLI   DDRINV2(DDRPTR),X'03'                               0212 00103000
         BNE   @RF00212                                            0212 00104000
*         DERLRC=VALID;             /* INDICATE SWAP IS OK   @YM04069*/ 00105000
         MVI   DERLRC,X'00'                                        0213 00106000
*       ELSE                        /* IF OPERATOR SWAP, CONTINUE    */ 00107000
*         IF ASVTAVAL(DDRUASID)='0'B THEN/* IS USERS ADDRESS SPACE 0214 00108000
*                                      STILL ACTIVE          @YM04069*/ 00109000
         B     @RC00212                                            0214 00110000
@RF00212 LH    @14,DDRUASID(,DDRPTR)                               0214 00111000
         SLA   @14,2                                               0214 00112000
         L     @12,CVTPTR                                          0214 00113000
         L     @11,ASVTPTR(,@12)                                   0214 00114000
         ST    @14,@TF00001                                        0214 00115000
         ALR   @14,@11                                             0214 00116000
         TM    ASVTAVAL-4(@14),B'10000000'                         0214 00117000
         BNZ   @RF00214                                            0214 00118000
*           DO;                     /* YES,USER IS ACTIVE    @YM04069*/ 00119000
*             DT1CBS=DT1CBS&&DT1CBS;/* CLEAR CONTROL BLOCKS          */ 00120000
         XC    DT1CBS(124),DT1CBS                                  0216 00121000
*             SRBPTR=ADDR(DT1SRB);  /* INIT SRB PTR          @YM04069*/ 00122000
         LA    SRBPTR,DT1SRB                                       0217 00123000
*             SRBID='SRB ';         /* SRB ID                @YM04069*/ 00124000
         MVC   SRBID(4,SRBPTR),@CC02977                            0218 00125000
*             SRBASCB=ASVTENTY(DDRUASID);/* USER ASCB        @YM04069*/ 00126000
         L     @14,@TF00001                                        0219 00127000
         L     @14,ASVTENTY-4(@14,@11)                             0219 00128000
         ST    @14,SRBASCB(,SRBPTR)                                0219 00129000
*             SRBEP=ADDR(IGFDTSRB); /* SRB ENTRY POINT       @YM04069*/ 00130000
         LA    @14,IGFDTSRB                                        0220 00131000
         ST    @14,SRBEP(,SRBPTR)                                  0220 00132000
*             SRBRMTR=ADDR(SRBRECOV);/* PUT ENTRY POINT OF SRB     0221 00133000
*                                      RECOVERY POST ROUTINE IN    0221 00134000
*                                      SRBRMTR               @OZ10025*/ 00135000
         L     @14,@CA02979                                        0221 00136000
         ST    @14,SRBRMTR(,SRBPTR)                                0221 00137000
*             SRBPARM=DDRPTR;       /* DDRCOM ADDRESS        @YM04069*/ 00138000
         ST    DDRPTR,SRBPARM(,SRBPTR)                             0222 00139000
*             SRBPTCB=OLDTCB;       /* OLD TCB ADDRESS       @YM04069*/ 00140000
         L     @14,CVTTCBP(,@12)                                   0223 00141000
         MVC   SRBPTCB(4,SRBPTR),OLDTCB(@14)                       0223 00142000
*             SRBPASID=DDRASID;     /* USER ASID SPACE       @YM04069*/ 00143000
         MVC   SRBPASID(2,SRBPTR),DDRASID(DDRPTR)                  0224 00144000
*             DT1CODE=CODEBASE;     /* MODULE BASE REG       @YM04069*/ 00145000
         ST    CODEBASE,DT1CODE                                    0225 00146000
*             DT1DATA=DATABASE;     /* DATA BASE REG         @YM04069*/ 00147000
*                                                                  0226 00148000
         ST    DATABASE,DT1DATA                                    0226 00149000
*             /*******************************************************/ 00150000
*             /*                                                     */ 00151000
*             /* SCHEDULE PART OF IGFDT1 TO BE EXECUTED IN THE USER'S*/ 00152000
*             /* ADDRESS SPACE                               @YM04069*/ 00153000
*             /*                                                     */ 00154000
*             /*******************************************************/ 00155000
*                                                                  0227 00156000
*             GENERATE REFS(SRBPTR,CVTLSMQ,SRBFLNK) FLOWS(IGFDTSRB)     00157000
*                 (SCHEDULE SRB=(SRBPTR),SCOPE=LOCAL);/*     @YM04069*/ 00158000
*                                                                  0227 00159000
         SCHEDULE SRB=(SRBPTR),SCOPE=LOCAL                              00160000
*             /*******************************************************/ 00161000
*             /*                                                     */ 00162000
*             /* WAIT ON THE ABOVE SRB WHICH EXECUTES IN THE USER    */ 00163000
*             /* ASID                                                */ 00164000
*             /*                                                     */ 00165000
*             /*******************************************************/ 00166000
*                                                                  0228 00167000
*             DO;                   /* WAIT ECB(DT1ECB)              */ 00168000
*               RESPECIFY                                          0229 00169000
*                (GPR00P,                                          0229 00170000
*                 GPR01P) RESTRICTED;                              0229 00171000
*               GPR00P=1;           /* LOAD WAIT COUNT               */ 00172000
         LA    GPR00P,1                                            0230 00173000
*               GPR01P=ADDR(DT1ECB);/* ADDRESS OF ECB                */ 00174000
         LA    GPR01P,DT1ECB                                       0231 00175000
*               SVC(1);             /* WAIT SVC                      */ 00176000
         SVC   1                                                   0232 00177000
*               RESPECIFY                                          0233 00178000
*                (GPR00P,                                          0233 00179000
*                 GPR01P) UNRESTRICTED;                            0233 00180000
*             END;                  /* WAIT ECB(DT1ECB) WAIT ON SRB     00181000
*                                                            @YM04069*/ 00182000
*             IF DERLRC=VALID THEN  /* IS RETURN CODE ZERO?          */ 00183000
         CLI   DERLRC,0                                            0235 00184000
         BNE   @RF00235                                            0235 00185000
*               IF DDRINV2=DDRVFM THEN/* IS A FROM VALIDATION?       */ 00186000
         CLI   DDRINV2(DDRPTR),X'01'                               0236 00187000
         BNE   @RF00236                                            0236 00188000
*                 IF DDRLNSL='1'B&DDRSIRB='0'B THEN/* DOES THE TAPE     00189000
*                                      HAVE NON-STANDARD LABELS AND     00190000
*                                      CAN DDR ISSUE A LOAD (NOTE -     00191000
*                                      DDR CANNOT ISSUE A LOAD IF IT    00192000
*                                      IS RUNNING UNDER AN SIRB)     */ 00193000
         TM    DDRLNSL(DDRPTR),B'00010000'                         0237 00194000
         BNO   @RF00237                                            0237 00195000
         TM    DDRSIRB(DDRPTR),B'00000100'                         0237 00196000
         BNZ   @RF00237                                            0237 00197000
*                   DO;             /* YES, MAKE SURE THAT THE     0238 00198000
*                                      NON-STANDARD LABELS         0238 00199000
*                                      POSITIONING ROUTINE IS IN THE    00200000
*                                      LPA                   @YM04069*/ 00201000
*                     DERLRC=INVALID;/* INDICATE INVALID. IF THE   0239 00202000
*                                      CHECK FOR THE NSL MODULE IS 0239 00203000
*                                      SUCCESSFUL THE RETURN CODE  0239 00204000
*                                      WILL BE RESET TO VALID      0239 00205000
*                                                            @OZ13174*/ 00206000
         MVI   DERLRC,X'04'                                        0239 00207000
*NSLLOAD:                                                          0240 00208000
*                     DO;           /* LOAD EP('NSLREPOS')         0240 00209000
*                                      ERRET(IGFDT1E)                */ 00210000
NSLLOAD  DS    0H                                                  0241 00211000
*                       RESPECIFY                                  0241 00212000
*                        (GPR00P,                                  0241 00213000
*                         GPR01P,                                  0241 00214000
*                         GPR15F) RESTRICTED;                      0241 00215000
*                       GPR00P=ADDR('NSLREPOS');/* LOAD ENTRY POINT     00216000
*                                      NAME ADDRESS                  */ 00217000
         LA    GPR00P,@CC02986                                     0242 00218000
*                       GPR01P=0;   /* JOBLIB OR LINKLIB DCB         */ 00219000
         SLR   GPR01P,GPR01P                                       0243 00220000
*                       GPR01P=GPR01P|'80000000'X;/* ERROR RETURN  0244 00221000
*                                      LABEL SPECIFIED               */ 00222000
         O     GPR01P,@CF00048                                     0244 00223000
*                       SVC(8);     /* ISSUE LOAD SVC                */ 00224000
         SVC   8                                                   0245 00225000
*                       IF GPR15F^=0 THEN/* LOAD WAS UNSUCCESSFUL    */ 00226000
         LTR   GPR15F,GPR15F                                       0246 00227000
         BNZ   @RT00246                                            0246 00228000
*                         GOTO IGFDT1E;/* BRANCH TO ERROR LOCATION   */ 00229000
*                       RESPECIFY                                  0248 00230000
*                        (GPR00P,                                  0248 00231000
*                         GPR01P,                                  0248 00232000
*                         GPR15F) UNRESTRICTED;                    0248 00233000
*                     END;          /* LOAD EP('NSLREPOS')         0249 00234000
*                                      ERRET(IGFDT1E)        @YM02599*/ 00235000
*                                                                  0249 00236000
*                     /***********************************************/ 00237000
*                     /*                                             */ 00238000
*                     /* CHECK FOR NSL MODULE                @YM02599*/ 00239000
*                     /*                                             */ 00240000
*                     /***********************************************/ 00241000
*                                                                  0250 00242000
*NSLDEL:                                                           0250 00243000
*                     DO;           /* DELETE EP('NSLREPOS')         */ 00244000
NSLDEL   DS    0H                                                  0251 00245000
*                       RESPECIFY                                  0251 00246000
*                        (GPR00P) RESTRICTED;                      0251 00247000
*                       GPR00P=ADDR('NSLREPOS');/* ->ENTRY NAME      */ 00248000
         LA    GPR00P,@CC02986                                     0252 00249000
*                       SVC(9);     /* DELETE SVC                    */ 00250000
         SVC   9                                                   0253 00251000
*                       RESPECIFY                                  0254 00252000
*                        (GPR00P) UNRESTRICTED;                    0254 00253000
*                     END;          /* DELETE EP('NSLREPOS') @YM02599   00254000
*                                      NSL MODULE WAS AVAILABLE    0255 00255000
*                                                            @YM04069*/ 00256000
*                     DERLRC=VALID; /* INDICATE SWAP CAN PROCEED   0256 00257000
*                                                            @YM04069*/ 00258000
         MVI   DERLRC,X'00'                                        0256 00259000
*IGFDT1E:                                                          0257 00260000
*                     ;             /*                       @YM04069*/ 00261000
IGFDT1E  DS    0H                                                  0258 00262000
*                     IF DERLRC^=VALID THEN/* IS NSL MODULE PRESENT? */ 00263000
         CLI   DERLRC,0                                            0258 00264000
         BE    @RF00258                                            0258 00265000
*                       DO;         /* NO, INDICATE THAT THE SWAP  0259 00266000
*                                      CANNOT CONTINUE               */ 00267000
*                         DDRINV1=DDRINS;/* INDICATE NOT SUPPORTED 0260 00268000
*                                                            @YM04069*/ 00269000
         MVI   DDRINV1(DDRPTR),X'03'                               0260 00270000
*                         DDRTER1=DDRTIU;/* INDICATE INVALID USER  0261 00271000
*                                      EXIT                  @YM04069*/ 00272000
         MVI   DDRTER1(DDRPTR),X'06'                               0261 00273000
*                       END;        /*                       @YM04069*/ 00274000
*                     ELSE                                         0263 00275000
*                       ;           /* YES, VALID SWAP       @YM02599*/ 00276000
@RF00258 DS    0H                                                  0264 00277000
*                   END;            /*                       @YM04069*/ 00278000
*                 ELSE                                             0265 00279000
*                   ;               /* NOT NSL - TAPE IS OK FOR SWAP */ 00280000
@RF00237 DS    0H                                                  0266 00281000
*               ELSE                                               0266 00282000
*                 ;                 /* NOT A FROM VALIDATION @YM04069*/ 00283000
@RF00236 DS    0H                                                  0267 00284000
*             ELSE                                                 0267 00285000
*               ;                   /* NOT A VALID SWAP      @YM04069*/ 00286000
@RF00235 DS    0H                                                  0268 00287000
*           END;                    /*                       @YM04069*/ 00288000
*IGFDT1R:                           /* ESTAE RETRY ENTRY POINT       */ 00289000
*   DO;                             /* ?IGFDERR CANCEL               */ 00290000
*                                                                  0269 00291000
@RF00214 DS    0H                                                  0269 00292000
@RC00212 DS    0H                                                  0269 00293000
@RF00211 DS    0H                                                  0269 00294000
@RC00209 DS    0H                                                  0269 00295000
IGFDT1R  DS    0H                                                  0270 00296000
*     /***************************************************************/ 00297000
*     /*                                                             */ 00298000
*     /* IGFDERR - CANCEL RECOVERY ENVIRONMENT                       */ 00299000
*     /*                                                             */ 00300000
*     /***************************************************************/ 00301000
*                                                                  0270 00302000
*     DERFUNK=DERCANC;              /* INDICATE CANCEL               */ 00303000
         MVI   DERFUNK,X'02'                                       0270 00304000
*     CALL IGFDE0(DERPLIST);        /* CANCEL ESTAE                  */ 00305000
         LA    @14,DERPLIST                                        0271 00306000
         ST    @14,@AL00001                                        0271 00307000
         L     @15,@CV00095                                        0271 00308000
         LA    @01,@AL00001                                        0271 00309000
         BALR  @14,@15                                             0271 00310000
*   END;                            /* ?IGFDERR CANCEL       @YM04069*/ 00311000
*   RETURN CODE(DERLRC);            /* RETURN TO CALLER      @YM04069*/ 00312000
         SLR   @12,@12                                             0273 00313000
         IC    @12,DERLRC                                          0273 00314000
         L     @13,4(,@13)                                         0273 00315000
         L     @00,@SIZDATD                                        0273 00316000
         LR    @01,@09                                             0273 00317000
         FREEMAIN R,LV=(0),A=(1)                                        00318000
         LR    @15,@12                                             0273 00319000
         L     @14,12(,@13)                                        0273 00320000
         LM    @00,@12,20(@13)                                     0273 00321000
         BR    @14                                                 0273 00322000
*                                                                  0274 00323000
*   /*****************************************************************/ 00324000
*   /*                                                               */ 00325000
*   /* THIS SUBROUTINE IS ENTERED FROM THE USER ADDRESS SPACE VIA A  */ 00326000
*   /* SRB. IGFDTSRB EXECUTES UNDER SRB (DT1SRB) IN SUBPOOL 245.     */ 00327000
*   /* INPUT REGISTERS ARE R0=DT1SRB ADDRESS, AND R15=IGFDTSRB ENTRY */ 00328000
*   /* POINT ADDRESS                                         @YM04069*/ 00329000
*   /*                                                               */ 00330000
*   /*****************************************************************/ 00331000
*                                                                  0274 00332000
*IGFDTSRB:                                                         0274 00333000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @YM04069*/ 00334000
IGFDTSRB DS    0H                                                  0275 00335000
*   SRBPTR=R0;                      /* INIT SRB BASE         @YM04069*/ 00336000
         LR    SRBPTR,R0                                           0275 00337000
*   GENERATE REFS(SRBPTR) SETS(DATABASE)                           0276 00338000
*       (L DATABASE,DT1DATA-DT1SRB(0,SRBPTR));/* INIT DATA BASE      */ 00339000
         L DATABASE,DT1DATA-DT1SRB(0,SRBPTR)                            00340000
*   CODEBASE=DT1CODE;               /* INIT MODULE BASE REG  @YM04069*/ 00341000
         L     CODEBASE,DT1CODE                                    0277 00342000
*   DDRPTR=SRBPARM;                 /* INIT DDRCOM BASE REG  @YM04069*/ 00343000
         L     DDRPTR,SRBPARM(,SRBPTR)                             0278 00344000
*   STM(R0,RTNCODE,DT1SAVE);        /* SAVE REGS IN DATA AREA      0279 00345000
*                                                            @YM04069*/ 00346000
         STM   R0,RTNCODE,DT1SAVE                                  0279 00347000
*   GENERATE REFS(PSALITA,FLC) SETS(R11,R12,R14);                  0280 00348000
*                                                                  0280 00349000
SETLOCK  SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,               @YM04069**00350000
               RELATED=(LOCLMEM,IGFDT1(FREELOCK)) GET LOCAL LOCK        00351000
*   /*****************************************************************/ 00352000
*   /*                                                               */ 00353000
*   /* FROM VALIDATION                                       @YM04069*/ 00354000
*   /*                                                               */ 00355000
*   /*****************************************************************/ 00356000
*                                                                  0281 00357000
*   IF DDRINV2=DDRVFM THEN          /* IS IT 'FROM' VALIDATION     0281 00358000
*                                                            @YM04069*/ 00359000
         CLI   DDRINV2(DDRPTR),X'01'                               0281 00360000
         BNE   @RF00281                                            0281 00361000
*     DO;                                                          0282 00362000
*       UCBPTR=DDRFMUCB;            /* POINT TO 'FROM' UCB           */ 00363000
*                                                                  0283 00364000
         L     UCBPTR,DDRFMUCB(,DDRPTR)                            0283 00365000
*       /*************************************************************/ 00366000
*       /*                                                           */ 00367000
*       /* DETERMINE LABEL TYPE                                      */ 00368000
*       /*                                                           */ 00369000
*       /*************************************************************/ 00370000
*                                                                  0284 00371000
*       IF UCBBSTR='1'B THEN                                       0284 00372000
         TM    UCBBSTR(UCBPTR),B'00000100'                         0284 00373000
         BNO   @RF00284                                            0284 00374000
*         DDRLAL='1'B;              /* ANSI LABELED TAPE             */ 00375000
         OI    DDRLAL(DDRPTR),B'10000000'                          0285 00376000
*       ELSE                                                       0286 00377000
*         IF UCBNSLTP='1'B THEN                                    0286 00378000
         B     @RC00284                                            0286 00379000
@RF00284 TM    UCBNSLTP(UCBPTR),B'01000000'                        0286 00380000
         BNO   @RF00286                                            0286 00381000
*           DDRLNSL='1'B;           /* NON-STANDARD LABELED TAPE     */ 00382000
         OI    DDRLNSL(DDRPTR),B'00010000'                         0287 00383000
*         ELSE                                                     0288 00384000
*           IF UCBDADI='1'B THEN                                   0288 00385000
         B     @RC00286                                            0288 00386000
@RF00286 TM    UCBDADI(UCBPTR),B'00000001'                         0288 00387000
         BNO   @RF00288                                            0288 00388000
*             DDRLSD='1'B;          /* STANDARD LABELED TAPE         */ 00389000
         OI    DDRLSD(DDRPTR),B'00001000'                          0289 00390000
*           ELSE                                                   0290 00391000
*             IF UCBNLTP='1'B THEN                                 0290 00392000
         B     @RC00288                                            0290 00393000
@RF00288 TM    UCBNLTP(UCBPTR),B'10000000'                         0290 00394000
         BNO   @RF00290                                            0290 00395000
*               DDRLNL='1'B;        /* NON-LABELED TAPE              */ 00396000
         OI    DDRLNL(DDRPTR),B'00100000'                          0291 00397000
*       IF DDRSYS='1'B&UCBMOUNT='1'B&DCBOFIOF='1'B THEN/* TEST FOR 0292 00398000
*                                      SUPPORTABLE OPEN              */ 00399000
@RF00290 DS    0H                                                  0292 00400000
@RC00288 DS    0H                                                  0292 00401000
@RC00286 DS    0H                                                  0292 00402000
@RC00284 TM    DDRSYS(DDRPTR),B'01000000'                          0292 00403000
         BNO   @RF00292                                            0292 00404000
         TM    UCBMOUNT(UCBPTR),B'10000000'                        0292 00405000
         BNO   @RF00292                                            0292 00406000
         L     @14,DCBPTR(,DDRPTR)                                 0292 00407000
         TM    DCBOFIOF+40(@14),B'00000001'                        0292 00408000
