         BNE   @RF00368                                            0368 00780000
*               WDCBPTR=WDEBPTR->DEBDCBB;/* YES, PICK UP DCB ADDRESS    00781000
*                                      AND INDICATE DEB FOUND        */ 00782000
         SLR   WDCBPTR,WDCBPTR                                     0369 00783000
         ICM   WDCBPTR,7,DEBDCBB(WDEBPTR)                          0369 00784000
*             ELSE                  /* NO, CONTINUE SEARCH           */ 00785000
*               WDEBPTR=WDEBPTR->DEBDEBB;/* POINT TO NEXT DEB ON   0370 00786000
*                                      QUEUE                         */ 00787000
         B     @RC00368                                            0370 00788000
@RF00368 SLR   @14,@14                                             0370 00789000
         ICM   @14,7,DEBDEBB(WDEBPTR)                              0370 00790000
         LR    WDEBPTR,@14                                         0370 00791000
*           END;                    /* END OF DEB SEARCH LOOP        */ 00792000
@RC00368 DS    0H                                                  0371 00793000
@DE00367 SLR   @14,@14                                             0371 00794000
         CR    WDCBPTR,@14                                         0371 00795000
         BNE   @DC00367                                            0371 00796000
         CR    WDEBPTR,@14                                         0371 00797000
         BNE   @DL00367                                            0371 00798000
@DC00367 DS    0H                                                  0372 00799000
*           IF WDCBPTR=0 THEN       /* IF DEB WAS NOT FOUND, CONTINUE   00800000
*                                      SEARCH                        */ 00801000
         LTR   WDCBPTR,WDCBPTR                                     0372 00802000
         BNZ   @RF00372                                            0372 00803000
*             WTCBPTR=WTCBPTR->TCBTCB;/* PICK UP ADDRESS OF NEXT TCB    00804000
*                                      ON QUEUE                      */ 00805000
         L     WTCBPTR,TCBTCB(,WTCBPTR)                            0373 00806000
*         END;                      /* END OF TCB SCAN LOOP          */ 00807000
@RF00372 DS    0H                                                  0374 00808000
@DE00365 SLR   @14,@14                                             0374 00809000
         CR    WDCBPTR,@14                                         0374 00810000
         BNE   @DC00365                                            0374 00811000
         CR    WTCBPTR,@14                                         0374 00812000
         BNE   @DL00365                                            0374 00813000
@DC00365 DS    0H                                                  0375 00814000
*         IF WDCBPTR^=0             /* WAS DEB FOUND                 */ 00815000
*           THEN                    /* YES, CONTINUE VALIDATION      */ 00816000
         SLR   @14,@14                                             0375 00817000
         CR    WDCBPTR,@14                                         0375 00818000
         BE    @RF00375                                            0375 00819000
*           IF DDRUDCB=0 THEN       /* WERE CONTROL BLOCKS FOUND   0376 00820000
