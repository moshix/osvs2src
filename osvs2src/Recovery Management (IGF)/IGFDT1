         BNE   @RF00386                                            0386 00853000
*               DO;                 /* SAME BLOCKS           @YM04069*/ 00854000
*                 DERLRC=VALID;     /* THE BLOCKS ARE STILL THE SAME    00855000
*                                      - USER IS VALID               */ 00856000
         MVI   DERLRC,X'00'                                        0388 00857000
*                 DDRUMODE=DEBSDVM; /* USER MODE SET         @YM04069*/ 00858000
