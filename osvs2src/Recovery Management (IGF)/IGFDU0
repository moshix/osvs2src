*CSLOOP:                                                           0123 00108000
*   ;                                                              0123 00109000
CSLOOP   DS    0H                                                  0124 00110000
*   ALTERCNT=COUNT+1;               /* INCREMENT COUNT               */ 00111000
         LA    ALTERCNT,1                                          0124 00112000
         AR    ALTERCNT,COUNT                                      0124 00113000
*   CS(COUNT,ALTERCNT,CSDUCNT);     /* IF THE COUNT=CSDUCNT THAN   0125 00114000
