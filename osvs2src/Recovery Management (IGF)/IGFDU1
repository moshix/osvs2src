         USING @DATD,@09                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @09,8(,@13)                                         0001 00020000
         LR    @13,@09                                             0001 00021000
*                                   /* LIST VERSION                  */ 00022000
*   DO;                             /* ?IGFDERR INIT                 */ 00023000
*                                                                  0095 00024000
*     /***************************************************************/ 00025000
*     /*                                                             */ 00026000
*     /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT                   */ 00027000
*     /*                                                             */ 00028000
*     /***************************************************************/ 00029000
*                                                                  0096 00030000
*     DERPLIST=DERPLIST&&DERPLIST;  /* CLEAR PARAMETER LIST          */ 00031000
         XC    DERPLIST(48),DERPLIST                               0096 00032000
*     DERFUNK=DERINIT;              /* INDICATE INIT FUNCTION        */ 00033000
         MVI   DERFUNK,X'01'                                       0097 00034000
*     DERREC=ADDR(DERRECPM);        /* POINT TO REC PARMS            */ 00035000
         LA    @14,DERRECPM                                        0098 00036000
         ST    @14,DERREC                                          0098 00037000
*     DERRETRY=ADDR(IGFDU1R);       /* POINT TO RETRY ROUTINE        */ 00038000
         LA    @14,IGFDU1R                                         0099 00039000
         ST    @14,DERRETRY                                        0099 00040000
*     GEN SETS(DERDATD)(MVC DERDATD,@SIZDATD);/* SET UP SPN, SIZE    */ 00041000
         MVC DERDATD,@SIZDATD                                           00042000
*     DERPFX=DERRMASK;              /* SET UP RUB MASK               */ 00043000
         MVC   DERPFX(2),@CB00604                                  0101 00044000
*     DERRCODE=CODEBASE;            /* SAVE CODEREG                  */ 00045000
         ST    CODEBASE,DERRCODE                                   0102 00046000
*     DERRDATA=DATABASE;            /* SAVE DATAREG                  */ 00047000
