         ST    @14,@AL00001                                        0106 00055000
         L     @15,@CV00556                                        0106 00056000
         LA    @01,@AL00001                                        0106 00057000
         BALR  @14,@15                                             0106 00058000
*   END;                            /* ?IGFDERR INIT                 */ 00059000
*                                   /* ESTAE ESTABLISHED             */ 00060000
*   DERLRC=4;                       /* SET LOCAL RETURN CODE TO 4    */ 00061000
         MVI   DERLRC,X'04'                                        0108 00062000
*   CALL IGFDV1;                    /* BRANCH TO DEVICE INDEPENDENT     00063000
*                                      VALIDATION                    */ 00064000
         L     @15,@CV00557                                        0109 00065000
         BALR  @14,@15                                             0109 00066000
*   IF RTNCODE=12 THEN              /* INVALID VALIDATION            */ 00067000
         CH    RTNCODE,@CH00608                                    0110 00068000
         BNE   @RF00110                                            0110 00069000
*     DERLRC=12;                    /* YES -EXIT                     */ 00070000
         MVI   DERLRC,X'0C'                                        0111 00071000
*   ELSE                                                           0112 00072000
*     IF RTNCODE=0 THEN             /* VALID VALIDATION              */ 00073000
         B     @RC00110                                            0112 00074000
@RF00110 LTR   RTNCODE,RTNCODE                                     0112 00075000
         BNZ   @RF00112                                            0112 00076000
*       DO;                         /* YES - A VALID RETURN FROM   0113 00077000
*                                      IGFDV0                        */ 00078000
*         IF DDRINV2=DDRVUS THEN    /* A USER REQUEST?               */ 00079000
*                                                                  0114 00080000
         CLI   DDRINV2(DDRPTR),X'03'                               0114 00081000
         BNE   @RF00114                                            0114 00082000
*           /*********************************************************/ 00083000
*           /*                                                       */ 00084000
*           /* USER VALIDATE REQUEST                                 */ 00085000
*           /*                                                       */ 00086000
*           /*********************************************************/ 00087000
*                                                                  0115 00088000
*           DERLRC=0;               /* YES - NO UNIT RECORD DEVICE 0115 00089000
*                                      DEPENDENT USER VALIDATION     */ 00090000
         MVI   DERLRC,X'00'                                        0115 00091000
*         IF DDRINV2=DDRVCM THEN    /* TO VALIDATE REQUEST ?       0116 00092000
*                                      YES-COMPARE THE FOLLOWING   0116 00093000
*                                      FIELDS - FROM UCB OPTION FLAGS   00094000
*                                      WITH TO UCB OPTION FLAGS -  0116 00095000
*                                      FROM UCB DEVICE TYPE WITH TO     00096000
*                                      UCB DEVICE TYPE - FROM UCB  0116 00097000
*                                      MODEL BITS WITH TO UCB MODEL     00098000
*                                      BITS (ONLY THE LAST FOUR MODEL   00099000
*                                      BITS)                         */ 00100000
*                                                                  0116 00101000
@RF00114 CLI   DDRINV2(DDRPTR),X'02'                               0116 00102000
         BNE   @RF00116                                            0116 00103000
*           /*********************************************************/ 00104000
*           /*                                                       */ 00105000
*           /* TO VALIDATE REQUEST                                   */ 00106000
*           /*                                                       */ 00107000
*           /*********************************************************/ 00108000
*                                                                  0117 00109000
*           IF DDRFMUCB->UCBTBYT2=DDRTOUCB->UCBTBYT2&DDRFMUCB->UCBTBYT4 00110000
*               =DDRTOUCB->UCBTBYT4&(DDRFMUCB->UCBTBYT1&'0F'X)=(   0117 00111000
*               DDRTOUCB->UCBTBYT1&'0F'X)THEN/* ARE DEVICE TYPES AND    00112000
*                                      FEATURES THE SAME             */ 00113000
         L     @14,DDRFMUCB(,DDRPTR)                               0117 00114000
         L     @12,DDRTOUCB(,DDRPTR)                               0117 00115000
         CLC   UCBTBYT2(1,@14),UCBTBYT2(@12)                       0117 00116000
         BNE   @RF00117                                            0117 00117000
         CLC   UCBTBYT4(1,@14),UCBTBYT4(@12)                       0117 00118000
         BNE   @RF00117                                            0117 00119000
         MVC   @TS00001(1),UCBTBYT1(@14)                           0117 00120000
