         MVC   @TS00001+1(1),UCBTBYT1(@12)                         0117 00122000
         NI    @TS00001+1,X'0F'                                    0117 00123000
         CLC   @TS00001(1),@TS00001+1                              0117 00124000
         BNE   @RF00117                                            0117 00125000
*             DERLRC=0;             /* YES                           */ 00126000
         MVI   DERLRC,X'00'                                        0118 00127000
*           ELSE                    /* NO                            */ 00128000
*             DDRINV1=DDRIIN;       /* SET INCOMPATIBLE FLAG         */ 00129000
         B     @RC00117                                            0119 00130000
@RF00117 MVI   DDRINV1(DDRPTR),X'04'                               0119 00131000
*         IF DDRINV2=DDRVFM THEN    /* FROM VALIDATE REQUEST         */ 00132000
*                                                                  0120 00133000
@RC00117 DS    0H                                                  0120 00134000
@RF00116 CLI   DDRINV2(DDRPTR),X'01'                               0120 00135000
         BNE   @RF00120                                            0120 00136000
*           /*********************************************************/ 00137000
*           /*                                                       */ 00138000
*           /* YES - FROM VALIDATE REQUEST                           */ 00139000
*           /*                                                       */ 00140000
*           /*********************************************************/ 00141000
*                                                                  0121 00142000
*           IF DDRFMUCB->UCBNOTRD='1'B THEN/* DEVICE NOT READY       */ 00143000
         L     @14,DDRFMUCB(,DDRPTR)                               0121 00144000
         TM    UCBNOTRD(@14),B'01000000'                           0121 00145000
         BNO   @RF00121                                            0121 00146000
*             DERLRC=0;             /* YES-DEVICE IS NOT READY       */ 00147000
         MVI   DERLRC,X'00'                                        0122 00148000
*           ELSE                                                   0123 00149000
*             DO;                   /* NOT FROM REQUEST              */ 00150000
         B     @RC00121                                            0123 00151000
@RF00121 DS    0H                                                  0124 00152000
*               DDRINV1=DDRINS;     /* NOT SUPPORTED                 */ 00153000
