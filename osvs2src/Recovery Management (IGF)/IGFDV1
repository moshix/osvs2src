*                                      ALREADY BEEN PURGED           */ 00405000
*           THEN                    /* NO, CONTINUE WITH SWAP        */ 00406000
         TM    DDRSYS(DDRPTR),B'01000000'                          0267 00407000
         BNO   @RT00267                                            0267 00408000
         TM    DDRPRG(DDRPTR),B'00001000'                          0267 00409000
         BO    @RF00267                                            0267 00410000
@RT00267 DS    0H                                                  0268 00411000
*           DERLRC=VALID;           /* RESET THE EXTERNAL RETURN CODE   00412000
*                                      TO INDICATE THAT USER REQUEST    00413000
*                                      HAS NOT BEEN PURGED           */ 00414000
         MVI   DERLRC,X'00'                                        0268 00415000
*         ELSE                                                     0269 00416000
*           ;                       /* YES, SWAP WILL BE TERMINATED     00417000
*                                      SINCE EXTERNAL RETURN CODE  0269 00418000
*                                      STILL INDICATES INVALID SWAP  */ 00419000
@RF00267 DS    0H                                                  0270 00420000
*       ELSE                        /* NOT 'TO', 'FROM' OR 'USER'  0270 00421000
*                                      REQUEST                       */ 00422000
*                                                                  0270 00423000
*         /***********************************************************/ 00424000
*         /*                                                         */ 00425000
*         /* INVALID PARAMETER IN THE DDRCOM FOR VALIDATE REQUEST    */ 00426000
*         /*                                                         */ 00427000
*         /***********************************************************/ 00428000
*                                                                  0270 00429000
*         DO;                                                      0270 00430000
         B     @RC00266                                            0270 00431000
@RF00266 DS    0H                                                  0271 00432000
*           DDRTER1=DDRTCE;         /* INDICATE 'CATASTROPHIC ERROR' */ 00433000
         MVI   DDRTER1(DDRPTR),X'07'                               0271 00434000
*           DERLRC=BADPARM;         /* RESET EXTERNAL RETURN CODE TO    00435000
*                                      INDICATE INVALID REQUEST      */ 00436000
         MVI   DERLRC,X'0C'                                        0272 00437000
*         END;                                                     0273 00438000
*                                                                  0273 00439000
*   /*****************************************************************/ 00440000
*   /*                                                               */ 00441000
*   /* COMMON EXIT FROM DEVICE INDEPENDENT VALIDATION - AT THIS      */ 00442000
*   /* POINT, THE EXTERNAL RETURN CODE VARIABLE (DERLRC) CONTAINS THE*/ 00443000
*   /* RETURN CODE TO BE PASSED BACK TO THE CALLER                   */ 00444000
*   /*                                                               */ 00445000
*   /*****************************************************************/ 00446000
*                                                                  0274 00447000
*IGFDV1R:                           /* ESTAE RETRY ENTRY POINT       */ 00448000
*   DO;                             /* ?IGFDERR CANCEL               */ 00449000
*                                                                  0274 00450000
@RC00266 DS    0H                                                  0274 00451000
@RC00207 DS    0H                                                  0274 00452000
@RC00164 DS    0H                                                  0274 00453000
IGFDV1R  DS    0H                                                  0275 00454000
*     /***************************************************************/ 00455000
*     /*                                                             */ 00456000
*     /* IGFDERR - CANCEL RECOVERY ENVIRONMENT                       */ 00457000
*     /*                                                             */ 00458000
*     /***************************************************************/ 00459000
*                                                                  0275 00460000
*     DERFUNK=DERCANC;              /* INDICATE CANCEL               */ 00461000
         MVI   DERFUNK,X'02'                                       0275 00462000
*     CALL IGFDE0(DERPLIST);        /* CANCEL ESTAE                  */ 00463000
         LA    @14,DERPLIST                                        0276 00464000
