*                                      IS SUPPORTED BY DDR           */ 00690000
         SLR   DV3RC,DV3RC                                         0324 00691000
*         END;                                                     0325 00692000
*     END;                                                         0326 00693000
*   RETURN;                         /* RETURN TO CALLER              */ 00694000
@EL00003 DS    0H                                                  0327 00695000
@EF00003 DS    0H                                                  0327 00696000
@ER00003 LM    @14,@06,12(@13)                                     0327 00697000
         LM    @08,@12,52(@13)                                     0327 00698000
         BR    @14                                                 0327 00699000
*   END IGFDV3;                     /* END OF INTERNAL SUBROUTINES   */ 00700000
         B     @EL00003                                            0328 00701000
@PB00003 DS    0H                                                  0329 00702000
*   GEN NOREFS NOSETS(EJECT);                                      0329 00703000
         EJECT                                                          00704000
*                                                                  0330 00705000
*   /*****************************************************************/ 00706000
*   /*                                                               */ 00707000
*   /* SUBROUTINE IGFDVJES - INVOKES JES3 TO VALIDATE A 'TO' DEVICE  */ 00708000
*   /*                                                       @Y02BKCI*/ 00709000
*   /*                                                               */ 00710000
*   /*****************************************************************/ 00711000
*                                                                  0330 00712000
*IGFDVJES:                                                         0330 00713000
*   PROC;                                                          0330 00714000
         B     @PB00004                                            0330 00715000
IGFDVJES STM   @14,@01,@SA00004                                    0330 00716000
         STM   @03,@12,@SA00004+16                                 0330 00717000
*   DVJESRC=VALID;                  /* INIT RETURN CODE      @Y02BKCI*/ 00718000
         SLR   DVJESRC,DVJESRC                                     0331 00719000
