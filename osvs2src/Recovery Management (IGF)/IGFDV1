*   /*****************************************************************/ 00484000
*   /*                                                               */ 00485000
*   /* SUBROUTINE IGFDV2 - FINDS THE UCB FOR A GIVEN DEVICE ADDRESS  */ 00486000
*   /*                                                               */ 00487000
*   /*****************************************************************/ 00488000
*                                                                  0280 00489000
*IGFDV2:                            /* SUBROUTINE ENTRY POINT        */ 00490000
*   PROCEDURE;                                                     0280 00491000
         B     @PB00002                                            0280 00492000
IGFDV2   STM   @14,@02,12(@13)                                     0280 00493000
         ST    @04,36(,@13)                                        0280 00494000
         STM   @06,@12,44(@13)                                     0280 00495000
*   DV2RC=INVALID;                  /* INDICATE UCB NOT FOUND        */ 00496000
         LA    DV2RC,4                                             0281 00497000
*   UCBPTR=0;                       /* INDICATE UCB NOT FOUND YET    */ 00498000
*                                                                  0282 00499000
         SLR   UCBPTR,UCBPTR                                       0282 00500000
*   /*****************************************************************/ 00501000
*   /*                                                               */ 00502000
*   /* INSPECT THE DEVICE ADDRESS AND INSURE THAT IT CONTAINS ONLY   */ 00503000
*   /* VALID HEXADECIMAL DIGITS                                      */ 00504000
*   /*                                                               */ 00505000
*   /*****************************************************************/ 00506000
*                                                                  0283 00507000
*   DO INDEX=1 TO LENGTH(DV1CUA) WHILE((DV1CUA(INDEX)>='A'&DV1CUA(INDEX 00508000
*         )<='F')|(DV1CUA(INDEX)>='0'&DV1CUA(INDEX)<='9'));/* SCAN 0283 00509000
*                                      THE ENTIRE DEVICE ADDRESS   0283 00510000
*                                      FIELD, CHECKING THAT EACH BYTE   00511000
*                                      CONTAINS A CHARACTER BETWEEN     00512000
*                                      'A' AND 'F' OR '0' AND '9'    */ 00513000
         LA    INDEX,1                                             0283 00514000
@DL00283 LA    @14,DV1CUA-1(INDEX)                                 0283 00515000
         CLI   0(@14),C'A'                                         0283 00516000
         BL    @GL00005                                            0283 00517000
         LA    @14,DV1CUA-1(INDEX)                                 0283 00518000
         CLI   0(@14),C'F'                                         0283 00519000
         BNH   @DB00283                                            0283 00520000
@GL00005 LA    @14,DV1CUA-1(INDEX)                                 0283 00521000
         CLI   0(@14),C'0'                                         0283 00522000
         BL    @DC00283                                            0283 00523000
         LA    @14,DV1CUA-1(INDEX)                                 0283 00524000
         CLI   0(@14),C'9'                                         0283 00525000
         BH    @DC00283                                            0283 00526000
@DB00283 DS    0H                                                  0284 00527000
*   END;                            /* END OF SCAN LOOP              */ 00528000
         AL    INDEX,@CF00079                                      0284 00529000
         C     INDEX,@CF00055                                      0284 00530000
         BNH   @DL00283                                            0284 00531000
@DC00283 DS    0H                                                  0285 00532000
*   IF INDEX>LENGTH(DV1CUA) THEN    /* IS ENTIRE FIELD VALID         */ 00533000
         C     INDEX,@CF00055                                      0285 00534000
         BNH   @RF00285                                            0285 00535000
*     DO;                           /* YES, FIND UCB FOR THIS DEVICE */ 00536000
*                                                                  0286 00537000
*       /*************************************************************/ 00538000
*       /*                                                           */ 00539000
*       /* UCB LOOKUP - SERIAL SEARCH OF UCBS                        */ 00540000
*       /*                                                           */ 00541000
*       /*************************************************************/ 00542000
*                                                                  0287 00543000
*       DO UCBLUPTR=CVTILK2 BY LENGTH(UCBLUENT)/* SCAN UCB LOOKUP  0287 00544000
*                                      TABLE                         */ 00545000
*             WHILE((UCBLUENT=0)    /* SKIP DUMMY ENTRIES            */ 00546000
*             |(UCBLUENT^=STOPPER   /* STOP WHEN STOPPER ENTRY IS  0287 00547000
*                                      REACHED                       */ 00548000
*             &UCBLUENT->UCBNAME^=DV1CUA));/* OR WHEN SPECIFIED    0287 00549000
*                                      DEVICE IS FOUND               */ 00550000
         L     @14,CVTPTR                                          0287 00551000
         L     UCBLUPTR,CVTILK2(,@14)                              0287 00552000
         B     @DE00287                                            0287 00553000
@DL00287 DS    0H                                                  0288 00554000
*       END;                        /* END OF UCB SCAN LOOP          */ 00555000
         AL    UCBLUPTR,@CF00057                                   0288 00556000
@DE00287 LH    @14,UCBLUENT(,UCBLUPTR)                             0288 00557000
         N     @14,@CF01690                                        0288 00558000
         LTR   @14,@14                                             0288 00559000
         BZ    @DL00287                                            0288 00560000
         LH    @14,UCBLUENT(,UCBLUPTR)                             0288 00561000
         N     @14,@CF01690                                        0288 00562000
         C     @14,@CF00047                                        0288 00563000
         BE    @DC00287                                            0288 00564000
         CLC   UCBNAME(3,@14),DV1CUA                               0288 00565000
         BNE   @DL00287                                            0288 00566000
@DC00287 DS    0H                                                  0289 00567000
*       IF UCBLUENT^=STOPPER THEN   /* WAS STOPPER ENTRY REACHED     */ 00568000
         LH    @14,UCBLUENT(,UCBLUPTR)                             0289 00569000
         N     @14,@CF01690                                        0289 00570000
         C     @14,@CF00047                                        0289 00571000
         BE    @RF00289                                            0289 00572000
*         DO;                       /* NO, UCB WAS FOUND             */ 00573000
*           DV2RC=VALID;            /* RESET INTERNAL RETURN CODE TO    00574000
*                                      INDICATE UCB FOUND            */ 00575000
         SLR   DV2RC,DV2RC                                         0291 00576000
*           UCBPTR=UCBLUENT;        /* PASS BACK UCB ADDRESS         */ 00577000
         LR    UCBPTR,@14                                          0292 00578000
*         END;                                                     0293 00579000
*     END;                                                         0294 00580000
*   RETURN;                         /* RETURN TO CALLER              */ 00581000
@EL00002 DS    0H                                                  0295 00582000
@EF00002 DS    0H                                                  0295 00583000
@ER00002 LM    @14,@02,12(@13)                                     0295 00584000
         L     @04,36(,@13)                                        0295 00585000
         LM    @06,@12,44(@13)                                     0295 00586000
         BR    @14                                                 0295 00587000
*   END IGFDV2;                     /* END OF INTERNAL PROCEDURE     */ 00588000
         B     @EL00002                                            0296 00589000
@PB00002 DS    0H                                                  0297 00590000
*   GENERATE NOREFS NOSETS(EJECT);                                 0297 00591000
         EJECT                                                          00592000
*                                                                  0298 00593000
*   /*****************************************************************/ 00594000
*   /*                                                               */ 00595000
*   /* SUBROUTINE IGFDV3 - LOCATE DEVICE CHARACTERISTICS TABLE ENTRY */ 00596000
*   /*                                                               */ 00597000
*   /*****************************************************************/ 00598000
*                                                                  0298 00599000
*IGFDV3:                            /* BEGINNING OF SUBROUTINE       */ 00600000
*   PROCEDURE;                                                     0298 00601000
         B     @PB00003                                            0298 00602000
IGFDV3   STM   @14,@06,12(@13)                                     0298 00603000
         STM   @08,@12,52(@13)                                     0298 00604000
