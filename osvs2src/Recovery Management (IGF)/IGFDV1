         B     @RC00307                                            0312 00644000
@RF00307 TM    UCB3UREC(UCBPTR),B'00001000'                        0312 00645000
         BNO   @RF00312                                            0312 00646000
*         DO;                       /* UNIT RECORD DEVICE            */ 00647000
*           DSTPTR=ADDR(DSTDSTU);                                  0314 00648000
         LA    DSTPTR,DSTDSTU                                      0314 00649000
