         TITLE 'IGFPBUCR     - PER PROCESSOR MCH INITIALIZATION        *00001000
                        '                                               00002000
IGFPBUCR CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(24)                                             0001 00007000
         DC    C'IGFPBUCR 77334  UZ85100 '                         0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
*                                                                       00014000
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF           00015000
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES       00016000
* WILL ASSEMBLE PROPERLY.                                               00017000
*                                                                  0137 00018000
*/********************************************************************/ 00019000
*/*                                                                  */ 00020000
*/*MAINLINE-                                                         */ 00021000
*/*          IGFPBUCR                                                */ 00022000
*/*                                                                  */ 00023000
*/********************************************************************/ 00024000
*                                                                  0137 00025000
*   ABSZERO=0;                      /* ABSOLUTE LOCATION ZERO        */ 00026000
         SLR   ABSZERO,ABSZERO                                     0137 00027000
*   RWAPTR=RVTRWAPT;                /* LOAD BASE FOR RWA             */ 00028000
         L     @11,CVTPTR(,ABSZERO)                                0138 00029000
         L     @11,RVTPTR(,@11)                                    0138 00030000
         L     RWAPTR,RVTRWAPT(,@11)                               0138 00031000
*   IF INPREG=0                     /* CHECK FOR 0 - ALLOCATE IN REG0*/ 00032000
*     THEN                                                         0139 00033000
         LTR   INPREG,INPREG                                       0139 00034000
         BZ    @RT00139                                            0139 00035000
*     GOTO ALLOCATE;                /* ALLOCATE ROUTINE              */ 00036000
*   ELSE                                                           0141 00037000
*     IF INPREG=4                   /* CHECK FOR 4 - DEALLOCATE IN 0141 00038000
*                                      REG0                          */ 00039000
*       THEN                                                       0141 00040000
         C     INPREG,@CF00046                                     0141 00041000
         BE    @RT00141                                            0141 00042000
*       GOTO DEALLOC;               /* DEALLOCATE ROUTINE            */ 00043000
*     ELSE                                                         0143 00044000
*       IF INPREG=8                 /* CHECK FOR 8 - CR INIT IN REG0 */ 00045000
*         THEN                                                     0143 00046000
         C     INPREG,@CF00038                                     0143 00047000
         BE    @RT00143                                            0143 00048000
*         GOTO CRINIT;              /* CR INITIALIZATION             */ 00049000
*       ELSE                                                       0145 00050000
*         IF INPREG=12              /* CHECK FOR 12 - CR RESET       */ 00051000
*           THEN                                                   0145 00052000
         C     INPREG,@CF00032                                     0145 00053000
         BE    @RT00145                                            0145 00054000
*           GOTO CRRESET;           /* RESET CR                      */ 00055000
*         ELSE                                                     0147 00056000
*           ;                                                      0147 00057000
*RETRN4:                                                           0148 00058000
*   ;                                                              0148 00059000
RETRN4   DS    0H                                                  0149 00060000
*   RETURN CODE(4);                 /* RETURN TO CALLER              */ 00061000
         LA    @15,4                                               0149 00062000
         L     @14,12(,@13)                                        0149 00063000
         LM    @00,@12,20(@13)                                     0149 00064000
         BR    @14                                                 0149 00065000
*ALLOCATE:                                                         0150 00066000
*   ;                                                              0150 00067000
ALLOCATE DS    0H                                                  0151 00068000
*   IF CPUREG>CPUHI                 /* IS INPUT CPU ADDRESS > HIGHEST   00069000
*                                      ADDRESS                       */ 00070000
*     THEN                                                         0151 00071000
         C     CPUREG,CPUHI                                        0151 00072000
         BH    @RT00151                                            0151 00073000
*     GOTO RETRN4;                  /* SET FAILING RETURN CODE       */ 00074000
*   ELSE                                                           0153 00075000
*     ;                                                            0153 00076000
*   PCCAPTR=PCCAT00P(CPUREG+1);     /* GET PCCA ADDRESS              */ 00077000
         LR    @11,CPUREG                                          0154 00078000
         SLA   @11,2                                               0154 00079000
         L     @09,CVTPTR(,ABSZERO)                                0154 00080000
         L     @09,CVTPCCAT(,@09)                                  0154 00081000
         L     PCCAPTR,PCCAT00P(@11,@09)                           0154 00082000
*   GEN(   LRA   REALADR,0(PCCAPTR));/* REAL ADR OF PCCA     @YM00964*/ 00083000
            LRA   REALADR,0(PCCAPTR)                                    00084000
*   RWAPCCAR(CPUREG+1)=REALADR;     /* STORE PCCA REAL IN RWA      0156 00085000
*                                                            @YM00964*/ 00086000
         LR    @11,CPUREG                                          0156 00087000
         SLA   @11,2                                               0156 00088000
         ST    REALADR,RWAPCCAR(@11,RWAPTR)                        0156 00089000
*   IF PCCAPTR=0                    /* CHECK FOR NO PCCA ADDRESS     */ 00090000
*     THEN                                                         0157 00091000
         SLR   @11,@11                                             0157 00092000
         CR    PCCAPTR,@11                                         0157 00093000
         BE    @RT00157                                            0157 00094000
*     GOTO RETRN4;                  /* SET FAILING RETURN CODE       */ 00095000
*   ELSE                                                           0159 00096000
*     ;                                                            0159 00097000
*   IF PCCALRBV=0                   /* CHECK IF LRB EXISTS           */ 00098000
*     THEN                                                         0160 00099000
         C     @11,PCCALRBV(,PCCAPTR)                              0160 00100000
         BNE   @RF00160                                            0160 00101000
*     DO;                           /* PCCALRBV = 0                  */ 00102000
*       GEN;                                                       0162 00103000
*                /* STORE PROCESSOR ID  */                              00104000
       STIDP   CPUID                                                    00105000
*       GMLGTH=328+MCEL;            /* LENGTH FOR LRB GETMAIN        */ 00106000
         LA    GMLGTH,328                                          0163 00107000
         AH    GMLGTH,MCEL                                         0163 00108000
*       IF GMLGTH>4095              /* CHECK LENGTH NOT GREATER THAN    00109000
*                                      PAGE                          */ 00110000
*         THEN                                                     0164 00111000
         C     GMLGTH,@CF02029                                     0164 00112000
         BH    @RT00164                                            0164 00113000
*         GO TO RETRN4;             /* SET FAILING RETURN CODE       */ 00114000
*       ELSE                                                       0166 00115000
*         ;                                                        0166 00116000
*       GEN;                                                       0167 00117000
*             /* GET CORE FOR LRB  */                                   00118000
       GETMAIN   R,LV=(GMLGTH),SP=245                                   00119000
*       LRBPTR=R1;                  /* BASE POINTER FOR LRB          */ 00120000
         LR    LRBPTR,R1                                           0168 00121000
*       PCCALRBV=R1;                /* STORE VIRTUAL ADDRESS OF LRB  */ 00122000
         ST    R1,PCCALRBV(,PCCAPTR)                               0169 00123000
*       GEN;                                                       0170 00124000
*               /* GET REAL ADDRESS OF LRB  */                          00125000
       LRA  R4,0(R1)                                                    00126000
*       PCCALRBR=R4;                /* REAL ADDRESS OF LRB IN PCCA   */ 00127000
         ST    R4,PCCALRBR(,PCCAPTR)                               0171 00128000
*       LRBHCPID=LRBHCPID&&LRBHCPID;/* ZERO OUT WORK AREA            */ 00129000
         XC    LRBHCPID(8,LRBPTR),LRBHCPID(LRBPTR)                 0172 00130000
*       PACK(LRBHCPID(7:8),CVTNUMB);/* PACK RELEASE NO. FROM CVT     */ 00131000
         L     @11,CVTPTR(,ABSZERO)                                0173 00132000
         AL    @11,@CF02075                                        0173 00133000
         PACK  LRBHCPID+6(2,LRBPTR),CVTNUMB-252(2,@11)             0173 00134000
*       GEN REFS(LRBHCPID);                                        0174 00135000
      CVB  R4,LRBHCPID(,LRBPTR)     STRIP OFF ZONES                     00136000
*       GEN;                                                       0175 00137000
*             /*  SHIFT OFF NON PERTINENT INFO */                       00138000
       SLL   R4,11                                                      00139000
       SRL   R4,11                                                      00140000
*       LRB(1:64)='00'X;            /* ZERO HEADER PORTION OF LRB    */ 00141000
         XC    LRB+1(63,LRBPTR),LRB+1(LRBPTR)                      0176 00142000
         MVI   LRB(LRBPTR),X'00'                                   0176 00143000
*       LRBHREL=R4;                 /* STORE IN RELEASE NUMBER       */ 00144000
         STC   R4,LRBHREL(,LRBPTR)                                 0177 00145000
*       LRBHREL=LRBHREL|VS2BIT;     /* OR IN VS2 ID                  */ 00146000
         SLR   @11,@11                                             0178 00147000
         IC    @11,LRBHREL(,LRBPTR)                                0178 00148000
         O     @11,@CF02018                                        0178 00149000
         STC   @11,LRBHREL(,LRBPTR)                                0178 00150000
*       LRBMPWL='08'X;              /* PHYSICAL WORD LENGTH          */ 00151000
         MVI   LRBMPWL(LRBPTR),X'08'                               0179 00152000
*       LRBHTYPE=LRBHMCH;           /* INITIALE RECORD TYPE          */ 00153000
         MVI   LRBHTYPE(LRBPTR),X'13'                              0180 00154000
*       LRBHSW0='40'X;              /* N S MACHINE - LRBHNS          */ 00155000
         MVI   LRBHSW0(LRBPTR),X'40'                               0181 00156000
*     END;                          /* PCCALRBV = 0                  */ 00157000
*   ELSE                                                           0183 00158000
*     ;                                                            0183 00159000
@RF00160 DS    0H                                                  0184 00160000
*   IF PCCAPWAV=0                   /* CHECK IF PWA EXISTS           */ 00161000
*     THEN                                                         0184 00162000
         L     @11,PCCAPWAV(,PCCAPTR)                              0184 00163000
         LTR   @11,@11                                             0184 00164000
         BNZ   @RF00184                                            0184 00165000
*     DO;                           /* PCCAPWAV = 0                  */ 00166000
*       GMLGTH=LENGTH(PWA);         /* LENGTH OF PWA FOR GETMAIN     */ 00167000
         LA    GMLGTH,1008                                         0186 00168000
*       GEN;                                                       0187 00169000
*             /*  GETMAIN FOR PWA  */                                   00170000
        GETMAIN  R,LV=(GMLGTH),SP=245                                   00171000
*       PCCAPWAV=R1;                /* VIRTUAL ADDRESS OF PWA IN PCCA*/ 00172000
         ST    R1,PCCAPWAV(,PCCAPTR)                               0188 00173000
*       GEN;                                                       0189 00174000
*              /* GET REAL ADDRESS OF PWA  */                           00175000
       LRA  R4,0(R1)                                                    00176000
*       PCCAPWAR=R4;                /* STORE REAL ADDRESS OF PWA     */ 00177000
         ST    R4,PCCAPWAR(,PCCAPTR)                               0190 00178000
*       PWAFRRCT='00000000'X;       /* ZERO OUT FRS COUNT            */ 00179000
         SLR   @11,@11                                             0191 00180000
         L     @09,PWAPTR(,PCCAPTR)                                0191 00181000
         ST    @11,PWAFRRCT(,@09)                                  0191 00182000
*       PWAFRRCD='00000000'X;       /* ZERO FRR MESSAGE CODE         */ 00183000
         ST    @11,PWAFRRCD(,@09)                                  0192 00184000
*       PWAMFAFL(1:16)='00'X;       /* ZERO ALL RESERVED BITS AND MFA   00185000
*                                      FLAG                          */ 00186000
         XC    PWAMFAFL+1(15,@09),PWAMFAFL+1(@09)                  0193 00187000
         MVI   PWAMFAFL(@09),X'00'                                 0193 00188000
*       PWASRTHB=RWASRDF;           /* HIR THRESHOLD DEFAULT         */ 00189000
         MVC   PWASRTHB(8,@09),RWASRDF(RWAPTR)                     0194 00190000
*       PWADGTHB=RWADGDF;           /* BUFFER THRESHOLD DEFAULT      */ 00191000
         MVC   PWADGTHB(8,@09),RWADGDF(RWAPTR)                     0195 00192000
*       PWAPDTHB=RWAPDDF;           /* INSTRUCTION PROCESSING DAM- 0196 00193000
*                                      AGE THRESHOLD DEFAULT @G55CP2P*/ 00194000
         MVC   PWAPDTHB(16,@09),RWAPDDF(RWAPTR)                    0196 00195000
*       PWASDTHB=RWASDDF;           /* SYSTEM DAMAGE THRESHOLD     0197 00196000
*                                      DEFAULT               @G55CP2P*/ 00197000
         MVC   PWASDTHB(16,@09),RWASDDF(RWAPTR)                    0197 00198000
*       PWAIVTHB=RWAIVDF;           /* INVALID REG OR PSW THRESHOLD     00199000
*                                      DEFAULT               @G55CP2P*/ 00200000
         MVC   PWAIVTHB(16,@09),RWAIVDF(RWAPTR)                    0198 00201000
*       PWATCTHB=RWATCDF;           /* TOD CLOCK DAMAGE THRESHOLD  0199 00202000
*                                      DEFAULT               @G55CP2P*/ 00203000
         MVC   PWATCTHB(16,@09),RWATCDF(RWAPTR)                    0199 00204000
*       PWAPTTHB=RWAPTDF;           /* CPU TIMER DAMAGE THRESHOLD  0200 00205000
*                                      DEFAULT               @G55CP2P*/ 00206000
         MVC   PWAPTTHB(16,@09),RWAPTDF(RWAPTR)                    0200 00207000
*       PWACCTHB=RWACCDF;           /* CLOCK COMPARATOR DAMAGE     0201 00208000
*                                      THRESHOLD DEFAULT     @G55CP2P*/ 00209000
*                                                                  0201 00210000
         MVC   PWACCTHB(16,@09),RWACCDF(RWAPTR)                    0201 00211000
*       /*************************************************************/ 00212000
*       /*                                                           */ 00213000
*       /* INIT IGFPNRFH'S SRB FOR NUCLEUS REFRESH.          @G559P2L*/ 00214000
*       /*                                                           */ 00215000
*       /*************************************************************/ 00216000
*                                                                  0202 00217000
*       PWANRSRB=PWANRSRB&&PWANRSRB;/* CLEAR IGFPNRFH SRB.   @G559P2L*/ 00218000
         XC    PWANRSRB(44,@09),PWANRSRB(@09)                      0202 00219000
*       RFY                                                        0203 00220000
*         SRBSECT BASED(ADDR(PWANRSRB));/*                   @G559P2L*/ 00221000
*       SRBID='SRB ';               /* IDENTIFY AS AN SRB    @G559P2L*/ 00222000
         AL    @09,@CF02067                                        0204 00223000
         MVC   SRBID(4,@09),@CC02035                               0204 00224000
*       SRBPASID='0001'X;           /* SET TO RUN IN MASTER'S MEMORY.   00225000
*                                                            @G559P2L*/ 00226000
         MVC   SRBPASID(2,@09),@CH02036                            0205 00227000
*       SRBASCB=PSAAOLD;            /* SET TO MASTER'S ASCB. @G559P2L*/ 00228000
         L     @11,PSAAOLD(,ABSZERO)                               0206 00229000
         ST    @11,SRBASCB(,@09)                                   0206 00230000
*       SRBRMTR=ADDR(CVTBRET);      /* RESOURCE MANAGER IS A BR 14.     00231000
*                                                            @G559P2L*/ 00232000
         L     @11,CVTPTR(,ABSZERO)                                0207 00233000
         LA    @07,CVTBRET(,@11)                                   0207 00234000
         ST    @07,SRBRMTR(,@09)                                   0207 00235000
*       SRBCPAFF=CSDMASK((PSACPULA&'0F'X)+1);/* RUN ON THIS CPU.   0208 00236000
*                                                            @G559P2L*/ 00237000
         LH    @07,PSACPULA(,ABSZERO)                              0208 00238000
         N     @07,@CF02037                                        0208 00239000
         ALR   @07,@07                                             0208 00240000
         L     @11,CVTCSD(,@11)                                    0208 00241000
         ALR   @11,@07                                             0208 00242000
         MVC   SRBCPAFF(2,@09),CSDMASK(@11)                        0208 00243000
*       SRBEP=ADDR(IGFPNRFH);       /* PUT IN ROUTINE ADDRESS.     0209 00244000
*                                                            @ZM45605*/ 00245000
         L     @11,@CA01993                                        0209 00246000
         ST    @11,SRBEP(,@09)                                     0209 00247000
*     END;                          /* PCCAPWAV = 0                  */ 00248000
*   ELSE                                                           0211 00249000
*     ;                                                            0211 00250000
@RF00184 DS    0H                                                  0212 00251000
*                                                                  0212 00252000
*/********************************************************************/ 00253000
*/*                                                                  */ 00254000
*/*SEGMENT-                                                          */ 00255000
*/*          BLDIOEL--PART 1                                         */ 00256000
*/*                                                                  */ 00257000
*/*FUNCTION-                                                         */ 00258000
*/*          BUILD THE IOEL AND THE MESSAGE BUFFER AND THE LRB       */ 00259000
*/*                                                                  */ 00260000
*/*OPERATION-                                                        */ 00261000
*/*          IF THE CONDITION CODE IS ZERO FROM A STIDC FOR AN       */ 00262000
*/*          ONLINE CHANNEL THEN THE THE LENGTH OF THE IOEL HAS      */ 00263000
*/*          BEEN STORED IN THE PSA.THE IOEL LENGTH TO BE PLACED IN  */ 00264000
*/*          THE PCCA IS THE GREATER OF THE VALUE STORED BY STIDC    */ 00265000
*/*          OR 112.                                                 */ 00266000
*/*                                                                  */ 00267000
*/*          IF ALL ONLINE CHANNELS PRODUCE A STIDC CONDITION CODE   */ 00268000
*/*          THAT IS NOT EQUAL TO ZERO THEN THE PCCA IOEL LENGTH     */ 00269000
*/*          WILL BE SET TO 640.                                     */ 00270000
*/*                                                                  */ 00271000
*/*N.B.-                                                             */ 00272000
*/*          THE CAT TABLE IS THE ONE FOR THE EXECUTING CPU SINCE    */ 00273000
*/*          THE STIDC WILL OPERATE AGAINST THE CHANNELS OF THE      */ 00274000
*/*          EXECUTING CPU.THE IOEL LENGTH WILL BE PLACED IN THE     */ 00275000
*/*          PCCA FOR THE CPU AS SPECIFIED BY REGISTER ONE ON ENTRY  */ 00276000
*/*          TO IGFPBUCR.                                            */ 00277000
*/*                                                                  */ 00278000
*/*CHANGE ACTIVITY-                                                  */ 00279000
*/*      @G51BP2O-                                                   */ 00280000
*/*          NEW CHANNEL SUPPORT FOR SU51                            */ 00281000
*/*                                                                  */ 00282000
*/********************************************************************/ 00283000
*                                                                  0212 00284000
*   RFY                                                            0212 00285000
*    (R1,                           /* USED FOR THE DO LOOP          */ 00286000
*     R3,                           /* POINTER TO A CAT ENTRY        */ 00287000
*     R7)                           /* CHANNEL NUMBER FOR STIDC      */ 00288000
*         RSTD;                     /* WILL BE UNRSTD AT END         */ 00289000
*   ABSZERO=ZERO;                   /* PSA ADDRESS OF EXECUTING CPU  */ 00290000
         SLR   ABSZERO,ABSZERO                                     0213 00291000
*   R3=ADDR(PSAPCCAV->              /* PCCA FOR EXECUTING CPU        */ 00292000
*       PCCACAT);                   /* CAT FOR EXECUTING CPU         */ 00293000
         L     R3,PSAPCCAV(,ABSZERO)                               0214 00294000
         LA    R3,PCCACAT(,R3)                                     0214 00295000
*   R7=ZERO;                        /* START WITH CHANNEL ZERO       */ 00296000
         SLR   R7,R7                                               0215 00297000
*   PCCACELL=ZERO;                  /* PCCA FOR CPU INDICATED BY R1  */ 00298000
         SLR   @11,@11                                             0216 00299000
         STH   @11,PCCACELL(,PCCAPTR)                              0216 00300000
*   DO R1=1                         /* LOOP THROUGH ALL CHANNELS     */ 00301000
*         TO LENGTH(PCCACAT)/       /* TOTAL LENGTH OF CAT IN PCCA   */ 00302000
*         LENGTH(CAT)               /* LENGTH OF ONE CAT ENTRY       */ 00303000
*         WHILE PCCACELL=ZERO;      /* UNTIL IOEL LENGTH IS GOTTEN   */ 00304000
         LA    R1,1                                                0217 00305000
@DL00217 LH    @11,PCCACELL(,PCCAPTR)                              0217 00306000
         LTR   @11,@11                                             0217 00307000
         BNZ   @DC00217                                            0217 00308000
*     IF CATNOP=NO                  /* I.E. CHANNEL IS ONLINE        */ 00309000
*       THEN                                                       0218 00310000
         TM    CATNOP(R3),B'01000000'                              0218 00311000
         BNZ   @RF00218                                            0218 00312000
*       DO;                                                        0219 00313000
*         GEN REFS(R7)              /* R7 = '0000NN00'X              */ 00314000
*             (STIDC 0(R7));        /* OUTPUT PLACED IN PSA          */ 00315000
         STIDC 0(R7)                                                    00316000
*         BC(NONZERO,BADSTIDC);     /* BRANCH FOR NON ZERO CC        */ 00317000
         BC    7,BADSTIDC                                          0221 00318000
*         PCCACELL=MAX(FLCCHIL,     /* = VALUE STORED BY STIDC       */ 00319000
*             112);                 /* = MIN IOEL LENGTH POSSIBLE    */ 00320000
         LH    @11,FLCCHIL(,ABSZERO)                               0222 00321000
         LA    @09,112                                             0222 00322000
         CR    @11,@09                                             0222 00323000
         BNL   *+6                                                      00324000
         LR    @11,@09                                             0222 00325000
         STH   @11,PCCACELL(,PCCAPTR)                              0222 00326000
*BADSTIDC:                          /* ENTRY FOR NON ZERO STIDC CC   */ 00327000
*       END;                                                       0223 00328000
BADSTIDC DS    0H                                                  0224 00329000
*     R7=R7+INCRCHNL;               /* '0000NN00'X + '00000100'X     */ 00330000
@RF00218 AL    R7,@CF01971                                         0224 00331000
*     R3=R3+                        /* CURRENT CAT ENTRY             */ 00332000
*         LENGTH(CAT);              /* OFFSET TO NEXT CAT ENTRY      */ 00333000
         AL    R3,@CF00038                                         0225 00334000
*   END;                                                           0226 00335000
         AL    R1,@CF00069                                         0226 00336000
         C     R1,@CF00143                                         0226 00337000
         BNH   @DL00217                                            0226 00338000
@DC00217 DS    0H                                                  0227 00339000
*   RFY                                                            0227 00340000
*    (R1,                                                          0227 00341000
*     R3,                                                          0227 00342000
*     R7) UNRSTD;                                                  0227 00343000
*   IF PCCACELL=ZERO                /* => ALL CHANNELS STIDC CC > 0  */ 00344000
*     THEN                                                         0228 00345000
         LH    @11,PCCACELL(,PCCAPTR)                              0228 00346000
         LTR   @11,@11                                             0228 00347000
         BNZ   @RF00228                                            0228 00348000
*     PCCACELL=IOELMAXL;            /* MAXIMUM IOEL LENGTH           */ 00349000
         MVC   PCCACELL(2,PCCAPTR),@CH01973                        0229 00350000
*                                                                  0230 00351000
*/********************************************************************/ 00352000
*/*                                                                  */ 00353000
*/*SEGMENT-                                                          */ 00354000
*/*          BLDIOEL--PART 2                                         */ 00355000
*/*                                                                  */ 00356000
*/*FUNCTION-                                                         */ 00357000
*/*          GET SPACE FOR NEW IOEL ETC                              */ 00358000
*/*                                                                  */ 00359000
*/*OPERATION-                                                        */ 00360000
*/*          GET SPACE FROM SQA FOR A MESSAGE BUFFER AND THE IOEL    */ 00361000
*/*          AND AN LRB.                                             */ 00362000
*/*                                                                  */ 00363000
*/*          THE ADDRESS OF THE GOTTEN AREA WILL PLACED IN THE PCCA  */ 00364000
*/*                                                                  */ 00365000
*/*CHANGE ACTIVITY-                                                  */ 00366000
*/*      @G51BP2O-                                                   */ 00367000
*/*          NEW CHANNEL SUPPORT FOR SU51                            */ 00368000
*/*                                                                  */ 00369000
*/********************************************************************/ 00370000
*                                                                  0230 00371000
*   PCCACCHM=ZERO;                  /* INDICATE NO SPACE GOTTEN      */ 00372000
@RF00228 SLR   @11,@11                                             0230 00373000
         ST    @11,PCCACCHM(,PCCAPTR)                              0230 00374000
*   DO;                             /* GETMAIN (R) LV(PCCACELL+LENGTH   00375000
*                                      (CDACCHM1)+LENGTH(LRB)+LENGTH(   00376000
*                                      LRBCCCH)+LENGTH(LRBCVAR)+16*LE   00377000
*                                      NGTH(LRBCMP))SP(245)          */ 00378000
*     RESPECIFY                                                    0232 00379000
*      (GPR01F,                                                    0232 00380000
*       GPR00F) RESTRICTED;                                        0232 00381000
*     GPR00F=PCCACELL+LENGTH(CDACCHM1)+LENGTH(LRB)+LENGTH(LRBCCCH)+     00382000
*         LENGTH(LRBCVAR)+16*LENGTH(LRBCMP);/* LENGTH REQUESTED      */ 00383000
         LA    GPR00F,216                                          0233 00384000
         AH    GPR00F,PCCACELL(,PCCAPTR)                           0233 00385000
*     GPR01F=245;                   /* SP IN REG 1                   */ 00386000
         LA    GPR01F,245                                          0234 00387000
*     GEN(SLL   GPR01F,24)                                         0235 00388000
*     NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;                 0235 00389000
         SLL   GPR01F,24                                                00390000
*     GPR00F=GPR00F|GPR01F;         /* SP IN HIGH BYTE               */ 00391000
         OR    GPR00F,GPR01F                                       0236 00392000
*     GPR01F=-1;                    /* INDICATE A GETMAIN REQUEST    */ 00393000
         SLR   GPR01F,GPR01F                                       0237 00394000
         BCTR  GPR01F,0                                            0237 00395000
*     SVC(10);                      /* R-FORM OF GETMAIN             */ 00396000
         SVC   10                                                  0238 00397000
*     RESPECIFY                                                    0239 00398000
*      (GPR01F,                                                    0239 00399000
*       GPR00F) UNRESTRICTED;                                      0239 00400000
*   END;                            /* GETMAIN (R) LV(PCCACELL+LENGTH   00401000
*                                      (CDACCHM1)+LENGTH(LRB)+LENGTH(   00402000
*                                      LRBCCCH)+LENGTH(LRBCVAR)+16*LE   00403000
*                                      NGTH(LRBCMP))SP(245)          */ 00404000
*   RFY                                                            0241 00405000
*     R1 RSTD;                                                     0241 00406000
*   PCCACCHM=R1;                    /* BEGINNING OF GOTTEN AREA      */ 00407000
         ST    R1,PCCACCHM(,PCCAPTR)                               0242 00408000
*   RFY                                                            0243 00409000
*     R1 UNRSTD;                                                   0243 00410000
*                                                                  0244 00411000
*/********************************************************************/ 00412000
*/*                                                                  */ 00413000
*/*SEGMENT-                                                          */ 00414000
*/*          BLDIOEL--PART 3                                         */ 00415000
*/*                                                                  */ 00416000
*/*FUNCTION-                                                         */ 00417000
*/*          LINK THE GOTTEN AREA TO THE PCCA AND THE PSA            */ 00418000
*/*                                                                  */ 00419000
*/*OPERATION-                                                        */ 00420000
*/*          THE MESSAGE BUFFER APPEARS FIRST IN THE GOTTEN AREA.    */ 00421000
*/*          THE POINTER TO THE MESSAGE BUFFER IS SET BY GETMAIN.    */ 00422000
*/*                                                                  */ 00423000
*/*          THE LRB FOLLOWS THE MESSAGE BUFFER AND THE IOEL         */ 00424000
*/*          FOLLOWS THE LRB.POINTERS TO THEM ARE IN THE PCCA.       */ 00425000
*/*                                                                  */ 00426000
*/*          THE FIRST WORD OF THE IOEL IS SET TO ALL FOXES.         */ 00427000
*/*                                                                  */ 00428000
*/*          THE REMAINDER OF THE INITIALIZATION REQUIRES I/O AND    */ 00429000
*/*          EXTERNAL INTERRUPTS BE DISABLED.                        */ 00430000
*/*                                                                  */ 00431000
*/*          WHEN UPDATING THE PSA THE CORRECT ADDRESS FOR THE PSA   */ 00432000
*/*          HAS TO BE USED.THE PSA ADDRESS IS A FUNCTION OF THE     */ 00433000
*/*          PROCESSOR NUMBER CONTAINED IN REGISTER ONE ON INPUT TO  */ 00434000
*/*          IGFPBUCR.IF THE PROCESSOR INVOLVED IS NOT THE           */ 00435000
*/*          EXECUTING PROCESSOR THEN THAT PROCESSORS PSA ADDRESS    */ 00436000
*/*          IS OBTAINED FROM ITS PCCA.IF THE PROCESSOR INVOLVED IS  */ 00437000
*/*          THE EXECUTING PROCESSOR THEN AN ADDRESS OF ZERO IS      */ 00438000
*/*          USED IN ORDER TO AVOID REVERSE PREFIXING WHEN RUNNING   */ 00439000
*/*          IN A MULTIPROCESSOR ENVIRONMENT.                        */ 00440000
*/*                                                                  */ 00441000
*/*CHANGE ACTIVITY-                                                  */ 00442000
*/*      @G51BP2O-                                                   */ 00443000
*/*          NEW CHANNEL SUPPORT FOR SU51                            */ 00444000
*/*                                                                  */ 00445000
*/********************************************************************/ 00446000
*                                                                  0244 00447000
*   PCCAELBA=PCCACCHM+              /* LOCATE THE LRB AFTER THE      */ 00448000
*       LENGTH(CDACCHM1);           /* MESSAGE BUFFER                */ 00449000
         L     @11,PCCACCHM(,PCCAPTR)                              0244 00450000
         LA    @09,64                                              0244 00451000
         ALR   @09,@11                                             0244 00452000
         ST    @09,PCCAELBA(,PCCAPTR)                              0244 00453000
*   PCCAELAD=PCCACCHM+              /* LOCATE THE IOEL AFTER THE     */ 00454000
*       LENGTH(CDACCHM1)+           /* MESSAGE BUFFER AND THE        */ 00455000
*       LENGTH(LRBCCCH)+            /* LRB                           */ 00456000
*       LENGTH(LRB);                /* MORE LRB                      */ 00457000
         AL    @11,@CF02069                                        0245 00458000
         ST    @11,PCCAELAD(,PCCAPTR)                              0245 00459000
*   IOELAREA=FOXES;                 /* INIT BEGINNING OF IOEL        */ 00460000
         MVC   IOELAREA(4,@11),@CB01969                            0246 00461000
*   IF PCCAPTR                      /* PCCA FOR CPU INDICATED BY R1  */ 00462000
*       ^=                          /* IF NOT SAME PCCA              */ 00463000
*       PSAPCCAV                    /* PCCA FOR EXECUTING CPU        */ 00464000
*     THEN                                                         0247 00465000
