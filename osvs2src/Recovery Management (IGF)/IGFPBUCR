*   GEN SETS(SMBYTE)                /* RE-ENABLE I/O                 */ 00498000
*       (STOSM SMBYTE,X'FF'-X'FC'); /* AND EXTERNAL INTERRUPTS       */ 00499000
         STOSM SMBYTE,X'FF'-X'FC'                                       00500000
*   ABSZERO=ZERO;                   /* RESET EXECUTING CPU PSA ADDR  */ 00501000
