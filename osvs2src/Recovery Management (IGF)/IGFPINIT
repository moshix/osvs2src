         TM    RMSBLDL(@05),B'10000000'                            0130 00032000
         BO    @RT00130                                            0130 00033000
*     GOTO BLDLRTN;                 /* GO HANDLE BLDL ENTRY REQUEST  */ 00034000
*   ELSE                                                           0132 00035000
*     IF RMSLOAD='1'B               /* LOAD BIT TESTED               */ 00036000
*       THEN                                                       0132 00037000
