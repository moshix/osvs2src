*             ELSE                  /*                       @Z30RSMT*/ 00294000
*               DO;                 /* NONE OF THE ABOVE             */ 00295000
         B     @RC00172                                            0174 00296000
@RF00172 DS    0H                                                  0175 00297000
*                 SPACEREQ=0;       /* GIVE 'SPACEREQ' A VALUE SO  0175 00298000
*                                      PL/S DOESN'T GET CONFUSED     */ 00299000
         SLR   SPACEREQ,SPACEREQ                                   0175 00300000
*                 RETCODE=BADEVTYP; /* INDICATE BAD DEVICE TYPE      */ 00301000
         LA    RETCODE,12                                          0176 00302000
