         TITLE 'IGFPTREC - MCH EMERGENCY RECORDER                      *00001000
                        '                                               00002000
IGFPTREC CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGFPTREC  75.301'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*                                                                  0106 00018000
*   /*****************************************************************/ 00019000
*   /*                                                               */ 00020000
*   /* READ LOGREC HEADER AND PREPARE TO WRITE TERMINATION RECORD    */ 00021000
*   /*                                                               */ 00022000
*   /*****************************************************************/ 00023000
*                                                                  0106 00024000
*   CALL READHDR;                   /* ATTEMPT TO READ HEADER        */ 00025000
         BAL   @14,READHDR                                         0106 00026000
*   IF RETCODE^=IOGOOD THEN         /* Q. HEADER SUCCESSFULLY READ   */ 00027000
         LTR   RETCODE,RETCODE                                     0107 00028000
         BNZ   @RT00107                                            0107 00029000
*     RETURN;                       /* NO, RETURN TO CALLER          */ 00030000
*   UPDATSW='0'B;                   /* INITIALIZE 'HEADER UPDATED' 0109 00031000
*                                      SWITCH                        */ 00032000
         NI    UPDATSW,B'01111111'                                 0109 00033000
*   REMBYTES=HDRREMBT;              /* GET NUMBER OF BYTES REMAINING    00034000
*                                      ON CURRENT TRACK              */ 00035000
         LH    REMBYTES,HDRREMBT                                   0110 00036000
         N     REMBYTES,@CF02443                                   0110 00037000
*   WRITESK=HDRLAST;                /* GET BBCCHHR OF LAST RECORD  0111 00038000
*                                      WRITTEN                       */ 00039000
         MVC   WRITESK(7),HDRLAST                                  0111 00040000
*   RECADDR=ADDR(LRB);              /* PUT ADDR OF TERMINATION RECORD   00041000
*                                      IN CCW                        */ 00042000
         L     @07,CVTPTR                                          0112 00043000
         L     @07,RVTPTR(,@07)                                    0112 00044000
         L     @07,RWAPTR(,@07)                                    0112 00045000
         L     @07,LRBPTR(,@07)                                    0112 00046000
         STCM  @07,7,RECADDR                                       0112 00047000
*   RECLNH=LRBTLNH;                 /* PUT LENGTH OF TERMINATION   0113 00048000
*                                      RECORD IN CCW                 */ 00049000
*                                                                  0113 00050000
         L     @07,LRBTLNH(,@07)                                   0113 00051000
         STH   @07,RECLNH                                          0113 00052000
*   /*****************************************************************/ 00053000
*   /*                                                               */ 00054000
*   /* CHECK LOGREC DATA SET TO DETERMINE IF AND WHERE RECORD SHOULD */ 00055000
*   /* BE WRITTEN                                                    */ 00056000
*   /*                                                               */ 00057000
*   /*****************************************************************/ 00058000
*                                                                  0114 00059000
*CHECKLOG:                                                         0114 00060000
*   CALL SEEIFFIT;                  /* SEE IF RECORD WILL FIT ON   0114 00061000
*                                      CURRENT TRACK                 */ 00062000
CHECKLOG BAL   @14,SEEIFFIT                                        0114 00063000
*   IF RETCODE=BADEVTYP THEN        /* Q. VALID DEVICE TYPE FOR    0115 00064000
*                                      LOGREC                        */ 00065000
         C     RETCODE,@CF00032                                    0115 00066000
         BE    @RT00115                                            0115 00067000
*     RETURN;                       /* NO, RETURN TO CALLER          */ 00068000
*   IF RETCODE^=TOOBIG THEN         /* Q. RECORD TOO BIG EVEN FOR  0117 00069000
*                                      EMPTY TRACK                   */ 00070000
         C     RETCODE,@CF00042                                    0117 00071000
         BE    @RF00117                                            0117 00072000
*     DO;                           /* NO, CONTINUE TESTS - OTHERWISE   00073000
*                                      FORGET THIS RECORD            */ 00074000
*       IF RETCODE=ENDTRK THEN      /* Q. RECORD TOO BIG FOR       0119 00075000
*                                      REMAINDER OF CURRENT TRACK    */ 00076000
         C     RETCODE,@CF00040                                    0119 00077000
         BNE   @RF00119                                            0119 00078000
*         DO;                                                      0120 00079000
*           CALL CCHHUPDT;          /* YES, UPDATE TO NEXT TRACK IF     00080000
*                                      ANY                           */ 00081000
         BAL   @14,CCHHUPDT                                        0121 00082000
*           IF RETCODE=DSFULL THEN  /* Q. MORE TRACKS ON DATA SET    */ 00083000
         C     RETCODE,@CF00040                                    0122 00084000
         BE    @RT00122                                            0122 00085000
*             GOTO DONEREC;         /* NO, HAVE TO QUIT HERE         */ 00086000
*         END;                                                     0124 00087000
*                                                                  0124 00088000
*       /*************************************************************/ 00089000
*       /*                                                           */ 00090000
*       /* WRITE A RECORD ON LOGREC                                  */ 00091000
*       /*                                                           */ 00092000
*       /*************************************************************/ 00093000
*                                                                  0125 00094000
*       CALL WRITEREC;              /* ATTEMPT TO WRITE RECORD       */ 00095000
@RF00119 BAL   @14,WRITEREC                                        0125 00096000
*       IF RETCODE^=IOGOOD THEN     /* Q. RECORD WRITTEN SUCCESSFULLY*/ 00097000
         LTR   RETCODE,RETCODE                                     0126 00098000
         BNZ   @RT00126                                            0126 00099000
*         GOTO DONEREC;             /* NO, FORGET CURRENT AND ANY  0127 00100000
*                                      REMAINING RECORDS             */ 00101000
*     END;                                                         0128 00102000
*                                                                  0128 00103000
*   /*****************************************************************/ 00104000
*   /*                                                               */ 00105000
*   /* GET NEXT SYSTEM RECORD TO BE RECORDED                         */ 00106000
*   /*                                                               */ 00107000
*   /*****************************************************************/ 00108000
*                                                                  0129 00109000
*   GEN(RECORD TYPE=TERM)                                          0129 00110000
*   REFS(FLCCVT,PSA,CVTRECRQ,CVTMAP);                              0129 00111000
@RF00117 DS    0H                                                  0129 00112000
         RECORD TYPE=TERM                                               00113000
*   RFY                                                            0130 00114000
*     R15 RSTD;                     /* RESTRICT R15                  */ 00115000
*   IF R15=RECFOUND THEN            /* Q. RECORD FOUND               */ 00116000
         LTR   R15,R15                                             0131 00117000
         BNZ   @RF00131                                            0131 00118000
*     DO;                                                          0132 00119000
*       RECADDR=R1;                 /* YES, PUT RECORD ADDR IN CCW   */ 00120000
         STCM  R1,7,RECADDR                                        0133 00121000
*       RECLNH=R0;                  /* PUT RECORD LENGTH IN CCW      */ 00122000
         STH   R0,RECLNH                                           0134 00123000
*       GOTO CHECKLOG;              /* ATTEMPT TO RECORD IT          */ 00124000
         B     CHECKLOG                                            0135 00125000
*     END;                                                         0136 00126000
*   RFY                                                            0137 00127000
*     R15 UNRSTD;                   /* UNRESTRICT R15                */ 00128000
*                                                                  0137 00129000
@RF00131 DS    0H                                                  0138 00130000
*   /*****************************************************************/ 00131000
*   /*                                                               */ 00132000
*   /* WRITE THE UPDATED LOGREC HEADER AND AN EOF ON SYS1.LOGREC     */ 00133000
*   /*                                                               */ 00134000
*   /*****************************************************************/ 00135000
*                                                                  0138 00136000
*DONEREC:                                                          0138 00137000
*   IF UPDATSW='1'B THEN            /* Q. ANY RECORDS WRITTEN TO   0138 00138000
*                                      LOGREC                        */ 00139000
DONEREC  TM    UPDATSW,B'10000000'                                 0138 00140000
         BNO   @RF00138                                            0138 00141000
*     DO;                                                          0139 00142000
*       CALL WRITEHDR;              /* YES, WRITE THE UPDATED HEADER */ 00143000
         BAL   @14,WRITEHDR                                        0140 00144000
*       CALL WRITEOF;               /* WRITE AN EOF                  */ 00145000
         BAL   @14,WRITEOF                                         0141 00146000
*     END;                                                         0142 00147000
*   RETURN;                         /* RETURN TO CALLER              */ 00148000
@EL00001 L     @13,4(,@13)                                         0143 00149000
@EF00001 DS    0H                                                  0143 00150000
@ER00001 LM    @14,@12,12(@13)                                     0143 00151000
         BR    @14                                                 0143 00152000
*READHDR:                                                          0144 00153000
*                                                                  0144 00154000
*   /*****************************************************************/ 00155000
*   /*                                                               */ 00156000
*   /* SUBROUTINE TO READ SYS1.LOGREC HEADER RECORD                  */ 00157000
*   /*                                                               */ 00158000
*   /*****************************************************************/ 00159000
*                                                                  0144 00160000
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0144 00161000
         B     @EL00001                                            0144 00162000
READHDR  STM   @14,@01,12(@13)                                     0144 00163000
         STM   @03,@12,32(@13)                                     0144 00164000
*   IF DEBPTR^=0 THEN               /* Q. LOGREC DEB INITIALIZED     */ 00165000
         L     @15,CVTPTR                                          0145 00166000
         L     @15,DCBPTR-1(,@15)                                  0145 00167000
         L     @15,DEBPTR+40(,@15)                                 0145 00168000
         LTR   @15,@15                                             0145 00169000
         BZ    @RF00145                                            0145 00170000
*     DO;                           /* YES, PREPARE TO READ HEADER   */ 00171000
*       LOGSKBB=DEBBINUM;           /* GET BB OF LOGREC HEADER RECORD*/ 00172000
         AL    @15,@CF00845                                        0147 00173000
         MVC   LOGSKBB(2),DEBBINUM(@15)                            0147 00174000
*       LOGSKCC=DEBSTRCC;           /* GET CC                        */ 00175000
         MVC   LOGSKCC(2),DEBSTRCC(@15)                            0148 00176000
*       LOGSKHH=DEBSTRHH;           /* GET HH                        */ 00177000
         MVC   LOGSKHH(2),DEBSTRHH(@15)                            0149 00178000
*       HREADCMD=READDATA;          /* INSURE CORRECT COMMAND CODE IN   00179000
*                                      'READ DATA' CCW               */ 00180000
         MVI   HREADCMD,X'06'                                      0150 00181000
*       R1=ADDR(HCCWS);             /* POINT TO CCW'S TO READ HEADER */ 00182000
         LA    R1,HCCWS                                            0151 00183000
*       BACKPTR=R13;                /* CHAIN SAVE AREA TO CALLER'S   */ 00184000
         ST    R13,BACKPTR                                         0152 00185000
*       R13=ADDR(REGSAVE);          /* ESTABLISH NEW SAVE AREA PTR   */ 00186000
         LA    R13,REGSAVE                                         0153 00187000
*       CALL IGFPTRIO;              /* ATTEMPT I/O TO READ HEADER    */ 00188000
         BAL   @14,IGFPTRIO                                        0154 00189000
*       R13=BACKPTR;                /* RESTORE CALLER'S SAVE AREA PTR*/ 00190000
         L     R13,BACKPTR                                         0155 00191000
*       IF RETCODE=IOGOOD           /* IF I/O WAS SUCCESSFUL         */ 00192000
*           &HDRCHKBT='FF'X THEN    /* AND 'CHECK BYTE' IN HEADER IS    00193000
*                                      CORRECT                       */ 00194000
         LTR   RETCODE,RETCODE                                     0156 00195000
         BNZ   @RF00156                                            0156 00196000
         CLI   HDRCHKBT,X'FF'                                      0156 00197000
         BE    @RT00156                                            0156 00198000
*         RETURN;                   /* THEN RETURN TO CALLER WITH  0157 00199000
*                                      'IOGOOD' CODE                 */ 00200000
*     END;                                                         0158 00201000
@RF00156 DS    0H                                                  0159 00202000
*   RETCODE=DSNOTRDY;               /* INDICATE LOGREC CAN'T BE USED */ 00203000
@RF00145 LA    RETCODE,4                                           0159 00204000
*   END READHDR;                    /* RETURN TO CALLER              */ 00205000
@EL00002 DS    0H                                                  0160 00206000
@EF00002 DS    0H                                                  0160 00207000
@ER00002 LM    @14,@01,12(@13)                                     0160 00208000
         LM    @03,@12,32(@13)                                     0160 00209000
         BR    @14                                                 0160 00210000
*SEEIFFIT:                                                         0161 00211000
*                                                                  0161 00212000
*   /*****************************************************************/ 00213000
