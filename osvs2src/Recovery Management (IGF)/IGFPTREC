         B     @EL00003                                            0191 00357000
*CCHHUPDT:                                                         0192 00358000
*                                                                  0192 00359000
*   /*****************************************************************/ 00360000
*   /*                                                               */ 00361000
*   /* SUBROUTINE TO UPDATE TARGET CCHH ON SYS1.LOGREC AFTER TRACK   */ 00362000
*   /* FULL CONDITION                                                */ 00363000
*   /*                                                               */ 00364000
*   /*****************************************************************/ 00365000
*                                                                  0192 00366000
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0192 00367000
*                                                                  0192 00368000
CCHHUPDT STM   @14,@01,12(@13)                                     0192 00369000
         STM   @03,@05,32(@13)                                     0192 00370000
         STM   @07,@12,48(@13)                                     0192 00371000
*   /*****************************************************************/ 00372000
*   /*                                                               */ 00373000
*   /* CHECK FOR DATA SET FULL                                       */ 00374000
*   /*                                                               */ 00375000
*   /*****************************************************************/ 00376000
*                                                                  0193 00377000
*   IF WRITCCHH^<HDRHILIM THEN      /* Q. LAST RECORD WRITTEN ON LAST   00378000
*                                      TRACK OF DATA SET             */ 00379000
         CLC   WRITCCHH(4),HDRHILIM                                0193 00380000
         BL    @RF00193                                            0193 00381000
*     DO;                                                          0194 00382000
*       RETCODE=DSFULL;             /* YES, INDICATE DATA SET FULL   */ 00383000
         LA    RETCODE,4                                           0195 00384000
*       RETURN;                     /* RETURN TO CALLER              */ 00385000
@EL00004 DS    0H                                                  0196 00386000
@EF00004 DS    0H                                                  0196 00387000
@ER00004 LM    @14,@01,12(@13)                                     0196 00388000
         LM    @03,@05,32(@13)                                     0196 00389000
         LM    @07,@12,48(@13)                                     0196 00390000
         BR    @14                                                 0196 00391000
*     END;                                                         0197 00392000
*                                                                  0197 00393000
*   /*****************************************************************/ 00394000
*   /*                                                               */ 00395000
*   /* CHECK FOR END OF CYLINDER                                     */ 00396000
*   /*                                                               */ 00397000
*   /*****************************************************************/ 00398000
*                                                                  0198 00399000
*   IF WRITEHH^<HDRHITRK THEN       /* Q. LAST RECORD WRITTEN ON LAST   00400000
*                                      TRACK OF CYLINDER             */ 00401000
@RF00193 CLC   WRITEHH(2),HDRHITRK                                 0198 00402000
         BL    @RF00198                                            0198 00403000
*     DO;                                                          0199 00404000
*       WRITECC=WRITECC+1;          /* YES, BUMP ADDR TO NEXT      0200 00405000
*                                      CYLINDER                      */ 00406000
         LH    @02,WRITECC                                         0200 00407000
         LA    @02,1(,@02)                                         0200 00408000
         STH   @02,WRITECC                                         0200 00409000
*       WRITEHH=0;                  /* SET HEAD ADDR TO ZERO         */ 00410000
         SLR   @02,@02                                             0201 00411000
         STH   @02,WRITEHH                                         0201 00412000
*     END;                                                         0202 00413000
*   ELSE                                                           0203 00414000
*     WRITEHH=WRITEHH+1;            /* BUMP HEAD ADDR BY ONE SINCE 0203 00415000
*                                      NOT AT END OF CYLINDER        */ 00416000
         B     @RC00198                                            0203 00417000
@RF00198 LH    @02,WRITEHH                                         0203 00418000
         LA    @02,1(,@02)                                         0203 00419000
         STH   @02,WRITEHH                                         0203 00420000
*   REMBYTES=HDRMAXBT-SPACEREQ;     /* BYTES REMAINING ON TRACK AFTER   00421000
*                                      CURRENT RECORD IS WRITTEN = 0204 00422000
*                                      MAX NUMBER OF BYTES TRACK CAN    00423000
*                                      HOLD MINUS NUMBER OF BYTES  0204 00424000
*                                      NEEDED TO WRITE CURRENT RECORD*/ 00425000
@RC00198 LH    REMBYTES,HDRMAXBT                                   0204 00426000
         N     REMBYTES,@CF02443                                   0204 00427000
         SLR   REMBYTES,SPACEREQ                                   0204 00428000
*   WRITERM1=0;                     /* SET RECORD NUMBER FOR SEARCH     00429000
*                                      TO ZERO                       */ 00430000
         MVI   WRITERM1,X'00'                                      0205 00431000
*   RETCODE=NOTFULL;                /* INDICATE DATA SET NOT FULL    */ 00432000
         SLR   RETCODE,RETCODE                                     0206 00433000
*   END CCHHUPDT;                   /* RETURN TO CALLER              */ 00434000
         B     @EL00004                                            0207 00435000
*WRITEREC:                                                         0208 00436000
*                                                                  0208 00437000
*   /*****************************************************************/ 00438000
*   /*                                                               */ 00439000
*   /* SUBROUTINE TO WRITE A RECORD TO SYS1.LOGREC                   */ 00440000
*   /*                                                               */ 00441000
*   /*****************************************************************/ 00442000
*                                                                  0208 00443000
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0208 00444000
*                                                                  0208 00445000
WRITEREC STM   @14,@01,12(@13)                                     0208 00446000
         STM   @03,@12,32(@13)                                     0208 00447000
*   /*****************************************************************/ 00448000
*   /*                                                               */ 00449000
*   /* SET UP COUNT FIELD OF RECORD                                  */ 00450000
*   /*                                                               */ 00451000
*   /*****************************************************************/ 00452000
*                                                                  0209 00453000
*   CNTCCHH=WRITCCHH;               /* SET CCHH OF RECORD TO BE    0209 00454000
*                                      WRITTEN                       */ 00455000
         MVC   CNTCCHH(4),WRITCCHH                                 0209 00456000
*   CNTR=WRITERM1+1;                /* SET RECORD NUMBER OF RECORD TO   00457000
*                                      BE WRITTEN                    */ 00458000
         IC    @07,WRITERM1                                        0210 00459000
         LA    @07,1(,@07)                                         0210 00460000
         STC   @07,CNTR                                            0210 00461000
*   CNTDL=RECLNH;                   /* SET LENGTH OF RECORD TO BE  0211 00462000
*                                      WRITTEN                       */ 00463000
*                                                                  0211 00464000
         LH    @07,RECLNH                                          0211 00465000
         N     @07,@CF02443                                        0211 00466000
         STH   @07,CNTDL                                           0211 00467000
*   /*****************************************************************/ 00468000
*   /*                                                               */ 00469000
*   /* ATTEMPT I/O AND CHECK RESULTS                                 */ 00470000
*   /*                                                               */ 00471000
*   /*****************************************************************/ 00472000
*                                                                  0212 00473000
*   RWCKDCHN='1'B;                  /* INSURE DATA CHAINING ON IN  0212 00474000
*                                      WCKD CCW                      */ 00475000
         OI    RWCKDCHN,B'10000000'                                0212 00476000
*   R1=ADDR(RCCWS);                 /* POINT TO CCW'S TO WRITE RECORD*/ 00477000
         LA    R1,RCCWS                                            0213 00478000
*   BACKPTR=R13;                    /* CHAIN SAVE AREA TO CALLER'S   */ 00479000
         ST    R13,BACKPTR                                         0214 00480000
*   R13=ADDR(REGSAVE);              /* ESTABLISH NEW SAVE AREA PTR   */ 00481000
         LA    R13,REGSAVE                                         0215 00482000
*   CALL IGFPTRIO;                  /* ATTEMPT I/O TO WRITE RECORD   */ 00483000
         BAL   @14,IGFPTRIO                                        0216 00484000
*   R13=BACKPTR;                    /* RESTORE CALLER'S SAVE AREA PTR*/ 00485000
         L     R13,BACKPTR                                         0217 00486000
*   IF RETCODE^=IOGOOD THEN         /* Q. I/O SUCCESSFUL             */ 00487000
         LTR   RETCODE,RETCODE                                     0218 00488000
         BNZ   @RT00218                                            0218 00489000
*     RETURN;                       /* NO, RETURN WITH CODE PASSED BY   00490000
*                                      IGFPTRIO                      */ 00491000
*   WRITERM1=WRITERM1+1;            /* UPDATE SEEK ADDR TO RECORD  0220 00492000
*                                      JUST WRITTEN                  */ 00493000
         IC    @07,WRITERM1                                        0220 00494000
         LA    @07,1(,@07)                                         0220 00495000
         STC   @07,WRITERM1                                        0220 00496000
*   HDRLAST=WRITESK;                /* UPDATE LOGREC HEADER WITH ADDR   00497000
*                                      OF RECORD JUST WRITTEN        */ 00498000
         MVC   HDRLAST(7),WRITESK                                  0221 00499000
*   HDRREMBT=REMBYTES;              /* UPDATE HEADER WITH BYTES    0222 00500000
*                                      REMAINING ON CURRENT TRACK    */ 00501000
         STH   REMBYTES,HDRREMBT                                   0222 00502000
*   UPDATSW='1'B;                   /* INDICATE HEADER HAS BEEN    0223 00503000
*                                      UPDATED                       */ 00504000
         OI    UPDATSW,B'10000000'                                 0223 00505000
*   END WRITEREC;                   /* RETURN TO CALLER WITH 'IOGOOD'   00506000
*                                      RETURN CODE                   */ 00507000
@EL00005 DS    0H                                                  0224 00508000
@EF00005 DS    0H                                                  0224 00509000
@ER00005 LM    @14,@01,12(@13)                                     0224 00510000
         LM    @03,@12,32(@13)                                     0224 00511000
         BR    @14                                                 0224 00512000
*WRITEHDR:                                                         0225 00513000
*                                                                  0225 00514000
*   /*****************************************************************/ 00515000
*   /*                                                               */ 00516000
*   /* SUBROUTINE TO WRITE SYS1.LOGREC HEADER RECORD                 */ 00517000
*   /*                                                               */ 00518000
*   /*****************************************************************/ 00519000
*                                                                  0225 00520000
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0225 00521000
