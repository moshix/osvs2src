*                                      LIST FORM OF WTO      @YM02558*/ 00612000
@RC00196 LA    @10,WTOMSG+5(OUTCOUNT)                              0199 00613000
         MVC   0(2,@10),GENROUT                                    0199 00614000
*   R1=ADDR(WTOMSG);                /* SET UP REG 1 TO DO THE WTO    */ 00615000
         LA    R1,WTOMSG                                           0200 00616000
*   GEN;                                                           0201 00617000
   WTO  MF=(E,(R1))                                                     00618000
*   RETURN;                         /* RETURN TO MAINLINE            */ 00619000
@EL00006 DS    0H                                                  0202 00620000
@EF00006 DS    0H                                                  0202 00621000
@ER00006 LM    @14,@12,@SA00006                                    0202 00622000
         BR    @14                                                 0202 00623000
*   END WTORTN;                                                    0203 00624000
*   END IGFPWMSG                                                   0204 00625000
