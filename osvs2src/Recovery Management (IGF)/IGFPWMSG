         TITLE 'IGFPWMSG - OPERATOR MESSAGE WRITER                     *00001000
                        '                                               00002000
IGFPWMSG CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'IGFPWMSG  77.125'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @12,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@12                                         0001 00013000
         L     @00,@SIZDATD                                        0001 00014000
         GETMAIN  R,LV=(0)                                              00015000
         LR    @11,@01                                             0001 00016000
         USING @DATD,@11                                           0001 00017000
         ST    @13,@SA00001+4                                      0001 00018000
         LM    @00,@01,20(@13)                                     0001 00019000
         ST    @11,8(,@13)                                         0001 00020000
         LR    @13,@11                                             0001 00021000
*                                                                  0058 00022000
*   /*****************************************************************/ 00023000
*   /*                                                               */ 00024000
*   /* SEARCH MESSAGE BUFFER POOL FOR ACTIVE BUFFERS                 */ 00025000
*   /*                                                               */ 00026000
*   /*****************************************************************/ 00027000
*                                                                  0058 00028000
*   DO MSGPTR=RVTMSGPT BY LENGTH(MSG) WHILE MSGPTR<RVTMSGPT+RVTMSGLN;   00029000
         L     @10,CVTPTR                                          0058 00030000
         L     @10,RVTPTR(,@10)                                    0058 00031000
         L     MSGPTR,RVTMSGPT(,@10)                               0058 00032000
         B     @DE00058                                            0058 00033000
@DL00058 DS    0H                                                  0059 00034000
*                                   /*                       @YM00999*/ 00035000
*     TS(MSGCTL);                   /* IS BUFFER BUSY                */ 00036000
         TS    MSGCTL(MSGPTR)                                      0059 00037000
*     BC(8,NEXT);                   /* NO. GET NEXT BUFFER           */ 00038000
*                                                                  0060 00039000
         BC    8,NEXT                                              0060 00040000
*     /***************************************************************/ 00041000
*     /*                                                             */ 00042000
*     /* ACTIVE BUFFER HAS BEEN FOUND. NOW IT MUST BE DETERMINED IF A*/ 00043000
*     /* VALID MESSAGE NUMBER IS CONTAINED IN THE MSGNUM FIELD. NOTE */ 00044000
*     /* THAT MSGNUM='00'X MEANS THAT THE BUFFER IS NOT COMPLETELY   */ 00045000
*     /* SET UP YET.                                                 */ 00046000
*     /*                                                             */ 00047000
*     /***************************************************************/ 00048000
*                                                                  0061 00049000
*     IF MSGNUM^='00'X THEN         /* IS MESSAGE COMPLETELY SET UP     00050000
*                                                            @YM00999*/ 00051000
         CLI   MSGNUM(MSGPTR),X'00'                                0061 00052000
         BE    @RF00061                                            0061 00053000
*       DO;                         /* YES, PROCESS IT       @YM00999*/ 00054000
*                                                                  0062 00055000
*         /***********************************************************/ 00056000
*         /*                                                         */ 00057000
*         /* THE VARIABLES NECESSARY FOR THE PROCESSING OF EACH      */ 00058000
*         /* INDIVIDUAL MESSAGE MUST BE INITIALIZED AT THIS TIME.    */ 00059000
*         /*                                                         */ 00060000
*         /***********************************************************/ 00061000
*                                                                  0063 00062000
*         IF MSGNUM='58'X THEN      /* IS THIS THE ACR MESSAGE     0063 00063000
*                                                            @G55DP2M*/ 00064000
         CLI   MSGNUM(MSGPTR),X'58'                                0063 00065000
         BNE   @RF00063                                            0063 00066000
*           IF MSGTAB1='FF'X THEN   /* YES- IS THE MESSAGE TO BE   0064 00067000
*                                      PRINTED               @G55DP2M*/ 00068000
         CLI   MSGTAB1(MSGPTR),X'FF'                               0064 00069000
         BNE   @RF00064                                            0064 00070000
*             MSGTAB1='00'X;        /* YES- RESET INDICATOR  @G55DP2M*/ 00071000
         MVI   MSGTAB1(MSGPTR),X'00'                               0065 00072000
*           ELSE                                                   0066 00073000
*             GOTO OUT;             /* NO - GET OUT OF LOOP  @G55DP2M*/ 00074000
*         FOUNDRC=NOTFND;           /* INDICATE THAT MSGNUM NOT FOUND*/ 00075000
@RF00063 LA    FOUNDRC,4                                           0067 00076000
*         MSGPTIND=MSGNUM;          /* GET MESSAGE NUMBER FROM RVT 0068 00077000
*                                      BUFFER                        */ 00078000
         MVC   MSGPTIND(1),MSGNUM(MSGPTR)                          0068 00079000
*         INDEX=1;                  /* INITIALIZE THE STACK WITH   0069 00080000
*                                      POINTER TO TOP                */ 00081000
         LA    @10,1                                               0069 00082000
         LR    INDEX,@10                                           0069 00083000
*         TBLFCNT=1;                /* INITIALIZE TABLE FIELD      0070 00084000
*                                      COUNT(ALLOW FOR TWO ENTRIES)  */ 00085000
         LR    TBLFCNT,@10                                         0070 00086000
*         VARFCNT=0;                /* INITIALIZE VARIABLE FIELD   0071 00087000
*                                      COUNT(ALLOW FOR TWO HEX(X)  0071 00088000
*                                      REPLACES OR ONE CHAR.(C)    0071 00089000
*                                      REPLACE                       */ 00090000
         SLR   VARFCNT,VARFCNT                                     0071 00091000
*         OUTCOUNT=1;               /* INITIALIZE COUNT OF OUTPUT  0072 00092000
*                                      CHARACTERS TO 1               */ 00093000
         LR    OUTCOUNT,@10                                        0072 00094000
*         OUTBUFF=' ';              /* CLEAR ACTUAL WTO OUTPUT BUFFER*/ 00095000
         MVI   OUTBUFF+1,C' '                                      0073 00096000
         MVC   OUTBUFF+2(118),OUTBUFF+1                            0073 00097000
         MVI   OUTBUFF,C' '                                        0073 00098000
*/****************************************************************   */ 00099000
*/*                                                                  */ 00100000
*/*  SUBROUTINE FIND WILL BE USED TO LOCATE THE MESSAGE NUMBER       */ 00101000
*/*   IF THE MESSAGE NUMBER IS NOT IN THE TABLE,FIND WILL RE-        */ 00102000
*/*   TURN A VALUE IN THE FOUND RETURN CODE(FOUNDRC) OF              */ 00103000
*/*   FOUR. A VALUE OF 0(FND) INDICATES MESSAGE FRAGMENT FOUND       */ 00104000
*/*                                                                  */ 00105000
*/****************************************************************   */ 00106000
*                                                                  0074 00107000
*         CALL FIND;                /* FIND MESSAGE TO PROCESS       */ 00108000
         BAL   @14,FIND                                            0074 00109000
*         IF FOUNDRC=FND THEN       /* IS MESSAGE FOUND ?            */ 00110000
         LTR   FOUNDRC,FOUNDRC                                     0075 00111000
         BNZ   @RF00075                                            0075 00112000
*           DO;                     /* YES - GET READY FOR WTO     0076 00113000
*                                                            @YM00999*/ 00114000
*             DO WHILE INDEX>0;                                    0077 00115000
         B     @DE00077                                            0077 00116000
@DL00077 DS    0H                                                  0078 00117000
*               TEXTPTR=STAKPTR(INDEX);/* LAST TEXT ADDRESS          */ 00118000
         LR    @10,INDEX                                           0078 00119000
         SLA   @10,3                                               0078 00120000
         L     TEXTPTR,STAKPTR-8(@10)                              0078 00121000
*               DO TEXTCNT=STAKCNT(INDEX) BY-1 WHILE TEXTCNT>0;    0079 00122000
         L     TEXTCNT,STAKCNT-8(@10)                              0079 00123000
         B     @DE00079                                            0079 00124000
@DL00079 DS    0H                                                  0080 00125000
*                 OUTBUFF(OUTCOUNT)=INCHAR;/* MOVE ONE CHAR          */ 00126000
         LA    @10,OUTBUFF-1(OUTCOUNT)                             0080 00127000
         MVC   0(1,@10),INCHAR(TEXTPTR)                            0080 00128000
*                 IF INCHAR='%' THEN                               0081 00129000
         CLI   INCHAR(TEXTPTR),C'%'                                0081 00130000
         BNE   @RF00081                                            0081 00131000
*                   DO;                                            0082 00132000
*                     TEXTPTR=TEXTPTR+1;/* INCREMENT POSITION OF   0083 00133000
*                                      TEXTPTR                       */ 00134000
         AL    TEXTPTR,@CF00078                                    0083 00135000
*                     IF INCHAR='X'|INCHAR='D' THEN                0084 00136000
         CLI   INCHAR(TEXTPTR),C'X'                                0084 00137000
         BE    @RT00084                                            0084 00138000
         CLI   INCHAR(TEXTPTR),C'D'                                0084 00139000
         BNE   @RF00084                                            0084 00140000
@RT00084 DS    0H                                                  0085 00141000
*                       CALL CONVERT;/* CONVERT TO PRINTABLE CHAR    */ 00142000
         BAL   @14,CONVERT                                         0085 00143000
*                     ELSE                                         0086 00144000
*                       IF INCHAR='C' THEN                         0086 00145000
         B     @RC00084                                            0086 00146000
@RF00084 CLI   INCHAR(TEXTPTR),C'C'                                0086 00147000
         BNE   @RF00086                                            0086 00148000
*                         CALL CHARCONV;/* MOVE CHARACTERS           */ 00149000
         BAL   @14,CHARCONV                                        0087 00150000
*                       ELSE                                       0088 00151000
*                         IF INCHAR='T' THEN                       0088 00152000
         B     @RC00086                                            0088 00153000
@RF00086 CLI   INCHAR(TEXTPTR),C'T'                                0088 00154000
         BNE   @RF00088                                            0088 00155000
*                           CALL REPLACE;/* REPLACE WITH STRING      */ 00156000
         BAL   @14,REPLACE                                         0089 00157000
*                         ELSE                                     0090 00158000
*                           OUTCOUNT=OUTCOUNT+1;/* % NOT FOLLOWED BY    00159000
*                                      VALID CHAR                    */ 00160000
         B     @RC00088                                            0090 00161000
@RF00088 AL    OUTCOUNT,@CF00078                                   0090 00162000
*                   END;                                           0091 00163000
*                 ELSE                                             0092 00164000
*                   DO;                                            0092 00165000
         B     @RC00081                                            0092 00166000
@RF00081 DS    0H                                                  0093 00167000
*                     OUTCOUNT=OUTCOUNT+1;/* INCREMENT OUTPUT COUNTER*/ 00168000
         LA    @10,1                                               0093 00169000
         ALR   OUTCOUNT,@10                                        0093 00170000
*                     TEXTPTR=TEXTPTR+1;/* INCREMENT POINTER TO INPUT   00171000
*                                      TEXT                          */ 00172000
         ALR   TEXTPTR,@10                                         0094 00173000
*                   END;                                           0095 00174000
*               END;                /* THIS ENDS THE PROCESSING FOR A   00175000
*                                      MESSAGE FRAGMENT              */ 00176000
*                                                                  0096 00177000
@RC00081 BCTR  TEXTCNT,0                                           0096 00178000
@DE00079 LTR   TEXTCNT,TEXTCNT                                     0096 00179000
         BP    @DL00079                                            0096 00180000
*               /*****************************************************/ 00181000
*               /*                                                   */ 00182000
*               /* NOW UPDATE THE INDEX VALUE TO SEE IF FRAGMENTS    */ 00183000
*               /* HAVE BEEN STACKED                                 */ 00184000
*               /*                                                   */ 00185000
*               /*****************************************************/ 00186000
*                                                                  0097 00187000
*               INDEX=INDEX-1;      /* DECREMENT SKELETON POSITION 0097 00188000
*                                      POINTER                       */ 00189000
         BCTR  INDEX,0                                             0097 00190000
*             END;                                                 0098 00191000
*                                                                  0098 00192000
@DE00077 LTR   INDEX,INDEX                                         0098 00193000
         BP    @DL00077                                            0098 00194000
*             /*******************************************************/ 00195000
*             /*                                                     */ 00196000
*             /* A COMPLETE MESSAGE HAS BEEN SET UP AT THIS POINT NOW*/ 00197000
*             /* THE MESSAGE MUST BE WRITTEN TO THE OPERATOR AND     */ 00198000
*             /* FURTHUR SEARCH OF THE RVT MESSAGE BUFFER RESUMED    */ 00199000
*             /*                                                     */ 00200000
*             /*******************************************************/ 00201000
*                                                                  0099 00202000
*             CALL WTORTN;          /* ISSUE WTO                     */ 00203000
         BAL   @14,WTORTN                                          0099 00204000
*             IF MSGNUM='58'X THEN                                 0100 00205000
         CLI   MSGNUM(MSGPTR),X'58'                                0100 00206000
         BNE   @RF00100                                            0100 00207000
*               DO;                 /* THIS IS THE ACR MSG   @ZM45609*/ 00208000
*                 MSGVAR1=0;        /* RESET CPU ADDR IN MSG @ZM45609*/ 00209000
         SLR   @10,@10                                             0102 00210000
         STCM  @10,15,MSGVAR1(MSGPTR)                              0102 00211000
*                 GOTO OUT;         /* DONT RESET MSGNUM OR MSGCTL 0103 00212000
*                                                            @ZM45609*/ 00213000
         B     OUT                                                 0103 00214000
*               END;                /*                       @ZM45609*/ 00215000
*           END;                    /*                       @YM00999*/ 00216000
@RF00100 DS    0H                                                  0106 00217000
*         MSGNUM='00'X;             /* RESET MESSAGE NUMBER IN BUFFER   00218000
*                                                            @YM00999*/ 00219000
@RF00075 MVI   MSGNUM(MSGPTR),X'00'                                0106 00220000
*NEXT:                                                             0107 00221000
*         ;                                                        0107 00222000
NEXT     DS    0H                                                  0108 00223000
*         MSGCTL='00'X;             /* ZERO MESSAGE CONTROL FLAG FOR    00224000
*                                      FREE BUFFER                   */ 00225000
         MVI   MSGCTL(MSGPTR),X'00'                                0108 00226000
*       END;                                                       0109 00227000
*OUT:                                                              0110 00228000
*     ;                                                            0110 00229000
@RF00061 DS    0H                                                  0110 00230000
OUT      DS    0H                                                  0111 00231000
*   END;                                                           0111 00232000
*                                                                  0111 00233000
         AL    MSGPTR,@CF00032                                     0111 00234000
@DE00058 L     @10,CVTPTR                                          0111 00235000
         L     @10,RVTPTR(,@10)                                    0111 00236000
         L     @04,RVTMSGPT(,@10)                                  0111 00237000
         AL    @04,RVTMSGLN(,@10)                                  0111 00238000
         CR    MSGPTR,@04                                          0111 00239000
         BL    @DL00058                                            0111 00240000
*   /*****************************************************************/ 00241000
*   /*                                                               */ 00242000
*   /* ENTRY AT THIS POINT INDICATES THAT ALL POSSIBLE MESSAG ES HAVE*/ 00243000
*   /* BEEN WRITTEN.                                                 */ 00244000
*   /*                                                               */ 00245000
*   /*****************************************************************/ 00246000
*                                                                  0112 00247000
*   RETURN;                         /* RETURN TO CALLER              */ 00248000
*                                                                  0112 00249000
@EL00001 L     @13,4(,@13)                                         0112 00250000
@EF00001 L     @00,@SIZDATD                                        0112 00251000
         LR    @01,@11                                             0112 00252000
         FREEMAIN R,LV=(0),A=(1)                                        00253000
@ER00001 LM    @14,@12,12(@13)                                     0112 00254000
         BR    @14                                                 0112 00255000
*   /*****************************************************************/ 00256000
*   /*                                                               */ 00257000
*   /* FIND SUBROUTINE IS USED TO LOCATE A MESSAGE FRAGMENT IN THE   */ 00258000
*   /* MESSAGE SKELETON . A POINTER TO THE DESIRED FRAGMENT EXISTS AT*/ 00259000
*   /* FIELD MSGPTIND. THIS VALUE IS COMPARED AGAINST THE SKELKEY    */ 00260000
*   /* FIELD OF EACH TABLE ENTRY.                                    */ 00261000
*   /*                                                               */ 00262000
*   /*****************************************************************/ 00263000
*                                                                  0113 00264000
*FIND:                                                             0113 00265000
*   PROC;                           /* PROCEDURE TO LOCATE MESSAGE 0113 00266000
*                                      FRAGMENT                      */ 00267000
FIND     STM   @14,@05,12(@13)                                     0113 00268000
         STM   @07,@12,48(@13)                                     0113 00269000
*   FOUNDRC=NOTFND;                 /* INITIALIZE BAD RETURN CODE    */ 00270000
         LA    FOUNDRC,4                                           0114 00271000
*   DO SKELNDX=1 TO SKELENT WHILE FOUNDRC^=FND;/* GET READY TO SEARCH   00272000
*                                      SKELETON              @YM00999*/ 00273000
         LA    SKELNDX,1                                           0115 00274000
         B     @DE00115                                            0115 00275000
@DL00115 LTR   FOUNDRC,FOUNDRC                                     0115 00276000
         BZ    @DC00115                                            0115 00277000
*     IF SKELKEY(SKELNDX)=MSGPTIND  /* SEARCH SKELETON KEY           */ 00278000
*       THEN                                                       0116 00279000
         LR    @10,SKELNDX                                         0116 00280000
         MH    @10,@CH01056                                        0116 00281000
         LA    @07,SKELKEY-57(@10)                                 0116 00282000
         CLC   0(1,@07),MSGPTIND                                   0116 00283000
         BNE   @RF00116                                            0116 00284000
*       DO;                                                        0117 00285000
*         STAKPTR(INDEX)=ADDR(SKELTEXT(SKELNDX));/* FOUND            */ 00286000
         LR    @07,INDEX                                           0118 00287000
         SLA   @07,3                                               0118 00288000
         LA    @15,SKELTEXT-57(@10)                                0118 00289000
         ST    @15,STAKPTR-8(@07)                                  0118 00290000
*         STAKCNT(INDEX)=SKELLNG(SKELNDX);/* SAVE LENGTH             */ 00291000
         SLR   @15,@15                                             0119 00292000
         IC    @15,SKELLNG-57(@10)                                 0119 00293000
         ST    @15,STAKCNT-8(@07)                                  0119 00294000
*         FOUNDRC=FND;              /* INDICATE MSG. FRAGMENT FOUND  */ 00295000
         SLR   FOUNDRC,FOUNDRC                                     0120 00296000
*       END;                                                       0121 00297000
*     ELSE                                                         0122 00298000
*       ;                                                          0122 00299000
@RF00116 DS    0H                                                  0123 00300000
*   END;                                                           0123 00301000
         AL    SKELNDX,@CF00078                                    0123 00302000
@DE00115 C     SKELNDX,SKELENT                                     0123 00303000
         BNH   @DL00115                                            0123 00304000
@DC00115 DS    0H                                                  0124 00305000
*   RETURN;                         /* RETURN TO MAINLINE            */ 00306000
@EL00002 DS    0H                                                  0124 00307000
@EF00002 DS    0H                                                  0124 00308000
@ER00002 LM    @14,@05,12(@13)                                     0124 00309000
         LM    @07,@12,48(@13)                                     0124 00310000
         BR    @14                                                 0124 00311000
*   END FIND;                                                      0125 00312000
*REPLACE:                                                          0126 00313000
*   PROC;                           /* PROCEDURE TO PROCESS 'T'    0126 00314000
*                                      REPLACE CHARACTERS            */ 00315000
REPLACE  STM   @14,@01,@SA00003                                    0126 00316000
         STM   @05,@06,@SA00003+16                                 0126 00317000
         ST    @08,@SA00003+24                                     0126 00318000
         STM   @10,@12,@SA00003+28                                 0126 00319000
*   TBLFCNT=TBLFCNT+1;              /* INCREMENT TABLE FIELD COUNT   */ 00320000
         LA    @10,1                                               0127 00321000
         ALR   TBLFCNT,@10                                         0127 00322000
*   IF TBLFCNT<4                                                   0128 00323000
*                                                                  0128 00324000
*   /*****************************************************************/ 00325000
*   /*                                                               */ 00326000
*   /* IF THE TFC IS LESS THAN 4,THEN A VALID SUBSTUTION CAN OCCUR.  */ 00327000
*   /* IT IS NECESSARY TO UPDATE THE CURRENT POINTERS,SAVE THEM IN   */ 00328000
*   /* THE STAK,BUMP THE STAK INDEX AND SEE IF A VALID SUBSTUTION    */ 00329000
*   /* CHARACTER IS FOUND IN THE SKELETON.                           */ 00330000
*   /*                                                               */ 00331000
*   /*****************************************************************/ 00332000
*                                                                  0128 00333000
*     THEN                                                         0128 00334000
         C     TBLFCNT,@CF00070                                    0128 00335000
         BNL   @RF00128                                            0128 00336000
*     DO;                                                          0129 00337000
*       TEXTPTR=TEXTPTR+1;          /* UPDATE TEXTPTR TO REFLECT 'T'    00338000
*                                      CHAR                          */ 00339000
         ALR   TEXTPTR,@10                                         0130 00340000
*       TEXTCNT=TEXTCNT-1;          /* UPDATE TEXT CNT TO REFLECT 'T'   00341000
*                                      CHAR                          */ 00342000
         BCTR  TEXTCNT,0                                           0131 00343000
*       STAKPTR(INDEX)=TEXTPTR;     /* SAVE TEXT POINTER IN STACK    */ 00344000
         LR    @08,INDEX                                           0132 00345000
         SLA   @08,3                                               0132 00346000
         ST    TEXTPTR,STAKPTR-8(@08)                              0132 00347000
*       STAKCNT(INDEX)=TEXTCNT;     /* SAVE TEXT COUNT IN STACK      */ 00348000
         ST    TEXTCNT,STAKCNT-8(@08)                              0133 00349000
*       INDEX=INDEX+1;              /* INCREMENT STACK INDEX         */ 00350000
         ALR   INDEX,@10                                           0134 00351000
*       IF TBLFCNT=2 THEN                                          0135 00352000
         C     TBLFCNT,@CF00090                                    0135 00353000
         BNE   @RF00135                                            0135 00354000
*         MSGPTIND=MSGTAB1;                                        0136 00355000
         MVC   MSGPTIND(1),MSGTAB1(MSGPTR)                         0136 00356000
*       ELSE                                                       0137 00357000
*         MSGPTIND=MSGTAB2;         /* TBLFCNT DICTATES CORRECT    0137 00358000
*                                      MSGTAB FIELD                  */ 00359000
         B     @RC00135                                            0137 00360000
@RF00135 MVC   MSGPTIND(1),MSGTAB2(MSGPTR)                         0137 00361000
*       CALL FIND;                  /* GO FIND STRING TO SUBSTITUTE  */ 00362000
*                                                                  0138 00363000
@RC00135 BAL   @14,FIND                                            0138 00364000
*       /*************************************************************/ 00365000
*       /*                                                           */ 00366000
*       /* DETERMINE IF APPROPIATE ENTRY IN MSGTAB DID CONTAIN A     */ 00367000
*       /* VALID MESSAGE FRAGMENT CODE.                              */ 00368000
*       /*                                                           */ 00369000
*       /*************************************************************/ 00370000
*                                                                  0139 00371000
*       IF FOUNDRC=FND THEN                                        0139 00372000
         LTR   FOUNDRC,FOUNDRC                                     0139 00373000
         BNZ   @RF00139                                            0139 00374000
*         DO;                       /* VALID CODE ?                  */ 00375000
*           INDEX=INDEX+1;          /* YES,INSURE INDEX WILL BE    0141 00376000
*                                      DECREMENTED CORRECTLY         */ 00377000
         AL    INDEX,@CF00078                                      0141 00378000
*           TEXTCNT=0;              /* FORCE COUNT TO ZERO,PROPAGATE    00379000
*                                      INDEX DECREM NT AND RE-LOOP   */ 00380000
         SLR   TEXTCNT,TEXTCNT                                     0142 00381000
*         END;                                                     0143 00382000
*       ELSE                                                       0144 00383000
*         DO;                       /* MESSAGE FRAGMENT NUMBER     0144 00384000
*                                      INVALID,MUST MOVE             */ 00385000
*                                                                  0144 00386000
         B     @RC00139                                            0144 00387000
@RF00139 DS    0H                                                  0145 00388000
*           /*********************************************************/ 00389000
*           /*                                                       */ 00390000
*           /* 'T' CHARACTER AND RETURN TO MAINLINE PROCESSING       */ 00391000
*           /*                                                       */ 00392000
*           /*********************************************************/ 00393000
*                                                                  0145 00394000
*           OUTBUFF(OUTCOUNT+1)='T';/* MOVE 'T' THAT BROUGHT US HERE */ 00395000
         LA    @10,OUTBUFF(OUTCOUNT)                               0145 00396000
         MVI   0(@10),C'T'                                         0145 00397000
*           OUTCOUNT=OUTCOUNT+2;    /* ADJUST OUTCOUNT FOR BOTH '%'     00398000
*                                      AND 'T'                       */ 00399000
         AL    OUTCOUNT,@CF00090                                   0146 00400000
*           INDEX=INDEX-1;          /* RESTORE INDEX TO REFLECT VALUE   00401000
*                                      AT ENTRY TO THIS SUBROUTINE   */ 00402000
         BCTR  INDEX,0                                             0147 00403000
*         END;                                                     0148 00404000
*     END;                                                         0149 00405000
*   ELSE                                                           0150 00406000
*     OUTCOUNT=OUTCOUNT+1;          /* ADJUST COUNT SO 'T' IS MOVED  */ 00407000
         B     @RC00128                                            0150 00408000
@RF00128 AL    OUTCOUNT,@CF00078                                   0150 00409000
*   RETURN;                         /* RETURN TO MAINLINE            */ 00410000
@EL00003 DS    0H                                                  0151 00411000
@EF00003 DS    0H                                                  0151 00412000
@ER00003 LM    @14,@01,@SA00003                                    0151 00413000
         LM    @05,@06,@SA00003+16                                 0151 00414000
         L     @08,@SA00003+24                                     0151 00415000
         LM    @10,@12,@SA00003+28                                 0151 00416000
         BR    @14                                                 0151 00417000
*   END REPLACE;                                                   0152 00418000
         B     @EL00003                                            0152 00419000
*CHARCONV:                                                         0153 00420000
*   PROC;                           /* PROCEDURE TO HANDLE 'C' CHAR.    00421000
*                                      REPLACE STRINGS               */ 00422000
CHARCONV STM   @14,@01,12(@13)                                     0153 00423000
         STM   @05,@06,40(@13)                                     0153 00424000
         STM   @09,@12,56(@13)                                     0153 00425000
*   VARFCNT=VARFCNT+4;              /* ADJUST VARFCNT TO GET CORRECT    00426000
*                                      INDEX                         */ 00427000
         AL    VARFCNT,@CF00070                                    0154 00428000
*   IF VARFCNT<5                                                   0155 00429000
*                                                                  0155 00430000
*   /*****************************************************************/ 00431000
*   /*                                                               */ 00432000
*   /* IF VARFCNT IS LESS THAN FIVE THAN THIS IS A VALID CHARACTER   */ 00433000
*   /* SUBSTITUTION.THE INDICES WILL BE UPDATED AND THE 8 CHARACTERS */ 00434000
*   /* MOVED.IF THE VARFCNT(VARIABLE FIELD COUNT) IS 5 OR MORE THAN  */ 00435000
*   /* THE CHARACTER SUBSTUTION IS INVALID NAD THE 'C' MUST BE MOVED */ 00436000
*   /* LIKE ANY OTHER INPUT CHARACTER                                */ 00437000
*   /*                                                               */ 00438000
*   /*****************************************************************/ 00439000
*                                                                  0155 00440000
*     THEN                                                         0155 00441000
         C     VARFCNT,@CF00747                                    0155 00442000
         BNL   @RF00155                                            0155 00443000
*     DO;                                                          0156 00444000
*       STAKPTR(INDEX)=TEXTPTR+1;   /* SAVE TEXTPTR (ALLOW FOR 'C')  */ 00445000
         LR    @10,INDEX                                           0157 00446000
         SLA   @10,3                                               0157 00447000
         LA    @09,1                                               0157 00448000
         LR    @06,TEXTPTR                                         0157 00449000
         ALR   @06,@09                                             0157 00450000
         ST    @06,STAKPTR-8(@10)                                  0157 00451000
*       STAKCNT(INDEX)=TEXTCNT-1;   /* SAVE COUNT (ALLOW FOR 'C')    */ 00452000
         LR    @06,TEXTCNT                                         0158 00453000
         BCTR  @06,0                                               0158 00454000
         ST    @06,STAKCNT-8(@10)                                  0158 00455000
*       INDEX=INDEX+1;              /* INCREMENT STACK INDEX         */ 00456000
         ALR   INDEX,@09                                           0159 00457000
*       TEXTPTR=ADDR(MSGCHAR);      /* ADDRESS OF DATA TO MOVE       */ 00458000
         LA    TEXTPTR,MSGCHAR(,MSGPTR)                            0160 00459000
*       TEXTCNT=LENGTH(MSGCHAR);    /* THE COUNT TO BE MOVED         */ 00460000
         LA    TEXTCNT,8                                           0161 00461000
*     END;                                                         0162 00462000
*   ELSE                                                           0163 00463000
*     OUTCOUNT=OUTCOUNT+1;          /* ADJUST COUNT SO THAT 'C' IS 0163 00464000
*                                      MOVED                         */ 00465000
         B     @RC00155                                            0163 00466000
@RF00155 AL    OUTCOUNT,@CF00078                                   0163 00467000
*   RETURN;                         /* RETURN TO THE MAINLINE        */ 00468000
@EL00004 DS    0H                                                  0164 00469000
@EF00004 DS    0H                                                  0164 00470000
@ER00004 LM    @14,@01,12(@13)                                     0164 00471000
         LM    @05,@06,40(@13)                                     0164 00472000
         LM    @09,@12,56(@13)                                     0164 00473000
         BR    @14                                                 0164 00474000
*   END CHARCONV;                                                  0165 00475000
         B     @EL00004                                            0165 00476000
*CONVERT:                                                          0166 00477000
*   PROC;                           /* PROCEDURE TO HANDLE 'X' AND '    00478000
*                                      D' NUMERICAL MSG. SUBST.      */ 00479000
CONVERT  STM   @14,@02,12(@13)                                     0166 00480000
         STM   @05,@06,40(@13)                                     0166 00481000
         STM   @09,@12,56(@13)                                     0166 00482000
*   VARFCNT=VARFCNT+4;              /* INCREMENT USE OF VARIABLE   0167 00483000
*                                      FIELD                         */ 00484000
         AL    VARFCNT,@CF00070                                    0167 00485000
*   IF VARFCNT>8 THEN               /* ONLY TWO USAGES OF VAR. FIELD    00486000
*                                      ARE PERMITT ED                */ 00487000
         C     VARFCNT,@CF00076                                    0168 00488000
         BH    @RT00168                                            0168 00489000
*     RETURN;                       /* THIS IS AN INVALID USE OF THE    00490000
*                                      X OR D FUNCTION               */ 00491000
*   ELSE                                                           0170 00492000
*     ;                             /* NULL ELSE, NOW PROCESS THE X     00493000
*                                      OR D CHARACTER                */ 00494000
*   IF INCHAR='X'                   /* IS THIS HEX                   */ 00495000
*     THEN                                                         0171 00496000
         CLI   INCHAR(TEXTPTR),C'X'                                0171 00497000
         BNE   @RF00171                                            0171 00498000
*     DO;                           /* YES, CONVERT MSG. VARIABLE TO    00499000
*                                      PRINTABLE HEX                 */ 00500000
*       UNPK(DECPACK,MSGOVER);      /* UNPACK HEX DATA               */ 00501000
         LR    @10,MSGPTR                                          0173 00502000
         ALR   @10,VARFCNT                                         0173 00503000
         UNPK  DECPACK(9),MSGOVER(5,@10)                           0173 00504000
*       TR(DECPACK,TAB);            /* TRANSLATE DATA FOR PRINTING   */ 00505000
         LA    @10,TABLE                                           0174 00506000
         AL    @10,@CF01203                                        0174 00507000
         TR    DECPACK(9),TAB(@10)                                 0174 00508000
*       LMAX=7;                     /* ALLOW FOR 7 LEADING ZEROES TO    00509000
*                                      BE DELETED                    */ 00510000
         LA    LMAX,7                                              0175 00511000
*     END;                                                         0176 00512000
*   ELSE                                                           0177 00513000
*     DO;                           /* NO, MSG VARIABLE MUST BE    0177 00514000
*                                      CONVERTED TO PRINT. DECIMAL   */ 00515000
         B     @RC00171                                            0177 00516000
@RF00171 DS    0H                                                  0178 00517000
*       R1=MSGOVER(1:4);            /* GET ADDR OF CORRECT MSG.    0178 00518000
*                                      VARIABLE                      */ 00519000
         LR    @10,MSGPTR                                          0178 00520000
         ALR   @10,VARFCNT                                         0178 00521000
         ICM   R1,15,MSGOVER(@10)                                  0178 00522000
*       CVD(R1,UNPKOUT);            /* CONVERT TO DECIMAL            */ 00523000
         CVD   R1,UNPKOUT                                          0179 00524000
*       UNPK(OUTPUT,UNPKOUT);       /* MAKE IT PRINTABLE             */ 00525000
         UNPK  OUTPUT(16),UNPKOUT(8)                               0180 00526000
*       LOWORDER=LOWORDER|SIGNZONE; /* CORRECT FOR UNPK ZONE         */ 00527000
         OI    LOWORDER,B'11110000'                                0181 00528000
*       LMAX=15;                    /* ALLOW FOR 17 LEADING ZEROS TO    00529000
*                                      BE SUPRESSED                  */ 00530000
         LA    LMAX,15                                             0182 00531000
*     END;                                                         0183 00532000
*                                                                  0183 00533000
*   /*****************************************************************/ 00534000
*   /*                                                               */ 00535000
*   /* CALCULATE THE NUMBER OF LEADING ZEROS TO BE SUPRESSED         */ 00536000
*   /*                                                               */ 00537000
*   /*****************************************************************/ 00538000
*                                                                  0184 00539000
*   DO N=1 BY 1 TO LMAX WHILE OUTPUT(N)='0';                       0184 00540000
@RC00171 LA    N,1                                                 0184 00541000
         B     @DE00184                                            0184 00542000
@DL00184 LA    @10,OUTPUT-1(N)                                     0184 00543000
         CLI   0(@10),C'0'                                         0184 00544000
         BNE   @DC00184                                            0184 00545000
*   END;                                                           0185 00546000
         AL    N,@CF00078                                          0185 00547000
@DE00184 CR    N,LMAX                                              0185 00548000
         BNH   @DL00184                                            0185 00549000
@DC00184 DS    0H                                                  0186 00550000
*   N=N-1;                          /* GET ACTUAL NUMBER OF LEADING     00551000
*                                      ZEROS                         */ 00552000
*                                                                  0186 00553000
         BCTR  N,0                                                 0186 00554000
*   /*****************************************************************/ 00555000
*   /*                                                               */ 00556000
*   /* NOW MOVE THE DATA FROM THE WORK AREA TO THE WTO OUTPUT        */ 00557000
*   /*                                                               */ 00558000
*   /*****************************************************************/ 00559000
*                                                                  0187 00560000
*   OUTBUFF(OUTCOUNT:OUTCOUNT+LMAX-N)=OUTPUT(N+1:LMAX+1);          0187 00561000
         LA    @10,OUTBUFF-1(OUTCOUNT)                             0187 00562000
         LR    @09,OUTCOUNT                                        0187 00563000
         ALR   @09,LMAX                                            0187 00564000
         SLR   @09,N                                               0187 00565000
         LR    @15,@09                                             0187 00566000
         SLR   @15,OUTCOUNT                                        0187 00567000
         LA    @01,OUTPUT(N)                                       0187 00568000
         EX    @15,@SM01204                                        0187 00569000
*   OUTCOUNT=OUTCOUNT+LMAX-N+1;     /* ADJUST OUTBUFF COUNT          */ 00570000
         LA    @10,1                                               0188 00571000
         ALR   @09,@10                                             0188 00572000
         LR    OUTCOUNT,@09                                        0188 00573000
*   TEXTPTR=TEXTPTR+1;              /* ALLOW FOR 'X' OR 'D'          */ 00574000
         ALR   TEXTPTR,@10                                         0189 00575000
*   TEXTCNT=TEXTCNT-2;              /* ALLOW FOR BOTH % AND X OR D   */ 00576000
         BCTR  TEXTCNT,0                                           0190 00577000
         BCTR  TEXTCNT,0                                           0190 00578000
*   RETURN;                         /* RETURN TO THE MAINLINE        */ 00579000
@EL00005 DS    0H                                                  0191 00580000
@EF00005 DS    0H                                                  0191 00581000
@ER00005 LM    @14,@02,12(@13)                                     0191 00582000
         LM    @05,@06,40(@13)                                     0191 00583000
         LM    @09,@12,56(@13)                                     0191 00584000
         BR    @14                                                 0191 00585000
*   END CONVERT;                                                   0192 00586000
         B     @EL00005                                            0192 00587000
*WTORTN:                                                           0193 00588000
*   PROC;                           /* PROCEDURE TO ACCOMPLISH ACTUAL   00589000
*                                      WRITE TO OPERATOR             */ 00590000
WTORTN   STM   @14,@12,@SA00006                                    0193 00591000
*   WTOLNG=OUTCOUNT-1+4;            /* LENGTH IS SIZE OF MESSAGE + 4    00592000
*                                      BYTE HEADER           @YM02558*/ 00593000
         LA    @10,3(,OUTCOUNT)                                    0194 00594000
         STCM  @10,3,WTOLNG                                        0194 00595000
*   WTOFLG=GENMCS;                  /* GET MCS FLAGS FROM LIST FORM     00596000
*                                      OF WTO                        */ 00597000
         MVC   WTOFLG(2),GENMCS                                    0195 00598000
*   IF MSGNUM='58'X THEN                                           0196 00599000
         CLI   MSGNUM(MSGPTR),X'58'                                0196 00600000
         BNE   @RF00196                                            0196 00601000
*     WTOMSG(OUTCOUNT+4:OUTCOUNT+5)=ACRDESC;/* ACR DESC CODE @ZM45609*/ 00602000
         LA    @10,WTOMSG+3(OUTCOUNT)                              0197 00603000
         MVC   0(2,@10),ACRDESC                                    0197 00604000
*   ELSE                                                           0198 00605000
*     WTOMSG(OUTCOUNT+4:OUTCOUNT+5)=GENDESC;/* GET DESCRIPTOR CODES     00606000
*                                      FROM LIST FORM OF WTO @YM02558*/ 00607000
