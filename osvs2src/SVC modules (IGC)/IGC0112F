*ACQBIO:                                                           0134 00239000
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);/*                  @ZA27588*/ 00240000
*                                                                  0134 00241000
ACQBIO   DS    0H                                                  0135 00242000
*   /*****************************************************************/ 00243000
*   /*                                                               */ 00244000
*   /* THIS PROC PROCESSES ACQUIRES BEFORE THEY ARE SENT     @ZA27588*/ 00245000
*   /* TO THE MSC. IT ENQS ON THE VOLID, SEARCHES THE        @ZA27588*/ 00246000
*   /* VOLUME LIST FOR A MATCHING ENTRY. IF NO ENTRY IS      @ZA27588*/ 00247000
*   /* FOUND, IT CALLS FINDSLOT TO SEE IF ANY NEW SLOTS      @ZA27588*/ 00248000
*   /* ARE AVAILABLE. IF ONE IS THE PROCESS STARTS OVER.     @ZA27588*/ 00249000
*   /* IF ONE IS NOT AVAILABLE, PROCESSING TERMINATES WITH   @ZA27588*/ 00250000
*   /* RETURN CODE OF ZERO. IF A MATCHING SLOT IS FOUND      @ZA27588*/ 00251000
*   /* BITS ARE TURNED ON IN THE LIST MATCHING THE CYLS      @ZA27588*/ 00252000
*   /* IN THE EXTENTS. IF ANY OF THE BITS CHANGE STATE, THE  @ZA27588*/ 00253000
*   /* RETURN CODE IS SET TO ZERO TO INDICATE THAT THE       @ZA27588*/ 00254000
*   /* ACQUIRE MUST BE SENT TO THE MSC. IF NONE CHANGE       @ZA27588*/ 00255000
*   /* THEN THE RETURN CODE IS SET TO FOUR. THE EXCEPTION    @ZA27588*/ 00256000
*   /* TO THIS IS ACQUIRE WITH BIND OR DELAYED RESPONSE      @ZA27588*/ 00257000
*   /* WHERE THE RETURN CODE IS ALWAYS SET TO ZERO.          @ZA27588*/ 00258000
*   /* AT THE END OF PROCESSING, THE VOLID IS DEQUEUED.      @ZA27588*/ 00259000
*   /*                                                               */ 00260000
*   /*****************************************************************/ 00261000
*                                                                  0135 00262000
*   RFY                                                            0135 00263000
*     R14 RSTD;                     /*                       @ZA27588*/ 00264000
*   ACQRET=R14;                     /* SAVE RETURN ADDRESS   @ZA27588*/ 00265000
         LR    ACQRET,R14                                          0136 00266000
*   RFY                                                            0137 00267000
*     R14 UNRSTD;                   /*                       @ZA27588*/ 00268000
*   ORDVOLID=ARVOL;                 /* SAVE VOLID FROM ORDER @ZA27588*/ 00269000
         MVC   ORDVOLID(6),ARVOL(ORDREG)                           0138 00270000
*   ENQVOLID=ORDVOLID;              /* SAVE VOLID FOR SVC126 ESTAE   */ 00271000
*                                                                  0139 00272000
         L     @08,@PC00001+12                                     0139 00273000
         MVC   ENQVOLID(6,@08),ORDVOLID                            0139 00274000
*   /*****************************************************************/ 00275000
*   /*                                                               */ 00276000
*   /* ENQ ON VOLID                                          @ZA27588*/ 00277000
*   /*                                                               */ 00278000
*   /*****************************************************************/ 00279000
*                                                                  0140 00280000
*   RFY                                                            0140 00281000
*    (R0,                                                          0140 00282000
*     R1,                                                          0140 00283000
*     R14,                                                         0140 00284000
*     R15) RSTD;                    /*                       @ZA27588*/ 00285000
*   GEN REFS(QNAME,ORDVOLID,ENQDEQ) SETS(ENQDEQ);                  0141 00286000
*                                                         /*@ZA27588*/  00287000
        B   EQDQEND                                           @ZA27588  00288000
ENQLIST ENQ (,,E,6,SYSTEM),MF=L                               @ZA27588  00289000
ENQEND  EQU *                                                 @ZA27588  00290000
ENQL    EQU ENQEND-ENQLIST                                    @ZA27588  00291000
DEQLIST DEQ (,,6,SYSTEM),MF=L                                 @ZA27588  00292000
DEQEND  EQU *                                                 @ZA27588  00293000
DEQL    EQU DEQEND-DEQLIST                                    @ZA27588  00294000
EQDQEND DS  0H                                                @ZA27588  00295000
        MVC ENQDEQ(ENQL),ENQLIST                              @ZA27588  00296000
        ENQ (QNAME,ORDVOLID),MF=(E,ENQDEQ)                    @ZA27588  00297000
*   RFY                                                            0142 00298000
*    (R0,                                                          0142 00299000
*     R1,                                                          0142 00300000
*     R14,                                                         0142 00301000
*     R15) UNRSTD;                  /*                       @ZA27588*/ 00302000
*   ENQSW=ON;                       /* INDICATE ENQUEUED     @ZA27588*/ 00303000
         L     @08,@PC00001+12                                     0143 00304000
         OI    ENQSW(@08),B'10000000'                              0143 00305000
*ACQBIO01:                          /* RESTART POINT         @ZA27588*/ 00306000
*   SLOTPTR=ADDR(ICBVLENT(1));      /* INIT VOLUME LIST PTR  @ZA27588*/ 00307000
ACQBIO01 L     @08,@PC00001                                        0144 00308000
         L     @08,ICBPTR(,@08)                                    0144 00309000
         L     SLOTPTR,ICBVOLST(,@08)                              0144 00310000
         LA    SLOTPTR,ICBVLENT(,SLOTPTR)                          0144 00311000
*   LOOPSW=OFF;                     /* TURN OFF LOOP CONTROLLER      */ 00312000
*                                                                  0145 00313000
         NI    LOOPSW,B'10111111'                                  0145 00314000
*   /*****************************************************************/ 00315000
*   /*                                                               */ 00316000
*   /* SEARCH THROUGH VOLUME LIST FOR MATCHING VOLID.        @ZA27588*/ 00317000
*   /* IF MATCH FOUND THEN LOOP SWITCH WILL BE TURNED        @ZA27588*/ 00318000
*   /* ON TO TERMINATE LOOP. IF MATCH NOT FOUND THEN         @ZA27588*/ 00319000
*   /* AN ATTEMPT WILL BE MADE TO FIND AN EMPTY              @ZA27588*/ 00320000
*   /* SLOT TO USE AND REPROCESS THE ACQUIRE.                @ZA27588*/ 00321000
*   /*                                                               */ 00322000
*   /*****************************************************************/ 00323000
*                                                                  0146 00324000
*   DO VOLIMIT=ONE TO ICBVLCNT WHILE LOOPSW=OFF;/*           @ZA27588*/ 00325000
         LA    VOLIMIT,1                                           0146 00326000
         B     @DE00146                                            0146 00327000
@DL00146 TM    LOOPSW,B'01000000'                                  0146 00328000
         BNZ   @DC00146                                            0146 00329000
*     IF SLTVOLID^=ORDVOLID THEN    /* IS THIS A MATCH       @ZA27588*/ 00330000
         CLC   SLTVOLID(6,SLOTPTR),ORDVOLID                        0147 00331000
         BE    @RF00147                                            0147 00332000
*       SLOTPTR=SLOTPTR+LENGTH(VOLENTRY);/* NO, BUMP POINTER         */ 00333000
         AL    SLOTPTR,@CF00839                                    0148 00334000
*     ELSE                          /* YES, PROCESS ACQUIRE  @ZA27588*/ 00335000
*       DO;                         /*                       @ZA27588*/ 00336000
         B     @RC00147                                            0149 00337000
@RF00147 DS    0H                                                  0150 00338000
*         BYPASSSW=ON;              /* SET TO BYPASS IO INITIALLY    */ 00339000
*                                                                  0150 00340000
         OI    BYPASSSW,B'10000000'                                0150 00341000
*         /***********************************************************/ 00342000
*         /*                                                         */ 00343000
*         /* LOOP THROUGH AND PROCESS EACH EXTENT            @ZA27588*/ 00344000
*         /*                                                         */ 00345000
*         /***********************************************************/ 00346000
*                                                                  0151 00347000
*         DO EXTINDEX=ONE TO ARNMEXT;/*                      @ZA27588*/ 00348000
         LA    EXTINDEX,1                                          0151 00349000
         B     @DE00151                                            0151 00350000
@DL00151 DS    0H                                                  0152 00351000
*           EXTENT=AREXT(EXTINDEX); /* SAVE COPY OF EXTENT           */ 00352000
         LR    @07,EXTINDEX                                        0152 00353000
         SLA   @07,2                                               0152 00354000
         ALR   @07,ORDREG                                          0152 00355000
         MVC   EXTENT(4),AREXT-4(@07)                              0152 00356000
*           IF BEGCYL<=ENDCYL&ENDCYL<=MAXCYLS THEN/* ARE CYLINDERS 0153 00357000
*                                      VALID                         */ 00358000
         CLC   BEGCYL(2),ENDCYL                                    0153 00359000
         BH    @RF00153                                            0153 00360000
         LH    @07,ENDCYL                                          0153 00361000
         N     @07,@CF00993                                        0153 00362000
         C     @07,@CF00936                                        0153 00363000
         BH    @RF00153                                            0153 00364000
*             DO;                   /* YES                   @ZA27588*/ 00365000
*                                                                  0154 00366000
*               /*****************************************************/ 00367000
*               /*                                                   */ 00368000
*               /* CALCULATE NUMBER OF CYLINDERS IN EXTENT, STARTING */ 00369000
*               /* BYTE POINTER AND INITIAL MASK FOR ORING   @ZA27588*/ 00370000
*               /*                                                   */ 00371000
*               /*****************************************************/ 00372000
*                                                                  0155 00373000
*               CYLCOUNT=ENDCYL-BEGCYL+ONE;/*                @ZA27588*/ 00374000
         LA    @15,1                                               0155 00375000
         LH    @14,ENDCYL                                          0155 00376000
         N     @14,@CF00993                                        0155 00377000
         LH    @01,BEGCYL                                          0155 00378000
         N     @01,@CF00993                                        0155 00379000
         SLR   @14,@01                                             0155 00380000
         ALR   @14,@15                                             0155 00381000
         LR    CYLCOUNT,@14                                        0155 00382000
*               BYTESS=(BEGCYL/EIGHT)+ONE;/*                 @ZA27588*/ 00383000
         LR    BYTESS,@01                                          0156 00384000
         SRL   BYTESS,3                                            0156 00385000
         ALR   BYTESS,@15                                          0156 00386000
*               WORKBYTE=(BEGCYL//EIGHT)+ONE;/*              @ZA27588*/ 00387000
         LR    @00,@01                                             0157 00388000
         SRA   @00,31                                              0157 00389000
         D     @00,@CF00048                                        0157 00390000
         ALR   @00,@15                                             0157 00391000
         LR    WORKBYTE,@00                                        0157 00392000
*               MASK=ACQTABLE(WORKBYTE);/*                   @ZA27588*/ 00393000
         LA    @01,ACQTABLE-1(WORKBYTE)                            0158 00394000
         MVC   MASK(1),0(@01)                                      0158 00395000
*               CYLHOLD=CYLBITS;    /* SAVE COPY OF BITS     @ZA27588*/ 00396000
*                                                                  0159 00397000
         MVC   CYLHOLD(54),CYLBITS(SLOTPTR)                        0159 00398000
*               /*****************************************************/ 00399000
*               /*                                                   */ 00400000
*               /* LOOP THROUGH CYLINDERS IN EXTENT SETTING EACH     */ 00401000
*               /*                                           @ZA27588*/ 00402000
*               /* MATCHING BIT ON.                          @ZA27588*/ 00403000
*               /*                                                   */ 00404000
*               /*****************************************************/ 00405000
*                                                                  0160 00406000
*               DO I=ONE TO CYLCOUNT;/*                      @ZA27588*/ 00407000
         LR    I,@15                                               0160 00408000
         B     @DE00160                                            0160 00409000
@DL00160 DS    0H                                                  0161 00410000
*                 CYLBITS(BYTESS:BYTESS)=CYLBITS(BYTESS:BYTESS)|MASK;   00411000
         LR    @01,SLOTPTR                                         0161 00412000
         ALR   @01,BYTESS                                          0161 00413000
         OC    CYLBITS-1(1,@01),MASK                               0161 00414000
*                                   /* OR IN BIT                     */ 00415000
*                 SRL(MASK,ONE);    /* SHIFT MASK            @ZA27588*/ 00416000
         SLR   @15,@15                                             0162 00417000
         IC    @15,MASK                                            0162 00418000
         SRL   @15,1                                               0162 00419000
         STC   @15,MASK                                            0162 00420000
*                 IF MASK=ZERO THEN /* IS BYTE BOUNDARY CROSSED      */ 00421000
         CLI   MASK,0                                              0163 00422000
         BNE   @RF00163                                            0163 00423000
*                   DO;             /* YES                   @ZA27588*/ 00424000
*                     MASK=ACQMRSET;/* SET MASK FOR NEXT BYTE        */ 00425000
         MVC   MASK(1),ACQMRSET                                    0165 00426000
*                     BYTESS=BYTESS+ONE;/* BUMP BYTE POINTER         */ 00427000
         AL    BYTESS,@CF00050                                     0166 00428000
*                   END;            /*                       @ZA27588*/ 00429000
*               END;                /*                       @ZA27588*/ 00430000
@RF00163 AL    I,@CF00050                                          0168 00431000
@DE00160 CR    I,CYLCOUNT                                          0168 00432000
         BNH   @DL00160                                            0168 00433000
