*   OCLINK=OPCWAITN;                /* PUT OPCE ON FID0 WAIT CHAIN   */ 00901000
         L     @05,OPCWAITN(,ROPCAVT)                              0673 00902000
         ST    @05,OCLINK(,ROPCE)                                  0673 00903000
*   OPCWAITN=ROPCE;                 /*                               */ 00904000
