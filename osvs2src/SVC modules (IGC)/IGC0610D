*       RETURN;                     /* RETURN TO IEDQCA              */ 00790000
@EL00001 DS    0H                                               02S0563 00791000
@EF00001 DS    0H                                               02S0563 00792000
@ER00001 BR    @14                                              02S0563 00793000
*     END;                                                      02S0564 00794000
*   ELSE                            /* RETURN STATE NOT SET          */ 00795000
*     DO;                                                       02S0565 00796000
@RF00559 DS    0H                                               02S0566 00797000
*       IF EXITSET=OFF              /* TEST FOR EXIT PARAMETERS SET  */ 00798000
*         THEN                      /* EXIT PARAMETERS ARE NOT SET   */ 00799000
         TM    EXITSET,B'01000000'                              02S0566 00800000
         BNZ   @RF00566                                         02S0566 00801000
*         DO;                                                   02S0567 00802000
*           OPCMODID=OCWTG;         /* SET NEXT MODULE TO GET CONTROL*/ 00803000
