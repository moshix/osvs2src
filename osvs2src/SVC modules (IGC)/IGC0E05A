         DROP  R2                                                       62840000
TESTAIB  LTR   R2,R2               DOES AN AIB EXIST                    62860000
         BZR   R15                 IF NOT RETURN                        62880000
         USING IEDQAIB,R2                                               62900000
         TM    AIBSTATE,AIBBUSY    HAS THE AIB BEEN ALLOCATED           62920000
         BZ    NEXTAIB             NO, GO GET NEXT ADDRESS              62940000
         CLI   AIBKEY,AIBAPKEY     IS IT AN OPCTL AIB                   62960000
         BE    NEXTAIB             YES, GO GET NEXT AIB                 62980000
         CLC   AIBECBA(L'OPN),OPN  IF OPEN AIB                          63000000
         BE    NEXTAIB             BRANCH                               63020000
         CLC   AIBECBA(L'CLOS),CLOS IF CLOSE AIB                        63040000
         BE    NEXTAIB             BRANCH                               63060000
         CLC   AIBECBA(L'QOT1),QOT1 IF ABEND AIB                        63080000
         BE    NEXTAIB             BRANCH                               63100000
         CLI   AIBKEY,X'60'        IS IT A CKREQ AIB                    63120000
         BNE   NOTCKREQ            NO, BRANCH                           63140000
         CLC   ACRV11,AIBCPEWA     COMPARE PEWA ADDRESSES               63160000
         BE    ISAIB               IF EQUAL, BRANCH                     63180000
         B     NEXTAIB             GO GET NEXT AIB                      63200000
NOTCKREQ CLC   ACRV11,AIBGPEWA     COMPARE PEWA ADDRESSES               63220000
         BE    ISAIB               IF EQUAL, BRANCH                     63240000
