         TITLE 'IGC0E05A  TCAM ABEND FORMAT                     MACROS' 05990000
         MACRO                                                          06040000
&ACR     ACR   &Y,&X,&OFFSET,&CALL=,&LIST=                              06090000
         AIF   (T'&CALL EQ 'O').ACR10                                   06140000
&ACR     DC    AL1(ACRRTNF)        INDICATE ROUTINE CALL                06190000
         DC    AL3(&CALL)          POINTER TO ROUTINE                   06240000
         MEXIT                                                          06290000
.ACR10   AIF   (T'&LIST EQ 'O').ACR20                                   06340000
&ACR     DC    AL1(ACRLISTF)       INDICATE LIST CHAIN                  06390000
         DC    AL3(&LIST)          POINTER TO LIST                      06440000
         MEXIT                                                          06490000
.ACR20   AIF   (&Y LT 32).ACR30                                         06540000
         MNOTE 8,'Y VALUE OF &Y EXCEEDS 31'                             06590000
.ACR30   AIF   (&X LT 32).ACR40                                         06640000
         MNOTE 8,'X VALUE OF &X EXCEEDS 31'                             06690000
.ACR40   ANOP                                                           06740000
&ACR     DC    AL1(&Y*4)           Y                                    06790000
         DC    AL1(&X*4)           X                                    06840000
         DC    AL2(&OFFSET)        DISPLACEMENT                         06890000
         MEND                                                           06940000
         SPACE 8                                                        06990000
         MACRO                                                          07040000
&PRNT    PRNT                                                           07090000
