         MEND                                                           07190000
         SPACE 6                                                        07240000
         MACRO                                                          07290000
&OPTYP   OPTYP &X,&C                                                    07340000
&OPTYP   DC    XL1'&X'             OPTION ARGUMENT                      07390000
         DC    CL1'&C'             CHARACTER                            07440000
         MEND                                                           07490000
         EJECT                                                          07540000
         MACRO                                                          07590000
&PDR     PDR   &N,&M,&I,&PRT=PRTASA,&FLG=0,&CHN=0,&RTN=,&OFF=           07640000
         LCLA  &LEN                                                     07690000
&LEN     SETA  L'&PRT-1                                                 07740000
&PDR     DC    A(&M)               MASK                                 07790000
         AIF   (T'&RTN EQ 'O').PDR10                                    07840000
         DC    A(&RTN)             OFFSET CALCULATION ROUTINE           07890000
         DC    AL1(&FLG+PDRRTN+&LEN) FLAGS                              07940000
         AGO   .PDR20                                                   07990000
.PDR10   DC    AL2(&OFF(1))        STARTING OFFSET                      08040000
         DC    AL2(&OFF(2))        ENDING OFFSET                        08090000
         DC    AL1(&FLG+&LEN)      FLAGS AND LENGTH - 1 FOR MOVE        08140000
.PDR20   DC    CL10'&N'            NAME OF CONTROL BLOCK                08190000
         DC    AL1(&I*4)           INDEX                                08240000
         DC    AL1(&PRT-PRTASA)    PRINT COLUMN IN FORMAT      @ZM56635 08290000
         DC    AL2(&CHN)           CHAIN LINK OFFSET                    08340000
.MEND    SPACE                                                          08390000
         MEND                                                           08440000
         SPACE 8                                                        08490000
         MACRO                                                          08540000
&TDR     TDR   &N,&T,&L,&MASKS=                                         08590000
&TDR     DC    A(&MASKS)           MASKS VALUE                          08640000
         DC    CL4'&N'             TYPE NAME                            08690000
         AIF   (K'&T EQ 2).TDRHEX                                       08740000
         DC    AL1(&T)             TYPE LOOKUP ARGUMENT                 08790000
         AGO   .TDRPTR                                                  08840000
.TDRHEX  DC    XL1'&T'             TYPE LOOKUP ARGUMENT                 08890000
.TDRPTR  DC    AL3(&L)             POINTER TO ACR LIST                  08940000
         SPACE                                                          08990000
         MEND                                                           09040000
         TITLE 'IGC0E05A  TCAM ABEND FORMAT                    EQUATES' 09090000
*              REGS 0,1,2,3,4,15 MAY BE USED BY ANY LEVEL WITHOUT       09140000
*              RESTORING THE CONTENTS ON RETURN FROM A CALLED LEVEL     09190000
*              UNLESS THEY WHERE A PARAMETER TO THAT LEVEL.             09240000
         SPACE 2                                                        09290000
R0       EQU   0                   WORK REG                             09340000
R1       EQU   1                   PARM, LIST, AND WORK REG             09390000
R2       EQU   2                   WORK REG FOR ALL LEVELS              09440000
R3       EQU   3                   WORK REG FOR ALL LEVELS              09490000
R4       EQU   4                   WORK REG FOR ALL LEVELS              09540000
R5       EQU   5                   PDR TABLE                            09590000
R6       EQU   6                   POINTER TO PRINT BUFFER              09640000
R7       EQU   7                   CURRENT OPTIONS MASK                 09690000
R8       EQU   8                   3RD LEVEL ROUTINE FOR 2ND LEVEL      09740000
R9       EQU   9                   POINTER TO TCX, TABLE ADDR  @XM05835 09790000
R10      EQU   10                  POINTER TO AVT                       09840000
R11      EQU   11                  POINTER TO ABEND PARM LIST           09890000
R12      EQU   12                  BASE REG                             09940000
R13      EQU   13                  SAVE AREA AND WORKAREA POINTER       09990000
R14      EQU   14                  RETURN                               10040000
R15      EQU   15                  CALL, WORK, AND RETURN CODE REG      10090000
         SPACE 2                                                        10140000
D0       EQU   0                   DISPLACEMENT OF ZERO                 10190000
D1       EQU   1                   DISPLACEMENT OF ONE                  10240000
D2       EQU   2                   DISPLACEMENT OF TWO                  10290000
D3       EQU   3                   DISPLACEMENT OF THREE                10340000
