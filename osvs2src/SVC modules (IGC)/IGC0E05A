PGMCHK   EQU   X'0C'                MASK FOR 0CX COMP. CODES   @OZ29004 14490182
         TITLE 'IGC0E05A  TCAM ABEND FORMAT                      SETUP' 14540000
IGC0E05A CSECT                                                          14590000
         USING IGC0E05A,R15                                             14640000
         SAVE  (14,12),,IGC0E05A-&SYSPARM                               14690000
         LR    R12,R15             SETUP BASE REG                       14740000
         LA    R8,2048(,R12)                                            14790000
         LA    R8,2048(,R8)         SETUP SECOND BASE REG               14840000
         USING IGC0E05A+4096,R8                                         14890000
         DROP  R15                                                      15180000
         USING IGC0E05A,R12                                             15185000
         LR    R11,R1              SAVE POINTER TO ABEND PARM LIST      15190000
         USING ABDPL,R11                                                15195000
*                                                                       15200000
*        VALIDATE THAT TCAM'S FORMATED DUMP SHOULD BE RUN               15205000
*                                                                       15210000
         TM    ADPLFLAG,ADPLSYTM   OS/VS1 SYSTEM                        15215000
         BO    EXIT1               YES - - EXIT                @ZM46635 15220000
         L     R4,ADPLTCB          ABENDING TCB                         15225000
         L     R5,ADPLCVT          CVT BASE                             15230000
         LH    R3,ADPLASID         ABENDING TASK ASID                   15235000
         LTR   R4,R4               MEMORY TERMINATION                   15240000
         BZ    EXIT1               YES - - EXIT                @ZM46635 15245000
         USING CVT,R5              CVT BASE                             15250000
         L     R5,CVTAQAVT         TCX ADDR                             15255000
         LA    R5,D0(,R5)          CLEAR HI ORDER BYTE                  15260000
         LTR   R5,R5               TCX BUILT                            15265000
         BZ    EXIT1               NO - - EXIT                 @ZM46635 15270000
         DROP  R5                                                       15275000
         USING IEDQTCX,R5          TCX BASE                             15280000
         LH    R2,TCXASID          MCP ASID                             15285000
         L     R5,TCXTCB           MCP TCB                              15290000
         CR    R2,R3               CORRECT MEMORY                       15295000
         BNE   EXIT1               NO - - EXIT                 @ZM46635 15300000
         USING IEDQTCB,R4          ABENDING TCB BASE                    15305000
         LR    R6,R4               SAVE ABENDING TCB ADDR               15310000
         LA    R4,D0(,R4)          CLEAR HI ORDER BYTE                  15315000
         LA    R5,D0(,R5)          CLEAR HI ORDER BYTE                  15320000
         CR    R4,R5               SAVE TCB                    @ZM46635 15325000
         BNE   EXIT1               BRANCH IF MCP NOT ABENDING           15330000
         L     R4,TCBJSTCB         JOBSTEP TCB                          15335000
         LA    R4,D0(,R4)          CLEAR HI ORDER BYTE                  15340000
         DROP  R4                                                       15345000
         USING IEDQTCB,R5          MCP TCB                              15350000
         L     R5,TCBJSTCB         JOB STEP TCB                         15355000
         LA    R5,D0(R5)           CLEAR HI ORDER BYTE                  15360000
         DROP  R5                                                       15365000
         CR    R4,R5               SAME JS TCB                          15370000
         BNE   EXIT1               NO - - RETURN               @ZM46635 15375000
         USING IEDQTCB,R4          ABENDING TCB                         15380000
         LR    R4,R6               RESTORE ABENDING TCB ADDR            15385000
         L     R5,TCBCMP           COMPLETION CODE                      15390000
         LA    R5,D0(,R5)          CLEAR FLAG                           15395000
         LTR   R5,R5               DID TASK ABEND                       15400000
         BZ    EXIT1               NO - - DO NOT FORMAT        @ZM46635 15405000
         L     R6,ADPLBUF          LOAD PRINT BUFFER ADDRESS            15410000
         BCTR  R6,0                BACK UP TO CARRIAGE COUNTROL         15415000
         USING PRTLINE,R6                                               15420000
         GETMAIN EC,LV=DYNAMS,A=ADPLDYNM,SP=253,MF=(E,ADPLWK1)          15660000
         LTR   R15,R15             TEST IF GETMAIN WORKED               15700000
         BP    ABORT1              NO ABORT                             15740000
         L     R15,ADPLDYNM        POINT TO DYNAMIC AREA                15780000
         USING DYNAM,R15                                                15840000
         ST    R13,SAVEAREA+D4     CHAIN CALLER'S SAVEAREA              15900000
         ST    R15,D8(R13)         BACK CHAIN MY SAVE AREA              15960000
         DROP  R15                                                      16020000
         LR    R13,R15             POINT TO MY SAVE AREA                16080000
         USING DYNAM,R13                                                16140000
         TITLE 'IGC0E05A  TCAM ABEND FORMAT                   MAINLINE' 16800000
         XC    ACRVECT(ACRVECTS),ACRVECT  CLEAR ALL ADDRESS CELLS       16840000
         MVC   ACRV1,ADPLCVT       INITIALIZE POINTER TO CVT   @XM05835 16880000
         LA    R9,ACRBASIC         POINT TO ADDRESS COMPUTATION LIST    16920000
*                                                              @XM05835 16980000
         BAL   R14,ACRTN           CALL ADDRESS COMPUTATION ROUTINE     17100000
*                                                              @XM05835 17130000
         LM    R9,R10,ACRV3        LOAD TCX AND AVT ADDRESSES           17160000
         USING IEDQTCX,R9                                               17200000
         USING IEDQAVTD,R10                                             17240000
         TM    TCXABEPL+D3,ADFRUN  HAS FORMAT ROUTINE ALREADY RUN ?     17280000
         BO    EXIT                YES                                  17320000
         OI    TCXABEPL+D3,ADFRUN  SET FIRST TIME SWITCH ON             17360000
         MVC   PARMTCX,TCXABEPL    MOVE TCX PARAMETERS         @XM05835 17400000
         DROP  R9                                              @XM05835 17440000
         LA    R0,TENLINES         SPACE TEN LINES                      17480000
         BAL   R14,LINSPACE        NUMBER OF LINES SO SPACE             17520000
         LA    R1,IEDQAVTD         POINT TO AVT                         17570000
         SH    R1,TWO              COMPUTE AVT LEVEL/MOD ADDRESS        17620000
         MVC   PRTTITLE,TITLE      MOVE TITLE TO PRINT                  17670000
         MVC   PRTTMOD,D1(R1)      MOVE TCAM MODIFICATION LEVEL TO PRT. 17720000
         IC    R1,D0(R1)           GET TCAM LEVEL                       17770000
         LA    R1,LVLCNVT(R1)      CONVERT A TO 0 THRU I TO 9           17820000
         STC   R1,PRTTLVL          STORE TCAM LEVEL IN TITLE            17870000
         PRNT                      PRINT TITLE LINE                     17920000
         MVI   PRTASA,SP3          FORCE NEXT LINE TO TRIPLE SPACE      17970000
         LA    R5,PDRBASIC         POINT TO BASIC HEX DUMP TABLE        18020000
         L     R7,PARMTCX          SET OPT MASK TO PARM LIST @XM05835   18070000
         BAL   R14,HEXDUMP         CALL HEX DUMP ROUTINE                18120000
         LA    R0,THREELIN         NUMBER OF LINES TO SPACE             18170000
         BAL   R14,LINSPACE        BRANCH TO SPACE LINES                18300000
         MVC   PRTHDR1,HEAD1       MOVE HEADER FOR PRINTING             18330000
         PRNT                      PRINT THE HEADER                     18360000
         MVC   ROUTADDR,ATBLPRNT   POINT TO OUTPUT ROUTINE              18390000
         BAL   R14,TNTLKUP         CALL TERMINAL NAME TABLE LOOKUP      18420000
         BAL   R14,MHNLKUP         CALL MH NAME TABLE LOOKUP            18470000
         L     R1,OPTIONS          LOAD OPTIONS MASK                    18520000
         N     R1,PARMTCX          AND AGAINST TCX PARAMETERS  @XM05835 18570000
         BZ    DONE                ALL OK, FINISHED                     18620000
         LA    R0,THREELIN         NUMBER OF LINES TO SPACE             18670000
         BAL   R14,LINSPACE        BRANCH TO SPACE LINES                18720000
         MVC   ROUTADDR,AOPTPRNT   POINT TO OUTPUT ROUTINE              18760000
         BAL   R14,TNTLKUP         CALL TERMINAL NAME TABLE LOOKUP      18810000
         BAL   R14,MHNLKUP         CALL MH NAME TABLE LOOKUP            18900000
DONE     LA    R0,TENLINES         NUMBER OF LINES TO SPACE             18940000
         BAL   R14,LINSPACE        BRANCH TO SPACE LINES                18980000
         SLR   R10,R10             CLEAR RETURN TO ZERO                 19020000
         TITLE 'IGC0E05A  TCAM ABEND FORMAT        EXIT AND ERROR RTNS' 19080000
EXIT     SLR   R15,R15             CLEAR SPIE EXIT POINTER              19140000
         BAL   R14,SPIESET         SET ABDUMPS EXIT TO ZERO             19200000
         LR    R1,R13              LOAD DYNAMIC AREA ADDR               19230000
         L     R13,D4(R13)         POINT TO CALLER'S SAVE AREA          19260000
         FREEMAIN R,LV=DYNAMS,A=(1),SP=253                              19280000
EXIT1    EQU   *                                                        19300000
         L     R14,D12(R13)        RESTORE CALLERS RETURN ADDRESS       19320000
         SLR   R15,R15             SET RETURN CODE REG WITH RETURN CODE 19380000
         LM    R0,R12,D20(R13)     RESTORE CALLERS REGS                 19440000
         BR    R14                 RETURN TO CALLER                     19500000
         SPACE 4                                                        19510000
ABORT2   EQU   *                                                        19520000
         STM   R14,R12,12(R13)       SAVE REGS                 @ZM46635 19523000
         L     R12,SDWAGR12-SDWA(R1) RELOAD BASE REG           @ZM46635 19526000
         SETRP RC=0,REGS=(14,12)    CONTINUE TERMINATION       @OZ29004 19530082
ABORT    L     R13,ADPLDYNM        GET DYNAMIC AREA POINTER             19540000
         B     EXIT                                                     19550000
