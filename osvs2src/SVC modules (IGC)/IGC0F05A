*                                      TIME CONVERSION ROUTINE TO  1134 03397000
*                                      SAVE SECONDS            Y01980*/ 03398000
*                                                                  1134 03399000
*   /*****************************************************************/ 03400000
*   /*                                                               */ 03401000
*   /*                                                       @YL02014*/ 03402000
*   /* RESTRICTED REGISTERS                                  @YL02014*/ 03403000
*   /*                                                       @YL02014*/ 03404000
*   /*                                                               */ 03405000
*   /*****************************************************************/ 03406000
*                                                                  1135 03407000
*   RESPECIFY                                                      1135 03408000
*    (PARMREG1,                                                    1135 03409000
*     PARMREG2) RESTRICTED;         /*                         Y02006   03410000
*                                      RESTRICT PARM REGISTERS Y01980*/ 03411000
*   RESPECIFY                                                      1136 03412000
*    (W,                                                           1136 03413000
*     R4,                                                          1136 03414000
*     DAYREG,                                                      1136 03415000
*     YEARREG) RESTRICTED;          /*                         Y02006   03416000
*                                      RESTRICT WORK REGISTERS Y01980*/ 03417000
*                                                                  1136 03418000
*   /*****************************************************************/ 03419000
*   /*                                                               */ 03420000
*   /*                                                       @YL02014*/ 03421000
*   /* BEGIN TIME OF DAY CONVERSION. MOVE THE TOD VALUE TO A DOUBLE  */ 03422000
*   /* WORD AREA AND ADD THE CVTTZ VALUE TO TOD BIT POSITIONS 0-31.  */ 03423000
*   /* THIS WILL CONVERT THE TOD VALUE TO LOCAL TIME.        @YL02014*/ 03424000
*   /*                                                       @YL02014*/ 03425000
*   /*                                                               */ 03426000
*   /*****************************************************************/ 03427000
*                                                                  1137 03428000
*   CVRTAREA=TODVALUE;              /* MOVE TOD VALUE TO A DOUBLEWORD   03429000
*                                      WORKAREA                Y01980*/ 03430000
         L     @07,TODPTR(,PARMREG1)                               1137 03431000
         MVC   CVRTAREA(8),TODVALUE(@07)                           1137 03432000
*   TODWORD1=TODWORD1+CVTTZVAL;     /* CHANGE TOD TO LOCAL TIMEY01980*/ 03433000
*                                                                  1138 03434000
         L     @07,CVTTZPTR(,PARMREG1)                             1138 03435000
         L     @00,TODWORD1                                        1138 03436000
         AL    @00,CVTTZVAL(,@07)                                  1138 03437000
         ST    @00,TODWORD1                                        1138 03438000
*   /*****************************************************************/ 03439000
*   /*                                                               */ 03440000
*   /*                                                       @YL02014*/ 03441000
*   /* CALCULATE TOTAL NUMBER OF MINUTES SINCE THE EPOCH TIME THE    */ 03442000
*   /* NUMBER OF MICROSECONDS THAT HAVE PASSED IN THE CURRENT MINUTE.*/ 03443000
*   /*                                                       @YL02014*/ 03444000
*   /*                                                               */ 03445000
*   /*****************************************************************/ 03446000
*                                                                  1139 03447000
*   GEN(LM R4,W,CVRTAREA);          /* PUT TOD VALUE IN EVEN-ODD   1139 03448000
*                                      REGISTER PAIR           Y01980*/ 03449000
         LM R4,W,CVRTAREA                                               03450000
*   GEN(SRDL R4,12);                /* SHIFT TOD VALUE TO REMOVE THE    03451000
*                                      INSIGNIFICANT BITS      Y01980*/ 03452000
         SRDL R4,12                                                     03453000
*   GEN(D R4,SIXTYMIL);             /* DIVID TOD VALUE BY NUMBER OF     03454000
*                                      MICROSECONDS IN A MINUTE. W -    03455000
*                                      CONTAINS TOTAL MINUTES SINCE     03456000
*                                      THE EPOCH TIME          Y01980   03457000
*                                      L - CONTAINS TOTAL NUMBER OF     03458000
*                                      MICROSECONDS INTO CURRENT   1141 03459000
*                                      MINUTE                  Y01980*/ 03460000
*                                                                  1141 03461000
         D R4,SIXTYMIL                                                  03462000
*   /*****************************************************************/ 03463000
*   /*                                                               */ 03464000
*   /*                                                       @YL02014*/ 03465000
*   /* GO TO PROPER CONVERSION ROUTINE FOR PROCESSING THIS TOD       */ 03466000
*   /* CONVERT REQUEST. USE THE REQUEST CODE IN PARMREG2 TO INDEX THE*/ 03467000
*   /* TODBRTAB. THE CODES AND ASSOCIATED ROUTINE ENTRY LABELS ARE AS*/ 03468000
*   /* FOLLOWS:                                              @YL02014*/ 03469000
*   /*                                                       @YL02014*/ 03470000
*   /* CODE ENTRY TYPE OF CONVERSION                         @YL02014*/ 03471000
*   /*                                                       @YL02014*/ 03472000
*   /* 1 FULLDATE CONVERT TO A FULL DATE AND TIME 2 RELTVTME CONVERT */ 03473000
*   /* TO RELATIVE TIME                                      @YL02014*/ 03474000
*   /*                                                       @YL02014*/ 03475000
*   /*                                                               */ 03476000
*   /*****************************************************************/ 03477000
*                                                                  1142 03478000
*   GOTO TODBRTAB(PARMREG2);        /* BRANCH ON REQUEST CODE  Y01980*/ 03479000
         LR    @07,PARMREG2                                        1142 03480000
         SLA   @07,2                                               1142 03481000
         L     @07,TODBRTAB-4(@07)                                 1142 03482000
         BR    @07                                                 1142 03483000
*                                                                  1143 03484000
*   /*****************************************************************/ 03485000
*   /*                                                               */ 03486000
*   /*                                                       @YL02014*/ 03487000
*   /* RELATIVE TIME CONVERSION --                           @YL02014*/ 03488000
*   /*                                                       @YL02014*/ 03489000
*   /* THIS ROUTINE RECEIVES CONTROL AT ENTRY 'RELTVTME'. IT CONVERTS*/ 03490000
*   /* THE TOD VALUE TO RELATIVE SECONDS.MICROSECONDS FROM 0 A.M. OF */ 03491000
*   /* THE CURRENT DAY REFLECTED BY THE TOD VALUE. THE CONVERTED TIME*/ 03492000
*   /* IS PLACED LEFT JUSTIFIED IN THE OUTPUT AREA PASSED BY THE     */ 03493000
*   /* TODCVRT CALLER AND IS IN THE FORM SSSSS.MMMMMM (EBCDIC). UPON */ 03494000
*   /* ENTRY TO THIS ROUTINE THE TOD VALUE HAS ALREADY BEEN CHANGED  */ 03495000
*   /* TO TOTAL MINUTES (IN W) AND NUMBER OF MICROSECONDS INTO       */ 03496000
*   /* CURRENT MINUTE (IN L). WHEN PROCESSING IS COMPLETED CONTROL IS*/ 03497000
*   /* RETURNED TO THE TODCNVRT CALLER                       @YL02014*/ 03498000
*   /*                                                       @YL02014*/ 03499000
*   /*                                                               */ 03500000
*   /*****************************************************************/ 03501000
*                                                                  1143 03502000
*RELTVTME:                                                         1143 03503000
*   IF TODVALUE=ZEROTOD THEN        /* IS TOD VALUE ZERO ?     Y01980*/ 03504000
RELTVTME L     @07,TODPTR(,PARMREG1)                               1143 03505000
         CLC   TODVALUE(8,@07),ZEROTOD                             1143 03506000
         BNE   @RF01143                                            1143 03507000
*     DO;                           /* YES, PASS BACK A TIME OF ZERO */ 03508000
*       TIMEERIA=ZEROTIME;          /* MOVE ZERO TIME INTO OUT AREA     03509000
*                                                              Y01980*/ 03510000
         L     @07,OUTAREA(,PARMREG1)                              1145 03511000
         MVI   TIMEERIA+12(@07),C' '                               1145 03512000
         MVC   TIMEERIA+13(11,@07),TIMEERIA+12(@07)                1145 03513000
         MVC   TIMEERIA(12,@07),ZEROTIME                           1145 03514000
*       RETURN CODE(4);             /* INDICATE ZERO TOD TO CALLER 1146 03515000
*                                                              Y01980*/ 03516000
         LA    @15,4                                               1146 03517000
         L     @14,@SA00012                                        1146 03518000
         LM    @00,@12,@SA00012+8                                  1146 03519000
         BR    @14                                                 1146 03520000
*     END;                          /*                       @YL02014*/ 03521000
*   SECONDS=R4/1000000;             /* CALCULATE NUMBER OF SECONDS 1148 03522000
*                                      INTO CURRENT MINUTE.    Y01980*/ 03523000
@RF01143 LA    @07,TIMELEFT                                        1148 03524000
         L     @15,@CF02857                                        1148 03525000
         ST    @15,@TF00001                                        1148 03526000
