         BNE   @RF00441                                            0441 00913000
*                       DO;         /* START OF FIRST TIMESTAMP    0442 00914000
*                                      RECORD PATH           @YL02014*/ 00915000
*                         FOXGTF=TSGTFOPT;/* GET GTFOPTS FROM 1ST  0443 00916000
*                                      TIMESTAMP CONTROL RCD @YL02014*/ 00917000
         MVC   FOXGTF(4),TSGTFOPT(@15)                             0443 00918000
*                         FMTGTF=ADDR(FOXGTF);/* STORE GTF OPTIONS IN   00919000
*                                      PARM LIST FOR FORMAT        0444 00920000
*                                      APPENDAGES            @YL02014*/ 00921000
         LA    @14,FOXGTF                                          0444 00922000
         ST    @14,FMTGTF                                          0444 00923000
*                         IF TSGTFYES='1'B THEN/* TIMESTAMP ON       */ 00924000
         TM    TSGTFYES(@15),B'00000001'                           0445 00925000
         BNO   @RF00445                                            0445 00926000
*                           DO;     /* START OF TIMESTAMP ON PATH    */ 00927000
*                             OFSTDATA=TSDATAOF;/* SET UP DATA OFFSET*/ 00928000
         MVC   OFSTDATA(2),TSDATAOF+2                              0447 00929000
*                             OFSTEID=TSEIDOF;/* SET UP EID OFFSET   */ 00930000
         MVC   OFSTEID(2),TSEIDOF+2                                0448 00931000
*                           END;    /* END OF TIMESTAMP ON PATH      */ 00932000
*                         ELSE                                     0450 00933000
*                           DO;     /* START OF TIMESTAMP OFF PATH   */ 00934000
         B     @RC00445                                            0450 00935000
@RF00445 DS    0H                                                  0451 00936000
*                             OFSTDATA=DATAOF;/* SET UP DATA OFFSET  */ 00937000
         MVC   OFSTDATA(2),DATAOF+2                                0451 00938000
*                             OFSTEID=EIDOF;/* SET UP EID OFFSET     */ 00939000
         MVC   OFSTEID(2),EIDOF+2                                  0452 00940000
*                           END;    /* END OF TIMESTAMP OFF PATH     */ 00941000
*                         FIRSTSW='0'B;/* TURN OFF FIRST TIMESTAMP 0454 00942000
*                                      RECORD INDICATOR      @YL02014*/ 00943000
@RC00445 NI    FIRSTSW,B'01111111'                                 0454 00944000
*                         CALL CONREC;                             0455 00945000
         BAL   @14,CONREC                                          0455 00946000
*                       END;        /* END OF FIRST TIMESTAMP      0456 00947000
*                                                            @YL02014*/ 00948000
*                     ELSE                                         0457 00949000
*                       DO;         /* START OF FID IN ERROR PATH  0457 00950000
*                                                            @YL02014*/ 00951000
         B     @RC00441                                            0457 00952000
@RF00441 DS    0H                                                  0458 00953000
*                         IF(FIRSTSW='1'B)&(RCDFID^='01'X) THEN/*  0458 00954000
*                                      CHECK FOR FIRST RCD   @YL02014*/ 00955000
         TM    FIRSTSW,B'10000000'                                 0458 00956000
         BNO   @RF00458                                            0458 00957000
         L     @01,CURREC                                          0458 00958000
         CLI   RCDFID(@01),X'01'                                   0458 00959000
         BE    @RF00458                                            0458 00960000
*                           DO;     /* START OF ERR ON 1ST RCD PATH     00961000
*                                                            @YL02014*/ 00962000
*                             MSGIND=3;/* INDICATE MSG IEA809I     0460 00963000
*                                                            @YL02014*/ 00964000
         MVC   MSGIND(2),@CH00294                                  0460 00965000
*                             LLEFT=0;/* INDICATE GET FRESH BUFFER TO   00966000
*                                      BUFFMGR               @YL02014*/ 00967000
         SLR   @15,@15                                             0461 00968000
         STH   @15,LLEFT                                           0461 00969000
*                             CALL ERRPROC;/* CALL ERROR RTN TO PRINT   00970000
*                                      MSG                   @YL02014*/ 00971000
         BAL   @14,ERRPROC                                         0462 00972000
*                           END;    /* END OF ERR ON 1ST RCD PATH  0463 00973000
*                                                            @YL02014*/ 00974000
*                         ELSE                                     0464 00975000
*                           DO;     /* START OF FID VALID? PATH      */ 00976000
         B     @RC00458                                            0464 00977000
@RF00458 DS    0H                                                  0465 00978000
*                             IF(RCDFID='01'X)/* IS FID EQUAL TO HEX    00979000
*                                      01                            */ 00980000
*                                 |(RCDFID='02'X)/* OR HEX 02      0465 00981000
*                                                            @YL02014*/ 00982000
*                                 |(RCDFID='03'X)/* OR HEX 03      0465 00983000
*                                                            @YL02014*/ 00984000
*                               THEN                               0465 00985000
         L     @15,CURREC                                          0465 00986000
         CLI   RCDFID(@15),X'01'                                   0465 00987000
         BE    @RT00465                                            0465 00988000
         CLI   RCDFID(@15),X'02'                                   0465 00989000
         BE    @RT00465                                            0465 00990000
         CLI   RCDFID(@15),X'03'                                   0465 00991000
         BNE   @RF00465                                            0465 00992000
@RT00465 DS    0H                                                  0466 00993000
*                               DO; /* START OF VALID CONTROL RECORD    00994000
*                                      PATH                  @YL02014*/ 00995000
*                                 CALL CONREC;                     0467 00996000
         BAL   @14,CONREC                                          0467 00997000
*                               END;/* END OF VALID CONTROL RECORD 0468 00998000
*                                      PATH                          */ 00999000
*                           END;    /* END OF FID VALID? PATH        */ 01000000
*                       END;        /* END OF FID IN ERROR PATH      */ 01001000
*                   END;            /* END OF VALID FID FIELD        */ 01002000
*               END;                /* END OF CONTROL REC PATH     0472 01003000
*                                                            @YL02014*/ 01004000
*             ELSE                                                 0473 01005000
*               DO;                 /* START OF TRACE RECORD PATH    */ 01006000
         B     @RC00437                                            0473 01007000
@RF00437 DS    0H                                                  0474 01008000
*                 IF RCDAID='FF'X THEN/* CHECK FOR VALID AID @YL02014*/ 01009000
         L     @15,CURREC                                          0474 01010000
         CLI   RCDAID(@15),X'FF'                                   0474 01011000
         BNE   @RF00474                                            0474 01012000
*                   DO;             /* START OF VALID AID PATH     0475 01013000
*                                                            @YL02014*/ 01014000
*                     FMTEID=CURREC+OFSTEID;/* PUT POINTER TO EID OF    01015000
*                                      RCD INTO PARM LIST FOR FORMAT    01016000
