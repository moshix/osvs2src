         BNE   @RF00479                                            0479 01038000
*                       DO;         /* START OF MATCHING ASCB PATH   */ 01039000
*                                                                  0480 01040000
*                         /*******************************************/ 01041000
*                         /*                                         */ 01042000
*                         /*                                 @YL02014*/ 01043000
*                         /*                                 @YL02014*/ 01044000
*                         /*                                 @YL02014*/ 01045000
*                         /*                                         */ 01046000
*                         /*******************************************/ 01047000
*                                                                  0481 01048000
*                         IF TRCCLS(1:4)='1110'B THEN/* CHECK FOR  0481 01049000
*                                      USER RECORD           @YL02014*/ 01050000
         TM    TRCCLS(@14),B'11100000'                             0481 01051000
         BNO   @RF00481                                            0481 01052000
         TM    TRCCLS(@14),B'00010000'                             0481 01053000
         BNZ   @RF00481                                            0481 01054000
*                           DO;     /* START OF USER RECORD PATH     */ 01055000
*                             CALL USRFORMT;/*               @YL02014*/ 01056000
         BAL   @14,USRFORMT                                        0483 01057000
*                           END;    /* END OF USER RECORD PATH     0484 01058000
*                                                            @YL02014*/ 01059000
*                         ELSE                                     0485 01060000
*                           DO;     /* START OF NOT USER RECORD PATH */ 01061000
         B     @RC00481                                            0485 01062000
@RF00481 DS    0H                                                  0486 01063000
*                             CALL SYSFORMT;/*               @YL02014*/ 01064000
         BAL   @14,SYSFORMT                                        0486 01065000
*                           END;    /* END OF NOT USER RECORD      0487 01066000
*                                                            @YL02014*/ 01067000
*                       END;        /* END OF MATCHING ASCB PATH     */ 01068000
*                   END;            /* END OF VALID AID PATH @YL02014*/ 01069000
*                 ELSE                                             0490 01070000
*                   DO;             /* START OF NOT VALID AID OR USR    01071000
*                                      PATH                  @YL02014*/ 01072000
         B     @RC00474                                            0490 01073000
@RF00474 DS    0H                                                  0491 01074000
*                     IF FIRSTSW='1'B THEN/* CHECK FOR FIRST RCD   0491 01075000
*                                                            @YL02014*/ 01076000
         TM    FIRSTSW,B'10000000'                                 0491 01077000
         BNO   @RF00491                                            0491 01078000
*                       DO;         /* START OF ERR ON 1ST RCD PATH     01079000
*                                                            @YL02014*/ 01080000
*                         MSGIND=3; /* INDICATE MSG IEA809I  @YL02014*/ 01081000
         MVC   MSGIND(2),@CH00294                                  0493 01082000
*                         LLEFT=0;  /* INDICATE GET FRESH BUFFER TO     01083000
*                                      BUFFMGR               @YL02014*/ 01084000
         SLR   @15,@15                                             0494 01085000
         STH   @15,LLEFT                                           0494 01086000
*                         CALL ERRPROC;/* CALL ERROR RTN TO PRINT MSG   01087000
*                                                            @YL02014*/ 01088000
         BAL   @14,ERRPROC                                         0495 01089000
*                       END;        /* END OF ERR ON 1ST RCD PATH  0496 01090000
*                                                            @YL02014*/ 01091000
*                     ELSE                                         0497 01092000
*                       DO;         /* START OF AID IN ERROR PATH  0497 01093000
*                                                            @YL02014*/ 01094000
         B     @RC00491                                            0497 01095000
@RF00491 DS    0H                                                  0498 01096000
*                         MSGIND=6; /* INDICATE MSG          @YL02014*/ 01097000
         MVC   MSGIND(2),@CH01274                                  0498 01098000
*                         CALL ERRPROC;/*                    @YL02014*/ 01099000
         BAL   @14,ERRPROC                                         0499 01100000
*                         CALL HEXFORMT;/*                   @YL02014*/ 01101000
         BAL   @14,HEXFORMT                                        0500 01102000
*                       END;        /* END OF AID IN ERROR   @YL02014*/ 01103000
*                   END;            /* END OF NOT VALID AID PATH     */ 01104000
*               END;                /* END OF TRACE RECORD PATH      */ 01105000
*           END;                    /* END OF VALID LL PATH  @YL02014*/ 01106000
*         ELSE                                                     0505 01107000
*           DO;                     /* START OF INVALID LL PATH    0505 01108000
*                                                            @YL02014*/ 01109000
         B     @RC00434                                            0505 01110000
@RF00434 DS    0H                                                  0506 01111000
*             ERRBIT='0'B;          /* TURN OFF ERROR INDICATOR    0506 01112000
*                                                            @YL02014*/ 01113000
         NI    ERRBIT,B'10111111'                                  0506 01114000
*           END;                    /* END OF INVALID LL PATH      0507 01115000
*                                                            @YL02014*/ 01116000
*       END;                        /* END OF NOT EOF PATH   @YL02014*/ 01117000
@RC00434 DS    0H                                                  0509 01118000
*   END;                            /* END OF BUFF MGR PATH  @YL02014*/ 01119000
@RF00432 DS    0H                                                  0509 01120000
@DE00430 TM    EOFIND,B'00100000'                                  0509 01121000
         BZ    @DL00430                                            0509 01122000
*   ADTEDCTL='0'B;                  /*                       @YL02014*/ 01123000
         NI    ADTEDCTL,B'10111111'                                0510 01124000
*   END CONTROL;                                                   0511 01125000
@EL00003 DS    0H                                                  0511 01126000
@EF00003 DS    0H                                                  0511 01127000
@ER00003 LM    @14,@12,@SA00003                                    0511 01128000
         BR    @14                                                 0511 01129000
*BUFFMGR:                                                          0512 01130000
*   PROC;                                                          0512 01131000
BUFFMGR  STM   @14,@12,@SA00004                                    0512 01132000
*   ADTEDBUF='1'B;                  /* SET AUDIT FOR CALLED PROGRAM  */ 01133000
         OI    ADTEDBUF,B'00100000'                                0513 01134000
*   IF LLEFT=0 THEN                 /* IS REMAINING LENGTH OF BUFFER    01135000
*                                      EQUAL TO ZERO         @YL02014*/ 01136000
         SLR   @15,@15                                             0514 01137000
         CH    @15,LLEFT                                           0514 01138000
         BNE   @RF00514                                            0514 01139000
*     DO;                           /* START OF GET FRESH BUFFER PATH*/ 01140000
*       READECB=READECB&&READECB;   /* ZERO OUT ECB FOR AHLREAD      */ 01141000
         L     @14,READECB                                         0516 01142000
         LR    @07,@14                                             0516 01143000
         XR    @07,@14                                             0516 01144000
         ST    @07,READECB                                         0516 01145000
*       DO;                         /* AHLREAD DATAAREA(OUTBUFFR)ECB(   01146000
*                                      READECB)TCB(ADPLTCB->TCB)     */ 01147000
*                                                                  0517 01148000
*         /***********************************************************/ 01149000
*         /*                                                         */ 01150000
*         /* THE FOLLOWING CODE IS GENERATED BY THE AHLREAD MACRO. IT*/ 01151000
*         /* LOCATES GTF, BUILDS A PARAMETER LIST, AND GETS AND      */ 01152000
*         /* SCHEDULES AN SRB FOR AHLREADR                    @Y02014*/ 01153000
*         /*                                                         */ 01154000
*         /***********************************************************/ 01155000
*                                                                  0518 01156000
*         DCL                                                      0518 01157000
*           1 AHLRDAT INT BDY(WORD) BASED,/* MAPPING OF WORKAREA FOR    01158000
*                                      THIS MACRO                    */ 01159000
*            2 * CHAR(12) BDY(WORD),/* WORKAREA FOR EXEC FORM GETMAIN*/ 01160000
*             3 AHLRDGM PTR(31) BDY(WORD),/* PLIST FOR EXEC FORM   0518 01161000
*                                      GETMAIN                       */ 01162000
*            2 AHLRDPTR PTR(31);    /* WORD WHERE GETMAIN WILL RETURN   01163000
*                                      ADDR OF THE STORAGE IT HAS  0518 01164000
*                                      GOTTEN                        */ 01165000
*         DCL                                                      0519 01166000
*           1 AHLRDSQA BASED(AHLSRB) BDY(WORD),/* PLIST AND SRB      */ 01167000
*            2 AHLSRBAS CHAR(LENGTH(SRB)),/* AREA FOR SRB            */ 01168000
*            2 AHLRDPL BDY(WORD),   /* TRACE DATA REQUEST PARAMETER     01169000
*                                      LIST                          */ 01170000
*             3 AHLRDPQC PTR(31),   /* THIS IS THE AHLRDPLQ QUEUE  0519 01171000
*                                      CHAIN FIELD AND WILL POINT TO    01172000
*                                      THE NEXT AHLRDPL OR BE 0      */ 01173000
*             3 AHLRDID CHAR(8),    /* THIS ID IS MATCHED WITH THE 0519 01174000
*                                      MCQENAME TO DETERMINE WHICH 0519 01175000
*                                      APPLICATION THE DATA IS TO  0519 01176000
*                                      COME FROM                     */ 01177000
*             3 * PTR(16),                                         0519 01178000
*             3 AHLRDDID CHAR(6) BDY(WORD,3),/* ID OF THE TRACE DATA    01179000
*                                      REQUEST                       */ 01180000
*              4 AHLRDASI PTR(16),  /* CURRENT ASID                  */ 01181000
*              4 AHLRDTCB PTR(31),  /* TCB FOR WHICH DUMP IS BEING 0519 01182000
*                                      TAKEN, OR DEFAULTED TO 0 FOR     01183000
*                                      AN SVCDUMP                    */ 01184000
*             3 AHLRDECB PTR(31),   /* ADDR OF THE ECB TO BE POSTED     01185000
*                                      WITH THE RETURN CODE WHEN THE    01186000
*                                      FUNCTION IS COMPLETED         */ 01187000
*             3 AHLRDATA PTR(31),   /* ADDRESS OF THE DATA AREA WHERE   01188000
*                                      AHLREADR IS TO PUT ITS OUTPUT */ 01189000
*             3 AHLRDASC PTR(31),   /* ASCB ADDR WHERE ECB IS TO BE     01190000
*                                      FOUND                         */ 01191000
*             3 AHLRDPX2 PTR(31),   /* POST CODE FOR THE REQUESTOR OF   01192000
*                                      THE DATA                      */ 01193000
*             3 AHLRDSRB PTR(31);   /* ADDR OF THE SRB USED FOR THIS    01194000
*                                      REQUEST                       */ 01195000
*         DCL                                                      0520 01196000
*           AHLSRB REG(1) PTR(31),  /* SRB MAPPING ADDRESS - THIS  0520 01197000
*                                      MUST BE GPR1 BECAUSE IT IS  0520 01198000
*                                      USED FOR THE SCHEDULE MACRO   */ 01199000
*           AHLRC PTR(31) REG(15);  /* USED TO CHECK RETURN CODES    */ 01200000
*         DCL                                                      0521 01201000
*           AHLRC4 BIT(32) CONSTANT('40000004'X),/* USED TO SET ECB     01202000
*                                      TO RETURN CODE OF 4 AND SET 0521 01203000
*                                      THE POST BIT ON IF AHLREADR 0521 01204000
*                                      CANNOT BE SCHEDULE            */ 01205000
*           AHLSPSQA FIXED(16) CONSTANT(245);/* SQA SUBPOOL FOR    0521 01206000
*                                      GETMAIN                       */ 01207000
*         DCL                                                      0522 01208000
*           1 AHLRDG DEFINED(AHLRDGM),/* LIST FOR GETMAIN            */ 01209000
*            2 AHLRDG01 PTR(31),    /* LENGTH                        */ 01210000
*            2 AHLRDG02 CHAR(1),    /* HIARCHY                       */ 01211000
*            2 AHLRDG03 PTR(24),    /* ADDR OF ADDR LIST             */ 01212000
*            2 AHLRDG04 CHAR(1),    /* TYPE OF REQUEST               */ 01213000
