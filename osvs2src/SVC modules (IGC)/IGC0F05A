*   GEN(CVD R4,CVRTAREA);           /* CONVERT HOURS TO DECIMALY01980*/ 03781000
         CVD R4,CVRTAREA                                                03782000
*   GEN(UNPK 0(2,PARMREG2),CVRTAREA+6(2));/* UNPACK HOURS      Y01980*/ 03783000
         UNPK 0(2,PARMREG2),CVRTAREA+6(2)                               03784000
*   GEN(OI 1(PARMREG2),X'F0');      /* CHANGE SIGN TO A NUMERIC ZONE    03785000
*                                                              Y01980*/ 03786000
*                                                                  1192 03787000
         OI 1(PARMREG2),X'F0'                                           03788000
*   /*****************************************************************/ 03789000
*   /*                                                               */ 03790000
*   /*                                                       @YL02014*/ 03791000
*   /* CALCULATE THE CURRENT YEAR AND JULIAN DAY USING 'TIMELEFT'    */ 03792000
*   /* WHICH CONTAINS THE TOTAL NUMBER OF ELAPSED DAYS SINCE 1900.   */ 03793000
*   /* FIRST CHECK WHETHER THE TIME IS IN THE 1900 THROUGH 1903 ERA  */ 03794000
*   /* (I.E., LESS THAN 1460 ELAPSED DAYS). IF SO, THE TIME MUST BE  */ 03795000
*   /* HANDLED IN A SPECIAL WAY SINCE 1900 IS NOT A LEAP YEAR AND    */ 03796000
*   /* EACH YEAR IN THAT FOUR YEAR SPAN THUS CONTAINS EXACTLY 365    */ 03797000
*   /* DAYS. IF THE ELAPSED TIME HOWEVER IS GREATER THAN 1459 DAYS   */ 03798000
*   /* THEN EVERY FOUR YEARS CONTAINS 1461 DAYS RATHER THAN 1460 AND */ 03799000
*   /* THE FIRST YEAR OF EACH GROUP IS A LEAP YEAR. THESE FOUR YEAR  */ 03800000
*   /* GROUPS ARE REFERRED TO AS LEAP YEAR GROUPS (E.G., 1904-1907,  */ 03801000
*   /* 1908-1911, ETC.). THE YEAR AND THE DAY WHEN THE ELAPSED TIME  */ 03802000
*   /* FALLS INTO ONE OF THESE GROUPS MUST BE CALCULATED USING       */ 03803000
*   /* STANDARD FORMULAS DIFFERENT FROM THE FORMULAS USED WHEN THE   */ 03804000
*   /* TIME IS BETWEEN 1900 AND JANUARY 1, 1904.             @YL02014*/ 03805000
*   /*                                                       @YL02014*/ 03806000
*   /*                                                               */ 03807000
*   /*****************************************************************/ 03808000
*                                                                  1193 03809000
*   IF TIMELEFT<1460 THEN           /* IS TIME ELAPSED WITHIN THE  1193 03810000
*                                      1900 THRU 1903 ERA?     Y01980*/ 03811000
         L     @07,TIMELEFT                                        1193 03812000
         CH    @07,@CH02863                                        1193 03813000
         BNL   @RF01193                                            1193 03814000
*NOLEAPYR:                                                         1194 03815000
*     DO;                           /* YES CALCULATE YEAR AND DAY  1194 03816000
*                                      WITHIN THE 1900 THRU 1903 ERA    03817000
*                                                              Y01980*/ 03818000
NOLEAPYR DS    0H                                                  1195 03819000
*       DAYREG=(TIMELEFT//365)+1;   /* CALCULATE CURRENT DAY   Y01980*/ 03820000
         L     @07,TIMELEFT                                        1195 03821000
         LA    @15,365                                             1195 03822000
         LR    @14,@07                                             1195 03823000
         ST    @15,@TF00001                                        1195 03824000
         SRDA  @14,32                                              1195 03825000
         D     @14,@TF00001                                        1195 03826000
         AL    @14,@CF00063                                        1195 03827000
         LR    DAYREG,@14                                          1195 03828000
*       YEARREG=1900+(TIMELEFT/365);/* CALCULATE CURRENT YR.   Y01980*/ 03829000
         LR    @14,@07                                             1196 03830000
         SRDA  @14,32                                              1196 03831000
         D     @14,@TF00001                                        1196 03832000
         AH    @15,@CH02867                                        1196 03833000
         LR    YEARREG,@15                                         1196 03834000
*     END NOLEAPYR;                 /*                       @YL02014*/ 03835000
*                                                                  1197 03836000
*   /*****************************************************************/ 03837000
*   /*                                                               */ 03838000
*   /*                                                       @YL02014*/ 03839000
*   /* THE TIME ELAPSED IS 1460 OR MORE DAYS (I.E., THE CURRENT YEAR */ 03840000
*   /* AND DAY MUST BE WITHIN A LEAP YEAR GROUP). THUS ADJUST THE    */ 03841000
*   /* ELAPSED TIME CONTAINED IN 'TIMELEFT' SO THAT 1904 CAN BE USED */ 03842000
*   /* AS THE STARTING POINT FOR CALCULATING THE CURRENT YEAR AND    */ 03843000
*   /* DAY. NEXT CALCULATE W, THE RELATIVE LEAP YEAR GROUP SINCE     */ 03844000
*   /* 1904, AND L, THE NUMBER OF DAYS THAT HAVE ELAPSED IN THE      */ 03845000
*   /* CURRENT LEAP YEAR GROUP. FINALLY, USE THESE VALUES IN THE     */ 03846000
*   /* FORMULAS FOR CALCULATING THE CURRENT DAY AND YEAR WHEN ELAPSED*/ 03847000
*   /* TIME IS GREATER THAN 1459.                            @YL02014*/ 03848000
*   /*                                                       @YL02014*/ 03849000
*   /* IN THE FORMULA USED TO CALCULATE THE CURRENT JULIAN DAY, THE  */ 03850000
*   /* EXPRESSION IN PARENTHESES IS THE NUMBER OF WHOLE DAYS THAT    */ 03851000
*   /* HAVE PAST IN THE CURRENT YEAR. BECAUSE THE CURRENT DAY HAS NOT*/ 03852000
*   /* YET ELAPSED, IT IS NOT INCLUDED IN THE RESULT PRODUCED BY THE */ 03853000
*   /* EXPRESSION AND THEREFORE 1 MUST BE ADDED TO GET THE ACTUAL    */ 03854000
*   /* JULIAN DAY. WITHIN THE EXPRESSION THE FIRST TERM, R, MUST BE  */ 03855000
*   /* ADJUSTED BY 1 (I.E., L/731) IF THE TOTAL DAYS INTO THE LEAP   */ 03856000
*   /* YEAR GROUP IS GREATER OR EQUAL TO 2 YEARS AND AGAIN BY 1 (I.E.*/ 03857000
*   /* L/1096) IF THE TOTAL DAYS IS EQUAL TO OR GREATER THAN 3 YEARS.*/ 03858000
*   /* THE AFFECT OF THIS IS THAT THE SECOND YEAR AND THIRD YEAR ARE */ 03859000
*   /* FORCED TO 366 DAYS ONCE THE TOTAL DAYS INTO THE LEAP YEAR     */ 03860000
*   /* GROUP EXCEEDS EACH OF THESE YEARS. THIS IS NECESSARY BECAUSE A*/ 03861000
*   /* DIVISOR OF 366 IS USED.                               @YL02014*/ 03862000
*   /*                                                       @YL02014*/ 03863000
*   /* IN THE FORMULA FOR CALCULATING THE YEAR, 1904 IS THE BASE     */ 03864000
*   /* YEAR. THE SECOND TERM (W * 4) YIELDS THE RELATIVE NUMBER OF   */ 03865000
*   /* YEARS SINCE 1904 UP TO AND INCLUDING THE LEAP YEAR IN THE     */ 03866000
*   /* CURRENT LEAP YEAR GROUP. THE THIRD TERM ( (L-1)/365 ) YIELDS  */ 03867000
*   /* THE RELATIVE YEAR WITHIN THE CURRENT LEAP YEAR GROUP. THUS IF */ 03868000
*   /* THE CURRENT YEAR IS A LEAP YEAR THE RELATIVE YEAR WITHIN THE  */ 03869000
*   /* CURRENT LEAP YEAR GROUP IS 0 AND THE THIRD TERM WILL YIELD A  */ 03870000
*   /* RESULT OF ZERO.                                       @YL02014*/ 03871000
*   /*                                                       @YL02014*/ 03872000
*   /*                                                               */ 03873000
*   /*****************************************************************/ 03874000
*                                                                  1198 03875000
*   ELSE                                                           1198 03876000
*LEAPYR:                                                           1198 03877000
*     DO;                           /* DATE IS GREATER THAN 1903   1198 03878000
*                                                              Y01980*/ 03879000
         B     @RC01193                                            1198 03880000
@RF01193 DS    0H                                                  1198 03881000
LEAPYR   DS    0H                                                  1199 03882000
*       TIMELEFT=TIMELEFT-1460;     /* ADJUST ELAPSED TIME SO 1904 1199 03883000
*                                      BASE CAN BE USED IN FORMULA 1199 03884000
*                                                              Y01980*/ 03885000
         L     @07,TIMELEFT                                        1199 03886000
         SH    @07,@CH02863                                        1199 03887000
         ST    @07,TIMELEFT                                        1199 03888000
*       W=TIMELEFT/1461;            /* DIVIDE TOTAL DAYS SINCE EPOCH    03889000
*                                      BY NUMBER OF DAYS IN A LEAP 1200 03890000
*                                      YEAR GROUP. THE RESULT, W,  1200 03891000
*                                      WILL BE THE RELATIVE LEAP YEAR   03892000
*                                      GROUP SINCE THE EPOCH.  Y01980*/ 03893000
         LA    @15,1461                                            1200 03894000
         LR    @14,@07                                             1200 03895000
         ST    @15,@TF00001                                        1200 03896000
         SRDA  @14,32                                              1200 03897000
         D     @14,@TF00001                                        1200 03898000
         LR    W,@15                                               1200 03899000
*       R4=TIMELEFT//1461;          /* CALCULATE TOTAL DAYS ELAPSED     03900000
*                                      IN CURRENT LEAP YEAR GROUP  1201 03901000
*                                                              Y01980*/ 03902000
         LR    @14,@07                                             1201 03903000
         SRDA  @14,32                                              1201 03904000
