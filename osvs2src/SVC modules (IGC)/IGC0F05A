*   ALLU='1'B;                      /* INDICATE PRINT USR RCDS     0409 00819000
*                                                            @YL02014*/ 00820000
         OI    ALLU,B'10000000'                                    0409 00821000
*   ALLP='1'B;                      /* INDICATE PRINT PI RCDS      0410 00822000
*                                                            @YL02014*/ 00823000
         OI    ALLP,B'10000000'                                    0410 00824000
*   EXT='1'B;                       /* INDICATE PRINT EXT RCDS     0411 00825000
*                                                            @YL02014*/ 00826000
*   DSP='1'B;                       /* INDICATE PRINT DSP RCDS     0412 00827000
*                                                            @YL02014*/ 00828000
*   RNIO='1'B;                      /* INDICATE PRNT RNIO RCDS     0413 00829000
