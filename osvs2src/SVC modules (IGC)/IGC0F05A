*   SRM='1'B;                       /* INDICATE PRINT SRM RCDS     0415 00833000
*                                                            @YL02014*/ 00834000
*                                                                  0415 00835000
         OI    EXT,B'11001110'                                     0415 00836000
*   /*****************************************************************/ 00837000
*   /*                                                               */ 00838000
*   /*                                                       @YL02014*/ 00839000
*   /* ZERO OUT SYSTAB AND USRTABL AND                       @YL02014*/ 00840000
*   /* BLDLLIST AND TEMPIBUF,BLANK OUT TEMPOBUF              @YL02014*/ 00841000
*   /*                                                               */ 00842000
*   /*****************************************************************/ 00843000
*                                                                  0416 00844000
*   ZEROSYS=''B;                    /* ZERO OUT SYSTAB       @YL02014*/ 00845000
         XC    ZEROSYS(256),ZEROSYS                                0416 00846000
*   ZEROUSR=''B;                    /* ZERO OUT USRTABL      @YL02014*/ 00847000
         XC    ZEROUSR(256),ZEROUSR                                0417 00848000
*   ZEROBLDL=''B;                   /* ZERO OUT BLDLLIST     @YL02014*/ 00849000
         XC    ZEROBLDL(178),ZEROBLDL                              0418 00850000
*   ZEROIBUF(1:256)=''B;            /* ZERO OUT TEMP RECORD  @YL02014*/ 00851000
         XC    ZEROIBUF(256),ZEROIBUF                              0419 00852000
*   ZEROIBUF(256:284)=''B;          /* ZERO OUT REST OF TEMP RCD     */ 00853000
         XC    ZEROIBUF+255(29),ZEROIBUF+255                       0420 00854000
*   TEMPOBUF='';                    /* BLANK OUT TEMP PRINT BUF      */ 00855000
         MVI   TEMPOBUF,C' '                                       0421 00856000
         MVC   TEMPOBUF+1(120),TEMPOBUF                            0421 00857000
*   ADTEDINT='0'B;                  /* TURN OFF AUDIT FLAGS  @YL02014*/ 00858000
         NI    ADTEDINT,B'01111111'                                0422 00859000
*   END INITPROC;                                                  0423 00860000
@EL00002 DS    0H                                                  0423 00861000
@EF00002 DS    0H                                                  0423 00862000
@ER00002 LM    @14,@12,@SA00002                                    0423 00863000
         BR    @14                                                 0423 00864000
*CONTROL:                                                          0424 00865000
*   PROC;                                                          0424 00866000
CONTROL  STM   @14,@12,@SA00003                                    0424 00867000
*   ADTEDCTL='1'B;                  /* INDICATE CONTROL AS CALLED  0425 00868000
*                                      ROUTINE               @YL02014*/ 00869000
         OI    ADTEDCTL,B'01000000'                                0425 00870000
*   FIRSTSW='1'B;                   /* GTF OPTION NEEDED     @YL02014*/ 00871000
         OI    FIRSTSW,B'10000000'                                 0426 00872000
*   LLEFT=0;                        /* INIT # OF BYTES LEFT IN BUFFER*/ 00873000
         SLR   @15,@15                                             0427 00874000
         STH   @15,LLEFT                                           0427 00875000
*   ERRBIT='0'B;                    /* INIT ERROR BIT        @YL02014*/ 00876000
*   EOFIND='0'B;                    /* INIT EOFIND BIT       @YL02014*/ 00877000
         NI    ERRBIT,B'10011111'                                  0429 00878000
*   DO WHILE(EOFIND='0'B);          /* START OF BUFF MGR PATH      0430 00879000
*                                                            @YL02014*/ 00880000
         B     @DE00430                                            0430 00881000
@DL00430 DS    0H                                                  0431 00882000
*     CALL BUFFMGR;                 /*                       @YL02014*/ 00883000
         BAL   @14,BUFFMGR                                         0431 00884000
*     IF EOFIND='0'B THEN                                          0432 00885000
         TM    EOFIND,B'00100000'                                  0432 00886000
         BNZ   @RF00432                                            0432 00887000
*       DO;                         /* START OF NOT EOF PATH @YL02014*/ 00888000
*         IF ERRBIT='0'B THEN       /* CHECK FOR VALID LL FIELDS     */ 00889000
         TM    ERRBIT,B'01000000'                                  0434 00890000
         BNZ   @RF00434                                            0434 00891000
*           DO;                     /* START OF VALID LL PATH      0435 00892000
*                                                            @YL02014*/ 00893000
*             FMTINPUT=CURREC;      /* PUT POINTER TO CURRENT RECORD    00894000
*                                      INTO PARM LIST FOR FORMAT   0436 00895000
*                                      APPENDAGES            @YL02014*/ 00896000
         L     @15,CURREC                                          0436 00897000
         ST    @15,FMTINPUT                                        0436 00898000
*             IF RCDAID='00'X THEN  /* CHECK FOR GTF CONTROL       0437 00899000
*                                      RECORD(00)                    */ 00900000
         CLI   RCDAID(@15),X'00'                                   0437 00901000
         BNE   @RF00437                                            0437 00902000
*               DO;                 /* START OF CONTROL REC PATH     */ 00903000
*                 IF RCDFID^='00'X THEN/* CHECK FOR VALID FID FIELD  */ 00904000
         CLI   RCDFID(@15),X'00'                                   0439 00905000
         BE    @RF00439                                            0439 00906000
*                   DO;             /* START OF VALID FID FIELD      */ 00907000
*                     IF(FIRSTSW='1'B)&(RCDFID='01'X) THEN/* CHECK 0441 00908000
*                                      FOR TIMESTAMP RECORD  @YL02014*/ 00909000
         TM    FIRSTSW,B'10000000'                                 0441 00910000
         BNO   @RF00441                                            0441 00911000
