         TITLE 'IGC0K10D, OPERATOR CONTROL MESSAGE MODULE              *00001000
                        '                                               00002000
IGC0K10D CSECT ,                                                01S0001 00003000
@PROLOG  BALR  @12,0                                            01S0001 00004000
@PSTART  DS    0H                                               01S0001 00005000
         USING @PSTART,@12                                      01S0001 00006000
*                                   /*                                  00007000
IGC0K10D IEDHJN AFTERHJN,HJN           MODULE IDENTIFICATION MACRO      00008000
*   ROPCAVT=RPARM;                  /* ESTABLISH ADDRESSABILITY TO      00009000
*                                      THE OPERATOR CONTROL ADDRESS     00010000
*                                      VECTOR TABLE                  */ 00011000
         LR    ROPCAVT,RPARM                                    02S0176 00012000
*   RFY                                                         02S0177 00013000
*     RPARM UNRSTD;                 /* RELEASE THE REGISTER     02S0177 00014000
*                                      RESTRICTED AT ENTRY TO THE       00015000
*                                      MODULE THAT CONTAINS THE 02S0177 00016000
*                                      ADDRESS OF THE OPERATOR  02S0177 00017000
*                                      CONTROL ADDRESS VECTOR TABLE  */ 00018000
*   REG10=OPCWORK+LENGTH(MSGWORKA); /* SET THE BASE REGISTER TO BE      00019000
*                                      USED FOR ADDRESSING AUTOMATIC    00020000
*                                      DATA WITH THE ADDRESS OF THE     00021000
*                                      SECTION OF THE OPERATOR  02S0178 00022000
*                                      CONTROL RESIDENT WORK AREA       00023000
*                                      THAT IS AFTER THE SECTION USED   00024000
*                                      TO CONSTRUCT MESSAGES         */ 00025000
         LA    REG10,84                                         02S0178 00026000
         AL    REG10,OPCWORK(,ROPCAVT)                          02S0178 00027000
*   GEN REFS(REG10) NOSETS;                                     02S0179 00028000
*                                   /*                                  00029000
         USING @DATD,REG10             PROVIDE ADDRESSABILITY TO ALL    00030000
*                                      AUTOMATIC DATA                   00031000
*   RSAVE=ADDR(OPCSAVE);            /* SET THE SAVE AREA ADDRESS        00032000
*                                      REGISTER WITH THE ADDRESS OF     00033000
*                                      THE OPERATOR CONTROL SAVE AREA*/ 00034000
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          02S0180 00035000
*                                                               02S0181 00036000
*   /*****************************************************************/ 00037000
*   /*                                                               */ 00038000
*   /* PROCESS: THIS SEGMENT DOES THE PROCESSING THAT IS NECESSARY TO*/ 00039000
*   /* PERFORM THE FUNCTION OF THE MODULE.                           */ 00040000
*   /*                                                               */ 00041000
*   /*****************************************************************/ 00042000
*                                                               02S0181 00043000
*                                                               03S0181 00044000
*   /*****************************************************************/ 00045000
*   /*                                                               */ 00046000
*   /* PROCESS.INITIAL: THIS SEGMENT DOES THE INITIAL PROCESSING.    */ 00047000
*   /*                                                               */ 00048000
*   /*****************************************************************/ 00049000
*                                                               03S0181 00050000
*   RAVT=OPCAVTPT;                  /* SET THE BASE POINTER FOR THE     00051000
*                                      TCAM ADDRESS VECTOR TABLE     */ 00052000
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0181 00053000
*   ROPCE=OPCCOPCE;                 /* SET THE BASE POINTER FOR THE     00054000
*                                      OPERATOR CONTROL ELEMENT      */ 00055000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0182 00056000
*   SEARCH=YES;                     /* INITIALIZE THE "SEARCH" SWITCH   00057000
*                                      TO INDICATE THAT A SEARCH IS     00058000
*                                      TO BE PERFORMED               */ 00059000
         OI    SEARCH,B'10000000'                               02S0183 00060000
*   DO PTRENTRY=ADDR(TABDATA) BY LENGTH(TABENTRY) WHILE(SEARCH=YES);    00061000
         LA    PTRENTRY,TABDATA                                 02S0184 00062000
         B     @DE00184                                         02S0184 00063000
@DL00184 DS    0H                                               02S0185 00064000
*                                   /* INCREMENT THE POINTER TO 02S0185 00065000
*                                      ENTRIES IN THE MESSAGE DATA      00066000
*                                      TABLE WHILE THE SEARCH IS IN     00067000
*                                      PROGRESS                      */ 00068000
*     IF TABMNUM=REG00              /* MATCHING MESSAGE NUMBERS      */ 00069000
*       THEN                        /* STOP THE SEARCH AND BUILD THE    00070000
*                                      APPROPRIATE MESSAGE           */ 00071000
         CH    REG00,TABMNUM(,PTRENTRY)                         02S0185 00072000
         BNE   @RF00185                                         02S0185 00073000
*                                                               03S0186 00074000
*       /*************************************************************/ 00075000
*       /*                                                           */ 00076000
*       /* PROCESS.BUILDMSG: THIS SEGMENT STOPS THE SEARCH FOR A     */ 00077000
*       /* MATCHING MESSAGE TABLE ENTRY, BUILDS THE APPROPRIATE      */ 00078000
*       /* MESSAGE IN THE OPERATOR CONTROL RESIDENT WORK AREA AND    */ 00079000
*       /* DOES THE PREPARATION THAT IS NECESSARY TO TRANSFER CONTROL*/ 00080000
*       /* TO THE LAST MODULE IN THE MESSAGE MODULE CHAIN.           */ 00081000
*       /*                                                           */ 00082000
*       /*************************************************************/ 00083000
*                                                               03S0186 00084000
*       DO;                                                     03S0186 00085000
*         SEARCH=STOP;              /* INDICATE THAT THE SEARCH IS TO   00086000
*                                      BE STOPPED                    */ 00087000
         NI    SEARCH,B'01111111'                               03S0187 00088000
*         MSGLNGTH=TABLNGTH;        /* SET THE LENGTH OF THE MESSAGE    00089000
*                                      IN THE OPERATOR CONTROL  03S0188 00090000
*                                      RESIDENT WORK AREA            */ 00091000
         L     @11,OPCWORK(,ROPCAVT)                            03S0188 00092000
         LH    @01,TABLNGTH(,PTRENTRY)                          03S0188 00093000
         STH   @01,MSGLNGTH(,@11)                               03S0188 00094000
*         MSGTEXT(1:MSGLNGTH)=MSGSTRNG(1:MSGLNGTH);/* PUT THE MESSAGE   00095000
*                                      CHARACTER STRING INTO THE        00096000
*                                      MESSAGE TEXT FIELD OF THE WORK   00097000
*                                      AREA                          */ 00098000
         BCTR  @01,0                                            03S0189 00099000
         L     @15,TABMPTR(,PTRENTRY)                           03S0189 00100000
         EX    @01,@SM01172                                     03S0189 00101000
*         IF TABCASE>0              /* ADDITIONAL PROCESSING OF THE     00102000
*                                      MESSAGE IS NECESSARY          */ 00103000
*           THEN                    /* PROCESS THE UNIQUE       03S0190 00104000
*                                      REQUIREMENTS OF THE MESSAGE   */ 00105000
         L     @11,TABCASE(,PTRENTRY)                           03S0190 00106000
         LTR   @11,@11                                          03S0190 00107000
         BNP   @RF00190                                         03S0190 00108000
*                                                               04S0191 00109000
*           /*********************************************************/ 00110000
*           /*                                                       */ 00111000
*           /* PROCESS.BUILDMSG.CASE: THIS SEGMENT IMPLEMENTS THE    */ 00112000
*           /* CASE CONTROL STRUCTURE IN ORDER TO SATISFY THE UNIQUE */ 00113000
*           /* REQUIREMENTS OF THE MESSAGES WITH VARIABLE CHARACTER  */ 00114000
*           /* FIELDS THAT ARE BUILT IN THE OPERATOR CONTROL RESIDENT*/ 00115000
*           /* WORK AREA BY THIS MODULE.                             */ 00116000
*           /*                                                       */ 00117000
*           /*********************************************************/ 00118000
*                                                               04S0191 00119000
*           DO;                     /* CASE(TABCASE)NAMES(SUBORDIN,AL   00120000
*                                      LSUBOR,INVALRLN,ACTIVATE,LDMOD   00121000
*                                      ULE,ACTDEACT,CLUSTNME)        */ 00122000
*             DCL                                               04S0192 00123000
*               CL0001@ LABEL BASED(CP0001@(TABCASE)) VALUERANGE(       00124000
*                   SUBORDIN,ALLSUBOR,INVALRLN,ACTIVATE,LDMODULE,       00125000
*                   ACTDEACT,CLUSTNME);/* BRANCH TARGET              */ 00126000
*             DCL                                               04S0193 00127000
*               CP0001@(7) PTR(31) INIT(ADDR(SUBORDIN),ADDR(ALLSUBOR),  00128000
*                   ADDR(INVALRLN),ADDR(ACTIVATE),ADDR(LDMODULE),ADDR(  00129000
*                   ACTDEACT),ADDR(CLUSTNME));                  04S0193 00130000
*             GOTO CL0001@;         /* BRANCH TO SELECTED CASE       */ 00131000
         SLA   @11,2                                            04S0194 00132000
         L     @01,CP0001@-4(@11)                               04S0194 00133000
         BR    @01                                              04S0194 00134000
*                                                               05S0195 00135000
*             /*******************************************************/ 00136000
*             /*                                                     */ 00137000
*             /* PROCESS.BUILDMSG.CASE.SUBORDIN: THIS SEGMENT PLACES */ 00138000
*             /* THE HOLD OR RELEASE COMMAND,THE RESOURCE NAME,AND   */ 00139000
*             /* THE RELATIVE LINE NUMBER INTO THE REQUIRED FIELDS IN*/ 00140000
*             /* THE MESSAGE TEXT                                    */ 00141000
*             /*                                                     */ 00142000
*             /*******************************************************/ 00143000
*                                                               05S0195 00144000
*SUBORDIN:                                                      05S0195 00145000
*             DO;                   /* SUBCASE                       */ 00146000
SUBORDIN DS    0H                                               05S0196 00147000
*               IF OCOPTCDE=OCHOLD  /* IF THE COMMAND OPERATION CODE    00148000
*                                      IS A HOLD COMMAND             */ 00149000
*                 THEN                                          05S0196 00150000
         CLI   OCOPTCDE(ROPCE),X'6C'                            05S0196 00151000
         BNE   @RF00196                                         05S0196 00152000
*                 DO;                                           05S0197 00153000
*                   OPTCMD(1:LENGTH(OPTHELD))=OPTHELD;/* SOME   05S0198 00154000
*                                      SUBORDINATES WERE HELD        */ 00155000
         L     @01,OPCWORK(,ROPCAVT)                            05S0198 00156000
         MVC   OPTCMD(4,@01),@CC01038                           05S0198 00157000
*                 END;                                          05S0199 00158000
*               ELSE                                            05S0200 00159000
*                 OPTCMD=OPTRELSE;  /* SOME SUBORDINATES WERE   05S0200 00160000
*                                      RELEASED                      */ 00161000
         B     @RC00196                                         05S0200 00162000
@RF00196 L     @01,OPCWORK(,ROPCAVT)                            05S0200 00163000
         MVC   OPTCMD(8,@01),@CC01040                           05S0200 00164000
*               GRPULNME=OCBACKUP;  /* PLACE SUBORDINATE NAME INTO      00165000
*                                      MESSAGE TEXT                  */ 00166000
@RC00196 L     @11,OPCWORK(,ROPCAVT)                            05S0201 00167000
         MVC   GRPULNME(8,@11),OCBACKUP(ROPCE)                  05S0201 00168000
*               CVD(OCRLN,OPCDOUBL);                            05S0202 00169000
         SLR   @01,@01                                          05S0202 00170000
         IC    @01,OCRLN(,ROPCE)                                05S0202 00171000
         CVD   @01,OPCDOUBL(,ROPCAVT)                           05S0202 00172000
*               UNPK(OPCDOUBL,OPCDOUBL);                        05S0203 00173000
         UNPK  OPCDOUBL(8,ROPCAVT),OPCDOUBL(8,ROPCAVT)          05S0203 00174000
*               OPCDOUBL(8:8)=OPCDOUBL(8:8)|ZONEMASK;           05S0204 00175000
         OI    OPCDOUBL+7(ROPCAVT),C'0'                         05S0204 00176000
*               IF OCRLN=NORLN THEN /* DO WE WANT RLN        @YM07016*/ 00177000
         CLI   OCRLN(ROPCE),0                                   05S0205 00178000
         BNE   @RF00205                                         05S0205 00179000
*                 MSGLNGTH=MSGLNGTH-MFOUR;/* IF NOT SHORTEN MSG 05S0206 00180000
*                                                            @YM07016*/ 00181000
         LH    @01,MSGLNGTH(,@11)                               05S0206 00182000
         SL    @01,@CF00327                                     05S0206 00183000
         STH   @01,MSGLNGTH(,@11)                               05S0206 00184000
*               ELSE                                            05S0207 00185000
*                 RLNMBR=OPCDOUBL(6:8);/* PLACE RELATIVE LINE NUMBER    00186000
*                                      INTO MESSAGE TEXT             */ 00187000
         B     @RC00205                                         05S0207 00188000
@RF00205 L     @01,OPCWORK(,ROPCAVT)                            05S0207 00189000
         MVC   RLNMBR(3,@01),OPCDOUBL+5(ROPCAVT)                05S0207 00190000
*               GOTO CE0001@;       /* BRANCH TO END OF CASE         */ 00191000
         B     CE0001@                                          05S0208 00192000
*             END;                  /* END OF SUBCASE                */ 00193000
*                                                               05S0210 00194000
*             /*******************************************************/ 00195000
*             /*                                                     */ 00196000
*             /* PROCESS.BUILDMSG.CASE.ALLSUBOR: THIS SEGMENT PLACES */ 00197000
*             /* THE HOLD OR RELEASE COMMAND, THE RESOURCE NAME,AND  */ 00198000
*             /* THE RELATIVE LINE NUMBER INTO THE REQUIRED FIELDS IN*/ 00199000
*             /* THE MESSAGE TEXT                                    */ 00200000
*             /*                                                     */ 00201000
*             /*******************************************************/ 00202000
*                                                               05S0210 00203000
*ALLSUBOR:                                                      05S0210 00204000
