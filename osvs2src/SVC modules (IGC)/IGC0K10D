*         DO;                       /* PREPARE TO TRANSFER CONTROL TO   00382000
*                                      THE NEXT MESSAGE MODULE TO       00383000
*                                      CONTINUE THE SEARCH           */ 00384000
*           SEARCH=STOP;            /* INDICATE THAT THE SEARCH IS TO   00385000
*                                      BE STOPPED                    */ 00386000
         NI    SEARCH,B'01111111'                               02S0255 00387000
*           OPCMODID='0L';          /* SET MODULE IDENTIFICATION        00388000
*                                      CHARACTERS FOR THE NEXT  02S0256 00389000
*                                      MESSAGE MODULE IN THE CHAIN   */ 00390000
         MVC   OPCMODID(2,ROPCAVT),@CC01169                     02S0256 00391000
*         END;                                                  02S0257 00392000
*   END;                                                        02S0258 00393000
@RF00253 DS    0H                                               02S0258 00394000
@RC00185 AL    PTRENTRY,@CF00032                                02S0258 00395000
@DE00184 TM    SEARCH,B'10000000'                               02S0258 00396000
         BO    @DL00184                                         02S0258 00397000
