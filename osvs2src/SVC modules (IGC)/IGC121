*       /*                                                           */ 00123000
*       /*************************************************************/ 00124000
*                                                                  0236 00125000
*       GO TO ERRETURN;             /*                       @ZA26357*/ 00126000
         B     ERRETURN                                            0236 00127000
*CHKIOMB:                           /* CHECK IOMB VALIDITY   @ZA26357*/ 00128000
*                                                                  0237 00129000
*       /*************************************************************/ 00130000
*       /*                                                           */ 00131000
*       /* RW2 POINTS TO VALID AMBL.                         @ZA26357*/ 00132000
*       /* RW3 POINTS TO VALID AMB.                          @ZA26357*/ 00133000
*       /*                                                           */ 00134000
*       /*************************************************************/ 00135000
*                                                                  0237 00136000
*       RESPECIFY                                                  0237 00137000
*         IDAAMB BASED(RW3);        /*                       @ZA26357*/ 00138000
CHKIOMB  DS    0H                                                  0238 00139000
*       IF AMBLSR=OFF&              /* IF NOT SHR RES        @ZA26357*/ 00140000
*           AMBGSR=OFF THEN         /*                       @ZA26357*/ 00141000
         TM    AMBLSR(RW3),B'01100000'                             0238 00142000
         BNZ   @RF00238                                            0238 00143000
*         DO;                       /*                       @ZA26357*/ 00144000
*           RW4=AMBIOMB;            /* LOAD FIRST IOMB       @ZA26357*/ 00145000
         L     RW4,AMBIOMB(,RW3)                                   0240 00146000
*           DO WHILE RW4^=ZERO;     /* LOOP THRU IOMB        @ZA26357*/ 00147000
         B     @DE00241                                            0241 00148000
@DL00241 DS    0H                                                  0242 00149000
*             IF RIOMB=RW4 THEN     /* OUR IOMB ON CHAIN     @ZA26357*/ 00150000
         CR    RIOMB,RW4                                           0242 00151000
         BE    @RT00242                                            0242 00152000
*               GO TO ALLVALID;     /* IOMB VALID            @ZA26357*/ 00153000
*             RW4=RW4->IOMNXT1;     /* LOAD NEXT IOMB        @ZA26357*/ 00154000
         L     RW4,IOMNXT1(,RW4)                                   0244 00155000
*           END;                                                   0245 00156000
@DE00241 LTR   RW4,RW4                                             0245 00157000
         BNZ   @DL00241                                            0245 00158000
*ERRETURN:                                                         0246 00159000
*           R2=EIGHT;               /* REASON CODE 8 INTO REGISTER   */ 00160000
ERRETURN LA    R2,8                                                0246 00161000
*           DO;                     /* ABEND(377) DUMP SYSTEM        */ 00162000
*             RESPECIFY                                            0248 00163000
*              (GPR01F) RESTRICTED;                                0248 00164000
*             GPR01F=((377)&'00000FFF'X)*4096;/* COMP CODE IN BITS 0249 00165000
*                                      8-19                          */ 00166000
         L     GPR01F,@CF03965                                     0249 00167000
*             GPR01F=GPR01F|'80000000'X;/* DUMP/STEP/DUMPOPTS FLAG   */ 00168000
         O     GPR01F,@CF00094                                     0250 00169000
*             SVC(13);              /* ISSUE ABEND SVC               */ 00170000
         SVC   13                                                  0251 00171000
*             RESPECIFY                                            0252 00172000
*              (GPR01F) UNRESTRICTED;                              0252 00173000
*           END;                    /* ABEND(377) DUMP SYSTEM      0253 00174000
*                                      AMB/IOMB INVALID      @ZA26357*/ 00175000
*         END;                      /*                       @ZA26357*/ 00176000
*     END VALIDCBS;                 /*                       @ZA26357*/ 00177000
@RF00238 DS    0H                                                  0256 00178000
*   RESPECIFY                                                      0256 00179000
*     RW4 UNRESTRICTED;             /*                       @ZA26357*/ 00180000
@RF00222 DS    0H                                                  0257 00181000
*ALLVALID:                          /*                       @ZA26357*/ 00182000
*   IOMBVAL=ON;                     /* IOMBADR IS VALID      @ZA26357*/ 00183000
ALLVALID OI    IOMBVAL(R6),B'10000000'                             0257 00184000
*   RESPECIFY                                                      0258 00185000
*     R6 UNRESTRICTED;              /*                       @ZA33053*/ 00186000
*                                                                  0258 00187000
*   /*****************************************************************/ 00188000
*   /*                                                               */ 00189000
*   /* RW2 POINTS TO VALID AMBL.                             @ZA26357*/ 00190000
*   /* RW3 POINTS TO VALID AMB.                              @ZA26357*/ 00191000
*   /* RIOMB POINTS TO VALID IOMB.                           @ZA26357*/ 00192000
*   /*                                                               */ 00193000
*   /*****************************************************************/ 00194000
*                                                                  0259 00195000
*   IF PLHVAMB=ON THEN              /* IF PLHAMB ADR VALID   @ZA26357*/ 00196000
         L     @15,IOMPLH(,RIOMB)                                  0259 00197000
         TM    PLHVAMB(@15),B'00100000'                            0259 00198000
         BNO   @RF00259                                            0259 00199000
*     DO;                           /* RPL IS PRESENT.       @ZA26357*/ 00200000
*       IF(PLHCRPL->RPLREQ^=RPLPFMTD&/* IS THIS RPL FOR      @ZA26357*/ 00201000
*           PLHCRPL->RPLREQ^=RPLPFMTI)|/* PREFORMAT FROM     @ZA26357*/ 00202000
*           AMBLCOMP^=EOVBUSY THEN  /* EOV?                  @ZA26357*/ 00203000
         L     @15,PLHCRPL(,@15)                                   0261 00204000
         CLI   RPLREQ(@15),8                                       0261 00205000
         BE    @GL00002                                            0261 00206000
         CLI   RPLREQ(@15),9                                       0261 00207000
         BNE   @RT00261                                            0261 00208000
@GL00002 CLI   AMBLCOMP(RW2),X'FF'                                 0261 00209000
         BE    @RF00261                                            0261 00210000
@RT00261 DS    0H                                                  0262 00211000
*         DO;                       /* NO, LOCK OUT EOV      @ZA26357*/ 00212000
*           RESPECIFY                                              0263 00213000
*            (R15) RESTRICTED;      /*                       @ZA26357*/ 00214000
*           TS(AMBAMBXN->AMBXRSC);  /* TRY TO GET AMB        @ZA26357*/ 00215000
