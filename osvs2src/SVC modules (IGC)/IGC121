*           BC(8,GOTAMB);           /* BR IF GOT AMB         @ZA26357*/ 00218000
         BC    8,GOTAMB                                            0265 00219000
*           GEN SETS(RW1,RW2,RW3,FRRSCURR,FRRSELEN)/*        @ZA26357*/ 00220000
*               REFS(PSACSTK,FRRS,FRRSEMP)/* REMOVE FRR      @ZA26357*/ 00221000
*               (SETFRR  D,WRKREGS=(RW2,RW3));/*             @ZA26357*/ 00222000
         SETFRR  D,WRKREGS=(RW2,RW3)                                    00223000
*           R15=DREAMB;             /* DEFER REQ TO GET AMB  @ZA26357*/ 00224000
         LA    R15,12                                              0267 00225000
*           RETURN;                 /* EXIT MODULE           @ZA26357*/ 00226000
@EL00001 DS    0H                                                  0268 00227000
@EF00001 DS    0H                                                  0268 00228000
@ER00001 BR    @14                                                 0268 00229000
*GOTAMB:                            /*                       @ZA26357*/ 00230000
*           IF IOMLOCK^=ZERO THEN   /* IF IOMB LOCKED BY EOV @ZA26357*/ 00231000
GOTAMB   CLI   IOMLOCK(RIOMB),0                                    0269 00232000
         BE    @RF00269                                            0269 00233000
*             DO;                   /* MUST DEFER REQUEST.   @ZA26357*/ 00234000
*               AMBAMBXN->AMBXRSC=0;/* FREE THE AMB          @ZA26357*/ 00235000
         L     @10,AMBAMBXN(,RW3)                                  0271 00236000
         MVI   AMBXRSC(@10),X'00'                                  0271 00237000
*               GEN SETS(RW1,RW2,RW3,FRRSCURR,FRRSELEN)/*    @ZA26357*/ 00238000
*                   REFS(PSACSTK,FRRS,FRRSEMP)/* REMOVE FRR  @ZA26357*/ 00239000
*                   (SETFRR  D,WRKREGS=(RW2,RW3));/*         @ZA26357*/ 00240000
         SETFRR  D,WRKREGS=(RW2,RW3)                                    00241000
*               R15=DREQPR;         /* INDICATE IOMB DEFER   @ZA26357*/ 00242000
         LA    R15,4                                               0273 00243000
*               RETURN;             /* RETURN FOR DEFERRAL   @ZA26357*/ 00244000
         B     @EL00001                                            0274 00245000
*             END;                  /* END OF IOMB LOCKED    @ZA26357*/ 00246000
*           AMBAMBXN->AMBXRSC=0;    /* FREE THE AMB          @ZA26357*/ 00247000
@RF00269 L     @10,AMBAMBXN(,RW3)                                  0276 00248000
         MVI   AMBXRSC(@10),X'00'                                  0276 00249000
*           RESPECIFY                                              0277 00250000
*            (R15) UNRESTRICTED;    /*                       @ZA26357*/ 00251000
*         END;                      /* END OF NON-EOV CODE   @ZA26357*/ 00252000
*     END;                          /*                       @ZA26357*/ 00253000
@RF00261 DS    0H                                                  0280 00254000
*                                                                  0280 00255000
*/********************************************************************/ 00256000
*/*             TEST IF EOV IN PROGRESS                              */ 00257000
*/********************************************************************/ 00258000
*                                                                  0280 00259000
*TESTEOV:                                                          0280 00260000
*                                                                  0280 00261000
*   /*****************************************************************/ 00262000
*   /*                                                               */ 00263000
*   /* TEST IF EOV PROCESSING COMPLETED                              */ 00264000
*   /*                                                               */ 00265000
*   /*****************************************************************/ 00266000
*                                                                  0280 00267000
*   RESPECIFY                                                      0280 00268000
*    (R15) RSTD;                                                   0280 00269000
@RF00259 DS    0H                                                  0280 00270000
TESTEOV  DS    0H                                                  0281 00271000
