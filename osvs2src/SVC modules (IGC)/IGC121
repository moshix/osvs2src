*   IF AMBGSR=ON THEN               /* IF GLOBAL SHR         @ZA26357*/ 00313000
         L     @06,IOMAMB(,RIOMB)                                  0293 00314000
         TM    AMBGSR(@06),B'00100000'                             0293 00315000
         BNO   @RF00293                                            0293 00316000
*     DO;                           /*                       @ZA26357*/ 00317000
*       SRBASCB=ADDR(ASCB);         /* SET PTR TO CUR ACSB   @ZA26357*/ 00318000
         L     @06,PSAAOLD                                         0295 00319000
         ST    @06,SRBASCB(,@10)                                   0295 00320000
*       SRBPASID=ASCBASID;          /* SET ASID              @ZA26357*/ 00321000
         LH    @06,ASCBASID(,@06)                                  0296 00322000
         N     @06,@CF03980                                        0296 00323000
         STH   @06,SRBPASID(,@10)                                  0296 00324000
*       SRBPARM=ADDR(IOSB);         /* PARM IS IOSB          @ZA26357*/ 00325000
         ST    IOSBPTR,SRBPARM(,@10)                               0297 00326000
*     END;                          /*                       @ZA26357*/ 00327000
*   IQETCB=R4;                      /* TCB ADDRESS IN IQE    @ZA26357*/ 00328000
@RF00293 L     @10,IOMIQE(,RIOMB)                                  0299 00329000
         ST    R4,IQETCB(,@10)                                     0299 00330000
*   RESPECIFY                                                      0300 00331000
*    (R4,                                                          0300 00332000
