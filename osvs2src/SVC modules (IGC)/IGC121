RBPRFLNA EQU 32                                                         00007000
PRBLEN   EQU 136                                                        00008000
SIRBLEN  EQU 200                                                        00009000
TIRBLEN  EQU 136                                                        00010000
IRBLEN   EQU 128                                                        00011000
SVRBLEN  EQU 224                                                        00012000
IGC121   CSECT                                                          00013000
          BALR  @11,0                    ESTABLISH BASE REGISTER VALUE  00014000
          USING *,@11                    BASE REGISTER IS R11           00015000
          B     STARTCOD                 BYPASS DC AND EQU STATEMENTS   00016000
          DC    AL1(18)                  LENGTH OF ID          @Z40TSDT 00017000
          DC    C' IGC121 '              MODULE IDENTIFIER     @Z40TSDT 00018000
          DC    C' &SYSDATE '            DATE OF COMPILATION   @Z40TSDT 00019000
*STARTCOD:                                                         0205 00020000
*   RIOMB=R1;                       /* IOMB PTR TO RIOMB             */ 00021000
STARTCOD LR    RIOMB,R1                                            0205 00022000
*   RBUFC=R15;                      /* BUFC ADDRESS IN RBUFC REG     */ 00023000
         LR    RBUFC,R15                                           0206 00024000
*   RESPECIFY                                                      0207 00025000
*    (R0,                                                          0207 00026000
*     R1,                                                          0207 00027000
*     R15) UNRSTD;                                                 0207 00028000
*                                                                  0207 00029000
*/*                                                                  */ 00030000
*/********************************************************************/ 00031000
*/*             ESTABLISH EXISTENCE OF AN FRR                        */ 00032000
*/********************************************************************/ 00033000
*                                                                  0208 00034000
*   RESPECIFY                                                      0208 00035000
*    (R6,                                                          0208 00036000
*     RW2,                                                         0208 00037000
*     RW3) RSTD;                    /*                       @ZA33053*/ 00038000
*   R6=ADDR(IDA121F1);              /* ADDRESS OF FRR IN REG ZA33053 */ 00039000
         LA    R6,IDA121F1                                         0209 00040000
*   GEN SETS(R6,RW2,RW3,FRRSCURR,FRRSFRRA,FRRSPARM) REFS(PSACSTK,  0210 00041000
*       PSALSFCC,FRRS,FRRSENTR,FRRSLAST,FRRSELEN)                  0210 00042000
*       (SETFRR  A,FRRAD=(R6),WRKREGS=(RW2,RW3),PARMAD=(R6));/*    0210 00043000
*                                                            @ZA33053*/ 00044000
         SETFRR  A,FRRAD=(R6),WRKREGS=(RW2,RW3),PARMAD=(R6)             00045000
*   IOMBADR=RIOMB;                  /* IOMB PTR INTO PARM FIELD      */ 00046000
         ST    RIOMB,IOMBADR(,R6)                                  0211 00047000
*   IOMBVAL=OFF;                    /* IOMBADR MAY BE BAD    @ZA26357*/ 00048000
         NI    IOMBVAL(R6),B'01111111'                             0212 00049000
*   BASEREG=BASE11;                                                0213 00050000
         ST    BASE11,BASEREG(,R6)                                 0213 00051000
*                                                                  0214 00052000
*/********************************************************************/ 00053000
*/*             VALIDITY CHECK AMB AND IOMB CONTROL BLOCKS           */ 00054000
*/********************************************************************/ 00055000
*                                                                  0214 00056000
*   RESPECIFY                       /*                       @ZA32639*/ 00057000
*     IDAAMB BASED(RW3);            /*                       @ZA32639*/ 00058000
*   IF PLHVAMB=ON THEN              /* IF PLHAMB IS VALID    @ZA32639*/ 00059000
         L     @15,IOMPLH(,RIOMB)                                  0215 00060000
         TM    PLHVAMB(@15),B'00100000'                            0215 00061000
         BNO   @RF00215                                            0215 00062000
*     RW3=PLHAMB;                   /* USE PLHAMB POINTER    @ZA32639*/ 00063000
         L     RW3,PLHAMB(,@15)                                    0216 00064000
*   ELSE                            /* PLHAMB IS NOT VALID   @ZA32639*/ 00065000
*     RW3=IOMAMB;                   /* MUST USE IOMAMB PTR   @ZA32639*/ 00066000
         B     @RC00215                                            0217 00067000
@RF00215 L     RW3,IOMAMB(,RIOMB)                                  0217 00068000
*   RW2=AMBPAMBL;                   /* AMBL PTR FROM AMB     @ZA32639*/ 00069000
@RC00215 L     RW2,AMBPAMBL(,RW3)                                  0218 00070000
*   RESPECIFY                                                      0219 00071000
*     RW1 RSTD;                     /*                       @ZA33053*/ 00072000
*   RW1=RBPSWKEY;                   /* USER KEY FROM PSW IN RB       */ 00073000
         L     @15,PSATOLD                                         0220 00074000
         L     @15,TCBRBP(,@15)                                    0220 00075000
         SLR   RW1,RW1                                             0220 00076000
         IC    RW1,RBPSWKEY(,@15)                                  0220 00077000
*   RW1=RW1&KEYMASK;                /* ZERO THE LAST FOUR BITS       */ 00078000
         N     RW1,@CF00096                                        0221 00079000
*   IF RW1>=KEY8 THEN               /* CALLER NOT IN PRIVLEGED MODE  */ 00080000
         C     RW1,@CF00092                                        0222 00081000
         BL    @RF00222                                            0222 00082000
*VALIDCBS:                                                         0223 00083000
*     DO;                           /* CHECK VALIDITY OF AMB/IOMB    */ 00084000
VALIDCBS DS    0H                                                  0224 00085000
*       RESPECIFY                                                  0224 00086000
*         RW4 RESTRICTED;           /*                       @ZA26357*/ 00087000
*       RESPECIFY                                                  0225 00088000
*        (IDAAMBL) BASED(RW2);      /*                       @ZA26357*/ 00089000
*       IF AMBLDTA=RW3|             /* IF AMBL POINTS BACK   @Z30SSLC*/ 00090000
*           AMBLIX=RW3 THEN         /* IF AMBL POINTS BACK   @Z30SSLC*/ 00091000
         C     RW3,AMBLDTA(,RW2)                                   0226 00092000
         BE    @RT00226                                            0226 00093000
         C     RW3,AMBLIX(,RW2)                                    0226 00094000
         BNE   @RF00226                                            0226 00095000
@RT00226 DS    0H                                                  0227 00096000
*         DO;                       /*                       @Z30SSLC*/ 00097000
*           RW4=JSCBSHR;            /* POINT TO FIRST VAT    @Z30SSLC*/ 00098000
         L     @15,PSATOLD                                         0228 00099000
         L     @15,TCBJSCB(,@15)                                   0228 00100000
         L     RW4,JSCBSHR(,@15)                                   0228 00101000
*           RESPECIFY                                              0229 00102000
*             IDAVAT BASED(RW4);    /*                       @Z30SSLC*/ 00103000
*           DO UNTIL RW4=0;         /* LOOP THRU VAT CHAIN   @Z30SSLC*/ 00104000
@DL00230 DS    0H                                                  0231 00105000
*             IF VATAMBL(AMBLENO)=ADDR(IDAAMBL) THEN/* PTS BACK      */ 00106000
         SLR   @15,@15                                             0231 00107000
         IC    @15,AMBLENO(,RW2)                                   0231 00108000
         SLA   @15,2                                               0231 00109000
         C     RW2,VATAMBL-4(@15,RW4)                              0231 00110000
         BE    @RT00231                                            0231 00111000
*               GO TO CHKIOMB;      /* CHECK IOMB            @Z30SSLC*/ 00112000
*             RW4=VATNEXT;          /* POINT TO NEXT VAT     @Z30SSLC*/ 00113000
         L     RW4,VATNEXT(,RW4)                                   0233 00114000
*           END;                    /*                       @Z30SSLC*/ 00115000
@DE00230 LTR   RW4,RW4                                             0234 00116000
         BNZ   @DL00230                                            0234 00117000
*         END;                      /*                       @Z30SSLC*/ 00118000
*                                                                  0235 00119000
*       /*************************************************************/ 00120000
*       /*                                                           */ 00121000
