*   PLMVLNTH=LENGTH(XINSMODE);      /* SET LENGTH FOR MOVE           */ 00462000
         MVC   PLMVLNTH(2,@09),@CH00045                            0554 00463000
*   PLMVTCTG=ON;                    /* INDICATE TARG CONTIGUOUS      */ 00464000
