*   CALL IEDIAP04;                  /* CONVERT TNT TO NET ADDR       */ 00221000
         L     @09,AVTSAVTP(,RAVT)                                 0502 00222000
         L     @15,SAVTTNTX(,@09)                                  0502 00223000
         BALR  @14,@15                                             0502 00224000
*   XCINCODE=CD1XCIN;               /* SET UP COMMAND CODE           */ 00225000
         L     @09,WKCINRU(,RWORK)                                 0503 00226000
         MVC   XCINCODE(3,@09),@CB00835                            0503 00227000
*   XCINFMT=CINITFMT;               /* SET UP FORMAT                 */ 00228000
         MVI   XCINFMT(@09),X'00'                                  0504 00229000
*   XCINORG=CINITORG;               /* SET UP INITIATE ORIGIN        */ 00230000
         MVI   XCINORG(@09),X'20'                                  0505 00231000
*   XCINADDR=RF;                    /* SET UP SLU NETWORK ADDR       */ 00232000
         STCM  RF,3,XCINADDR(@09)                                  0506 00233000
*/*                                                                  */ 00234000
*                                                                  0507 00235000
