*   TRHDR1=OFF;                     /* TURN OFF RESPONSE BITS        */ 00189000
*   TRHDR2=OFF;                     /* IN INIT-SELF BEFORE           */ 00190000
*   TRHER=OFF;                      /* COPYING THE RH                */ 00191000
