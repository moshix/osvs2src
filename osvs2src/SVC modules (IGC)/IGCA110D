*         WKSIBSW=ON;               /* SET SIB SWITCH ON             */ 00574000
         OI    WKSIBSW(RWORK),B'10000000'                          0579 00575000
*     END;                                                         0580 00576000
*   ELSE                            /* NO SIBS EXIST                 */ 00577000
*     WKSIBSW=ON;                   /* SET SIB SWITCH ON             */ 00578000
         B     @RC00576                                            0581 00579000
