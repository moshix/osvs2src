         BE    @RF00519                                            0519 00329000
@RT00519 DS    0H                                                  0520 00330000
*     OPCMODID=PART2INT;            /* CONTINUE WITH IGCAT10D        */ 00331000
         MVC   OPCMODID(2,ROPCAVT),@CC01680                        0520 00332000
*   ELSE                                                           0521 00333000
*     DO;                                                          0521 00334000
         B     @RC00519                                            0521 00335000
@RF00519 DS    0H                                                  0522 00336000
*       CALL DISCARD;               /* FREE BUFFER                   */ 00337000
