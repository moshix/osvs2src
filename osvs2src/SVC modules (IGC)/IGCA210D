*       WKSENSE=NMODESNS;           /* SET UP SENSE FIELD            */ 00232000
         MVC   WKSENSE(4,RWORK),NMODESNS                           0490 00233000
*     ELSE                          /* NO ERROR CONDITIONS EXISTS    */ 00234000
*       DO;                                                        0491 00235000
         B     @RC00489                                            0491 00236000
@RF00489 DS    0H                                                  0492 00237000
*         WKSENSE=RUERRSNS;         /* ASSUME RU ERROR       @YM07277*/ 00238000
         MVC   WKSENSE(4,RWORK),RUERRSNS                           0492 00239000
