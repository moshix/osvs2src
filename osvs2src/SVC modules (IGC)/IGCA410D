*                                                               04S0454 00131000
*           OCWTGB=OCWTG;           /* SAVE THE OPERATOR CONTROL        00132000
*                                      ELEMENT "WHERE TO GO" FIELD IN   00133000
*                                      THE OPERATOR CONTROL ELEMENT     00134000
*                                      "WHERE TO GO BACK-UP" FIELD   */ 00135000
         MVC   OCWTGB(2,ROPCE),OCWTG(ROPCE)                     04S0454 00136000
*           RPARMREG=1;             /* SET THE NUMBER OF BUFFER UNITS   00137000
*                                      DESIRED TO ONE (1) BUFFER UNIT*/ 00138000
         LA    RPARMREG,1                                       04S0455 00139000
