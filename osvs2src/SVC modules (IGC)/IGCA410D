         BNZ   @RF00475                                         03S0475 00249000
*             OCPARM=OCRCGPOS;      /* INDICATE A GOOD RESPONSE AND     00250000
*                                      THAT NO SENSE IS INCLUDED IN     00251000
*                                      THE RESPONSE UNIT DATA        */ 00252000
         MVC   OCPARM(2,ROPCE),@CH00061                         03S0476 00253000
*           ELSE                    /* SET A NEGATIVE RETURN CODE IN    00254000
*                                      THE OPERATOR CONTROL ELEMENT  */ 00255000
*             OCPARM=OCRCGNEG;      /* INDICATE A GOOD RESPONSE AND     00256000
*                                      THAT SENSE DATA IS INCLUDED IN   00257000
*                                      THE RESPONSE UNIT DATA        */ 00258000
         B     @RC00475                                         03S0477 00259000
@RF00475 MVC   OCPARM(2,ROPCE),@CH00128                         03S0477 00260000
*         END;                                                  03S0478 00261000
*       ELSE                        /* PROCESS THE BAD "ACITVATE        00262000
