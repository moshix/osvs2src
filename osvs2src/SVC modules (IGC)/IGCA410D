*       /*                                                           */ 00096000
*       /* PROCESS.REQUEST: THIS SEGMENT DOES THE PROCESSING FOR AN  */ 00097000
*       /* "ACTIVATE LOGICAL UNIT" REQUEST.                          */ 00098000
*       /*                                                           */ 00099000
*       /*************************************************************/ 00100000
*                                                               03S0450 00101000
*       RPARMREG=REQSGET;           /* SET THE TYPE OF INTERTASK        00102000
*                                      REQUEST TO INDICATE THAT THE     00103000
*                                      REQUEST IS FOR GETTING A 03S0450 00104000
*                                      SESSION INFORMATION CONTROL      00105000
*                                      BLOCK (SIB)                   */ 00106000
         LA    RPARMREG,3                                       03S0450 00107000
*       CALL IPSIBREQ;              /* CALL AN INTERNAL PROCEDURE TO    00108000
*                                      TRY AND OBTAIN A SIB          */ 00109000
         BAL   @14,IPSIBREQ                                     03S0451 00110000
*       IF REQRCD1=OBTAINED         /* TEST FOR A GOOD RETURN CODE      00111000
*                                      FROM THE EXTERNAL POSTWAIT       00112000
*                                      ROUTINE THAT INDICATES THAT A    00113000
*                                      SIB HAS BEEN OBTAINED         */ 00114000
*         THEN                      /* PROCEED WITH NORMAL PROCESSING   00115000
*                                      OF THE "ACTIVATE LOGICAL UNIT"   00116000
*                                      REQUEST                       */ 00117000
