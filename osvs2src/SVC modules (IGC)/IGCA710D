         CS    @09,@05,0(@15)                                   04S0447 00303000
         BNE   @GL00002                                         04S0447 00304000
*             END;                                              04S0448 00305000
*           ELSE                    /* SET A BAD RETURN CODE AND SET    00306000
*                                      THE STATE OF THE LINK AS "LINK   00307000
*                                      RESET"                        */ 00308000
*             DO;                                               04S0449 00309000
         B     @RC00443                                         04S0449 00310000
@RF00443 DS    0H                                               04S0450 00311000
*               OCPARM=OCRCBNEG;    /* SET THE RETURN CODE FIELD IN     00312000
*                                      THE OPERATOR CONTROL ELEMENT  */ 00313000
         MVC   OCPARM(2,ROPCE),@CH00994                         04S0450 00314000
*               TRMLSTAT=TRMLRSET;  /* SET THE STATE OF THE LINK TO     00315000
*                                      "LINK RESET"                  */ 00316000
         LR    @11,RTRM                                         04S0451 00317000
         AL    @11,@CF01491                                     04S0451 00318000
         NI    TRMLSTAT-1(@11),B'11001111'                      04S0451 00319000
*             END;                                              04S0452 00320000
*         END;                                                  03S0453 00321000
@RC00443 DS    0H                                               03S0454 00322000
*       OPCMODID=OCWTGB;            /* SET THE MODULE IDENTIFICATION    00323000
*                                      CHARACTERS FOR THE XCTL MACRO    00324000
*                                      INSTRUCTION FROM THE OPERATOR    00325000
*                                      CONTROL ELEMENT "WHERE TO GO     00326000
*                                      BACK-UP" FIELD                */ 00327000
@RC00435 MVC   OPCMODID(2,ROPCAVT),OCWTGB(ROPCE)                03S0454 00328000
