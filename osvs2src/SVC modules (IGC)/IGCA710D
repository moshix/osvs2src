         L     @15,SAVTTNTX(,@11)                               04S0425 00173000
         BALR  @14,@15                                          04S0425 00174000
*       ACTLNETA=RRETCODE;          /* SET THE NETWORK ADDRESS FIELD    00175000
*                                      IN THE "ACTIVATE LINK" REQUEST   00176000
*                                      UNIT DATA                     */ 00177000
         L     @14,OCUNIT(,ROPCE)                               04S0426 00178000
         STCM  RRETCODE,3,ACTLNETA(@14)                         04S0426 00179000
*       RFY                                                     04S0427 00180000
*         RRETCODE UNRSTD;          /* RELEASE THE RETURN CODE  04S0427 00181000
*                                      REGISTER                      */ 00182000
*       TRMLSTAT=TRMLAPND;          /* SET THE STATE OF THE LINK AS     00183000
*                                      "ACTIVE PENDING"              */ 00184000
         LR    @11,RTRM                                         03S0428 00185000
         AL    @11,@CF01491                                     03S0428 00186000
         NI    TRMLSTAT-1(@11),B'11011111'                      03S0428 00187000
         OI    TRMLSTAT-1(@11),B'00010000'                      03S0428 00188000
*       OCWTGB=OCWTG;               /* SET THE OPERATOR CONTROL 03S0429 00189000
*                                      "WHERE TO GO BACK-UP" FIELD      00190000
*                                      WITH THE MODULE IDENTIFICATION   00191000
*                                      LETTERS FROM THE OPERATOR        00192000
*                                      CONTROL "WHERE TO GO" FIELD   */ 00193000
         MVC   OCWTGB(2,ROPCE),OCWTG(ROPCE)                     03S0429 00194000
*       OCWTG=MODID;                /* SET THE "WHERE TO GO" FIELD TO   00195000
*                                      INDICATE THAT THE RESPONSE TO    00196000
