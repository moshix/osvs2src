         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0405 00068000
*   ROPCE=OPCCOPCE;                 /* ESTABLISH ADDRESSABILITY TO      00069000
*                                      THE INPUT OPERATOR CONTROL       00070000
*                                      ELEMENT                       */ 00071000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0406 00072000
*   RPARMREG=OCTRMTBL;              /* SET UP THE PARAMETER (TERMINAL   00073000
