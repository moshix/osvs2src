         BNZ   @RF00435                                         03S0435 00231000
*         DO;                                                   03S0436 00232000
*           OCPARM=OCRCGPOS;        /* SET THE RETURN CODE FIELD IN     00233000
*                                      THE OPERATOR CONTROL ELEMENT  */ 00234000
         MVC   OCPARM(2,ROPCE),@CH00061                         03S0437 00235000
*           TRMLSTAT=TRMLACTV;      /* SET THE STATE OF THE LINK AS     00236000
*                                      "LINK ACTIVE"                 */ 00237000
