         SL    @11,@CF01488                                     03S0438 00239000
         OI    TRMLSTAT(@11),B'00110000'                        03S0438 00240000
*           TRMRSACT=ON;            /* INDICATE THAT THE RESOURCE IS    00241000
*                                      ACTIVE                @YM03736*/ 00242000
         LA    @15,TRMRSACT(,@11)                               03S0439 00243000
         LR    @14,@15                                          03S0439 00244000
         LA    @09,3                                            03S0439 00245000
         NR    @14,@09                                          03S0439 00246000
         LCR   @14,@14                                          03S0439 00247000
         ALR   @15,@14                                          03S0439 00248000
         L     @09,0(,@15)                                      03S0439 00249000
@GL00001 LR    @05,@09                                          03S0439 00250000
         O     @05,@CB01495+3(@14)                              03S0439 00251000
         CS    @09,@05,0(@15)                                   03S0439 00252000
         BNE   @GL00001                                         03S0439 00253000
*         END;                                                  03S0440 00254000
*       ELSE                        /* DETERMINE THE TYPE OF NEGATIVE   00255000
*                                      RETURN CODE AND SET THE LINK     00256000
*                                      STATE                         */ 00257000
*         DO;                                                   03S0441 00258000
         B     @RC00435                                         03S0441 00259000
@RF00435 DS    0H                                               03S0442 00260000
*                                                               04S0442 00261000
*           /*********************************************************/ 00262000
*           /*                                                       */ 00263000
*           /* PROCESS.RESPONSE.NEGATIVE: THIS SEGMENT DETERMINES THE*/ 00264000
*           /* TYPE OF THE NEGATIVE RESPONSE THAT WAS RECEIVED, SETS */ 00265000
*           /* THE APPROPRIATE RETURN CODE, AND SETS THE STATE OF THE*/ 00266000
*           /* LINK ACCORDINGLY.                                     */ 00267000
*           /*                                                       */ 00268000
*           /*********************************************************/ 00269000
*                                                               04S0442 00270000
*           PTRDATA=PTRDATA+LENGTH(SENSE);/* INDEX POINTER BY THE       00271000
*                                      LENGTH OF THE SENSE DATA      */ 00272000
         AL    PTRDATA,@CF00328                                 04S0442 00273000
*           IF SENSYS=FUNCACT       /* TEST THE SYSTEM SENSE FOR AN     00274000
*                                      INDICATION THAT THE LINK IS      00275000
*                                      ALREADY ACTIVE                */ 00276000
*             THEN                  /* SET A GOOD RETURN CODE AND SET   00277000
*                                      THE STATE OF THE LINK TO "LINK   00278000
*                                      ACTIVE"                       */ 00279000
         L     @11,OCUNIT(,ROPCE)                               04S0443 00280000
         CLC   SENSYS(2,@11),@CB01465                           04S0443 00281000
         BNE   @RF00443                                         04S0443 00282000
*             DO;                                               04S0444 00283000
*               OCPARM=OCRCGNEG;    /* SET THE RETURN CODE FIELD IN     00284000
*                                      THE OPERATOR CONTROL ELEMENT  */ 00285000
         MVC   OCPARM(2,ROPCE),@CH00128                         04S0445 00286000
*               TRMLSTAT=TRMLACTV;  /* SET THE STATE OF THE LINK TO     00287000
*                                      "LINK ACTIVE"                 */ 00288000
         LR    @11,RTRM                                         04S0446 00289000
         SL    @11,@CF01488                                     04S0446 00290000
         OI    TRMLSTAT(@11),B'00110000'                        04S0446 00291000
*               TRMRSACT=ON;        /* INDICATE THAT THE RESOURCE IS    00292000
*                                      ACTIVE                @YM03736*/ 00293000
         LA    @15,TRMRSACT(,@11)                               04S0447 00294000
         LR    @14,@15                                          04S0447 00295000
         LA    @09,3                                            04S0447 00296000
         NR    @14,@09                                          04S0447 00297000
         LCR   @14,@14                                          04S0447 00298000
         ALR   @15,@14                                          04S0447 00299000
         L     @09,0(,@15)                                      04S0447 00300000
@GL00002 LR    @05,@09                                          04S0447 00301000
