         MVC   OPCMODID(2,ROPCAVT),@CC01469                     03S0431 00204000
*     END;                                                      02S0432 00205000
*   ELSE                            /* PROCESS THE "ACTIVATE LINK"      00206000
*                                      RESPONSE                      */ 00207000
*     DO;                                                       02S0433 00208000
         B     @RC00410                                         02S0433 00209000
@RF00410 DS    0H                                               02S0434 00210000
*                                                               03S0434 00211000
*       /*************************************************************/ 00212000
*       /*                                                           */ 00213000
*       /* PROCESS.RESPONSE: THIS SEGMENT DOES THE PROCESSING THAT IS*/ 00214000
*       /* NECESSARY WHEN AN "ACTIVATE LINK" RESPONSE IS RECEIVED.   */ 00215000
*       /*                                                           */ 00216000
*       /*************************************************************/ 00217000
*                                                               03S0434 00218000
*       PTRDATA=ADDR(PRUDATA);      /* ESTABLISH ADDRESSABILITY TO      00219000
*                                      THE RESPONSE UNIT DATA        */ 00220000
         L     @11,OCUNIT(,ROPCE)                               03S0434 00221000
         LA    PTRDATA,PRUDATA(,@11)                            03S0434 00222000
*       IF PRURHSDI=OFF             /* TEST FOR THE ABSENCE OF SENSE    00223000
*                                      DATA                          */ 00224000
*         THEN                      /* SET THE RETURN CODE TO   03S0435 00225000
*                                      INDICATE A POSITIVE RESPONSE     00226000
*                                      AND SET THE STATE OF THE LINK    00227000
*                                      AS "LINK ACTIVE"              */ 00228000
