*     DO;                                                       02S0411 00094000
*                                                               03S0412 00095000
*       /*************************************************************/ 00096000
*       /*                                                           */ 00097000
*       /* PROCESS.REQUEST: THIS SEGMENT DOES THE PROCESSING THAT IS */ 00098000
*       /* NECESSARY WHEN AN "ACTIVATE LINK" REQUEST IS RECEIVED. THE*/ 00099000
*       /* ALLOCBUF EXTERNAL ROUTINE IS USED TO OBTAIN A BUFFER FOR  */ 00100000
*       /* BUILDING THE PACKED REQUEST UNIT. THE PACKED REQUEST UNIT */ 00101000
*       /* IS BUILT, THE LINK STATE IS SET, AND THE APPROPRIATE      */ 00102000
*       /* FIELDS ARE SET IN PREPARATION FOR SENDING OUT THE REQUEST.*/ 00103000
*       /*                                                           */ 00104000
*       /*************************************************************/ 00105000
*                                                               03S0412 00106000
*       RPARMREG=1;                 /* SET THE NUMBER OF UNITS  03S0412 00107000
*                                      DESIRED TO ONE (1) BUFFER UNIT*/ 00108000
         LA    RPARMREG,1                                       03S0412 00109000
*       CALL ALLOCBUF;              /* CALL TO GET A BUFFER UNIT     */ 00110000
         L     @15,OPCGETBF(,ROPCAVT)                           03S0413 00111000
         BALR  @14,@15                                          03S0413 00112000
*       OCUNIT=RPARMREG;            /* ATTACH THE BUFFER UNIT TO THE    00113000
*                                      OPERATOR CONTROL ELEMENT      */ 00114000
         ST    RPARMREG,OCUNIT(,ROPCE)                          03S0414 00115000
*       IEDPRUND=RHPACKTH;          /* SET THE REQUEST HEADER AND THE   00116000
*                                      PACKED RESPONSE UNIT HEADER   */ 00117000
         L     @11,OCUNIT(,ROPCE)                               03S0415 00118000
         LR    @09,@11                                          03S0415 00119000
         AL    @09,@CF01490                                     03S0415 00120000
         XC    IEDPRUND+4(4,@09),IEDPRUND+4(@09)                03S0415 00121000
         MVC   IEDPRUND(4,@09),@CB01475                         03S0415 00122000
*       PRUDATCT=LENGTH(ACTLDATA);  /* SET THE DATA COUNT FIELD TO      00123000
*                                      THE NUMBER OF BYTES IN THE       00124000
*                                      REQUEST UNIT DATA             */ 00125000
         MVC   PRUDATCT(2,@11),@CH00154                         03S0416 00126000
*                                                               04S0417 00127000
*       /*************************************************************/ 00128000
*       /*                                                           */ 00129000
*       /* PROCESS.REQUEST.ACTLBLD: THIS SEGMENT BUILDS THE REQUEST  */ 00130000
*       /* UNIT DATA FOR THE "ACTIVATE LINK" PACKED REQUEST UNIT.    */ 00131000
*       /*                                                           */ 00132000
*       /*************************************************************/ 00133000
*                                                               04S0417 00134000
*       ACTLNETS=CD1NETS;           /* SET THE NETWORK SERVICES 04S0417 00135000
