         TITLE 'IGCAB10D, DEACTIVATE A PHYSICAL UNIT                   *00001000
                        '                                               00002000
IGCAB10D CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
*                                                                  0449 00005000
         USING  *,RBASE                                                 00006000
         LR     RBASE,RF            SET UP ADDRESSIBILITY               00007000
IGCAB10D IEDHJN START,HJN                                               00008000
*/*                                                                  */ 00009000
*                                                                  0450 00010000
*                                   /* INITIALIZATION                */ 00011000
*   ROPCAVT=RPARM;                  /* OP CONTROL AVT ADDRESS        */ 00012000
         LR    ROPCAVT,RPARM                                       0450 00013000
*   ROPCE=OPCCOPCE;                 /* CURRENT ELEMENT ADDRESS       */ 00014000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                            0451 00015000
*   RAVT=OPCAVTPT;                  /* TCAM AVT ADDRESS              */ 00016000
         L     RAVT,OPCAVTPT(,ROPCAVT)                             0452 00017000
*   RWORK=OPCWORK;                  /* OP CONTROL WORK AREA          */ 00018000
         L     RWORK,OPCWORK(,ROPCAVT)                             0453 00019000
*   RSAVE=ADDR(OPCSAVE);            /* OP CONTROL SAVE AREA          */ 00020000
         LA    RSAVE,OPCSAVE(,ROPCAVT)                             0454 00021000
*/*                                                                  */ 00022000
*                                                                  0455 00023000
*   IF OCRESP=OFF THEN              /* REQUEST FOR DACTPU? ...     0455 00024000
*                                      PROCESS                       */ 00025000
         TM    OCRESP(ROPCE),B'00000001'                           0455 00026000
         BNZ   @RF00455                                            0455 00027000
*     CALL DACTPURQ;                                               0456 00028000
*                                                                  0456 00029000
         BAL   @14,DACTPURQ                                        0456 00030000
*/*                                                                  */ 00031000
*                                                                  0457 00032000
*   ELSE                            /* PROCESS THE RESPONSE          */ 00033000
*     DO;                                                          0457 00034000
         B     @RC00455                                            0457 00035000
@RF00455 DS    0H                                                  0458 00036000
*       RPARM=OCTRMTBL;             /* TNT FOR PHYSICAL UNIT         */ 00037000
         LH    RPARM,OCTRMTBL(,ROPCE)                              0458 00038000
         N     RPARM,@CF01603                                      0458 00039000
*       CALL IEDQTNT;               /* GET TTE FOR PU                */ 00040000
         L     @15,AVTRNMPT(,RAVT)                                 0459 00041000
         BALR  @14,@15                                             0459 00042000
*       RTRM=RPARM;                 /* SAVE PHYSICAL UNIT'S TTE      */ 00043000
         LR    RTRM,RPARM                                          0460 00044000
*       WKCMDPTR=ADDR(PRUDATA);     /* SET UP COMMAND POINTER        */ 00045000
         L     @09,OCUNIT(,ROPCE)                                  0461 00046000
         LA    @00,PRUDATA(,@09)                                   0461 00047000
         ST    @00,WKCMDPTR(,RWORK)                                0461 00048000
*/*                                                                  */ 00049000
*                                                                  0462 00050000
*       IF PRURHSDI=ON THEN         /* SENSE INCLUDED                */ 00051000
         AL    @09,@CF01604                                        0462 00052000
         TM    PRURHSDI(@09),B'00000100'                           0462 00053000
         BNO   @RF00462                                            0462 00054000
*         WKCMDPTR=WKCMDPTR+LENGTH(IEDSNSD);/* BUMP COMMAND POINTER     00055000
*                                      PAST SENSE DATA               */ 00056000
         AL    @00,@CF00327                                        0463 00057000
         ST    @00,WKCMDPTR(,RWORK)                                0463 00058000
*       IF COMMAND=CD1DATPU THEN    /* RESPONSE TO DACTPU            */ 00059000
@RF00462 L     @09,WKCMDPTR(,RWORK)                                0464 00060000
         CLI   COMMAND(@09),X'12'                                  0464 00061000
         BNE   @RF00464                                            0464 00062000
*         CALL DACTPURP;            /* PROCESS RESPONSE              */ 00063000
         BAL   @14,DACTPURP                                        0465 00064000
*       ELSE                        /* RESPONSE TO DISCONTACT        */ 00065000
*         CALL FINALRSP;            /* PROCESS RESPONSE              */ 00066000
         B     @RC00464                                            0466 00067000
@RF00464 BAL   @14,FINALRSP                                        0466 00068000
*     END;                                                         0467 00069000
*                                                                  0467 00070000
@RC00464 DS    0H                                                  0468 00071000
*/*                                                                  */ 00072000
*                                                                  0468 00073000
*   RPARM=ROPCAVT;                  /* OP CONTROL AVT ADDRESS        */ 00074000
@RC00455 LR    RPARM,ROPCAVT                                       0468 00075000
*   GEN REFS(ROPCAVT,OPCXCTL) EXIT NOSEQFLOW;                      0469 00076000
         XCTL  SF=(E,OPCXCTL(ROPCAVT))                                  00077000
*DACTPURP:                                                         0470 00078000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0470 00079000
*                                                                  0470 00080000
DACTPURP DS    0H                                                  0471 00081000
*   /*****************************************************************/ 00082000
*   /*                                                               */ 00083000
*   /* THIS ROUTINE PROCESSES A RESPONSE TO DEACTIVATE PHYSICAL UNIT.*/ 00084000
*   /* THE SIB IS FREED AS A RESULT OF DACTPU RESPONSE PROCESSING.   */ 00085000
*   /* FOR AN NCP PU THE RESPONSE IS PROCESSED AS THE FINAL RESPONSE.*/ 00086000
*   /* FOR AN NON-NCP PU A DISCONTACT REQUEST MUST BE SENT, THEN THE */ 00087000
*   /* DISCONTACT RESPONSE IS PROCESSED AS THE FINAL RESPONSE.       */ 00088000
*   /*                                                               */ 00089000
*   /*****************************************************************/ 00090000
*                                                                  0471 00091000
*   WKSAVE1=RE;                     /* SAVE REG 14                   */ 00092000
         ST    RE,WKSAVE1(,RWORK)                                  0471 00093000
*/*                                                                  */ 00094000
*                                                                  0472 00095000
*                                   /* SET UP TO FREE SIB            */ 00096000
*   REQTYPE=REQSFREE;               /* INDICATOR TO FREE SIB         */ 00097000
         LA    @09,12                                              0472 00098000
         ALR   @09,RWORK                                           0472 00099000
         MVI   REQTYPE(@09),X'04'                                  0472 00100000
*   REQQCBA=SAVTXTRQ;               /* POSTWAIT QCB                  */ 00101000
         L     @11,AVTSAVTP(,RAVT)                                 0473 00102000
         MVC   REQQCBA(3,@09),SAVTXTRQ+1(@11)                      0473 00103000
*   REQPRI=PRISIBRQ;                /* FREE SIB PRIORITY             */ 00104000
         MVI   REQPRI(@09),X'E5'                                   0474 00105000
*   REQRQCBA=ADDR(OPCPWQCB);        /* OP COB QCB            @YM08403*/ 00106000
         LA    @00,OPCPWQCB(,ROPCAVT)                              0475 00107000
         STCM  @00,7,REQRQCBA(@09)                                 0475 00108000
*   REQRPRI=PRISIBRQ;               /* RETURN PRIORITY               */ 00109000
         MVI   REQRPRI(@09),X'E5'                                  0476 00110000
*   REQSTTE=RTRM;                   /* TTE FOR THE PU                */ 00111000
         ST    RTRM,REQSTTE(,@09)                                  0477 00112000
*   REQSTNT=SAVTSCPT;               /* SSCP TNT                      */ 00113000
         LH    @00,SAVTSCPT(,@11)                                  0478 00114000
         N     @00,@CF01603                                        0478 00115000
         STH   @00,REQSTNT(,@09)                                   0478 00116000
*   RPARM=ADDR(WKREQELE);           /* ADDRESS OF REQUEST ELE        */ 00117000
         LA    RPARM,WKREQELE(,RWORK)                              0479 00118000
*   CALL POSTWAIT;                  /* FREE THE SIB                  */ 00119000
         L     @15,OPCPSTWT(,ROPCAVT)                              0480 00120000
         BALR  @14,@15                                             0480 00121000
*/*                                                                  */ 00122000
*                                                                  0481 00123000
*   IF TRMTYPE=TRMLNCP THEN         /* NCP PU ?                      */ 00124000
         LR    @09,IEDQTPTR                                        0481 00125000
         AL    @09,@CF01130                                        0481 00126000
         CLI   TRMTYPE-11(@09),X'0F'                               0481 00127000
         BNE   @RF00481                                            0481 00128000
*     CALL FINALRSP;                /* PROCESS RESPONSE AS FINAL     */ 00129000
         BAL   @14,FINALRSP                                        0482 00130000
*   ELSE                            /* NON-NCP PU                    */ 00131000
*     CALL DISCORQ;                 /* BUILD DISCONTACT REQUEST      */ 00132000
         B     @RC00481                                            0483 00133000
@RF00481 BAL   @14,DISCORQ                                         0483 00134000
*/*                                                                  */ 00135000
*                                                                  0484 00136000
*   RE=WKSAVE1;                     /* RESTORE REG 14                */ 00137000
@RC00481 L     RE,WKSAVE1(,RWORK)                                  0484 00138000
*   END DACTPURP;                   /* RETURN TO CALLER              */ 00139000
@EL00002 DS    0H                                                  0485 00140000
@EF00002 DS    0H                                                  0485 00141000
@ER00002 BR    @14                                                 0485 00142000
*DACTPURQ:                                                         0486 00143000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0486 00144000
*                                                                  0486 00145000
DACTPURQ DS    0H                                                  0487 00146000
*   /*****************************************************************/ 00147000
*   /*                                                               */ 00148000
*   /* THIS ROUTINE PROCESSES THE OPCE THAT REQUESTS A DEACTIVATE    */ 00149000
*   /* PHYSICAL UNIT. A DEACT PU COMMAND IS BUILT IN A "PACKED RU"   */ 00150000
*   /* WHICH IS ATTACHED TO THE OPCE. THE OPCE WITH THE ATTACHED RU  */ 00151000
*   /* IS THEN PASSED TO IGC0210D.                                   */ 00152000
*   /*                                                               */ 00153000
*   /*****************************************************************/ 00154000
*                                                                  0487 00155000
*   WKSAVE1=RE;                     /* SAVE REG 14                   */ 00156000
         ST    RE,WKSAVE1(,RWORK)                                  0487 00157000
*/*                                                                  */ 00158000
*                                                                  0488 00159000
*   RPARM=1;                        /* SET NO OF UNITS TO 1          */ 00160000
         LA    RPARM,1                                             0488 00161000
*   CALL ALLOCBUF;                  /* REQUEST A BUFFER              */ 00162000
         L     @15,OPCGETBF(,ROPCAVT)                              0489 00163000
         BALR  @14,@15                                             0489 00164000
*   OCUNIT=RPARM;                   /* SAVE BUFFER ADDRESS           */ 00165000
         ST    RPARM,OCUNIT(,ROPCE)                                0490 00166000
*   OCWTGB=OCWTG;                   /* SAVE "WHERE TO GO" FIELD      */ 00167000
         MVC   OCWTGB(2,ROPCE),OCWTG(ROPCE)                        0491 00168000
*/*                                                                  */ 00169000
*                                                                  0492 00170000
*                                   /* BUILD COMMAND IN PACKED RU    */ 00171000
*   PRURHTH=SCRHTH;                 /* SET UP RH AND TH              */ 00172000
         L     @09,OCUNIT(,ROPCE)                                  0492 00173000
         LR    @11,@09                                             0492 00174000
         AL    @11,@CF01604                                        0492 00175000
         MVC   PRURHTH(4,@11),@CB01573                             0492 00176000
*   PRUTTCIN=OCTRMTBL;              /* SET UP DESTINATION            */ 00177000
         LH    @00,OCTRMTBL(,ROPCE)                                0493 00178000
         N     @00,@CF01603                                        0493 00179000
         STH   @00,PRUTTCIN(,@09)                                  0493 00180000
*   PRUDATCT=LENGTH(IEDXDPUD);      /* DATA COUNT FIELD              */ 00181000
         MVC   PRUDATCT(2,@09),@CH00127                            0494 00182000
*   XDPUCODE=CD1DATPU;              /* COMMAND CODE                  */ 00183000
         AL    @09,@CF00235                                        0495 00184000
         MVI   XDPUCODE(@09),X'12'                                 0495 00185000
*   XDPUTYPE=FINAL;                 /* TYPE                          */ 00186000
         MVI   XDPUTYPE(@09),X'01'                                 0496 00187000
*/*                                                                  */ 00188000
*                                                                  0497 00189000
*   OCWTG=MODIDAB;                  /* SET RETURN TO THIS MODULE     */ 00190000
         MVC   OCWTG(2,ROPCE),@CC01576                             0497 00191000
*   OPCMODID=MODID02;               /* XCTL TO IGC0210D              */ 00192000
         MVC   OPCMODID(2,ROPCAVT),@CC01578                        0498 00193000
*/*                                                                  */ 00194000
*                                                                  0499 00195000
*   RE=WKSAVE1;                     /* RESTORE REG 14                */ 00196000
         L     RE,WKSAVE1(,RWORK)                                  0499 00197000
*   END DACTPURQ;                   /* RETURN TO CALLER              */ 00198000
@EL00003 DS    0H                                                  0500 00199000
@EF00003 DS    0H                                                  0500 00200000
@ER00003 BR    @14                                                 0500 00201000
*DISCORQ:                                                          0501 00202000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0501 00203000
*                                                                  0501 00204000
DISCORQ  DS    0H                                                  0502 00205000
*   /*****************************************************************/ 00206000
*   /*                                                               */ 00207000
*   /* THIS ROUTINE BUILDS A DISCONTACT COMMAND IN THE DEACT PU      */ 00208000
*   /* RESPONSE BUFFER AND PASSES IT TO IGC0210D. THE CONTACT STATE  */ 00209000
*   /* IS SET TO REFLECT THAT DISCONTACT WAS SENT.                   */ 00210000
*   /*                                                               */ 00211000
*   /*****************************************************************/ 00212000
*                                                                  0502 00213000
*   RFY                                                            0502 00214000
*     RZERO RSTD;                                                  0502 00215000
*   WKSAVE2=RE;                     /* SAVE REG 14                   */ 00216000
         ST    RE,WKSAVE2(,RWORK)                                  0503 00217000
*/*                                                                  */ 00218000
*                                                                  0504 00219000
*   OCRESP=OFF;                     /* INDICATE NOT A RESPONSE       */ 00220000
         NI    OCRESP(ROPCE),B'11111110'                           0504 00221000
*/*                                                                  */ 00222000
*                                                                  0505 00223000
*                                   /* BUILD COMMAND IN PACKED RU    */ 00224000
*   PRURHTH=FMDRHTH;                /* SET UP FMD RH AND TH          */ 00225000
         L     @09,OCUNIT(,ROPCE)                                  0505 00226000
         LR    @11,@09                                             0505 00227000
         AL    @11,@CF01604                                        0505 00228000
         MVC   PRURHTH(4,@11),@CB01571                             0505 00229000
*   PRUDATCT=LENGTH(IEDXDSCD);      /* DATA COUNT FIELD              */ 00230000
         MVC   PRUDATCT(2,@09),@CH00153                            0506 00231000
*   XDSCCODE=CD1XDSC;               /* DISCONTACT COMMAND            */ 00232000
         MVC   XDSCCODE+12(3,@09),@CB00724                         0507 00233000
*/*                                                                  */ 00234000
*                                                                  0508 00235000
*   RPARM=OCTRMTBL;                 /* PU'S TNT                      */ 00236000
         LH    RPARM,OCTRMTBL(,ROPCE)                              0508 00237000
         N     RPARM,@CF01603                                      0508 00238000
*   CALL IEDIAP04;                  /* CONVERT TNT TO NET. ADDR      */ 00239000
         L     @09,AVTSAVTP(,RAVT)                                 0509 00240000
         L     @15,SAVTTNTX(,@09)                                  0509 00241000
         BALR  @14,@15                                             0509 00242000
*   XDSCADDR=RF;                    /* SAVE NETWORK ADDRESS          */ 00243000
         L     @09,OCUNIT(,ROPCE)                                  0510 00244000
         STCM  RF,3,XDSCADDR+12(@09)                               0510 00245000
*/*                                                                  */ 00246000
*                                                                  0511 00247000
*   TRMPCONT=TRMCRPND;              /* CONTACT RESET PND STATE       */ 00248000
         LR    @09,IEDQTPTR                                        0511 00249000
         AL    @09,@CF01606                                        0511 00250000
         NI    TRMPCONT-1(@09),B'11111011'                         0511 00251000
         OI    TRMPCONT-1(@09),B'00000011'                         0511 00252000
*   RPARM=RTRM;                     /* PU'S TTE                      */ 00253000
         LR    RPARM,RTRM                                          0512 00254000
*   DO UNTIL TRMTYPE=TRMLNCP;       /* IS THIS LOCAL NCP             */ 00255000
@DL00513 DS    0H                                                  0514 00256000
*     CALL COHORTLK;                /* GET SUPERIOR RESOURCE         */ 00257000
         L     @15,OPCHORT(,ROPCAVT)                               0514 00258000
         BALR  @14,@15                                             0514 00259000
*     RTRM=RPARM;                   /* SAVE SUPERIOR RESOURCE'S TTE     00260000
*                                      ADDRESS                       */ 00261000
         LR    RTRM,RPARM                                          0515 00262000
*   END;                                                           0516 00263000
@DE00513 LR    @09,IEDQTPTR                                        0516 00264000
         AL    @09,@CF01130                                        0516 00265000
         CLI   TRMTYPE-11(@09),X'0F'                               0516 00266000
         BNE   @DL00513                                            0516 00267000
*   PRUTTCIN=RZERO;                 /* DESTINATION IS LOCAL NCP      */ 00268000
         L     @09,OCUNIT(,ROPCE)                                  0517 00269000
         STH   RZERO,PRUTTCIN(,@09)                                0517 00270000
*/*                                                                  */ 00271000
*                                                                  0518 00272000
*   OCWTG=MODIDAB;                  /* SET RETURN TO THIS MODULE     */ 00273000
         MVC   OCWTG(2,ROPCE),@CC01576                             0518 00274000
*   OPCMODID=MODID02;               /* XCTL TO IGC0210D              */ 00275000
         MVC   OPCMODID(2,ROPCAVT),@CC01578                        0519 00276000
*/*                                                                  */ 00277000
*                                                                  0520 00278000
*   RE=WKSAVE2;                     /* RESTORE REG 14                */ 00279000
         L     RE,WKSAVE2(,RWORK)                                  0520 00280000
*   RFY                                                            0521 00281000
*     RZERO UNRSTD;                                                0521 00282000
*   END DISCORQ;                    /* RETURN TO CALLER              */ 00283000
@EL00004 DS    0H                                                  0522 00284000
@EF00004 DS    0H                                                  0522 00285000
@ER00004 BR    @14                                                 0522 00286000
*FINALRSP:                                                         0523 00287000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0523 00288000
*                                                                  0523 00289000
FINALRSP DS    0H                                                  0524 00290000
*   /*****************************************************************/ 00291000
*   /*                                                               */ 00292000
*   /* THIS ROUTINE PROCESSES A RESPONSE TO DACTPU FOR AN NCP PU OR A*/ 00293000
*   /* RESPONSE TO DISCONTACT FOR A NON-NCP PU. A GOOD RETURN CODE IS*/ 00294000
*   /* SET IN OCPARM TO INDICATE THE TYPE OF RESPONSE RECEIVED.      */ 00295000
*   /*                                                               */ 00296000
*   /*****************************************************************/ 00297000
*                                                                  0524 00298000
*   IF TRMTYPE^=TRMLNCP THEN        /* IS THIS A PHYSICAL UNIT       */ 00299000
         LR    @09,IEDQTPTR                                        0524 00300000
         SL    @09,@CF01602                                        0524 00301000
         CLI   TRMTYPE(@09),X'0F'                                  0524 00302000
         BE    @RF00524                                            0524 00303000
*     TRMPCONT=TRMCRSET;            /* SET CONTACT RESET STATE       */ 00304000
         NI    TRMPCONT(@09),B'11111000'                           0525 00305000
*   IF PRURHSDI=ON THEN             /* SENSE INCLUDED                */ 00306000
@RF00524 L     @09,OCUNIT(,ROPCE)                                  0526 00307000
         AL    @09,@CF01604                                        0526 00308000
         TM    PRURHSDI(@09),B'00000100'                           0526 00309000
         BNO   @RF00526                                            0526 00310000
*     OCPARM=OCRCGNEG;              /* SET GOOD RTN CODE WITH SENSE     00311000
*                                      DATA                          */ 00312000
         MVC   OCPARM(2,ROPCE),@CH00127                            0527 00313000
*   ELSE                            /* NO SENSE DATA INCLUDED        */ 00314000
*     OCPARM=OCRCGPOS;              /* GOOD RTN CODE WITHOUT SENSE   */ 00315000
         B     @RC00526                                            0528 00316000
@RF00526 MVC   OCPARM(2,ROPCE),@CH00060                            0528 00317000
*/*                                                                  */ 00318000
*                                                                  0529 00319000
*   OPCMODID=OCWTGB;                /* SET RETURN TO OP SERVICES     */ 00320000
@RC00526 MVC   OPCMODID(2,ROPCAVT),OCWTGB(ROPCE)                   0529 00321000
*/*                                                                  */ 00322000
*                                                                  0530 00323000
*   END FINALRSP;                   /* RETURN TO CALLER              */ 00324000
@EL00005 DS    0H                                                  0530 00325000
@EF00005 DS    0H                                                  0530 00326000
@ER00005 BR    @14                                                 0530 00327000
*   END IGCAB10D                                                   0531 00328000
*                                                                  0531 00329000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */ 00330000
