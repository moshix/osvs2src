*                                                            @Y17XAWW*/ 00283000
         MVC   OCPARM(2,ROPCE),@CH00127                         04S0453 00284000
*                 TRMLSTAT=TRMLRSET;/* SET STATE OF LINK TO "LINK       00285000
*                                      RESET"                @Y17XAWW*/ 00286000
