*       CMDDATA=CMDDATA&&CMDDATA;   /* ZERO COMMAND FLD      @G36XRRU*/ 00285000
         XC    CMDDATA(13,RPRU),CMDDATA(RPRU)                      0581 00286000
*       PRURHTH=RHTHFID1;           /* SET UP RH/TH FLAGS    @ZM47762*/ 00287000
         MVC   PRURHTH(4,@14),@CB02000                             0582 00288000
