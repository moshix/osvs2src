*             OPERANDF=OPERANDF+NUMSHIFT;/* YES, SHIFT CHARACTER     */ 00222000
         LA    @14,57                                              0562 00223000
         SLR   @11,@11                                             0562 00224000
         IC    @11,OPERANDF(,ROPERAND)                             0562 00225000
         ALR   @14,@11                                             0562 00226000
         STC   @14,OPERANDF(,ROPERAND)                             0562 00227000
*           ELSE                    /* NO                            */ 00228000
*             GOTO ERROR1;          /* ERROR                         */ 00229000
*VALID:                                                            0564 00230000
*           ROPERAND=ROPERAND+ONE;  /* POINT TO NEXT CHARACTER       */ 00231000
