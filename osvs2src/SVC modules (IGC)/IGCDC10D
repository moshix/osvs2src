         BL    @RT00537                                            0537 00144000
         CLI   TRMTYPE(@14),X'0F'                                  0537 00145000
         BNH   @RF00537                                            0537 00146000
@RT00537 DS    0H                                                  0538 00147000
*     DO;                           /*                       @G36XRRU*/ 00148000
*       RZERO=NUM173;               /* INVALID NCP ENTRY     @G36XRRU*/ 00149000
         LA    RZERO,173                                           0539 00150000
*       GOTO ERROR;                 /* WRITE ERROR MSG       @G36XRRU*/ 00151000
