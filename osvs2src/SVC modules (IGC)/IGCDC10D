         L     @15,SAVTTNTX(,@11)                                  0585 00299000
         BALR  @14,@15                                             0585 00300000
*       RFY                                                        0586 00301000
*        (RPARM,                                                   0586 00302000
*         RSAVE) UNRSTD;            /*                       @G36XRRU*/ 00303000
*       CMDNETA=REG15;              /* PUT IT IN REQUEST     @G36XRRU*/ 00304000
         STCM  REG15,3,CMDNETA(RPRU)                               0587 00305000
