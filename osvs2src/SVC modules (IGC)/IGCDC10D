*     DO;                           /* INVALID ENTRY         @G36XRRU*/ 00118000
*       RZERO=INVALMSG;             /* INVALID ENTRY MSG     @G36XRRU*/ 00119000
         LA    RZERO,18                                            0527 00120000
*       GOTO ERROR;                 /* WRITE ERROR MSG       @G36XRRU*/ 00121000
         B     ERROR                                               0528 00122000
*     END;                          /*                       @G36XRRU*/ 00123000
*   ELSE                                                           0530 00124000
*     DO;                           /* VALID ENTRY           @G36XRRU*/ 00125000
@RF00525 DS    0H                                                  0531 00126000
*       OCTRMTBL=REG15;             /* SAVE TTCIN            @G36XRRU*/ 00127000
