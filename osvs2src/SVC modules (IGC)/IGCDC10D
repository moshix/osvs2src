@RT00543 DS    0H                                                  0544 00170000
*         DO;                       /* NO, ERROR             @G36XRRU*/ 00171000
*           RZERO=NUM400;           /* NCP NOT ACTIVE MSG    @G36XRRU*/ 00172000
         LA    RZERO,400                                           0545 00173000
*           GOTO ERROR;             /* WRITE ERROR MSG       @G36XRRU*/ 00174000
         B     ERROR                                               0546 00175000
*         END;                      /*                       @G36XRRU*/ 00176000
*       RFY                                                        0548 00177000
*         RTRM UNRSTD;              /*                       @G36XRRU*/ 00178000
@RF00543 DS    0H                                                  0549 00179000
*       RFY                                                        0549 00180000
*        (RZERO,                                                   0549 00181000
*         RPARM) UNRSTD;            /*                       @G36XRRU*/ 00182000
*                                                                  0550 00183000
*/********************************************************************/ 00184000
*/* EXAMINE STORAGE ADDRESS OPERAND                                  */ 00185000
*/********************************************************************/ 00186000
*                                                                  0550 00187000
*       ROPERAND=ROPERAND+ONE;      /* BUMP OPERAND POINTER PAST   0550 00188000
*                                      COMMA                         */ 00189000
         LA    ROPERAND,1(,ROPERAND)                               0550 00190000
*       RSORCFLD=ROPERAND;          /* SAVE ADDRESS OF OPERAND       */ 00191000
         LR    RSORCFLD,ROPERAND                                   0551 00192000
*       OCBACKUP(ONE:FIVE)=STORADDR(ONE:FIVE);/* SAVE ADDR   @ZM47883*/ 00193000
         MVC   OCBACKUP(5,ROPCE),STORADDR(RSORCFLD)                0552 00194000
*       RCOUNTER=ZERO;              /* INITIALIZE COUNT OF CHAR IN 0553 00195000
*                                      OPERAND                       */ 00196000
         SLR   RCOUNTER,RCOUNTER                                   0553 00197000
*LOOP:                                                             0554 00198000
*       IF OPERAND^=BLANK THEN      /* END OF OPERANDS ?             */ 00199000
LOOP     CLI   OPERAND(ROPERAND),X'40'                             0554 00200000
         BE    @RF00554                                            0554 00201000
*         DO;                       /* NO                            */ 00202000
*           RCOUNTER=RCOUNTER+ONE;  /* INCREMENT COUNT OF CHAR       */ 00203000
