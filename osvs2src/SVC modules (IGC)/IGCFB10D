         TITLE 'IGCFB10D - RVT AND HIERARCHY BUILD - LOAD I            *00001000
                        '                                               00002000
IGCFB10D CSECT ,                                                01S0001 00003000
@PROLOG  BALR  @12,0                                            01S0001 00004000
@PSTART  DS    0H                                               01S0001 00005000
         USING @PSTART,@12                                      01S0001 00006000
*                                   /*                                  00007000
IGCFB10D IEDHJN AFTERHJN,HJN                                            00008000
*                                                               01S0404 00009000
*   /*****************************************************************/ 00010000
*   /*                                                               */ 00011000
*   /* INITIALIZATION                                                */ 00012000
*   /*                                                               */ 00013000
*   /*****************************************************************/ 00014000
*                                                               01S0404 00015000
*   ROPCAVT=RPARM;                  /* SET POINTER TO OP. CTL. AVT   */ 00016000
         LR    ROPCAVT,RPARM                                    01S0404 00017000
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */ 00018000
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0405 00019000
*   ROPCE=OPCCOPCE;                 /* SET POINTER TO CURRENT OPCE   */ 00020000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         01S0406 00021000
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */ 00022000
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          01S0407 00023000
*   RDATA=OPCWORK;                  /* SET DATA POINTER TO OP. CTL.     00024000
*                                      WORK AREA                     */ 00025000
         L     RDATA,OPCWORK(,ROPCAVT)                          01S0408 00026000
*   FLAGS1='00'X;                   /* CLEAR FLAG BYTE 1             */ 00027000
         MVI   FLAGS1(RDATA),X'00'                              01S0409 00028000
*                                                               01S0410 00029000
*   /*****************************************************************/ 00030000
*   /*                                                               */ 00031000
*   /* USING OCTRMTBL, GET NCP TTE ADDR.                             */ 00032000
*   /*                                                               */ 00033000
*   /*****************************************************************/ 00034000
*                                                               01S0410 00035000
*   RPARM=OCTRMTBL;                 /* CONTAINS TTCIN OF NCP TTE     */ 00036000
         LH    RPARM,OCTRMTBL(,ROPCE)                           01S0410 00037000
         N     RPARM,@CF02944                                   01S0410 00038000
*   RBRNCH=AVTRNMPT;                /* ADDR. OF CONVERSION ROUTINE   */ 00039000
         L     RBRNCH,AVTRNMPT(,RAVT)                           01S0411 00040000
*   CALL SUBRTNE;                   /* SUBRTNE BASED ON RBRNCH       */ 00041000
         BALR  @14,@15                                          01S0412 00042000
*   RTRM=RPARM;                     /* COPY TTE ADDRESS              */ 00043000
         LR    RTRM,RPARM                                       01S0413 00044000
*   NCPTTEA=RTRM;                   /* SAVE NCP TTE ADDRESS IN W.A.  */ 00045000
         ST    RTRM,NCPTTEA(,RDATA)                             01S0414 00046000
*   RZERO=TRMNCPIM;                 /* PARM TELLING IEDQTL TO LOOK      00047000
*                                      FOR NCP DEVICE DEPENDENT 01S0415 00048000
*                                      FIELDS                        */ 00049000
         LA    RZERO,8                                          01S0415 00050000
*   RBRNCH=AVTDDFT;                 /* ADDR. OF ROUTINE TO LOCATE       00051000
*                                      DEVICE DEPENDENT FIELDS       */ 00052000
         L     RBRNCH,AVTDDFT(,RAVT)                            01S0416 00053000
*   CALL SUBRTNE;                   /* RETURNS ADDR. IN R15(RRETCODE)*/ 00054000
*                                                               01S0417 00055000
         BALR  @14,@15                                          01S0417 00056000
*   /*****************************************************************/ 00057000
*   /*                                                               */ 00058000
*   /* DEVICE DEPENDENT FIELD OF NCP CONTAINS THE IPL TEXT NAME FOR  */ 00059000
*   /* THE NCP. ADD AN 'R' TO THIS NAME TO OBTAIN THE NAME OF THE    */ 00060000
*   /* LOAD MODULE CONTAINING THE RRT AND MOVE THIS NAME INTO THE    */ 00061000
*   /* PARAMETER LIST FOR THE BLDL MACRO IN ORDER TO LOCATE THE RRT  */ 00062000
*   /* MODULE                                                        */ 00063000
*   /*                                                               */ 00064000
*   /*****************************************************************/ 00065000
*                                                               01S0418 00066000
*   IPLTXTNM=RRETCODE->NCPTXID;     /* STORE IPL TEXT NAME FROM 01S0418 00067000
*                                      DEVICE DEPENDENT FIELD        */ 00068000
         MVI   IPLTXTNM+8(RDATA),C' '                           01S0418 00069000
         MVC   IPLTXTNM(8,RDATA),NCPTXID(RRETCODE)              01S0418 00070000
*   DO BYTEPTR=ADDR(IPLTXTNM) BY 1 WHILE(BYTECHAR^=' ');/* LOCATE       00071000
*                                      FIRST BLANK IN NAME           */ 00072000
         LA    @14,IPLTXTNM(,RDATA)                             01S0419 00073000
         ST    @14,BYTEPTR(,RDATA)                              01S0419 00074000
         B     @DE00419                                         01S0419 00075000
@DL00419 DS    0H                                               01S0420 00076000
*   END;                                                        01S0420 00077000
         LA    @14,1                                            01S0420 00078000
         AL    @14,BYTEPTR(,RDATA)                              01S0420 00079000
         ST    @14,BYTEPTR(,RDATA)                              01S0420 00080000
@DE00419 L     @14,BYTEPTR(,RDATA)                              01S0420 00081000
         CLI   BYTECHAR(@14),C' '                               01S0420 00082000
         BNE   @DL00419                                         01S0420 00083000
*   BYTECHAR='R';                   /* ADD 'R' TO END OF NAME        */ 00084000
         MVI   BYTECHAR(@14),C'R'                               01S0421 00085000
*   BLDLNAME=IPLTXTNM(1:8);         /* STORE IPL TEXT NAME IN BLDL      00086000
*                                      PARM LIST                     */ 00087000
         MVC   BLDLNAME(8,RDATA),IPLTXTNM(RDATA)                01S0422 00088000
*   BLDNTRYN=1;                     /* NUMBER OF BLDL ENTRIES        */ 00089000
         MVC   BLDNTRYN(2,RDATA),@CH00077                       01S0423 00090000
*   BLDNTRYL=LENGTH(BLDLNTRY);      /* LENGTH OF BLDL ENTRY          */ 00091000
         MVC   BLDNTRYL(2,RDATA),@CH00120                       01S0424 00092000
*   RPARM=0;                        /* PARM FOR BLDL                 */ 00093000
         SLR   RPARM,RPARM                                      01S0425 00094000
*   RZERO=ADDR(BLDLIST);            /* ADDR. OF BLDL PARAMETER LIST  */ 00095000
         LA    RZERO,BLDLIST(,RDATA)                            01S0426 00096000
*   GEN REFS(RZERO,RPARM) SETS(RRETCODE,BLDLDATA);              01S0427 00097000
* SYSTEM DEPENDENCY FOR MACRO                                           00098000
         BLDL   (RPARM),(RZERO)                                         00099000
*   IF RRETCODE=0 THEN              /* IF LOAD MODULE LOCATED        */ 00100000
         LTR   RRETCODE,RRETCODE                                01S0428 00101000
         BNZ   @RF00428                                         01S0428 00102000
*     DO;                                                       01S0429 00103000
*                                                               01S0429 00104000
*       /*************************************************************/ 00105000
*       /*                                                           */ 00106000
*       /* LOAD RRT                                                  */ 00107000
*       /*                                                           */ 00108000
*       /*************************************************************/ 00109000
*                                                               01S0430 00110000
*       GEN REFS(BLDLNAME,RDATA);                               01S0430 00111000
*                                   /*                                  00112000
* SYSTEM DEPENDENCY FOR MACROS                                          00113000
         LOAD  DE=BLDLNAME(RDATA)                           @YM02942*/  00114000
*       RRTPTR=RZERO;               /* SAVE RRT ADDR.                */ 00115000
*                                                               01S0431 00116000
         ST    RZERO,RRTPTR(,RDATA)                             01S0431 00117000
*       /*************************************************************/ 00118000
*       /*                                                           */ 00119000
*       /* CHECK RRT HEADER AND DETERMINE STORAGE NEED FOR RVT       */ 00120000
*       /*                                                           */ 00121000
*       /*************************************************************/ 00122000
*                                                               01S0432 00123000
*                                                               02S0432 00124000
*       /*************************************************************/ 00125000
*       /*                                                           */ 00126000
*       /* THIS SEGMENT CHECKS THE FIRST BYTE OF THE RESOURCE        */ 00127000
*       /* RESOLUTION TABLE TO DETERMINE IF IT IS FOR AN NCP6 OR     */ 00128000
*       /* PRE-NCP6. THE APPROPRIATE PROCESSING ROUTINE IS THEN      */ 00129000
*       /* CALLED. IF ANY ERRORS ARE DETECTED IN THE PROCESSING      */ 00130000
*       /* ROUTINE, OCPARM AND RZERO ARE SET WITH THE 'ERROR IN RRT' */ 00131000
*       /* MSG. NO.                                                  */ 00132000
*       /*                                                           */ 00133000
*       /*************************************************************/ 00134000
*                                                               02S0432 00135000
*       IF RRTPTR->BYTECHAR^='00'X THEN/* IF THIS IS AN NCP6 RRT     */ 00136000
         L     @14,RRTPTR(,RDATA)                               02S0432 00137000
         CLI   BYTECHAR(@14),X'00'                              02S0432 00138000
         BE    @RF00432                                         02S0432 00139000
*         DO;                                                   02S0433 00140000
*                                                               02S0433 00141000
*           /*********************************************************/ 00142000
*           /*                                                       */ 00143000
*           /* PROCESS RRT FOR NCP6                                  */ 00144000
*           /*                                                       */ 00145000
*           /*********************************************************/ 00146000
*                                                               02S0434 00147000
*                                                               03S0434 00148000
*           /*********************************************************/ 00149000
*           /*                                                       */ 00150000
*           /* THIS SEGMENT IS EXECUTED WHEN A RESOURCE RESOLUTION   */ 00151000
*           /* TABLE HAS BEEN LOADED INDICATING THE AN NCP6.         */ 00152000
*           /*                                                       */ 00153000
*           /*********************************************************/ 00154000
*                                                               03S0434 00155000
*           RFY                                                 03S0434 00156000
*             RSTSEG BASED(ENTRYPTR);/* SEGMENT HEADER FORMAT        */ 00157000
*           RFY                                                 03S0435 00158000
*             RSTHDR BASED(ENTRYPTR);/* HEADER SEGMENT ENTRY FORMAT  */ 00159000
*           RFY                                                 03S0436 00160000
*             RSTSYSEN BASED(ENTRYPTR);/* SYSTEM SEGMENT ENTRY FORMAT*/ 00161000
*           RFY                                                 03S0437 00162000
*             RSTHOST BASED(ENTRYPTR);/* HOST SEGMENT ENTRY FORMAT   */ 00163000
*           RFY                                                 03S0438 00164000
*             IEDQQCB BASED(TRMDESTQ);/* QCB ADDR.                   */ 00165000
*           RFY                                                 03S0439 00166000
*             IHADCB BASED(QCBDCBAD);/* DCB ADDR.                    */ 00167000
*                                                               03S0440 00168000
*           /*********************************************************/ 00169000
*           /*                                                       */ 00170000
*           /* THE SUBROUTINE SEARCHER IS USED TO OBTAIN             */ 00171000
*           /* ADDRESSABILITY TO THE SYSTEM SEGMENT ENTRY IN THE     */ 00172000
*           /* HEADER SEGMENT                                        */ 00173000
*           /*                                                       */ 00174000
*           /*********************************************************/ 00175000
*                                                               03S0440 00176000
*           SRCH=RSTSYSEG;          /* SET PARM FOR SYSTEM SEG. ENTRY*/ 00177000
         MVI   SRCH(RDATA),X'01'                                03S0440 00178000
*           CALL SEARCHER;          /* SETS ENTRYPTR TO ADDR. OF        00179000
*                                      SYSTEM SEGMENT ENTRY IN HEADER   00180000
*                                      SEGMENT                       */ 00181000
*                                                               03S0441 00182000
         BAL   @14,SEARCHER                                     03S0441 00183000
*           /*********************************************************/ 00184000
*           /*                                                       */ 00185000
*           /* ADDRESSABILITY TO THE SYSTEM SEGMENT ENTRIES IS       */ 00186000
*           /* OBTAINED AND THE THE SUBAREA LENGTH IS OBTAINED       */ 00187000
*           /*                                                       */ 00188000
*           /*********************************************************/ 00189000
*                                                               03S0442 00190000
*           ENTRYPTR=RSTSGOFF+RRTPTR;/* ADD OFFSET OF SYSTEM SEGMENT    00191000
*                                      TO THE RST ADDRESS TO POINT TO   00192000
*                                      SYSTEM SEGMENT                */ 00193000
         L     @14,ENTRYPTR(,RDATA)                             03S0442 00194000
         ICM   @11,15,RSTSGOFF(@14)                             03S0442 00195000
         AL    @11,RRTPTR(,RDATA)                               03S0442 00196000
         ST    @11,ENTRYPTR(,RDATA)                             03S0442 00197000
*           ENTRYPTR=ENTRYPTR+RSTSEGHL;/* ADD SEGMENT HEADER LENGTH     00198000
*                                      TO OBTAIN ADDRESS OF SYSTEM      00199000
*                                      SEGMENT ENTRIES               */ 00200000
         SLR   @14,@14                                          03S0443 00201000
         IC    @14,RSTSEGHL(,@11)                               03S0443 00202000
         ALR   @14,@11                                          03S0443 00203000
         ST    @14,ENTRYPTR(,RDATA)                             03S0443 00204000
*           IF RSTSALTH=SAVTSUBL THEN/* IF SUBAREA LENGTH IN RST IS     00205000
*                                      EQUAL TO LENGTH IN SAVT       */ 00206000
         L     @11,AVTSAVTP(,RAVT)                              03S0444 00207000
         SLR   @09,@09                                          03S0444 00208000
         ICM   @09,3,RSTSALTH(@14)                              03S0444 00209000
         SLR   @14,@14                                          03S0444 00210000
         IC    @14,SAVTSUBL(,@11)                               03S0444 00211000
         CR    @09,@14                                          03S0444 00212000
         BNE   @RF00444                                         03S0444 00213000
*             DO;                                               03S0445 00214000
*                                                               03S0445 00215000
*               /*****************************************************/ 00216000
*               /*                                                   */ 00217000
*               /* SUBAREA LENGTH IS EQUAL. NEXT, USE SEARCHER TO    */ 00218000
*               /* OBTAIN THE ADDR. OF THE HOST SEGMENT ENTRY IN THE */ 00219000
*               /* HEADER SEGMENT.                                   */ 00220000
*               /*                                                   */ 00221000
*               /*****************************************************/ 00222000
*                                                               03S0446 00223000
*               SRCH=RSTHSTSG;      /* PARM INDICATING THAT THE HOST    00224000
*                                      SEGMENT ENTRY SHOULD BE  03S0446 00225000
*                                      LOCATED                       */ 00226000
         MVI   SRCH(RDATA),X'02'                                03S0446 00227000
*               CALL SEARCHER;      /* SETS ENTRYPTR WITH THE ADDR.     00228000
*                                      OF THE HOST SEGMENT ENTRY IN     00229000
*                                      THE HEADER SEGMENT            */ 00230000
         BAL   @14,SEARCHER                                     03S0447 00231000
*               IF ERRFLG=OFF THEN  /* IF NO ERROR WAS ENCOUNTERED   */ 00232000
         TM    ERRFLG(RDATA),B'10000000'                        03S0448 00233000
         BNZ   @RF00448                                         03S0448 00234000
*                 DO;                                           03S0449 00235000
*                                                               03S0449 00236000
*                   /*************************************************/ 00237000
*                   /*                                               */ 00238000
*                   /* LOCATE THE BEGINNING OF THE ENTRIES IN THE    */ 00239000
*                   /* HOST SEGMENT                                  */ 00240000
*                   /*                                               */ 00241000
*                   /*************************************************/ 00242000
*                                                               03S0450 00243000
*                   ENTRYPTR=RRTPTR+RSTSGOFF;/* ADD THE OFFSET OF THE   00244000
*                                      HOST SEGMENT TO THE ADDR. OF     00245000
*                                      THE RST TO OBTAIN        03S0450 00246000
*                                      ADDRESSABILITY TO THE HOST       00247000
*                                      SEGMENT                       */ 00248000
         L     @14,ENTRYPTR(,RDATA)                             03S0450 00249000
         ICM   @11,15,RSTSGOFF(@14)                             03S0450 00250000
         AL    @11,RRTPTR(,RDATA)                               03S0450 00251000
         ST    @11,ENTRYPTR(,RDATA)                             03S0450 00252000
*                   CTR=RSTNOENT;   /* SET COUNTER TO NUMBER OF 03S0451 00253000
*                                      ENTRIES IN HOST SEGMENT       */ 00254000
         SLR   @14,@14                                          03S0451 00255000
         ICM   @14,3,RSTNOENT(@11)                              03S0451 00256000
         ST    @14,CTR(,RDATA)                                  03S0451 00257000
*                   ENTRYSIZ=RSTENTSZ;/* COPY ENTRY SIZE             */ 00258000
         SLR   @14,@14                                          03S0452 00259000
         ICM   @14,3,RSTENTSZ(@11)                              03S0452 00260000
         ST    @14,ENTRYSIZ(,RDATA)                             03S0452 00261000
*                   ENTRYPTR=ENTRYPTR+RSTSEGHL;/* ADD SEGMENT HEADER    00262000
*                                      LENGTH TO OBTAIN ADDR. OF        00263000
*                                      START OF ENTRIES              */ 00264000
*                                                               03S0453 00265000
         SLR   @14,@14                                          03S0453 00266000
         IC    @14,RSTSEGHL(,@11)                               03S0453 00267000
         ALR   @14,@11                                          03S0453 00268000
         ST    @14,ENTRYPTR(,RDATA)                             03S0453 00269000
*                   /*************************************************/ 00270000
*                   /*                                               */ 00271000
*                   /* ATTEMPT TO LOCATE TCAM'S SUBAREA IN HOST      */ 00272000
*                   /* SEGMENT ENTRIES                               */ 00273000
*                   /*                                               */ 00274000
*                   /*************************************************/ 00275000
*                                                               03S0454 00276000
*                   ERRFLG=ON;      /* INIT FOR LOOP                 */ 00277000
         OI    ERRFLG(RDATA),B'10000000'                        03S0454 00278000
*                   DO WHILE(CTR^=0);/* CTR INITIALLY SET TO NUMBER     00279000
*                                      OF ENTRIES                    */ 00280000
         B     @DE00455                                         03S0455 00281000
@DL00455 DS    0H                                               03S0456 00282000
*                     RZERO=SAVTSUBV;/* COPY TCAM SUBAREA            */ 00283000
         L     @14,AVTSAVTP(,RAVT)                              03S0456 00284000
         SLR   RZERO,RZERO                                      03S0456 00285000
         IC    RZERO,SAVTSUBV(,@14)                             03S0456 00286000
*                     ISOSUBA=(8*LENGTH(SAVTSUBV))-SAVTSUBL;/* NO.      00287000
*                                      BITS TO RT. JUSTIFY           */ 00288000
         LA    @11,8                                            03S0457 00289000
         SLR   @09,@09                                          03S0457 00290000
         IC    @09,SAVTSUBL(,@14)                               03S0457 00291000
         SLR   @11,@09                                          03S0457 00292000
         STC   @11,ISOSUBA(,RDATA)                              03S0457 00293000
*                     SRL(RZERO,ISOSUBA);/* RIGHT JUSTIFY SUBAREA    */ 00294000
         SRL   RZERO,0(@11)                                     03S0458 00295000
*                     IF RZERO=RSTHSTSA THEN/* IF THIS IS TCAM'S        00296000
*                                      SUBAREA                       */ 00297000
         L     @14,ENTRYPTR(,RDATA)                             03S0459 00298000
         SLR   @11,@11                                          03S0459 00299000
         ICM   @11,3,RSTHSTSA(@14)                              03S0459 00300000
         CR    RZERO,@11                                        03S0459 00301000
         BNE   @RF00459                                         03S0459 00302000
*                       DO;                                     03S0460 00303000
*                         ERRFLG=OFF;/* TURN OFF ERROR FLAG          */ 00304000
         NI    ERRFLG(RDATA),B'01111111'                        03S0461 00305000
*                         CTR=0;    /* SET TO EXIT LOOP              */ 00306000
         SLR   @14,@14                                          03S0462 00307000
         ST    @14,CTR(,RDATA)                                  03S0462 00308000
*                       END;                                    03S0463 00309000
*                     ELSE                                      03S0464 00310000
*                       DO;                                     03S0464 00311000
         B     @RC00459                                         03S0464 00312000
@RF00459 DS    0H                                               03S0465 00313000
*                         ENTRYPTR=ENTRYPTR+ENTRYSIZ;/* POINT TO NEXT   00314000
*                                      ENTRY                         */ 00315000
         L     @14,ENTRYPTR(,RDATA)                             03S0465 00316000
         AL    @14,ENTRYSIZ(,RDATA)                             03S0465 00317000
         ST    @14,ENTRYPTR(,RDATA)                             03S0465 00318000
*                         CTR=CTR-1;/* DECREMENT LOOP COUNTER        */ 00319000
         L     @14,CTR(,RDATA)                                  03S0466 00320000
         BCTR  @14,0                                            03S0466 00321000
         ST    @14,CTR(,RDATA)                                  03S0466 00322000
*                       END;                                    03S0467 00323000
*                   END;            /* END OF LOOP                   */ 00324000
@RC00459 DS    0H                                               03S0468 00325000
@DE00455 L     @14,CTR(,RDATA)                                  03S0468 00326000
         LTR   @14,@14                                          03S0468 00327000
         BNZ   @DL00455                                         03S0468 00328000
*                   IF ERRFLG=OFF THEN/* IF TCAM'S SUBAREA FOUND     */ 00329000
         TM    ERRFLG(RDATA),B'10000000'                        03S0469 00330000
         BNZ   @RF00469                                         03S0469 00331000
*                     IF(RSTKEYLN=AVTKEYLE)&/* IF NCP AND TCAM UNIT     00332000
*                                      SIZE IS THE SAME              */ 00333000
*                         ((RSTBUFPD>=MINBUFPD)&(RSTBUFPD<=(AVTKEYLE-   00334000
*                         TXTBFPSZ)))/* IF CORRECT PAD SIZE SPECIFIED   00335000
*                                      IN THE RRT                    */ 00336000
*                       THEN                                    03S0470 00337000
         L     @14,ENTRYPTR(,RDATA)                             03S0470 00338000
         LH    @11,AVTKEYLE(,RAVT)                              03S0470 00339000
         SLR   @09,@09                                          03S0470 00340000
         ICM   @09,3,RSTKEYLN(@14)                              03S0470 00341000
         CR    @11,@09                                          03S0470 00342000
         BNE   @RF00470                                         03S0470 00343000
         SLR   @09,@09                                          03S0470 00344000
         ICM   @09,3,RSTBUFPD(@14)                              03S0470 00345000
         C     @09,@CF01873                                     03S0470 00346000
         BL    @RF00470                                         03S0470 00347000
         SL    @11,@CF02913                                     03S0470 00348000
         CR    @09,@11                                          03S0470 00349000
         BH    @RF00470                                         03S0470 00350000
*                       DO;                                     03S0471 00351000
*                         PADSIZE=RSTBUFPD;/* SAVE PADSIZE FOR LATER    00352000
*                                      USE                   @VS44922*/ 00353000
*                                                               03S0472 00354000
         STC   @09,PADSIZE(,RDATA)                              03S0472 00355000
*                         /*******************************************/ 00356000
*                         /*                                         */ 00357000
*                         /* SET UNIT COUNT IN DCB                   */ 00358000
*                         /*                                         */ 00359000
*                         /*******************************************/ 00360000
*                                                               03S0473 00361000
*                         DCBUNTCT=RSTUNITN;/* SET NUMBER OF UNITS IN   00362000
*                                      BUFFER                        */ 00363000
*                                                               03S0473 00364000
         L     @11,TRMDESTQ-1(,RTRM)                            03S0473 00365000
         L     @11,QCBDCBAD-1(,@11)                             03S0473 00366000
         MVC   DCBUNTCT+48(1,@11),RSTUNITN+1(@14)               03S0473 00367000
*                         /*******************************************/ 00368000
*                         /*                                         */ 00369000
*                         /* LOCATE RESOURCE SEGMENT USING SEARCHER  */ 00370000
*                         /*                                         */ 00371000
*                         /*******************************************/ 00372000
*                                                               03S0474 00373000
*                         SRCH=RSTRSORC;/* PARM TO INDICATE RESOURCE    00374000
*                                      SEG.                          */ 00375000
         MVI   SRCH(RDATA),X'04'                                03S0474 00376000
*                         CALL SEARCHER;/* SETS ENTRYPTR TO THE ADDR.   00377000
*                                      OF THE RESOURCE SEGMENT ENTRY    00378000
*                                      IN THE HEADER SEGMENT         */ 00379000
*                                                               03S0475 00380000
         BAL   @14,SEARCHER                                     03S0475 00381000
*                         /*******************************************/ 00382000
*                         /*                                         */ 00383000
*                         /* OBTAIN NUMBER OF ENTRIES AND NUMBER OF  */ 00384000
*                         /* PRE-SNA ENTRIES. ALSO, OBTAIN ADDR. OF  */ 00385000
*                         /* FIRST ENTRY IN THE RESOURCE SEGMENT     */ 00386000
*                         /*                                         */ 00387000
*                         /*******************************************/ 00388000
*                                                               03S0476 00389000
*                         ENTRYPTR=RSTSGOFF+RRTPTR;/* ADD OFFSET OF     00390000
*                                      RESOURCE SEGMENT TO THE RST      00391000
*                                      ADDRESS TO POINT TO RESOURCE     00392000
*                                      SEGMENT                       */ 00393000
         L     @14,ENTRYPTR(,RDATA)                             03S0476 00394000
         ICM   @11,15,RSTSGOFF(@14)                             03S0476 00395000
         AL    @11,RRTPTR(,RDATA)                               03S0476 00396000
         ST    @11,ENTRYPTR(,RDATA)                             03S0476 00397000
*                         ENTRYNUM=RSTNOENT;                    03S0477 00398000
         SLR   @14,@14                                          03S0477 00399000
         ICM   @14,3,RSTNOENT(@11)                              03S0477 00400000
         ST    @14,ENTRYNUM(,RDATA)                             03S0477 00401000
*                         PSNANUM=RSTPSNAN;/* COPY NO. OF PRE-SNA       00402000
*                                      RESOURCES                     */ 00403000
         SLR   @14,@14                                          03S0478 00404000
