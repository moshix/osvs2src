         L     @15,TCXQE8(,@11)                                 04S0541 00686000
         BALR  @14,@15                                          04S0541 00687000
*         RVTINDEX=RRETCODE;        /* PLACE TTCIN (OR 0 IF NAME NOT    00688000
*                                      FOUND) INTO RVT ENTRY         */ 00689000
         L     @14,ENTADDRV(,RDATA)                             04S0542 00690000
         STH   RRETCODE,RVTINDEX(,@14)                          04S0542 00691000
*         IF RVTINDEX^=0 THEN       /* SET RVT ENTRY                 */ 00692000
         L     @14,ENTADDRV(,RDATA)                             04S0543 00693000
         LH    @14,RVTINDEX(,@14)                               04S0543 00694000
         LTR   @14,@14                                          04S0543 00695000
         BZ    @RF00543                                         04S0543 00696000
*           DO;                     /* =0                            */ 00697000
*             RPARM=RVTINDEX;       /* IF NOT                        */ 00698000
         LR    RPARM,@14                                        04S0545 00699000
*             CALL IEDQTNT;         /* 3705 RESOURCE                 */ 00700000
         L     @15,AVTRNMPT(,RAVT)                              04S0546 00701000
         BALR  @14,@15                                          04S0546 00702000
*             RTRM=RPARM;           /* OR                            */ 00703000
         LR    RTRM,RPARM                                       04S0547 00704000
*             IF TRMPREF^=ON|TRMDIAL^=ON|NATODAF^=RRTRSID THEN/* IF     00705000
*                                      NON-DIAL RESOURCE OR ALREADY     00706000
*                                      ON ANOTHER NCP THEN CLEAR RVT    00707000
