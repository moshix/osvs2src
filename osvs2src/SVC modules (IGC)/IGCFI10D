         TITLE 'IGCFI10D - RVT AND HIERARCHY BUILD                     *00001000
                        '                                               00002000
IGCFI10D CSECT ,                                                01S0001 00003000
@PROLOG  BALR  @12,0                                            01S0001 00004000
@PSTART  DS    0H                                               01S0001 00005000
         USING @PSTART,@12                                      01S0001 00006000
*                                   /*                                  00007000
IGCFI10D IEDHJN AFTERHJN,HJN                                            00008000
*                                      ADDRESSABILITY TO NAT ENTRY   */ 00009000
*   ROPCAVT=RPARM;                  /* SET POINTER TO OP. CTL. AVT   */ 00010000
         LR    ROPCAVT,RPARM                                    01S0383 00011000
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */ 00012000
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0384 00013000
*   ROPCE=OPCCOPCE;                 /* SET POINTER TO CURRENT OPCE   */ 00014000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         01S0385 00015000
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */ 00016000
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          01S0386 00017000
*   RDATA=OPCWORK;                  /* SET POINTER TO OP.CTL. WORK      00018000
*                                      AREA                          */ 00019000
         L     RDATA,OPCWORK(,ROPCAVT)                          01S0387 00020000
*   FLAGS1='00'X;                   /* CLEAR FLAG BYTE 1             */ 00021000
         MVI   FLAGS1(RDATA),X'00'                              01S0388 00022000
*   OCADNCP=ON;                     /* FLAG AS F ACT,NCP             */ 00023000
         LR    @14,ROPCE                                        01S0389 00024000
         AL    @14,@CF02814                                     01S0389 00025000
         OI    OCADNCP-2(@14),B'00010000'                       01S0389 00026000
*   OPCADGOT=OPCADGOT+1;            /* INCREMENT OPCADGOT            */ 00027000
         LA    @14,1                                            01S0390 00028000
         SLR   @11,@11                                          01S0390 00029000
         ICM   @11,3,OPCADGOT(ROPCAVT)                          01S0390 00030000
         ALR   @14,@11                                          01S0390 00031000
         STH   @14,OPCADGOT(,ROPCAVT)                           01S0390 00032000
*                                                               02S0391 00033000
*   /*****************************************************************/ 00034000
*   /*                                                               */ 00035000
*   /* THIS SEGMENT CREATES AN ENTRY IN THE SUBAREA TABLE FOR THE    */ 00036000
*   /* CURRENT NCP AND CREATES ENTRIES IN THE NETWORK ADDRESS TABLE  */ 00037000
*   /* FOR THE NCP AND EACH OF ITS RESOURCES.                        */ 00038000
*   /*                                                               */ 00039000
*   /*****************************************************************/ 00040000
*                                                               02S0391 00041000
*   RFY                                                         02S0391 00042000
*     IEDSATD BASED(RWORK);         /* SUBAREA TABLE ENTRY           */ 00043000
*   RFY                                                         02S0392 00044000
*     RWORK RSTD;                                               02S0392 00045000
*   RTRM=NCPTTEA;                   /* SET POINTER TO NCP TTE        */ 00046000
         L     RTRM,NCPTTEA(,RDATA)                             02S0393 00047000
*   DCBPTR=QCBDCBAD;                /* SET POINTER TO DCB            */ 00048000
         ICM   @14,7,TRMDESTQ(RTRM)                             02S0394 00049000
         SLR   @11,@11                                          02S0394 00050000
         ICM   @11,7,QCBDCBAD(@14)                              02S0394 00051000
         ST    @11,DCBPTR(,RDATA)                               02S0394 00052000
*   RWORK=RSCPTR->RRTRSID;          /* LOAD NETWORK ADDR. OF FIRST      00053000
*                                      RESOURCE IN RRT TO OBTAIN        00054000
*                                                             SUBAREA*/ 00055000
         L     @14,RSCPTR(,RDATA)                               02S0395 00056000
         SLR   RWORK,RWORK                                      02S0395 00057000
         ICM   RWORK,3,RRTRSID(@14)                             02S0395 00058000
*   ISOSUBA=(8*LENGTH(RRTRSID))-SAVTSUBL;/* OBTAIN NO. OF BITS TO       00059000
*                                      SHIFT TO ISOLATE SUBAREA      */ 00060000
         L     @09,AVTSAVTP(,RAVT)                              02S0396 00061000
         LA    @07,16                                           02S0396 00062000
         SLR   @06,@06                                          02S0396 00063000
         IC    @06,SAVTSUBL(,@09)                               02S0396 00064000
         SLR   @07,@06                                          02S0396 00065000
         STC   @07,ISOSUBA(,RDATA)                              02S0396 00066000
*   SRL(RWORK,ISOSUBA);             /* ISOLATE SUBAREA               */ 00067000
         SRL   RWORK,0(@07)                                     02S0397 00068000
*   RWORK=SAVTSAT+((RWORK-ADJX)*LENGTH(IEDSATD));/* OBTAIN PTR. TO      00069000
*                                      SAT ENTRY                     */ 00070000
         LR    @07,RWORK                                        02S0398 00071000
         BCTR  @07,0                                            02S0398 00072000
         SLA   @07,3                                            02S0398 00073000
         AL    @07,SAVTSAT(,@09)                                02S0398 00074000
         LR    RWORK,@07                                        02S0398 00075000
*   LCBPTR=DCBIOBAD+DCBEIOBX-LCBPRFSZ;/* SET LCB ADDR. IN ENTRY      */ 00076000
         AH    @11,@CH01349                                     02S0399 00077000
         SLR   @07,@07                                          02S0399 00078000
         IC    @07,DCBEIOBX(,@11)                               02S0399 00079000
         AL    @07,DCBIOBAD(,@11)                               02S0399 00080000
         SH    @07,@CH01853                                     02S0399 00081000
         ST    @07,LCBPTR(,RDATA)                               02S0399 00082000
*   SATRNLCB=LCBPTR;                /* SAVE LCB IN SAT ENTRY         */ 00083000
         ST    @07,SATRNLCB(,RWORK)                             02S0400 00084000
*   SATRNRVT=RVTPTR;                /* SET RVT ADDR. IN ENTRY        */ 00085000
         L     @11,RVTPTR(,RDATA)                               02S0401 00086000
         ST    @11,SATRNRVT(,RWORK)                             02S0401 00087000
*   ENTCTR=ENTRYNUM;                /* SET COUNTER FOR LOOP TO SET UP   00088000
*                                      NAT ENTRIES FOR THIS NCP AND     00089000
*                                      ITS RESOURCES TO NO. OF RRT      00090000
*                                      ENTRIES                       */ 00091000
         MVC   ENTCTR(4,RDATA),ENTRYNUM(RDATA)                  02S0402 00092000
*   ENTADDRR=RSCPTR;                /* POINT TO START OF RESOURCE       00093000
*                                      ENTRIES IN RRT                */ 00094000
         ST    @14,ENTADDRR(,RDATA)                             02S0403 00095000
*   ENTADDRV=RVTPTR;                /* POINT TO FIRST ENTRY IN RVT   */ 00096000
         ST    @11,ENTADDRV(,RDATA)                             02S0404 00097000
*   NATODAF=RRTRSID&SAVTZELM;       /* 'AND' RSID WITH SUBAREA MASK     00098000
*                                      TO OBTAIN NCP NETWORK ADDR.   */ 00099000
         LH    @07,RVTINDEX(,@11)                               02S0405 00100000
         BCTR  @07,0                                            02S0405 00101000
         ALR   @07,@07                                          02S0405 00102000
         AL    @07,SAVTNAT(,@09)                                02S0405 00103000
         SLR   @06,@06                                          02S0405 00104000
         ICM   @06,3,SAVTZELM(@09)                              02S0405 00105000
         SLR   @09,@09                                          02S0405 00106000
         ICM   @09,3,RRTRSID(@14)                               02S0405 00107000
         NR    @06,@09                                          02S0405 00108000
         STH   @06,NATODAF(,@07)                                02S0405 00109000
*   ENTADDRV=ENTADDRV+LENGTH(RVTINDEX);/* POINT PAST NCP ENTRY       */ 00110000
         AH    @11,@CH00236                                     02S0406 00111000
         ST    @11,ENTADDRV(,RDATA)                             02S0406 00112000
*   DO WHILE(ENTCTR^=0);            /* CREATE NAT ENTRIES FOR THE       00113000
*                                      RESOURCES OFF THE NCP         */ 00114000
         B     @DE00407                                         02S0407 00115000
@DL00407 DS    0H                                               02S0408 00116000
*     IF RVTINDEX^=0 THEN           /* INDICATES RESOURCE CAN BE USED   00117000
*                                      BY TCAM                       */ 00118000
         L     @14,ENTADDRV(,RDATA)                             02S0408 00119000
         LH    @14,RVTINDEX(,@14)                               02S0408 00120000
         LTR   @14,@14                                          02S0408 00121000
         BZ    @RF00408                                         02S0408 00122000
*       IF RRTDEVCE=ON&RRTSWTCH=ON&RRTSNA=OFF&RRTCTERM=OFF THEN/*       00123000
*                                                            @YM08444*/ 00124000
         L     @11,ENTADDRR(,RDATA)                             02S0409 00125000
         TM    RRTDEVCE(@11),B'01000100'                        02S0409 00126000
         BNO   @RF00409                                         02S0409 00127000
         TM    RRTSNA(@11),B'01100000'                          02S0409 00128000
         BNZ   @RF00409                                         02S0409 00129000
*         IF NATODAF=0 THEN         /* IF PRESNA DIAL TERM AND NOT ON   00130000
*                                                            @YM08444*/ 00131000
         L     @09,AVTSAVTP(,RAVT)                              02S0410 00132000
         BCTR  @14,0                                            02S0410 00133000
         ALR   @14,@14                                          02S0410 00134000
         AL    @14,SAVTNAT(,@09)                                02S0410 00135000
         ICM   @09,3,NATODAF(@14)                               02S0410 00136000
         BNZ   @RF00410                                         02S0410 00137000
*           NATODAF=RRTRSID;        /* OTHER NCP THEN SET ITS NAT       00138000
*                                      ENTRY                 @YM08444*/ 00139000
         MVC   NATODAF(2,@14),RRTRSID(@11)                      02S0411 00140000
*         ELSE                                                  02S0412 00141000
*           ;                       /* ELSE LEAVE IT ALONE   @YM08444*/ 00142000
@RF00410 DS    0H                                               02S0413 00143000
*       ELSE                        /* ELSE SET NAT ENTRY FOR OTHER     00144000
*                                      DEVICES               @YM08444*/ 00145000
*         NATODAF=RRTRSID;          /* RSID = NETWORK ADDR.          */ 00146000
         B     @RC00409                                         02S0413 00147000
@RF00409 L     @14,AVTSAVTP(,RAVT)                              02S0413 00148000
         L     @11,ENTADDRV(,RDATA)                             02S0413 00149000
         LH    @11,RVTINDEX(,@11)                               02S0413 00150000
         BCTR  @11,0                                            02S0413 00151000
         ALR   @11,@11                                          02S0413 00152000
         AL    @11,SAVTNAT(,@14)                                02S0413 00153000
         L     @14,ENTADDRR(,RDATA)                             02S0413 00154000
         MVC   NATODAF(2,@11),RRTRSID(@14)                      02S0413 00155000
*     ELSE                                                      02S0414 00156000
*       ;                                                       02S0414 00157000
@RF00408 DS    0H                                               02S0415 00158000
*     ENTADDRV=ENTADDRV+LENGTH(RVTINDEX);/* POINT TO NEXT RVT ENTRY  */ 00159000
@RC00408 LA    @14,2                                            02S0415 00160000
         AL    @14,ENTADDRV(,RDATA)                             02S0415 00161000
         ST    @14,ENTADDRV(,RDATA)                             02S0415 00162000
*     ENTADDRR=ENTADDRR+ENTRYSIZ;   /* POINT TO NEXT RRT ENTRY       */ 00163000
         L     @14,ENTADDRR(,RDATA)                             02S0416 00164000
         AL    @14,ENTRYSIZ(,RDATA)                             02S0416 00165000
         ST    @14,ENTADDRR(,RDATA)                             02S0416 00166000
*     ENTCTR=ENTCTR-1;              /* DECREMENT LOOP CTR.           */ 00167000
         L     @14,ENTCTR(,RDATA)                               02S0417 00168000
         BCTR  @14,0                                            02S0417 00169000
         ST    @14,ENTCTR(,RDATA)                               02S0417 00170000
*   END;                                                        02S0418 00171000
@DE00407 ICM   @14,15,ENTCTR(RDATA)                             02S0418 00172000
         BNZ   @DL00407                                         02S0418 00173000
*   RFY                                                         02S0419 00174000
*     RWORK UNRSTD;                                             02S0419 00175000
*   RFY                                                         02S0420 00176000
*     IEDSATD;                                                  02S0420 00177000
*                                                               02S0421 00178000
*   /*****************************************************************/ 00179000
*   /*                                                               */ 00180000
*   /* THIS SEGMENT CONTROLS THE BUILDING OF THE HIERARCHY.          */ 00181000
*   /*                                                               */ 00182000
*   /*****************************************************************/ 00183000
*                                                               02S0421 00184000
*   ENTCTR=ENTRYNUM;                /* SET CTR. TO NO. OF RRT ENTRIES*/ 00185000
         MVC   ENTCTR(4,RDATA),ENTRYNUM(RDATA)                  02S0421 00186000
*   BSCTR=PSNANUM;                  /* SET CTR. FOR NO. OF PRE-SNA      00187000
*                                      RESOURCES                     */ 00188000
         MVC   BSCTR(4,RDATA),PSNANUM(RDATA)                    02S0422 00189000
*   ENTADDRR=RSCPTR;                /* POINT TO FIRST RRT RESOURCE      00190000
*                                      ENTRY                         */ 00191000
         MVC   ENTADDRR(4,RDATA),RSCPTR(RDATA)                  02S0423 00192000
*   ENTADDRV=RVTPTR+LENGTH(RVTINDEX);/* POINT TO SECOND RVT ENTRY    */ 00193000
         LA    @14,2                                            02S0424 00194000
         AL    @14,RVTPTR(,RDATA)                               02S0424 00195000
         ST    @14,ENTADDRV(,RDATA)                             02S0424 00196000
*                                                               03S0425 00197000
*   /*****************************************************************/ 00198000
*   /*                                                               */ 00199000
*   /* THIS SEGMENT DOES THE FOLLOWING: 1) SETS RVTINDEX TO 0 FOR    */ 00200000
*   /* THOSE RESOURCES UNDER A RESOURCE THAT HAD BEEN SET 0 BY A     */ 00201000
*   /* PREVIOUS SEGMENT. THIS MAINTAINS THE HIERARCHICAL INTEGRITY OF*/ 00202000
*   /* THE RESOURCES. 2) SETS TRMGP FOR PRE-SNA TERMINALS UNDER A    */ 00203000
*   /* CLUSTER WHEN RRTGPTRM IS SET IN THE RRT AND THE CLUSTER       */ 00204000
*   /* RVTINDEX IS NOT 0.                                            */ 00205000
*   /*                                                               */ 00206000
*   /*****************************************************************/ 00207000
*                                                               03S0425 00208000
*/* THIS SEGMENT IS CONTROLLED BY 4 SWITCHES. THEY HAVE THE FOLLOWING*/ 00209000
*/* MEANING WHEN ON:                                                 */ 00210000
*/* LINEDEL : INDICATES THAT A LINE'S RVTINDEX IS 0 AND THAT RVTINDEX*/ 00211000
*/*           SHOULD BE SET TO 0 FOR ALL RESOURCES UNDER THE LINE.   */ 00212000
*/* PUDEL   : INDICATES THAT A PU'S RVTINDEX IS 0 AND THAT RVTINDEX  */ 00213000
*/*           SHOULD BE SET TO 0 FOR ALL LU'S UNDER THE PU.          */ 00214000
*/* CTERMDEL: SET WHEN A PRE-SNA SWITCHED LINE IS ENCOUNTERED.       */ 00215000
*/*           INDICATES THAT ALL TERMINALS WITHOUT RRTCTERM SET      */ 00216000
*/*           SHOULD HAVE THEIR RVTINDEX SET TO 0.                   */ 00217000
*/* GPCLUST : INDICATES A PRE-SNA CLUSTER HAS BEEN ENCOUNTERED AND   */ 00218000
*/*           THAT ALL TERMINALS UNDERNEATH WHICH HAVE RRTGPTRM SET  */ 00219000
*/*           SHOULD HAVE TRMGP SET.                                 */ 00220000
*                                                               03S0425 00221000
*   DO WHILE(ENTCTR^=0);            /* LOOP THROUGH RVT AND AND RRT  */ 00222000
         B     @DE00425                                         03S0425 00223000
@DL00425 DS    0H                                               03S0426 00224000
*     IF RRTLINE=ON THEN            /* IF RESOURCE IS A LINE         */ 00225000
         L     @14,ENTADDRR(,RDATA)                             03S0426 00226000
         TM    RRTLINE(@14),B'10000000'                         03S0426 00227000
         BNO   @RF00426                                         03S0426 00228000
*       DO;                                                     03S0427 00229000
*         FLAGS2='00'X;             /* RESET ALL SWITCHES FOR NEW       00230000
*                                      LINE                          */ 00231000
         MVI   FLAGS2(RDATA),X'00'                              03S0428 00232000
*         IF RVTINDEX^=0 THEN                                   03S0429 00233000
         SLR   @11,@11                                          03S0429 00234000
         L     @09,ENTADDRV(,RDATA)                             03S0429 00235000
         CH    @11,RVTINDEX(,@09)                               03S0429 00236000
         BE    @RF00429                                         03S0429 00237000
*           IF BSCTR>0 THEN         /* IF THIS IS A PRE-SNA LINE     */ 00238000
         C     @11,BSCTR(,RDATA)                                03S0430 00239000
         BNL   @RF00430                                         03S0430 00240000
*             IF RRTSWTCH=ON THEN   /* IF THIS IS A SWITCHED LINE    */ 00241000
         TM    RRTSWTCH(@14),B'00000100'                        03S0431 00242000
         BNO   @RF00431                                         03S0431 00243000
*               CTERMDEL=ON;        /* SWITCHED LINE ENCOUNTERED FLAG*/ 00244000
         OI    CTERMDEL(RDATA),B'00100000'                      03S0432 00245000
*             ELSE                                              03S0433 00246000
*               ;                                               03S0433 00247000
@RF00431 DS    0H                                               03S0434 00248000
*           ELSE                                                03S0434 00249000
*             ;                                                 03S0434 00250000
@RF00430 DS    0H                                               03S0435 00251000
*         ELSE                                                  03S0435 00252000
*           LINEDEL=ON;             /* SET RVTINDEX FOR ALL     03S0435 00253000
*                                      SUBORDINATE RESOURCES TO 0    */ 00254000
         B     @RC00429                                         03S0435 00255000
@RF00429 OI    LINEDEL(RDATA),B'10000000'                       03S0435 00256000
*       END;                                                    03S0436 00257000
*     ELSE                                                      03S0437 00258000
*       IF LINEDEL=ON THEN          /* IF LINE RVTINDEX IS 0         */ 00259000
         B     @RC00426                                         03S0437 00260000
@RF00426 TM    LINEDEL(RDATA),B'10000000'                       03S0437 00261000
         BNO   @RF00437                                         03S0437 00262000
*         RVTINDEX=0;               /* ZERO RVTINDEX FOR THIS   03S0438 00263000
*                                      RESOURCE                      */ 00264000
         L     @14,ENTADDRV(,RDATA)                             03S0438 00265000
         SLR   @11,@11                                          03S0438 00266000
         STH   @11,RVTINDEX(,@14)                               03S0438 00267000
*       ELSE                                                    03S0439 00268000
*         IF RRTDEVCE=ON THEN       /* IF PRE-SNA TERM. OR PU        */ 00269000
         B     @RC00437                                         03S0439 00270000
@RF00437 L     @14,ENTADDRR(,RDATA)                             03S0439 00271000
         TM    RRTDEVCE(@14),B'01000000'                        03S0439 00272000
         BNO   @RF00439                                         03S0439 00273000
*           IF RRTSNA=OFF THEN      /* IF PRE-SNA TERM.              */ 00274000
         TM    RRTSNA(@14),B'00100000'                          03S0440 00275000
         BNZ   @RF00440                                         03S0440 00276000
*             IF CTERMDEL=OFF THEN  /* IF NOT ON SWITCHED LINE       */ 00277000
         TM    CTERMDEL(RDATA),B'00100000'                      03S0441 00278000
         BNZ   @RF00441                                         03S0441 00279000
*               IF RRTCLSTR=OFF THEN/* IF NOT A CLUSTER              */ 00280000
         TM    RRTCLSTR(@14),B'10000000'                        03S0442 00281000
         BNZ   @RF00442                                         03S0442 00282000
*                 IF RRTGPTRM=OFF THEN/* IF RRT INDICATES NOT GEN.      00283000
*                                      POLL                          */ 00284000
         TM    RRTGPTRM(@14),B'00001000'                        03S0443 00285000
         BNZ   @RF00443                                         03S0443 00286000
*                   GPCLUST=OFF;    /* TURN OFF GEN. POLL CLUSTER SW.   00287000
*                                      ONLY SPECIFIC POLL FOR THIS      00288000
*                                      TERM.                         */ 00289000
         NI    GPCLUST(RDATA),B'11101111'                       03S0444 00290000
*                 ELSE                                          03S0445 00291000
*                   IF RVTINDEX=0 THEN                          03S0445 00292000
         B     @RC00443                                         03S0445 00293000
@RF00443 L     @14,ENTADDRV(,RDATA)                             03S0445 00294000
         ICM   @11,3,RVTINDEX(@14)                              03S0445 00295000
         BZ    @RT00445                                         03S0445 00296000
*                     ;             /* IF INDEX ALREADY 0            */ 00297000
*                   ELSE                                        03S0447 00298000
*                     IF GPCLUST=ON THEN/* IF TERM. UNDER GEN. POLL     00299000
*                                      CLUST.                        */ 00300000
         TM    GPCLUST(RDATA),B'00010000'                       03S0447 00301000
         BNO   @RF00447                                         03S0447 00302000
*                       DO;                                     03S0448 00303000
*                         RPARM=RVTINDEX;/* SET PARAMETER WITH TTCIN */ 00304000
         L     @14,ENTADDRV(,RDATA)                             03S0449 00305000
         LH    RPARM,RVTINDEX(,@14)                             03S0449 00306000
*                         CALL IEDQTNT;/* CONVERT TTCIN TO TTE       */ 00307000
         L     @15,AVTRNMPT(,RAVT)                              03S0450 00308000
         BALR  @14,@15                                          03S0450 00309000
*                         RTRM=RPARM;/* TTE ADDR.                    */ 00310000
         LR    RTRM,RPARM                                       03S0451 00311000
*                         TRMGP=ON; /* SET GEN. POLL FLAG IN TTE     */ 00312000
         LR    @14,RTRM                                         03S0452 00313000
         AL    @14,@CF02819                                     03S0452 00314000
         OI    TRMGP-6(@14),B'00000100'                         03S0452 00315000
*                       END;                                    03S0453 00316000
*                     ELSE                                      03S0454 00317000
*                       ;                                       03S0454 00318000
@RF00447 DS    0H                                               03S0455 00319000
*               ELSE                                            03S0455 00320000
*                 IF RVTINDEX=0 THEN/* IF INDEX ALREADY 0            */ 00321000
         B     @RC00442                                         03S0455 00322000
@RF00442 L     @14,ENTADDRV(,RDATA)                             03S0455 00323000
         ICM   @11,3,RVTINDEX(@14)                              03S0455 00324000
         BNZ   @RF00455                                         03S0455 00325000
*                   GPCLUST=OFF;    /* TURN OFF GEN. POLL CLUST. FLAG*/ 00326000
         NI    GPCLUST(RDATA),B'11101111'                       03S0456 00327000
*                 ELSE                                          03S0457 00328000
*                   GPCLUST=ON;     /* TURN ON GEN. POLL CLUST. FLAG */ 00329000
         B     @RC00455                                         03S0457 00330000
@RF00455 OI    GPCLUST(RDATA),B'00010000'                       03S0457 00331000
*             ELSE                                              03S0458 00332000
*               IF RRTCTERM=ON THEN                             03S0458 00333000
         B     @RC00441                                         03S0458 00334000
@RF00441 L     @14,ENTADDRR(,RDATA)                             03S0458 00335000
         TM    RRTCTERM(@14),B'01000000'                        03S0458 00336000
         BO    @RT00458                                         03S0458 00337000
*                 ;                 /* IF RRT INDICATES CTERM        */ 00338000
*               ELSE                                            03S0460 00339000
*                 RVTINDEX=0;       /* ZERO INDEX FOR NON CTERM ON      00340000
*                                      SWITCHED LINE                 */ 00341000
         L     @14,ENTADDRV(,RDATA)                             03S0460 00342000
         SLR   @11,@11                                          03S0460 00343000
         STH   @11,RVTINDEX(,@14)                               03S0460 00344000
*           ELSE                                                03S0461 00345000
*             IF RVTINDEX=0 THEN    /* IF PU INDEX 0                 */ 00346000
         B     @RC00440                                         03S0461 00347000
@RF00440 L     @14,ENTADDRV(,RDATA)                             03S0461 00348000
         ICM   @11,3,RVTINDEX(@14)                              03S0461 00349000
         BNZ   @RF00461                                         03S0461 00350000
*               PUDEL=ON;           /* ZERO INDEX FOR ALL LU'S BELOW */ 00351000
         OI    PUDEL(RDATA),B'01000000'                         03S0462 00352000
*             ELSE                                              03S0463 00353000
*               PUDEL=OFF;          /* PU OD                         */ 00354000
         B     @RC00461                                         03S0463 00355000
@RF00461 NI    PUDEL(RDATA),B'10111111'                         03S0463 00356000
*         ELSE                                                  03S0464 00357000
*           IF PUDEL=OFF THEN                                   03S0464 00358000
         B     @RC00439                                         03S0464 00359000
@RF00439 TM    PUDEL(RDATA),B'01000000'                         03S0464 00360000
         BZ    @RT00464                                         03S0464 00361000
*             ;                     /* IF THIS LU'S PU'S INDEX ^0    */ 00362000
*           ELSE                                                03S0466 00363000
*             RVTINDEX=0;           /* ZERO INDEX FOR LU.            */ 00364000
         L     @14,ENTADDRV(,RDATA)                             03S0466 00365000
         SLR   @11,@11                                          03S0466 00366000
         STH   @11,RVTINDEX(,@14)                               03S0466 00367000
*     ENTCTR=ENTCTR-1;              /* DECREMENT LOOP CTR.           */ 00368000
