@RT00548 DS    0H                                               04S0549 00724000
*               RVTINDEX=0;         /* ENTRY                 @YM08444*/ 00725000
         L     @14,ENTADDRV(,RDATA)                             04S0549 00726000
         SLR   @11,@11                                          04S0549 00727000
         STH   @11,RVTINDEX(,@14)                               04S0549 00728000
*             ELSE                                              04S0550 00729000
*               ;                                               04S0550 00730000
@RF00548 DS    0H                                               04S0551 00731000
*           END;                                                04S0551 00732000
*         ELSE                                                  04S0552 00733000
*           ;                                                   04S0552 00734000
@RF00543 DS    0H                                               04S0553 00735000
*       END;                                                    04S0553 00736000
*     ELSE                                                      04S0554 00737000
*       ;                                                       04S0554 00738000
@RF00538 DS    0H                                               04S0555 00739000
*     ENTADDRV=ENTADDRV+LENGTH(RVTINDEX);/* POINT TO NEXT ENTRY IN      00740000
*                                      RVT                           */ 00741000
         LA    @14,2                                            04S0555 00742000
         AL    @14,ENTADDRV(,RDATA)                             04S0555 00743000
         ST    @14,ENTADDRV(,RDATA)                             04S0555 00744000
*     ENTADDRR=ENTADDRR+ENTRYSIZ;   /* POINT TO NEXT ENTRY IN RRT    */ 00745000
         L     @14,ENTADDRR(,RDATA)                             04S0556 00746000
         AL    @14,ENTRYSIZ(,RDATA)                             04S0556 00747000
         ST    @14,ENTADDRR(,RDATA)                             04S0556 00748000
*   END;                                                        04S0557 00749000
         SLR   @14,@14                                          04S0557 00750000
         BCTR  @14,0                                            04S0557 00751000
         AL    @14,ENTCTR(,RDATA)                               04S0557 00752000
         ST    @14,ENTCTR(,RDATA)                               04S0557 00753000
@DE00537 LTR   @14,@14                                          04S0557 00754000
         BP    @DL00537                                         04S0557 00755000
*                                                               02S0558 00756000
*   /*****************************************************************/ 00757000
*   /*                                                               */ 00758000
*   /* THIS SEGMENT POSTS THE ERB IN THE LCB TO BUFFER REQUEST IN    */ 00759000
*   /* ORDER FOR BUFFERS TO BE ASSIGNED TO THE LCB TO READ THE       */ 00760000
*   /* RESPONSE TO THE ACTIVATE NCP CMND. WHICH THE SSCP WILL SEND   */ 00761000
*   /* WHEN POSTED BY IGCFI10D.                                      */ 00762000
*   /*                                                               */ 00763000
*   /*****************************************************************/ 00764000
*                                                               02S0558 00765000
*   LCBRBCT1=1;                     /* NO. OF BUFFERS REQUESTED      */ 00766000
         L     @14,LCBPTR(,RDATA)                               02S0558 00767000
         MVI   LCBRBCT1(@14),X'01'                              02S0558 00768000
*   LCBRBCT2=DCBUNTCT;              /* NO. OF UNITS IN THE BUFFER    */ 00769000
         L     @11,DCBPTR(,RDATA)                               02S0559 00770000
         MVC   LCBRBCT2(1,@14),DCBUNTCT+48(@11)                 02S0559 00771000
*   LCBERBPY=PRIACTIV;              /* BUFFER REQUEST PRIORITY       */ 00772000
         MVI   LCBERBPY(@14),X'E4'                              02S0560 00773000
*   LCBERBQB=ADDR(AVTBFREB);        /* BUFFER REQUEST QCB    @YM02946*/ 00774000
         LA    @11,AVTBFREB(,RAVT)                              02S0561 00775000
         STCM  @11,7,LCBERBQB(@14)                              02S0561 00776000
*   LCBRCQCB=LCBPTR;                /* RETURN BUFFER TO LCB          */ 00777000
         ST    @14,LCBRCQCB(,@14)                               02S0562 00778000
*   LCBPRCPG=ON;                    /* TURN ON FLAG INDICATING  02S0563 00779000
*                                      LCBRCQCB CONTAINS RETURN QCB  */ 00780000
*   LCBDLNKN=OFF;                   /* PREVENT DBL LCB POST  @YM03690*/ 00781000
         OI    LCBPRCPG(@14),B'00000010'                        02S0564 00782000
         NI    LCBDLNKN(@14),B'11111110'                        02S0564 00783000
*   AQCTL1=ADDR(LCBERB);            /* SET 1ST WORD OF AQCTL PARM.      00784000
*                                      LIST TO POINT TO ERB          */ 00785000
         LA    @14,LCBERB(,@14)                                 02S0565 00786000
         ST    @14,AQCTL1(,ROPCAVT)                             02S0565 00787000
*   AQCTL2=AQCTL1;                  /* COPY                          */ 00788000
