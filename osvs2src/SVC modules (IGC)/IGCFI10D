*   END;                                                        03S0514 00551000
@DE00477 ICM   @14,15,ENTCTR(RDATA)                             03S0514 00552000
         BNZ   @DL00477                                         03S0514 00553000
*                                                               03S0515 00554000
*   /*****************************************************************/ 00555000
*   /*                                                               */ 00556000
*   /* THIS SEGMENT PERFORMS TWO FUNCTIONS WHICH RESULTS IN THE      */ 00557000
*   /* COMPLETION OF THE HIERARCHY IN THE TERMINAL TABLE AND THE RVT.*/ 00558000
*   /* 1) IT LOOPS THROUGH THE TERM. TABLE HIERARCHY ALREADY BUILT   */ 00559000
*   /* LOCATING THOSE RESOURCES WHOSE SUBSTRUCT IS SET TO ITS TTCIN  */ 00560000
*   /* (INDICATING THE RESOURCE IS LOWEST IN THE HIERARCY) AND SETS  */ 00561000
*   /* THE SUBSTRUCT TO ZERO. (WHILE LOOPING, TRMINPG IS SET FOR SNA */ 00562000
*   /* RESOURCES INDICATING ACTIVATE IN PROGRESS FOR A SUPERIOR      */ 00563000
*   /* RESOURCE.) 2) IT CALLS RVTSET TO SET THE RVT HIERARCHY.       */ 00564000
*   /*                                                               */ 00565000
*   /*****************************************************************/ 00566000
*                                                               03S0515 00567000
*   ENTADDRV=RVTPTR;                /* POINT TO FIRST ENTRY IN RVT   */ 00568000
