         ST    @11,LASTLINE(,RDATA)                             03S0490 00467000
*               TRMCOHRT=LASTLINE->TRMSUBST;/* SET COHORT OF TERM TO    00468000
*                                      LAST TERM OR LINE             */ 00469000
         LR    @09,RTRM                                         03S0491 00470000
         SLR   @09,@14                                          03S0491 00471000
