*   GEN;                                                        02S0570 00797000
         AQCTL                                                          00798000
*                                                               02S0571 00799000
*   /*****************************************************************/ 00800000
*   /*                                                               */ 00801000
*   /* THIS SEGMENT BUILDS AN OPCE AND POSTS THE SSCP TO ACTIVATE THE*/ 00802000
*   /* NCP.                                                          */ 00803000
*   /*                                                               */ 00804000
*   /*****************************************************************/ 00805000
*                                                               02S0571 00806000
*   RPARM=1;                        /* NO. OF BUFFER UNITS REQUIRED     00807000
*                                      TO BUILD AN OPCE              */ 00808000
         LA    RPARM,1                                          02S0571 00809000
*   CALL ALLOCBUF;                  /* ALLOCATE A BUFFER             */ 00810000
         L     @15,OPCGETBF(,ROPCAVT)                           02S0572 00811000
         BALR  @14,@15                                          02S0572 00812000
*   ROPCE=RPARM;                    /* COPY ADDR. OF NEW OPCE        */ 00813000
         LR    ROPCE,RPARM                                      02S0573 00814000
*   OCPTRSAV=OPCCOPCE;              /* PUT POINTER TO CURRENT OPCE IN   00815000
*                                      NEW OPCE                      */ 00816000
         L     @14,OPCCOPCE(,ROPCAVT)                           02S0574 00817000
         ST    @14,OCPTRSAV(,ROPCE)                             02S0574 00818000
*   OCELMPRI=PRIHIFLG;              /* SET PRIORITY                  */ 00819000
         MVI   OCELMPRI(ROPCE),X'E9'                            02S0575 00820000
*   OCPRIFLG=OPRA0;                 /* SET FLAG FIELD FOR SSCP POST  */ 00821000
         MVI   OCPRIFLG(ROPCE),X'01'                            02S0576 00822000
*   OCWTG=RSPHNDLR;                 /* ACTIVATE RESPONSE HANDLER     */ 00823000
         MVC   OCWTG(2,ROPCE),@CC02778                          02S0577 00824000
*   OCACTREQ=ON;                    /* SET ACT-DEACT FLAG    @YM03705*/ 00825000
         OI    OCACTREQ(ROPCE),B'00001000'                      02S0578 00826000
*   OCTRMTBL=OPCCOPCE->OCTRMTBL;    /* SET ORIG.(NCP) RESOURCE TTCIN    00827000
*                                      IN NEW OPCE                   */ 00828000
         MVC   OCTRMTBL(2,ROPCE),OCTRMTBL(@14)                  02S0579 00829000
*   OCSWITCH=OCACTNCP;              /* SET CODE FOR ACTIVATE (ERP) OF   00830000
*                                      NCP                           */ 00831000
         MVI   OCSWITCH(ROPCE),X'70'                            02S0580 00832000
*   OPCCOPCE=ROPCE;                 /* SET CURRENT OPCE TO POINT TO     00833000
*                                      NEW OPCE                      */ 00834000
         ST    ROPCE,OPCCOPCE(,ROPCAVT)                         02S0581 00835000
*   OPCMODID=TOA0;                  /* SET TO XCTL TO SSCP           */ 00836000
