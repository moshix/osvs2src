*                                                               02S0579 00751000
*   /*****************************************************************/ 00752000
*   /*                                                               */ 00753000
*   /* BLDOPCE: THIS PROC BUILDS OPCES FOR DEACT LINKS, ACT LINKS,   */ 00754000
*   /* AND STARTLINE REQUESTS. THEY ARE PUT ON THE SVC102 LIST. IF   */ 00755000
*   /* USE THE LAST BUFFER THEN NEED MORE THEN THE OUT FLAG IS SET ON*/ 00756000
*   /* TO EXIT THE OUTER LOOP AND SET UP FOR REDRIVE                 */ 00757000
*   /*                                                               */ 00758000
*   /*****************************************************************/ 00759000
*                                                               02S0579 00760000
*BLDOPCE:                                                       02S0579 00761000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                            02S0579 00762000
BLDOPCE  DS    0H                                               02S0580 00763000
*   RFY                                                         02S0580 00764000
*     REG14 RSTD;                   /* RESTRICT REG                  */ 00765000
*   BLDOSAV=REG14;                  /* SAVE RETURN ADDR              */ 00766000
         ST    REG14,BLDOSAV                                    02S0581 00767000
*   RFY                                                         02S0582 00768000
*     REG14 UNRSTD;                 /* FREE REG FOR USE              */ 00769000
*   RFY                                                         02S0583 00770000
*     REG09 RSTD;                   /* RESTRICT REG                  */ 00771000
*   REG09=OCUNIT;                   /* GET PTR TO ATTACHED UNIT      */ 00772000
         L     REG09,OCUNIT(,ROPCE)                             02S0584 00773000
*   OCUNIT=REG09->OCUNIT;           /* DELINK UNIT                   */ 00774000
         MVC   OCUNIT(4,ROPCE),OCUNIT(REG09)                    02S0585 00775000
*   RFY                                                         02S0586 00776000
*     IEDQOPCE BASED(REG09);        /* RESET OPCE BASE               */ 00777000
*   OCUNIT=PRUINTIC;                /* SET UP INVALID TIC            */ 00778000
         MVC   OCUNIT(4,REG09),@CF01412                         02S0587 00779000
*   OCTRMTBL=ROPCE->OCTRMTBL;       /* SET UP TTCIN                  */ 00780000
         MVC   OCTRMTBL(2,REG09),OCTRMTBL(ROPCE)                02S0588 00781000
*   OCPARM=0;                       /* CLEAR OCPARM                  */ 00782000
