         BR    @14                                              02S0710 01116000
*                                                               02S0711 01117000
*   /*****************************************************************/ 01118000
*   /*                                                               */ 01119000
*   /* THIS ROUTINE CLEANS UP FROM AN ACTIVATION OF THE NCP BROUGHT  */ 01120000
*   /* ABOUT BY AN F ACT=NCP. THE IPL'ED FLAG IN THE DEVICE DEPENDENT*/ 01121000
*   /* FIELDS OF THE NCP IS TURNED OFF IF ACTIVATION DID NOT FAIL    */ 01122000
*   /* BECAUSE OF A SIB SHORTAGE. IGCFD10D IS LINKED TO TO DELETE THE*/ 01123000
*   /* HIERARCHY, THE SAT ENTRY FOR THIS NCP, THE NAT ENTRIES, AND TO*/ 01124000
*   /* FREE THE RVT.                                                 */ 01125000
*   /*                                                               */ 01126000
*   /*****************************************************************/ 01127000
*                                                               02S0711 01128000
*CLNUPNCP:                                                      02S0711 01129000
*   PROC OPTIONS(RETREG,SAVE(14));                              02S0711 01130000
CLNUPNCP ST    @14,@SA00006                                     02S0711 01131000
*   IF OCPTRSAV->OCPARM^=SIBSGMSG THEN/* IF NOT SIB SHORTAGE         */ 01132000
         L     @11,OCPTRSAV(,ROPCE)                             02S0712 01133000
         CLC   OCPARM(2,@11),@CH01667                           02S0712 01134000
         BE    @RF00712                                         02S0712 01135000
*     DO;                                                       02S0713 01136000
*       RPARM=OCTRMTBL;             /* SET PARM. WITH NCP TTCIN      */ 01137000
         SLR   RPARM,RPARM                                      02S0714 01138000
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          02S0714 01139000
*       CALL NCPDVCDP;              /* OBTAIN DEVICE DEPENDENT FIELDS*/ 01140000
         BAL   @14,NCPDVCDP                                     02S0715 01141000
*       NCPIPLD=OFF;                /* TURN OFF IPL'ED FLAG          */ 01142000
         NI    NCPIPLD(RTNCODE),B'11110111'                     02S0716 01143000
*     END;                                                      02S0717 01144000
*   ELSE                                                        02S0718 01145000
*     ;                                                         02S0718 01146000
@RF00712 DS    0H                                               02S0719 01147000
*   OPCCOPCE=OCPTRSAV;              /* MAKE ORIG. OPCE THE CURRENT      01148000
*                                      OPCE                          */ 01149000
         MVC   OPCCOPCE(4,ROPCAVT),OCPTRSAV(ROPCE)              02S0719 01150000
*   RPARM=ROPCAVT;                  /* SET ADDR. OF OP. CTL. AVT     */ 01151000
         LR    RPARM,ROPCAVT                                    02S0720 01152000
*   GEN;                                                        02S0721 01153000
*          /*                                                           01154000
         LINK  EP=IGCFD10D         */                                   01155000
*   END CLNUPNCP;                                               02S0722 01156000
