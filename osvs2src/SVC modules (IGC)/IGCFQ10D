*         /***********************************************************/ 01162000
*         /*                                                         */ 01163000
*         /* SET COMMON FIELDS IN OPCE                               */ 01164000
*         /*                                                         */ 01165000
*         /***********************************************************/ 01166000
*                                                               01S0882 01167000
*         OCPTRSAV=ROPCE->OCPTRSAV; /* SET ADDR. OF ORIG. OPCE       */ 01168000
         MVC   OCPTRSAV(4,UNITPTR),OCPTRSAV(ROPCE)              01S0882 01169000
*         OCSWITCH=OCOPYDSI;        /* SET COMMAND CODE              */ 01170000
         MVI   OCSWITCH(UNITPTR),X'E3'                          01S0883 01171000
*         OCLINK=ADDR(AVTOPCOB);    /* SET QCB ADDR.                 */ 01172000
         LA    @14,AVTOPCOB(,RAVT)                              01S0884 01173000
