         ICM   RPARM,3,OCTRMTBL(ROPCE)                          03S0646 00143000
*                 CALL IEDQTNT;     /* .... ADDR.                    */ 00144000
         L     @15,AVTRNMPT(,RAVT)                              03S0647 00145000
         BALR  @14,@15                                          03S0647 00146000
*                 RZERO=TRMDDIGA;   /* OBTAIN ADDR. .....            */ 00147000
         LH    RZERO,@CH02076                                   03S0648 00148000
*                 CALL IEDQTL;      /* .... OF DEVICE DEPENDENT FIELD*/ 00149000
