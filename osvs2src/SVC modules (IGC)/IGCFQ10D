*                                                               01S0636 00085000
         B     @RC00634                                         01S0636 00086000
@RF00634 DS    0H                                               01S0637 00087000
*       /*************************************************************/ 00088000
*       /*                                                           */ 00089000
*       /* THIS IS A RESPONSE OPCE OR A REQUEST FROM IGCFL10D.       */ 00090000
*       /*                                                           */ 00091000
*       /*************************************************************/ 00092000
*                                                               01S0637 00093000
*       ORGNOPCE->OCRESPCT=ORGNOPCE->OCRESPCT-1;/* DECREMENT RSP. CT.*/ 00094000
         SLR   @14,@14                                          01S0637 00095000
         ICM   @14,3,OCRESPCT(ORGNOPCE)                         01S0637 00096000
         BCTR  @14,0                                            01S0637 00097000
         STH   @14,OCRESPCT(,ORGNOPCE)                          01S0637 00098000
*                                                               02S0638 00099000
*       /*************************************************************/ 00100000
*       /*                                                           */ 00101000
*       /* ---------- THIS SEGMENT PROCESSES RESPONSES --------      */ 00102000
*       /*                                                           */ 00103000
*       /*************************************************************/ 00104000
*                                                               02S0638 00105000
*       IF OCSWITCH=OCRPLDSI THEN   /* IF REPLACE RESPONSE           */ 00106000
         CLI   OCSWITCH(ROPCE),X'E4'                            02S0638 00107000
         BNE   @RF00638                                         02S0638 00108000
*         FREUTFLG=ON;              /* SET FLAG TO FREE UNITS        */ 00109000
         OI    FREUTFLG,B'10000000'                             02S0639 00110000
*       ELSE                                                    02S0640 00111000
*         IF OCSWITCH=OCOPYDSI THEN /* IF COPY RESPONSE              */ 00112000
         B     @RC00638                                         02S0640 00113000
@RF00638 CLI   OCSWITCH(ROPCE),X'E3'                            02S0640 00114000
         BNE   @RF00640                                         02S0640 00115000
*           DO;                                                 02S0641 00116000
*                                                               02S0641 00117000
*             /*******************************************************/ 00118000
*             /*                                                     */ 00119000
*             /* DETERMINE IF REPLACE SHOULD BE DONE.                */ 00120000
*             /*                                                     */ 00121000
*             /*******************************************************/ 00122000
*                                                               02S0642 00123000
*                                                               03S0642 00124000
*             /*******************************************************/ 00125000
*             /*                                                     */ 00126000
*             /* --- THIS SEGMENT PROCESSES "COPY DEVICE SESSION     */ 00127000
*             /* INFO." RESPONSES                                    */ 00128000
*             /*                                                     */ 00129000
*             /*******************************************************/ 00130000
*                                                               03S0642 00131000
*             RFY                                               03S0642 00132000
*               IEDDSI1 BASED(ADDR(PRUDATA)+LENGTH(IEDBDU));    03S0642 00133000
*             RFY                                               03S0643 00134000
*               IEDQTRM BASED(RPARM);                           03S0643 00135000
*             IF BDUSYRSP=GOODRSP THEN/* IF GOOD RESPONSE ON COPY    */ 00136000
