*   ROPCE=OPCCOPCE;                 /* SET POINTER TO CURRENT OPCE   */ 00017000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         01S0625 00018000
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */ 00019000
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0626 00020000
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */ 00021000
