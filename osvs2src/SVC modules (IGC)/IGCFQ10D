*   FLAGS=''B;                      /* INIT FLAGS FIELD              */ 00037000
*                                                               01S0630 00038000
         MVI   FLAGS,X'00'                                      01S0630 00039000
*   /*****************************************************************/ 00040000
*   /*                                                               */ 00041000
*   /* CALCULATE 50% OF TCAM'S LINE UNITS                            */ 00042000
*   /*                                                               */ 00043000
*   /*****************************************************************/ 00044000
*                                                               01S0631 00045000
*   HALFUNIT=AVTNOLBF;              /* # OF TCAM'S LINE UNITS        */ 00046000
