         ALR   @07,PTRCHAR                                      05S0361 00222000
         ALR   @11,@07                                          05S0361 00223000
         EX    @14,@SM01896                                     05S0361 00224000
*                 DO PTRCHAR=ADDR(CHARRLN) TO ADDR(CHARRLN)+LENGTH(     00225000
*                       CHARRLN)    /* SCAN THE RELATIVE LINE NUMBER    00226000
*                                      OPERAND                       */ 00227000
*                       WHILE(RETCODE=0);/* THE CHARACTERS ARE  05S0362 00228000
*                                      NUMERIC                       */ 00229000
         LA    PTRCHAR,CHARRLN                                  05S0362 00230000
         B     @DE00362                                         05S0362 00231000
@DL00362 LTR   RETCODE,RETCODE                                  05S0362 00232000
         BNZ   @DC00362                                         05S0362 00233000
*                   IF SCANCHAR<'0'|SCANCHAR>'9'/* THE CHARACTER IS     00234000
*                                      NOT NUMERIC                   */ 00235000
*                     THEN          /* INDICATE THAT AN INVALID 05S0363 00236000
*                                      OPERAND HAS BEEN ENCOUNTERED  */ 00237000
         CLI   SCANCHAR(PTRCHAR),C'0'                           05S0363 00238000
         BL    @RT00363                                         05S0363 00239000
         CLI   SCANCHAR(PTRCHAR),C'9'                           05S0363 00240000
         BNH   @RF00363                                         05S0363 00241000
@RT00363 DS    0H                                               05S0364 00242000
*                     DO;                                       05S0364 00243000
*                       RETCODE=4;  /* SET THE RETURN CODE           */ 00244000
         LA    RETCODE,4                                        05S0365 00245000
*                       REG00=MSGINVOP;/* SET REGISTER ZERO (0) WITH    00246000
*                                      THE APPROPRIATE MESSAGE NUMBER*/ 00247000
         LA    REG00,18                                         05S0366 00248000
*                     END;                                      05S0367 00249000
*                 END;                                          05S0368 00250000
