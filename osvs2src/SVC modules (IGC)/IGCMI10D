*   REG1=ADDR(FSDDCB);              /* DCB LIST ADDR                 */ 00403000
         LA    REG1,FSDDCB(,@15)                                   0501 00404000
*   OPCIPLCT=OPCIPLCT-1;            /* DECREMENT COUNT OF 3705'S   0502 00405000
