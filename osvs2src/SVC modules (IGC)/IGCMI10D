*         REG1,                                                    0546 00584000
*         REG5) RSTD;                                              0546 00585000
*       REG7=ADDR(BLKADDR);         /* ADR OF FIRST BLOCK ADDRESS    */ 00586000
         LA    REG7,BLKADDR(,@15)                                  0547 00587000
*       REG1=BLKADDR;               /* FIRST BLOCK ADDR              */ 00588000
