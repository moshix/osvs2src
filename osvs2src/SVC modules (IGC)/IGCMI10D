*   IF OPCIPLCT=0 THEN              /* WHEN THE COUNT IS ZERO, CLOSE    00411000
*                                      THE IPL DATA SET      @YM08447*/ 00412000
         CLI   OPCIPLCT(ROPCAVT),0                                 0503 00413000
         BNE   @RF00503                                            0503 00414000
*     DO;                           /*                       @YM08447*/ 00415000
