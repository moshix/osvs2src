*               WORKREG=WORKREG-1;  /* SUB 1 FROM LEN                */ 00126000
         BCTR  WORKREG,0                                           0293 00127000
*             END;                                                 0294 00128000
*         END;                                                     0295 00129000
*                                                                  0295 00130000
