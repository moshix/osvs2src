*             DO;                                                  0288 00117000
*               GOTO ERRHAND1;                                     0289 00118000
         B     ERRHAND1                                            0289 00119000
*             END;                                                 0290 00120000
*           ELSE                                                   0291 00121000
*             DO;                                                  0291 00122000
@RF00287 DS    0H                                                  0292 00123000
*               RADVPTR=RADVPTR+1;  /* ADV PAST COMMAND              */ 00124000
