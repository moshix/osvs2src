         TITLE 'IGCMP10D - 3705 DUMP ROUTINE                           *00001000
                        '                                               00002000
IGCMP10D CSECT ,                                                   0001 00003000
@PROLOG  DS    0H                                                  0002 00004000
         BALR  RBASE,0                  ADDRESSIBILITY                  00005000
         USING *,RBASE                  SET BASE REG                    00006000
IGCMP10D IEDHJN AFTERHJN,HJN            IDENTIFICATION MACRO   @Y17XARL 00007000
*                                                                  0449 00008000
*/********************************************************************/ 00009000
*/* THIS ROUTINE GETS CONTROL FROM IGCN410D AND FURTHER SYNTAX       */ 00010000
*/* CHECKS THE COMMAND TO DETERMINE IF A DUMP DCB HAS BEEN CODED     */ 00011000
*/* AND OPENED OR FROM IGC0010D AFTER COMPLETION OF 3705 I/O         */ 00012000
*/********************************************************************/ 00013000
*                                                                  0449 00014000
*   ROPCAVT=REG1;                   /* SET OPCAVT BASE       @Y17XARL*/ 00015000
         LR    ROPCAVT,REG1                                        0449 00016000
*   IF OPCCOPCE->OCSWITCH=OCDMPC THEN/* IF OPCE INITIAL INPUT      0450 00017000
*                                                            @YM02908*/ 00018000
         L     @15,OPCCOPCE(,ROPCAVT)                              0450 00019000
         CLI   OCSWITCH(@15),X'0A'                                 0450 00020000
         BNE   @RF00450                                            0450 00021000
*     RCOM=OCIPLWA;                 /* GET WORK AREA ADDR    @YM02908*/ 00022000
         L     RCOM,OCIPLWA(,@15)                                  0451 00023000
*   ELSE                                                           0452 00024000
*     DO;                           /* ELEMENT FROM OPCOB    @YM02908*/ 00025000
         B     @RC00450                                            0452 00026000
@RF00450 DS    0H                                                  0453 00027000
*       RCOM=OPCCOPCE;              /* WORK AREA ADDRESS     @YM02908*/ 00028000
         L     RCOM,OPCCOPCE(,ROPCAVT)                             0453 00029000
*       OPCCOPCE=COMOPCE;           /* OPCE BASE             @YM02908*/ 00030000
         L     @00,COMOPCE(,RCOM)                                  0454 00031000
         ST    @00,OPCCOPCE(,ROPCAVT)                              0454 00032000
*     END;                          /*                       @YM02908*/ 00033000
*   RLCB=OCTRLCB;                   /* GET LCB BASE          @YM02908*/ 00034000
@RC00450 L     @15,OPCCOPCE(,ROPCAVT)                              0456 00035000
         L     @15,OCTRUNIT(,@15)                                  0456 00036000
         L     RLCB,OCTRLCB(,@15)                                  0456 00037000
*   RSAVE=OPCWORK;                  /* GET SAVE AREA ADDR    @YM02908*/ 00038000
         L     RSAVE,OPCWORK(,ROPCAVT)                             0457 00039000
*   IF COMPRI=PRIHIFLG THEN         /* IS ENTRY FROM DLC     @YM02908*/ 00040000
         CLI   COMPRI(RCOM),233                                    0458 00041000
         BNE   @RF00458                                            0458 00042000
*     IF COMIOINP=OFF THEN          /* IF NOT WAITING FOR I/O TO   0459 00043000
*                                      COMPLETE              @YM02908*/ 00044000
         TM    COMIOINP(RCOM),B'10000000'                          0459 00045000
         BZ    @RT00459                                            0459 00046000
*       GOTO FROMDLC;               /* YES, BRANCH           @YM02908*/ 00047000
*   TS(LCBTSTSW);                   /* TEST FOR I/O IN PROGRESS FOR     00048000
*                                      THIS NCP              @YM02908*/ 00049000
@RF00458 TS    LCBTSTSW(RLCB)                                      0461 00050000
*   BC(CCNONZ,READYQ);              /* IF I/O IN PROGRESS - GO TO  0462 00051000
*                                      POST ENEMENT TO OPCOB @YM02908*/ 00052000
         BC    4,READYQ                                            0462 00053000
*   COMIOINP=OFF;                   /* TURN OFF I/O IN PROGRESS FLAG    00054000
*                                      IF SET                @YM02908*/ 00055000
         NI    COMIOINP(RCOM),B'01111111'                          0463 00056000
*   OCTRIP=OFF;                     /* RESET IN PROGRESS     @Y17XARL*/ 00057000
         L     @15,OPCCOPCE(,ROPCAVT)                              0464 00058000
         L     @15,OCTRUNIT(,@15)                                  0464 00059000
         NI    OCTRIP(@15),B'01111111'                             0464 00060000
*   IF NCPDPDCB=ZER0                /* WAS DUMP DCB SPECIFIED      0465 00061000
*                                                            @Y17XARL*/ 00062000
*     THEN                          /* NO                            */ 00063000
         L     @15,OCTRDVD(,@15)                                   0465 00064000
         ICM   @14,15,NCPDPDCB(@15)                                0465 00065000
         LTR   @14,@14                                             0465 00066000
         BZ    @RT00465                                            0465 00067000
*     GOTO BADDCB;                  /* WRITE OUT ERROR MESSAGE       */ 00068000
*   WDCBPTR=NCPDPDCB;               /* SET DUMP DCB IN OPEN LIST   0467 00069000
*                                                            @Y17XARL*/ 00070000
         STCM  @14,7,WDCBPTR(ROPCAVT)                              0467 00071000
*   WDCBEND=ENDLIST;                /* SET END OF LIST INDICATOR   0468 00072000
*                                                            @Y17XARL*/ 00073000
         MVI   WDCBEND(ROPCAVT),X'8F'                              0468 00074000
*   GEN EXIT SEQFLOW REFS(WDCBLIST) SETS(REG1) NODEFS NOFLOWS;     0469 00075000
*                                                          /*@Y17XARL*/ 00076000
         OPEN  MF=(E,WDCBLIST(ROPCAVT)) OPEN DUMP DCB          @Y17XARL 00077000
*   IF DCBOFOPN=OFF                 /* IS THE DUMP DCB OPEN          */ 00078000
*     THEN                          /* NO                            */ 00079000
         L     @15,OPCCOPCE(,ROPCAVT)                              0470 00080000
         L     @15,OCTRUNIT(,@15)                                  0470 00081000
         L     @15,OCTRDVD(,@15)                                   0470 00082000
         L     @15,DCBPTR(,@15)                                    0470 00083000
         TM    DCBOFOPN(@15),B'00010000'                           0470 00084000
         BZ    @RT00470                                            0470 00085000
*     GOTO BADDCB;                  /* WRITE OUT ERROR MESSAGE       */ 00086000
*/********************************************************************/ 00087000
*/* THIS ROUTINE IS ENTERED AFTER IT IS DETERMINED THAT A DUMP       */ 00088000
*/* OF THE 3705 IS POSSIBLE.                                         */ 00089000
*/********************************************************************/ 00090000
*                                                                  0472 00091000
*   RBUF1=ADDR(COMINBF1);           /* POINTERS TO BUFFER BEING      */ 00092000
         LA    RBUF1,COMINBF1(,RCOM)                               0472 00093000
*   RBUF2=ADDR(COMINBF2);           /* WRITTEN AND ONE BEING FILLED  */ 00094000
         LA    RBUF2,COMINBF2(,RCOM)                               0473 00095000
*                                                                  0474 00096000
*/********************************************************************/ 00097000
*/* THIS SECTION OF CODE DETERMINES FROM THE UCB WHETHER THE 3705    */ 00098000
*/* HAS A MAXI OR A MINI CHANNEL ADAPTER AND SELECTS THE APPROPRIATE */ 00099000
*/* PROGRAMS TO SEND TO THE 3705.                                    */ 00100000
*/********************************************************************/ 00101000
*                                                                  0474 00102000
*   GEN(BLDL 0,BLDLIST);                                           0474 00103000
         BLDL 0,BLDLIST                                                 00104000
*   IF RF>ZER0                      /* BLDL SUCCESSFUL               */ 00105000
*     THEN                          /* NO                            */ 00106000
         LTR   RF,RF                                               0475 00107000
         BNP   @RF00475                                            0475 00108000
*     DO;                                                          0476 00109000
*       RMSGNO=MBLDLFL;             /* MESSAGE NUMBER        @Y17XARL*/ 00110000
         LA    RMSGNO,165                                          0477 00111000
*       GOTO EXIT1;                 /* EXIT TO IGCMI10D      @Y17XARL*/ 00112000
         B     EXIT1                                               0478 00113000
*     END;                                                         0479 00114000
*   NCPIPLD=OFF;                    /* 3705 NOT IPLED        @Y17XARL*/ 00115000
@RF00475 L     @15,OPCCOPCE(,ROPCAVT)                              0480 00116000
         L     @15,OCTRUNIT(,@15)                                  0480 00117000
         L     @14,OCTRDVD(,@15)                                   0480 00118000
         NI    NCPIPLD(@14),B'11110111'                            0480 00119000
*   IF UCBTBYT4=MINICA              /* IS THIS A MINI CHANNEL ADAPTER   00120000
*                                                            @Y17XARL*/ 00121000
*     THEN                          /* YES                           */ 00122000
         L     @15,UCBPTR(,@15)                                    0481 00123000
         CLI   UCBTBYT4(@15),X'15'                                 0481 00124000
         BNE   @RF00481                                            0481 00125000
*     WKREG=ADDR(MINIAA);           /* SET FOR MINI ROUTINES         */ 00126000
         LA    WKREG,MINIAA                                        0482 00127000
*   ELSE                            /* NO                            */ 00128000
*     WKREG=ADDR(MAXIBB);           /* SET FOR MAXI ROUTINES         */ 00129000
         B     @RC00481                                            0483 00130000
@RF00481 LA    WKREG,MAXIBB                                        0483 00131000
*   COMTTRZ=WKREG;                  /* SAVE FOR FUTURE DELETION      */ 00132000
@RC00481 ST    WKREG,COMTTRZ(,RCOM)                                0484 00133000
*   GEN(LOAD EPLOC=(WKREG));        /* LOAD THE ROUTINES             */ 00134000
         LOAD EPLOC=(WKREG)                                             00135000
*   RPH1=REG0;                      /* SAVE PHASE ONE ADDRESS        */ 00136000
         LR    RPH1,REG0                                           0486 00137000
*   RPH2=RPH1+RPH1->MAPLEN;         /* ADDR OF PHASE 2       @ZA01103*/ 00138000
         LR    RPH2,RPH1                                           0487 00139000
         AH    RPH2,MAPLEN(,RPH1)                                  0487 00140000
*   GEN(LA RPH2,7(RPH2));           /* ADD 7 AND SHIFT       @ZA01103*/ 00141000
         LA RPH2,7(RPH2)                                                00142000
*   GEN(SRA RPH2,3);                /* TO INSURE                     */ 00143000
         SRA RPH2,3                                                     00144000
*   GEN(SLA RPH2,3);                /* A DOUBLE WORD BOUNDARY        */ 00145000
         SLA RPH2,3                                                     00146000
*                                                                  0491 00147000
*/********************************************************************/ 00148000
*/* THIS SECTION OF CODE SENDS THE 3705 PORTION OF THE DUMP PROGRAM  */ 00149000
*/* TO THE 3705.                                                     */ 00150000
*/********************************************************************/ 00151000
*                                                                  0491 00152000
*   COMNEXT1=NEXTLEN;               /* INITIALIZE AREA               */ 00153000
         L     @00,NEXTLEN                                         0491 00154000
         ST    @00,COMNEXT1(,RCOM)                                 0491 00155000
*   CPA=CPA&&CPA;                   /* CLEAR CHANNEL PROGRAM AREA  0492 00156000
*                                                            @Y17XARL*/ 00157000
         XC    CPA(24,RLCB),CPA(RLCB)                              0492 00158000
*   LCBSTART=ADDR(CPA);             /* ADDR OF CHANNEL PROGRAM     0493 00159000
*                                                            @Y17XARL*/ 00160000
         LA    @00,CPA(,RLCB)                                      0493 00161000
         STCM  @00,7,LCBSTART(RLCB)                                0493 00162000
*   RNADR=ZER0;                     /* INITALIZE REGISTER            */ 00163000
         SLR   RNADR,RNADR                                         0494 00164000
*   LCBIOSB=LCBIOSBS;               /* RESTORE SAVED IOSB ADDRESS  0495 00165000
*                                                            @ZM47822*/ 00166000
         MVC   LCBIOSB(3,RLCB),LCBIOSBS(RLCB)                      0495 00167000
*   CPANOOP=NOPCCW;                 /* SET NO-OP CCW         @Y17XARL*/ 00168000
         MVC   CPANOOP(8,RLCB),NOPCCW                              0496 00169000
*   CPADATA2=ADDR(NOPCCW);          /* SET ADDR OF CCW       @Y17XARL*/ 00170000
         LA    @00,NOPCCW                                          0497 00171000
         STCM  @00,7,CPADATA2(RLCB)                                0497 00172000
*   CPADATA1=RPH1;                  /* ADDR OF PHASE 1       @Y17XARL*/ 00173000
         STCM  RPH1,7,CPADATA1(RLCB)                               0498 00174000
*   CPACMD1=CCWIPL;                 /* SET IPL COMMAND CODE  @Y17XARL*/ 00175000
         MVI   CPACMD1(RLCB),X'05'                                 0499 00176000
*   CPAFLG1=CHAIN;                  /* SET FLAGS             @Y17XARL*/ 00177000
         MVI   CPAFLG1(RLCB),X'60'                                 0500 00178000
*   CPACNT1=RPH1->MAPLEN;           /* SET LENGTH IN CCW     @Y17XARL*/ 00179000
         LH    @00,MAPLEN(,RPH1)                                   0501 00180000
         STH   @00,CPACNT1(,RLCB)                                  0501 00181000
*   OCTRADR=ADDR(PHASE1);           /* RESUME ADDR AFTER PHASE1    0502 00182000
*                                      WRITTEN               @Y17XARL*/ 00183000
         L     @15,OPCCOPCE(,ROPCAVT)                              0502 00184000
         L     @15,OCTRUNIT(,@15)                                  0502 00185000
         LA    @00,PHASE1                                          0502 00186000
         ST    @00,OCTRADR(,@15)                                   0502 00187000
*   GOTO CALLIOW;                   /* GO WRITE PHASE 1      @Y17XARL*/ 00188000
         B     CALLIOW                                             0503 00189000
*PHASE1:                            /*                       @Y17XARL*/ 00190000
*   CPADATA1=RPH2;                  /* ADDR OF PHASE 2       @Y17XARL*/ 00191000
PHASE1   STCM  RPH2,7,CPADATA1(RLCB)                               0504 00192000
*   CPACMD1=CCWWRITE;               /* SET WRITE COMMAND CODE      0505 00193000
*                                                            @Y17XARL*/ 00194000
         MVI   CPACMD1(RLCB),X'01'                                 0505 00195000
*   CPACNT1=RPH2->MAPLEN;           /* SET LENGTH IN CCW     @Y17XARL*/ 00196000
         LH    @00,MAPLEN(,RPH2)                                   0506 00197000
         STH   @00,CPACNT1(,RLCB)                                  0506 00198000
*   OCTRADR=ADDR(PHASE2);           /* RESUME ADDR AFTER PHASE 2   0507 00199000
*                                      WRITTEN               @Y17XARL*/ 00200000
         L     @15,OPCCOPCE(,ROPCAVT)                              0507 00201000
         L     @15,OCTRUNIT(,@15)                                  0507 00202000
         LA    @00,PHASE2                                          0507 00203000
         ST    @00,OCTRADR(,@15)                                   0507 00204000
*   GOTO CALLIOW;                   /* GO WRITE PHASE 2      @Y17XARL*/ 00205000
         B     CALLIOW                                             0508 00206000
*PHASE2:                            /*                       @Y17XARL*/ 00207000
*   WKREG=COMTTRZ;                  /* ADDRESS OF MODULE             */ 00208000
PHASE2   L     WKREG,COMTTRZ(,RCOM)                                0509 00209000
*   GEN(DELETE EPLOC=(WKREG));      /* DELETE THE ROUTINES           */ 00210000
         DELETE EPLOC=(WKREG)                                           00211000
*                                                                  0511 00212000
*/********************************************************************/ 00213000
*/* THIS SECTION READS THE LOCAL STORE REGISTERS AND THE ADDRESS OF  */ 00214000
*/* THE END OF CORE IN THE 3705 AND WRITES THEM TO THE FIRST BLOCK   */ 00215000
*/* OF THE WORK DATA SET. THEN IT READS SUCCESSIVE 512 BYTE BLOCKS   */ 00216000
*/* OF 3705 STORAGE AND WRITES THEM TO THE WORK DATA SET UNTIL IT    */ 00217000
*/* REACHES THE END OF 3705 STORAGE.                                 */ 00218000
*/********************************************************************/ 00219000
*                                                                  0511 00220000
*   CPADATA1=RBUF1;                 /* IO BUFFER             @Y17XARL*/ 00221000
         STCM  RBUF1,7,CPADATA1(RLCB)                              0511 00222000
*   CPACMD1=CCWREAD;                /* SET READ COMMAND      @Y17XARL*/ 00223000
         MVI   CPACMD1(RLCB),X'02'                                 0512 00224000
*   CPACNT1=REGCNT;                 /* LENGTH OF DATA TO READ      0513 00225000
*                                                            @Y17XARL*/ 00226000
         MVC   CPACNT1(2,RLCB),@CH00082                            0513 00227000
*   OCTRADR=ADDR(READREG);          /* RESUME ADDR AFTER REGS ARE  0514 00228000
*                                      READ                  @Y17XARL*/ 00229000
         L     @15,OPCCOPCE(,ROPCAVT)                              0514 00230000
         L     @15,OCTRUNIT(,@15)                                  0514 00231000
         LA    @00,READREG                                         0514 00232000
         ST    @00,OCTRADR(,@15)                                   0514 00233000
*   GOTO CALLIOW;                   /* GO READ REGS          @Y17XARL*/ 00234000
         B     CALLIOW                                             0515 00235000
*READREG:                           /*                       @Y17XARL*/ 00236000
*   RENDADR=RBUF1->MAPB&TOFF;       /* ISOLATE ENDING ADDRESS FOR THE   00237000
*                                      DUMP                          */ 00238000
READREG  L     RENDADR,MAPB(,RBUF1)                                0516 00239000
         N     RENDADR,TOFF                                        0516 00240000
*   CPA=CCWAREA;                    /* SET UP FOR DATA TRANSFER    0517 00241000
*                                                            @Y17XARL*/ 00242000
         MVC   CPA(24,RLCB),CCWAREA                                0517 00243000
*   CPADATA1=ADDR(COMNEXT);         /* NEXT DATA TO READ     @Y17XARL*/ 00244000
         LA    @00,COMNEXT(,RCOM)                                  0518 00245000
         STCM  @00,7,CPADATA1(RLCB)                                0518 00246000
*   CPACMD1=CCWWRITE;               /* SET WRITE COMMAND     @Y17XARL*/ 00247000
         MVI   CPACMD1(RLCB),X'01'                                 0519 00248000
*   CPADATA3=ADDR(CPACMD3);         /* SET ADDR OF CCW       @Y17XARL*/ 00249000
         LA    @00,CPACMD3(,RLCB)                                  0520 00250000
         STCM  @00,7,CPADATA3(RLCB)                                0520 00251000
*   IF RENDADR<=SIZ48K              /* IS IT LT 48 K                 */ 00252000
*     THEN                          /* YES                           */ 00253000
         C     RENDADR,@CF00087                                    0521 00254000
         BH    @RF00521                                            0521 00255000
*     DO;                                                          0522 00256000
*       RFY                                                        0523 00257000
*         RCTR RSTD;                /*                       @Y17XARL*/ 00258000
*       WKREG=RBUF1+FIVE;           /* IF THIS MACHINE IS 48K OR     */ 00259000
         LA    WKREG,5                                             0524 00260000
         ALR   WKREG,RBUF1                                         0524 00261000
*       RCTR=EIGHT;                 /* SMALLER THERE MAY BE          */ 00262000
         LA    RCTR,8                                              0525 00263000
*LOOP:                              /* EXTRA BITS ON IN THE AREA     */ 00264000
*       WKREG->MAPC='00'X;          /* WHERE THE GROUP ZERO REGS   0526 00265000
*                                                            @Y17XARL*/ 00266000
LOOP     MVI   MAPC(WKREG),X'00'                                   0526 00267000
*       WKREG=WKREG+FOUR;           /* WERE STORED. THESE BITS       */ 00268000
         AL    WKREG,@CF00059                                      0527 00269000
*       GEN(BCT RCTR,LOOP);         /* MUST BE TURNED OFF            */ 00270000
         BCT RCTR,LOOP                                                  00271000
*       RFY                                                        0529 00272000
*         RCTR UNRSTD;              /*                       @Y17XARL*/ 00273000
*     END;                                                         0530 00274000
*IOLOOP:                                                           0531 00275000
*   CPADATA2=RBUF2;                 /* UPDATE BUFFER POINTERS      0531 00276000
*                                                            @Y17XARL*/ 00277000
@RF00521 DS    0H                                                  0531 00278000
IOLOOP   STCM  RBUF2,7,CPADATA2(RLCB)                              0531 00279000
*   RBUF2=RBUF1;                    /* RBUF2 IS NOW FILLED BUFFER    */ 00280000
         LR    RBUF2,RBUF1                                         0532 00281000
*   RBUF1=CPADATA2;                 /* RBUF1 IS EMPTY BUFFER @Y17XARL*/ 00282000
         L     @15,CPADATA2-1(,RLCB)                               0533 00283000
         LA    @15,0(,@15)                                         0533 00284000
         LR    RBUF1,@15                                           0533 00285000
*   CPACMD2=CCWREAD;                /* INSERT READ CMD       @Y17XARL*/ 00286000
         MVI   CPACMD2(RLCB),X'02'                                 0534 00287000
*   RFY                                                            0535 00288000
*     RDCB RSTD;                    /*                       @Y17XARL*/ 00289000
*   RDCB=NCPDPDCB;                  /* GET DUMP DCB ADDRESS  @Y17XARL*/ 00290000
         L     @15,OPCCOPCE(,ROPCAVT)                              0536 00291000
         L     @15,OCTRUNIT(,@15)                                  0536 00292000
         L     @15,OCTRDVD(,@15)                                   0536 00293000
         ICM   RDCB,15,NCPDPDCB(@15)                               0536 00294000
*   GEN;                                                           0537 00295000
*             /*                                             @Y17XARL*/ 00296000
    WRITE  COMDECB(RCOM),SF,(4),(6),MF=E                     @Y17XARL   00297000
    CHECK  COMDECB(RCOM)                                     @Y17XARL   00298000
*   RFY                                                            0538 00299000
*     RDCB UNRSTD;                  /*                       @Y17XARL*/ 00300000
*   RNADR=RNADR+BUFSIZE;            /* CALCULATE ADDR OF NEXT DAT    */ 00301000
         AH    RNADR,BUFSIZE                                       0539 00302000
*   IF RNADR=RENDADR THEN           /* ALL OF CORE           @OY12412*/ 00303000
         CR    RNADR,RENDADR                                       0540 00304000
         BNE   @RF00540                                            0540 00305000
*     LCBSTAT1=DUMPFINL;            /* INDICATE DUMP FINAL CHANNEL 0541 00306000
*                                      PROGRAM TO IGG019TH   @YM08434*/ 00307000
         MVI   LCBSTAT1(RLCB),X'FE'                                0541 00308000
*   COMNEXT=RNADR;                  /* ADDR TO BE SENT TO 3705       */ 00309000
@RF00540 ST    RNADR,COMNEXT(,RCOM)                                0542 00310000
*   OCTRADR=ADDR(IOLOOP);           /* RESUME ADDR AFTER I/O @Y17XARL*/ 00311000
         L     @04,OPCCOPCE(,ROPCAVT)                              0543 00312000
         L     @04,OCTRUNIT(,@04)                                  0543 00313000
         LA    @00,IOLOOP                                          0543 00314000
         ST    @00,OCTRADR(,@04)                                   0543 00315000
*                                                                  0544 00316000
*   /*****************************************************************/ 00317000
*   /*                                                               */ 00318000
*   /* STARTIO OCTRADR IS SET ALREADY TO POINT TO RESUME ADDR AFTER  */ 00319000
*   /* DLC RETURNS TO THIS MODULE.                           @ZM47618*/ 00320000
*   /*                                                               */ 00321000
*   /*****************************************************************/ 00322000
*                                                                  0544 00323000
*CALLIOW:                                                          0544 00324000
*   CPACOM=OCIPLWA;                 /* SAVE ADDR OF WORK AREA      0544 00325000
*                                                            @Y17XARL*/ 00326000
CALLIOW  L     @04,OPCCOPCE(,ROPCAVT)                              0544 00327000
         L     @00,OCIPLWA(,@04)                                   0544 00328000
         ST    @00,CPACOM(,RLCB)                                   0544 00329000
*   COMOPCE=OPCCOPCE;               /* SAVE OPCE ADDRESS     @Y17XARL*/ 00330000
         ST    @04,COMOPCE(,RCOM)                                  0545 00331000
*   COMPRI=PRIHIFLG;                /* SET PRIORITY          @Y17XARL*/ 00332000
         MVI   COMPRI(RCOM),X'E9'                                  0546 00333000
*   COMFLAGS=COMDLCMP;              /* DLC RETURN TO MP      @Y17XARL*/ 00334000
         MVI   COMFLAGS(RCOM),X'12'                                0547 00335000
*   COMQCB=ADDR(AVTOPCOB);          /* SET ADDRESS OF OP CONTROL   0548 00336000
*                                      QUEUE                 @Y17XARL*/ 00337000
         L     @04,IEDQAPTR(,ROPCAVT)                              0548 00338000
         LA    @04,AVTOPCOB(,@04)                                  0548 00339000
         STCM  @04,7,COMQCB(RCOM)                                  0548 00340000
*   LCBSIO=SIOCODE;                 /* INDICATE SIO TO QEB   @ZM47618*/ 00341000
         MVI   LCBSIO(RLCB),X'30'                                  0549 00342000
*   LCBCP=LCBSTART;                 /* START OF CHAN PGM     @ZM47618*/ 00343000
         L     @04,LCBSTART-1(,RLCB)                               0550 00344000
         LA    @04,0(,@04)                                         0550 00345000
         ST    @04,LCBCP(,RLCB)                                    0550 00346000
*   REG1=ADDR(LCBQEBPL);            /* PARM LIST FOR QEB     @ZM47618*/ 00347000
         LA    REG1,LCBQEBPL(,RLCB)                                0551 00348000
*   GEN(AQCTL);                     /* GO TO SIO TO 3705     @ZM47618*/ 00349000
         AQCTL                                                          00350000
*   OCTRIP=ON;                      /* IN PROGRESS           @Y17XARL*/ 00351000
         L     @04,OPCCOPCE(,ROPCAVT)                              0553 00352000
         L     @04,OCTRUNIT(,@04)                                  0553 00353000
         OI    OCTRIP(@04),B'10000000'                             0553 00354000
*   OCTRSAVE=RBUF2;                 /* SAV ADDR OF AREA INTO WHICH 0554 00355000
*                                      REGS ARE SAVED        @Y17XARL*/ 00356000
