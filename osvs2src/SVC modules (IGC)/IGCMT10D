         BH    @RT00323                                            0323 00302000
*     GOTO CMDINV;                  /* YES, COMMAND INVALID    Y06328*/ 00303000
*   RBYSCN=RBYSCN-1;                /* REDUCE CHAR COUNT FOR EXECUTE    00304000
*                                                              Y06328*/ 00305000
         BCTR  RBYSCN,0                                            0325 00306000
*   GEN( EX RBYSCN,MOVE );          /* OPERAND IN OPCDOUBL     Y06328*/ 00307000
