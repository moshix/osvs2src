         OI    ERRFLG,B'10000000'                                  0387 00586000
*             RZERO=SYNMSG;                                        0388 00587000
         LA    RZERO,18                                            0388 00588000
*           END;                                                   0389 00589000
*         ELSE                                                     0390 00590000
