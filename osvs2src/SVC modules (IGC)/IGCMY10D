@RT00370 DS    0H                                                  0371 00529000
*     DO;                                                          0371 00530000
*       ERRFLG=ON;                                                 0372 00531000
         OI    ERRFLG,B'10000000'                                  0372 00532000
*       RZERO=SYNMSG;                                              0373 00533000
         LA    RZERO,18                                            0373 00534000
*     END;                                                         0374 00535000
