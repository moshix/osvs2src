         BNE   @RF00419                                            0419 00716000
*         DO;                                                      0420 00717000
*                                                                  0420 00718000
*           /*********************************************************/ 00719000
*           /*                                                       */ 00720000
*           /* WRONG RLN SPECIFIED                                   */ 00721000
*           /*                                                       */ 00722000
*           /*********************************************************/ 00723000
*                                                                  0421 00724000
*           ERRFLG=ON;              /* SET ERROR FLAG                */ 00725000
         OI    ERRFLG,B'10000000'                                  0421 00726000
*           RZERO=SYNMSG;           /* SET SYNTAX ERROR MSG. NO.     */ 00727000
         LA    RZERO,18                                            0422 00728000
*           RLNCTR=ZERO;            /* SET TO GET OUT OF LOOP        */ 00729000
         SLR   RLNCTR,RLNCTR                                       0423 00730000
*         END;                                                     0424 00731000
*     END;                                                         0425 00732000
@RF00419 DS    0H                                                  0425 00733000
@DE00412 LTR   RLNCTR,RLNCTR                                       0425 00734000
         BNZ   @DL00412                                            0425 00735000
*   ELSE                                                           0426 00736000
*     DO;                                                          0426 00737000
*                                                                  0426 00738000
         B     @RC00411                                            0426 00739000
@RF00411 DS    0H                                                  0427 00740000
*       /*************************************************************/ 00741000
*       /*                                                           */ 00742000
*       /* SUBSTRUCT IS 0 INDICATING NO LINES UNDER GROUP ENTRY      */ 00743000
*       /*                                                           */ 00744000
