*         /***********************************************************/ 00552000
*         /*                                                         */ 00553000
*         /* CONVERT RLN TO A BINARY VALUE                           */ 00554000
*         /*                                                         */ 00555000
*         /***********************************************************/ 00556000
*                                                                  0381 00557000
*         DECRLN='000';             /* INITIALIZE                    */ 00558000
         MVC   DECRLN(3),@CC01854                                  0381 00559000
*         DECRLN(RLNMAXL-FLDLGTH+1:RLNMAXL)=/* MOVE FIELD            */ 00560000
*             WORKAREA(POS1:FLDLGTH);                              0382 00561000
         LA    @14,3                                               0382 00562000
         SLR   @14,FLDLGTH                                         0382 00563000
         LA    @11,DECRLN(@14)                                     0382 00564000
         LA    @14,3                                               0382 00565000
         SLR   @14,FLDLGTH                                         0382 00566000
         LCR   @14,@14                                             0382 00567000
