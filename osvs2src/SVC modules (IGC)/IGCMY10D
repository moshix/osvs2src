*   RFY                                                            0360 00482000
*     RRET RSTD;                                                   0360 00483000
*   GRP14SAV=RRET;                  /* SAVE RETURN ADDR.             */ 00484000
         ST    RRET,GRP14SAV                                       0361 00485000
*   RFY                                                            0362 00486000
*     RRET UNRSTD;                                                 0362 00487000
*                                                                  0363 00488000
*   /*****************************************************************/ 00489000
*   /*                                                               */ 00490000
*   /* SCAN WORKAREA FOR NEXT DELIMITER (COMMA) OR END OF CMND.      */ 00491000
*   /* RESULTANT FIELD SHOULD DEFINE THE RLN. WHEN SCANNING,INSURE   */ 00492000
*   /* EACH CHARACTER IN FIELD ARE EBCIDIC NUMBERS.                  */ 00493000
*   /*                                                               */ 00494000
*   /*****************************************************************/ 00495000
*                                                                  0363 00496000
*   NUMERIC=YES;                    /* INITIALIZE                    */ 00497000
         OI    NUMERIC,B'10000000'                                 0363 00498000
*   LASTBYTE=WAPTR+WABYTES-1;       /* SET UPPER LIMIT FOR LOOP AS 0364 00499000
*                                      LAST BYTE IN CMND. STRING     */ 00500000
         L     @14,WAPTR                                           0364 00501000
         LH    LASTBYTE,WABYTES                                    0364 00502000
         ALR   LASTBYTE,@14                                        0364 00503000
         BCTR  LASTBYTE,0                                          0364 00504000
