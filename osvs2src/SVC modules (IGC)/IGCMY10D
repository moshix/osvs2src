         BZ    @RF00411                                            0411 00681000
*     DO WHILE(RLNCTR^=0);                                         0412 00682000
*                                                                  0412 00683000
         B     @DE00412                                            0412 00684000
@DL00412 DS    0H                                                  0413 00685000
*       /*************************************************************/ 00686000
*       /*                                                           */ 00687000
*       /* LOOP UNTIL COUNTER GOES TO ZERO OR THE GROUP COHORT       */ 00688000
*       /* POINTER POINTS BACK TO THE GROUP ENTRY INDICATING THE RLN */ 00689000
*       /* SPECIFIED IS TOO LARGE                                    */ 00690000
*       /*                                                           */ 00691000
*       /*************************************************************/ 00692000
*                                                                  0413 00693000
*       RLNCTR=RLNCTR-1;            /* DECREMENT LOOP CTL.           */ 00694000
         BCTR  RLNCTR,0                                            0413 00695000
