*GOODDCB:                                                       03S0368 00115000
*   RDCB=RPARM;                     /* SET DCB BASE                  */ 00116000
@RC00356 DS    0H                                               03S0368 00117000
GOODDCB  LR    RDCB,RPARM                                       03S0368 00118000
*   IF DCBDSGTR=OFF THEN            /* CHECK DCB FOR 3705            */ 00119000
         TM    DCBDSGTR+20(RDCB),B'00000100'                    03S0369 00120000
         BNZ   @RF00369                                         03S0369 00121000
*     DO;                                                       03S0370 00122000
*       RZERO=NO3705;               /* SET NOT 3705 MSG NUMBER       */ 00123000
         LH    RZERO,NO3705                                     03S0371 00124000
*       CALL ERREXIT;               /* CALL THE ERROR EXIT RTN       */ 00125000
         BAL   @14,ERREXIT                                      03S0372 00126000
*     END;                                                      03S0373 00127000
*   ELSE                                                        03S0374 00128000
*     DO;                                                       03S0374 00129000
         B     @RC00369                                         03S0374 00130000
@RF00369 DS    0H                                               03S0375 00131000
*       RLCB=DCBIOBAD+DCBEIOBX-LCBOFF;/* SET UP FIRST LCB ADDRESS    */ 00132000
         LA    @11,20                                           03S0375 00133000
         ALR   @11,RDCB                                         03S0375 00134000
