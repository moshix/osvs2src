         IC    RLCB,DCBEIOBX(,@11)                              03S0375 00136000
         AL    RLCB,DCBIOBAD(,@11)                              03S0375 00137000
         SL    RLCB,@CF00044                                    03S0375 00138000
*       RPARM=LCBTTCIN;             /* GET NCP TTCIN                 */ 00139000
