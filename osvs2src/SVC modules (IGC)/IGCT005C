*           RDXPTR=RDXPTR+LENGTH(RDXENTRY);/* GO TO NEXT ENTRY       */ 00798000
@RF00377 AH    RDXPTR,@CH00076                                     0384 00799000
*       END RDXLOOP;                /* RETURN TO CHECK NEXT ENTRY    */ 00800000
