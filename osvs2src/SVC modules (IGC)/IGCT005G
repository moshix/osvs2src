         OI    SDWAFREE(GPR01P),B'00000100'                        0423 00905000
         NI    SDWARCRD(GPR01P),B'01111111'                        0423 00906000
*     GPR01P->SDWAREQ='0'B;         /* TURN OFF DUMP INDICATOR       */ 00907000
         NI    SDWAREQ(GPR01P),B'01111111'                         0424 00908000
