         LA    @12,BLKERR2-1(BLKCTR)                               0393 00811000
         MVC   RCODE(1,SDWAPTR),0(@12)                             0393 00812000
*   WRKREG=WKACLEAN;                /* LOAD CLEAN RC FOR INDEX       */ 00813000
         SR    WRKREG,WRKREG                                       0394 00814000
