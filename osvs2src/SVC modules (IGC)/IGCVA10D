         MVI   OPCAQREQ(ROPCAVT),X'0C'                             0809 01179000
*   OPCAQRQ2=ENDLIST;               /* MARK END OF LIST              */ 01180000
         MVI   OPCAQRQ2(ROPCAVT),X'80'                             0810 01181000
*   RPARM=ADDR(OPCAQCTL);           /* GET PTR TO LIST               */ 01182000
         LA    RPARM,OPCAQCTL(,ROPCAVT)                            0811 01183000
*   SVC(102);                       /* ISSUE SVC 102                 */ 01184000
         SVC   102                                                 0812 01185000
*   OPCMODID=MOD00;                 /* SET UP TO EXIT                */ 01186000
         MVC   OPCMODID(2,ROPCAVT),@CC03893                        0813 01187000
*   RZERO=0;                        /* CLEAR                         */ 01188000
         SLR   RZERO,RZERO                                         0814 01189000
*   RRET=OPCWRKSP(2);               /* RESTORE RETURN ADDR           */ 01190000
