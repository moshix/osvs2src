*       DO WHILE(RWORK7->OCTRMTBL^=RZERO&RWORK7^=0);/* LOOK FOR OPCE    00399000
*                                      ON WAIT CHAIN         @YM04624*/ 00400000
         B     @DE00539                                            0539 00401000
@DL00539 DS    0H                                                  0540 00402000
*         RWORK8=RWORK7;            /* SET NEW PREVIOUS      @YM04624*/ 00403000
         LR    RWORK8,RWORK7                                       0540 00404000
*         RWORK7=RWORK7->OCLINK;    /* GET NEXT OPCE         @YM04624*/ 00405000
