*   RFY                                                            0819 01200000
*     RCURRENT RSTD;                                               0819 01201000
*   OPCWRKSP(4)=RRET;               /* SAVE THE RETURN ADDR          */ 01202000
         ST    RRET,OPCWRKSP+12(,ROPCAVT)                          0820 01203000
*DOITAGIN:                                                         0821 01204000
*   RCURRENT=TCXREADY;              /* GET CURRENT ELEM              */ 01205000
DOITAGIN L     @15,CVTPTR                                          0821 01206000
         L     @15,TCXPTR(,@15)                                    0821 01207000
         L     RCURRENT,TCXREADY(,@15)                             0821 01208000
*   LCBLINK=RCURRENT;               /* LINK IT TO THE PLCB           */ 01209000
