*   CS(RCURRENT,RLCB,TCXREADY);     /* SWAP PLCB ON ASYN QUE         */ 01211000
         CS    RCURRENT,@03,TCXREADY(@15)                          0823 01212000
*   BC(7,DOITAGIN);                 /* FAILED TRY IT AGAIN           */ 01213000
         BC    7,DOITAGIN                                          0824 01214000
*   OPCAQFST=ADDR(AVTOSECB);        /* GET THE ECB ADDRESS           */ 01215000
         LA    @15,AVTOSECB(,IEDQAPTR)                             0825 01216000
         STCM  @15,7,OPCAQFST(ROPCAVT)                             0825 01217000
*   OPCAQREQ=REQSTCDE;              /* SET POST TYPE                 */ 01218000
