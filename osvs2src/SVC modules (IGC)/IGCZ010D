*                         OPCWRKSP(4)=RPARM;/* SAVE OPCE ADDRESS        00165000
*                                                            @YM08446*/ 00166000
@RC00403 ST    RPARM,OPCWRKSP+12(,ROPCAVT)                      04S0406 00167000
*                         CALL IPBLDACT;/* BUILD DEACT OPCE          */ 00168000
         BAL   @14,IPBLDACT                                     04S0407 00169000
*                         OPREQCB=ADDR(AVTOPCOB);/* SETUP OP CONTROL    00170000
*                                      QCB                           */ 00171000
         LA    @09,AVTOPCOB(,RAVT)                              04S0408 00172000
