*                                      OPCE                          */ 00261000
         MVC   OCCONID(1,ROPCE),OCCONID(RPARM)                  03S0440 00262000
*                 OCCONSOL=ON;      /* INDICATE FROM CONSOLE         */ 00263000
         OI    OCCONSOL(ROPCE),B'10000000'                      03S0441 00264000
*               END;                                            03S0442 00265000
*             ELSE                                              03S0443 00266000
*               OCDEST=RPARM->OCDEST;/* SAVE OCDEST FROM ORIGINAL       00267000
*                                                            @OZ30354*/ 00268000
