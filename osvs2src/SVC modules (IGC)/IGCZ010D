*                         OPREPRI=PRIHIFLG;/* SETUP POST PRIORITY    */ 00174000
         MVI   OPREPRI(RPARM),X'E9'                             04S0409 00175000
*                         OPREFLG1=OPRFC;/* SETUP FLAG VALUE         */ 00176000
         MVI   OPREFLG1(RPARM),X'09'                            04S0410 00177000
*                       END;        /*                       @YM08098*/ 00178000
*                   END;                                        04S0412 00179000
@RF00395 DS    0H                                               04S0413 00180000
*               END;                                            04S0413 00181000
@RF00389 DS    0H                                               04S0414 00182000
*             RSAT=RSAT+LENGTH(IEDSATD);/* POINT TO NEXT ENTRY       */ 00183000
@RF00387 AL    RSAT,@CF00045                                    03S0414 00184000
*             RCTR=RCTR-ONE;        /* DECREMENT NUMBER OF ENTRIES   */ 00185000
         BCTR  RCTR,0                                           03S0415 00186000
*           END;                                                03S0416 00187000
@DE00386 SLR   @09,@09                                          03S0416 00188000
         CR    RCTR,@09                                         03S0416 00189000
         BNE   @DL00386                                         03S0416 00190000
*           IF OPCECTR>0 THEN                                   03S0417 00191000
         CH    @09,OPCECTR(,ROPCE)                              03S0417 00192000
         BNL   @RF00417                                         03S0417 00193000
*             DO;                   /* IF ANY DEACTS TO BE DONE 03S0418 00194000
*                                                            @YM08121*/ 00195000
*               OPCAQWD1=OPCWRKSP(1);/* SETUP ADDRESS OF FIRST OPCE  */ 00196000
