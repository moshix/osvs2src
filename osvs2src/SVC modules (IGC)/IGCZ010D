         N     @07,@CF02669                                     04S0389 00092000
         CR    @09,@07                                          04S0389 00093000
         BE    @RF00389                                         04S0389 00094000
*                   DO;             /* AND THIS IS NOT A HOST SUBAREA*/ 00095000
*                     OCTRMTBL=RVTINDEX;/* SAVE NCP TNT              */ 00096000
         STH   @09,OCTRMTBL(,ROPCE)                             04S0391 00097000
*                     RPARM=OCTRMTBL;/* NCP TNT OFFSET       @YM08098*/ 00098000
         LR    RPARM,@09                                        04S0392 00099000
*                     CALL IEDQTNT; /* GET TTE ADDRESS       @YM08098*/ 00100000
         L     @15,AVTRNMPT(,RAVT)                              04S0393 00101000
         BALR  @14,@15                                          04S0393 00102000
*                     RTRM=RPARM-LENGTH(IEDNTRM);/* TRM         04S0394 00103000
*                                      ADDRESSABILITY        @YM08098*/ 00104000
         LR    RTRM,RPARM                                       04S0394 00105000
         SL    RTRM,@CF02354                                    04S0394 00106000
*                     IF TRMNCPCL=OFF THEN/* CLOSEDOWN NOT ALREADY IN   00107000
*                                      PROGRESS              @YM08098*/ 00108000
