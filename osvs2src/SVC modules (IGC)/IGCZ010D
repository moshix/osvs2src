*               OPCAQWD2=RPARM;     /* SETUP ADDRESS OF LAST OPCE    */ 00199000
         ST    RPARM,OPCAQWD2(,ROPCAVT)                         03S0420 00200000
*               OPCAQREQ=OPREQ;     /* INDICATE MORE THAN ONE OPCE   */ 00201000
         MVC   OPCAQREQ(1,ROPCAVT),OPREQ                        03S0421 00202000
*               RPARM=ADDR(OPCAQCTL);/* SETUP ADDR OF AQCTL LIST     */ 00203000
         LA    RPARM,OPCAQCTL(,ROPCAVT)                         03S0422 00204000
*               GEN( SVC 102);      /* POST OPCES                    */ 00205000
          SVC 102                                                       00206000
*               OPCMODID=MOD00;     /* SETUP ID OF NEXT MOD          */ 00207000
         MVC   OPCMODID(2,ROPCAVT),MOD00                        03S0424 00208000
*               OCWTG=''B;          /* CLEAR WTG FIELD       @YM08121*/ 00209000
         XC    OCWTG(2,ROPCE),OCWTG(ROPCE)                      03S0425 00210000
*             END;                                              03S0426 00211000
*         END;                                                  03S0427 00212000
*       ELSE                                                    03S0428 00213000
*         CALL IPTSOCK;             /* CHECK FOR TSO IN SYSTEM  03S0428 00214000
*                                                            @OZ30501*/ 00215000
         B     @RC00381                                         03S0428 00216000
@RF00381 BAL   @14,IPTSOCK                                      03S0428 00217000
*       RFY                                                     03S0429 00218000
*        (RSAT,                                                 03S0429 00219000
*         RCTR) UNRSTD;                                         03S0429 00220000
@RC00381 DS    0H                                               03S0430 00221000
*     END;                                                      02S0430 00222000
*   ELSE                                                        02S0431 00223000
*     IF OCSWITCH=OCDEACTE THEN                                 02S0431 00224000
         B     @RC00374                                         02S0431 00225000
@RF00374 CLI   OCSWITCH(ROPCE),X'23'                            02S0431 00226000
         BNE   @RF00431                                         02S0431 00227000
*       DO;                         /* IF THIS IS AN ENTRY FROM 02S0432 00228000
*                                      DEACT, PROCESS DEACT RESPONSE */ 00229000
*                                                               03S0433 00230000
*         /***********************************************************/ 00231000
*         /*                                                         */ 00232000
*         /* PROCESS.DEACT: THIS SEGMENT DETERMINES IF INITIAL       */ 00233000
*         /* REQUEST WAS FOR A SYSTEM CLOSEDOWN OR CLOSEDOWN OF ONE  */ 00234000
*         /* NCP. IF CLOSEDOWN OF ONE NCP, ORIGINAL OPCE IS FREED AND*/ 00235000
*         /* CONTROL IS PASSED TO CHECKPOINT, ALONG WITH DEACT OPCE. */ 00236000
*         /* IF REQUEST WAS FOR A SYSTEM CLOSEDOWN, DEACT OPCE IS    */ 00237000
*         /* FREED. IF THIS IS LAST OPCE TO BE FREED, CONTROL IS     */ 00238000
*         /* PASSED TO CHECKPOINT. ELSE MORE DEACT OPCES EXPECTED AND*/ 00239000
*         /* CONTROL PASSED TO IGC0010D                              */ 00240000
*         /*                                                         */ 00241000
*         /***********************************************************/ 00242000
*                                                               03S0433 00243000
*         RFY                                                   03S0433 00244000
*           RTEMP RSTD;                                         03S0433 00245000
*         RPARM=OCPTRSAV;           /* SETUP PTR TO ORIGINAL OPCE    */ 00246000
         L     RPARM,OCPTRSAV(,ROPCE)                           03S0434 00247000
*         IF RPARM->CLOSEONE=ON THEN                            03S0435 00248000
         TM    CLOSEONE(RPARM),B'10000000'                      03S0435 00249000
         BNO   @RF00435                                         03S0435 00250000
*           DO;                     /* IF REQST TO CLOSEDOWN ONE NCP,   00251000
*                                      SEND DEACT OPCE TO CHKPOINT   */ 00252000
