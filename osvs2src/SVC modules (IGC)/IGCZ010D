         TITLE 'IGCZ010D, MCPCLOSE SCAN/MAP/DISPATCH ROUTINE           *00001000
                        '                                               00002000
IGCZ010D CSECT ,                                                01S0001 00003000
@PROLOG  DS    0H                                               01S0002 00004000
         USING *,RBASE                  ESTABLISH A BASE REGISTER       00005000
         LR    RBASE,REG15              SETUP BASE REG                  00006000
IGCZ010D IEDHJN AFTERHJN,HJN            MODULE IDENTIFICATION MACRO     00007000
*   ROPCAVT=RPARM;                  /* SET BASE POINTER FOR OP  03S0369 00008000
*                                      CONTROL AVT                   */ 00009000
         LR    ROPCAVT,RPARM                                    03S0369 00010000
*   RAVT=OPCAVTPT;                  /* SET THE BASE POINTER FOR THE     00011000
*                                      TCAM ADDRESS VECTOR TABLE     */ 00012000
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0370 00013000
*   ROPCE=OPCCOPCE;                 /* SET THE BASE POINTER FOR THE     00014000
*                                      OPERATOR CONTROL ELEMENT      */ 00015000
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0371 00016000
*   RSAVE=ADDR(OPCSAVE);            /* SETUP A SAVEAREA              */ 00017000
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          03S0372 00018000
*   OPCFLAG1=ZERO;                  /* CLEAR CONTROL SWITCHES        */ 00019000
         MVI   OPCFLAG1(ROPCAVT),X'00'                          03S0373 00020000
*   IF OPCFLAG2=ENTRY2 THEN                                     02S0374 00021000
         CLC   OPCFLAG2(1,ROPCAVT),ENTRY2                       02S0374 00022000
         BNE   @RF00374                                         02S0374 00023000
*     DO;                           /* IF THIS IS A RE-ENTRY FROM       00024000
*                                      IGCZ110D, PROCESS SYSTEM 02S0375 00025000
*                                      CLOSEDOWN                     */ 00026000
*                                                               03S0376 00027000
*       /*************************************************************/ 00028000
*       /*                                                           */ 00029000
*       /* PROCESS.REENTRY: THIS SEGMENT CONTINUES PROCESSING SYSTEM */ 00030000
*       /* CLOSEDOWN AFTER RETURN FROM IGCZ110D. IF A SAT TABLE      */ 00031000
*       /* EXISTS, 3705 DEACTIVATION IS NECESSARY.THE SAT TABLE IS   */ 00032000
*       /* USED TO FIND THE TTCIN OF EACH NCP IN THE SYSTEM. A DEACT */ 00033000
*       /* OPCE IS BUILT FOR EACH NCP FOUND AND THE TTCIN OF EACH NCP*/ 00034000
*       /* IS STORED IN THE DEACT OPCE. THESE DEACT OPCES ARE POSTED */ 00035000
*       /* TO THE OP CONTROL QCB AND CONTROL IS PASSED TO IGC0010D.  */ 00036000
*       /* IF 3705 DEACTIVATION NOT NECESSARY, CONTROL IS PASSED TO  */ 00037000
*       /* THE CHECKPOINT REOUTINE, IGC0610D, TO PERFORM AN          */ 00038000
*       /* ENVIRONMENT CHECKPOINT                                    */ 00039000
*       /*                                                           */ 00040000
*       /*************************************************************/ 00041000
*                                                               03S0376 00042000
*       RFY                                                     03S0376 00043000
*        (RSAT,                                                 03S0376 00044000
*         RCTR) RSTD;                                           03S0376 00045000
*       OPCFLAG2=ZERO;              /* RESET ENTRY FLAG              */ 00046000
         MVI   OPCFLAG2(ROPCAVT),X'00'                          03S0377 00047000
*       OCPARM=ZERO;                /* SETUP MESSAGE NUMBER  @YM08121*/ 00048000
         SLR   @09,@09                                          03S0378 00049000
         STH   @09,OCPARM(,ROPCE)                               03S0378 00050000
*       OCWTG=MOD00;                /* NEXT MOD TO GET CONTROL AFTER    00051000
*                                      CHKPOINT              @YM08121*/ 00052000
         MVC   OCWTG(2,ROPCE),MOD00                             03S0379 00053000
*       OPCMODID=MOD06;             /* SETUP ID OF CKPT MOD  @YM08121*/ 00054000
         MVC   OPCMODID(2,ROPCAVT),MOD06                        03S0380 00055000
*       IF AVTVTMCP=ON THEN                                     03S0381 00056000
         TM    AVTVTMCP(RAVT),B'10000000'                       03S0381 00057000
         BNO   @RF00381                                         03S0381 00058000
*         DO;                       /* IF 3705 ENVIRONMENT BUILD        00059000
*                                      DEACT OPCES FOR ALL NCPS 03S0382 00060000
*                                                            @OY16432*/ 00061000
*           RSAT=SAVTSAT;           /* POINT TO START OF SAT TABLE   */ 00062000
         L     @07,AVTSAVTP(,RAVT)                              03S0383 00063000
         L     RSAT,SAVTSAT(,@07)                               03S0383 00064000
*           RCTR=SAVTSUBA;          /* SETUP NUMBER OF SAT ENTRIES   */ 00065000
         SLR   RCTR,RCTR                                        03S0384 00066000
         IC    RCTR,SAVTSUBA(,@07)                              03S0384 00067000
*           OPCECTR=0;              /* CLEAR OPCE COUNTER            */ 00068000
         STH   @09,OPCECTR(,ROPCE)                              03S0385 00069000
*           DO UNTIL(RCTR=0);       /* SEARCH THROUGH ALL OF SAT        00070000
*                                      TABLE FOR RVT ENTRIES         */ 00071000
@DL00386 DS    0H                                               03S0387 00072000
*                                                               04S0387 00073000
*             /*******************************************************/ 00074000
*             /*                                                     */ 00075000
*             /* PROCESS.REENTRY.SCANRVT: THIS SEGMENT GETS NCP TNT  */ 00076000
*             /* FROM RVT, STORES IT IN ORIGINAL OPCE, GETS A BUFFER */ 00077000
*             /* AND BUILDS A DEACT OPCE WHICH LATER GETS POSTED TO  */ 00078000
*             /* THE OP CONTROL QCB                                  */ 00079000
*             /*                                                     */ 00080000
*             /*******************************************************/ 00081000
*                                                               04S0387 00082000
*             IF SATRNRVT^=ZERO THEN                            04S0387 00083000
         L     @09,SATRNRVT(,RSAT)                              04S0387 00084000
         LTR   @09,@09                                          04S0387 00085000
         BZ    @RF00387                                         04S0387 00086000
*               DO;                 /* IF RVT POINTER EXISTS         */ 00087000
*                 IF RVTINDEX^=SAVTTCPU THEN                    04S0389 00088000
