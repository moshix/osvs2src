*                         IF OCQUCKN=ON THEN/* QUICK CLOSEDOWN  04S0398 00125000
*                                                            @YM08098*/ 00126000
         TM    OCQUCKN(ROPCE),B'10000000'                       04S0398 00127000
         BNO   @RF00398                                         04S0398 00128000
*                           TRMQUCKN=ON;/* SET QUICK CLOSE INDICATOR    00129000
*                                                            @YM08098*/ 00130000
         LA    @11,TRMQUCKN(,RTRM)                              04S0399 00131000
         LR    @10,@11                                          04S0399 00132000
         LA    @09,3                                            04S0399 00133000
         NR    @10,@09                                          04S0399 00134000
         LCR   @10,@10                                          04S0399 00135000
         ALR   @11,@10                                          04S0399 00136000
         L     @09,0(,@11)                                      04S0399 00137000
@GL00002 LR    @07,@09                                          04S0399 00138000
         O     @07,@CB02674+3(@10)                              04S0399 00139000
         CS    @09,@07,0(@11)                                   04S0399 00140000
         BNE   @GL00002                                         04S0399 00141000
*                         RPARM=ONE;/* SETUP TO GET ONE BUFFER       */ 00142000
@RF00398 LA    RPARM,1                                          04S0400 00143000
*                         CALL ALLOCBUF;/* GET BUFFER                */ 00144000
         L     @15,OPCGETBF(,ROPCAVT)                           04S0401 00145000
         BALR  @14,@15                                          04S0401 00146000
*                         OPCECTR=OPCECTR+ONE;/* INCREMENT OPCE 04S0402 00147000
*                                      COUNTER                       */ 00148000
         LA    @09,1                                            04S0402 00149000
         LH    @07,OPCECTR(,ROPCE)                              04S0402 00150000
         ALR   @07,@09                                          04S0402 00151000
         STH   @07,OPCECTR(,ROPCE)                              04S0402 00152000
*                         IF OPCECTR=ONE THEN/* IF THIS IS FIRST OPCE*/ 00153000
         CR    @07,@09                                          04S0403 00154000
         BNE   @RF00403                                         04S0403 00155000
*                           OPCWRKSP(1)=RPARM;/* SAVE ADDR OF FIRST     00156000
*                                      OPCE                          */ 00157000
         ST    RPARM,OPCWRKSP(,ROPCAVT)                         04S0404 00158000
*                         ELSE                                  04S0405 00159000
*                           OPCWRKSP(4)->OCELMPTR=RPARM;/* ATTACH       00160000
*                                      OPCE TO LAST OPCE     @YM08446*/ 00161000
         B     @RC00403                                         04S0405 00162000
@RF00403 L     @09,OPCWRKSP+12(,ROPCAVT)                        04S0405 00163000
