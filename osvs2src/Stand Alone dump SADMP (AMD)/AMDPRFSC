         TITLE 'AMDPRFSC - FORMATTING SERVICE CONTROL ROUTINE          *00001000
                       '                                                00002000
AMDPRFSC CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL2(@EP00001-@MAINENT)                                   00007000
         DC    AL1(16)                                          01S0001 00008000
         DC    C'AMDPRFSC  73.353'                              01S0001 00009000
FORMAT   DS    0H                                               01S0001 00010000
         USING *,@15                                            01S0001 00011000
         B     @PROLOG                                          01S0001 00012000
         DC    AL2(@EP00095-FORMAT)                                     00013000
         ENTRY FORMAT                                                   00014000
TCBSMRY  DS    0H                                               01S0001 00015000
         USING *,@15                                            01S0001 00016000
         B     @PROLOG                                          01S0001 00017000
         DC    AL2(@EP00675-TCBSMRY)                                    00018000
         ENTRY TCBSMRY                                                  00019000
         DROP  @15                                                      00020000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00021000
         BALR  @11,0                                            01S0001 00022000
@PSTART  LA    @10,4095(,@11)                                   01S0001 00023000
         USING @PSTART,@11                                      01S0001 00024000
         USING @PSTART+4095,@10                                 01S0001 00025000
         ST    @13,@SA00001+4                                   01S0001 00026000
         LA    @14,@SA00001                                     01S0001 00027000
         ST    @14,8(,@13)                                      01S0001 00028000
         LR    @13,@14                                          01S0001 00029000
         AH    @15,4(,@15)                                      01S0001 00030000
         BR    @15                                              01S0001 00031000
@EP00001 DS    0H                                               01S0002 00032000
*/*                                                                  */ 00033000
*/* INITIALIZATION ROUTINE - ENTRY FROM AMDPRSEG                     */ 00034000
*/*                                                                  */ 00035000
*/* STORE 2 ENTRY POINT ADDRS IN COMMON FOR OTHER ROUTINES TO USE    */ 00036000
*AFORMAT=ADDR(FORMAT);                                          01S0092 00037000
         L     @15,@CA02162                                     01S0092 00038000
         ST    @15,AFORMAT(,COMBASE)                            01S0092 00039000
*ATCBSMRY=ADDR(TCBSMRY);                                        01S0093 00040000
         L     @15,@CA02163                                     01S0093 00041000
         ST    @15,ATCBSMRY(,COMBASE)                           01S0093 00042000
*RETURN;                                                        01S0094 00043000
@EL00001 L     @13,4(,@13)                                      01S0094 00044000
@EF00001 DS    0H                                               01S0094 00045000
@ER00001 LM    @14,@12,12(@13)                                  01S0094 00046000
         BR    @14                                              01S0094 00047000
*/*                                                             01S0095 00048000
*   FORMAT - ENTRY TO FORMAT CONTROL BLOCKS.                    01S0095 00049000
*     THIS ROUTINE PERFORMS INITIALIZATION FOR THE STOP AND ERROR       00050000
*     ROUTINES AND ACTIVATES THEM. THEN THE PARAMETER LIST IS   01S0095 00051000
*     EXAMINED TO DETERMINE THE PROCESSING NEEDED. THE PARMLIST 01S0095 00052000
*     WILL CONTAIN EITHER AN INDICATOR THAT ALL ASCBS ARE TO BE 01S0095 00053000
*     DONE, OR AN ASCB ADDRESS AND THE CORRESPONDING ASID. AFTER        00054000
*     THE FORMATTING IS COMPLETED, THE ERROR ROUTINE IS DE-     01S0095 00055000
*     ACTIVATED, AND THE ASCBLIST IS CLEANED UP. RETURN IS MADE 01S0095 00056000
*     TO THE CALLER WITH THE STOP ROUTINE STILL ACTIVE BECAUSE  01S0095 00057000
*     TCB SUMMARY TABLES HAVE PROBABLY BEEN CREATED. THE CALLER 01S0095 00058000
*     MUST EVENTUALLY CALL TCBSMRY TO FREE THE TABLES           01S0095 00059000
*     AND DE-ACTIVATE THE STOP ROUTINE.                         01S0095 00060000
*                                                                    */ 00061000
*FORMAT:                                                        01S0095 00062000
*  ENTRY (PASCB,PASID);                                         01S0095 00063000
@EP00095 MVC   @PC00001(8),0(@01)                               01S0095 00064000
*RFY R13 RSTD;                                                  01S0096 00065000
*SAVER13=R13;                           /* SAVE R13 FOR ERR OR STOP  */ 00066000
         ST    R13,SAVER13                                      01S0097 00067000
*RFY R13 UNRSTD;                                                01S0098 00068000
*SAVERR=ERRADDR;                        /* SWAP ERROR ROUTINES       */ 00069000
         MVC   SAVERR(4),ERRADDR(COMBASE)                       01S0099 00070000
*ERRADDR=ADDR(FSCERR);                                          01S0100 00071000
         LA    @15,FSCERR                                       01S0100 00072000
         ST    @15,ERRADDR(,COMBASE)                            01S0100 00073000
*IF STOPSAVD = OFF THEN                 /* DO NOT SWAP STOP ROUTINE  */ 00074000
         TM    STOPSAVD,B'00000001'                             01S0101 00075000
         BNZ   @RF00101                                         01S0101 00076000
*  DO;                                  /* IF ALREADY THERE          */ 00077000
*    SAVSTOP=STOPEXIT;                                          01S0103 00078000
         MVC   SAVSTOP(4),STOPEXIT(COMBASE)                     01S0103 00079000
*    STOPEXIT=ADDR(FSCSTOP);                                    01S0104 00080000
         LA    @15,FSCSTOP                                      01S0104 00081000
         ST    @15,STOPEXIT(,COMBASE)                           01S0104 00082000
*    STOPSAVD=ON;                                               01S0105 00083000
         OI    STOPSAVD,B'00000001'                             01S0105 00084000
*  END;                                                         01S0106 00085000
*/* GET CURRENT TCB ADDRESS (IF AVAILABLE) FOR AMDPRFTC              */ 00086000
*                                                               01S0107 00087000
* PREG=ADDR(PSATOLD);                                           01S0107 00088000
@RF00101 LA    PREG,PSATOLD                                     01S0107 00089000
* R0=0;                                                         01S0108 00090000
         SR    R0,R0                                            01S0108 00091000
* GEN REFS(BRRDDATA)SETS(PREG,READERR)                          01S0109 00092000
*     FLOWS(FSCERR)                                             01S0109 00093000
*   (BRREAD (PREG),,ASID=(R0));                                 01S0109 00094000
         BRREAD (PREG),,ASID=(R0)                                       00095000
*IF READERR = OFF THEN                                          01S0110 00096000
         TM    READERR,B'00000010'                              01S0110 00097000
         BNZ   @RF00110                                         01S0110 00098000
*  CURTCB=RDPTR;                                                01S0111 00099000
         L     @15,@CA01872                                     01S0111 00100000
         SR    @14,@14                                          01S0111 00101000
         ICM   @14,7,RDPTR(PREG)                                01S0111 00102000
         ST    @14,CURTCB-CURTCB(,@15)                          01S0111 00103000
*ELSE                                                           01S0112 00104000
*  DO;                                                          01S0112 00105000
         B     @RC00110                                         01S0112 00106000
@RF00110 DS    0H                                               01S0113 00107000
*    CURTCB=0;                                                  01S0113 00108000
         L     @15,@CA01872                                     01S0113 00109000
         SR    @14,@14                                          01S0113 00110000
         ST    @14,CURTCB-CURTCB(,@15)                          01S0113 00111000
*    READERR=OFF;                                               01S0114 00112000
         NI    READERR,B'11111101'                              01S0114 00113000
*  END;                                                         01S0115 00114000
*IF PASCB=ALLFLAG THEN                  /* SHOULD WE DO ALL ASCBS    */ 00115000
@RC00110 L     @15,@PC00001                                     01S0116 00116000
         CLC   PASCB(4,@15),@CF01848                            01S0116 00117000
         BNE   @RF00116                                         01S0116 00118000
*  DO;                                  /* YES, GO GET AS MANY ASCBS */ 00119000
*                                       /*    AS POSSIBLE            */ 00120000
*/*                                                             02S0118 00121000
*   THIS SEGMENT ATTEMPTS TO PROCESS ALL ASCBS IN THE DUMP      02S0118 00122000
*                                                                    */ 00123000
*/* IF CVT IS IN THE DUMP, WE MAY FIND SOME ASCBS SOMEWHERE          */ 00124000
*IF CVTADDR ^= 0 THEN                                           02S0118 00125000
         L     @15,CVTADDR(,COMBASE)                            02S0118 00126000
         LTR   @15,@15                                          02S0118 00127000
         BZ    @RF00118                                         02S0118 00128000
*  DO;                                                          02S0119 00129000
*    CVTPTR=CVTADDR;                                            02S0120 00130000
         LR    CVTPTR,@15                                       02S0120 00131000
*/* READ FOR ASVT POINTER                                            */ 00132000
*                                                               02S0121 00133000
*    PREG=ADDR(CVTASVT);                                        02S0121 00134000
         LA    PREG,CVTASVT(,CVTPTR)                            02S0121 00135000
*    GEN REFS(BRRDDATA)SETS(PREG,READERR)                       02S0122 00136000
*        FLOWS(FSCERR)                                          02S0122 00137000
*      (BRREAD (PREG));                                         02S0122 00138000
         BRREAD (PREG)                                                  00139000
*/* IF ASVT POINTER WAS READ, USE IT                                 */ 00140000
*    IF READERR=OFF THEN                                        02S0123 00141000
         TM    READERR,B'00000010'                              02S0123 00142000
         BNZ   @RF00123                                         02S0123 00143000
*      DO;                                                      02S0124 00144000
*        ASVTPTR=RDPTR;                                         02S0125 00145000
         SR    ASVTPTR,ASVTPTR                                  02S0125 00146000
         ICM   ASVTPTR,7,RDPTR(PREG)                            02S0125 00147000
*        IF ASVTPTR ^= 0 THEN           /* IS ASVT THERE             */ 00148000
         LTR   ASVTPTR,ASVTPTR                                  02S0126 00149000
         BZ    @RF00126                                         02S0126 00150000
*          DO;                          /* YES                       */ 00151000
*/* READ MAXIMUM USER FIELD FROM ASVT (FOR LOOP CONTROL)             */ 00152000
*                                                               02S0128 00153000
*            PREG=ADDR(ASVTMAXU);                               02S0128 00154000
         LA    PREG,ASVTMAXU(,ASVTPTR)                          02S0128 00155000
*            GEN REFS(BRRDDATA)SETS(PREG,READERR)               02S0129 00156000
*                FLOWS(FSCERR)                                  02S0129 00157000
*              (BRREAD (PREG));                                 02S0129 00158000
         BRREAD (PREG)                                                  00159000
*/* IF ASVTMAXU WAS READ OK, LOOP THRU ASVT GETTING ASCBS            */ 00160000
*            IF READERR=OFF THEN                                02S0130 00161000
         TM    READERR,B'00000010'                              02S0130 00162000
         BNZ   @RF00130                                         02S0130 00163000
*              DO;                                              02S0131 00164000
*                ASMAXU=RDWORD;                                 02S0132 00165000
         MVC   ASMAXU(4),RDWORD(PREG)                           02S0132 00166000
*/* INITIALIZE COUNTERS FOR ASVT LOOP                                */ 00167000
*                ASCBCNT=1;                                     02S0133 00168000
         LA    @06,1                                            02S0133 00169000
         LR    ASCBCNT,@06                                      02S0133 00170000
*/*                                                             03S0134 00171000
*   THIS SEGMENT SEARCHES THE ASVT FOR ASCBS                    03S0134 00172000
*                                                                    */ 00173000
*DO INCTR=1 TO ASMAXU WHILE OVERLIMT=OFF; /* LOOP THRU ASVT          */ 00174000
         LR    INCTR,@06                                        03S0134 00175000
         B     @DE00134                                         03S0134 00176000
@DL00134 TM    OVERLIMT,B'10000000'                             03S0134 00177000
         BNZ   @DC00134                                         03S0134 00178000
*/* READ AN ENTRY                                                    */ 00179000
*                                                               03S0135 00180000
*  PREG=ADDR(ASVTENTY(INCTR));                                  03S0135 00181000
         LR    @09,INCTR                                        03S0135 00182000
         SLA   @09,2                                            03S0135 00183000
         LA    PREG,ASVTENTY-4(@09,ASVTPTR)                     03S0135 00184000
*  R0=0;                                                        03S0136 00185000
         SR    R0,R0                                            03S0136 00186000
*  GEN REFS(BRRDDATA)SETS(PREG,READERR)                         03S0137 00187000
*      FLOWS(FSCERR)                                            03S0137 00188000
*    (BRREAD (PREG),,ASID=(R0));                                03S0137 00189000
         BRREAD (PREG),,ASID=(R0)                                       00190000
*/* IF ENTRY WAS READ OK, CHECK FOR ASCB POINTER IN ENTRY            */ 00191000
*  IF READERR=OFF THEN                                          03S0138 00192000
         TM    READERR,B'00000010'                              03S0138 00193000
         BNZ   @RF00138                                         03S0138 00194000
*    DO;                                                        03S0139 00195000
*      IF READALL(1)=OFF & READALL(9:32) ^= '000000'X THEN      03S0140 00196000
         TM    READALL(PREG),B'10000000'                        03S0140 00197000
         BNZ   @RF00140                                         03S0140 00198000
         CLC   READALL+1(3,PREG),@CB02171                       03S0140 00199000
         BE    @RF00140                                         03S0140 00200000
*        DO;                                                    03S0141 00201000
*          TEMPASCB=RDPTR;                                      03S0142 00202000
         SR    @09,@09                                          03S0142 00203000
         ICM   @09,7,RDPTR(PREG)                                03S0142 00204000
         ST    @09,TEMPASCB                                     03S0142 00205000
*          RFY ASCB BASED(TEMPASCB);                            03S0143 00206000
*/* READ ASID FROM ASCB                                              */ 00207000
*                                                               03S0144 00208000
*          PREG=ADDR(ASCBASID);                                 03S0144 00209000
         LA    PREG,ASCBASID(,@09)                              03S0144 00210000
*          GEN REFS(BRRDDATA)SETS(PREG,READERR)                 03S0145 00211000
*              FLOWS(FSCERR)                                    03S0145 00212000
*            (BRREAD (PREG));                                   03S0145 00213000
         BRREAD (PREG)                                                  00214000
*/* IF ASID WAS READ OK, USE IT                                      */ 00215000
*          IF READERR=OFF THEN                                  03S0146 00216000
         TM    READERR,B'00000010'                              03S0146 00217000
         BNZ   @RF00146                                         03S0146 00218000
*            TEMPASID=RDHWORD;                                  03S0147 00219000
         MVC   TEMPASID(2),RDHWORD(PREG)                        03S0147 00220000
*          ELSE                                                 03S0148 00221000
*/* OTHERWISE, CALCULATE WHAT IT SHOULD BE                           */ 00222000
*            DO;                                                03S0148 00223000
         B     @RC00146                                         03S0148 00224000
@RF00146 DS    0H                                               03S0149 00225000
*              TEMPASID=INCTR;                                  03S0149 00226000
         STH   INCTR,TEMPASID                                   03S0149 00227000
*              READERR=OFF;                                     03S0150 00228000
         NI    READERR,B'11111101'                              03S0150 00229000
*            END;                                               03S0151 00230000
*/* QUEUE UP THE ASCB (QUEUEING USED TO CATCH DUPLICATES)            */ 00231000
*          LISTSAVE=TCBLIST;                                    03S0152 00232000
@RC00146 L     LISTSAVE,TCBLIST(,COMBASE)                       03S0152 00233000
*          TCBLIST=ASCBLIST;                                    03S0153 00234000
         MVC   TCBLIST(4,COMBASE),ASCBLIST                      03S0153 00235000
*          RFY R2 RSTD;                                         03S0154 00236000
*          R2=TEMPASCB;                                         03S0155 00237000
         L     R2,TEMPASCB                                      03S0155 00238000
*          CALL ASCBSAV;                                        03S0156 00239000
         L     @15,ATCBSAVE(,COMBASE)                           03S0156 00240000
         BALR  @14,@15                                          03S0156 00241000
*          RFY R2 UNRSTD;                                       03S0157 00242000
*          RFY R15 RSTD;                                        03S0158 00243000
*          RC=R15;                      /* SAVE RETURN CODE          */ 00244000
         ST    R15,RC                                           03S0159 00245000
*          RFY R15 UNRSTD;                                      03S0160 00246000
*          ASCBLIST=TCBLIST;                                    03S0161 00247000
         MVC   ASCBLIST(4),TCBLIST(COMBASE)                     03S0161 00248000
*          TCBLIST=LISTSAVE;                                    03S0162 00249000
         ST    LISTSAVE,TCBLIST(,COMBASE)                       03S0162 00250000
*          IF RC=0 THEN                 /* WAS ASCB ALREADY THERE    */ 00251000
         L     @09,RC                                           03S0163 00252000
         LTR   @09,@09                                          03S0163 00253000
         BNZ   @RF00163                                         03S0163 00254000
*            DO;                        /* NO                        */ 00255000
*              IF ASCBCNT>ASCBMAX THEN  /* ARE WE OVER ASCB LIMIT    */ 00256000
         CH    ASCBCNT,ASCBMAX(,COMBASE)                        03S0165 00257000
         BNH   @RF00165                                         03S0165 00258000
*                OVERLIMT=ON;           /* YES, LEAVE LOOP, ISSUE MSG*/ 00259000
         OI    OVERLIMT,B'10000000'                             03S0166 00260000
*              ELSE                     /* NO, PROCESS ASCB          */ 00261000
*                DO;                                            03S0167 00262000
         B     @RC00165                                         03S0167 00263000
@RF00165 DS    0H                                               03S0168 00264000
*                  CALL DOADDRSP (TEMPASCB,TEMPASID);           03S0168 00265000
         LA    @01,@AL00168                                     03S0168 00266000
         BAL   @14,DOADDRSP                                     03S0168 00267000
*/* SKIP TO NEW PAGE                                                 */ 00268000
*                                                               03S0169 00269000
*                  GEN REFS(AWRITE) SETS(CURBUF) FLOWS(FSCSTOP) 03S0169 00270000
*                    (BRWRITE SKIP);                            03S0169 00271000
         BRWRITE SKIP                                                   00272000
*                  ASCBCNT = ASCBCNT+1;                         03S0170 00273000
         AH    ASCBCNT,@CH00076                                 03S0170 00274000
*                END;                                           03S0171 00275000
*            END;                                               03S0172 00276000
*        END;                                                   03S0173 00277000
*    END;                                                       03S0174 00278000
*  ELSE                                                         03S0175 00279000
*/* COULDN'T READ ASVT ENTRY - ISSUE MSG & USE DISP CHAIN FOR REST   */ 00280000
*/*   OF ASCBS                                                       */ 00281000
*    DO;                                                        03S0175 00282000
         B     @RC00138                                         03S0175 00283000
@RF00138 DS    0H                                               03S0176 00284000
*      BUFFER(1:LENGTH(ASENTERM))=ASENTERM;                     03S0176 00285000
*                                                               03S0176 00286000
         L     @09,CURBUF(,COMBASE)                             03S0176 00287000
         MVC   BUFFER(29,@09),ASENTERM                          03S0176 00288000
*      GEN REFS(AWRITE) SETS(CURBUF) FLOWS(FSCSTOP)             03S0177 00289000
*        (BRWRITE 1);                                           03S0177 00290000
         BRWRITE 1                                                      00291000
*      INCTR=ASMAXU;                    /* TO EXIT FROM LOOP         */ 00292000
         L     INCTR,ASMAXU                                     03S0178 00293000
*      USEDSP=ON;                                               03S0179 00294000
*      READERR=OFF;                                             03S0180 00295000
