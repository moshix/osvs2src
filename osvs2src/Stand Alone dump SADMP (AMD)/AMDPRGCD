         TITLE 'AMDPRGCD - GENERAL CPUDATA EXECUTOR                    *00001000
                        '                                               00002000
AMDPRGCD CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'AMDPRGCD  77.136'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @11,0                                            01S0001 00011000
@PSTART  LA    @10,4095(,@11)                                   01S0001 00012000
         LA    @09,4095(,@10)                                   01S0001 00013000
         LA    @08,4095(,@09)                                   01S0001 00014000
         USING @PSTART,@11                                      01S0001 00015000
         USING @PSTART+4095,@10                                 01S0001 00016000
         USING @PSTART+8190,@09                                 01S0001 00017000
         USING @PSTART+12285,@08                                01S0001 00018000
         ST    @13,@SA00001+4                                   01S0001 00019000
         LA    @14,@SA00001                                     01S0001 00020000
         ST    @14,8(,@13)                                      01S0001 00021000
         LR    @13,@14                                          01S0001 00022000
*   GCDSAVRG=R13;                   /* SAVE ADDR OF GCD SAVE AREA    */ 00023000
         ST    R13,GCDSAVRG                                     01S0296 00024000
*   RESPECIFY                                                   01S0297 00025000
*     R13 UNRESTRICTED;                                         01S0297 00026000
*                                                               02S0298 00027000
*   /*****************************************************************/ 00028000
*   /*                                                               */ 00029000
*   /* ALL INTERNAL SWITCHES AND WORK VALUES ARE SET TO ZERO, EXCEPT */ 00030000
*   /* FOR THE CPUADDR, AND THE HEADER IS PRINTED                    */ 00031000
*   /*                                                               */ 00032000
*   /*****************************************************************/ 00033000
*                                                               02S0298 00034000
*   GCDERSAV=ERRADDR;               /* SAVE BRREAD ERROR RTN ADDR    */ 00035000
         MVC   GCDERSAV(4),ERRADDR(COMBASE)                     02S0298 00036000
*   ERRADDR=ADDR(GCDERRTN);         /* PUT ADDR OF GCD BRREAD ERROR     00037000
*                                      RTN IN COMMON                 */ 00038000
         LA    @03,GCDERRTN                                     02S0299 00039000
         ST    @03,ERRADDR(,COMBASE)                            02S0299 00040000
*   STPERSAV=STOPEXIT;              /* SAVE STOP RTN ADDR            */ 00041000
         MVC   STPERSAV(4),STOPEXIT(COMBASE)                    02S0300 00042000
*   STOPEXIT=ADDR(GCDSTOP);         /* PUT ADDR OF GCD STOP RTN IN      00043000
*                                      COMMON                        */ 00044000
         LA    @03,GCDSTOP                                      02S0301 00045000
         ST    @03,STOPEXIT(,COMBASE)                           02S0301 00046000
*   PSAVIRT=0;                                                  02S0302 00047000
         SLR   @03,@03                                          02S0302 00048000
         ST    @03,PSAVIRT                                      02S0302 00049000
*   LTABLPTR=0;                                                 02S0303 00050000
         SLR   LTABLPTR,LTABLPTR                                02S0303 00051000
*   PTABLPTR=0;                                                 02S0304 00052000
         ST    @03,PTABLPTR                                     02S0304 00053000
*   CTABLPTR=0;                                                 02S0305 00054000
         SLR   CTABLPTR,CTABLPTR                                02S0305 00055000
*   TABLPTR=0;                                                  02S0306 00056000
         SLR   TABLPTR,TABLPTR                                  02S0306 00057000
*   LCCAPTR=0;                                                  02S0307 00058000
         ST    @03,LCCAPTR                                      02S0307 00059000
*   PCCAPTR=0;                                                  02S0308 00060000
         ST    @03,PCCAPTR                                      02S0308 00061000
*   CVTPTR=0;                                                   02S0309 00062000
         ST    @03,CVTPTR                                       02S0309 00063000
*   CCAVTPTR=0;                                                 02S0310 00064000
         ST    @03,CCAVTPTR                                     02S0310 00065000
*   DSSPSAVT=0;                                                 02S0311 00066000
         ST    @03,DSSPSAVT                                     02S0311 00067000
*   LCACPUAD=0;                                                 02S0312 00068000
         SLR   LCACPUAD,LCACPUAD                                02S0312 00069000
*   PCACPUAD=0;                                                 02S0313 00070000
         SLR   PCACPUAD,PCACPUAD                                02S0313 00071000
*   CPUADRCK=0;                                                 02S0314 00072000
         SLR   CPUADRCK,CPUADRCK                                02S0314 00073000
*   CCACNT0=0;                                                  02S0315 00074000
         SLR   CCACNT0,CCACNT0                                  02S0315 00075000
*   CCACNTRD=0;                                                 02S0316 00076000
         SLR   CCACNTRD,CCACNTRD                                02S0316 00077000
*   J=0;                                                        02S0317 00078000
         MVI   J,X'00'                                          02S0317 00079000
*   CPUADDR='FF'X;                                              02S0318 00080000
         LA    @03,255                                          02S0318 00081000
         STH   @03,CPUADDR                                      02S0318 00082000
*   GCDSWS='00'X;                                               02S0319 00083000
         MVI   GCDSWS,X'00'                                     02S0319 00084000
*   SUBRTNSW='00'X;                                             02S0320 00085000
         MVI   SUBRTNSW,X'00'                                   02S0320 00086000
*   GEN REFS(AWRITE) FLOWS(GCDSTOP)(BRWRITE SKIP);/* SKIP TO A NEW      00087000
*                                      PAGE                          */ 00088000
         BRWRITE SKIP                                                   00089000
*   OUTPUT(45:44+LENGTH(GCDTITLE))=GCDTITLE;/* PRINT TITLE           */ 00090000
         L     @03,CURBUF(,COMBASE)                             02S0322 00091000
         MVC   OUTPUT+44(32,@03),GCDTITLE                       02S0322 00092000
*   GEN(BRWRITE 3);                                             02S0323 00093000
         BRWRITE 3                                                      00094000
*   IF CVTADDR=0 THEN               /* IS THERE A CVT                */ 00095000
         ICM   @03,15,CVTADDR(COMBASE)                          01S0324 00096000
         BNZ   @RF00324                                         01S0324 00097000
*     DO;                           /* NO                            */ 00098000
*                                                               02S0326 00099000
*       /*************************************************************/ 00100000
*       /*                                                           */ 00101000
*       /* ATTEMPT TO FORMAT THE LCCA AND PCCA FOR THE PSA AT VIRT 0,*/ 00102000
*       /* ALSO FORMAT THE PSA AND THE REGISTERS                     */ 00103000
*       /*                                                           */ 00104000
*       /*************************************************************/ 00105000
*                                                               02S0326 00106000
*       OUTPUT(2:1+LENGTH(MSG01))=MSG01;/* PRINT UNABLE TO ACCESS CVT   00107000
*                                      MSG                           */ 00108000
         L     @01,CURBUF(,COMBASE)                             02S0326 00109000
         MVC   OUTPUT+1(58,@01),MSG01                           02S0326 00110000
*       CALL PRNTSKIP;              /* PRINT THE MESSAGE             */ 00111000
         BAL   @14,PRNTSKIP                                     02S0327 00112000
*       LCASW=SWON;                 /* INDICATE NO LCCAVT            */ 00113000
*       PCASW=SWON;                 /* INDICATE NO PCCAVT            */ 00114000
         OI    LCASW,B'01100000'                                02S0329 00115000
*     END;                                                      01S0330 00116000
*   ELSE                                                        01S0331 00117000
*     DO;                           /* YES - CVT ADDR IS NON ZERO    */ 00118000
         B     @RC00324                                         01S0331 00119000
@RF00324 DS    0H                                               01S0332 00120000
*       CVTPTR=CVTADDR;             /* ADDR FOR CVT MACRO            */ 00121000
         MVC   CVTPTR(4),CVTADDR(COMBASE)                       01S0332 00122000
*                                                               02S0333 00123000
*       /*************************************************************/ 00124000
*       /*                                                           */ 00125000
*       /* GET STORAGE FOR INTERNAL TABLES AND INITIALIZE THEM       */ 00126000
*       /*                                                           */ 00127000
*       /*************************************************************/ 00128000
*                                                               02S0333 00129000
*                                                               03S0333 00130000
*       /*************************************************************/ 00131000
*       /*                                                           */ 00132000
*       /* STORAGE IS GETMAINED FOR THE INTERNAL CCAV TABLES AND THE */ 00133000
*       /* PHYSICAL CPU ADDR TABLE (INDICATES FOR WHICH CPU'S THE    */ 00134000
*       /* REGISTERS HAVE BEEN PRINTED)                              */ 00135000
*       /*                                                           */ 00136000
*       /*************************************************************/ 00137000
*                                                               03S0333 00138000
*       RESPECIFY                                               03S0333 00139000
*         R1 RESTRICTED;                                        03S0333 00140000
*       GEN(GETMAIN R,LV=160);                                  03S0334 00141000
         GETMAIN R,LV=160                                               00142000
*       LTABLPTR=R1;                /* ADDR OF BEGINNING OF GETMAINED   00143000
*                                      AREA - ALSO ADDR OF LCCA TABLE*/ 00144000
         LR    LTABLPTR,R1                                      03S0335 00145000
*       RESPECIFY                                               03S0336 00146000
*         R1 UNRESTRICTED;                                      03S0336 00147000
*       PTABLPTR=LTABLPTR+CCAVSIZE; /* ADDR OF BEGINNING OF PCCA        00148000
*                                      TABLE                         */ 00149000
         LA    @03,64                                           03S0337 00150000
         ALR   @03,LTABLPTR                                     03S0337 00151000
         ST    @03,PTABLPTR                                     03S0337 00152000
*       CTABLPTR=LTABLPTR+(2*CCAVSIZE);/* ADDR OF BEGINNING OF PHYS     00153000
*                                      CPU ADDR TABLE                */ 00154000
         LA    CTABLPTR,128                                     03S0338 00155000
         ALR   CTABLPTR,LTABLPTR                                03S0338 00156000
*       TABINIT1=TABINIT1&&TABINIT1;/* IN CCA TABLES TO 0            */ 00157000
         XC    TABINIT1(128,LTABLPTR),TABINIT1(LTABLPTR)        03S0339 00158000
*       TABINIT2(1)='FF'X;          /* INIT PHYS CPU ADDR TABLE TO      00159000
*                                      X'FF'                         */ 00160000
         MVI   TABINIT2(CTABLPTR),X'FF'                         03S0340 00161000
*       EVAL(ADDR(TABINIT2(1)))->TABINIT2(2:LENGTH(TABINIT2))=TABINIT2( 00162000
*           1:LENGTH(TABINIT2)-1);                              03S0341 00163000
         MVC   TABINIT2+1(31,CTABLPTR),TABINIT2(CTABLPTR)       03S0341 00164000
*                                                               02S0342 00165000
*       /*************************************************************/ 00166000
*       /*                                                           */ 00167000
*       /* BEGIN PROCESSING BY ATTEMPTING TO GET CSD                 */ 00168000
*       /*                                                           */ 00169000
*       /*************************************************************/ 00170000
*                                                               02S0342 00171000
*       BRNCHADR=ADDR(CSDERR);      /* ERROR RETURN POINT FOR THIS      00172000
*                                      BRREAD                        */ 00173000
         LA    @03,CSDERR                                       02S0342 00174000
         ST    @03,BRNCHADR                                     02S0342 00175000
*       RESPECIFY                                               02S0343 00176000
*         PREG RESTRICTED;                                      02S0343 00177000
*       PREG=ADDR(CVTCSD);          /* PTR TO CSD                    */ 00178000
         L     PREG,CVTPTR                                      02S0344 00179000
         LA    PREG,CVTCSD(,PREG)                               02S0344 00180000
*       GEN REFS(BRRDDATA) FLOWS(GCDERRTN)(BRREAD (PREG));      02S0345 00181000
         BRREAD (PREG)                                                  00182000
*       IF TEMPADR1^=0 THEN         /* IS PTR TO CSD NOT 0           */ 00183000
         ICM   @00,7,TEMPADR1(PREG)                             02S0346 00184000
         BZ    @RF00346                                         02S0346 00185000
*         DO;                       /* YES                           */ 00186000
*                                                               02S0347 00187000
*           /*********************************************************/ 00188000
*           /*                                                       */ 00189000
*           /* FORMAT THE CSD BY FIELDS                              */ 00190000
*           /*                                                       */ 00191000
*           /*********************************************************/ 00192000
*                                                               02S0348 00193000
*                                                               03S0348 00194000
*           /*********************************************************/ 00195000
*           /*                                                       */ 00196000
*           /* VALIDITY CHECK THE CSD AND FORMAT IT ONE LINE AT A    */ 00197000
*           /* TIME                                                  */ 00198000
*           /*                                                       */ 00199000
*           /*********************************************************/ 00200000
*                                                               03S0348 00201000
*           CSDPTR=TEMPADDR;        /* ADDR OF CSD IN DUMP           */ 00202000
         L     @00,TEMPADDR(,PREG)                              03S0348 00203000
         ST    @00,CSDPTR                                       03S0348 00204000
*           PREG=CSDPTR;            /* GET CSD                       */ 00205000
         LR    PREG,@00                                         03S0349 00206000
*           GEN(BRREAD (PREG));                                 03S0350 00207000
         BRREAD (PREG)                                                  00208000
*           IF VALIDCK^='CSD ' THEN /* DOES CSD VALIDITY CHECK       */ 00209000
         CLC   VALIDCK(4,PREG),@CC02813                         03S0351 00210000
         BE    @RF00351                                         03S0351 00211000
*             DO;                   /* NO                            */ 00212000
*               RESPECIFY                                       03S0353 00213000
*                 PREG UNRESTRICTED;                            03S0353 00214000
*               OUTPUT(2:1+LENGTH(MSG3))=MSG3;/* PRINT VALIDITY CHECK   00215000
*                                      FAILED MSG                    */ 00216000
         L     @01,CURBUF(,COMBASE)                             03S0354 00217000
         MVC   OUTPUT+1(25,@01),MSG3                            03S0354 00218000
*               CALL PRNTSKIP;                                  03S0355 00219000
         BAL   @14,PRNTSKIP                                     03S0355 00220000
*             END;                                              03S0356 00221000
*           BRNCHADR=ADDR(GMSG4);   /* ERROR RETURN POINT FOR BRREADS   00222000
*                                      BY GFR                        */ 00223000
@RF00351 LA    @15,GMSG4                                        03S0357 00224000
         ST    @15,BRNCHADR                                     03S0357 00225000
*           RESPECIFY                                           03S0358 00226000
*            (R0,                                               03S0358 00227000
*             R1) RESTRICTED;                                   03S0358 00228000
*           R0=0;                   /* 0 ASID FOR RDC                */ 00229000
         SLR   R0,R0                                            03S0359 00230000
*           DO I=1 TO DIM(FMTTABL1);/* CALL THE FORMAT RTN TO FORMAT    00231000
*                                      THE CSD ONE LINE AT A TIME    */ 00232000
         LA    I,1                                              03S0360 00233000
@DL00360 DS    0H                                               03S0361 00234000
*             R1=FMTTABL1(I);       /* ADDR OF EACH LINE TO BE  03S0361 00235000
*                                      FORMATTED                     */ 00236000
         LR    @05,I                                            03S0361 00237000
         SLA   @05,2                                            03S0361 00238000
         L     R1,FMTTABL1-4(@05)                               03S0361 00239000
*             CALL AMDPRGFR;        /* FORMAT THE LINE               */ 00240000
         L     @15,AFMTLINE(,COMBASE)                           03S0362 00241000
         BALR  @14,@15                                          03S0362 00242000
*             GEN SETS(R1,R14,R15)(BRWRITE 1);                  03S0363 00243000
         BRWRITE 1                                                      00244000
*           END;                                                03S0364 00245000
         AH    I,@CH00050                                       03S0364 00246000
         CH    I,@CH00033                                       03S0364 00247000
         BNH   @DL00360                                         03S0364 00248000
*           CALL PRNTSKIP;          /* SKIP TWO LINES                */ 00249000
         BAL   @14,PRNTSKIP                                     03S0365 00250000
*           RESPECIFY                                           03S0366 00251000
*            (R0,                                               03S0366 00252000
*             R1) UNRESTRICTED;                                 03S0366 00253000
*           GO TO CSDOUT;           /* ATTEMPT TO GET THE LCCAVT     */ 00254000
*                                                               03S0367 00255000
         B     CSDOUT                                           03S0367 00256000
*           /*********************************************************/ 00257000
*           /*                                                       */ 00258000
*           /* THIS ROUTINE WILL GET CONTROL ONLY IF A BRREAD FAILS  */ 00259000
*           /* WHILE FORMATTING THE CSD                              */ 00260000
*           /*                                                       */ 00261000
*           /*********************************************************/ 00262000
*                                                               03S0368 00263000
*GMSG4:                                                         03S0368 00264000
*           GEN(BRWRITE 1);         /* PRINT PARTIALLY FILLED BUFFER */ 00265000
GMSG4    BRWRITE 1                                                      00266000
*           OUTPUT(2:1+LENGTH(MSG4))=MSG4;/* PRINT ERROR FORMATTING     00267000
*                                      CSD MSG                       */ 00268000
         L     @01,CURBUF(,COMBASE)                             03S0369 00269000
         MVC   OUTPUT+1(26,@01),MSG4                            03S0369 00270000
*           CALL PRNTSKIP;          /* PRINT THE MESSAGE             */ 00271000
         BAL   @14,PRNTSKIP                                     03S0370 00272000
*CSDOUT:                                                        03S0371 00273000
*           ;                       /* ATTEMPT TO GET THE LCCAVT     */ 00274000
CSDOUT   DS    0H                                               03S0372 00275000
*         END;                                                  02S0372 00276000
*       ELSE                        /* NO - PTR TO CSD IS 0          */ 00277000
*         DO;                                                   02S0373 00278000
         B     @RC00346                                         02S0373 00279000
@RF00346 DS    0H                                               02S0374 00280000
*           OUTPUT(2:1+LENGTH(MSG5))=MSG5;/* PRINT CSD ADDR 0 MSG    */ 00281000
         L     @01,CURBUF(,COMBASE)                             02S0374 00282000
         MVC   OUTPUT+1(19,@01),MSG5                            02S0374 00283000
*           CALL PRNTSKIP;          /* PRINT THE MESSAGE             */ 00284000
         BAL   @14,PRNTSKIP                                     02S0375 00285000
*         END;                                                  02S0376 00286000
*       GO TO CSDEXIT;                                          02S0377 00287000
*                                                               02S0377 00288000
         B     CSDEXIT                                          02S0377 00289000
*       /*************************************************************/ 00290000
*       /*                                                           */ 00291000
*       /* THIS ROUTINE WILL GET CONTROL ONLY IF A BRREAD FAILS WHILE*/ 00292000
*       /* GETTING THE CSD                                           */ 00293000
*       /*                                                           */ 00294000
*       /*************************************************************/ 00295000
*                                                               02S0378 00296000
*CSDERR:                                                        02S0378 00297000
*       OUTPUT(2:1+LENGTH(MSG02))=MSG02;/* PRINT ERROR FINDING CSD      00298000
*                                      MSG                           */ 00299000
CSDERR   L     @01,CURBUF(,COMBASE)                             02S0378 00300000
         MVC   OUTPUT+1(17,@01),MSG02                           02S0378 00301000
*       CALL PRNTSKIP;              /* PRINT THE MESSAGE             */ 00302000
         BAL   @14,PRNTSKIP                                     02S0379 00303000
*CSDEXIT:                                                       02S0380 00304000
*       ;                                                       02S0380 00305000
CSDEXIT  DS    0H                                               02S0381 00306000
*                                                               02S0381 00307000
*       /*************************************************************/ 00308000
*       /*                                                           */ 00309000
*       /* THE LCCAVT IS READ AND EACH NON-ZERO LCCA ADDR IS COPIED  */ 00310000
*       /* TO THE SAME RELATIVE POSITION IN THE GETMAINED STORAGE    */ 00311000
*       /* THUS CREATING AN INTERNAL LCCAVT                          */ 00312000
*       /*                                                           */ 00313000
*       /*************************************************************/ 00314000
*                                                               02S0381 00315000
*       BRNCHADR=ADDR(LCAVTERR);    /* ERROR RETURN POINT FOR THIS      00316000
*                                      BRREAD                        */ 00317000
         LA    @15,LCAVTERR                                     02S0381 00318000
         ST    @15,BRNCHADR                                     02S0381 00319000
*       RESPECIFY                                               02S0382 00320000
*         PREG RESTRICTED;                                      02S0382 00321000
*       PREG=ADDR(CVTLCCAT);        /* ADDR OF LCCAVT - IN CVT       */ 00322000
         L     PREG,CVTPTR                                      02S0383 00323000
         LA    PREG,CVTLCCAT(,PREG)                             02S0383 00324000
*       GEN(BRREAD (PREG));                                     02S0384 00325000
         BRREAD (PREG)                                                  00326000
*       IF TEMPADR1^=0 THEN         /* IS LCCAVT ADDR NOT 0          */ 00327000
         ICM   @00,7,TEMPADR1(PREG)                             02S0385 00328000
         BZ    @RF00385                                         02S0385 00329000
*         DO;                       /* YES                           */ 00330000
*           CCAVTPTR=TEMPADDR;      /* ADDR OF LCCAVT                */ 00331000
         MVC   CCAVTPTR(4),TEMPADDR(PREG)                       02S0387 00332000
*           RESPECIFY                                           02S0388 00333000
*             PREG UNRESTRICTED;                                02S0388 00334000
*           CCAVTSW=SWOFF;          /* INDICATE PROCESSING LCCAVT    */ 00335000
         NI    CCAVTSW,B'01111111'                              02S0389 00336000
*           CALL CCAVTABL;          /* PROCESS THE LCCAVT FILLING THE   00337000
*                                      GETMAINED STORAGE             */ 00338000
         BAL   @14,CCAVTABL                                     02S0390 00339000
*         END;                                                  02S0391 00340000
*       ELSE                                                    02S0392 00341000
*         DO;                       /* NO - LCCAVT ADDR IS 0         */ 00342000
         B     @RC00385                                         02S0392 00343000
@RF00385 DS    0H                                               02S0393 00344000
*LCAVTERR:                                                      02S0393 00345000
*           OUTPUT(2:1+LENGTH(MSG10))=MSG10;/* PRINT ERROR FINDING      00346000
*                                      LCCAVT MSG                    */ 00347000
LCAVTERR L     @01,CURBUF(,COMBASE)                             02S0393 00348000
         MVC   OUTPUT+1(20,@01),MSG10                           02S0393 00349000
*           CALL PRNTSKIP;          /* PRINT THE MESSAGE             */ 00350000
         BAL   @14,PRNTSKIP                                     02S0394 00351000
*           LCASW=SWON;             /* INDICATE LCCAVT NOT AVAILABLE */ 00352000
         OI    LCASW,B'01000000'                                02S0395 00353000
