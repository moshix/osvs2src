*       STGADPSA=PREG;              /* ADDR OF PSA IN STORAGE - FOR     02049000
*                                      FMTPTRNS                      */ 02050000
         ST    PREG,STGADPSA                                    02S0919 02051000
*       PREG=PREG+(ADDR(PSAPSA)-ADDR(PSA));/* DISPLACEMENT IN PSA OF    02052000
*                                      EBCDIC NAME 'PSA'             */ 02053000
         AH    PREG,@CH03000                                    02S0920 02054000
*       IF VALIDCK^='PSA ' THEN     /* DOES THE PSA VALIDITY CHECK   */ 02055000
         CLC   VALIDCK(4,PREG),@CC02879                         02S0921 02056000
         BE    @RF00921                                         02S0921 02057000
*         DO;                       /* NO                            */ 02058000
*           RESPECIFY                                           02S0923 02059000
*             PREG UNRESTRICTED;                                02S0923 02060000
*           OUTPUT(2:1+LENGTH(MSG43))=MSG43;/* PRINT VALIDITY CKECK     02061000
*                                      FAILED MSG                    */ 02062000
         L     @06,CURBUF(,COMBASE)                             02S0924 02063000
         MVC   OUTPUT+1(25,@06),MSG43                           02S0924 02064000
*           CALL PRNTSKIP;                                      02S0925 02065000
         BAL   @14,PRNTSKIP                                     02S0925 02066000
*         END;                                                  02S0926 02067000
*       CALL PSAFORMT;              /* FORMAT UNPREFIXED 0-4K        */ 02068000
@RF00921 BAL   @14,PSAFORMT                                     02S0927 02069000
*     END;                                                      02S0928 02070000
*   GO TO BNDRYOUT;                 /* PREPARE TO RETURN             */ 02071000
*                                                               02S0929 02072000
         B     BNDRYOUT                                         02S0929 02073000
*   /*****************************************************************/ 02074000
*   /*                                                               */ 02075000
*   /* THIS ROUTINE WILL GET CONTROL ONLY IF THE BRREAD FAILS WHILE  */ 02076000
*   /* TRYING TO ACCESS THE UNPREFIXED 0-4K STORAGE CONVERT THE      */ 02077000
*   /* VIRTUAL UNPREFIXED STORAGE ADDRESS TO PRINTABLE HEX FOR       */ 02078000
*   /* MESSAGE                                                       */ 02079000
*   /*                                                               */ 02080000
*   /*****************************************************************/ 02081000
*                                                               02S0930 02082000
*   RESPECIFY                                                   02S0930 02083000
*    (R1,                                                       02S0930 02084000
*     R4) RESTRICTED;                                           02S0930 02085000
*GMSG46:                                                        02S0931 02086000
*   R1=ADDR(PSAVIRT);               /* ADDR TO CONVERT               */ 02087000
GMSG46   LA    R1,PSAVIRT                                       02S0931 02088000
*   R4=ADDR(ADDR46);                /* ADDR OF OUTPUT LOCATION FOR      02089000
*                                      CNVT RTN                      */ 02090000
         LA    R4,ADDR46                                        02S0932 02091000
*   CALL ADRCNVT;                   /* AADRCNVT RTN                  */ 02092000
         L     @15,AADRCNVT(,COMBASE)                           02S0933 02093000
         BALR  @14,@15                                          02S0933 02094000
*   RESPECIFY                                                   02S0934 02095000
*    (R1,                                                       02S0934 02096000
*     R4) UNRESTRICTED;                                         02S0934 02097000
*   OUTPUT(2:1+LENGTH(MSG46))=MSG46;/* PRINT UNABLE TO FORMAT   02S0935 02098000
*                                      UNPREFIXED STORAGE MSG        */ 02099000
         L     @06,CURBUF(,COMBASE)                             02S0935 02100000
         MVC   OUTPUT+1(64,@06),MSG46                           02S0935 02101000
*   CALL PRNTSKIP;                                              02S0936 02102000
         BAL   @14,PRNTSKIP                                     02S0936 02103000
*BNDRYOUT:                                                      02S0937 02104000
*   ;                               /* PREPARE TO RETURN             */ 02105000
BNDRYOUT DS    0H                                               02S0938 02106000
*                                                               02S0938 02107000
*   /*****************************************************************/ 02108000
*   /*                                                               */ 02109000
*   /* DETERMINE IF A FREEMAIN IS NECESSARY AND RESTORE THE BRREAD   */ 02110000
*   /* AND STOP ERROR ROUTINE ADDRESSES PREVIOUSLY SAVED. IF NORMAL  */ 02111000
*   /* TERMINATION, RETURN TO CALLER. IF IN 'STOP' PROCESSING MODE,  */ 02112000
*   /* BRANCH TO THE NEXT STOP ROUTINE.                              */ 02113000
*   /*                                                               */ 02114000
*   /*****************************************************************/ 02115000
*                                                               02S0938 02116000
*GCDEXIT:                                                       02S0938 02117000
*   IF LTABLPTR^=0 THEN             /* WAS A GETMAIN DONE            */ 02118000
GCDEXIT  LTR   LTABLPTR,LTABLPTR                                02S0938 02119000
         BZ    @RF00938                                         02S0938 02120000
*     DO;                           /* YES                           */ 02121000
*       RESPECIFY                                               02S0940 02122000
*         PREG RESTRICTED;                                      02S0940 02123000
*       PREG=LTABLPTR;              /* BEGINNING ADDR OF GETMAINED      02124000
*                                      AREA                          */ 02125000
         LR    PREG,LTABLPTR                                    02S0941 02126000
*       GEN(FREEMAIN R,LV=160,A=(1));                           02S0942 02127000
         FREEMAIN R,LV=160,A=(1)                                        02128000
*       RESPECIFY                                               02S0943 02129000
*         PREG UNRESTRICTED;                                    02S0943 02130000
*     END;                                                      02S0944 02131000
*   ERRADDR=GCDERSAV;               /* PREVIOUS BRREAD ERROR RTN ADDR*/ 02132000
@RF00938 MVC   ERRADDR(4,COMBASE),GCDERSAV                      02S0945 02133000
*   STOPEXIT=STPERSAV;              /* PREVIOUS STOP RTN ADDR        */ 02134000
         L     @15,STPERSAV                                     02S0946 02135000
         ST    @15,STOPEXIT(,COMBASE)                           02S0946 02136000
*   IF GCDSTPSW=SWON THEN           /* HAS 'STOP' BEEN REQUESTED     */ 02137000
         TM    GCDSTPSW,B'10000000'                             02S0947 02138000
         BNO   @RF00947                                         02S0947 02139000
*     DO;                           /* YES                           */ 02140000
*       RESPECIFY                                               02S0949 02141000
*         R15 RESTRICTED;                                       02S0949 02142000
*       R15=STPERSAV;               /* ADDR OF NEXT STOP RTN TO GET     02143000
*                                      CONTROL                       */ 02144000
         L     @14,STPERSAV                                     02S0950 02145000
         LR    R15,@14                                          02S0950 02146000
*       GO TO NEXTSTOP;             /* BRANCH TO NEXT STOP RTN       */ 02147000
         BR    R15                                              02S0951 02148000
*       RESPECIFY                                               02S0952 02149000
*         R15 UNRESTRICTED;                                     02S0952 02150000
*     END;                                                      02S0953 02151000
*   ELSE                                                        02S0954 02152000
*     RETURN;                       /* RETURN TO CALLER OF GCD       */ 02153000
*                                                               02S0955 02154000
*   /*****************************************************************/ 02155000
*   /*                                                               */ 02156000
*   /* THIS ROUTINE WILL GET CONTROL ONLY IF THE OPERATOR REPLIES    */ 02157000
*   /* 'STOP' TO MESSAGE AMD156I.                                    */ 02158000
*   /*                                                               */ 02159000
*   /*****************************************************************/ 02160000
*                                                               02S0955 02161000
*GCDSTOP:                           /* REESTABLISH ADDRESSABILITY    */ 02162000
*   GEN;                                                        02S0955 02163000
GCDSTOP  DS    0H                                               02S0955 02164000
         DROP  R11,R10,R9,R8                                            02165000
         USING *,R15                                                    02166000
         L     R13,GCDSAVRG                                             02167000
         LM    R14,R12,12(R13)                                          02168000
         DROP  R15                                                      02169000
         USING @PSTART,R11                                              02170000
         USING @PSTART+4095,R10                                         02171000
         USING @PSTART+8190,R9                                          02172000
         USING @PSTART+12285,R8                                         02173000
*   GCDSTPSW=SWON;                  /* INDICATE 'STOP' MODE     02S0956 02174000
*                                      PROCESSING                    */ 02175000
         OI    GCDSTPSW,B'10000000'                             02S0956 02176000
*   GO TO GCDEXIT;                  /* PREPARE TO GO TO NEXT STOP RTN*/ 02177000
         B     GCDEXIT                                          02S0957 02178000
*                                                               02S0958 02179000
*   /*****************************************************************/ 02180000
*   /*                                                               */ 02181000
*   /* BRREAD ERROR ROUTINE - REESTABLISH ADDRESSABILITY AND BRANCH  */ 02182000
*   /* TO THE ADDRESS PUT IN BRNCHADR PRIOR TO BRREAD                */ 02183000
*   /*                                                               */ 02184000
*   /*****************************************************************/ 02185000
*                                                               02S0958 02186000
*GCDERRTN:                          /* REESTABLISH ADDRESSABILITY    */ 02187000
*   GEN REFS(GCDSAVRG);                                         02S0958 02188000
GCDERRTN DS    0H                                               02S0958 02189000
         DROP  R11,R10,R9,R8                                            02190000
         USING *,R15                                                    02191000
         B     LOADREG                                                  02192000
GCDSAVRG DC    F'0'                                                     02193000
LOADREG  L     R13,GCDSAVRG                                             02194000
         LM    R14,R12,12(R13)                                          02195000
         DROP  R15                                                      02196000
         USING @PSTART,R11                                              02197000
         USING @PSTART+4095,R10                                         02198000
         USING @PSTART+8190,R9                                          02199000
         USING @PSTART+12285,R8                                         02200000
*   GO TO BREADERR;                 /* BRANCH TO SPECIFIC FUNCTION   */ 02201000
         L     @03,BRNCHADR                                     02S0959 02202000
         BR    @03                                              02S0959 02203000
*CCAVTABL:                                                      02S0960 02204000
*   PROC;                                                       02S0960 02205000
*                                                               02S0960 02206000
@EL00001 L     @13,4(,@13)                                      02S0960 02207000
@EF00001 DS    0H                                               02S0960 02208000
@ER00001 LM    @14,@12,12(@13)                                  02S0960 02209000
         BR    @14                                              02S0960 02210000
CCAVTABL STM   @14,@12,@SA00002                                 02S0960 02211000
*   /*****************************************************************/ 02212000
*   /*                                                               */ 02213000
*   /* THIS PROCEDURE REMOVES ALL THE NON-ZERO CCA ADDRESSES FROM THE*/ 02214000
*   /* CCAV TABLES AND PUTS THEM IN THE SAME RELATIVE POSITION IN THE*/ 02215000
*   /* GETMAINED STORAGE - THEREBY CREATING INTERNAL CCAVTS. IF ALL  */ 02216000
*   /* THE CCA ADDRESSES IN EITHER TABLE ARE ZERO, OR IF NO NON-ZERO */ 02217000
*   /* ADDRESSES ARE FOUND DUE TO BRREAD ERRORS THE APPROPIATE SWITCH*/ 02218000
*   /* IS TURNED ON (LCASW/PCASW).                                   */ 02219000
*   /*                                                               */ 02220000
*   /*****************************************************************/ 02221000
*                                                               02S0961 02222000
*   BRNCHADR=ADDR(CTRBADD);         /* ERROR RETURN POINT FOR THESE     02223000
*                                      BRREADS                       */ 02224000
         LA    @06,CTRBADD                                      02S0961 02225000
         ST    @06,BRNCHADR                                     02S0961 02226000
*   CCACNT0=0;                      /* INITIALIZE ERROR COUNTER VALUE*/ 02227000
         SLR   CCACNT0,CCACNT0                                  02S0962 02228000
*   CCACNTRD=0;                     /* INITIALIZE ERROR COUNTER VALUE*/ 02229000
         SLR   CCACNTRD,CCACNTRD                                02S0963 02230000
*   IF CCAVTSW=SWOFF THEN           /* IS LCCAVT BEING PROCESSED     */ 02231000
         TM    CCAVTSW,B'10000000'                              02S0964 02232000
         BNZ   @RF00964                                         02S0964 02233000
*     TABLPTR=LTABLPTR;             /* YES - ADDR OF STORAGE FOR        02234000
*                                      LCCAVT                        */ 02235000
         LR    TABLPTR,LTABLPTR                                 02S0965 02236000
*   ELSE                                                        02S0966 02237000
*     TABLPTR=PTABLPTR;             /* NO - ADDR OF STORAGE FOR 02S0966 02238000
*                                      PCCAVT                        */ 02239000
         B     @RC00964                                         02S0966 02240000
@RF00964 L     TABLPTR,PTABLPTR                                 02S0966 02241000
*   DO J=0 TO NUMENTRY BY LENGTH(CCAENTRY);/* SEARCH CCAVT FOR  02S0967 02242000
*                                      NON-ZERO ADDRS                */ 02243000
@RC00964 SLR   @06,@06                                          02S0967 02244000
         STC   @06,J                                            02S0967 02245000
@DL00967 DS    0H                                               02S0968 02246000
*     RESPECIFY                                                 02S0968 02247000
*       PREG RESTRICTED;                                        02S0968 02248000
*     PREG=CCAVTPTR+J;              /* GET THIS ENTRY IN SYSTEM CCAVT*/ 02249000
         AL    @06,CCAVTPTR                                     02S0969 02250000
         LR    PREG,@06                                         02S0969 02251000
*     GEN(BRREAD (PREG));                                       02S0970 02252000
         BRREAD (PREG)                                                  02253000
*     IF TEMPADR1^=0 THEN           /* IS THIS CCA ADDR NOT 0        */ 02254000
         ICM   @06,7,TEMPADR1(PREG)                             02S0971 02255000
         BZ    @RF00971                                         02S0971 02256000
*       DO;                         /* YES                           */ 02257000
*         CCAENTRY=TEMPADDR;        /* PUT CCA ADDR IN GETMAINED        02258000
*                                      TABLE AT SAME RELATIVE   02S0973 02259000
*                                      POSITION                      */ 02260000
         MVC   CCAENTRY(4,TABLPTR),TEMPADDR(PREG)               02S0973 02261000
*         RESPECIFY                                             02S0974 02262000
*           PREG UNRESTRICTED;                                  02S0974 02263000
*       END;                                                    02S0975 02264000
*     ELSE                          /* CCA ADDR IS 0                 */ 02265000
*       CCACNT0=CCACNT0+1;          /* INDICATES THIS CCA ADDR WAS      02266000
*                                      ZERO                          */ 02267000
         B     @RC00971                                         02S0976 02268000
@RF00971 LA    CCACNT0,1(,CCACNT0)                              02S0976 02269000
*NXTENTRY:                                                      02S0977 02270000
*     TABLPTR=TABLPTR+LENGTH(CCAENTRY);/* INCREMENT PTR TO REFERENCE    02271000
*                                      NEXT ENTRY IN CCAVT           */ 02272000
@RC00971 DS    0H                                               02S0977 02273000
NXTENTRY AH    TABLPTR,@CH00039                                 02S0977 02274000
*   END;                                                        02S0978 02275000
         LA    @06,4                                            02S0978 02276000
         SLR   @03,@03                                          02S0978 02277000
         IC    @03,J                                            02S0978 02278000
         ALR   @06,@03                                          02S0978 02279000
         STC   @06,J                                            02S0978 02280000
         CH    @06,@CH02093                                     02S0978 02281000
         BNH   @DL00967                                         02S0978 02282000
*   IF CCACNT0=16|(CCACNT0+CCACNTRD)=16 THEN/* ARE ALL CCA ADDRS ZERO   02283000
*                                      OR SOME ZERO AND REMAINING HAD   02284000
*                                      BRREAD ERRORS                 */ 02285000
