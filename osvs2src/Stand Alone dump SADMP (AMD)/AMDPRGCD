*   /*****************************************************************/ 02004000
*   /*                                                               */ 02005000
*   /* IF THE UNPREFIXED 0-4K STORAGE HAS NOT BEEN FORMATTED IT IS   */ 02006000
*   /* FORMATTED NOW. IF IT HAS BEEN FORMATTED, PROCESSING           */ 02007000
*   /* TERMINATES. IF INPUT IS DSS OR SVCDUMP, AND THE UNPREFIXED    */ 02008000
*   /* STORAGE IS AVAILABLE IT IS FORMATTED NOW.                     */ 02009000
*   /*                                                               */ 02010000
*   /*****************************************************************/ 02011000
*                                                               02S0906 02012000
*BNDRYCK:                                                       02S0906 02013000
*   IF PSAVIRT//4096^=0 THEN        /* IS VIRT PSA ADDR VALID        */ 02014000
@RC00896 DS    0H                                               02S0906 02015000
BNDRYCK  L     @00,PSAVIRT                                      02S0906 02016000
         SRDA  @00,32                                           02S0906 02017000
         D     @00,@CF02905                                     02S0906 02018000
         LTR   @00,@00                                          02S0906 02019000
         BZ    @RF00906                                         02S0906 02020000
*     DO;                           /* NO                            */ 02021000
*       OUTPUT(2:1+LENGTH(MSG44))=MSG44;/* PRINT 4K BNDRY ERROR MSG  */ 02022000
         L     @06,CURBUF(,COMBASE)                             02S0908 02023000
         MVC   OUTPUT+1(56,@06),MSG44                           02S0908 02024000
*       CALL PRNTSKIP;                                          02S0909 02025000
         BAL   @14,PRNTSKIP                                     02S0909 02026000
*     END;                                                      02S0910 02027000
*   ELSE                                                        02S0911 02028000
*     DO;                           /* YES - VIRT PSA ADDR IS VALID  */ 02029000
         B     @RC00906                                         02S0911 02030000
@RF00906 DS    0H                                               02S0912 02031000
*       GEN(BRWRITE SKIP);          /* SKIP TO A NEW PAGE            */ 02032000
         BRWRITE SKIP                                                   02033000
*       OUTPUT(45:44+LENGTH(UNPRHDR))=UNPRHDR;/* PRINT HEADER        */ 02034000
         L     @06,CURBUF(,COMBASE)                             02S0913 02035000
         MVC   OUTPUT+44(35,@06),UNPRHDR                        02S0913 02036000
*       GEN(BRWRITE 3);                                         02S0914 02037000
         BRWRITE 3                                                      02038000
*       BRNCHADR=ADDR(GMSG46);      /* ERROR RETURN POINT FOR THIS      02039000
*                                      BRREAD                        */ 02040000
