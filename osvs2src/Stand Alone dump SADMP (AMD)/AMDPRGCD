         L     @07,CURBUF(,COMBASE)                             02S1201 03018000
         MVC   OUTPUT+1(46,@07),MSG50                           02S1201 03019000
*             END;                                              02S1202 03020000
*           CALL PRNTSKIP;                                      02S1203 03021000
@RC01193 BAL   @14,PRNTSKIP                                     02S1203 03022000
*         END;                                                  02S1204 03023000
*     END;                                                      02S1205 03024000
*   RETURN;                         /* RETURN TO CALLER              */ 03025000
         B     @EL00006                                         02S1206 03026000
*   END PCAMATCH;                                               02S1207 03027000
         B     @EL00006                                         02S1207 03028000
*LOGCLCPU:                                                      02S1208 03029000
*   PROC;                                                       02S1208 03030000
*                                                               02S1208 03031000
LOGCLCPU STM   @14,@05,@SA00007                                 02S1208 03032000
         STM   @07,@12,@SA00007+32                              02S1208 03033000
*   /*****************************************************************/ 03034000
*   /*                                                               */ 03035000
*   /* THE LOGICAL CPU ADDRESS FOR THIS PSA IS READ FROM THE LCCA. IF*/ 03036000
*   /* THE READ FAILS THE PHYSICAL CPU ADDRESS IS READ FROM THE PSA  */ 03037000
*   /*                                                               */ 03038000
*   /*****************************************************************/ 03039000
*                                                               02S1209 03040000
*   BRNCHADR=ADDR(PYSCPUAD);        /* ERROR RETURN POINT FOR THIS      03041000
*                                      BRREAD                        */ 03042000
         LA    @15,PYSCPUAD                                     02S1209 03043000
         ST    @15,BRNCHADR                                     02S1209 03044000
*   RESPECIFY                                                   02S1210 03045000
*     PREG RESTRICTED;                                          02S1210 03046000
*   PREG=ADDR(LCCACPUA);            /* GET LOG CPU ADDR FROM LCCA FOR   03047000
*                                      THIS PSA                      */ 03048000
         L     PREG,LCCAPTR                                     02S1211 03049000
         LA    PREG,LCCACPUA(,PREG)                             02S1211 03050000
*   GEN(BRREAD (PREG));                                         02S1212 03051000
         BRREAD (PREG)                                                  03052000
*   CPUADDR=CPUTMPRY;               /* LOG CPU ADDR                  */ 03053000
         LH    @00,CPUTMPRY(,PREG)                              02S1213 03054000
         STH   @00,CPUADDR                                      02S1213 03055000
*   CPUADDR=CPUADDR-'40'X;          /* CONVERT TO PHYS CPU ADDR      */ 03056000
*                                                               02S1214 03057000
         SH    @00,@CH01767                                     02S1214 03058000
         STH   @00,CPUADDR                                      02S1214 03059000
*   /*****************************************************************/ 03060000
*   /*                                                               */ 03061000
*   /* DETERMINE IF THE PHYSICAL CPU ADDRESS SHOULD BE PUT IN THE    */ 03062000
*   /* PHYSICAL CPU TABLE                                            */ 03063000
*   /*                                                               */ 03064000
*   /*****************************************************************/ 03065000
*                                                               02S1215 03066000
*CPUADSAV:                                                      02S1215 03067000
*   IF LTABLPTR^=0 THEN             /* IS THERE A CPU ADDR TABLE     */ 03068000
CPUADSAV LTR   LTABLPTR,LTABLPTR                                02S1215 03069000
         BZ    @RF01215                                         02S1215 03070000
*     DO;                           /* YES                           */ 03071000
*       IF PCARTNSW=SWON THEN       /* PROCESSING PSA AT VIRT 0      */ 03072000
         TM    PCARTNSW,B'00000100'                             02S1217 03073000
         BNO   @RF01217                                         02S1217 03074000
*         CTABLPTR=CTABLPTR+LENGTH(REGENTRY);/* NO - ADDR OF NEXT       03075000
*                                      ENTRY IN PHYS CPU ADDR TABLE  */ 03076000
         AH    CTABLPTR,@CH00107                                02S1218 03077000
*       IF CPUADDR='FFFF'X THEN     /* WAS ADDR IN CCA F'S           */ 03078000
@RF01217 CLC   CPUADDR(2),@CH02834                              02S1219 03079000
         BNE   @RF01219                                         02S1219 03080000
*         CPUADDR='0050'X;          /* YES - INDICATES ADDR IN CCA      03081000
*                                      WAS F'S                       */ 03082000
         MVC   CPUADDR(2),@CH02927                              02S1220 03083000
*       REGENTRY=CPUADDR;           /* PUT THIS CPU ADDR IN TABLE    */ 03084000
@RF01219 MVC   REGENTRY(2,CTABLPTR),CPUADDR                     02S1221 03085000
*     END;                                                      02S1222 03086000
*                                                               02S1222 03087000
*   /*****************************************************************/ 03088000
*   /*                                                               */ 03089000
*   /* PRINT THE REGS, PSA, AND STORE STATUS INFO                    */ 03090000
*   /*                                                               */ 03091000
*   /*****************************************************************/ 03092000
*                                                               02S1223 03093000
*   GEN(BRWRITE SKIP);              /* SKIP TO A NEW PAGE            */ 03094000
@RF01215 DS    0H                                               02S1223 03095000
         BRWRITE SKIP                                                   03096000
*   CALL REGPRINT;                                              02S1224 03097000
         BAL   @14,REGPRINT                                     02S1224 03098000
*   RETURN;                         /* RETURN TO CALLER              */ 03099000
*                                                               02S1225 03100000
@EL00007 DS    0H                                               02S1225 03101000
@EF00007 DS    0H                                               02S1225 03102000
@ER00007 LM    @14,@05,@SA00007                                 02S1225 03103000
         LM    @07,@12,@SA00007+32                              02S1225 03104000
         BR    @14                                              02S1225 03105000
*   /*****************************************************************/ 03106000
*   /*                                                               */ 03107000
*   /* THIS ROUTINE WILL GET CONTROL ONLY IF THE BRREAD FAILS WHILE  */ 03108000
*   /* GETTING THE LOGICAL CPU ADDRESS FROM THE LCCA                 */ 03109000
*   /*                                                               */ 03110000
*   /*****************************************************************/ 03111000
*                                                               02S1226 03112000
*PYSCPUAD:                                                      02S1226 03113000
*   BRNCHADR=ADDR(GMSG51D);         /* ERROR RETURN POINT FOR THIS      03114000
*                                      BRREAD                        */ 03115000
PYSCPUAD LA    @00,GMSG51D                                      02S1226 03116000
         ST    @00,BRNCHADR                                     02S1226 03117000
*   RESPECIFY                                                   02S1227 03118000
*     PREG RESTRICTED;                                          02S1227 03119000
*   PREG=ADDR(PSACPUSA);            /* PHYS CPU ADDR FROM PSA   02S1228 03120000
