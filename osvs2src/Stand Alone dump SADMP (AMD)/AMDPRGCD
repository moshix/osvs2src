*           PCASW=SWON;             /* INDICATE PCCAVT NOT AVAILABLE */ 00400000
         OI    PCASW,B'00100000'                                02S0411 00401000
*         END;                                                  02S0412 00402000
*     END;                                                      01S0413 00403000
