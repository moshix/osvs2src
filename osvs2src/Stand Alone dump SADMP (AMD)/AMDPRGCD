         L     PREG,PSAVIRT                                     02S1228 03122000
         LA    PREG,PSACPUSA(,PREG)                             02S1228 03123000
*   CPUADDR=CPUTMPRY;               /* PHYS CPU ADDR                 */ 03124000
         MVC   CPUADDR(2),CPUTMPRY(PREG)                        02S1229 03125000
*   RESPECIFY                                                   02S1230 03126000
