*   /*****************************************************************/ 03573000
*   /*                                                               */ 03574000
*   /* ERROR RETURN POINT IF BRREAD FAILS FOR THIS PHYSICAL CPU      */ 03575000
*   /* ADDRESS IN THE PCCA CURRENTLY BEING PROCESSED.                */ 03576000
*   /*                                                               */ 03577000
*   /*****************************************************************/ 03578000
*                                                               02S1372 03579000
*                                                               03S1372 03580000
*   /*****************************************************************/ 03581000
*   /*                                                               */ 03582000
*   /* THIS ROUTINE GETS CONTROL ONLY IF THE BRREAD FAILS FOR THE    */ 03583000
*   /* PHYSICAL CPU ADDRESS IN THE CURRENT PCCA                      */ 03584000
*   /*                                                               */ 03585000
*   /*****************************************************************/ 03586000
*                                                               03S1372 03587000
*LCPUADDR:                                                      03S1372 03588000
*   IF LCCAPTR^=0 THEN              /* IS LCCA FOR THIS PSA AVAILABLE*/ 03589000
LCPUADDR ICM   @15,15,LCCAPTR                                   03S1372 03590000
         BZ    @RF01372                                         03S1372 03591000
*     DO;                           /* YES                           */ 03592000
*       PCARTNSW=SWON;              /* INDICATE PCCA AVAILABLE       */ 03593000
*                                                               03S1374 03594000
         OI    PCARTNSW,B'00000100'                             03S1374 03595000
*       /*************************************************************/ 03596000
*       /*                                                           */ 03597000
*       /* ATTEMPT TO GET LOGICAL CPU ADDRESS FROM LCCA              */ 03598000
*       /*                                                           */ 03599000
*       /*************************************************************/ 03600000
*                                                               03S1375 03601000
*       CALL LOGCLCPU;                                          03S1375 03602000
         BAL   @14,LOGCLCPU                                     03S1375 03603000
*     END;                                                      03S1376 03604000
*   ELSE                                                        03S1377 03605000
*     DO;                           /* NO - LCCA IS UNAVAILABLE      */ 03606000
         B     @RC01372                                         03S1377 03607000
@RF01372 DS    0H                                               03S1378 03608000
*       IF PSABADSW=SWOFF THEN      /* IS PSA FOR PCCA AVAILABLE     */ 03609000
         TM    PSABADSW,B'00001000'                             03S1378 03610000
         BNZ   @RF01378                                         03S1378 03611000
*         DO;                       /* YES                           */ 03612000
*           BRNCHADR=ADDR(GMSG51B); /* ERROR RETURN POINT FOR THIS      03613000
*                                      BRREAD                        */ 03614000
         LA    @07,GMSG51B                                      03S1380 03615000
         ST    @07,BRNCHADR                                     03S1380 03616000
*           RESPECIFY                                           03S1381 03617000
*             PREG RESTRICTED;                                  03S1381 03618000
*           PREG=ADDR(PSACPUSA);    /* GET PHY CPU ADDR      @YM03489*/ 03619000
         L     PREG,PSAVIRT                                     03S1382 03620000
         LA    PREG,PSACPUSA(,PREG)                             03S1382 03621000
*           GEN(BRREAD (PREG));                                 03S1383 03622000
         BRREAD (PREG)                                                  03623000
*           CPUADDR=CPUTMPRY;       /* PHYS CPU ADDR                 */ 03624000
         LH    @07,CPUTMPRY(,PREG)                              03S1384 03625000
         STH   @07,CPUADDR                                      03S1384 03626000
*           RESPECIFY                                           03S1385 03627000
*             PREG UNRESTRICTED;                                03S1385 03628000
*           CTABLPTR=CTABLPTR+LENGTH(REGENTRY);/* ADDR OF NEXT ENTRY    03629000
*                                      IN PHYS CPU ADDR TABLE        */ 03630000
         AH    CTABLPTR,@CH00107                                03S1386 03631000
*           IF CPUADDR='FFFF'X THEN /* WAS ADDR IN CCA F'S           */ 03632000
         C     @07,@CF02834                                     03S1387 03633000
         BNE   @RF01387                                         03S1387 03634000
*             CPUADDR='0050'X;      /* YES - INDICATES ADDR IN CCA      03635000
*                                      WAS F'S                       */ 03636000
         MVC   CPUADDR(2),@CH02927                              03S1388 03637000
*           REGENTRY=CPUADDR;       /* PUT THIS CPU ADDR IN TABLE    */ 03638000
@RF01387 MVC   REGENTRY(2,CTABLPTR),CPUADDR                     03S1389 03639000
*           CALL REGPRINT;          /* FORMAT THE STORE STATUS DATA  */ 03640000
         BAL   @14,REGPRINT                                     03S1390 03641000
*         END;                                                  03S1391 03642000
*       ELSE                                                    03S1392 03643000
*         DO;                       /* NO - PSA IS UNAVAILABLE       */ 03644000
         B     @RC01378                                         03S1392 03645000
@RF01378 DS    0H                                               03S1393 03646000
*GMSG51B:                                                       03S1393 03647000
*           OUTPUT(2:1+LENGTH(MSG51))=MSG51;/* PRINT UNABLE TO  03S1393 03648000
*                                      DETERMINE CPU ADDR MSG        */ 03649000
GMSG51B  L     @07,CURBUF(,COMBASE)                             03S1393 03650000
         MVC   OUTPUT+1(72,@07),MSG51                           03S1393 03651000
*           CALL PRNTSKIP;                                      03S1394 03652000
         BAL   @14,PRNTSKIP                                     03S1394 03653000
*           GO TO PSAMSG2;          /* PRINT MESSAGE AND GET NEXT       03654000
*                                      PCCA                          */ 03655000
         B     PSAMSG2                                          03S1395 03656000
*         END;                                                  03S1396 03657000
*     END;                                                      03S1397 03658000
*   GO TO PSAREAD2;                 /* ATTEMPT TO FORMAT THE PSA     */ 03659000
         B     PSAREAD2                                         03S1398 03660000
*                                                               02S1399 03661000
*   /*****************************************************************/ 03662000
*   /*                                                               */ 03663000
*   /* THESE ROUTINES WILL GET CONTROL ONLY IF THE BRREADS FAIL WHILE*/ 03664000
*   /* GETTING OR FORMATTING THE PCCA.                               */ 03665000
*   /*                                                               */ 03666000
*   /*****************************************************************/ 03667000
*                                                               02S1399 03668000
*GMSG30C:                                                       02S1399 03669000
*                                                               02S1399 03670000
*   /*****************************************************************/ 03671000
*   /*                                                               */ 03672000
*   /* CONVERT THE PCCA ADDRESS TO PRINTABLE HEX FOR MESSAGE         */ 03673000
*   /*                                                               */ 03674000
*   /*****************************************************************/ 03675000
*                                                               02S1399 03676000
*   RESPECIFY                                                   02S1399 03677000
*    (R1,                                                       02S1399 03678000
*     R4) RESTRICTED;                                           02S1399 03679000
GMSG30C  DS    0H                                               02S1400 03680000
*   R1=ADDR(PCCAPTR);               /* ADDR TO CONVERT               */ 03681000
         LA    R1,PCCAPTR                                       02S1400 03682000
*   R4=ADDR(ADDR30);                /* ADDR OF OUTPUT LOCATION FOR      03683000
*                                      CNVT RTN                      */ 03684000
