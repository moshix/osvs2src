*               GEN(BRREAD (PREG));                             02S0569 00919000
         BRREAD (PREG)                                                  00920000
*               IF VALIDCK^='PCCA' THEN/* DOES THE PCCA VALIDITY        00921000
*                                      CHECK                         */ 00922000
         CLC   VALIDCK(4,PREG),@CC02856                         02S0570 00923000
         BE    @RF00570                                         02S0570 00924000
*                 DO;               /* NO                            */ 00925000
*                   RESPECIFY                                   02S0572 00926000
*                     PREG UNRESTRICTED;                        02S0572 00927000
*                   OUTPUT(2:1+LENGTH(MSG31))=MSG31;/* PRINT VALIDITY   00928000
*                                      CHECK FAILED MSG              */ 00929000
         L     @01,CURBUF(,COMBASE)                             02S0573 00930000
         MVC   OUTPUT+1(26,@01),MSG31                           02S0573 00931000
*                   CALL PRNTSKIP;                              02S0574 00932000
         BAL   @14,PRNTSKIP                                     02S0574 00933000
*                 END;                                          02S0575 00934000
*               BRNCHADR=ADDR(GMSG32A);/* ERROR RETURN POINT FOR        00935000
*                                      BRREADS BY GFR                */ 00936000
@RF00570 LA    @15,GMSG32A                                      02S0576 00937000
         ST    @15,BRNCHADR                                     02S0576 00938000
*               RESPECIFY                                       02S0577 00939000
*                (R0,                                           02S0577 00940000
*                 R1) RESTRICTED;                               02S0577 00941000
*               R0=0;               /* 0 ASID FOR RDC                */ 00942000
         SLR   R0,R0                                            02S0578 00943000
*               DO I=1 TO DIM(FMTTABL3);/* CALL THE FORMAT RTN TO       00944000
*                                      FORMAT THE PCCA ONE LINE AT A    00945000
*                                      TIME                          */ 00946000
         LA    I,1                                              02S0579 00947000
@DL00579 DS    0H                                               02S0580 00948000
*                 R1=FMTTABL3(I);   /* ADDR OF EACH LINE TO BE  02S0580 00949000
*                                      FORMATTED                     */ 00950000
         LR    @05,I                                            02S0580 00951000
         SLA   @05,2                                            02S0580 00952000
         L     R1,FMTTABL3-4(@05)                               02S0580 00953000
*                 CALL AMDPRGFR;    /* FORMAT THE LINE               */ 00954000
         L     @15,AFMTLINE(,COMBASE)                           02S0581 00955000
         BALR  @14,@15                                          02S0581 00956000
*                 GEN SETS(R1,R14,R15)(BRWRITE 1);              02S0582 00957000
         BRWRITE 1                                                      00958000
*               END;                                            02S0583 00959000
         AH    I,@CH00050                                       02S0583 00960000
         CH    I,@CH01967                                       02S0583 00961000
         BNH   @DL00579                                         02S0583 00962000
*               CALL PRNTSKIP;      /* SKIP TWO LINES                */ 00963000
         BAL   @14,PRNTSKIP                                     02S0584 00964000
*               RESPECIFY                                       02S0585 00965000
*                (R0,                                           02S0585 00966000
*                 R1) UNRESTRICTED;                             02S0585 00967000
*                                                               02S0585 00968000
*               /*****************************************************/ 00969000
*               /*                                                   */ 00970000
*               /* GET VIRTUAL ADDRESS OF PSA TO USE LATER TO GET AND*/ 00971000
*               /* FORMAT UNPREFIXED 0-4K. (FOR DSS OR SVCDUMP INPUT */ 00972000
*               /* ONLY)                                             */ 00973000
*               /*                                                   */ 00974000
*               /*****************************************************/ 00975000
*                                                               02S0586 00976000
*GETUNPFX:                                                      02S0586 00977000
*               IF QSADMP=SWOFF THEN/* IS INPUT DSS OR SVCDUMP       */ 00978000
GETUNPFX TM    QSADMP(COMBASE),B'00100000'                      02S0586 00979000
         BNZ   @RF00586                                         02S0586 00980000
*                 DO;               /* YES                           */ 00981000
*                   BRNCHADR=ADDR(UNPREADR);/* ERROR RETURN POINT FOR   00982000
