         ST    @15,DSSPSAVT                                     02S0633 01118000
*   OUTPUT(2:1+LENGTH(MSG57))=MSG57;/* PRINT ERROR FINDING PSA ADDR     01119000
*                                      MSG                           */ 01120000
         L     @01,CURBUF(,COMBASE)                             02S0634 01121000
         MVC   OUTPUT+1(75,@01),MSG57                           02S0634 01122000
*   CALL PRNTSKIP;                  /* PRINT THE MESSAGE             */ 01123000
         BAL   @14,PRNTSKIP                                     02S0635 01124000
*SADSWCK1:                                                      02S0636 01125000
*   ;                               /* DETERMINE INPUT TYPE          */ 01126000
SADSWCK1 DS    0H                                               02S0637 01127000
*   IF QSADMP=SWON THEN             /* IS INPUT SADMP                */ 01128000
         TM    QSADMP(COMBASE),B'00100000'                      01S0637 01129000
         BNO   @RF00637                                         01S0637 01130000
*     DO;                           /* YES                           */ 01131000
*                                                               02S0639 01132000
*       /*************************************************************/ 01133000
*       /*                                                           */ 01134000
*       /* IF RDC IS NOT PREFIXING PROCESSING CONTINUES BY           */ 01135000
*       /* DETERMINING IF ANY MORE CCAS CAN BE FORMATTED. IF RDC IS  */ 01136000
*       /* PREFIXING AND IF THE PCCA IS AVAILABLE, THE PHYSICAL CPU  */ 01137000
*       /* ADDRESS IS READ FROM IT. IF THE PCCA IS NOT A VAILABLE,   */ 01138000
*       /* AND THE LCCA IS, THE LOGICAL CPU ADDRESS IS READ AND      */ 01139000
*       /* CONVERTED TO THE PHYSICAL CPU ADDRESS, BY SUBTRACTING     */ 01140000
*       /* X'40'. IF NEITHER THE PCCA OR THE LCCA IS AVAILABLE, OR   */ 01141000
*       /* ACCESSING THEM CAUSES A BRREAD ERROR, THE PHYSICAL CPU    */ 01142000
*       /* ADDRESS IS READ FROM THE PSA FOR THIS PCCA.               */ 01143000
*       /*                                                           */ 01144000
*       /*************************************************************/ 01145000
*                                                               02S0639 01146000
*       IF PREFXRGV=0 THEN          /* IS RDC PREFIXING              */ 01147000
         ICM   @15,15,PREFXRGV(COMBASE)                         02S0639 01148000
         BNZ   @RF00639                                         02S0639 01149000
*         DO;                       /* NO                            */ 01150000
*           IF LCASW=SWOFF|PCASW=SWOFF THEN/* IS CPU ADDR TABLE BEING   01151000
*                                      USED                          */ 01152000
         TM    LCASW,B'01100000'                                02S0641 01153000
         BO    @RF00641                                         02S0641 01154000
*             REGENTRY='0040'X;     /* YES - INDICATES IPL'ED CPU       01155000
*                                      ADDR NOT FOUND                */ 01156000
         MVC   REGENTRY(2,CTABLPTR),@CH02860                    02S0642 01157000
*           GO TO SADMPOUT;         /* CHECK IF ANY MORE PROCESSING     01158000
*                                      CAN BE DONE                   */ 01159000
         B     SADMPOUT                                         02S0643 01160000
*         END;                                                  02S0644 01161000
*       ELSE                                                    02S0645 01162000
*         DO;                       /* YES - GET PHYS CPU ADDR       */ 01163000
@RF00639 DS    0H                                               02S0646 01164000
*           IF PCCAPTR1^=0 THEN     /* IS THERE A PCCA FOR THIS PSA  */ 01165000
         ICM   @15,7,PCCAPTR1                                   02S0646 01166000
         BZ    @RF00646                                         02S0646 01167000
*             DO;                   /* YES                           */ 01168000
*               BRNCHADR=ADDR(LOGCPUAD);/* ERROR RETURN POINT FOR       01169000
*                                      THIS BRREAD                   */ 01170000
         LA    @15,LOGCPUAD                                     02S0648 01171000
         ST    @15,BRNCHADR                                     02S0648 01172000
*               RESPECIFY                                       02S0649 01173000
*                 PREG RESTRICTED;                              02S0649 01174000
*               PREG=ADDR(PCCACPUA);/* GET PHYS CPU ADDR FROM PCCA   */ 01175000
         L     PREG,PCCAPTR                                     02S0650 01176000
         LA    PREG,PCCACPUA(,PREG)                             02S0650 01177000
*               GEN(BRREAD (PREG));                             02S0651 01178000
