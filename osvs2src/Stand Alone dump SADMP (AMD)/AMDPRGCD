*               PCASW=SWON;         /* INDICATE PCCAVT IS NOT USEFUL */ 02345000
         OI    PCASW,B'00100000'                                02S1001 02346000
*               OUTPUT(2:1+LENGTH(MSG19))=MSG19;/* PRINT ERROR  02S1002 02347000
*                                      ACCESSING PCCAVT MSG          */ 02348000
         L     @06,CURBUF(,COMBASE)                             02S1002 02349000
         MVC   OUTPUT+1(22,@06),MSG19                           02S1002 02350000
*             END;                                              02S1003 02351000
*           CALL PRNTSKIP;                                      02S1004 02352000
@RC00995 BAL   @14,PRNTSKIP                                     02S1004 02353000
*         END;                                                  02S1005 02354000
*     END;                                                      02S1006 02355000
*   RETURN;                         /* RETURN TO CALLER              */ 02356000
*                                                               02S1007 02357000
@EL00002 DS    0H                                               02S1007 02358000
@EF00002 DS    0H                                               02S1007 02359000
@ER00002 LM    @14,@12,@SA00002                                 02S1007 02360000
         BR    @14                                              02S1007 02361000
*   /*****************************************************************/ 02362000
*   /*                                                               */ 02363000
*   /* THIS ROUTINE WILL GET CONTROL ONLY IF A BRREAD FAILS WHILE    */ 02364000
*   /* READING AN ENTRY IN A CCAVT                                   */ 02365000
*   /*                                                               */ 02366000
*   /*****************************************************************/ 02367000
*                                                               02S1008 02368000
*CTRBADD:                                                       02S1008 02369000
*   CCACNTRD=CCACNTRD+1;            /* INDICATES BRREAD FAILED FOR      02370000
*                                      THIS CCA ADDR                 */ 02371000
CTRBADD  LA    CCACNTRD,1(,CCACNTRD)                            02S1008 02372000
*   CCAENTRY='FFFFFFFF'X;           /* INDICATE BRREAD FAILED FOR       02373000
*                                      THIS CCA ADDR                 */ 02374000
         MVC   CCAENTRY(4,TABLPTR),@CF02919                     02S1009 02375000
*   GO TO NXTENTRY;                 /* GET NEXT ENTRY IN CCAVT       */ 02376000
         B     NXTENTRY                                         02S1010 02377000
*   END CCAVTABL;                                               02S1011 02378000
         B     @EL00002                                         02S1011 02379000
*PSAFORMT:                                                      02S1012 02380000
*   PROC;                                                       02S1012 02381000
*                                                               02S1012 02382000
PSAFORMT STM   @14,@12,@SA00003                                 02S1012 02383000
*   /*****************************************************************/ 02384000
*   /*                                                               */ 02385000
*   /* THE PSA IS FORMATTED ONE LINE AT A TIME AND THEN THE REMAINDER*/ 02386000
*   /* OF THE 4K IS PRINTED IN HEX. NO BRREADS ARE DONE AS ALL OF PSA*/ 02387000
*   /* IS IN STORAGE.                                                */ 02388000
*   /*                                                               */ 02389000
*   /*****************************************************************/ 02390000
*                                                               02S1013 02391000
*   RESPECIFY                                                   02S1013 02392000
*     R1 RESTRICTED;                                            02S1013 02393000
*   DO I=1 TO DIM(FMTTABL4);        /* CALL THE FORMAT RTN TO FORMAT    02394000
*                                      THE PSA ONE LINE AT A TIME    */ 02395000
         LA    I,1                                              02S1014 02396000
@DL01014 DS    0H                                               02S1015 02397000
*     R1=FMTTABL4(I);               /* ADDR OF EACH LINE TO BE  02S1015 02398000
*                                      FORMATTED                     */ 02399000
         LR    @05,I                                            02S1015 02400000
         SLA   @05,2                                            02S1015 02401000
         L     R1,FMTTABL4-4(@05)                               02S1015 02402000
*     CALL AMDPRGFR;                /* FORMAT THE LINE               */ 02403000
         L     @15,AFMTLINE(,COMBASE)                           02S1016 02404000
         BALR  @14,@15                                          02S1016 02405000
*     GEN SETS(R1,R14,R15)(BRWRITE 1);                          02S1017 02406000
         BRWRITE 1                                                      02407000
*   END;                                                        02S1018 02408000
         AH    I,@CH00050                                       02S1018 02409000
         CH    I,@CH00234                                       02S1018 02410000
         BNH   @DL01014                                         02S1018 02411000
*   RESPECIFY                                                   02S1019 02412000
*     R1 UNRESTRICTED;                                          02S1019 02413000
*                                                               02S1019 02414000
*   /*****************************************************************/ 02415000
*   /*                                                               */ 02416000
*   /* BUILD THE PCB TO HAVE THE STORAGE UP TO THE FRR STACK PRINTED */ 02417000
*   /*                                                               */ 02418000
*   /*****************************************************************/ 02419000
*                                                               02S1020 02420000
*   PCBPBGN=ADDR(PSAUSEND);         /* LOW ADDR OF BLOCK             */ 02421000
         L     @15,PSAVIRT                                      02S1020 02422000
         LA    @14,PSAUSEND(,@15)                               02S1020 02423000
         ST    @14,PCBPBGN                                      02S1020 02424000
*   PCBPEND=ADDR(PSASTAK);          /* HIGH ADDR OF BLOCK            */ 02425000
         LA    @15,PSASTAK(,@15)                                02S1021 02426000
         ST    @15,PCBPEND                                      02S1021 02427000
*   PCBPASID=0;                     /* ASID FOR RDC                  */ 02428000
         SLR   @15,@15                                          02S1022 02429000
         STH   @15,PCBPASID                                     02S1022 02430000
*   PCBPFLG1='00'X;                 /* RESET OPTION FLAGS            */ 02431000
         MVI   PCBPFLG1,X'00'                                   02S1023 02432000
*   PCBPVIRT=SWON;                  /* INDICATE VIRTUAL REQUEST      */ 02433000
         OI    PCBPVIRT,B'10000000'                             02S1024 02434000
*   PCBPFUNC=PCBPENQ;               /* INDICATE ENQUEUE FUNCTION     */ 02435000
         NI    PCBPFUNC,B'01111111'                             02S1025 02436000
*   RESPECIFY                                                   02S1026 02437000
*     PREG RESTRICTED;                                          02S1026 02438000
*   PREG=ADDR(PCBPL);               /* ADDR OF PARM LIST             */ 02439000
         LA    PREG,PCBPL                                       02S1027 02440000
*   CALL GPCBENQ;                   /* ENQUEUE THIS PCB              */ 02441000
         L     @15,APCBENQ(,COMBASE)                            02S1028 02442000
         BALR  @14,@15                                          02S1028 02443000
*   RESPECIFY                                                   02S1029 02444000
*     PREG UNRESTRICTED;                                        02S1029 02445000
*   CALL AMDPRDPS;                  /* CALL DPS TO PRINT THE STORAGE */ 02446000
*                                                               02S1030 02447000
         L     @15,APRTSTG(,COMBASE)                            02S1030 02448000
         BALR  @14,@15                                          02S1030 02449000
*   /*****************************************************************/ 02450000
*   /*                                                               */ 02451000
*   /* FORMAT FRR STACK                                              */ 02452000
*   /*                                                               */ 02453000
*   /*****************************************************************/ 02454000
*                                                               02S1031 02455000
*   FRRSTACK=STGADPSA+(ADDR(PSASTAK)-ADDR(PSA));/* ADDR OF FRR STACK    02456000
*                                      IN STORAGE                    */ 02457000
         LA    @15,3072                                         02S1031 02458000
         AL    @15,STGADPSA                                     02S1031 02459000
         ST    @15,FRRSTACK                                     02S1031 02460000
*   GEN(BRWRITE 1);                 /* SKIP ONE LINE                 */ 02461000
         BRWRITE 1                                                      02462000
*   RESPECIFY                                                   02S1033 02463000
*     R1 RESTRICTED;                                            02S1033 02464000
*   DO I=1 TO DIM(FMTTABL5);        /* CALL THE FORMAT RTN TO FORMAT    02465000
*                                      THE PSA ONE LINE AT A TIME    */ 02466000
         LA    I,1                                              02S1034 02467000
@DL01034 DS    0H                                               02S1035 02468000
*     R1=FMTTABL5(I);               /* ADDR OF EACH LINE TO BE  02S1035 02469000
*                                      FORMATTED                     */ 02470000
         LR    @05,I                                            02S1035 02471000
         SLA   @05,2                                            02S1035 02472000
         L     R1,FMTTABL5-4(@05)                               02S1035 02473000
*     CALL AMDPRGFR;                                            02S1036 02474000
         L     @15,AFMTLINE(,COMBASE)                           02S1036 02475000
         BALR  @14,@15                                          02S1036 02476000
*     GEN SETS(R1,R14,R15)(BRWRITE 1);                          02S1037 02477000
         BRWRITE 1                                                      02478000
*   END;                                                        02S1038 02479000
         AH    I,@CH00050                                       02S1038 02480000
         CH    I,@CH00043                                       02S1038 02481000
         BNH   @DL01034                                         02S1038 02482000
*   RESPECIFY                                                   02S1039 02483000
*     R1 UNRESTRICTED;                                          02S1039 02484000
*                                                               02S1039 02485000
*   /*****************************************************************/ 02486000
*   /*                                                               */ 02487000
*   /* BUILD THE PCB TO HAVE THE REMAINING STORAGE PRINTED           */ 02488000
*   /*                                                               */ 02489000
*   /*****************************************************************/ 02490000
*                                                               02S1040 02491000
*   PCBPBGN=ADDR(PSASTAK)+608;      /* LOW ADDR OF BLOCK - THREE        02492000
*                                      WORDS OF THE RESERVED AREA       02493000
*                                      FOLLOWING THE STACK ARE  02S1040 02494000
*                                      FORMATTED TO ACHEIVE A 32 BYTE   02495000
*                                      MULTIPLE FOR DPS      @YM23252*/ 02496000
         L     @15,PSAVIRT                                      02S1040 02497000
         LA    @14,PSASTAK(,@15)                                02S1040 02498000
         AH    @14,@CH02923                                     02S1040 02499000
         ST    @14,PCBPBGN                                      02S1040 02500000
*   PCBPEND=4096+PSAVIRT;           /* HIGH ADDR OF BLOCK            */ 02501000
         AL    @15,@CF02905                                     02S1041 02502000
         ST    @15,PCBPEND                                      02S1041 02503000
*   PCBPASID=0;                     /* ASID FOR RDC                  */ 02504000
         SLR   @15,@15                                          02S1042 02505000
         STH   @15,PCBPASID                                     02S1042 02506000
*   PCBPFLG1='00'X;                 /* RESET OPTION FLAGS            */ 02507000
         MVI   PCBPFLG1,X'00'                                   02S1043 02508000
*   PCBPVIRT=SWON;                  /* INDICATE VIRTUAL REQUEST      */ 02509000
         OI    PCBPVIRT,B'10000000'                             02S1044 02510000
*   PCBPFUNC=PCBPENQ;               /* INDICATE ENQUEUE FUNCTION     */ 02511000
         NI    PCBPFUNC,B'01111111'                             02S1045 02512000
*   RESPECIFY                                                   02S1046 02513000
*     PREG RESTRICTED;                                          02S1046 02514000
*   PREG=ADDR(PCBPL);               /* ADDR OF PARM LIST             */ 02515000
         LA    PREG,PCBPL                                       02S1047 02516000
*   CALL GPCBENQ;                   /* ENQUEUE THIS PCB              */ 02517000
         L     @15,APCBENQ(,COMBASE)                            02S1048 02518000
         BALR  @14,@15                                          02S1048 02519000
*   RESPECIFY                                                   02S1049 02520000
*     PREG UNRESTRICTED;                                        02S1049 02521000
*   CALL AMDPRDPS;                  /* CALL DPS TO PRINT THE STORAGE */ 02522000
         L     @15,APRTSTG(,COMBASE)                            02S1050 02523000
         BALR  @14,@15                                          02S1050 02524000
*   RETURN;                                                     02S1051 02525000
@EL00003 DS    0H                                               02S1051 02526000
@EF00003 DS    0H                                               02S1051 02527000
@ER00003 LM    @14,@12,@SA00003                                 02S1051 02528000
         BR    @14                                              02S1051 02529000
*   END PSAFORMT;                                               02S1052 02530000
*SAVCPUAD:                                                      02S1053 02531000
*   PROC;                                                       02S1053 02532000
*                                                               02S1053 02533000
SAVCPUAD STM   @14,@05,@SA00004                                 02S1053 02534000
         STM   @07,@12,@SA00004+32                              02S1053 02535000
*   /*****************************************************************/ 02536000
*   /*                                                               */ 02537000
*   /* THE PHYSICAL CPU ADDRESS IS PUT IN THE PHYSICAL CPU ADDRESS   */ 02538000
*   /* TABLE AT THE NEXT AVAILABLE ENTRY.                            */ 02539000
*   /*                                                               */ 02540000
*   /*****************************************************************/ 02541000
*                                                               02S1054 02542000
*   CTABLPTR=CTABLPTR+LENGTH(REGENTRY);/* ADDR OF NEXT ENTRY IN PHYS    02543000
*                                      CPU ADDR TABLE                */ 02544000
         AH    CTABLPTR,@CH00107                                02S1054 02545000
*   IF CPUADDR='FFFF'X THEN         /* WAS ADDR IN CCA F'S           */ 02546000
         CLC   CPUADDR(2),@CH02834                              02S1055 02547000
         BNE   @RF01055                                         02S1055 02548000
*     CPUADDR='0050'X;              /* YES - INDICATES ADDR IN CCA      02549000
*                                      WAS F'S                       */ 02550000
         MVC   CPUADDR(2),@CH02927                              02S1056 02551000
*   REGENTRY=CPUADDR;               /* PUT THIS CPU ADDR IN TABLE    */ 02552000
@RF01055 MVC   REGENTRY(2,CTABLPTR),CPUADDR                     02S1057 02553000
*   CALL REGPRINT;                  /* PRINT THE REGS                */ 02554000
         BAL   @14,REGPRINT                                     02S1058 02555000
*   IF PCAGOSW=SWON THEN            /* IS THE PCCA AVAILABLE         */ 02556000
         TM    PCAGOSW,B'00010000'                              02S1059 02557000
         BNO   @RF01059                                         02S1059 02558000
*     DO;                           /* YES                           */ 02559000
*       CALL PSAREAD1;              /* GET AND FORMAT THE PSA        */ 02560000
         BAL   @14,PSAREAD1                                     02S1061 02561000
*     END;                                                      02S1062 02562000
*   RETURN;                                                     02S1063 02563000
@EL00004 DS    0H                                               02S1063 02564000
@EF00004 DS    0H                                               02S1063 02565000
@ER00004 LM    @14,@05,@SA00004                                 02S1063 02566000
         LM    @07,@12,@SA00004+32                              02S1063 02567000
         BR    @14                                              02S1063 02568000
*   END SAVCPUAD;                                               02S1064 02569000
         B     @EL00004                                         02S1064 02570000
*PSAREAD1:                                                      02S1065 02571000
*   PROC;                                                       02S1065 02572000
*                                                               02S1065 02573000
PSAREAD1 STM   @14,@12,@SA00005                                 02S1065 02574000
*   /*****************************************************************/ 02575000
*   /*                                                               */ 02576000
*   /* THE PSA FOR THIS PCCA IS VALIDITY CHECKED AND FORMATTED       */ 02577000
*   /*                                                               */ 02578000
*   /*****************************************************************/ 02579000
*                                                               02S1066 02580000
*   PCAGOSW=SWOFF;                  /* RESET SWITCH FOR NEXT PCCA    */ 02581000
         NI    PCAGOSW,B'11101111'                              02S1066 02582000
*   BRNCHADR=ADDR(PSAMSG1);         /* ERROR RETURN POINT FOR THIS      02583000
*                                      BRREAD                        */ 02584000
         LA    @15,PSAMSG1                                      02S1067 02585000
         ST    @15,BRNCHADR                                     02S1067 02586000
*   RESPECIFY                                                   02S1068 02587000
*     PREG RESTRICTED;                                          02S1068 02588000
*   PREG=ADDR(PCCAPSAV);            /* ADDR OF VIRT PSA ADDR - IN       02589000
*                                      PCCA                          */ 02590000
         L     PREG,PCCAPTR                                     02S1069 02591000
         LA    PREG,PCCAPSAV(,PREG)                             02S1069 02592000
*   GEN(BRREAD (PREG));                                         02S1070 02593000
         BRREAD (PREG)                                                  02594000
*   IF TEMPADR1^=0 THEN             /* IS VIRT PSA ADDR NOT 0        */ 02595000
         ICM   @00,7,TEMPADR1(PREG)                             02S1071 02596000
         BZ    @RF01071                                         02S1071 02597000
*     DO;                           /* YES                           */ 02598000
*       PSAVIRT=TEMPADDR;           /* VIRT ADDR OF PSA              */ 02599000
         L     @00,TEMPADDR(,PREG)                              02S1073 02600000
         ST    @00,PSAVIRT                                      02S1073 02601000
*       RESPECIFY                                               02S1074 02602000
*         PREG UNRESTRICTED;                                    02S1074 02603000
*       IF PSAVIRT//4096=0 THEN     /* IS PSA ADDR VALID             */ 02604000
         LR    @02,@00                                          02S1075 02605000
         SRDA  @02,32                                           02S1075 02606000
         D     @02,@CF02905                                     02S1075 02607000
         LTR   @02,@02                                          02S1075 02608000
         BNZ   @RF01075                                         02S1075 02609000
*         DO;                       /* YES                           */ 02610000
*           BRNCHADR=ADDR(PSAERR2); /* ERROR REUTRN POINT FOR THIS      02611000
*                                      BRREAD                        */ 02612000
         LA    @01,PSAERR2                                      02S1077 02613000
         ST    @01,BRNCHADR                                     02S1077 02614000
*           RESPECIFY                                           02S1078 02615000
*             PREG RESTRICTED;                                  02S1078 02616000
*           PREG=PSAVIRT;           /* VIRT ADDR OF PSA IN DUMP      */ 02617000
         LR    PREG,@00                                         02S1079 02618000
*           GEN(BRREAD (PREG));                                 02S1080 02619000
         BRREAD (PREG)                                                  02620000
*           STGADPSA=PREG;          /* ADDR OF PSA IN STORAGE - FOR     02621000
*                                      FMTPTRNS                      */ 02622000
         ST    PREG,STGADPSA                                    02S1081 02623000
*           PREG=PREG+(ADDR(PSAPSA)-ADDR(PSA));/* DISPLACEMENT IN PSA   02624000
*                                      OF EBCDIC NAME 'PSA'          */ 02625000
         AH    PREG,@CH03000                                    02S1082 02626000
*           IF VALIDCK^='PSA ' THEN /* DOES THE PSA VALIDITY CHECK   */ 02627000
         CLC   VALIDCK(4,PREG),@CC02879                         02S1083 02628000
         BE    @RF01083                                         02S1083 02629000
*             DO;                   /* NO                            */ 02630000
*               RESPECIFY                                       02S1085 02631000
*                 PREG UNRESTRICTED;                            02S1085 02632000
*               OUTPUT(2:1+LENGTH(MSG43))=MSG43;/* PRINT VALIDITY       02633000
*                                      CKECK FAILED MSG              */ 02634000
         L     @01,CURBUF(,COMBASE)                             02S1086 02635000
         MVC   OUTPUT+1(25,@01),MSG43                           02S1086 02636000
*               CALL PRNTSKIP;                                  02S1087 02637000
         BAL   @14,PRNTSKIP                                     02S1087 02638000
*             END;                                              02S1088 02639000
*           CALL PSAFORMT;          /* FORMAT THIS PSA               */ 02640000
@RF01083 BAL   @14,PSAFORMT                                     02S1089 02641000
*         END;                                                  02S1090 02642000
*       ELSE                                                    02S1091 02643000
*         DO;                       /* NO - PSA ADDR IS INVALID      */ 02644000
         B     @RC01075                                         02S1091 02645000
@RF01075 DS    0H                                               02S1092 02646000
*           OUTPUT(2:1+LENGTH(MSG44))=MSG44;/* PRINT PSA NOT ON 4K      02647000
*                                      BNDRY MSG                     */ 02648000
         L     @01,CURBUF(,COMBASE)                             02S1092 02649000
         MVC   OUTPUT+1(56,@01),MSG44                           02S1092 02650000
*           CALL PRNTSKIP;                                      02S1093 02651000
         BAL   @14,PRNTSKIP                                     02S1093 02652000
*         END;                                                  02S1094 02653000
*     END;                                                      02S1095 02654000
*   ELSE                                                        02S1096 02655000
*     DO;                           /* NO - VIRT PSA ADDR IS 0       */ 02656000
         B     @RC01071                                         02S1096 02657000
@RF01071 DS    0H                                               02S1097 02658000
*       OUTPUT(2:1+LENGTH(MSG47))=MSG47;/* PRINT PSA ADDR IS 0 MSG   */ 02659000
         L     @01,CURBUF(,COMBASE)                             02S1097 02660000
         MVC   OUTPUT+1(54,@01),MSG47                           02S1097 02661000
*       CALL PRNTSKIP;                                          02S1098 02662000
         BAL   @14,PRNTSKIP                                     02S1098 02663000
*     END;                                                      02S1099 02664000
*PAGESKIP:                                                      02S1100 02665000
*   GEN(BRWRITE SKIP);              /* SKIP TO A NEW PAGE            */ 02666000
@RC01071 DS    0H                                               02S1100 02667000
PAGESKIP BRWRITE SKIP                                                   02668000
*   RETURN;                         /* RETURN TO CALLER              */ 02669000
*                                                               02S1101 02670000
@EL00005 DS    0H                                               02S1101 02671000
@EF00005 DS    0H                                               02S1101 02672000
@ER00005 LM    @14,@12,@SA00005                                 02S1101 02673000
         BR    @14                                              02S1101 02674000
*   /*****************************************************************/ 02675000
*   /*                                                               */ 02676000
*   /* THIS ROUTINE WILL GET CONTROL IF THE BRREAD FAILS WHILE       */ 02677000
*   /* GETTING THE PSA POINTER FROM HTE PCCA OR WHILE GETTING THE PSA*/ 02678000
*   /*                                                               */ 02679000
*   /*****************************************************************/ 02680000
*                                                               02S1102 02681000
*PSAMSG1:                                                       02S1102 02682000
*   OUTPUT(2:1+LENGTH(MSG42))=MSG42;/* PRINT ERROR FINDING PSA MSG   */ 02683000
PSAMSG1  L     @01,CURBUF(,COMBASE)                             02S1102 02684000
         MVC   OUTPUT+1(60,@01),MSG42                           02S1102 02685000
*   CALL PRNTSKIP;                                              02S1103 02686000
         BAL   @14,PRNTSKIP                                     02S1103 02687000
*   GO TO PAGESKIP;                 /* SKIP TO A NEW PAGE            */ 02688000
         B     PAGESKIP                                         02S1104 02689000
*PSAERR2:                                                       02S1105 02690000
*                                                               02S1105 02691000
*   /*****************************************************************/ 02692000
*   /*                                                               */ 02693000
*   /* CONVERT THE PCCA ADDRESS TO PRINTABLE HEX FOR MESSAGE         */ 02694000
*   /*                                                               */ 02695000
*   /*****************************************************************/ 02696000
*                                                               02S1105 02697000
*   RESPECIFY                                                   02S1105 02698000
*    (R1,                                                       02S1105 02699000
*     R4) RESTRICTED;                                           02S1105 02700000
PSAERR2  DS    0H                                               02S1106 02701000
*   R1=ADDR(PCCAPTR);               /* ADDR TO CONVERT               */ 02702000
         LA    R1,PCCAPTR                                       02S1106 02703000
*   R4=ADDR(ADDR40);                /* ADDR OF OUTPUT LOCATION FOR      02704000
*                                      CNVT RTN                      */ 02705000
         LA    R4,ADDR40                                        02S1107 02706000
*   CALL ADRCNVT;                   /* AADRCNVT RTN                  */ 02707000
         L     @15,AADRCNVT(,COMBASE)                           02S1108 02708000
         BALR  @14,@15                                          02S1108 02709000
*   RESPECIFY                                                   02S1109 02710000
*    (R1,                                                       02S1109 02711000
*     R4) UNRESTRICTED;                                         02S1109 02712000
*   OUTPUT(2:1+LENGTH(MSG40))=MSG40;/* PRINT ERROR FINDING PSA MSG   */ 02713000
         L     @01,CURBUF(,COMBASE)                             02S1110 02714000
         MVC   OUTPUT+1(61,@01),MSG40                           02S1110 02715000
*   CALL PRNTSKIP;                                              02S1111 02716000
         BAL   @14,PRNTSKIP                                     02S1111 02717000
*   GO TO PAGESKIP;                 /* SKIP PAGE AND PROCESS NEXT       02718000
*                                      GROUP                         */ 02719000
         B     PAGESKIP                                         02S1112 02720000
*   END PSAREAD1;                                               02S1113 02721000
*PCAMATCH:                                                      02S1114 02722000
*   PROC;                                                       02S1114 02723000
*                                                               02S1114 02724000
PCAMATCH STM   @14,@04,@SA00006                                 02S1114 02725000
         STM   @07,@12,@SA00006+28                              02S1114 02726000
*   /*****************************************************************/ 02727000
*   /*                                                               */ 02728000
*   /* ACCESS THE INTERNAL PCCAVT AND DETERMINE IF THERE IS A PCCA   */ 02729000
*   /* FOR THIS CPU                                                  */ 02730000
*   /*                                                               */ 02731000
*   /*****************************************************************/ 02732000
*                                                               02S1115 02733000
*   TABLPTR=TABLPTR+CCAVSIZE;       /* POINT TO SAME RELATIVE   02S1115 02734000
*                                      POSITION IN PCCAVT            */ 02735000
         AH    TABLPTR,@CH00753                                 02S1115 02736000
*   NOPCCA=SWOFF;                   /* RESET SW FOR THIS ENTRY       */ 02737000
*   GETCPUAD=SWON;                  /* INDICATE TO GET LOG CPU ADDR     02738000
*                                      FROM LCCA                     */ 02739000
         OI    GETCPUAD,B'00000001'                             02S1117 02740000
         NI    NOPCCA,B'11111101'                               02S1117 02741000
*   IF ENTRY1^=0 THEN               /* IS THIS PCCA PTR NOT 0        */ 02742000
         ICM   @07,7,ENTRY1(TABLPTR)                            02S1118 02743000
         BZ    @RF01118                                         02S1118 02744000
*     DO;                           /* YES                           */ 02745000
*       IF ENTRY1='FFFFFF'X THEN    /* WAS THIS PCCA ENTRY READ 02S1120 02746000
*                                      SUCCESSFULLY                  */ 02747000
         CLC   ENTRY1(3,TABLPTR),@CF02883+1                     02S1120 02748000
         BNE   @RF01120                                         02S1120 02749000
*         DO;                       /* NO                            */ 02750000
*           IF LCCAPTR1=0|LCCAPTR1='FFFFFF'X THEN/* IS LCCA AVAILABLE*/ 02751000
         ICM   @07,7,LCCAPTR1                                   02S1122 02752000
         BZ    @RT01122                                         02S1122 02753000
         CLC   LCCAPTR1(3),@CF02883+1                           02S1122 02754000
         BNE   @RF01122                                         02S1122 02755000
@RT01122 DS    0H                                               02S1123 02756000
*             OUTPUT(2:1+LENGTH(MSG55))=MSG55;/* NO - PRINT LCCA,       02757000
*                                      PCCA, PSA NOT FORMATTED MSG   */ 02758000
         L     @07,CURBUF(,COMBASE)                             02S1123 02759000
         MVC   OUTPUT+1(36,@07),MSG55                           02S1123 02760000
*           ELSE                                                02S1124 02761000
*             DO;                   /* YES - LCCA IS AVAILABLE       */ 02762000
*                                                               02S1124 02763000
         B     @RC01122                                         02S1124 02764000
@RF01122 DS    0H                                               02S1125 02765000
*               /*****************************************************/ 02766000
*               /*                                                   */ 02767000
*               /* CONVERT THE LCCA ADDRESS TO PRINTABLE HEX FOR     */ 02768000
*               /* MESSAGE                                           */ 02769000
*               /*                                                   */ 02770000
*               /*****************************************************/ 02771000
*                                                               02S1125 02772000
*               RESPECIFY                                       02S1125 02773000
*                (R1,                                           02S1125 02774000
*                 R4) RESTRICTED;                               02S1125 02775000
*               R1=ADDR(LCCAPTR);   /* ADDR TO CONVERT               */ 02776000
         LA    R1,LCCAPTR                                       02S1126 02777000
*               R4=ADDR(ADDR50);    /* ADDR OF OUTPUT LOCATION FOR      02778000
*                                      CNVT RTN                      */ 02779000
         LA    R4,ADDR50                                        02S1127 02780000
*               CALL ADRCNVT;       /* AADRCNVT RTN                  */ 02781000
         L     @15,AADRCNVT(,COMBASE)                           02S1128 02782000
         BALR  @14,@15                                          02S1128 02783000
*               RESPECIFY                                       02S1129 02784000
*                (R1,                                           02S1129 02785000
*                 R4) UNRESTRICTED;                             02S1129 02786000
*               OUTPUT(2:1+LENGTH(MSG50))=MSG50;/* PRINT UNABLE TO      02787000
*                                      FORMAT PCCA,PSA MSG           */ 02788000
         L     @07,CURBUF(,COMBASE)                             02S1130 02789000
         MVC   OUTPUT+1(46,@07),MSG50                           02S1130 02790000
*             END;                                              02S1131 02791000
*           CALL PRNTSKIP;                                      02S1132 02792000
@RC01122 BAL   @14,PRNTSKIP                                     02S1132 02793000
*           NOPCCA=SWON;            /* INDICATE NO PCCA FOR THIS LCCA*/ 02794000
         OI    NOPCCA,B'00000010'                               02S1133 02795000
*           TABLPTR=TABLPTR-CCAVSIZE;/* RESET PTR TO ACCESS LCCAVT   */ 02796000
         SH    TABLPTR,@CH00753                                 02S1134 02797000
*           RETURN;                 /* RETURN TO CALLER              */ 02798000
@EL00006 DS    0H                                               02S1135 02799000
@EF00006 DS    0H                                               02S1135 02800000
@ER00006 LM    @14,@04,@SA00006                                 02S1135 02801000
         LM    @07,@12,@SA00006+28                              02S1135 02802000
         BR    @14                                              02S1135 02803000
*         END;                                                  02S1136 02804000
*       ELSE                                                    02S1137 02805000
*         DO;                       /* YES - PCCA ENTRY WAS READ        02806000
*                                      SUCCESSFULLY                  */ 02807000
@RF01120 DS    0H                                               02S1138 02808000
*           PCCAPTR=CCAENTRY;       /* ADDR OF THIS PCCA             */ 02809000
         MVC   PCCAPTR(4),CCAENTRY(TABLPTR)                     02S1138 02810000
*           TABLPTR=TABLPTR-CCAVSIZE;/* RESET PTR TO ACCESS LCCAVT   */ 02811000
         SH    TABLPTR,@CH00753                                 02S1139 02812000
*                                                               03S1140 02813000
*           /*********************************************************/ 02814000
*           /*                                                       */ 02815000
*           /* GET THE PCCA FOR THIS CPU AND FORMAT IT ONE LINE AT A */ 02816000
*           /* TIME. IF LCCA HAS NOT BEEN FORMATTED ATTEMPTED TO GET */ 02817000
*           /* IT THROUGH THE PSA AND FORMAT IT.                     */ 02818000
*           /*                                                       */ 02819000
*           /*********************************************************/ 02820000
*                                                               03S1140 02821000
*           IF LCCAPTR1=0|LCCAPTR1='FFFFFF'X THEN/* IS LCCA AVAILABLE*/ 02822000
         ICM   @07,7,LCCAPTR1                                   03S1140 02823000
         BZ    @RT01140                                         03S1140 02824000
         CLC   LCCAPTR1(3),@CF02883+1                           03S1140 02825000
         BNE   @RF01140                                         03S1140 02826000
@RT01140 DS    0H                                               03S1141 02827000
*             DO;                   /* NO                            */ 02828000
*               CALL PCAFIRST;      /* ATTEMPT TO GET LCCA THRU PSA  */ 02829000
         BAL   @14,PCAFIRST                                     03S1142 02830000
*               RETURN;             /* RETURN TO CALLER              */ 02831000
         B     @EL00006                                         03S1143 02832000
*             END;                                              03S1144 02833000
*           ELSE                                                03S1145 02834000
*             DO;                   /* YES - LCCA IS AVAILABLE       */ 02835000
@RF01140 DS    0H                                               03S1146 02836000
*               PCAGOSW=SWON;       /* INDICATE PROCESSING A PCCA    */ 02837000
         OI    PCAGOSW,B'00010000'                              03S1146 02838000
*               BRNCHADR=ADDR(GMSG30B);/* ERROR RETURN POINT FOR THIS   02839000
*                                      BRREAD                        */ 02840000
         LA    @07,GMSG30B                                      03S1147 02841000
         ST    @07,BRNCHADR                                     03S1147 02842000
*               RESPECIFY                                       03S1148 02843000
*                 PREG RESTRICTED;                              03S1148 02844000
*               PREG=PCCAPTR;       /* ADDR OF THE PCCA              */ 02845000
         L     PREG,PCCAPTR                                     03S1149 02846000
*               GEN(BRREAD (PREG));                             03S1150 02847000
         BRREAD (PREG)                                                  02848000
*               IF VALIDCK^='PCCA' THEN/* DOES THE PCCA VALIDITY        02849000
*                                      CHECK                         */ 02850000
         CLC   VALIDCK(4,PREG),@CC02856                         03S1151 02851000
         BE    @RF01151                                         03S1151 02852000
*                 DO;               /* NO                            */ 02853000
*                   RESPECIFY                                   03S1153 02854000
*                     PREG UNRESTRICTED;                        03S1153 02855000
*                   OUTPUT(2:1+LENGTH(MSG31))=MSG31;/* PRINT VALIDITY   02856000
*                                      CHECK FAILED MSG              */ 02857000
         L     @07,CURBUF(,COMBASE)                             03S1154 02858000
         MVC   OUTPUT+1(26,@07),MSG31                           03S1154 02859000
*                   CALL PRNTSKIP;                              03S1155 02860000
         BAL   @14,PRNTSKIP                                     03S1155 02861000
*                 END;                                          03S1156 02862000
*               BRNCHADR=ADDR(GMSG32C);/* ERROR RETURN POINT FOR        02863000
*                                      BRREADS BY GFR                */ 02864000
@RF01151 LA    @07,GMSG32C                                      03S1157 02865000
         ST    @07,BRNCHADR                                     03S1157 02866000
*               RESPECIFY                                       03S1158 02867000
*                (R0,                                           03S1158 02868000
*                 R1) RESTRICTED;                               03S1158 02869000
*               R0=0;               /* 0 ASID FOR RDC                */ 02870000
         SLR   R0,R0                                            03S1159 02871000
*               DO I=1 TO DIM(FMTTABL3);/* CALL THE FORMAT RTN TO       02872000
*                                      FORMAT THE PCCA ONE LINE AT A    02873000
*                                      TIME                          */ 02874000
         LA    I,1                                              03S1160 02875000
@DL01160 DS    0H                                               03S1161 02876000
*                 R1=FMTTABL3(I);   /* ADDR OF EACH LINE TO BE  03S1161 02877000
*                                      FORMATTED                     */ 02878000
         LR    @07,I                                            03S1161 02879000
         SLA   @07,2                                            03S1161 02880000
         L     R1,FMTTABL3-4(@07)                               03S1161 02881000
*                 CALL AMDPRGFR;    /* FORMAT THE LINE               */ 02882000
         L     @15,AFMTLINE(,COMBASE)                           03S1162 02883000
         BALR  @14,@15                                          03S1162 02884000
*                 GEN SETS(R1,R14,R15)(BRWRITE 1);              03S1163 02885000
         BRWRITE 1                                                      02886000
*               END;                                            03S1164 02887000
         AH    I,@CH00050                                       03S1164 02888000
         CH    I,@CH01967                                       03S1164 02889000
         BNH   @DL01160                                         03S1164 02890000
*               CALL PRNTSKIP;      /* SKIP TWO LINES                */ 02891000
         BAL   @14,PRNTSKIP                                     03S1165 02892000
*               RESPECIFY                                       03S1166 02893000
*                (R0,                                           03S1166 02894000
*                 R1) UNRESTRICTED;                             03S1166 02895000
*             END;                                              03S1167 02896000
*           GO TO PCCAOUT;          /* DETERMINE INPUT TYPE          */ 02897000
*                                                               03S1168 02898000
         B     PCCAOUT                                          03S1168 02899000
*           /*********************************************************/ 02900000
*           /*                                                       */ 02901000
*           /* THESE ROUTINES WILL GET CONTROL ONLY IF THE BRREADS   */ 02902000
*           /* FAIL WHILE GETTING OR FORMATTING THE PCCA             */ 02903000
*           /*                                                       */ 02904000
*           /*********************************************************/ 02905000
*                                                               03S1169 02906000
*GMSG30B:                                                       03S1169 02907000
*                                                               03S1169 02908000
*           /*********************************************************/ 02909000
*           /*                                                       */ 02910000
*           /* CONVERT THE PCCA ADDRESS TO PRINTABLE HEX FOR MESSAGE */ 02911000
*           /*                                                       */ 02912000
*           /*********************************************************/ 02913000
*                                                               03S1169 02914000
*           RESPECIFY                                           03S1169 02915000
*            (R1,                                               03S1169 02916000
*             R4) RESTRICTED;                                   03S1169 02917000
GMSG30B  DS    0H                                               03S1170 02918000
*           R1=ADDR(PCCAPTR);       /* ADDR TO CONVERT               */ 02919000
         LA    R1,PCCAPTR                                       03S1170 02920000
*           R4=ADDR(ADDR30);        /* ADDR OF OUTPUT LOCATION FOR      02921000
*                                      CNVT RTN                      */ 02922000
         LA    R4,ADDR30                                        03S1171 02923000
*           CALL ADRCNVT;           /* AADRCNVT RTN                  */ 02924000
         L     @15,AADRCNVT(,COMBASE)                           03S1172 02925000
         BALR  @14,@15                                          03S1172 02926000
*           RESPECIFY                                           03S1173 02927000
*            (R1,                                               03S1173 02928000
*             R4) UNRESTRICTED;                                 03S1173 02929000
*           OUTPUT(2:1+LENGTH(MSG30))=MSG30;/* PRINT ERROR FINDING      02930000
*                                      PCCA MSG                      */ 02931000
         L     @07,CURBUF(,COMBASE)                             03S1174 02932000
         MVC   OUTPUT+1(26,@07),MSG30                           03S1174 02933000
*           CALL PRNTSKIP;                                      03S1175 02934000
         BAL   @14,PRNTSKIP                                     03S1175 02935000
*           GO TO PCCAOUT;          /* DETERMINE INPUT TYPE          */ 02936000
         B     PCCAOUT                                          03S1176 02937000
*GMSG32C:                                                       03S1177 02938000
*           GEN(BRWRITE 1);         /* PRINT PARTIALLY FILLED BUFFER */ 02939000
GMSG32C  BRWRITE 1                                                      02940000
*           OUTPUT(2:1+LENGTH(MSG32))=MSG32;/* PRINT ERROR FORMATTING   02941000
*                                      PCCA MSG                      */ 02942000
         L     @07,CURBUF(,COMBASE)                             03S1178 02943000
         MVC   OUTPUT+1(27,@07),MSG32                           03S1178 02944000
*           CALL PRNTSKIP;                                      03S1179 02945000
         BAL   @14,PRNTSKIP                                     03S1179 02946000
*PCCAOUT:                                                       03S1180 02947000
*           ;                       /* DETERMINE INPUT TYPE          */ 02948000
PCCAOUT  DS    0H                                               03S1181 02949000
*                                                               02S1181 02950000
*           /*********************************************************/ 02951000
*           /*                                                       */ 02952000
*           /* DETERMINE IF THERE IS A PSA TO BE PROCESSED           */ 02953000
*           /*                                                       */ 02954000
*           /*********************************************************/ 02955000
*                                                               02S1181 02956000
*           IF QSADMP=SWOFF THEN    /* IS INPUT DSS OR SVCDUMP       */ 02957000
         TM    QSADMP(COMBASE),B'00100000'                      02S1181 02958000
         BNZ   @RF01181                                         02S1181 02959000
*             DO;                   /* YES                           */ 02960000
*               GETCPUAD=SWOFF;     /* INDICATE NOT TO GET LOG CPU      02961000
*                                      ADDR FROM LCCA                */ 02962000
         NI    GETCPUAD,B'11111110'                             02S1183 02963000
*               CALL PSAREAD1;      /* GET AND FORMAT THE PSA        */ 02964000
         BAL   @14,PSAREAD1                                     02S1184 02965000
*             END;                                              02S1185 02966000
*         END;                                                  02S1186 02967000
@RF01181 DS    0H                                               02S1187 02968000
*     END;                                                      02S1187 02969000
*   ELSE                                                        02S1188 02970000
*     DO;                           /* PCCA ADDR IS 0                */ 02971000
         B     @RC01118                                         02S1188 02972000
@RF01118 DS    0H                                               02S1189 02973000
*       TABLPTR=TABLPTR-CCAVSIZE;   /* RESET PTR TO ACCESS LCCAVT    */ 02974000
         SH    TABLPTR,@CH00753                                 02S1189 02975000
*       NOPCCA=SWON;                /* INDICATE NO PCCA FOR THIS LCCA*/ 02976000
         OI    NOPCCA,B'00000010'                               02S1190 02977000
*       IF LCCAPTR^=0 THEN          /* IS LCCA AVAILABLE             */ 02978000
         ICM   @07,15,LCCAPTR                                   02S1191 02979000
         BZ    @RF01191                                         02S1191 02980000
*         DO;                       /* YES                           */ 02981000
*           IF LCCAPTR1='FFFFFF'X THEN/* WAS LCCA READ SUCCESSFULY   */ 02982000
         CLC   LCCAPTR1(3),@CF02883+1                           02S1193 02983000
         BNE   @RF01193                                         02S1193 02984000
*             OUTPUT(2:1+LENGTH(MSG55))=MSG55;/* NO - PRINT LCCA,       02985000
*                                      PCCA, PSA NOT FORMATTED MSG   */ 02986000
         L     @07,CURBUF(,COMBASE)                             02S1194 02987000
         MVC   OUTPUT+1(36,@07),MSG55                           02S1194 02988000
*           ELSE                                                02S1195 02989000
*             DO;                   /* YES - LCCA WAS READ      02S1195 02990000
*                                      SUCCESSFULLY                  */ 02991000
*                                                               02S1195 02992000
         B     @RC01193                                         02S1195 02993000
@RF01193 DS    0H                                               02S1196 02994000
*               /*****************************************************/ 02995000
*               /*                                                   */ 02996000
*               /* CONVERT THE LCCA ADDRESS TO PRINTABLE HEX FOR     */ 02997000
*               /* MESSAGE                                           */ 02998000
*               /*                                                   */ 02999000
*               /*****************************************************/ 03000000
*                                                               02S1196 03001000
