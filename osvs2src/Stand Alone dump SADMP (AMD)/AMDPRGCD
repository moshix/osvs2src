*                     PREG UNRESTRICTED;                        02S0593 00997000
*                   IF VIRTADDR=0 THEN/* IS THERE A VIRT PSA ADDR    */ 00998000
         ICM   @15,7,VIRTADDR                                   02S0594 00999000
         BNZ   @RF00594                                         02S0594 01000000
*                     DO;           /* NO                            */ 01001000
*                       OUTPUT(2:1+LENGTH(MSG48))=MSG48;/* PRINT PSA    01002000
*                                      ADDR IS 0 MSG                 */ 01003000
         L     @01,CURBUF(,COMBASE)                             02S0596 01004000
         MVC   OUTPUT+1(69,@01),MSG48                           02S0596 01005000
*                       CALL PRNTSKIP;/* PRINT THE MESSAGE           */ 01006000
         BAL   @14,PRNTSKIP                                     02S0597 01007000
*                     END;                                      02S0598 01008000
*                 END;                                          02S0599 01009000
*             END;                                              02S0600 01010000
*           ELSE                                                02S0601 01011000
*             DO;                   /* NO - THERE IS NO PCCA TO 02S0601 01012000
*                                      PROCESS                       */ 01013000
         B     @RC00564                                         02S0601 01014000
@RF00564 DS    0H                                               02S0602 01015000
*SADSWCK3:                                                      02S0602 01016000
*               IF QSADMP=SWOFF THEN/* IS INPUT DSS/SVC DUMP         */ 01017000
SADSWCK3 TM    QSADMP(COMBASE),B'00100000'                      02S0602 01018000
