*         END;                                                  02S0989 02316000
*       CALL PRNTSKIP;                                          02S0990 02317000
@RC00981 BAL   @14,PRNTSKIP                                     02S0990 02318000
*     END;                                                      02S0991 02319000
*   ELSE                                                        02S0992 02320000
*     DO;                           /* NO - ALL CCA ADDRS WERE NOT      02321000
*                                      ZERO                          */ 02322000
         B     @RC00979                                         02S0992 02323000
@RF00979 DS    0H                                               02S0993 02324000
*       IF CCACNTRD=16 THEN         /* DID ALL READS FAIL WITH BRREAD   02325000
*                                      ERROR                         */ 02326000
         CH    CCACNTRD,@CH00041                                02S0993 02327000
         BNE   @RF00993                                         02S0993 02328000
*         DO;                       /* YES                           */ 02329000
*           IF CCAVTSW=SWOFF THEN   /* IS LCCAVT BEING PROCESSED     */ 02330000
         TM    CCAVTSW,B'10000000'                              02S0995 02331000
         BNZ   @RF00995                                         02S0995 02332000
*             DO;                   /* YES                           */ 02333000
*               LCASW=SWON;         /* INDICATE LCCAVT IS NOT USEFUL */ 02334000
         OI    LCASW,B'01000000'                                02S0997 02335000
*               OUTPUT(2:1+LENGTH(MSG12))=MSG12;/* PRINT ERROR  02S0998 02336000
*                                      ACCESSING LCCAVT MSG          */ 02337000
         L     @06,CURBUF(,COMBASE)                             02S0998 02338000
         MVC   OUTPUT+1(22,@06),MSG12                           02S0998 02339000
*             END;                                              02S0999 02340000
*           ELSE                                                02S1000 02341000
*             DO;                   /* NO - PCCAVT IS BEING PROCESSED*/ 02342000
