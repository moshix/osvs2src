*                                                               02S1231 03129000
         B     CPUADSAV                                         02S1231 03130000
*   /*****************************************************************/ 03131000
*   /*                                                               */ 03132000
*   /* THIS ROUTINE WILL GET CONTROL ONLY IF THE BRREAD FAILS WHILE  */ 03133000
*   /* GETTING THE PHYSICAL CPU ADDRESS FROM THE PSA                 */ 03134000
*   /*                                                               */ 03135000
*   /*****************************************************************/ 03136000
*                                                               02S1232 03137000
*GMSG51D:                                                       02S1232 03138000
*   IF LCASW=SWOFF|PCASW=SWOFF THEN /* IS CPU ADDR TABLE BEING USED  */ 03139000
GMSG51D  TM    LCASW,B'01100000'                                02S1232 03140000
         BO    @RF01232                                         02S1232 03141000
*     REGENTRY='0040'X;             /* YES - INDICATES IPL'ED CPU       03142000
*                                      ADDR NOT FOUND                */ 03143000
         MVC   REGENTRY(2,CTABLPTR),@CH02860                    02S1233 03144000
*   OUTPUT(2:1+LENGTH(MSG51))=MSG51;/* PRINT UNABLE TO DETERMINE CPU    03145000
*                                      ADD R MSG                     */ 03146000
@RF01232 L     @01,CURBUF(,COMBASE)                             02S1234 03147000
         MVC   OUTPUT+1(72,@01),MSG51                           02S1234 03148000
*   CALL PRNTSKIP;                                              02S1235 03149000
         BAL   @14,PRNTSKIP                                     02S1235 03150000
*   RETURN;                         /* RETURN TO CALLER              */ 03151000
         B     @EL00007                                         02S1236 03152000
*   END LOGCLCPU;                                               02S1237 03153000
*PCAFIRST:                                                      02S1238 03154000
*   PROC;                                                       02S1238 03155000
*                                                               02S1238 03156000
PCAFIRST STM   @14,@05,@SA00008                                 02S1238 03157000
         STM   @07,@12,@SA00008+32                              02S1238 03158000
*   /*****************************************************************/ 03159000
*   /*                                                               */ 03160000
*   /* THE PCCA WILL BE VALIDITY CHECKED AND FORMATTED. THE VIRTUAL  */ 03161000
*   /* ADDRESS OF THE PSA IS READ FROM THE PCCA. IF IT IS NON-ZERO   */ 03162000
*   /* THE VIRTUAL ADDRESS OF THE LCCA IS READ FROM THE PSA. THE LCCA*/ 03163000
*   /* IS VALIDITY CHECKED AND FORMATTED. IF INPUT IS SADMP, THE     */ 03164000
*   /* STORE-STATUS DATA IS FORMATTED AND THEN THE PSA IS FORMATTED. */ 03165000
*   /* IF INPUT IS DSS OR SVCDUMP THE PSA IS FORMATTED NEXT. IF THE  */ 03166000
*   /* VIRTUAL PSA ADDRESS READ FROM THE PCCA IS ZERO AN ERROR       */ 03167000
*   /* MESSAGE IS PRINTED AND PROCESSING CONTINUES BY CHECKING THE   */ 03168000
*   /* TYPE OF IN PUT, AS ABOVE. THE ONLY EXCEPTION IS THAT THE PSA  */ 03169000
*   /* WILL NOT BE FORMATTED.                                        */ 03170000
*   /*                                                               */ 03171000
*   /*****************************************************************/ 03172000
*                                                               02S1239 03173000
*   BRNCHADR=ADDR(GMSG30C);         /* ERROR RETURN POINT FOR THIS      03174000
*                                      BRREAD                        */ 03175000
         LA    @07,GMSG30C                                      02S1239 03176000
         ST    @07,BRNCHADR                                     02S1239 03177000
*   RESPECIFY                                                   02S1240 03178000
*     PREG RESTRICTED;                                          02S1240 03179000
*   PREG=PCCAPTR;                   /* ADDR OF THE PCCA              */ 03180000
         L     PREG,PCCAPTR                                     02S1241 03181000
*   GEN(BRREAD (PREG));                                         02S1242 03182000
         BRREAD (PREG)                                                  03183000
*   IF VALIDCK^='PCCA' THEN         /* DOES THE PCCA VALIDITY CHECK  */ 03184000
