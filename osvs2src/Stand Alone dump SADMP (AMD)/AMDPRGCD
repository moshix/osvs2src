*   RESPECIFY                                                   02S1415 03730000
*    (R1,                                                       02S1415 03731000
*     R4) UNRESTRICTED;                                         02S1415 03732000
*   OUTPUT(2:1+LENGTH(MSG40))=MSG40;/* PRINT ERROR FINDING PSA MSG   */ 03733000
         L     @07,CURBUF(,COMBASE)                             02S1416 03734000
         MVC   OUTPUT+1(61,@07),MSG40                           02S1416 03735000
*   CALL PRNTSKIP;                                              02S1417 03736000
         BAL   @14,PRNTSKIP                                     02S1417 03737000
*   RETURN;                         /* RETURN TO CALLER              */ 03738000
         B     @EL00008                                         02S1418 03739000
*   END PCAFIRST;                                               02S1419 03740000
         B     @EL00008                                         02S1419 03741000
*CSTAUSPT:                                                      02S1420 03742000
*   PROC;                                                       02S1420 03743000
*                                                               02S1420 03744000
CSTAUSPT STM   @14,@05,@SA00009                                 02S1420 03745000
         STM   @07,@12,@SA00009+32                              02S1420 03746000
*   /*****************************************************************/ 03747000
*   /*                                                               */ 03748000
*   /* IF THE CVT IS UNAVAILABLE OR THE CCAVTS ARE UNAVAILABLE THE   */ 03749000
*   /* PHYSICAL CPU ADDRESS TABLE IS NOT SEARCHED, BUT A LOOP IS     */ 03750000
*   /* EXECUTED ATTEMPTING TO READ AND FORMAT ANY STORE-STATUS DATA  */ 03751000
*   /* THAT MIGHT EXIST. IF THE CVT IS AVAILABLE THE TABLE IS        */ 03752000
*   /* SEARCHED AND AN ATTEMPT IS MADE TO READ AND FORMAT THE        */ 03753000
*   /* STORE-STATUS DATA FOR EACH CPU ADDRESS NOT IN THE TABLE.      */ 03754000
*   /*                                                               */ 03755000
*   /*****************************************************************/ 03756000
*                                                               02S1421 03757000
*   STATUSW=SWON;                   /* INDICATE PROCESSING CPU ADDRS    03758000
*                                      FROM TABLE                    */ 03759000
         OI    STATUSW,B'00010000'                              02S1421 03760000
*   IF LCASW^=SWON&PCASW^=SWON THEN /* IS CPU ADDR TABLE BEING USED  */ 03761000
         TM    LCASW,B'01100000'                                02S1422 03762000
         BNZ   @RF01422                                         02S1422 03763000
*     DO;                           /* YES                           */ 03764000
*       DO CPUADRCK=0 TO POSIBLAD;  /* ADDR TO COMPARE AGAINST TABLE    03765000
*                                      ENTRIES                       */ 03766000
         SLR   CPUADRCK,CPUADRCK                                02S1424 03767000
@DL01424 DS    0H                                               02S1425 03768000
*         CTABLPTR=LTABLPTR+(2*CCAVSIZE);/* ADDR OF BEGINNING OF        03769000
*                                      TABLE                         */ 03770000
         LA    CTABLPTR,128                                     02S1425 03771000
         ALR   CTABLPTR,LTABLPTR                                02S1425 03772000
*         DO K=0 TO NUMBCPUS;       /* SEARCH THE TABLE TO DETERMINE    03773000
*                                      WHICH ADDRS ARE MISSING       */ 03774000
         SLR   K,K                                              02S1426 03775000
@DL01426 DS    0H                                               02S1427 03776000
*           IF CPUADRCK^=REGENTRY THEN/* IS THIS ADDR IN TABLE       */ 03777000
         SLR   @15,@15                                          02S1427 03778000
         ICM   @15,3,REGENTRY(CTABLPTR)                         02S1427 03779000
         CR    CPUADRCK,@15                                     02S1427 03780000
         BE    @RF01427                                         02S1427 03781000
*             DO;                   /* NO                            */ 03782000
*               IF REGENTRY='FFFF'X THEN/* IS THIS THE END OF TABLE  */ 03783000
         C     @15,@CF02834                                     02S1429 03784000
         BE    @RT01429                                         02S1429 03785000
*                 GO TO GOFORMAT;   /* YES - FORMAT THE STATUS DATA     03786000
*                                      FOR THIS CPU                  */ 03787000
*               ELSE                /* NO - INCREMENT POINTER TO NEXT   03788000
*                                      ENTRY                         */ 03789000
*                 CTABLPTR=CTABLPTR+LENGTH(REGENTRY);/* UPDATE PTR TO   03790000
*                                      REFERENCE NEXT ENTRY          */ 03791000
         AH    CTABLPTR,@CH00107                                02S1431 03792000
*             END;                                              02S1432 03793000
*           ELSE                                                02S1433 03794000
*             GO TO NEXTVALU;       /* UPDATE COMPARE ADDR           */ 03795000
*         END;                                                  02S1434 03796000
         AH    K,@CH00050                                       02S1434 03797000
         CH    K,@CH00043                                       02S1434 03798000
         BNH   @DL01426                                         02S1434 03799000
*GOFORMAT:                                                      02S1435 03800000
*         CPUADDR=CPUADRCK;         /* CPU ADDR FOR BRREAD           */ 03801000
GOFORMAT STH   CPUADRCK,CPUADDR                                 02S1435 03802000
*         GEN(BRWRITE SKIP);        /* SKIP TO THE NEXT PAGE         */ 03803000
         BRWRITE SKIP                                                   03804000
*         CALL REGPRINT;            /* PRINT THE STORE STATUS DATA   */ 03805000
         BAL   @14,REGPRINT                                     02S1437 03806000
*NEXTVALU:                                                      02S1438 03807000
*       END;                                                    02S1438 03808000
NEXTVALU AH    CPUADRCK,@CH00050                                02S1438 03809000
         CH    CPUADRCK,@CH01648                                02S1438 03810000
         BNH   @DL01424                                         02S1438 03811000
*     END;                                                      02S1439 03812000
*   ELSE                                                        02S1440 03813000
*     DO;                           /* NO - CPU ADDR TABLE IS NOT       03814000
*                                      BEING USED                    */ 03815000
         B     @RC01422                                         02S1440 03816000
@RF01422 DS    0H                                               02S1441 03817000
*       CPUADRCK=CPUADDR;           /* CPU ADDR OF REGS ALREADY 02S1441 03818000
*                                      FORMATTED                     */ 03819000
         SLR   CPUADRCK,CPUADRCK                                02S1441 03820000
         ICM   CPUADRCK,3,CPUADDR                               02S1441 03821000
*       DO CPUADDR=0 TO POSIBLAD;   /* COMPARE THIS ADDR TO THE ONE     03822000
*                                      FOR THE REGS ALREADY PRINTED  */ 03823000
         SLR   @03,@03                                          02S1442 03824000
         STH   @03,CPUADDR                                      02S1442 03825000
@DL01442 DS    0H                                               02S1443 03826000
*         IF CPUADDR^=CPUADRCK THEN /* HAS DATA FOR THIS ADDR BEEN      03827000
*                                      PROCESSED                     */ 03828000
         CR    @03,CPUADRCK                                     02S1443 03829000
         BE    @RF01443                                         02S1443 03830000
*           DO;                     /* NO                            */ 03831000
*             GEN(BRWRITE SKIP);    /* SKIP TO THE NEXT PAGE         */ 03832000
         BRWRITE SKIP                                                   03833000
*             CALL REGPRINT;        /* ATTEMPT TO FORMAT THE DATA    */ 03834000
         BAL   @14,REGPRINT                                     02S1446 03835000
*           END;                                                02S1447 03836000
*       END;                                                    02S1448 03837000
@RF01443 LA    @03,1                                            02S1448 03838000
         SLR   @15,@15                                          02S1448 03839000
         ICM   @15,3,CPUADDR                                    02S1448 03840000
         ALR   @03,@15                                          02S1448 03841000
         STH   @03,CPUADDR                                      02S1448 03842000
         CH    @03,@CH01648                                     02S1448 03843000
         BNH   @DL01442                                         02S1448 03844000
*     END;                                                      02S1449 03845000
*   RETURN;                                                     02S1450 03846000
@EL00009 DS    0H                                               02S1450 03847000
@EF00009 DS    0H                                               02S1450 03848000
@ER00009 LM    @14,@05,@SA00009                                 02S1450 03849000
         LM    @07,@12,@SA00009+32                              02S1450 03850000
         BR    @14                                              02S1450 03851000
*   END CSTAUSPT;                                               02S1451 03852000
         B     @EL00009                                         02S1451 03853000
*REGPRINT:                                                      02S1452 03854000
*   PROC;                                                       02S1452 03855000
*                                                               02S1452 03856000
REGPRINT STM   @14,@12,@SA00010                                 02S1452 03857000
*   /*****************************************************************/ 03858000
*   /*                                                               */ 03859000
*   /* THIS ROUTINE CONVERTS THE PHYSICAL CPU ADDRESS TO PRINTABLE   */ 03860000
*   /* HEX, PRINTS A HEADER, AND FORMATS THE STORE-STATUS DATA. IF   */ 03861000
*   /* THE PHYSICAL CPU ADDRESS IS GREATER THAN 63 IT IS INVALID AND */ 03862000
*   /* CONTROL IS IMMEDIATELY RE TURNED TO THE CALLER. OTHERWISE A   */ 03863000
*   /* BRREAD IS DONE FOR THAT STATUS RECORD .                       */ 03864000
*   /*                                                               */ 03865000
*   /*****************************************************************/ 03866000
*                                                               02S1453 03867000
*   IF CPUADDR1<0|CPUADDR>=64 THEN  /* IS THIS A VALID CPU ADDR      */ 03868000
         ICM   @15,3,CPUADDR1                                   02S1453 03869000
         BM    @RT01453                                         02S1453 03870000
         CLC   CPUADDR(2),@CH00753                              02S1453 03871000
         BL    @RF01453                                         02S1453 03872000
@RT01453 DS    0H                                               02S1454 03873000
*     DO;                           /* NO                            */ 03874000
*       OUTPUT(2:1+LENGTH(MSG56))=MSG56;/* PRINT INVALID CPU ADDR MSG*/ 03875000
         L     @01,CURBUF(,COMBASE)                             02S1455 03876000
         MVC   OUTPUT+1(60,@01),MSG56                           02S1455 03877000
*       CALL PRNTSKIP;                                          02S1456 03878000
         BAL   @14,PRNTSKIP                                     02S1456 03879000
*       RETURN;                     /* RETURN TO CALLER              */ 03880000
@EL00010 DS    0H                                               02S1457 03881000
@EF00010 DS    0H                                               02S1457 03882000
@ER00010 LM    @14,@12,@SA00010                                 02S1457 03883000
         BR    @14                                              02S1457 03884000
*     END;                                                      02S1458 03885000
*                                                               02S1458 03886000
*   /*****************************************************************/ 03887000
*   /*                                                               */ 03888000
*   /* THE CPU ADDRESS IS VALID - ATTEMPT TO PROCESS THE STATUS      */ 03889000
*   /* RECORDS                                                       */ 03890000
*   /*                                                               */ 03891000
*   /*****************************************************************/ 03892000
*                                                               02S1459 03893000
*   FPREGS=SWOFF;                   /* RESET TO ALLOW PROCESSING OF     03894000
*                                      GPRS AND CTRS FIRST           */ 03895000
@RF01453 NI    FPREGS,B'11011111'                               02S1459 03896000
*   BRNCHADR=ADDR(IOERRCK);         /* ERROR RETURN POINT FOR THIS      03897000
*                                      BRREAD                        */ 03898000
         LA    @03,IOERRCK                                      02S1460 03899000
         ST    @03,BRNCHADR                                     02S1460 03900000
*   RESPECIFY                                                   02S1461 03901000
*     PREG RESTRICTED;                                          02S1461 03902000
*   PREG=CPUADDR;                   /* PHYS CPU ADDR FOR BRREAD      */ 03903000
         SLR   PREG,PREG                                        02S1462 03904000
         ICM   PREG,3,CPUADDR                                   02S1462 03905000
*   GEN(BRREAD (PREG),CPU);                                     02S1463 03906000
         BRREAD (PREG),CPU                                              03907000
*   STATSPTR=PREG;                  /* ADDR OF BEGINNING OF STATUS      03908000
*                                      RECORDS                       */ 03909000
         LR    STATSPTR,PREG                                    02S1464 03910000
*   RESPECIFY                                                   02S1465 03911000
*     PREG UNRESTRICTED;                                        02S1465 03912000
*                                                               02S1465 03913000
*   /*****************************************************************/ 03914000
*   /*                                                               */ 03915000
*   /* CONVERT THE PHYSICAL CPU ADDRESS TO PRINTABLE HEX AND PRINT   */ 03916000
*   /* HEADER                                                        */ 03917000
*   /*                                                               */ 03918000
*   /*****************************************************************/ 03919000
*                                                               02S1466 03920000
*   UNPAKADR=CPUADDR;               /* PHYS CPU ADDR TO BE CONVERTED */ 03921000
         MVC   UNPAKADR(2),CPUADDR                              02S1466 03922000
*   UNPK(UNPAKTO,UNPAKFRM);         /* UNPACK ADDR                   */ 03923000
         UNPK  UNPAKTO(5),UNPAKFRM(3)                           02S1467 03924000
*   TR(TRANSTO,EBCTABL);            /* TRANS TO PRINTABLE HEX        */ 03925000
         TR    TRANSTO(4),EBCTABL(COMBASE)                      02S1468 03926000
*   CNVTCPU=TRANSTO;                /* PUT IN TITLE                  */ 03927000
         MVC   CNVTCPU(4),TRANSTO                               02S1469 03928000
*   OUTPUT(41:40+LENGTH(REGTITL2))=REGTITL2;/* PRINT HEADER          */ 03929000
         L     @03,CURBUF(,COMBASE)                             02S1470 03930000
         MVC   OUTPUT+40(42,@03),REGTITL2                       02S1470 03931000
*   CALL PRNTSKIP;                                              02S1471 03932000
*                                                               02S1471 03933000
         BAL   @14,PRNTSKIP                                     02S1471 03934000
*   /*****************************************************************/ 03935000
*   /*                                                               */ 03936000
*   /* DETERMINE IF STATUS IN 4K IS VALID                            */ 03937000
*   /*                                                               */ 03938000
*   /*****************************************************************/ 03939000
*                                                               02S1472 03940000
*   IF INVALID=SWON THEN            /* IS DATA INVALID               */ 03941000
         TM    INVALID(STATSPTR),B'01000000'                    02S1472 03942000
         BNO   @RF01472                                         02S1472 03943000
*     DO;                           /* YES                           */ 03944000
*       IF GPREGS=SWON THEN         /* IS THIS THE IPL'D CPU         */ 03945000
         TM    GPREGS(STATSPTR),B'00100000'                     02S1474 03946000
         BNO   @RF01474                                         02S1474 03947000
*         DO;                       /* YES - ONLY GEN PURPOSE REGS      03948000
*                                      ARE VALID                     */ 03949000
*           OUTPUT(2:1+LENGTH(MSG54))=MSG54;/* PRINT ONLY GPRS ARE      03950000
*                                      VALID MSG                     */ 03951000
         L     @03,CURBUF(,COMBASE)                             02S1476 03952000
         MVC   OUTPUT+1(40,@03),MSG54                           02S1476 03953000
*           CALL PRNTSKIP;                                      02S1477 03954000
         BAL   @14,PRNTSKIP                                     02S1477 03955000
*           REGSW=SWOFF;            /* INDICATE GPRS ARE TO BE  02S1478 03956000
*                                      FORMATTED                     */ 03957000
         NI    REGSW,B'10111111'                                02S1478 03958000
*           CALL REGFOMAT;          /* ATTEMPT TO FORMAT THE GPRS    */ 03959000
         BAL   @14,REGFOMAT                                     02S1479 03960000
*           CALL PRNTSKIP;          /* SKIP TWO LINES                */ 03961000
         BAL   @14,PRNTSKIP                                     02S1480 03962000
*         END;                                                  02S1481 03963000
*       ELSE                                                    02S1482 03964000
*         DO;                       /* NO - THIS ISN'T THE IPL'D CPU */ 03965000
         B     @RC01474                                         02S1482 03966000
@RF01474 DS    0H                                               02S1483 03967000
*           OUTPUT(2:1+LENGTH(MSG53))=MSG53;/* PRINT STATUS DATA MAY    03968000
*                                      BE INVALID MSG                */ 03969000
         L     @03,CURBUF(,COMBASE)                             02S1483 03970000
