*   CALL ADRCNVT;                   /* AADRCNVT RTN                  */ 03686000
         L     @15,AADRCNVT(,COMBASE)                           02S1402 03687000
         BALR  @14,@15                                          02S1402 03688000
*   RESPECIFY                                                   02S1403 03689000
*    (R1,                                                       02S1403 03690000
*     R4) UNRESTRICTED;                                         02S1403 03691000
*   OUTPUT(2:1+LENGTH(MSG30))=MSG30;/* PRINT ERROR FINDING PCCA MSG  */ 03692000
         L     @07,CURBUF(,COMBASE)                             02S1404 03693000
         MVC   OUTPUT+1(26,@07),MSG30                           02S1404 03694000
*   CALL PRNTSKIP;                                              02S1405 03695000
         BAL   @14,PRNTSKIP                                     02S1405 03696000
*   GO TO LCARETRY;                 /* GET LCCA FOR THIS PCCA        */ 03697000
         B     LCARETRY                                         02S1406 03698000
*GMSG32B:                                                       02S1407 03699000
*   GEN(BRWRITE 1);                 /* PRINT PARTIALLY FILLED BUFFER */ 03700000
GMSG32B  BRWRITE 1                                                      03701000
*   OUTPUT(2:1+LENGTH(MSG32))=MSG32;/* PRINT ERROR FORMATTING PCCA      03702000
*                                      MSG                           */ 03703000
         L     @07,CURBUF(,COMBASE)                             02S1408 03704000
         MVC   OUTPUT+1(27,@07),MSG32                           02S1408 03705000
*   CALL PRNTSKIP;                                              02S1409 03706000
