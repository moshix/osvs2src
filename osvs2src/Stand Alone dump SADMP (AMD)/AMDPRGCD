*               RESPECIFY                                       02S0653 01183000
*                 PREG UNRESTRICTED;                            02S0653 01184000
*             END;                                              02S0654 01185000
*           ELSE                                                02S0655 01186000
*             DO;                   /* NO PCCA - GET LCCA - ALSO        01187000
*                                      RETURN POINT SHOULD BRREAD       01188000
*                                      FAIL FOR PHYS CPU ADDR IN PCCA*/ 01189000
         B     @RC00646                                         02S0655 01190000
@RF00646 DS    0H                                               02S0656 01191000
*LOGCPUAD:                                                      02S0656 01192000
*               IF LCCAPTR1^=0 THEN /* IS THERE AN LCCA FOR THIS PSA */ 01193000
LOGCPUAD ICM   @15,7,LCCAPTR1                                   02S0656 01194000
         BZ    @RF00656                                         02S0656 01195000
*                 DO;               /* YES - GET LOG CPU ADDR        */ 01196000
*                   PCARTNSW=SWOFF; /* INDICATE PROCESSING PSA AT       01197000
*                                      VITR 0                        */ 01198000
*                                                               02S0658 01199000
         NI    PCARTNSW,B'11111011'                             02S0658 01200000
*                   /*************************************************/ 01201000
*                   /*                                               */ 01202000
*                   /* ATTEMPT TO GET LOGICAL CPU ADDRESS FROM LCCA  */ 01203000
*                   /*                                               */ 01204000
*                   /*************************************************/ 01205000
*                                                               02S0659 01206000
*                   CALL LOGCLCPU;                              02S0659 01207000
         BAL   @14,LOGCLCPU                                     02S0659 01208000
*                   GO TO SADMPOUT;                             02S0660 01209000
         B     SADMPOUT                                         02S0660 01210000
*                 END;                                          02S0661 01211000
*               ELSE                                            02S0662 01212000
*                 DO;               /* NO LCCA - GET PHYS CPU ADDR      01213000
*                                      FROM PSA IF RDC IS PREFIXING  */ 01214000
@RF00656 DS    0H                                               02S0663 01215000
*                   BRNCHADR=ADDR(GMSG51C);/* ERROR RETURN POINT FOR    01216000
*                                      THIS BRREAD                   */ 01217000
         LA    @15,GMSG51C                                      02S0663 01218000
         ST    @15,BRNCHADR                                     02S0663 01219000
*                   RESPECIFY                                   02S0664 01220000
*                     PREG RESTRICTED;                          02S0664 01221000
*                   PREG=ADDR(PSACPUSA);/* GET PHY CPU ADDR  @YM03489*/ 01222000
         L     PREG,PSAVIRT                                     02S0665 01223000
         LA    PREG,PSACPUSA(,PREG)                             02S0665 01224000
*                   GEN(BRREAD (PREG));                         02S0666 01225000
         BRREAD (PREG)                                                  01226000
*                   CPUADDR=CPUTMPRY;/* PHYS CPU ADDR                */ 01227000
         MVC   CPUADDR(2),CPUTMPRY(PREG)                        02S0667 01228000
*                   RESPECIFY                                   02S0668 01229000
*                     PREG UNRESTRICTED;                        02S0668 01230000
*                 END;                                          02S0669 01231000
*             END;                                              02S0670 01232000
*         END;                                                  02S0671 01233000
*                                                               02S0671 01234000
@RC00646 DS    0H                                               02S0672 01235000
*       /*************************************************************/ 01236000
*       /*                                                           */ 01237000
*       /* DETERMINE IF THIS PHYSICAL CPU ADDRESS SHOULD BE PUT IN   */ 01238000
*       /* THE PHYSICAL CPU ADDRESS TABLE                            */ 01239000
*       /*                                                           */ 01240000
*       /*************************************************************/ 01241000
*                                                               02S0672 01242000
*       IF LCASW=SWOFF|PCASW=SWOFF THEN/* IS CPU ADDR TABLE BEING       01243000
*                                      USED                          */ 01244000
         TM    LCASW,B'01100000'                                02S0672 01245000
         BO    @RF00672                                         02S0672 01246000
*         REGENTRY=CPUADDR;         /* YES - PUT THIS CPU ADDR IN       01247000
*                                      TABLE                         */ 01248000
*                                                               02S0673 01249000
         MVC   REGENTRY(2,CTABLPTR),CPUADDR                     02S0673 01250000
*       /*************************************************************/ 01251000
*       /*                                                           */ 01252000
*       /* PRINT THE PSW, REGS, AND STORE-STATUS DATA                */ 01253000
*       /*                                                           */ 01254000
*       /*************************************************************/ 01255000
*                                                               02S0674 01256000
*       CALL REGPRINT;                                          02S0674 01257000
@RF00672 BAL   @14,REGPRINT                                     02S0674 01258000
*       GO TO SADMPOUT;             /* GET LCCA FROM PSA AT VIRT 0   */ 01259000
*                                                               02S0675 01260000
         B     SADMPOUT                                         02S0675 01261000
*       /*************************************************************/ 01262000
*       /*                                                           */ 01263000
*       /* THIS ROUTINE WILL GET CONTROL ONLY IF THE BRREAD FAILS    */ 01264000
*       /* WHILE GETTING THE PHYSICAL CPU ADDRESS FROM THE PSA       */ 01265000
*       /*                                                           */ 01266000
*       /*************************************************************/ 01267000
*                                                               02S0676 01268000
*GMSG51C:                                                       02S0676 01269000
*       IF LCASW=SWOFF|PCASW=SWOFF THEN/* IS CPU ADDR TABLE BEING       01270000
*                                      USED                          */ 01271000
GMSG51C  TM    LCASW,B'01100000'                                02S0676 01272000
         BO    @RF00676                                         02S0676 01273000
*         REGENTRY='0040'X;         /* YES - INDICATES IPL'ED CPU       01274000
*                                      ADDR NOT FOUND                */ 01275000
         MVC   REGENTRY(2,CTABLPTR),@CH02860                    02S0677 01276000
*       OUTPUT(2:1+LENGTH(MSG51))=MSG51;/* PRINT UNABLE TO DETERMINE    01277000
*                                      CPU ADDR MSG                  */ 01278000
@RF00676 L     @01,CURBUF(,COMBASE)                             02S0678 01279000
         MVC   OUTPUT+1(72,@01),MSG51                           02S0678 01280000
*       CALL PRNTSKIP;                                          02S0679 01281000
         BAL   @14,PRNTSKIP                                     02S0679 01282000
*SADMPOUT:                                                      02S0680 01283000
*       ;                           /* GET LCCA FROM PSA AT VIRT 0   */ 01284000
SADMPOUT DS    0H                                               02S0681 01285000
*     END;                                                      01S0681 01286000
*   ELSE                                                        01S0682 01287000
*     DO;                           /* NO - DSS OR SVCDUMP INPUT     */ 01288000
         B     @RC00637                                         01S0682 01289000
@RF00637 DS    0H                                               01S0683 01290000
*                                                               02S0683 01291000
*       /*************************************************************/ 01292000
*       /*                                                           */ 01293000
*       /* PRINT THE PSW AND REGISTERS USING THE IPLCPU ADDRESS IN   */ 01294000
*       /* COMMON FOR THE BRREAD                                     */ 01295000
*       /*                                                           */ 01296000
*       /*************************************************************/ 01297000
*                                                               02S0683 01298000
*       FPREGS=SWOFF;               /* RESET TO ALLOW PROCESSING OF     01299000
*                                      GPRS AND CTRS FIRST           */ 01300000
         NI    FPREGS,B'11011111'                               02S0683 01301000
*       RESPECIFY                                               02S0684 01302000
*         PREG RESTRICTED;                                      02S0684 01303000
*       PREG=IPLCPU;                /* PHYS CPU ADDR FOR BRREAD      */ 01304000
         SLR   PREG,PREG                                        02S0685 01305000
         ICM   PREG,3,IPLCPU(COMBASE)                           02S0685 01306000
*       GEN(BRREAD (PREG),CPU);                                 02S0686 01307000
         BRREAD (PREG),CPU                                              01308000
*       STATSPTR=PREG;              /* ADDR OF BEGINNING OF STATUS      01309000
*                                      RECORDS                       */ 01310000
         LR    STATSPTR,PREG                                    02S0687 01311000
*       RESPECIFY                                               02S0688 01312000
*         PREG UNRESTRICTED;                                    02S0688 01313000
*                                                               02S0688 01314000
*       /*************************************************************/ 01315000
*       /*                                                           */ 01316000
*       /* CONVERT THE PHYSICAL CPU ADDRESS FOR THE IPLCPU           */ 01317000
*       /*                                                           */ 01318000
*       /*************************************************************/ 01319000
*                                                               02S0689 01320000
*       UNPAKADR=IPLCPU;            /* PHYS CPU ADDR TO BE CONVERTED */ 01321000
         MVC   UNPAKADR(2),IPLCPU(COMBASE)                      02S0689 01322000
*       UNPK(UNPAKTO,UNPAKFRM);     /* UNPACK ADDR                   */ 01323000
         UNPK  UNPAKTO(5),UNPAKFRM(3)                           02S0690 01324000
*       TR(TRANSTO,EBCTABL);        /* TRANS TO PRINTABLE HEX        */ 01325000
         TR    TRANSTO(4),EBCTABL(COMBASE)                      02S0691 01326000
*       CNVTCPU=TRANSTO;            /* PUT ADDR IN HEADER            */ 01327000
         MVC   CNVTCPU(4),TRANSTO                               02S0692 01328000
*       OUTPUT(44:43+LENGTH(REGTITL1))=REGTITL1;/* PRINT HEADER      */ 01329000
         L     @03,CURBUF(,COMBASE)                             02S0693 01330000
         MVC   OUTPUT+43(21,@03),REGTITL1                       02S0693 01331000
*       OUTPUT(LENGTH(REGTITL1)+45:44+LENGTH(REGTITL1)+LENGTH(CPUHDR))= 01332000
*           CPUHDR;                                             02S0694 01333000
         MVC   OUTPUT+65(18,@03),CPUHDR                         02S0694 01334000
*       GEN(BRWRITE 3);                                         02S0695 01335000
*                                                               02S0695 01336000
         BRWRITE 3                                                      01337000
*       /*************************************************************/ 01338000
*       /*                                                           */ 01339000
*       /* FORMAT THE PSW AND REGISTERS                              */ 01340000
*       /*                                                           */ 01341000
*       /*************************************************************/ 01342000
*                                                               02S0696 01343000
*       CALL PSWFOMAT;              /* FORMAT THE PSW                */ 01344000
         BAL   @14,PSWFOMAT                                     02S0696 01345000
*       REGSW=SWOFF;                /* INDICATE GP REGS ARE TO BE       01346000
*                                      FORMATTED                     */ 01347000
         NI    REGSW,B'10111111'                                02S0697 01348000
*       CALL REGFOMAT;              /* FORMAT THE GP REGS            */ 01349000
         BAL   @14,REGFOMAT                                     02S0698 01350000
*       REGSW=SWON;                 /* INDICATE CT REGS ARE TO BE       01351000
*                                      FORMATTED                     */ 01352000
         OI    REGSW,B'01000000'                                02S0699 01353000
*       CALL REGFOMAT;              /* FORMAT THE CT REGS            */ 01354000
         BAL   @14,REGFOMAT                                     02S0700 01355000
*       FPREGS=SWON;                /* INDICATE TO FORMAT FP REGS    */ 01356000
         OI    FPREGS,B'00100000'                               02S0701 01357000
*       CALL REGFOMAT;              /* FORMAT THE FP REGS            */ 01358000
         BAL   @14,REGFOMAT                                     02S0702 01359000
*       CALL PRNTSKIP;              /* SKIP TWO LINES                */ 01360000
         BAL   @14,PRNTSKIP                                     02S0703 01361000
*     END;                                                      01S0704 01362000
*                                                               02S0705 01363000
*   /*****************************************************************/ 01364000
*   /*                                                               */ 01365000
*   /* THE TYPE OF INPUT IS FIRST DETERMINED. IF DSS OR SVCDUMP THE  */ 01366000
*   /* PSA AT VIRTUAL ADDRESS 0 IS VALIDITY CHECKED AND FORMATTED. IF*/ 01367000
*   /* SADMP INPUT, IT IS FIRST DETERMINED IF RDC IS PREFIXING       */ 01368000
*   /* BRREADS. IF PREFIXING IS NOT BEING DONE THE PSA AT LOCATION 0 */ 01369000
*   /* IS ACTUALLY THE UNPREFIXED 0-4K BLOCK, AND PROCESSING         */ 01370000
*   /* CONTINUES BY GETTING THE NEXT GROUP OF CONTROL BLOCKS TO BE   */ 01371000
*   /* FORMATTED. IF PREFIXING IS BEING DONE RDC WILL RETURN THE     */ 01372000
*   /* VIRTUAL ADDRESS OF THE CPU'S PSA, AND IT WILL BE VALIDITY     */ 01373000
*   /* CHECKED AND FORMATTED.                                        */ 01374000
*   /*                                                               */ 01375000
*   /*****************************************************************/ 01376000
*                                                               02S0705 01377000
*   IF QSADMP=SWON THEN             /* IS INPUT SADMP                */ 01378000
@RC00637 TM    QSADMP(COMBASE),B'00100000'                      02S0705 01379000
         BNO   @RF00705                                         02S0705 01380000
*     DO;                           /* YES                           */ 01381000
*       IF PREFXRGV=0 THEN          /* IS RDC PREFIXING              */ 01382000
         ICM   @15,15,PREFXRGV(COMBASE)                         02S0707 01383000
         BNZ   @RF00707                                         02S0707 01384000
*         DO;                       /* NO                            */ 01385000
*           OUTPUT(2:1+LENGTH(MSG45))=MSG45;/* PRINT PREFIX VALUE       01386000
*                                      UNAVAILABLE MSG               */ 01387000
         L     @01,CURBUF(,COMBASE)                             02S0709 01388000
         MVC   OUTPUT+1(66,@01),MSG45                           02S0709 01389000
*           CALL PRNTSKIP;                                      02S0710 01390000
         BAL   @14,PRNTSKIP                                     02S0710 01391000
*           IF LCASW=SWON&PCASW=SWON THEN/* IS ANY MORE PROCESSING      01392000
*                                      POSSIBLE                      */ 01393000
         TM    LCASW,B'01100000'                                02S0711 01394000
         BNO   @RF00711                                         02S0711 01395000
*             DO;                   /* NO                            */ 01396000
*               CALL CSTAUSPT;                                  02S0713 01397000
         BAL   @14,CSTAUSPT                                     02S0713 01398000
*               GEN(BRWRITE SKIP);                              02S0714 01399000
         BRWRITE SKIP                                                   01400000
*               OUTPUT(45:44+LENGTH(UNPRHDR))=UNPRHDR;/* PRINT HEADER*/ 01401000
         L     @01,CURBUF(,COMBASE)                             02S0715 01402000
         MVC   OUTPUT+44(35,@01),UNPRHDR                        02S0715 01403000
*               GEN(BRWRITE 3);                                 02S0716 01404000
         BRWRITE 3                                                      01405000
*             END;                                              02S0717 01406000
*           ELSE                                                02S0718 01407000
*             GO TO NXTGROUP;       /* YES - GET NEXT GROUP          */ 01408000
*         END;                                                  02S0719 01409000
*     END;                                                      02S0720 01410000
@RF00707 DS    0H                                               02S0721 01411000
*   BRNCHADR=ADDR(GMSG41);          /* ERROR RETURN POINT FOR THIS      01412000
*                                      BRREAD                        */ 01413000
@RF00705 LA    @15,GMSG41                                       02S0721 01414000
         ST    @15,BRNCHADR                                     02S0721 01415000
*   RESPECIFY                                                   02S0722 01416000
*     PREG RESTRICTED;                                          02S0722 01417000
*   PREG=PSAVIRT;                   /* VIRT ADDR OF PSA IN DUMP      */ 01418000
         L     PREG,PSAVIRT                                     02S0723 01419000
*   GEN(BRREAD (PREG));                                         02S0724 01420000
         BRREAD (PREG)                                                  01421000
*   STGADPSA=PREG;                  /* ADDR OF PSA IN STORAGE - FOR     01422000
*                                      FMTPTRNS                      */ 01423000
         ST    PREG,STGADPSA                                    02S0725 01424000
*   PREG=PREG+(ADDR(PSAPSA)-ADDR(PSA));/* DISPLACEMENT IN PSA OF        01425000
*                                      EBCDIC NAME 'PSA'             */ 01426000
         AH    PREG,@CH03000                                    02S0726 01427000
*   IF VALIDCK^='PSA ' THEN         /* DOES THE PSA VALIDITY CHECK   */ 01428000
         CLC   VALIDCK(4,PREG),@CC02879                         02S0727 01429000
         BE    @RF00727                                         02S0727 01430000
*     DO;                           /* NO                            */ 01431000
*       RESPECIFY                                               02S0729 01432000
*         PREG UNRESTRICTED;                                    02S0729 01433000
*       OUTPUT(2:1+LENGTH(MSG43))=MSG43;/* PRINT VALIDITY CKECK 02S0730 01434000
*                                      FAILED MSG                    */ 01435000
         L     @01,CURBUF(,COMBASE)                             02S0730 01436000
         MVC   OUTPUT+1(25,@01),MSG43                           02S0730 01437000
*       CALL PRNTSKIP;                                          02S0731 01438000
         BAL   @14,PRNTSKIP                                     02S0731 01439000
*     END;                                                      02S0732 01440000
*   CALL PSAFORMT;                  /* FORMAT THE PSA ONE LINE AT A     01441000
*                                      TIME                          */ 01442000
@RF00727 BAL   @14,PSAFORMT                                     02S0733 01443000
*   GO TO NXTGROUP;                 /* CHECK FOR MORE CCAS AND PSAS     01444000
*                                      TO PROCESS                    */ 01445000
*                                                               02S0734 01446000
         B     NXTGROUP                                         02S0734 01447000
*   /*****************************************************************/ 01448000
*   /*                                                               */ 01449000
*   /* THIS ROUTINE WILL GET CONTROL ONLY IF THE BRREAD FAILS WHILE  */ 01450000
*   /* GETTING THE PSA                                               */ 01451000
*   /*                                                               */ 01452000
*   /*****************************************************************/ 01453000
*                                                               02S0735 01454000
*GMSG41:                                                        02S0735 01455000
*                                                               02S0735 01456000
*   /*****************************************************************/ 01457000
*   /*                                                               */ 01458000
*   /* CONVERT THE VIRTUAL PSA ADDRESS TO PRINTABLE HEX FOR MESSAGE  */ 01459000
*   /*                                                               */ 01460000
*   /*****************************************************************/ 01461000
*                                                               02S0735 01462000
*   RESPECIFY                                                   02S0735 01463000
*    (R1,                                                       02S0735 01464000
*     R4) RESTRICTED;                                           02S0735 01465000
GMSG41   DS    0H                                               02S0736 01466000
*   R1=ADDR(PSAVIRT);               /* ADDR TO CONVERT               */ 01467000
         LA    R1,PSAVIRT                                       02S0736 01468000
*   R4=ADDR(ADDR41);                /* ADDR OF PUTPUT LOCATION FOR      01469000
*                                      CNVT RTN                      */ 01470000
         LA    R4,ADDR41                                        02S0737 01471000
*   CALL ADRCNVT;                   /* AADRCNVT RTN                  */ 01472000
         L     @15,AADRCNVT(,COMBASE)                           02S0738 01473000
         BALR  @14,@15                                          02S0738 01474000
*   RESPECIFY                                                   02S0739 01475000
*    (R1,                                                       02S0739 01476000
*     R4) UNRESTRICTED;                                         02S0739 01477000
*   OUTPUT(2:1+LENGTH(MSG41))=MSG41;/* PRINT ERROR FINDING PSA MSG   */ 01478000
         L     @01,CURBUF(,COMBASE)                             02S0740 01479000
         MVC   OUTPUT+1(52,@01),MSG41                           02S0740 01480000
*   CALL PRNTSKIP;                                              02S0741 01481000
         BAL   @14,PRNTSKIP                                     02S0741 01482000
*NXTGROUP:                                                      02S0742 01483000
*   ;                                                           02S0742 01484000
NXTGROUP DS    0H                                               02S0743 01485000
*                                                               02S0743 01486000
*   /*****************************************************************/ 01487000
*   /*                                                               */ 01488000
*   /* DETERMINE IF THERE ARE ANY MORE PSAS TO BE PROCESSED. IF A    */ 01489000
*   /* UNI-PROCESSOR ALL REMAINING POINTERS SHOULD BE ZERO AND       */ 01490000
*   /* PROCESSING WILL TERMINATE.                                    */ 01491000
*   /*                                                               */ 01492000
*   /*****************************************************************/ 01493000
*                                                               02S0743 01494000
*   IF LCASW=SWOFF THEN             /* IS THE LCCAVT AVAILABLE       */ 01495000
         TM    LCASW,B'01000000'                                02S0743 01496000
         BNZ   @RF00743                                         02S0743 01497000
*     DO;                           /* YES                           */ 01498000
*       TABLPTR=LTABLPTR;           /* ADDR OF INTERNAL LCCAVT       */ 01499000
         LR    TABLPTR,LTABLPTR                                 02S0745 01500000
*       DO J=0 TO NUMENTRY BY LENGTH(CCAENTRY);/* SEARCH THE LCCAVT     01501000
*                                      FOR NON- ZERO ADDRS           */ 01502000
         SLR   @04,@04                                          02S0746 01503000
         STC   @04,J                                            02S0746 01504000
@DL00746 DS    0H                                               02S0747 01505000
*         IF ENTRY1^=0 THEN         /* IS THIS LCCA PTR NOT 0        */ 01506000
         ICM   @04,7,ENTRY1(TABLPTR)                            02S0747 01507000
         BZ    @RF00747                                         02S0747 01508000
*           DO;                     /* YES - CHECK FOR BRREAD ERROR  */ 01509000
*             GEN(BRWRITE SKIP);    /* SKIP TO A NEW PAGE            */ 01510000
         BRWRITE SKIP                                                   01511000
*             IF ENTRY1='FFFFFF'X THEN/* WAS THIS ENTRY READ    02S0750 01512000
*                                      SUCCESSFULLY                  */ 01513000
         CLC   ENTRY1(3,TABLPTR),@CF02883+1                     02S0750 01514000
         BNE   @RF00750                                         02S0750 01515000
*               DO;                 /* NO                            */ 01516000
*                 IF PCASW=SWOFF THEN/* IS THE PCCAVT AVAILABLE      */ 01517000
         TM    PCASW,B'00100000'                                02S0752 01518000
         BNZ   @RF00752                                         02S0752 01519000
*                   DO;             /* YES                           */ 01520000
*                     LCCAPTR='00FFFFFF'X;/* INDICATE READ FAILED FOR   01521000
*                                      LCCA                          */ 01522000
         MVC   LCCAPTR(4),@CF02884                              02S0754 01523000
*                     CALL PCAMATCH;/* ATTEMPT TO GET THE PCCA FOR      01524000
*                                      THIS CPU                      */ 01525000
         BAL   @14,PCAMATCH                                     02S0755 01526000
*                   END;                                        02S0756 01527000
*                 ELSE                                          02S0757 01528000
*                   DO;             /* NO - PCCA IS UNAVAILABLE      */ 01529000
         B     @RC00752                                         02S0757 01530000
@RF00752 DS    0H                                               02S0758 01531000
*                     OUTPUT(2:1+LENGTH(MSG55))=MSG55;/* PRINT UNABLE   01532000
*                                      TO FORMAT LCCA,PCCA AND PSA      01533000
*                                      MSG                           */ 01534000
         L     @04,CURBUF(,COMBASE)                             02S0758 01535000
         MVC   OUTPUT+1(36,@04),MSG55                           02S0758 01536000
*                     CALL PRNTSKIP;                            02S0759 01537000
         BAL   @14,PRNTSKIP                                     02S0759 01538000
*                   END;                                        02S0760 01539000
*               END;                                            02S0761 01540000
*             ELSE                                              02S0762 01541000
*               DO;                 /* YES - ATTEMPT TO GET LCCA     */ 01542000
         B     @RC00750                                         02S0762 01543000
@RF00750 DS    0H                                               02S0763 01544000
*                 LCCAPTR=CCAENTRY; /* ADDR OF THIS LCCA             */ 01545000
         L     @04,CCAENTRY(,TABLPTR)                           02S0763 01546000
         ST    @04,LCCAPTR                                      02S0763 01547000
*                                                               03S0764 01548000
*                 /***************************************************/ 01549000
*                 /*                                                 */ 01550000
*                 /* SEARCH BOTH CCAVTS BEGINNING WITH THE LCCA FOR  */ 01551000
*                 /* CPU 0, FOR ALL NON -ZERO CCA POINTERS, AND      */ 01552000
*                 /* FORMAT THEM - IE. - GET THE LCCA POINTER FOR CPU*/ 01553000
*                 /* 0 AND PROCESS IT. THEN GET THE PCCA PTR FOR CPU */ 01554000
*                 /* 0 AND PROCESS IT. ALSO, FOR EACH PCCA, PROCESS  */ 01555000
*                 /* THE PSA FOR THAT CPU. IF SADMP INPUT, ALSO      */ 01556000
*                 /* PROCESS THE STORE-STATUS DATA.                  */ 01557000
*                 /*                                                 */ 01558000
*                 /***************************************************/ 01559000
*                                                               03S0764 01560000
*                 BRNCHADR=ADDR(GMSG20C);/* ERROR RETURN POINT FOR      01561000
*                                      THIS BRREAD                   */ 01562000
         LA    @15,GMSG20C                                      03S0764 01563000
         ST    @15,BRNCHADR                                     03S0764 01564000
*                 RESPECIFY                                     03S0765 01565000
*                   PREG RESTRICTED;                            03S0765 01566000
*                 PREG=LCCAPTR;     /* ADDR OF THE LCCA              */ 01567000
         LR    PREG,@04                                         03S0766 01568000
*                 GEN(BRREAD (PREG));                           03S0767 01569000
         BRREAD (PREG)                                                  01570000
*                 IF VALIDCK^='LCCA' THEN/* DOES THE LCCA VALIDITY      01571000
*                                      CHECK                         */ 01572000
         CLC   VALIDCK(4,PREG),@CC02838                         03S0768 01573000
         BE    @RF00768                                         03S0768 01574000
*                   DO;             /* NO                            */ 01575000
*                     RESPECIFY                                 03S0770 01576000
*                       PREG UNRESTRICTED;                      03S0770 01577000
*                     OUTPUT(2:1+LENGTH(MSG21))=MSG21;/* PRINT  03S0771 01578000
*                                      VALIDITY CHECK FAILED MSG     */ 01579000
         L     @04,CURBUF(,COMBASE)                             03S0771 01580000
         MVC   OUTPUT+1(26,@04),MSG21                           03S0771 01581000
*                     CALL PRNTSKIP;                            03S0772 01582000
         BAL   @14,PRNTSKIP                                     03S0772 01583000
*                   END;                                        03S0773 01584000
*                 BRNCHADR=ADDR(GMSG22A);/* ERROR RETURN POINT FOR      01585000
*                                      BRREADS BY GFR                */ 01586000
@RF00768 LA    @04,GMSG22A                                      03S0774 01587000
         ST    @04,BRNCHADR                                     03S0774 01588000
*                 RESPECIFY                                     03S0775 01589000
*                  (R0,                                         03S0775 01590000
*                   R1) RESTRICTED;                             03S0775 01591000
*                 R0=0;             /* 0 ASID FOR RDC                */ 01592000
         SLR   R0,R0                                            03S0776 01593000
*                 DO I=1 TO DIM(FMTTABL2);/* CALL THE FORMAT RTN TO     01594000
*                                      FORMAT THE LCCA ONE LINE AT A    01595000
*                                      TIME                          */ 01596000
         LA    I,1                                              03S0777 01597000
@DL00777 DS    0H                                               03S0778 01598000
*                   R1=FMTTABL2(I); /* ADDR OF EACH LINE TO BE  03S0778 01599000
*                                      FORMATTED                     */ 01600000
         LR    @15,I                                            03S0778 01601000
         SLA   @15,2                                            03S0778 01602000
