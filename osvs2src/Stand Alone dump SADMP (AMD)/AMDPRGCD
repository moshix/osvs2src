         STC   @04,J                                            03S0536 00811000
@DL00536 DS    0H                                               03S0537 00812000
*             IF ENTRY1^=0 THEN     /* IS THIS PCCA ADDR NOT 0       */ 00813000
         ICM   @04,7,ENTRY1(TABLPTR)                            03S0537 00814000
         BZ    @RF00537                                         03S0537 00815000
*               DO;                 /* YES                           */ 00816000
*                 IF PCCAPTR1^=0 THEN/* WAS PCCA ADDR IN PSA NOT 0   */ 00817000
         ICM   @04,7,PCCAPTR1                                   03S0539 00818000
         BZ    @RF00539                                         03S0539 00819000
*                   DO;             /* YES                           */ 00820000
*                     IF ENTRY1=PCCAPTR1 THEN/* IS THIS THE PCCA FOR    00821000
*                                      THIS PSA                      */ 00822000
         CLC   ENTRY1(3,TABLPTR),PCCAPTR1                       03S0541 00823000
         BE    @RT00541                                         03S0541 00824000
*                       GO TO PPTRESET;/* YES - RESET PTR IN TABLE      00825000
*                                      AND PROCESS PCCA              */ 00826000
*                   END;                                        03S0543 00827000
*                 ELSE                                          03S0544 00828000
*                   DO;             /* THIS PCCA ADDR IS NOT 0       */ 00829000
         B     @RC00539                                         03S0544 00830000
@RF00539 DS    0H                                               03S0545 00831000
*                     RESPECIFY                                 03S0545 00832000
*                       PREG RESTRICTED;                        03S0545 00833000
*                     PREG=ADDR(PCCACPUA);/* PHYSICAL CPU ADDR       */ 00834000
         L     PREG,PCCAPTR                                     03S0546 00835000
         LA    PREG,PCCACPUA(,PREG)                             03S0546 00836000
*                     GEN(BRREAD (PREG));                       03S0547 00837000
         BRREAD (PREG)                                                  00838000
*                     IF CPUADDR=CPUTMPRY THEN/* DO THE PHYS CPU        00839000
*                                      ADDRS MATCH                   */ 00840000
         SLR   @04,@04                                          03S0548 00841000
         ICM   @04,3,CPUADDR                                    03S0548 00842000
         CH    @04,CPUTMPRY(,PREG)                              03S0548 00843000
         BNE   @RF00548                                         03S0548 00844000
*                       DO;         /* YES - THIS IS THE PCCA FOR       00845000
*                                      THIS PSA                      */ 00846000
*                         PCCAPTR=CCAENTRY;/* PROCESS THIS PCCA      */ 00847000
         MVC   PCCAPTR(4),CCAENTRY(TABLPTR)                     03S0550 00848000
*PPTRESET:                                                      03S0551 00849000
*                         CCAENTRY=0;/* ZERO ADDR TO PREVENT    03S0551 00850000
*                                      REFORMATTING IT LATER         */ 00851000
PPTRESET SLR   @04,@04                                          03S0551 00852000
         ST    @04,CCAENTRY(,TABLPTR)                           03S0551 00853000
*                         GO TO PCAVALID;/* PROCESS THIS PCCA        */ 00854000
         B     PCAVALID                                         03S0552 00855000
*                       END;                                    03S0553 00856000
*                     RESPECIFY                                 03S0554 00857000
*                       PREG UNRESTRICTED;                      03S0554 00858000
@RF00548 DS    0H                                               03S0555 00859000
*                   END;                                        03S0555 00860000
*               END;                                            03S0556 00861000
@RC00539 DS    0H                                               03S0557 00862000
*PTABSCH1:                                                      03S0557 00863000
*             TABLPTR=TABLPTR+LENGTH(CCAENTRY);/* INCREMENT PTR TO      00864000
*                                      REFERENCE NEXT ENTRY IN PCCAVT*/ 00865000
@RF00537 DS    0H                                               03S0557 00866000
PTABSCH1 AH    TABLPTR,@CH00039                                 03S0557 00867000
*           END;                                                03S0558 00868000
         LA    @04,4                                            03S0558 00869000
         SLR   @15,@15                                          03S0558 00870000
         IC    @15,J                                            03S0558 00871000
         ALR   @04,@15                                          03S0558 00872000
         STC   @04,J                                            03S0558 00873000
         CH    @04,@CH02093                                     03S0558 00874000
         BNH   @DL00536                                         03S0558 00875000
*           GO TO PCCADRCK;         /* IS THERE A PCCA TO FORMAT     */ 00876000
*                                                               03S0559 00877000
         B     PCCADRCK                                         03S0559 00878000
*           /*********************************************************/ 00879000
*           /*                                                       */ 00880000
*           /* THIS ROUTINE WILL GET CONTROL ONLY IF THE BRREAD FAILS*/ 00881000
*           /* WHILE GETTING THE PHYSICAL CPU ADDRESS FROM THE PSA   */ 00882000
*           /*                                                       */ 00883000
*           /*********************************************************/ 00884000
*                                                               03S0560 00885000
*PCPUFAIL:                                                      03S0560 00886000
*           CPUADDR='FFFF'X;        /* PREVENT MATCHING THE PHYS CPU    00887000
*                                      ADDR IN PCCA                  */ 00888000
PCPUFAIL MVC   CPUADDR(2),@CH02834                              03S0560 00889000
*           IF PCCAPTR^=0 THEN      /* IS PCCA AVAILABLE             */ 00890000
         ICM   @15,15,PCCAPTR                                   03S0561 00891000
         BNZ   @RT00561                                         03S0561 00892000
*             GO TO PTABSRCH;       /* YES - FIND THE MATCHING PCCA     00893000
*                                      IN PCCAVT                     */ 00894000
*           ELSE                                                03S0563 00895000
*             GO TO SADSWCK3;       /* NO - PCCA IS UNAVAILABLE      */ 00896000
         B     SADSWCK3                                         03S0563 00897000
*PCCADRCK:                                                      02S0564 00898000
*           IF PCCAPTR1^=0 THEN     /* IS THERE A PCCA TO PROCESS    */ 00899000
PCCADRCK ICM   @15,7,PCCAPTR1                                   02S0564 00900000
         BZ    @RF00564                                         02S0564 00901000
*             DO;                   /* YES                           */ 00902000
*                                                               02S0565 00903000
*               /*****************************************************/ 00904000
*               /*                                                   */ 00905000
*               /* VALIDITY CHECK AND ATTEMPT TO FORMAT THIS PCCA    */ 00906000
*               /*                                                   */ 00907000
*               /*****************************************************/ 00908000
*                                                               02S0566 00909000
*PCAVALID:                                                      02S0566 00910000
*               BRNCHADR=ADDR(GMSG30A);/* ERROR RETURN POINT FOR THIS   00911000
*                                      BRREAD                        */ 00912000
PCAVALID LA    @15,GMSG30A                                      02S0566 00913000
         ST    @15,BRNCHADR                                     02S0566 00914000
*               RESPECIFY                                       02S0567 00915000
*                 PREG RESTRICTED;                              02S0567 00916000
*               PREG=PCCAPTR;       /* ADDR OF THE PCCA              */ 00917000
