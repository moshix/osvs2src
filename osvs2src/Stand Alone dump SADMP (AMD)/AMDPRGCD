         CR    CCACNT0,@06                                      02S0979 02287000
         BE    @RT00979                                         02S0979 02288000
         LR    @05,CCACNT0                                      02S0979 02289000
         ALR   @05,CCACNTRD                                     02S0979 02290000
         CR    @05,@06                                          02S0979 02291000
         BNE   @RF00979                                         02S0979 02292000
@RT00979 DS    0H                                               02S0980 02293000
*     DO;                           /* YES                           */ 02294000
*       IF CCAVTSW=SWOFF THEN       /* IS LCCAVT BEING PROCESSED     */ 02295000
         TM    CCAVTSW,B'10000000'                              02S0981 02296000
         BNZ   @RF00981                                         02S0981 02297000
*         DO;                       /* YES                           */ 02298000
*           LCASW=SWON;             /* INDICATE LCCAVT IS NOT USEFUL */ 02299000
         OI    LCASW,B'01000000'                                02S0983 02300000
*           OUTPUT(2:1+LENGTH(MSG11))=MSG11;/* PRINT ALL LCCA PTRS      02301000
*                                      ARE UNAVAILABLE MSG           */ 02302000
         L     @06,CURBUF(,COMBASE)                             02S0984 02303000
         MVC   OUTPUT+1(46,@06),MSG11                           02S0984 02304000
*         END;                                                  02S0985 02305000
*       ELSE                                                    02S0986 02306000
*         DO;                       /* NO - PCCAVT IS BEING PROCESSED*/ 02307000
         B     @RC00981                                         02S0986 02308000
