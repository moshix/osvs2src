         TITLE 'AMDSAAID ASID SEARCH ROUTINE                           *00001000
                       '                                                00002000
AMDSAAID CSECT ,                                                   0001 00003000
@MAINENT DS    0H                                                  0001 00004000
         USING *,@15                                               0001 00005000
         B     @PROLOG                                             0001 00006000
         DC    AL1(16)                                             0001 00007000
         DC    C'AMDSAAID  73.230'                                 0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                     0001 00010000
         BALR  @11,0                                               0001 00011000
@PSTART  DS    0H                                                  0001 00012000
         USING @PSTART,@11                                         0001 00013000
         ST    @13,@SA00001+4                                      0001 00014000
         LA    @14,@SA00001                                        0001 00015000
         ST    @14,8(,@13)                                         0001 00016000
         LR    @13,@14                                             0001 00017000
*   SAVEFORM(1:4) = 'LAID';         /* PLACE THE MODULE IDENTIFIER IN   00018000
*                                      THE FIRST WORD OF THE SAVE  0193 00019000
*                                      AREA. THIS IS A DEBUGGING AID */ 00020000
         MVC   SAVEFORM(4,R13),@CC01420                            0193 00021000
*   RESPECIFY RCB BASED (ADDR(AIDRCB));                            0194 00022000
*   RCBNEXT = VCTRCB;               /* SAVE BACKWARD RCB CHAIN     0195 00023000
*                                      POINTER                       */ 00024000
         LA    @10,AIDRCB                                          0195 00025000
         MVC   RCBNEXT(4,@10),VCTRCB(VCCTREG)                      0195 00026000
*   GEN REFS(R1) SETS (AIDSTOR) (STCTL R1,R1,AIDSTOR); /* SAVE     0196 00027000
*                                      SEGMENT TABLE ORGIN FOR THE 0196 00028000
*                                      RCB EXIT ROUTINE              */ 00029000
         STCTL R1,R1,AIDSTOR                                            00030000
*   EXITADDR = ADDR(NEXTASID);      /* ERROR EXIT ADDR               */ 00031000
         LA    @10,NEXTASID                                        0197 00032000
         ST    @10,EXITADDR                                        0197 00033000
*   RCBSAVE = R13;                  /* AID'S SAVE AREA POINTER       */ 00034000
         LA    @10,AIDRCB                                          0198 00035000
         ST    R13,RCBSAVE(,@10)                                   0198 00036000
*   RCBBASE = BASEREG;              /* BASE REGISTER OF AID          */ 00037000
         ST    BASEREG,RCBBASE(,@10)                               0199 00038000
*   VCTRCB = ADDR(AIDRCB);          /* PLACE AID RCB ON THE VCCT   0200 00039000
*                                      CHAIN                         */ 00040000
         ST    @10,VCTRCB(,VCCTREG)                                0200 00041000
*   ASCBPTR = 0;                    /* SET UP INITIAL ASCB POINTER   */ 00042000
         SR    @10,@10                                             0201 00043000
         LR    ASCBPTR,@10                                         0201 00044000
*   AIDASVTA = 0;                   /* INITIALIZE INITIAL ASVT     0202 00045000
*                                      ADDRESS                       */ 00046000
         LR    AIDASVTA,@10                                        0202 00047000
*   AIDMAX = 0;                     /* SET UP MAXIMUM COUNTER        */ 00048000
         LR    AIDMAX,@10                                          0203 00049000
*   CTBUFTYP = OFF;                 /* TURN OFF BUFFER SWITCH        */ 00050000
         L     @10,VCTCCTV(,VCCTREG)                               0204 00051000
         MVI   CTBUFTYP(@10),B'00000000'                           0204 00052000
*   CTLSQA = ON;                    /* TURN ON LSQA SWITCH           */ 00053000
         OI    CTLSQA(@10),B'00001000'                             0205 00054000
*                                                                  0206 00055000
*   /*****************************************************************/ 00056000
*   /*                                                               */ 00057000
*   /* OBTAIN THE ADDRESS OF THE ASVT AND VALIDITY CHECK THE ASVT. IF*/ 00058000
*   /* THE ASVT IS VALID, THE ASCB'S WILL BE OBTAINED FROM THE ASVT. */ 00059000
*   /* IF THE ASVT IS INVALID, AN ALTERNATE SEARCH USING THE         */ 00060000
*   /* DISPATCHABILITY QUEUE WILL BE UTILIZED                        */ 00061000
*   /*                                                               */ 00062000
*   /*****************************************************************/ 00063000
*                                                                  0206 00064000
*/* P OBTAIN ASVT ADDRESS FROM CVTASVT                               */ 00065000
*/* L AMDSAVCK: SVC 25 VALIDITY CHECK THE ASVT                       */ 00066000
*/* D (YES,,NO,ALTASCB) ASVT VALID ?                                 */ 00067000
*/* P AIDSVTA POINTS TO FIRST ASVT ENTRY                             */ 00068000
*/*ENTYQ: P (,VCKASCB) VCTASCB = AIDASCBA                            */ 00069000
*/*ALTASCB: P AIDALTSW = 1                                           */ 00070000
*/* P USE CVTASCBH QUEUE TO OBTAIN ASCB'S                            */ 00071000
*/* P ASVTMAXU = X'7FFF'                                             */ 00072000
*/* P (,VCKASCB) VCTASCB = CVTASCBH                                  */ 00073000
*   ASVTPTR = CVTASVT;              /* SET UP POINTER TO ASVT FROM 0206 00074000
*                                      THE CVT                       */ 00075000
         L     @10,CVTPTR                                          0206 00076000
         L     @10,CVTASVT(,@10)                                   0206 00077000
         LR    ASVTPTR,@10                                         0206 00078000
*   VCTCBADR = CVTASVT;             /* SET UP POINTER TO ASVT SO IT     00079000
*                                      CAN BE VALIDITY CHECKED       */ 00080000
         ST    @10,VCTCBADR(,VCCTREG)                              0207 00081000
*   VCTCBIDN = VASVTID;             /* SET UP ASVT ID FOR VALIDITY 0208 00082000
*                                      CHECK                         */ 00083000
         MVI   VCTCBIDN(VCCTREG),X'04'                             0208 00084000
*   SVC (VCKSVC);                   /* VALIDITY CHECK THE ASVT       */ 00085000
         SVC   25                                                  0209 00086000
*   RESPECIFY RETCODE RESTRICTED;                                  0210 00087000
*   IF RETCODE = 0 THEN                                            0211 00088000
         SR    @14,@14                                             0211 00089000
         CR    RETCODE,@14                                         0211 00090000
         BNE   @RF00211                                            0211 00091000
*     DO;                           /* IF VALID THEN DO THE FOLLOWING*/ 00092000
*       RESPECIFY RETCODE UNRESTRICTED;                            0213 00093000
*       AIDASVTA = ADDR(ASVTENTY(1)); /* POINT TO FIRST ENTRY        */ 00094000
         LA    AIDASVTA,ASVTENTY(,ASVTPTR)                         0214 00095000
*       AIDMAX = ASVTMAXU;          /* SET UP LIMITS ON ASCB'S DUMPED*/ 00096000
         L     AIDMAX,ASVTMAXU(,ASVTPTR)                           0215 00097000
*       IF AIDMAX=0 THEN                                           0216 00098000
         CR    AIDMAX,@14                                          0216 00099000
         BNE   @RF00216                                            0216 00100000
*         AIDMAX=DFLTASCB;          /* SET TO MAX IF 0               */ 00101000
*                                                                  0217 00102000
         LH    AIDMAX,@CH01416                                     0217 00103000
*ENTYQ:                                                            0218 00104000
*       VCTASCB = AIDASCBA;         /* POINT TO CURRENT ASCB ENTRY   */ 00105000
@RF00216 DS    0H                                                  0218 00106000
ENTYQ    MVC   VCTASCB(4,VCCTREG),AIDASCBA(AIDASVTA)               0218 00107000
*     END;                                                         0219 00108000
*   ELSE                                                           0220 00109000
*     DO;                                                          0220 00110000
         B     @RC00211                                            0220 00111000
@RF00211 DS    0H                                                  0221 00112000
*       AIDALTSW = ON;              /* USE DISPATCHABILITY QUEUE     */ 00113000
         OI    AIDALTSW,B'10000000'                                0221 00114000
*       AIDMAX = DFLTASCB;          /* LIMITS THE ASCB SEARCHING     */ 00115000
         LH    AIDMAX,@CH01416                                     0222 00116000
*       VCTASCB = CVTASCBH;         /* SET TO TOP ASCB ON DISPATCHER    00117000
*                                      CHAIN                         */ 00118000
         L     @10,CVTPTR                                          0223 00119000
         MVC   VCTASCB(4,VCCTREG),CVTASCBH(@10)                    0223 00120000
*     END;                                                         0224 00121000
*                                                                  0225 00122000
*   /*****************************************************************/ 00123000
*   /*                                                               */ 00124000
*   /* VALIDITY CHECK THE ASCB. IF IT IS INVALID, TRY AN ALTERNATE   */ 00125000
*   /* SEARCH PATH (IF AVAILABLE)                                    */ 00126000
*   /*                                                               */ 00127000
*   /*****************************************************************/ 00128000
*                                                                  0225 00129000
*/*VCKASCB: L AMDSAVCK: SVC 25 TO VALIDITY CHECK ASCB                */ 00130000
*/* D (YES,%AIDS,NO,) ASCB VALID ?                                   */ 00131000
*/* D (YES,NEXTASID,NO,) AIDALTSW = OFF ?                            */ 00132000
*/* D (YES,DEQRCB,NO,) AIDBACKS = ON ?                               */ 00133000
*/* P AIDBACKS = ON                                                  */ 00134000
*/* P OBTAIN CVT LOWER ASCB CHAIN                                    */ 00135000
*/* D (YES,DEQRCB,NO,) VCTASCB = 0                                   */ 00136000
*/* D (YES,DEQRCB,NO,VCKASCB) VCTASCB = AIDASCBL ?                   */ 00137000
*                                                                  0225 00138000
*VCKASCB:                                                          0225 00139000
*   VCTCBADR = VCTASCB;             /* PLACE ASCB ADDRESS INTO THE 0225 00140000
*                                      VALIDITY CHECK INPUT PARAMETER   00141000
*                                      DATA                          */ 00142000
@RC00211 DS    0H                                                  0225 00143000
VCKASCB  MVC   VCTCBADR(4,VCCTREG),VCTASCB(VCCTREG)                0225 00144000
*   VCTCBIDN = VASCBID;             /* ASCB IDENTIFIER FOR VALIDITY     00145000
*                                      CHECK                         */ 00146000
         MVI   VCTCBIDN(VCCTREG),X'02'                             0226 00147000
*   SVC (VCKSVC);                   /* VALIDITY CHECK THE ASCB       */ 00148000
         SVC   25                                                  0227 00149000
*   RESPECIFY RETCODE RESTRICTED;                                  0228 00150000
*   IF RETCODE ^= 0 THEN                                           0229 00151000
