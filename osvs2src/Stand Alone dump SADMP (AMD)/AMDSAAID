*/* D (YES,DEQRCB,NO,) AIDASCBA = 0 ?                                */ 00383000
*/* D (YES,NEXTASID,NO,ENTYQ) ASVTAVAI = OFF ?                       */ 00384000
*                                                                  0280 00385000
*NEXTASID:                                                         0280 00386000
*   AIDMAX = AIDMAX - 1;            /* FINISHED WITH PREVIOUS LOCAL     00387000
*                                      MEMORY                        */ 00388000
NEXTASID BCTR  AIDMAX,0                                            0280 00389000
*   IF AIDMAX <= 0 THEN                                            0281 00390000
         SR    @10,@10                                             0281 00391000
         CR    AIDMAX,@10                                          0281 00392000
         BNH   @RT00281                                            0281 00393000
*     GO TO DEQRCB;                 /* IF ALL MEMORIES HAVE BEEN   0282 00394000
*                                      COMPLETED--EXIT               */ 00395000
*   IF AIDALTSW = OFF THEN                                         0283 00396000
         TM    AIDALTSW,B'10000000'                                0283 00397000
         BNZ   @RF00283                                            0283 00398000
*     DO;                           /* PRIMARY ASVT CHAIN SEARCH     */ 00399000
*       AIDASVTA = AIDASVTA + LENGTH(ASVTENTY); /* BUMP TO NEXT ASVT    00400000
*                                      ENTRY                         */ 00401000
         AH    AIDASVTA,@CH00038                                   0285 00402000
*       IF AIDASCBA = 0 THEN                                       0286 00403000
         C     @10,AIDASCBA(,AIDASVTA)                             0286 00404000
         BE    @RT00286                                            0286 00405000
*         GO TO DEQRCB;             /* IF AT END OF CHAIN- WE HAVE 0287 00406000
*                                      COMPLETED PROCESSING          */ 00407000
*       IF AIDAVAL = OFF THEN                                      0288 00408000
         TM    AIDAVAL(AIDASVTA),B'10000000'                       0288 00409000
         BZ    @RT00288                                            0288 00410000
*         GO TO ENTYQ;              /* IF THE ASID IS ACTIVE, DUMP IT*/ 00411000
*       GO TO NEXTASID;             /* SKIP TO NEXT MEMORY           */ 00412000
         B     NEXTASID                                            0290 00413000
*     END;                                                         0291 00414000
*                                                                  0292 00415000
*   /*****************************************************************/ 00416000
*   /*                                                               */ 00417000
*   /* USING FORWARD OR BACKWARD CHAIN SEARCH. GET THE NEXT ASCB FROM*/ 00418000
*   /* THE APPROPRIATE ASCB QUQUE                                    */ 00419000
*   /*                                                               */ 00420000
*   /*****************************************************************/ 00421000
*                                                                  0292 00422000
*/*ALTRCB: D (YES,,NO,%QCK) AIDBACKS = OFF                           */ 00423000
*/* D (YES,DEQRCB,NO,) ASCBFWDP = 0 ?                                */ 00424000
*/* P (,VCKASCB) VCTASCB = ASCBFWDP                                  */ 00425000
*/*%QCK: P VCTASCB = ASCBBWDP                                        */ 00426000
*/* D (YES,VCKASCB,NO,) VCTASCB ^= AIDASCBL                          */ 00427000
*/* P (,VCKASCB) VCTASCB = ASCBFWDP                                  */ 00428000
*   IF AIDBACKS = OFF THEN                                         0292 00429000
@RF00283 TM    AIDBACKS,B'01000000'                                0292 00430000
         BNZ   @RF00292                                            0292 00431000
*     DO;                           /* IN FORWARD SEARCH             */ 00432000
*       IF ASCBFWDP = 0 THEN                                       0294 00433000
         L     @10,ASCBFWDP(,ASCBPTR)                              0294 00434000
         LTR   @10,@10                                             0294 00435000
         BZ    @RT00294                                            0294 00436000
*         GO TO DEQRCB;             /* AT END OF CHAIN--EXIT         */ 00437000
*       VCTASCB = ASCBFWDP;         /* PICK UP NEXT ASCB POINTER     */ 00438000
         ST    @10,VCTASCB(,VCCTREG)                               0296 00439000
*       GO TO VCKASCB;              /* PROCESS NEW LOCAL MEMORY      */ 00440000
         B     VCKASCB                                             0297 00441000
*     END;                                                         0298 00442000
*   IF ASCBBWDP = 0 THEN                                           0299 00443000
@RF00292 L     @10,ASCBBWDP(,ASCBPTR)                              0299 00444000
         LTR   @10,@10                                             0299 00445000
         BZ    @RT00299                                            0299 00446000
*     GO TO DEQRCB;                 /* AT END OF BACKWARD CHAIN      */ 00447000
*   VCTASCB = ASCBBWDP;             /* SET UP TO NEW LOCAL MEMORY    */ 00448000
         ST    @10,VCTASCB(,VCCTREG)                               0301 00449000
*   IF VCTASCB ^= AIDASCBL THEN                                    0302 00450000
         C     @10,AIDASCBL                                        0302 00451000
         BNE   @RT00302                                            0302 00452000
*     GO TO VCKASCB;                /* IF HAVE NOT GONE AS FAR BACK     00453000
*                                      DOWN THE CHAIN AS WE CAME UP     00454000
*                                      THE CHAN, THEN WE ARE STILL 0303 00455000
*                                      DUMPING UNIQUE LOCAL MEMORIES */ 00456000
*                                                                  0304 00457000
*   /*****************************************************************/ 00458000
*   /*                                                               */ 00459000
*   /* WE HAVE FINISHED DUMPING ALL LOCAL MEMORYS--DEQUEUE THE RCB   */ 00460000
*   /* AND EXIT TO CALLER                                            */ 00461000
*   /*                                                               */ 00462000
*   /*****************************************************************/ 00463000
*                                                                  0304 00464000
*/*DEQRCB: P DEQUEUE RCB                                             */ 00465000
*/* P RESTORE CALLER'S REGISTERS                                     */ 00466000
*/* R RETURN TO CALLER                                               */ 00467000
*/*AMDSAAID: END                                                     */ 00468000
*                                                                  0304 00469000
*DEQRCB:                                                           0304 00470000
*   VCTRCB = RCBNEXT;               /* RESTORE THE ORIGINAL RCB CHAIN*/ 00471000
DEQRCB   LA    @10,AIDRCB                                          0304 00472000
         MVC   VCTRCB(4,VCCTREG),RCBNEXT(@10)                      0304 00473000
*   RETURN CODE(0);                                                0305 00474000
         SR    @15,@15                                             0305 00475000
         L     @13,4(,@13)                                         0305 00476000
         L     @14,12(,@13)                                        0305 00477000
         LM    @00,@12,20(@13)                                     0305 00478000
         BR    @14                                                 0305 00479000
*   END AMDSAAID                                                   0306 00480000
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.      * 00481000
