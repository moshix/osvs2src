         BE    @RT00238                                            0238 00181000
         C     @10,AIDASCBL                                        0238 00182000
         BE    @RT00238                                            0238 00183000
*         GO TO DEQRCB;             /* IF AT END OF CHAIN, TERMINATE */ 00184000
*       GO TO VCKASCB;              /* OTHERWISE RESTART WITH THIS 0240 00185000
*                                      ASCB CHAIN                    */ 00186000
         B     VCKASCB                                             0240 00187000
*     END;                                                         0241 00188000
*                                                                  0242 00189000
*   /*****************************************************************/ 00190000
*   /*                                                               */ 00191000
*   /* THE ASCB IS VALID. OBTAIN THE JOBNAME ASSOCIATED WITH THE     */ 00192000
*   /* MEMORY                                                        */ 00193000
*   /*                                                               */ 00194000
*   /*****************************************************************/ 00195000
*                                                                  0242 00196000
*/*%AIDS: P SET UP ASCBPTR                                           */ 00197000
*/* D (YES,,NO,%NOSAID) AIDBACKS = OFF ?                             */ 00198000
*/* P SAVE LAST ASCB PROCESED                                        */ 00199000
*/*%NOSAID: P VCTASID = ASCBASID                                     */ 00200000
*/* D (YES,JBNS,NO,JBNI) ASCBJBNI = 0 ?                              */ 00201000
*/*JBNI: P (,CKJOB) VCTJBNME = ASCBJBNI                              */ 00202000
*/*JBNS: D (YES,%JOB,NO,) ASCBJBNI = 0 ?                             */ 00203000
*/* P (,CKJOB) VCTJBNME = ASCBJBNS                                   */ 00204000
*/*%JOB: P VCKJBNME = '*UNKNOWN'                                     */ 00205000
*/*CKJOB: P VALIDITY CHECK JOBNAME                                   */ 00206000
*/* D (YES,%MSG,NO,) JOBNAME VALID ?                                 */ 00207000
*/* P VCTJBNME = '*UNKNOWN'                                          */ 00208000
*   ASCBPTR = VCTASCB;              /* UPDATE THE POINTER TO THE ASCB*/ 00209000
