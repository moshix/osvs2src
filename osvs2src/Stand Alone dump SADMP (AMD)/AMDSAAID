         CR    RETCODE,@14                                         0229 00153000
         BE    @RF00229                                            0229 00154000
*     DO;                           /* IF ASCB INVALID USE AN      0230 00155000
*                                      ALTERNATE SEARCH              */ 00156000
*       RESPECIFY RETCODE UNRESTRICTED;                            0231 00157000
*       IF AIDALTSW = OFF THEN                                     0232 00158000
         TM    AIDALTSW,B'10000000'                                0232 00159000
         BZ    @RT00232                                            0232 00160000
*         GO TO NEXTASID;           /* IF IN PRIMARY SEARCH SKIP THIS   00161000
*                                      MEMORY AND CONTINUE WITH THE     00162000
*                                      NEXT MEMORY                   */ 00163000
*       IF AIDBACKS = ON THEN                                      0234 00164000
