*         GO TO DEQRCB;             /* WE ARE AT THE END OF ALL    0235 00167000
*                                      RECOVERY. THE ASVT IS INVALID.   00168000
*                                      THE ASCB FORWARD AND BACKWARD    00169000
*                                      SEARCHES HAVE GONE ASTRAY--OUR   00170000
*                                      ONLY RECOURSE IS TO GRACEFULLY   00171000
*                                      EXIT                          */ 00172000
*       AIDBACKS = ON;              /* SET BACKWARD SEARCH ON        */ 00173000
         OI    AIDBACKS,B'01000000'                                0236 00174000
*       VCTASCB = CVTASCBL;         /* PICK UP BACKWARD CVT CHAIN    */ 00175000
