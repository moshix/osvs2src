*   VCTCBIDN = VASPCTID;            /* SET ASPCT CONTROL BLOCK ID    */ 00087000
         MVI   VCTCBIDN(VCCTREG),X'13'                          01S0260 00088000
*   SVC (VCKSVC);                   /* CALL AMDSAVCK TO CHECK ASPCT  */ 00089000
         SVC   25                                               01S0261 00090000
*   IF RETCODE ^= 0 THEN            /* ASPCT VALID                   */ 00091000
         LTR   RETCODE,RETCODE                                  01S0262 00092000
         BNZ   @RT00262                                         01S0262 00093000
*     GO TO AMREXIT;                /* NO - TAKE ERROR PATH          */ 00094000
*                                                               01S0263 00095000
*   /*****************************************************************/ 00096000
*   /*                                                               */ 00097000
*   /* CALL AALSUB SUBROUTINE TO CONVERT LGVT ENTRY ADDRESS TO LPME  */ 00098000
*   /* ADDRESSES.                                                    */ 00099000
*   /*                                                               */ 00100000
*   /*****************************************************************/ 00101000
*                                                               01S0264 00102000
*/* P SET ASM IN CONTROL FLAG                                        */ 00103000
*/* S AALSUB: OBTAIN LPME ADDRESSES                                  */ 00104000
*/* P RESET ASM IN CONTROL FLAG                                      */ 00105000
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */ 00106000
*/* P OBTAIN PART ENTRY USING PRIMARY LPME                           */ 00107000
*/* P GET DIRECT ACCESS IODB                                         */ 00108000
*/* P GET BCT ADDRESS FROM IODBBCT                                   */ 00109000
*/* P PLACE AMB ADDRESS FROM PART ENTRY IN BCTAMB                    */ 00110000
*/* P PLACE RBA FROM LPME IN BCTRBA                                  */ 00111000
*   VCTASMIC = ON;                  /* INDICATE ASM IN CONTROL       */ 00112000
         OI    VCTASMIC(VCCTREG),B'00100000'                    01S0264 00113000
*   CALL AALSUB;                    /* CONVERT LGVT ENTRY TO LPME       00114000
*                                      ADDRESSES                     */ 00115000
         BAL   @14,AALSUB                                       01S0265 00116000
*   RESPECIFY (R1,RETCODE) RESTRICTED;                          01S0266 00117000
*   VCTASMIC = OFF;                 /* ASM NOT IN CONTROL            */ 00118000
         NI    VCTASMIC(VCCTREG),B'11011111'                    01S0267 00119000
*   IF RETCODE ^= 0 THEN            /* CONVERSION SUCCESSFUL         */ 00120000
         LTR   RETCODE,RETCODE                                  01S0268 00121000
         BNZ   @RT00268                                         01S0268 00122000
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */ 00123000
*   AIEPRME = RALSECTP + RALPMEDX * LENGTH(ASPLPME); /* COMPUTE THE     00124000
*                                      PRIMARY LPME ADDRESS USING THE   00125000
*                                      VALUES RETURNED FROM AALSUB   */ 00126000
         L     @14,RALPMEDX(,R1)                                01S0270 00127000
         SLA   @14,2                                            01S0270 00128000
         A     @14,RALSECTP(,R1)                                01S0270 00129000
         ST    @14,AIEPRME(,RAIE)                               01S0270 00130000
*   PAREPTR = ADDR(PARTENTS(ASPPRTNN+1)); /* COMPUTE PART ENTRY ADDR */ 00131000
         L     @14,LPMEPTR(,RAIE)                               01S0271 00132000
         SR    @09,@09                                          01S0271 00133000
         IC    @09,ASPPRTNN(,@14)                               01S0271 00134000
         SLA   @09,6                                            01S0271 00135000
         L     @05,CVTPTR                                       01S0271 00136000
         L     @05,ASMVTPTR(,@05)                               01S0271 00137000
         L     PAREPTR,PARTPTR(,@05)                            01S0271 00138000
         LA    PAREPTR,PARTENTS(@09,PAREPTR)                    01S0271 00139000
*   BCTAMB = PAREAMBP;              /* GET AMB ADDR FROM PART ENTRY  */ 00140000
         L     @09,@CA01700                                     01S0272 00141000
         L     @05,VCTLPA(,VCCTREG)                             01S0272 00142000
         AR    @09,@05                                          01S0272 00143000
         A     @05,IODBBCT(,@09)                                01S0272 00144000
         L     @09,PAREAMBP(,PAREPTR)                           01S0272 00145000
         ST    @09,BCTAMB(,@05)                                 01S0272 00146000
*   BCTRBA = ASPRSLOT;              /* GET RBA FROM LPME             */ 00147000
         SR    @00,@00                                          01S0273 00148000
         ICM   @00,3,ASPRSLOT(@14)                              01S0273 00149000
         ST    @00,BCTRBA(,@05)                                 01S0273 00150000
*   RESPECIFY (R1,RETCODE) UNRESTRICTED;                        01S0274 00151000
*                                                               01S0275 00152000
*   /*****************************************************************/ 00153000
*   /*                                                               */ 00154000
*   /* OBTAIN AMB ADDRESS AND VALIDITY CHECK THE AMB                 */ 00155000
*   /*                                                               */ 00156000
*   /*****************************************************************/ 00157000
*                                                               01S0275 00158000
*/* P GET AMB ADDRESS FROM BCTAMB                                    */ 00159000
*/* P SET VCTCBADR TO AMB ADDRESS                                    */ 00160000
*/* P SET VCTCBID TO AMB ID                                          */ 00161000
*/* L  AMDSAVCK:  VALIDITY CHECK AMB                                 */ 00162000
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */ 00163000
*   VCTCBADR = BCTAMB;              /* ADDRESS OF AMB FOR VALIDITY      00164000
*                                      CHECK                         */ 00165000
         ST    @09,VCTCBADR(,VCCTREG)                           01S0275 00166000
*   VCTCBIDN = VAMBID;              /* AMB CONTROL BLOCK ID          */ 00167000
         MVI   VCTCBIDN(VCCTREG),X'01'                          01S0276 00168000
*   SVC(VCKSVC);                    /* CALL AMDSAVCK TO CHECK AMB    */ 00169000
         SVC   25                                               01S0277 00170000
*   RESPECIFY RETCODE RESTRICTED;                               01S0278 00171000
*   IF RETCODE ^= 0 THEN            /* AMB VALID                     */ 00172000
         LTR   RETCODE,RETCODE                                  01S0279 00173000
         BNZ   @RT00279                                         01S0279 00174000
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */ 00175000
*                                                               01S0280 00176000
*   /*****************************************************************/ 00177000
*   /*                                                               */ 00178000
*   /* GET EDB HEADER ADDRESS FROM AMB AND VALIDITY CHECK THE EDB    */ 00179000
*   /*                                                               */ 00180000
*   /*****************************************************************/ 00181000
*                                                               01S0281 00182000
*/* P OBTAIN EDB HEADER ADDRESS FROM AMBEDB                          */ 00183000
*/* P SET VCTCBADR TO AMBEDB                                         */ 00184000
*/* P SET VCTCBID TO EDB HEADER ID                                   */ 00185000
*/* L  AMDSAVCK:  VALIDITY CHECK EDB HEADER                          */ 00186000
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */ 00187000
*/* D (NO,,YES,AMREXIT) EDBNO = 0                                    */ 00188000
*   RESPECIFY IDAAMB BASED(BCTAMB);                             01S0281 00189000
*   RESPECIFY IDAEDBHD BASED(AMBEDB);                           01S0282 00190000
*   VCTCBADR = AMBEDB;              /* ADDRESS OF EDB HEADER FOR        00191000
*                                      VALIDITY CHECK                */ 00192000
         L     @14,@CA01700                                     01S0283 00193000
         L     @09,VCTLPA(,VCCTREG)                             01S0283 00194000
         AR    @14,@09                                          01S0283 00195000
         A     @09,IODBBCT(,@14)                                01S0283 00196000
         L     @14,BCTAMB(,@09)                                 01S0283 00197000
         MVC   VCTCBADR(4,VCCTREG),AMBEDB(@14)                  01S0283 00198000
*   VCTCBIDN = VEDBID;              /* EDB HEADER CONTROL BLOCK ID   */ 00199000
         MVI   VCTCBIDN(VCCTREG),X'06'                          01S0284 00200000
*   SVC(VCKSVC);                    /* CALL AMDSAVCK TO CHECK EDB       00201000
*                                      HEADER                        */ 00202000
         SVC   25                                               01S0285 00203000
*   IF RETCODE ^= 0 | EDBNO = 0 THEN /* EDB HEADER VALID OR CONTAIN     00204000
*                                      ANY ENTRIES                   */ 00205000
         LTR   RETCODE,RETCODE                                  01S0286 00206000
         BNZ   @RT00286                                         01S0286 00207000
         L     @14,@CA01700                                     01S0286 00208000
         L     @09,VCTLPA(,VCCTREG)                             01S0286 00209000
         AR    @14,@09                                          01S0286 00210000
         A     @09,IODBBCT(,@14)                                01S0286 00211000
         L     @14,BCTAMB(,@09)                                 01S0286 00212000
         L     @14,AMBEDB(,@14)                                 01S0286 00213000
         CLI   EDBNO(@14),0                                     01S0286 00214000
         BE    @RT00286                                         01S0286 00215000
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */ 00216000
*   RESPECIFY RETCODE UNRESTRICTED;                             01S0288 00217000
*                                                               01S0289 00218000
*   /*****************************************************************/ 00219000
*   /*                                                               */ 00220000
*   /* EDB HEADER IS VALID. SAVE NUMBER AND LENGTH OF EDB ENTRIES AND*/ 00221000
*   /* SEARCH FOR AN EDB WHICH CONTAINS THE REQUESTED RBA.           */ 00222000
*   /*                                                               */ 00223000
*   /*****************************************************************/ 00224000
*                                                               01S0289 00225000
*/* P GET RBA FROM BCTRBA                                            */ 00226000
*/* P GET FIRST EDB                                                  */ 00227000
*/*AMREDBS: D (NO,,YES,AMREDBN) RBA < EDBLORBA                       */ 00228000
*/* D (NO,AMREDBF,YES,AMREDBN) RBA > EDBHIRBA                        */ 00229000
*/*AMREDBN: P DECREMENT EDB COUNT                                    */ 00230000
*/* D (NO,,YES,AMREXIT) LAST EDB                                     */ 00231000
*/* P (,AMREDBS) GET NEXT EDB                                        */ 00232000
*   RESPECIFY CONVOUT BASED(RPLH);                              01S0289 00233000
*   RESPECIFY RPLH RESTRICTED;                                  01S0290 00234000
*   RPLH = ADDR(CNVRTOUT);          /* SET UP DUMMY PLH POINTER      */ 00235000
         LA    RPLH,CNVRTOUT                                    01S0291 00236000
*   CONBB = EDBNO;                  /* SAVE NUMBER OF EDB ENTRIES    */ 00237000
         SR    @05,@05                                          01S0292 00238000
         IC    @05,EDBNO(,@14)                                  01S0292 00239000
         STH   @05,CONBB(,RPLH)                                 01S0292 00240000
*   CONCC = EDBLEN;                 /* SAVE LENGTH OF EDB ENTRIES    */ 00241000
         MVC   CONCC(2,RPLH),EDBLEN(@14)                        01S0293 00242000
*   RESPECIFY IDAEDB BASED(RWORK2);                             01S0294 00243000
*   RESPECIFY (RWORK0,RWORK2) RESTRICTED;                       01S0295 00244000
*   RWORK2 = AMBEDB + LENGTH(IDAEDBHD); /* POINT TO FIRST EDB ENTRY  */ 00245000
         AH    @14,@CH00060                                     01S0296 00246000
         LR    RWORK2,@14                                       01S0296 00247000
*   RWORK0 = BCTRBA * PAGESIZE;     /* GET REQUESTED RBA             */ 00248000
         L     RWORK0,BCTRBA(,@09)                              01S0297 00249000
         SLA   RWORK0,12                                        01S0297 00250000
*   DO I = 1 TO CONBB;              /* SEARCH ALL EDB ENTRIES        */ 00251000
         LA    I,1                                              01S0298 00252000
         B     @DE00298                                         01S0298 00253000
@DL00298 DS    0H                                               01S0299 00254000
*     IF (RWORK0 >= EDBLORBA) & (RWORK0 <= EDBHIRBA) THEN /* IS RBA     00255000
*                                      CONTAINED IN RANGE COVERED BY    00256000
*                                      THIS EDB                      */ 00257000
         CL    RWORK0,EDBLORBA(,RWORK2)                         01S0299 00258000
         BL    @RF00299                                         01S0299 00259000
         CL    RWORK0,EDBHIRBA(,RWORK2)                         01S0299 00260000
         BNH   @RT00299                                         01S0299 00261000
*       GO TO AMREDBF;              /* YES - PROPER EDB FOUND        */ 00262000
*     RWORK2 = RWORK2 + CONCC;      /* POINT TO NEXT EDB             */ 00263000
@RF00299 SR    @09,@09                                          01S0301 00264000
         ICM   @09,3,CONCC(RPLH)                                01S0301 00265000
         ALR   RWORK2,@09                                       01S0301 00266000
*   END;                            /* CONTINUE EDB SEARCH           */ 00267000
         AH    I,@CH00062                                       01S0302 00268000
@DE00298 SR    @09,@09                                          01S0302 00269000
         ICM   @09,3,CONBB(RPLH)                                01S0302 00270000
         CR    I,@09                                            01S0302 00271000
         BNH   @DL00298                                         01S0302 00272000
*   GO TO AMREXIT;                  /* NO EDB CONTAINS REQUESTED RBA */ 00273000
         B     AMREXIT                                          01S0303 00274000
*                                                               01S0304 00275000
*   /*****************************************************************/ 00276000
*   /*                                                               */ 00277000
*   /* EDB FOR REQUESTED RBA FOUND. CHECK THE LPMB IT POINTS TO FOR  */ 00278000
*   /* VALIDITY.                                                     */ 00279000
*   /*                                                               */ 00280000
*   /*****************************************************************/ 00281000
*                                                               01S0304 00282000
*/*AMREDBF: P GET LPMB ADDRESS FROM EDBLPMBA                         */ 00283000
*/* P SET VCTCBADR TO LPMB ADDRESS                                   */ 00284000
*/* P SET VCTCBID TO LPMB ID                                         */ 00285000
*/* L  AMDSAVCK:  VALIDITY CHECK LPMB                                */ 00286000
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */ 00287000
*                                                               01S0304 00288000
*AMREDBF:                                                       01S0304 00289000
*   VCTCBADR = EDBLPMBA;            /* LPMB ADDRESS FOR VALIDITY        00290000
*                                      CHECK                         */ 00291000
AMREDBF  MVC   VCTCBADR(4,VCCTREG),EDBLPMBA(RWORK2)             01S0304 00292000
*   VCTCBIDN = VLPMBID;             /* LPMB CONTROL BLOCK ID         */ 00293000
         MVI   VCTCBIDN(VCCTREG),X'0F'                          01S0305 00294000
*   SVC(VCKSVC);                    /* CALL AMDSAVCK TO CHECK LPMB   */ 00295000
         SVC   25                                               01S0306 00296000
*   RESPECIFY RETCODE RESTRICTED;                               01S0307 00297000
*   IF RETCODE ^= 0 THEN            /* LPMB VALID                    */ 00298000
         LTR   RETCODE,RETCODE                                  01S0308 00299000
         BNZ   @RT00308                                         01S0308 00300000
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */ 00301000
*   RESPECIFY RETCODE UNRESTRICTED;                             01S0310 00302000
*                                                               01S0311 00303000
*   /*****************************************************************/ 00304000
*   /*                                                               */ 00305000
*   /* LPMB VALID. SET UP INTERFACE TO VSAM CONVERT ROUTINE AND CALL */ 00306000
*   /* IT TO CONVERT THE RBA TO AN MBBCCHHR.                         */ 00307000
*   /*                                                               */ 00308000
*   /*****************************************************************/ 00309000
*                                                               01S0311 00310000
*/* P SET UP INTERFACE TO VSAM CONVERT ROUTINE                       */ 00311000
*/* S CONVERT: CONVERT RBA TO MBBCCHHR                               */ 00312000
*/* N RETURN CODE FROM CONVERT IS IN 'BB' IN WORK AREA               */ 00313000
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */ 00314000
*   RESPECIFY (RAMB,RPLH) RESTRICTED;                           01S0311 00315000
*   RAMB = BCTAMB;                  /* GET AMB ADDRESS               */ 00316000
         L     @09,@CA01700                                     01S0312 00317000
         L     @07,VCTLPA(,VCCTREG)                             01S0312 00318000
         AR    @09,@07                                          01S0312 00319000
         A     @07,IODBBCT(,@09)                                01S0312 00320000
         L     RAMB,BCTAMB(,@07)                                01S0312 00321000
*   CALL CONVERT;                   /* CALL VSAM CONVERT ROUTINE     */ 00322000
         BAL   @14,CONVERT                                      01S0313 00323000
*   IF CONBB ^= '0000'X THEN        /* CONVERT SUCCESSFUL            */ 00324000
         CLC   CONBB(2,RPLH),@CB01748                           01S0314 00325000
         BNE   @RT00314                                         01S0314 00326000
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */ 00327000
*   RESPECIFY (RWORK0,RWORK2,RAMB) UNRESTRICTED;                01S0316 00328000
*                                                               01S0317 00329000
*   /*****************************************************************/ 00330000
*   /*                                                               */ 00331000
*   /* MBBCCHHR OBTAINED SUCCESSFULLY. GET CPU ADDRESS, AND DEVICE   */ 00332000
*   /* ADDRESS AND ATTRIBUTES.                                       */ 00333000
*   /*                                                               */ 00334000
*   /*****************************************************************/ 00335000
*                                                               01S0317 00336000
*/* P SET UP EXTENT NUMBER FOR AMDSAUCB                              */ 00337000
*/* L  AMDSAUCB:  GET CPU ADDR, DEVICE ADDR AND ATTRIBUTES           */ 00338000
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */ 00339000
*   IODBCCHH = CONVOUT;             /* MOVE MBBCCHHR TO IODB         */ 00340000
         L     @09,@CA01700                                     01S0317 00341000
         A     @09,VCTLPA(,VCCTREG)                             01S0317 00342000
         MVC   IODBCCHH(8,@09),CONVOUT(RPLH)                    01S0317 00343000
*   SVC(UCBSVC);                    /* CALL AMDSAUCB TO GET ADDRESS     00344000
*                                      AND ATTRIBUTES                */ 00345000
         SVC   24                                               01S0318 00346000
*   RESPECIFY RETCODE RESTRICTED;                               01S0319 00347000
*   IF RETCODE ^= 0 THEN            /* PROCESSING SUCCESSFUL         */ 00348000
