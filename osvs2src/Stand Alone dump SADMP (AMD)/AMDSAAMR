         BNE   @RT00320                                         01S0320 00351000
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */ 00352000
*   RESPECIFY RETCODE UNRESTRICTED;                             01S0322 00353000
*                                                               01S0322 00354000
*   /*****************************************************************/ 00355000
*   /*                                                               */ 00356000
*   /* RE-INITIALIZE IODB WITH MBBCCHHR IN CASE OF PAGE FAULT IN     */ 00357000
*   /* AMDSAUCB. SET UP RETURN CODE OF 0.                            */ 00358000
*   /*                                                               */ 00359000
*   /*****************************************************************/ 00360000
*                                                               01S0323 00361000
*/* P MOVE MBBCCHHR TO D.A. IODBCCHH FROM WORK AREA                  */ 00362000
*/* P SET UP RETURN CODE OF 0                                        */ 00363000
*   IODBCCHH = CONVOUT;             /* MOVE MBBCCHHR TO IODB         */ 00364000
         L     @09,@CA01700                                     01S0323 00365000
         A     @09,VCTLPA(,VCCTREG)                             01S0323 00366000
         MVC   IODBCCHH(8,@09),CONVOUT(RPLH)                    01S0323 00367000
*   SAVEBACK -> SAVER15 = 0;        /* INDICATE SUCCESSFUL PROCESSING*/ 00368000
         L     @09,SAVEBACK(,R13)                               01S0324 00369000
         ST    @14,SAVER15(,@09)                                01S0324 00370000
*   RESPECIFY RPLH UNRESTRICTED;                                01S0325 00371000
*                                                               01S0326 00372000
*   /*****************************************************************/ 00373000
*   /*                                                               */ 00374000
*   /* DEQUEUE RCB, FREE SAVE AREA, AND RETURN TO CALLER.            */ 00375000
*   /*                                                               */ 00376000
*   /*****************************************************************/ 00377000
*                                                               01S0326 00378000
*/*AMRRET: P DEQUEUE RCB                                             */ 00379000
*/* P RESET ASM IN CONTROL FLAGS                                     */ 00380000
*/* P RESTORE ORIGINAL BUFFER REQUEST FLAGS                          */ 00381000
*/* L  AMDSAFRM:  FREE SAVE/WORK AREA                                */ 00382000
*/*AMRRETG: P RESTORE REGISTERS                                      */ 00383000
*/* R RETURN TO CALLER                                               */ 00384000
*                                                               01S0326 00385000
*AMRRET:                                                        01S0326 00386000
*   VCTRCB = RCBNEXT;               /* DEQUEUE RCB                   */ 00387000
AMRRET   LA    @09,AMRRCB                                       01S0326 00388000
         MVC   VCTRCB(4,VCCTREG),RCBNEXT(@09)                   01S0326 00389000
*   VCTASMIC = OFF;                 /* RESET ASM IN CONTROL FLAGS    */ 00390000
         NI    VCTASMIC(VCCTREG),B'11011111'                    01S0327 00391000
*   CTBUFTYP = AMRBUFT;             /* RESTORE BUFFER REQUEST FLAGS  */ 00392000
         L     @09,VCTCCTV(,VCCTREG)                            01S0328 00393000
         STC   AMRBUFT,CTBUFTYP(,@09)                           01S0328 00394000
*   RESPECIFY R1 RESTRICTED;                                    01S0329 00395000
*   R1 = R8;                        /* POINT TO AREA TO BE FREED     */ 00396000
         LR    R1,R8                                            01S0330 00397000
*   SVC(FRMSVC);                    /* CALL AMDSAFRM TO FREE SAVE       00398000
*                                      AREA                          */ 00399000
         SVC   5                                                01S0331 00400000
*   RETURN;                         /* RETURN TO CALLER              */ 00401000
*                                                               01S0332 00402000
@EL00001 L     @13,4(,@13)                                      01S0332 00403000
@EF00001 DS    0H                                               01S0332 00404000
@ER00001 LM    @14,@12,12(@13)                                  01S0332 00405000
         BR    @14                                              01S0332 00406000
*   /*****************************************************************/ 00407000
*   /*                                                               */ 00408000
*   /* RCB EXIT - SET UP RETURN CODE OF 4 AND TAKE RETURN PATH       */ 00409000
*   /*                                                               */ 00410000
*   /*****************************************************************/ 00411000
*                                                               01S0333 00412000
*/*AMREXIT: P (,AMRRET) SET UP RETURN CODE OF 4                      */ 00413000
*                                                               01S0333 00414000
*AMREXIT:                                                       01S0333 00415000
*   SAVEBACK -> SAVER15 = 4;        /* INDICATE ERROR DURING    01S0333 00416000
*                                      PROCESSING                    */ 00417000
AMREXIT  L     @09,SAVEBACK(,R13)                               01S0333 00418000
         MVC   SAVER15(4,@09),@CF00054                          01S0333 00419000
*   GO TO AMRRET;                   /* TAKE RETURN PATH              */ 00420000
         B     AMRRET                                           01S0334 00421000
*   /*****************************************************************/ 00422000
*   /*                                                               */ 00423000
*   /* AALSUB SUBROUTINE - THIS SUBROUTINE PERFORMS THE AUXILIARY    */ 00424000
*   /* STORAGE MANAGEMENT (ASM) FUNCTION OF CONVERTING AN LGVT ENTRY */ 00425000
*   /* ADDRESS TO AN LPME INDEX VALUE AND SECTOR POINTER. THESE      */ 00426000
*   /* MAY THEN BE USED TO COMPUTE THE LPME ADDRESS(ES) CORRESPONDING*/ 00427000
*   /* TO THE REQUESTED LPID.                                        */ 00428000
*   /*                                                               */ 00429000
*   /* INPUT - ASPCT ADDRESS IN ASPCTPTR                             */ 00430000
*   /*         AIE ADDRESS IN RAIE                                   */ 00431000
*   /*                                                               */ 00432000
*   /* OUTPUT - RETURN CODE IN REGISTER 15.                          */ 00433000
*   /*            00 - NO ERRORS, ADDRESS OF PARAMETER IN REGISTER 1.*/ 00434000
*   /*            60 - RPN OUT OF ADDRESS SPACE                      */ 00435000
*   /*            64 - LPME SECTOR NOT ALLOCATED                     */ 00436000
*   /*          OUTPUT PARAMETER CONSISTS OF THE LPME INDEX AND THE  */ 00437000
*   /*          SECTOR ADDRESS.                                      */ 00438000
*   /*                                                               */ 00439000
*   /* EXIT-NORMAL - RETURN TO CALLER WITH RETURN CODE               */ 00440000
*   /*                                                               */ 00441000
*   /* EXIT-ERROR - NONE                                             */ 00442000
*   /*                                                               */ 00443000
*   /* EXTERNAL-REFERENCES - NONE                                    */ 00444000
*   /*                                                               */ 00445000
*   /* MAPPING-MACROS-USED - ILRAIE,ILRAIA,ILRASPCT                  */ 00446000
*   /*                                                               */ 00447000
*   /* REGISTERS-MODIFIED - 1,15                                     */ 00448000
*   /*                                                               */ 00449000
*   /* NOTES - THE CALLING MODULE MUST INCLUDE THE MAPPING MACROS    */ 00450000
*   /*         LISTED ABOVE. A BASE POINTER FOR THE ASPCT MUST BE    */ 00451000
*   /*         SET BY THE CALLING ROUTINE. ALL LABELS AND DATA AREAS */ 00452000
*   /*         CREATED BY THIS SUBROUTINE BEGIN WITH 'AAL'. THIS     */ 00453000
*   /*         ROUTINE ALSO REQUIRES THAT THE CALLING MODULE         */ 00454000
*   /*         DECLARES REGISTERS R0-R15 AND POINTERS RAIE AND RAIA. */ 00455000
*   /*         THE AIE ADDRESS MUST BE IN RAIE.                      */ 00456000
*   /*                                                               */ 00457000
*   /*****************************************************************/ 00458000
*/*AALSUB: CHART                                                     */ 00459000
*/*HEADER                                                       02S0335 00460000
*/*AALSUB        LGVT ENTRY TO LPME CONVERT SUBROUTINE          02S0335 00461000
*/*                                               PAGE #             */ 00462000
*/*AALSUB: E AALSUB                                                  */ 00463000
*/* P GET AIA ADDRESS FROM AIE                                       */ 00464000
*/* D (YES,,NO,%AALRPN) RPN > MAX. RPN                               */ 00465000
*/*%AALR60: P (,%AALRET) SET RETURN CODE 60                          */ 00466000
*/*%AALRPN: P COMPUTE ASST INDEX                                     */ 00467000
*/* P COMPUTE LPME INDEX                                             */ 00468000
*/* D (YES,%AALR60,NO,) RPN > 16 MEG                                 */ 00469000
*/* P INDICATE BASE ASPCT                                            */ 00470000
*/* D (NO,,YES,%AALR60) EXTENSION ASPCT NEEDED                       */ 00471000
*/* P COMPUTE ASST POINTER USING INDEX                               */ 00472000
*/* D (NO,,YES,%AALSTE) LPME SECTOR ALLOCATED                        */ 00473000
*/* P (,%AALRET) SET RETURN CODE 64                                  */ 00474000
*/*%AALSTE: P GET LPME SECTOR ADDRESS                                */ 00475000
*/* P SET RETURN CODE 0                                              */ 00476000
*/*%AALRET: R RETURN TO CALLER                                       */ 00477000
*/*AALSUB: END                                                       */ 00478000
*                                                               02S0335 00479000
*AALSUB:                                                        02S0335 00480000
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                            02S0335 00481000
AALSUB   DS    0H                                               02S0336 00482000
*   DCL                                                         02S0336 00483000
*         AAL0 CONSTANT(0);                                     02S0336 00484000
*   DCL                                                         02S0337 00485000
*         AAL32 CONSTANT(32);                                   02S0337 00486000
*   DCL                                                         02S0338 00487000
*         AAL60 CONSTANT(60);                                   02S0338 00488000
*   DCL                                                         02S0339 00489000
*         AAL64 CONSTANT(64);                                   02S0339 00490000
*   DCL                                                         02S0340 00491000
*         AAL128 CONSTANT(128);                                 02S0340 00492000
*   DCL                                                         02S0341 00493000
*         AALONE CONSTANT(1);                                   02S0341 00494000
*   DCL                                                         02S0342 00495000
*         AALASSTX PTR(31);                                     02S0342 00496000
*   DCL                                                         02S0343 00497000
*         AALEXTDX PTR(31);                                     02S0343 00498000
*   DCL                                                         02S0344 00499000
*         AALASSTP PTR(31);                                     02S0344 00500000
*   DCL                                                         02S0345 00501000
*         AALASSTE PTR(31) BASED;                               02S0345 00502000
*   DCL                                                         02S0346 00503000
*         1 AALRPARM,               /* RETURN PARAMETER              */ 00504000
*             3 AALPMEDX PTR(31),   /* LPME INDEX VALUE              */ 00505000
*             3 AALSECTP PTR(31);   /* SECTOR POINTER                */ 00506000
*   RESPECIFY (R14,R15) RESTRICTED;                             02S0347 00507000
*   RAIA = RAIE -> AIEAIAP;         /* GET AIA ADDR FROM AIE         */ 00508000
         L     RAIA,AIEAIAP(,RAIE)                              02S0348 00509000
*   IF RAIA -> AIARPN > ASPMAXPN THEN /* REQUESTED RPN > MAXIMUM RPN */ 00510000
         L     @09,LGVTEPTR(,RAIE)                              02S0349 00511000
         L     @09,ASPCTPTR(,@09)                               02S0349 00512000
         CLC   AIARPN(4,RAIA),ASPMAXPN(@09)                     02S0349 00513000
         BNH   @RF00349                                         02S0349 00514000
*     R15 = AAL60;                  /* YES - RPN OUT OF ADDRESS SPACE*/ 00515000
         LA    R15,60                                           02S0350 00516000
*   ELSE                            /* NO -                          */ 00517000
*     DO;                                                       02S0351 00518000
         B     @RC00349                                         02S0351 00519000
@RF00349 DS    0H                                               02S0352 00520000
*       AALASSTX = (RAIA -> AIARPN / AAL64) + AALONE; /* ASST INDEX =   00521000
*                                      HIGH ORDER 27 BITS OF RPN     */ 00522000
         L     @09,AIARPN(,RAIA)                                02S0352 00523000
         LR    AALASSTX,@09                                     02S0352 00524000
         SRL   AALASSTX,6                                       02S0352 00525000
         AL    AALASSTX,@CF00062                                02S0352 00526000
*       AALPMEDX = RAIA -> AIARPN & '0000003F'X; /* LPME INDEX =        00527000
*                                      LOW ORDER 5 BITS OF RPN       */ 00528000
         N     @09,@CF01770                                     02S0353 00529000
         ST    @09,AALPMEDX                                     02S0353 00530000
*       IF AALASSTX > AAL64  THEN   /* RPN FOR ADDRESS > 16 MEG      */ 00531000
         CH    AALASSTX,@CH00801                                02S0354 00532000
         BNH   @RF00354                                         02S0354 00533000
*         R15 = AAL60;              /* YES - OUT OF ADDRESS SPACE    */ 00534000
         LA    R15,60                                           02S0355 00535000
*       ELSE                                                    02S0356 00536000
*         DO;                                                   02S0356 00537000
         B     @RC00354                                         02S0356 00538000
@RF00354 DS    0H                                               02S0357 00539000
*           AALEXTDX = AAL0;        /* INDICATE BASE ASPCT           */ 00540000
         SR    @09,@09                                          02S0357 00541000
         LR    AALEXTDX,@09                                     02S0357 00542000
*           IF ASPAXPGS => AALEXTDX THEN /* ANY ASST EXTENSIONS      */ 00543000
         L     @05,LGVTEPTR(,RAIE)                              02S0358 00544000
         L     @05,ASPCTPTR(,@05)                               02S0358 00545000
         CH    AALEXTDX,ASPAXPGS(,@05)                          02S0358 00546000
         BH    @RF00358                                         02S0358 00547000
*             DO;                                               02S0359 00548000
*                 AALASSTP = ADDR(ASPASSTE(AALASSTX)); /* SET UP ASST   00549000
*                                      ADDRESS                       */ 00550000
         LR    @07,AALASSTX                                     02S0360 00551000
         SLA   @07,2                                            02S0360 00552000
         LA    AALASSTP,ASPASSTE-4(@07,@05)                     02S0360 00553000
*               IF AALASSTP -> AALASSTE = AAL0 THEN /* ASST ENTRY 0  */ 00554000
         C     @09,AALASSTE(,AALASSTP)                          02S0361 00555000
         BNE   @RF00361                                         02S0361 00556000
*                 R15 = AAL64;      /* YES - SECTOR NOT ALLOCATED    */ 00557000
         LA    R15,64                                           02S0362 00558000
*               ELSE                                            02S0363 00559000
*                 DO;                                           02S0363 00560000
         B     @RC00361                                         02S0363 00561000
@RF00361 DS    0H                                               02S0364 00562000
*                   AALSECTP = AALASSTP -> AALASSTE; /* GET LPME        00563000
*                                      SECTOR ADDRESS                */ 00564000
         MVC   AALSECTP(4),AALASSTE(AALASSTP)                   02S0364 00565000
*                   R15 = AAL0;     /* SET UP 0 RETURN CODE          */ 00566000
         SR    R15,R15                                          02S0365 00567000
*                 END;                                          02S0366 00568000
*             END;                                              02S0367 00569000
*           ELSE                                                02S0368 00570000
*             R15 = AAL60;          /* ONLY BASE ASPCT USED FOR 02S0368 00571000
*                                      16 MEGABTYE AREA              */ 00572000
         B     @RC00358                                         02S0368 00573000
@RF00358 LA    R15,60                                           02S0368 00574000
*         END;                                                  02S0369 00575000
@RC00358 DS    0H                                               02S0370 00576000
*     END;                                                      02S0370 00577000
@RC00354 DS    0H                                               02S0371 00578000
*   RESPECIFY R1 RESTRICTED;                                    02S0371 00579000
@RC00349 DS    0H                                               02S0372 00580000
*   R1 = ADDR(AALRPARM);            /* SET UP RETURN PARAMETER       */ 00581000
         LA    R1,AALRPARM                                      02S0372 00582000
*   RETURN;                         /* RETURN TO CALLER              */ 00583000
@EL00002 DS    0H                                               02S0373 00584000
@EF00002 DS    0H                                               02S0373 00585000
@ER00002 BR    @14                                              02S0373 00586000
*   RESPECIFY (R1,R14,R15) UNRESTRICTED;                        02S0374 00587000
*   END AALSUB;                                                 02S0375 00588000
*                                                               01S0376 00589000
*   /*****************************************************************/ 00590000
*   /*                                                               */ 00591000
*   /* THIS IS THE VSAM RBA TO MBBCCHHR CONVERT ROUTINE. THE CODE IS */ 00592000
*   /* IDENTICAL WITH THAT IN MODULE IDA019R3, THE VSAM I/O MANAGER. */ 00593000
*   /* INPUT CONSISTS OF THE REQUESTED RBA IN RWORK0 (GPR 0), THE AMB*/ 00594000
*   /* ADDRESS IN RAMB (GPR 3), A DUMMY PLH POINTER IN RPLH (GPR 2), */ 00595000
*   /* OUTPUT IS PLACED IN THE DUMMY PLH (CNVRTOUT). THE 'BB' FIELD  */ 00596000
*   /* OF THE MBBCCHHR IS USED AS A RETURN CODE.                     */ 00597000
*   /*                                                               */ 00598000
*   /*****************************************************************/ 00599000
*                                                               01S0376 00600000
*/*CONVERT: E CONVERT                                                */ 00601000
*/* N VSAM RBA TO MBBCCHHR CONVERT RTN                               */ 00602000
*/* P SAVE NUMBER OF EDB ENTRIES                                     */ 00603000
*/* P SAVE EDB ENTRY LENGTH                                          */ 00604000
*/* P SET BLOCKSIZE TO 0                                             */ 00605000
*/* P GET FIRST EDB                                                  */ 00606000
*/*CONEDBS: D (NO,,YES,CONEDBN) RBA < EDBLORBA                       */ 00607000
*/* D (NO,,YES,CONEDBN) RBA > EDBHIRBA                               */ 00608000
*/* P MOVE 'M' FROM EDBM TO OUTPUT AREA                              */ 00609000
*/* P SET RC TO 0 IN OUTPUT AREA ('BB')                              */ 00610000
*/* P SAVE STARTING TRACK (EDBSTTRK)                                 */ 00611000
*/* P COMPUTE WORK1 = (RBA - EDBLORBA) (REL. OFFSET)                 */ 00612000
*/* P GET LPMB ADDRESS FROM EDBLPMBA                                 */ 00613000
*/* P SET 'CC' = WORK1 / LPMAUSZ                                     */ 00614000
*/* P SET WORK2 = REM( WORK1 / LPMAUSZ)                              */ 00615000
*/* P SET 'HH' = WORK2 / LPMBPTRK                                    */ 00616000
*/* P SET WORK1 = REM( WORK2 / LPMBPTRK)                             */ 00617000
*/* P SET 'R' = (WORK1 / LPMBLKSZ) + 1                               */ 00618000
*/* P SET WORK1 = CC * LPMTRKAU + HH + STARTING TRACK                */ 00619000
*/* P SET CC = WORK1 / LPMTPC                                        */ 00620000
*/* P SET HH = REM( WORK1 / LPMTPC)                                  */ 00621000
*/* P SET BLOCKSIZE = LPMBLKSZ                                       */ 00622000
*/*CONEDBN: D (YES,,NO,CONRET) BLOCKSIZE = 0                         */ 00623000
*/* P DECREMENT EDB COUNT                                            */ 00624000
*/* D (NO,,YES,%CONCHK) COUNT = 0                                    */ 00625000
*/* P (,CONEDBS) GET NEXT EDB                                        */ 00626000
*/*CONRET: P RESTORE REGISTERS                                       */ 00627000
*/* R RETURN TO CALLER                                               */ 00628000
*/*%CONCHK: D (YES,,NO,CONRET) BLOCKSIZE = 0                         */ 00629000
*/* P (,CONRET) SET RETURN CODE OF 4                                 */ 00630000
*                                                               01S0376 00631000
*CONVERT:                                                       01S0376 00632000
*   PROC OPTIONS(SAVE,NOSAVEAREA);                              01S0376 00633000
CONVERT  STM   @14,@12,12(@13)                                  01S0376 00634000
*   RESPECIFY (RWORK0,RWORK2,RAMB,RPLH) RESTRICTED;             01S0377 00635000
*   RESPECIFY IDAEDBHD BASED(RWORK2);                           01S0378 00636000
*   RWORK2 = AMBEDB;                /* GET EDB ADDRESS FROM AMB      */ 00637000
         L     @09,@CA01700                                     01S0379 00638000
         L     @01,VCTLPA(,VCCTREG)                             01S0379 00639000
         AR    @09,@01                                          01S0379 00640000
         A     @01,IODBBCT(,@09)                                01S0379 00641000
         L     @09,BCTAMB(,@01)                                 01S0379 00642000
         L     RWORK2,AMBEDB(,@09)                              01S0379 00643000
*   CONBB = EDBNO;                  /* SAVE NUMBER OF EDB ENTRIES    */ 00644000
         SR    @09,@09                                          01S0380 00645000
         IC    @09,EDBNO(,RWORK2)                               01S0380 00646000
         STH   @09,CONBB(,RPLH)                                 01S0380 00647000
*   CONCC = EDBLEN;                 /* SAVE EDB ENTRY LENGTH         */ 00648000
         MVC   CONCC(2,RPLH),EDBLEN(RWORK2)                     01S0381 00649000
*   RESPECIFY IDAEDB BASED(RWORK2);                             01S0382 00650000
*   RWORK2 = RWORK2 + LENGTH(IDAEDBHD);                         01S0383 00651000
*                                                               01S0383 00652000
         AL    RWORK2,@CF00060                                  01S0383 00653000
*   /*****************************************************************/ 00654000
*   /*                                                               */ 00655000
*   /* TEST INPUT RBA AGAINST LOW AND HIGH RBA RANGE FOR EACH EDB    */ 00656000
*   /* ENTRY TO FIND CORRECT ONE.                                    */ 00657000
*   /*                                                               */ 00658000
*   /*****************************************************************/ 00659000
*                                                               01S0384 00660000
*   CPABLKSZ = ZERO;                /* CLEAR                         */ 00661000
         SR    @09,@09                                          01S0384 00662000
         ST    @09,CPABLKSZ                                     01S0384 00663000
*   DO PLHSAVE3 = ONE TO CONBB WHILE (CPABLKSZ=ZERO);           01S0385 00664000
         LA    @09,1                                            01S0385 00665000
         B     @DE00385                                         01S0385 00666000
@DL00385 SR    @09,@09                                          01S0385 00667000
         C     @09,CPABLKSZ                                     01S0385 00668000
         BNE   @DC00385                                         01S0385 00669000
*     IF RWORK0 >= EDBLORBA & RWORK0 <= EDBHIRBA THEN /* RBA IN RANGE*/ 00670000
         L     @07,EDBLORBA(,RWORK2)                            01S0386 00671000
         CLR   RWORK0,@07                                       01S0386 00672000
         BL    @RF00386                                         01S0386 00673000
         CL    RWORK0,EDBHIRBA(,RWORK2)                         01S0386 00674000
         BH    @RF00386                                         01S0386 00675000
*       DO;                         /* EDB FOUND - NOW SET UP   01S0387 00676000
*                                      MBBCCHHR                      */ 00677000
*         CONM = EDBM;              /* M FROM EDB                    */ 00678000
         MVC   CONM(1,RPLH),EDBM(RWORK2)                        01S0388 00679000
*         CONBB = ZERO;             /* BB SET TO ZERO                */ 00680000
         STH   @09,CONBB(,RPLH)                                 01S0389 00681000
*         PLHSAVE3 = EDBSTTRK;      /* SAVE STARTING TRACK           */ 00682000
         MVC   PLHSAVE3(4),EDBSTTRK(RWORK2)                     01S0390 00683000
*         RESPECIFY RWORK3 RESTRICTED;                          01S0391 00684000
*         RWORK3 = RWORK0 - EDBLORBA; /* RBA RELATIVE TO EXTENT      */ 00685000
         L     @09,EDBLORBA(,RWORK2)                            01S0392 00686000
         LCR   @09,@09                                          01S0392 00687000
         ALR   @09,RWORK0                                       01S0392 00688000
         LR    RWORK3,@09                                       01S0392 00689000
*         RESPECIFY IDALPMB BASED(RWORK2);                      01S0393 00690000
*         RWORK2 = EDBLPMBA;        /* GET LPMB ADDRESS FROM EDB     */ 00691000
*                                                               01S0394 00692000
         L     RWORK2,EDBLPMBA(,RWORK2)                         01S0394 00693000
*   /*****************************************************************/ 00694000
*   /*                                                               */ 00695000
*   /* OPTIMIZE CODE BY USING ASSEMBLER CODE FOR DIVISION            */ 00696000
*   /*                                                               */ 00697000
*   /*****************************************************************/ 00698000
*                                                               01S0395 00699000
*         RESPECIFY (RWORK4,RWORK5) RESTRICTED;                 01S0395 00700000
*         GENERATE REFS(LPMAUSZ,LPMBPTRK);                      01S0396 00701000
*        CONCC = RWORK3 / LPMAUSZ                                       00702000
*        RWORK5 = RWORK3 // LPMAUSZ                                     00703000
         LR    RWORK5,RWORK3           RBA RELATIVE TO EXTENT IN ODD    00704000
         SR    RWORK4,RWORK4           ZERO EVEN REGISTER               00705000
         D     RWORK4,LPMAUSZ(,RWORK2) RELATIVE CYLINDER VALUE          00706000
         STH   RWORK5,CONCC(,RPLH)     STORE RELATIVE CYLINDER VALUE    00707000
         LR    RWORK5,RWORK4           REMAINDER INTO ODD REGISTER      00708000
*        CONHH = RWORK5 / LPMBPTRK                                      00709000
*        RWORK3 = RWORK5 // LPMBPTRK                                    00710000
         SR    RWORK4,RWORK4           ZERO EVEN REGISTER               00711000
         D     RWORK4,LPMBPTRK(,RWORK2) RELATIVE TRACK VALUE            00712000
         STH   RWORK5,CONHH(,RPLH)     STORE RELATIVE TRACK VALUE       00713000
         LR    RWORK3,RWORK4           REMAINDER INTO RWORK3            00714000
*         RESPECIFY (RWORK4,RWORK5) UNRESTRICTED;               01S0397 00715000
*         CONR = (RWORK3+LPMBLKSZ-ONE) / LPMBLKSZ + ONE; /* R        */ 00716000
         L     @09,LPMBLKSZ(,RWORK2)                            01S0398 00717000
         LR    @01,RWORK3                                       01S0398 00718000
         AR    @01,@09                                          01S0398 00719000
         BCTR  @01,0                                            01S0398 00720000
         LR    @04,@01                                          01S0398 00721000
         SRDA  @04,32                                           01S0398 00722000
         DR    @04,@09                                          01S0398 00723000
         AL    @05,@CF00062                                     01S0398 00724000
         STC   @05,CONR(,RPLH)                                  01S0398 00725000
*         RWORK3 = CONCC * LPMTRKAU + CONHH + PLHSAVE3;         01S0399 00726000
         SR    RWORK3,RWORK3                                    01S0399 00727000
         ICM   RWORK3,3,CONCC(RPLH)                             01S0399 00728000
         MH    RWORK3,LPMTRKAU(,RWORK2)                         01S0399 00729000
         SR    @09,@09                                          01S0399 00730000
         ICM   @09,3,CONHH(RPLH)                                01S0399 00731000
         AR    RWORK3,@09                                       01S0399 00732000
         A     RWORK3,PLHSAVE3                                  01S0399 00733000
*         RESPECIFY (RWORK4,RWORK5,R15) RESTRICTED;             01S0400 00734000
*         GENERATE REFS(LPMTPC);                                01S0401 00735000
*        CONCC = RWORK3 / LPMTPC                                        00736000
*        CONHH = RWORK3 // LPMTPC                                       00737000
         LR    RWORK5,RWORK3           LOAD ODD REGISTER                00738000
         SR    RWORK4,RWORK4           ZERO EVEN REGISTER               00739000
         LH    R15,LPMTPC(,RWORK2)                                      00740000
