         TITLE 'AMDSAAMR   ASM/VSAM CONVERSION ROUTINE                 *00001000
                       '                                                00002000
AMDSAAMR CSECT ,                                                01S0001 00003000
@MAINENT DS    0H                                               01S0001 00004000
         USING *,@15                                            01S0001 00005000
         B     @PROLOG                                          01S0001 00006000
         DC    AL1(16)                                          01S0001 00007000
         DC    C'AMDSAAMR  73.353'                              01S0001 00008000
         DROP  @15                                                      00009000
@PROLOG  STM   @14,@12,12(@13)                                  01S0001 00010000
         BALR  @11,0                                            01S0001 00011000
@PSTART  DS    0H                                               01S0001 00012000
         USING @PSTART,@11                                      01S0001 00013000
*   RAIE = R1;                      /* SAVE AIE ADDRESS              */ 00014000
         LR    RAIE,R1                                          01S0233 00015000
*   SVC(GTMSVC);                    /* CALL AMDSAGTM FOR STORAGE     */ 00016000
         SVC   4                                                01S0234 00017000
*   IF RETCODE ^= 0 THEN            /* STORAGE OBTAINED              */ 00018000
         LTR   RETCODE,RETCODE                                  01S0235 00019000
         BZ    @RF00235                                         01S0235 00020000
*     DO;                                                       01S0236 00021000
*        GENERATE REFS(R13,RETREG,RETCODE) SETS(R14,R12);       01S0237 00022000
         LM    R14,R12,12(R13)         RESTORE REGISTERS                00023000
         LA    RETCODE,4               SET ERROR RETURN CODE            00024000
         BR    RETREG                  RETURN TO CALLER                 00025000
*     END;                                                      01S0238 00026000
*   R8 = R1;                        /* GET ADDRESS OF WORK AREA      */ 00027000
@RF00235 LR    R8,R1                                            01S0239 00028000
*   GENERATE REFS(R8,R13) SETS(R15);                            01S0240 00029000
         USING @DATD,R8                MAKE WORK AREA ADDRESSABLE       00030000
         ST    R13,@SA00001+4          SAVE ADDR OF CALLERS SAVE AREA   00031000
         LA    R15,@SA00001            POINT TO SAVE AREA               00032000
*   SAVENEXT = R15;                 /* PLACE ADDRESS OF SAVE AREA IN    00033000
*                                      CALLER'S AREA                 */ 00034000
         ST    R15,SAVENEXT(,R13)                               01S0241 00035000
*   R13 = R15;                      /* POINT TO SAVE AREA            */ 00036000
         LR    R13,R15                                          01S0242 00037000
*   RESPECIFY (R1,RETCODE) UNRESTRICTED;                        01S0243 00038000
*   SAVENAME = 'AMR';               /* PLACE MODULE ID IN SAVE AREA  */ 00039000
         MVC   SAVENAME(3,R13),@CC01734                         01S0244 00040000
*   AMRBUFT = CTBUFTYP;             /* SAVE BUFFER REQUEST FLAGS     */ 00041000
         L     @09,VCTCCTV(,VCCTREG)                            01S0245 00042000
         SR    AMRBUFT,AMRBUFT                                  01S0245 00043000
         IC    AMRBUFT,CTBUFTYP(,@09)                           01S0245 00044000
*   CTBUFTYP = OFF;                 /* RESET BUFFER REQUEST FLAGS    */ 00045000
         MVI   CTBUFTYP(@09),B'00000000'                        01S0246 00046000
*   CTASM = ON;                     /* REQUEST ASM BUFFERS           */ 00047000
         OI    CTASM(@09),B'00010000'                           01S0247 00048000
*   RESPECIFY RCB BASED(ADDR(AMRRCB));                          01S0248 00049000
*   RCBNEXT = VCTRCB;               /* GET RCB CHAIN ADDRESS         */ 00050000
         LA    @09,AMRRCB                                       01S0249 00051000
         MVC   RCBNEXT(4,@09),VCTRCB(VCCTREG)                   01S0249 00052000
*   RCBEXIT = ADDR(AMREXIT);        /* POINT TO EXIT ADDRESS         */ 00053000
         LA    @05,AMREXIT                                      01S0250 00054000
         ST    @05,RCBEXIT(,@09)                                01S0250 00055000
*   RCBBASE = BASEREG;              /* GET BASE REGISTER VALUE       */ 00056000
         ST    BASEREG,RCBBASE(,@09)                            01S0251 00057000
*   RCBSAVE = R13;                  /* SAVE AREA ADDRESS IN RCB      */ 00058000
         ST    R13,RCBSAVE(,@09)                                01S0252 00059000
*   GENERATE REFS(R1) SETS(AMRWORK) (STCTL R1,R1,AMRWORK); /* GET       00060000
*                                      SEGMENT TABLE ADDRESS         */ 00061000
         STCTL R1,R1,AMRWORK                                            00062000
*   RCBSTOR = AMRWORK;              /* SEGMENT TABLE ADDRESS TO RCB  */ 00063000
         LA    @09,AMRRCB                                       01S0254 00064000
         MVC   RCBSTOR(4,@09),AMRWORK                           01S0254 00065000
*   VCTRCB = ADDR(AMRRCB);          /* MAKE RCB ACTIVE               */ 00066000
         ST    @09,VCTRCB(,VCCTREG)                             01S0255 00067000
*                                                               01S0256 00068000
*   /*****************************************************************/ 00069000
*   /*                                                               */ 00070000
*   /* VALIDITY CHECK THE ASPCT POINTED TO BY THE LGVT ENTRY.        */ 00071000
*   /* THE CHECK MUST BE MADE IN THE ASM MEMORY SINCE THE ASPCT IS   */ 00072000
*   /* NOT IN ANY COMMON AREA.                                       */ 00073000
*   /*                                                               */ 00074000
*   /*****************************************************************/ 00075000
*/* P SET VCTCBADR TO LGVENASP                                       */ 00076000
*/* P SET VCTCBID TO ASPCT ID                                        */ 00077000
*/* L  AMDSAVCK:  VALIDITY CHECK ASPCT                               */ 00078000
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */ 00079000
*                                                               01S0256 00080000
*   RESPECIFY AIE BASED(RAIE);                                  01S0256 00081000
*   RESPECIFY AIA BASED(RAIA);                                  01S0257 00082000
*   RESPECIFY RETCODE RESTRICTED;                               01S0258 00083000
*   VCTCBADR = LGVENASP;            /* GET ASPCT ADDR FROM LGVT ENTRY*/ 00084000
